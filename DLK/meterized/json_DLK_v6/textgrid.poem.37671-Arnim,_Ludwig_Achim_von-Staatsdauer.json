{"textgrid.poem.37671": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Staatsdauer", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sende ein F\u00e4hnlein voll Wohlgeruch", "tokens": ["Ich", "sen\u00b7de", "ein", "F\u00e4hn\u00b7lein", "voll", "Wohl\u00b7ge\u00b7ruch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "NN"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dir, Liebchen, die Zeit zu vertreiben,", "tokens": ["Dir", ",", "Lieb\u00b7chen", ",", "die", "Zeit", "zu", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Fern mu\u00df ich von dir noch bleiben,", "tokens": ["Fern", "mu\u00df", "ich", "von", "dir", "noch", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir duft' es im spielenden Flug.", "tokens": ["Dir", "duft'", "es", "im", "spie\u00b7len\u00b7den", "Flug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Ich schicke der Freunde wohl manchen hin,", "tokens": ["Ich", "schi\u00b7cke", "der", "Freun\u00b7de", "wohl", "man\u00b7chen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PIS", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Zu schaun, ob das F\u00e4hnlein noch flieget,", "tokens": ["Zu", "schaun", ",", "ob", "das", "F\u00e4hn\u00b7lein", "noch", "flie\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Mein fr\u00f6hliches Herze nicht tr\u00fcget,", "tokens": ["Mein", "fr\u00f6h\u00b7li\u00b7ches", "Her\u00b7ze", "nicht", "tr\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "PTKNEG", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Dein's flattert in spielendem Sinn.", "tokens": ["Dein's", "flat\u00b7tert", "in", "spie\u00b7len\u00b7dem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Das F\u00e4hnlein ich legte in's H\u00fcgelein,", "tokens": ["Das", "F\u00e4hn\u00b7lein", "ich", "leg\u00b7te", "in's", "H\u00fc\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wo Ameisen laufen ich sehe,", "tokens": ["Wo", "A\u00b7mei\u00b7sen", "lau\u00b7fen", "ich", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da klagete Nachtigall Wehe,", "tokens": ["Da", "kla\u00b7ge\u00b7te", "Nach\u00b7ti\u00b7gall", "We\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Ameisen laufen im Hain.", "tokens": ["Die", "A\u00b7mei\u00b7sen", "lau\u00b7fen", "im", "Hain", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Sie hatten ihr wandernd die Brut verzehrt.", "tokens": ["Sie", "hat\u00b7ten", "ihr", "wan\u00b7dernd", "die", "Brut", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sie raubet nun r\u00e4chend die Kleinen,", "tokens": ["Sie", "rau\u00b7bet", "nun", "r\u00e4\u00b7chend", "die", "Klei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sie haben nicht Zeit zum Vereinen,", "tokens": ["Sie", "ha\u00b7ben", "nicht", "Zeit", "zum", "Ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mit beiden nicht lang' es so w\u00e4hrt.", "tokens": ["Mit", "bei\u00b7den", "nicht", "lang'", "es", "so", "w\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PTKNEG", "ADV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Im Haufen, da sah's sonst wie Ordnung aus,", "tokens": ["Im", "Hau\u00b7fen", ",", "da", "sah's", "sonst", "wie", "Ord\u00b7nung", "aus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PIS", "ADV", "KOKOM", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da bauten sie dunkele G\u00e4nge,", "tokens": ["Da", "bau\u00b7ten", "sie", "dun\u00b7ke\u00b7le", "G\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sie schwitzten im engen Gedr\u00e4nge!", "tokens": ["Sie", "schwitz\u00b7ten", "im", "en\u00b7gen", "Ge\u00b7dr\u00e4n\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Nur davon noch duftet das Haus.", "tokens": ["Nur", "da\u00b7von", "noch", "duf\u00b7tet", "das", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Sie schmetterten manche mit Lasten todt,", "tokens": ["Sie", "schmet\u00b7ter\u00b7ten", "man\u00b7che", "mit", "Las\u00b7ten", "todt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "NN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und keine von allen durft' muchsen,", "tokens": ["Und", "kei\u00b7ne", "von", "al\u00b7len", "durft'", "much\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "APPR", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Verstohlen nur mochten sie schluchsen,", "tokens": ["Ver\u00b7stoh\u00b7len", "nur", "moch\u00b7ten", "sie", "schluch\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Das Dunkel lie\u00df munkeln von Noth.", "tokens": ["Das", "Dun\u00b7kel", "lie\u00df", "mun\u00b7keln", "von", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Die K\u00f6nigin m\u00fc\u00dfig erdenkt den Bau,", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", "m\u00fc\u00b7\u00dfig", "er\u00b7denkt", "den", "Bau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sie wei\u00df nur allein um die G\u00e4nge,", "tokens": ["Sie", "wei\u00df", "nur", "al\u00b7lein", "um", "die", "G\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wozu ist die L\u00e4nge der G\u00e4nge?", "tokens": ["Wo\u00b7zu", "ist", "die", "L\u00e4n\u00b7ge", "der", "G\u00e4n\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wozu der gewaltige Bau?", "tokens": ["Wo\u00b7zu", "der", "ge\u00b7wal\u00b7ti\u00b7ge", "Bau", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.8": {"line.1": {"text": "So fragen die M\u00e4nner, die denkend sind,", "tokens": ["So", "fra\u00b7gen", "die", "M\u00e4n\u00b7ner", ",", "die", "den\u00b7kend", "sind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "VVPP", "VAFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die andern alle noch sinnen,", "tokens": ["Die", "an\u00b7dern", "al\u00b7le", "noch", "sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie glauben es schon zu ersinnen,", "tokens": ["Sie", "glau\u00b7ben", "es", "schon", "zu", "er\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Einhaltend mit Arbeit geschwind.", "tokens": ["Ein\u00b7hal\u00b7tend", "mit", "Ar\u00b7beit", "ge\u00b7schwind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.9": {"line.1": {"text": "Ach wohl, wer die Zukunft ersinnen will,", "tokens": ["Ach", "wohl", ",", "wer", "die", "Zu\u00b7kunft", "er\u00b7sin\u00b7nen", "will", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$,", "PWS", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der siehet die Gegenwart schwinden,", "tokens": ["Der", "sie\u00b7het", "die", "Ge\u00b7gen\u00b7wart", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ei wisset, sie sollten sich winden", "tokens": ["Ei", "wis\u00b7set", ",", "sie", "soll\u00b7ten", "sich", "win\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PPER", "VMFIN", "PRF", "VVINF"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Die G\u00e4nge zum Brautgemach still.", "tokens": ["Die", "G\u00e4n\u00b7ge", "zum", "Braut\u00b7ge\u00b7mach", "still", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.10": {"line.1": {"text": "Die K\u00f6nigin selbst die versprochene Braut,", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", "selbst", "die", "ver\u00b7spro\u00b7che\u00b7ne", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "War noch vom Schwure gebunden,", "tokens": ["War", "noch", "vom", "Schwu\u00b7re", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zur Liebe sie war erst verbunden,", "tokens": ["Zur", "Lie\u00b7be", "sie", "war", "erst", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wenn herrlich die Kammer erbaut.", "tokens": ["Wenn", "herr\u00b7lich", "die", "Kam\u00b7mer", "er\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.11": {"line.1": {"text": "Die K\u00f6nigin \u00e4rgert zu Tode sich,", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", "\u00e4r\u00b7gert", "zu", "To\u00b7de", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "PRF", "$,"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Ameisen frierend verschmachten,", "tokens": ["Die", "A\u00b7mei\u00b7sen", "frie\u00b7rend", "ver\u00b7schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ja weil sie zu viel sich bedachten,", "tokens": ["Ja", "weil", "sie", "zu", "viel", "sich", "be\u00b7dach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOUS", "PPER", "PTKA", "PIS", "PRF", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ja weil sie nur dachten an sich.", "tokens": ["Ja", "weil", "sie", "nur", "dach\u00b7ten", "an", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOUS", "PPER", "ADV", "VVFIN", "APPR", "PRF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}