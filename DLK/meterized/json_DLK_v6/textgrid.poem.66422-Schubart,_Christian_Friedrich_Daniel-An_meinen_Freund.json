{"textgrid.poem.66422": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An meinen Freund", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund! ", "tokens": ["Freund", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.2": {"text": "O s\u00e4hst du meinen Busen beben!", "tokens": ["O", "s\u00e4hst", "du", "mei\u00b7nen", "Bu\u00b7sen", "be\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er wallt, und jeder Tropfen Blut", "tokens": ["Er", "wallt", ",", "und", "je\u00b7der", "Trop\u00b7fen", "Blut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "PIAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Spricht laut: mein Pythias soll leben.", "tokens": ["Spricht", "laut", ":", "mein", "Py\u00b7thi\u00b7as", "soll", "le\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$.", "PPOSAT", "NE", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}