{"dta.poem.10012": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Vergn\u00fcgungen  \n  der Melancholey.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20674-3", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nie auch wolle die Seele sich sch\u00f6ner zu bilden", "tokens": ["Nie", "auch", "wol\u00b7le", "die", "See\u00b7le", "sich", "sch\u00f6\u00b7ner", "zu", "bil\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "ART", "NN", "PRF", "ADJD", "PTKZU", "VVINF"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Durch den sanften und r\u00fchrenden Schmerz der tragi-", "tokens": ["Durch", "den", "sanf\u00b7ten", "und", "r\u00fch\u00b7ren\u00b7den", "Schmerz", "der", "tra\u00b7gi"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "KON", "ADJA", "NN", "ART", "TRUNC"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Sie, Melpomene, die im Cothurn erhaben einhertritt,", "tokens": ["Sie", ",", "Mel\u00b7po\u00b7me\u00b7ne", ",", "die", "im", "Cot\u00b7hurn", "er\u00b7ha\u00b7ben", "ein\u00b7her\u00b7tritt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$,", "PRELS", "APPRART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "In dem Leichengewand; sie ist des h\u00f6heren Mitleids", "tokens": ["In", "dem", "Lei\u00b7chen\u00b7ge\u00b7wand", ";", "sie", "ist", "des", "h\u00f6\u00b7he\u00b7ren", "Mit\u00b7leids"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Pflegemutter, Jetzt mag mit thr\u00e4nenstr\u00f6menden Augen", "tokens": ["Pfle\u00b7ge\u00b7mut\u00b7ter", ",", "Jetzt", "mag", "mit", "thr\u00e4\u00b7nen\u00b7str\u00f6\u00b7men\u00b7den", "Au\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VMFIN", "APPR", "ADJA", "NN"], "meter": "+-+----+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Ueber befleckte verwundete Liebe Monimia", "tokens": ["Ue\u00b7ber", "be\u00b7fleck\u00b7te", "ver\u00b7wun\u00b7de\u00b7te", "Lie\u00b7be", "Mo\u00b7ni\u00b7mia"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN", "NE"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.7": {"text": "Oder la\u00df Juliet ", "tokens": ["O\u00b7der", "la\u00df", "Ju\u00b7liet"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVIMP", "NE"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Jhres getreuen Romeo Lippen zum letztenmal k\u00fcssen,", "tokens": ["Ih\u00b7res", "ge\u00b7treu\u00b7en", "Ro\u00b7meo", "Lip\u00b7pen", "zum", "letz\u00b7ten\u00b7mal", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "APPRART", "ADV", "VVINF", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.9": {"text": "Seine Lippen, noch rauchend vom Brand des t\u00f6dtli-", "tokens": ["Sei\u00b7ne", "Lip\u00b7pen", ",", "noch", "rau\u00b7chend", "vom", "Brand", "des", "t\u00f6dt\u00b7li"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ADJD", "APPRART", "NN", "ART", "TRUNC"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.10": {"text": "La\u00df um einen vergeblichen Blick den Jaffeir ", "tokens": ["La\u00df", "um", "ei\u00b7nen", "ver\u00b7geb\u00b7li\u00b7chen", "Blick", "den", "Jaf\u00b7feir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Hinknien; oder la\u00df auch auf Desdemonen ", "tokens": ["Hin\u00b7kni\u00b7en", ";", "o\u00b7der", "la\u00df", "auch", "auf", "Des\u00b7de\u00b7mo\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "KON", "VVIMP", "ADV", "APPR", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Seiner Eifersucht Wuth die h\u00e4rtesten Drohungen sch\u00fct-", "tokens": ["Sei\u00b7ner", "Ei\u00b7fer\u00b7sucht", "Wuth", "die", "h\u00e4r\u00b7tes\u00b7ten", "Dro\u00b7hun\u00b7gen", "sch\u00fct"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "ART", "ADJA", "NN", "TRUNC"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.13": {"text": "Pl\u00f6tzlich rieselt der m\u00e4nnliche Strom von schwellenden", "tokens": ["Pl\u00f6tz\u00b7lich", "rie\u00b7selt", "der", "m\u00e4nn\u00b7li\u00b7che", "Strom", "von", "schwel\u00b7len\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "Auf die Wange herab, und bey dem Ungl\u00fcck des Bruders", "tokens": ["Auf", "die", "Wan\u00b7ge", "her\u00b7ab", ",", "und", "bey", "dem", "Un\u00b7gl\u00fcck", "des", "Bru\u00b7ders"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "$,", "KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "Schmilzt mein z\u00e4rtliches Herz in sympathetischen", "tokens": ["Schmilzt", "mein", "z\u00e4rt\u00b7li\u00b7ches", "Herz", "in", "sym\u00b7pa\u00b7the\u00b7ti\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "ADJA"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}}}}