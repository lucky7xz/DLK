{"textgrid.poem.37150": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Der Irrwisch", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Spiele nur immer, gaukelnder Betr\u00fcger,", "tokens": ["Spie\u00b7le", "nur", "im\u00b7mer", ",", "gau\u00b7keln\u00b7der", "Be\u00b7tr\u00fc\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Spiele nur immer deine leichten T\u00e4nze,", "tokens": ["Spie\u00b7le", "nur", "im\u00b7mer", "dei\u00b7ne", "leich\u00b7ten", "T\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Fl\u00fcchtiges Dunstkind, das des Wandrers F\u00fc\u00dfe", "tokens": ["Fl\u00fcch\u00b7ti\u00b7ges", "Dunst\u00b7kind", ",", "das", "des", "Wand\u00b7rers", "F\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "ART", "NN", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Br\u00fcnstig heranlockt,", "tokens": ["Br\u00fcns\u00b7tig", "her\u00b7an\u00b7lockt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Spr\u00f6de dann fliehet, endlich ins Verderben", "tokens": ["Spr\u00f6\u00b7de", "dann", "flie\u00b7het", ",", "end\u00b7lich", "ins", "Ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "$,", "ADV", "APPRART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "F\u00fchret. Ich kenne diese M\u00e4dchenr\u00e4nke!", "tokens": ["F\u00fch\u00b7ret", ".", "Ich", "ken\u00b7ne", "die\u00b7se", "M\u00e4d\u00b7chen\u00b7r\u00e4n\u00b7ke", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "VVFIN", "PDAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Lernte sie all' aus Deinem blauen Auge,", "tokens": ["Lern\u00b7te", "sie", "all'", "aus", "Dei\u00b7nem", "blau\u00b7en", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Flatternde Nais!", "tokens": ["Flat\u00b7tern\u00b7de", "Nais", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}