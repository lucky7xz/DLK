{"dta.poem.9899": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mich hat die natur gesetzt mitten zwischen helle lichter/", "tokens": ["Mich", "hat", "die", "na\u00b7tur", "ge\u00b7setzt", "mit\u00b7ten", "zwi\u00b7schen", "hel\u00b7le", "lich\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Gegen einen grund ich henge/ wohin ich nicht fallen kan:", "tokens": ["Ge\u00b7gen", "ei\u00b7nen", "grund", "ich", "hen\u00b7ge", "/", "wo\u00b7hin", "ich", "nicht", "fal\u00b7len", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$(", "PWAV", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Aus mir wird vor herr und frauen mancher unflath weggetha\u0303/", "tokens": ["Aus", "mir", "wird", "vor", "herr", "und", "frau\u00b7en", "man\u00b7cher", "un\u00b7flath", "weg\u00b7geth\u00e3", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "APPR", "NN", "KON", "VVFIN", "PIAT", "NN", "APPR", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Meine sch\u00f6nheit die gef\u00e4llt auch dem \u00e4rgsten splitter-richter.", "tokens": ["Mei\u00b7ne", "sch\u00f6n\u00b7heit", "die", "ge\u00b7f\u00e4llt", "auch", "dem", "\u00e4rgs\u00b7ten", "split\u00b7ter\u00b7rich\u00b7ter", "."], "to