{"dta.poem.9955": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "64.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin dieser fels/ bey dem alle menschen auff der erden/", "tokens": ["Ich", "bin", "die\u00b7ser", "fels", "/", "bey", "dem", "al\u00b7le", "men\u00b7schen", "auff", "der", "er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDAT", "NN", "$(", "APPR", "ART", "PIAT", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Auch die kl\u00fcgsten auf der see/ in den sand gesencket werden/", "tokens": ["Auch", "die", "kl\u00fcgs\u00b7ten", "auf", "der", "see", "/", "in", "den", "sand", "ge\u00b7sen\u00b7cket", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "APPR", "ART", "NN", "$(", "APPR", "ART", "NN", "VVPP", "VAINF", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Keiner k\u00f6mt mir gern zu nah/ den behertzten mach ich grauen/", "tokens": ["Kei\u00b7ner", "k\u00f6mt", "mir", "gern", "zu", "nah", "/", "den", "be\u00b7hertz\u00b7ten", "mach", "ich", "grau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$(", "ART", "ADJA", "VVFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Aller orten findt man mich/ will man gleich nicht nach mir", "tokens": ["Al\u00b7ler", "or\u00b7ten", "findt", "man", "mich", "/", "will", "man", "gleich", "nicht", "nach", "mir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PIS", "PPER", "$(", "VMFIN", "PIS", "ADV", "PTKNEG", "APPR", "PPER"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "schauen.", "tokens": ["schau\u00b7en", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}