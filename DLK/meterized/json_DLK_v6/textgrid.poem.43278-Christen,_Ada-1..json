{"textgrid.poem.43278": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcnf Treppen hoch, f\u00fcnf Treppen hoch,", "tokens": ["F\u00fcnf", "Trep\u00b7pen", "hoch", ",", "f\u00fcnf", "Trep\u00b7pen", "hoch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "$,", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Himmel nah, dem blauen,", "tokens": ["Dem", "Him\u00b7mel", "nah", ",", "dem", "blau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Tauben nur verm\u00f6gen noch", "tokens": ["Die", "Tau\u00b7ben", "nur", "ver\u00b7m\u00f6\u00b7gen", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In unser Heim zu schauen.", "tokens": ["In", "un\u00b7ser", "Heim", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Tief unten liegt die Welt, es dringt", "tokens": ["Tief", "un\u00b7ten", "liegt", "die", "Welt", ",", "es", "dringt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "ADV", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur in verlornen T\u00f6nen", "tokens": ["Nur", "in", "ver\u00b7lor\u00b7nen", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Herauf, was so bet\u00e4ubend klingt,", "tokens": ["Her\u00b7auf", ",", "was", "so", "be\u00b7t\u00e4u\u00b7bend", "klingt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Jubeln und ihr St\u00f6hnen.", "tokens": ["Ihr", "Ju\u00b7beln", "und", "ihr", "St\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wenn es auch oben einsam ist,", "tokens": ["Wenn", "es", "auch", "o\u00b7ben", "ein\u00b7sam", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du sehnst Dich nicht hinunter,", "tokens": ["Du", "sehnst", "Dich", "nicht", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wie Dein kleiner Vogel bist", "tokens": ["Und", "wie", "Dein", "klei\u00b7ner", "Vo\u00b7gel", "bist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPOSAT", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du immer froh und munter.", "tokens": ["Du", "im\u00b7mer", "froh", "und", "mun\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Vom Kirchthurm in die traute Ruh'", "tokens": ["Vom", "Kirch\u00b7thurm", "in", "die", "trau\u00b7te", "Ruh'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des St\u00fcbchens manchmal klingen", "tokens": ["Des", "St\u00fcb\u00b7chens", "manch\u00b7mal", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Glockenstimmen ... aber Du", "tokens": ["Die", "Glo\u00b7ckens\u00b7tim\u00b7men", "...", "a\u00b7ber", "Du"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$(", "KON", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kannst doch viel sch\u00f6ner singen.", "tokens": ["Kannst", "doch", "viel", "sch\u00f6\u00b7ner", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "F\u00fcnf Treppen hoch, f\u00fcnf Treppen hoch", "tokens": ["F\u00fcnf", "Trep\u00b7pen", "hoch", ",", "f\u00fcnf", "Trep\u00b7pen", "hoch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["CARD", "NN", "ADJD", "$,", "CARD", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Halt ich Dich treu geborgen,", "tokens": ["Halt", "ich", "Dich", "treu", "ge\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was gilt die Welt mir unten noch", "tokens": ["Was", "gilt", "die", "Welt", "mir", "un\u00b7ten", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ihren grauen Sorgen.", "tokens": ["Mit", "ih\u00b7ren", "grau\u00b7en", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}