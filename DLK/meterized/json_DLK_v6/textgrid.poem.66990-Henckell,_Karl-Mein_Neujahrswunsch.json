{"textgrid.poem.66990": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Mein Neujahrswunsch", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ich erw\u00fcnsche vom neuen Jahre?", "tokens": ["Was", "ich", "er\u00b7w\u00fcn\u00b7sche", "vom", "neu\u00b7en", "Jah\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da\u00df ich die Wurzel der Kraft mir wahre,", "tokens": ["Da\u00df", "ich", "die", "Wur\u00b7zel", "der", "Kraft", "mir", "wah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "PPER", "ADJA", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Festzustehen im Grund der Erden,", "tokens": ["Fest\u00b7zu\u00b7ste\u00b7hen", "im", "Grund", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Nicht zu lockern und morsch zu werden,", "tokens": ["Nicht", "zu", "lo\u00b7ckern", "und", "morsch", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "VVINF", "KON", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Mit den frisch ergr\u00fcnenden Bl\u00e4ttern", "tokens": ["Mit", "den", "frisch", "er\u00b7gr\u00fc\u00b7nen\u00b7den", "Bl\u00e4t\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Wieder zu trotzen Wind und Wettern,", "tokens": ["Wie\u00b7der", "zu", "trot\u00b7zen", "Wind", "und", "Wet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mag es \u00e4chzen und mag es krachen,", "tokens": ["Mag", "es", "\u00e4ch\u00b7zen", "und", "mag", "es", "kra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "KON", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Stark zu rauschen, ruhig zu lachen,", "tokens": ["Stark", "zu", "rau\u00b7schen", ",", "ru\u00b7hig", "zu", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "So in Regen wie Sonnenschein", "tokens": ["So", "in", "Re\u00b7gen", "wie", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KOKOM", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Freunden ein Baum des Lebens zu sein.", "tokens": ["Freun\u00b7den", "ein", "Baum", "des", "Le\u00b7bens", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}}}}