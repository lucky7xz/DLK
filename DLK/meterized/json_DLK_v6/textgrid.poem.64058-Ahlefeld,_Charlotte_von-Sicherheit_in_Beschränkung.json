{"textgrid.poem.64058": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Sicherheit in Beschr\u00e4nkung", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In des Baches reinen, hellen,", "tokens": ["In", "des", "Ba\u00b7ches", "rei\u00b7nen", ",", "hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leise nur bewegten Wellen,", "tokens": ["Lei\u00b7se", "nur", "be\u00b7weg\u00b7ten", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schl\u00fcpft das Fischlein froh dahin.", "tokens": ["Schl\u00fcpft", "das", "Fisc\u00b7hlein", "froh", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die klaren Fluthen geben", "tokens": ["Und", "die", "kla\u00b7ren", "Flut\u00b7hen", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seinem stillen, stummen Leben", "tokens": ["Sei\u00b7nem", "stil\u00b7len", ",", "stum\u00b7men", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nahrung und Vergn\u00fcgen hin.", "tokens": ["Nah\u00b7rung", "und", "Ver\u00b7gn\u00fc\u00b7gen", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sorgenlos und leicht und heiter", "tokens": ["Sor\u00b7gen\u00b7los", "und", "leicht", "und", "hei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwimmt es mit dem B\u00e4chlein weiter,", "tokens": ["Schwimmt", "es", "mit", "dem", "B\u00e4ch\u00b7lein", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis in's weite offne Meer", "tokens": ["Bis", "in's", "wei\u00b7te", "off\u00b7ne", "Meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPRART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich der Wellen Lauf ergiesset \u2013", "tokens": ["Sich", "der", "Wel\u00b7len", "Lauf", "er\u00b7gies\u00b7set", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach die fremde Bahn erschliesset", "tokens": ["Ach", "die", "frem\u00b7de", "Bahn", "er\u00b7schlies\u00b7set"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihm der Sorgen dunkles Heer.", "tokens": ["Ihm", "der", "Sor\u00b7gen", "dunk\u00b7les", "Heer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Furchtbar drohende Gestalten,", "tokens": ["Furcht\u00b7bar", "dro\u00b7hen\u00b7de", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Die in feuchter Tiefe walten,", "tokens": ["Die", "in", "feuch\u00b7ter", "Tie\u00b7fe", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Winden schreckend sich heran.", "tokens": ["Win\u00b7den", "schre\u00b7ckend", "sich", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Tod, mit bangem Schauer", "tokens": ["Und", "der", "Tod", ",", "mit", "ban\u00b7gem", "Schau\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Steht, so d\u00fcnkt ihm, auf der Lauer,", "tokens": ["Steht", ",", "so", "d\u00fcnkt", "ihm", ",", "auf", "der", "Lau\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "PPER", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Will ihm hundertf\u00e4ltig nahn.", "tokens": ["Will", "ihm", "hun\u00b7dert\u00b7f\u00e4l\u00b7tig", "nahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "ADJA", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber ach, ihm zu entfliehen,", "tokens": ["A\u00b7ber", "ach", ",", "ihm", "zu", "ent\u00b7flie\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wehrt das Schicksal nun, es ziehen", "tokens": ["Wehrt", "das", "Schick\u00b7sal", "nun", ",", "es", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leise M\u00e4chte es dahin.", "tokens": ["Lei\u00b7se", "M\u00e4ch\u00b7te", "es", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und des Meeres wilde Wogen", "tokens": ["Und", "des", "Mee\u00b7res", "wil\u00b7de", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Haben rauschend es betrogen", "tokens": ["Ha\u00b7ben", "rau\u00b7schend", "es", "be\u00b7tro\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Um den sichern, heitern Sinn.", "tokens": ["Um", "den", "si\u00b7chern", ",", "hei\u00b7tern", "Sinn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "So vertauscht des Landes stilles Leben", "tokens": ["So", "ver\u00b7tauscht", "des", "Lan\u00b7des", "stil\u00b7les", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mancher mit der St\u00e4dte lautem Streben,", "tokens": ["Man\u00b7cher", "mit", "der", "St\u00e4d\u00b7te", "lau\u00b7tem", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und gesellt sich finstern Sorgen zu.", "tokens": ["Und", "ge\u00b7sellt", "sich", "fins\u00b7tern", "Sor\u00b7gen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Nur die Abgeschiedenheit gew\u00e4hret Frieden,", "tokens": ["Nur", "die", "Ab\u00b7ge\u00b7schie\u00b7den\u00b7heit", "ge\u00b7w\u00e4h\u00b7ret", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Und der Stille nur entkeimt die Ruh.", "tokens": ["Und", "der", "Stil\u00b7le", "nur", "ent\u00b7keimt", "die", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}