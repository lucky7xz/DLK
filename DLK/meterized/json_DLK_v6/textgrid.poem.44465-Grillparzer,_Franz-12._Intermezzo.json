{"textgrid.poem.44465": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "12. Intermezzo", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im holden Mond der Maien,", "tokens": ["Im", "hol\u00b7den", "Mond", "der", "Mai\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn lichte Blumen bl\u00fchn,", "tokens": ["Wenn", "lich\u00b7te", "Blu\u00b7men", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gefl\u00fcgelte Schalmaien", "tokens": ["Ge\u00b7fl\u00fc\u00b7gel\u00b7te", "Schal\u00b7mai\u00b7en"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Die Waldesnacht durchziehn,", "tokens": ["Die", "Wal\u00b7des\u00b7nacht", "durch\u00b7ziehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da hebt sich eine Scholle,", "tokens": ["Da", "hebt", "sich", "ei\u00b7ne", "Schol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Liebe lauscht hervor,", "tokens": ["Die", "Lie\u00b7be", "lauscht", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob noch der Winter grolle,", "tokens": ["Ob", "noch", "der", "Win\u00b7ter", "grol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch laut der St\u00fcrme Chor?", "tokens": ["Noch", "laut", "der", "St\u00fcr\u00b7me", "Chor", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sieht gr\u00fcn sie nun die Weite,", "tokens": ["Sieht", "gr\u00fcn", "sie", "nun", "die", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ertr\u00e4gt sies nicht im Haus,", "tokens": ["Er\u00b7tr\u00e4gt", "sies", "nicht", "im", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie fliegt auf Spiel und Beute", "tokens": ["Sie", "fliegt", "auf", "Spiel", "und", "Beu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gleich andern V\u00f6geln aus.", "tokens": ["Gleich", "an\u00b7dern", "V\u00f6\u00b7geln", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch friert es etwa n\u00e4chtig,", "tokens": ["Doch", "friert", "es", "et\u00b7wa", "n\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sucht sie der Menschen Dach", "tokens": ["Sucht", "sie", "der", "Men\u00b7schen", "Dach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sch\u00fcrt ein Feuer m\u00e4chtig", "tokens": ["Und", "sch\u00fcrt", "ein", "Feu\u00b7er", "m\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In jungen Herzen wach.", "tokens": ["In", "jun\u00b7gen", "Her\u00b7zen", "wach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}