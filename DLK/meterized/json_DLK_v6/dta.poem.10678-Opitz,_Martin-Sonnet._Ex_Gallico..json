{"dta.poem.10678": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet. Ex Gallico.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr kalten Wasserb\u00e4ch/ jhr H\u00f6len/ vnd jhr Steine/", "tokens": ["Ihr", "kal\u00b7ten", "Was\u00b7ser\u00b7b\u00e4ch", "/", "jhr", "H\u00f6\u00b7len", "/", "vnd", "jhr", "Stei\u00b7ne", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PPOSAT", "NN", "$(", "KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr gr\u00fcnen Eichenbaum ihr sch\u00f6nsten in dem Waldt/", "tokens": ["Ihr", "gr\u00fc\u00b7nen", "Ei\u00b7chen\u00b7baum", "ihr", "sch\u00f6ns\u00b7ten", "in", "dem", "Waldt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach h\u00f6ret doch/ wie ich erseufftze manigfalt/", "tokens": ["Ach", "h\u00f6\u00b7ret", "doch", "/", "wie", "ich", "er\u00b7seufft\u00b7ze", "ma\u00b7nig\u00b7falt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "$(", "PWAV", "PPER", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schreibt auff mein Testament jhr vnbewohnten Haine/", "tokens": ["Schreibt", "auff", "mein", "Tes\u00b7ta\u00b7ment", "jhr", "vn\u00b7be\u00b7wohn\u00b7ten", "Hai\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Seit Secretarien/ wie ich vmb Vngl\u00fcck weine/", "tokens": ["Seit", "Se\u00b7cre\u00b7ta\u00b7ri\u00b7en", "/", "wie", "ich", "vmb", "Vn\u00b7gl\u00fcck", "wei\u00b7ne", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$(", "PWAV", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Grabts in die Rinden ein/ auff da\u00df es der gestallt", "tokens": ["Grabts", "in", "die", "Rin\u00b7den", "ein", "/", "auff", "da\u00df", "es", "der", "ge\u00b7stallt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "ART", "$(", "APPR", "KOUS", "PPER", "ART", "VVPP"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Wachs jmmerfort/ wie jhr: Ich aber sterbe bald/", "tokens": ["Wachs", "jm\u00b7mer\u00b7fort", "/", "wie", "jhr", ":", "Ich", "a\u00b7ber", "ster\u00b7be", "bald", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$(", "PWAV", "PPER", "$.", "PPER", "ADV", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Beraubet meiner selbst/ vnd ehnlich einem Scheine.", "tokens": ["Be\u00b7rau\u00b7bet", "mei\u00b7ner", "selbst", "/", "vnd", "ehn\u00b7lich", "ei\u00b7nem", "Schei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADV", "$(", "KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich sterb au\u00df Tyranney der sch\u00f6nen grausamkeit/", "tokens": ["Ich", "sterb", "au\u00df", "Ty\u00b7ran\u00b7ney", "der", "sch\u00f6\u00b7nen", "grau\u00b7sam\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Liebe/ die ohn sich ohn mich ist jederzeit/", "tokens": ["Der", "Lie\u00b7be", "/", "die", "ohn", "sich", "ohn", "mich", "ist", "je\u00b7der\u00b7zeit", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "APPR", "PRF", "APPR", "PPER", "VAFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die/ als ein Tigerthier/ mein Blut au\u00dfsaugen k\u00fcnnen", "tokens": ["Die", "/", "als", "ein", "Ti\u00b7ger\u00b7thier", "/", "mein", "Blut", "au\u00df\u00b7sau\u00b7gen", "k\u00fcn\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$(", "KOUS", "ART", "NN", "$(", "PPOSAT", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ade jhr W\u00e4ld ade/ ade jhr gr\u00fcne Lust/", "tokens": ["A\u00b7de", "jhr", "W\u00e4ld", "a\u00b7de", "/", "a\u00b7de", "jhr", "gr\u00fc\u00b7ne", "Lust", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "$(", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.13": {"text": "Ihr/ denen Venus vnd jhr Sohn nicht ist bewust/", "tokens": ["Ihr", "/", "de\u00b7nen", "Ve\u00b7nus", "vnd", "jhr", "Sohn", "nicht", "ist", "be\u00b7wust", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PDS", "NN", "KON", "PPOSAT", "NN", "PTKNEG", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die auch die Weisesten berauben jhrer Sinnen.", "tokens": ["Die", "auch", "die", "Wei\u00b7ses\u00b7ten", "be\u00b7rau\u00b7ben", "jhrer", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}}}}}