{"textgrid.poem.58929": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "148. Ein tapferer Sinn", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei stets dem Zug der Gnade treu", "tokens": ["Sei", "stets", "dem", "Zug", "der", "Gna\u00b7de", "treu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und keine M\u00fch noch Leiden scheu!", "tokens": ["Und", "kei\u00b7ne", "M\u00fch", "noch", "Lei\u00b7den", "scheu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was heut' ist schwer, wird morgen leicht;", "tokens": ["Was", "heut'", "ist", "schwer", ",", "wird", "mor\u00b7gen", "leicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "ADJD", "$,", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein tapfrer Sinn die Kron' erreicht.", "tokens": ["Ein", "tapf\u00b7rer", "Sinn", "die", "Kron'", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}