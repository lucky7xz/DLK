{"textgrid.poem.51289": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein gantzes Leben ist, o Mensch, ein hartes Joch.", "tokens": ["Dein", "gant\u00b7zes", "Le\u00b7ben", "ist", ",", "o", "Mensch", ",", "ein", "har\u00b7tes", "Joch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "$,", "FM", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Seele liegt verdeckt in einer Hand voll Koth.", "tokens": ["Die", "See\u00b7le", "liegt", "ver\u00b7deckt", "in", "ei\u00b7ner", "Hand", "voll", "Koth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den Leib vor der Geburt umkerckert Gall und Gifft.", "tokens": ["Den", "Leib", "vor", "der", "Ge\u00b7burt", "um\u00b7ker\u00b7ckert", "Gall", "und", "Gifft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Sinnen kn\u00fcpfft und schleust in Ketten denn die Zeit.", "tokens": ["Die", "Sin\u00b7nen", "kn\u00fcpfft", "und", "schleust", "in", "Ket\u00b7ten", "denn", "die", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "APPR", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Wille wird umbschr\u00e4nckt durch S\u00fcnd' und eitlen Wahn.", "tokens": ["Der", "Wil\u00b7le", "wird", "umbsc\u00b7hr\u00e4nckt", "durch", "S\u00fcnd'", "und", "eit\u00b7len", "Wahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und alles \u00fcbertrifft des Himmels fester Schlu\u00df.", "tokens": ["Und", "al\u00b7les", "\u00fc\u00b7bert\u00b7rifft", "des", "Him\u00b7mels", "fes\u00b7ter", "Schlu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie offte wird verk\u00fcrtzt dein Vorsatz Rath und Schlu\u00df.", "tokens": ["Wie", "off\u00b7te", "wird", "ver\u00b7k\u00fcrtzt", "dein", "Vor\u00b7satz", "Rath", "und", "Schlu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "VVFIN", "PPOSAT", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und du kanst nirgend hin als wie dich treibt dein Joch.", "tokens": ["Und", "du", "kanst", "nir\u00b7gend", "hin", "als", "wie", "dich", "treibt", "dein", "Joch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "ADV", "KOUS", "KOKOM", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie offt erwehlest du vor Himmel Geld und Koth,", "tokens": ["Wie", "offt", "er\u00b7weh\u00b7lest", "du", "vor", "Him\u00b7mel", "Geld", "und", "Koth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "APPR", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und stillst der Seelen Durst mit deines Geitzes Gifft.", "tokens": ["Und", "stillst", "der", "See\u00b7len", "Durst", "mit", "dei\u00b7nes", "Geit\u00b7zes", "Gifft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie offte bringst du durch den theuren Schatz die Zeit.", "tokens": ["Wie", "off\u00b7te", "bringst", "du", "durch", "den", "theu\u00b7ren", "Schatz", "die", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und gehst, wo dich hin schleppt dein eigner toller Wahn.", "tokens": ["Und", "gehst", ",", "wo", "dich", "hin", "schleppt", "dein", "eig\u00b7ner", "tol\u00b7ler", "Wahn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dein Wunsch und Himmel Reich ist ein gef\u00e4rbter Wahn.", "tokens": ["Dein", "Wunsch", "und", "Him\u00b7mel", "Reich", "ist", "ein", "ge\u00b7f\u00e4rb\u00b7ter", "Wahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und alles, was du thust, ein Ratschlag ohne Schlu\u00df.", "tokens": ["Und", "al\u00b7les", ",", "was", "du", "thust", ",", "ein", "Rat\u00b7schlag", "oh\u00b7ne", "Schlu\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du kr\u00fcmmst die Achseln dir in schwerer Dienste Joch,", "tokens": ["Du", "kr\u00fcmmst", "die", "Ach\u00b7seln", "dir", "in", "schwe\u00b7rer", "Diens\u00b7te", "Joch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "APPR", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und steckst nur tieff versenckt in der Begierde Koth.", "tokens": ["Und", "steckst", "nur", "tieff", "ver\u00b7senckt", "in", "der", "Be\u00b7gier\u00b7de", "Koth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "VVPP", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein Heil, auff das du denckst, ist deiner Seelen Gifft,", "tokens": ["Dein", "Heil", ",", "auff", "das", "du", "denckst", ",", "ist", "dei\u00b7ner", "See\u00b7len", "Gifft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und dann dein gantzes Du ein Raub und Traum der Zeit.", "tokens": ["Und", "dann", "dein", "gant\u00b7zes", "Du", "ein", "Raub", "und", "Traum", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "ADJA", "PPER", "ART", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Drumb rei\u00df, o Mensch, entzwey das Band der strengen Zeit,", "tokens": ["Drumb", "rei\u00df", ",", "o", "Mensch", ",", "ent\u00b7zwey", "das", "Band", "der", "stren\u00b7gen", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "FM", "NN", "$,", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und kratze durch Verstand den Rost aus deinem Wahn.", "tokens": ["Und", "krat\u00b7ze", "durch", "Ver\u00b7stand", "den", "Rost", "aus", "dei\u00b7nem", "Wahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Umbklammre dein Gem\u00fcth und dann des Himmels Schlu\u00df.", "tokens": ["Umb\u00b7klamm\u00b7re", "dein", "Ge\u00b7m\u00fcth", "und", "dann", "des", "Him\u00b7mels", "Schlu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und brich der Wollust Feld durch deines Lebens Joch.", "tokens": ["Und", "brich", "der", "Wol\u00b7lust", "Feld", "durch", "dei\u00b7nes", "Le\u00b7bens", "Joch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Entk\u00f6rpere die Seel aus diesem S\u00fcnden Koth.", "tokens": ["Ent\u00b7k\u00f6r\u00b7pe\u00b7re", "die", "Seel", "aus", "die\u00b7sem", "S\u00fcn\u00b7den", "Koth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PDAT", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ja Welt, und was ihr Thun, fleuch \u00e4rger noch als Gifft.", "tokens": ["Ja", "Welt", ",", "und", "was", "ihr", "Thun", ",", "fleuch", "\u00e4r\u00b7ger", "noch", "als", "Gifft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "$,", "KON", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "ADJD", "ADV", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "So wird in Heil verkehrt, in Artzney dieses Gifft,", "tokens": ["So", "wird", "in", "Heil", "ver\u00b7kehrt", ",", "in", "Artz\u00b7ney", "die\u00b7ses", "Gifft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "VVPP", "$,", "APPR", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So blickt der Himmel f\u00fcr in Abfall deiner Zeit.", "tokens": ["So", "blickt", "der", "Him\u00b7mel", "f\u00fcr", "in", "Ab\u00b7fall", "dei\u00b7ner", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So wird der Warheit Licht ausla\u00fctern diesen Wahn.", "tokens": ["So", "wird", "der", "War\u00b7heit", "Licht", "aus\u00b7la\u00fc\u00b7tern", "die\u00b7sen", "Wahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So zwingt durch Folgen auch den Sinn des Himmels Schlu\u00df.", "tokens": ["So", "zwingt", "durch", "Fol\u00b7gen", "auch", "den", "Sinn", "des", "Him\u00b7mels", "Schlu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ADV", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So wird zur Freyheit dir dein und der Dienste Joch.", "tokens": ["So", "wird", "zur", "Frey\u00b7heit", "dir", "dein", "und", "der", "Diens\u00b7te", "Joch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "PPER", "PPOSAT", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So scheint der Seelen Strahl durch diesen Staub und Koth.", "tokens": ["So", "scheint", "der", "See\u00b7len", "Strahl", "durch", "die\u00b7sen", "Staub", "und", "Koth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "APPR", "PDAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Verla\u00fcgnest du dich nicht, so bleibst und bistu Koth.", "tokens": ["Ver\u00b7la\u00fcg\u00b7nest", "du", "dich", "nicht", ",", "so", "bleibst", "und", "bis\u00b7tu", "Koth", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "$,", "ADV", "VVFIN", "KON", "NE", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bl\u00e4sest nichts von dir als Hoffarths volles Gifft.", "tokens": ["Und", "bl\u00e4\u00b7sest", "nichts", "von", "dir", "als", "Hof\u00b7farths", "vol\u00b7les", "Gifft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "APPR", "PPER", "KOUS", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du und dein Thun vergehst, und stirbest mit der Zeit.", "tokens": ["Du", "und", "dein", "Thun", "ver\u00b7gehst", ",", "und", "stir\u00b7best", "mit", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPOSAT", "NN", "VVFIN", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und was dich nur verdammt, du liebest deinen Wahn.", "tokens": ["Und", "was", "dich", "nur", "ver\u00b7dammt", ",", "du", "lie\u00b7best", "dei\u00b7nen", "Wahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVPP", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das Fehl zeucht Ohren ab dir der Noth feste Schlu\u00df,", "tokens": ["Das", "Fehl", "zeucht", "Oh\u00b7ren", "ab", "dir", "der", "Noth", "fes\u00b7te", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "APPR", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Bi\u00df endlich dich erw\u00fcrgt das angelegte Joch.", "tokens": ["Bi\u00df", "end\u00b7lich", "dich", "er\u00b7w\u00fcrgt", "das", "an\u00b7ge\u00b7leg\u00b7te", "Joch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "O Mensch! so schau und sprich: Ich bin nur Staub und Koth,", "tokens": ["O", "Mensch", "!", "so", "schau", "und", "sprich", ":", "Ich", "bin", "nur", "Staub", "und", "Koth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ADV", "ADJD", "KON", "ADJD", "$.", "PPER", "VAFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und iede S\u00fcnde flieh ich s\u00f6rglicher als Gifft!", "tokens": ["Und", "ie\u00b7de", "S\u00fcn\u00b7de", "flieh", "ich", "s\u00f6rg\u00b7li\u00b7cher", "als", "Gifft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So bist du au\u00dfer Welt und stehst auch \u00fcber Zeit.", "tokens": ["So", "bist", "du", "au\u00b7\u00dfer", "Welt", "und", "stehst", "auch", "\u00fc\u00b7ber", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "KON", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}