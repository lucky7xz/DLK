{"textgrid.poem.38535": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Maria", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Lindenschatten duftet ein Rasengrab,", "tokens": ["Im", "Lin\u00b7den\u00b7schat\u00b7ten", "duf\u00b7tet", "ein", "Ra\u00b7sen\u00b7grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von Tausendsch\u00f6n und Thimian rings umbl\u00fcht;", "tokens": ["Von", "Tau\u00b7send\u00b7sch\u00f6n", "und", "Thi\u00b7mi\u00b7an", "rings", "um\u00b7bl\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NE", "ADV", "VVPP", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Ein schwarzes Kreuz, das einsam aufragt,", "tokens": ["Ein", "schwar\u00b7zes", "Kreuz", ",", "das", "ein\u00b7sam", "auf\u00b7ragt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "K\u00fcndet dem Waller, hier ruh' ein Todter!", "tokens": ["K\u00fcn\u00b7det", "dem", "Wal\u00b7ler", ",", "hier", "ruh'", "ein", "Tod\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Voll Wehmuth klopfet schneller mein Herzensschlag!", "tokens": ["Voll", "Weh\u00b7muth", "klop\u00b7fet", "schnel\u00b7ler", "mein", "Her\u00b7zens\u00b7schlag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Mitleids Z\u00e4hre gleitet die Wang' herab:", "tokens": ["Des", "Mit\u00b7leids", "Z\u00e4h\u00b7re", "glei\u00b7tet", "die", "Wang'", "her\u00b7ab", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Denn hier, an dieser ernsten St\u00e4te,", "tokens": ["Denn", "hier", ",", "an", "die\u00b7ser", "erns\u00b7ten", "St\u00e4\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schlummert Maria den Todesschlummer!", "tokens": ["Schlum\u00b7mert", "Ma\u00b7ria", "den", "To\u00b7des\u00b7schlum\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Im reinen Herzen jeglicher Tugend Keim,", "tokens": ["Im", "rei\u00b7nen", "Her\u00b7zen", "jeg\u00b7li\u00b7cher", "Tu\u00b7gend", "Keim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Um ihre Stirn den himlischen Unschuldskranz,", "tokens": ["Um", "ih\u00b7re", "Stirn", "den", "him\u00b7li\u00b7schen", "Un\u00b7schulds\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Von Engelbr\u00fcdern ihr gewunden,", "tokens": ["Von", "En\u00b7gel\u00b7br\u00fc\u00b7dern", "ihr", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lebte sie Tage voll Seligkeiten.", "tokens": ["Leb\u00b7te", "sie", "Ta\u00b7ge", "voll", "Se\u00b7lig\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADJD", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Da kam ein schn\u00f6der, heuchelnder B\u00f6sewicht,", "tokens": ["Da", "kam", "ein", "schn\u00f6\u00b7der", ",", "heu\u00b7cheln\u00b7der", "B\u00f6\u00b7se\u00b7wicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die H\u00f6ll' im Busen, raubte den Unschuldskranz! \u2013", "tokens": ["Die", "H\u00f6ll'", "im", "Bu\u00b7sen", ",", "raub\u00b7te", "den", "Un\u00b7schulds\u00b7kranz", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie siechte hin, in ihrer Bl\u00fcte,", "tokens": ["Sie", "siech\u00b7te", "hin", ",", "in", "ih\u00b7rer", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Einst der geliebteste Stolz der M\u00e4dchen!", "tokens": ["Einst", "der", "ge\u00b7lieb\u00b7tes\u00b7te", "Stolz", "der", "M\u00e4d\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "S\u00fc\u00df sey dein Schlummer, unter der Blumengruft,", "tokens": ["S\u00fc\u00df", "sey", "dein", "Schlum\u00b7mer", ",", "un\u00b7ter", "der", "Blu\u00b7men\u00b7gruft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du sch\u00f6ne Seele! bis zum Erwachen s\u00fc\u00df!", "tokens": ["Du", "sch\u00f6\u00b7ne", "See\u00b7le", "!", "bis", "zum", "Er\u00b7wa\u00b7chen", "s\u00fc\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "APPR", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Da du, in hellverkl\u00e4rtem Glanze,", "tokens": ["Da", "du", ",", "in", "hell\u00b7ver\u00b7kl\u00e4r\u00b7tem", "Glan\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nimmer zu welken, in Eden aufbl\u00fchst!", "tokens": ["Nim\u00b7mer", "zu", "wel\u00b7ken", ",", "in", "E\u00b7den", "auf\u00b7bl\u00fchst", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Zu deinem H\u00fcgel wandelt die Weinerin,", "tokens": ["Zu", "dei\u00b7nem", "H\u00fc\u00b7gel", "wan\u00b7delt", "die", "Wei\u00b7ne\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bricht Todtenblumen, wendet den Z\u00e4hrenblik,", "tokens": ["Bricht", "Tod\u00b7ten\u00b7blu\u00b7men", ",", "wen\u00b7det", "den", "Z\u00e4h\u00b7ren\u00b7blik", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und Auferstehungsschauer beben", "tokens": ["Und", "Auf\u00b7er\u00b7ste\u00b7hungs\u00b7schau\u00b7er", "be\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch die ersch\u00fctterte M\u00e4dchenseele!", "tokens": ["Durch", "die", "er\u00b7sch\u00fct\u00b7ter\u00b7te", "M\u00e4d\u00b7chen\u00b7see\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}