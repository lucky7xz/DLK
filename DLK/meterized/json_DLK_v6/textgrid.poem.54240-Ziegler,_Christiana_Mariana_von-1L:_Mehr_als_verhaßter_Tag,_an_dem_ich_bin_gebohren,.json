{"textgrid.poem.54240": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mehr als verha\u00dfter Tag, an dem ich bin gebohren,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mehr als verha\u00dfter Tag, an dem ich bin gebohren,", "tokens": ["Mehr", "als", "ver\u00b7ha\u00df\u00b7ter", "Tag", ",", "an", "dem", "ich", "bin", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Schmertz, dem in der Welt nichts zu vergleichen ist!", "tokens": ["O", "Schmertz", ",", "dem", "in", "der", "Welt", "nichts", "zu", "ver\u00b7glei\u00b7chen", "ist", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "APPR", "ART", "NN", "PIS", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein liebster Seladon geht leider! nun verlohren,", "tokens": ["Mein", "liebs\u00b7ter", "Se\u00b7la\u00b7don", "geht", "lei\u00b7der", "!", "nun", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "$.", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den meine Seele sich zur Nahrung auserkiest.", "tokens": ["Den", "mei\u00b7ne", "See\u00b7le", "sich", "zur", "Nah\u00b7rung", "aus\u00b7er\u00b7kiest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ists m\u00f6glich? kanst du mich mein andres Ich verlassen.", "tokens": ["Ists", "m\u00f6g\u00b7lich", "?", "kanst", "du", "mich", "mein", "and\u00b7res", "Ich", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "VMFIN", "PPER", "PRF", "PPOSAT", "ADJA", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ergie\u00dft, ihr Augen, euch in eine Thr\u00e4nen-See.", "tokens": ["Er\u00b7gie\u00dft", ",", "ihr", "Au\u00b7gen", ",", "euch", "in", "ei\u00b7ne", "Thr\u00e4\u00b7nen\u00b7See", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "F\u00e4ngt Stern und Schicksal mich auf einmahl an zu hassen?", "tokens": ["F\u00e4ngt", "Stern", "und", "Schick\u00b7sal", "mich", "auf", "ein\u00b7mahl", "an", "zu", "has\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PPER", "APPR", "ADV", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ach Sch\u00f6nster! deine Flucht gebiehrt mir eitel Weh.", "tokens": ["Ach", "Sch\u00f6ns\u00b7ter", "!", "dei\u00b7ne", "Flucht", "ge\u00b7biehrt", "mir", "ei\u00b7tel", "Weh", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das Blut in Adern wallt, der Puls h\u00f6rt auf zu schlagen,", "tokens": ["Das", "Blut", "in", "A\u00b7dern", "wallt", ",", "der", "Puls", "h\u00f6rt", "auf", "zu", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich bin schon halb entseelt, mein blasses Angesicht", "tokens": ["Ich", "bin", "schon", "halb", "ent\u00b7seelt", ",", "mein", "blas\u00b7ses", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Kan unsrer Welt vielleicht mehr als ich selber sagen,", "tokens": ["Kan", "uns\u00b7rer", "Welt", "viel\u00b7leicht", "mehr", "als", "ich", "sel\u00b7ber", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "ADV", "KOUS", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil Mund und Feder mehr als halb gebrochen spricht.", "tokens": ["Weil", "Mund", "und", "Fe\u00b7der", "mehr", "als", "halb", "ge\u00b7bro\u00b7chen", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PIAT", "KOKOM", "ADJD", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dein Sterben r\u00fchret mir das innerste der Seelen,", "tokens": ["Dein", "Ster\u00b7ben", "r\u00fch\u00b7ret", "mir", "das", "in\u00b7ners\u00b7te", "der", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Dein j\u00e4her Scheide-Brief durchbohret Hertz und Brust.", "tokens": ["Dein", "j\u00e4\u00b7her", "Schei\u00b7de\u00b7Brief", "durch\u00b7boh\u00b7ret", "Hertz", "und", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich kan mein Leiden wohl erwehnen, nicht erzehlen,", "tokens": ["Ich", "kan", "mein", "Lei\u00b7den", "wohl", "er\u00b7weh\u00b7nen", ",", "nicht", "er\u00b7zeh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "ADV", "VVINF", "$,", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Dem Himmel ist mein Schmertz und meine Noth bewu\u00dft.", "tokens": ["Dem", "Him\u00b7mel", "ist", "mein", "Schmertz", "und", "mei\u00b7ne", "Noth", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Dein Umgang hiese mir ein Himmel hier auf Erden,", "tokens": ["Dein", "Um\u00b7gang", "hie\u00b7se", "mir", "ein", "Him\u00b7mel", "hier", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dein holdes Augenpaar, das nur geweyhet hie\u00df", "tokens": ["Dein", "hol\u00b7des", "Au\u00b7gen\u00b7paar", ",", "das", "nur", "ge\u00b7wey\u00b7het", "hie\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "ADV", "VVPP", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und dem an Anmuth nichts kan fast vergliechen werden,", "tokens": ["Und", "dem", "an", "An\u00b7muth", "nichts", "kan", "fast", "ver\u00b7glie\u00b7chen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "NN", "PIS", "VMFIN", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "War mir ein Lust-Revir und irdsches Paradie\u00df.", "tokens": ["War", "mir", "ein", "Lust\u00b7Re\u00b7vir", "und", "ird\u00b7sches", "Pa\u00b7ra\u00b7die\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Dein s\u00fc\u00df ambrirter Mund erquickte Geist und Hertze,", "tokens": ["Dein", "s\u00fc\u00df", "am\u00b7br\u00b7ir\u00b7ter", "Mund", "er\u00b7quick\u00b7te", "Geist", "und", "Hert\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "ADJA", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Dein Arm hie\u00df meinen Leib die sch\u00f6nste Ruhestatt.", "tokens": ["Dein", "Arm", "hie\u00df", "mei\u00b7nen", "Leib", "die", "sch\u00f6ns\u00b7te", "Ru\u00b7he\u00b7statt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Befand ich mich bey dir, so wust ich nichts von Schmertze,", "tokens": ["Be\u00b7fand", "ich", "mich", "bey", "dir", ",", "so", "wust", "ich", "nichts", "von", "Schmert\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPER", "$,", "ADV", "VVFIN", "PPER", "PIS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ich wurde deiner Huld und Liebe niemahls satt.", "tokens": ["Ich", "wur\u00b7de", "dei\u00b7ner", "Huld", "und", "Lie\u00b7be", "nie\u00b7mahls", "satt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "KON", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Dein und mein Hertze war in eine Form gegossen,", "tokens": ["Dein", "und", "mein", "Hert\u00b7ze", "war", "in", "ei\u00b7ne", "Form", "ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "KON", "PPOSAT", "VVFIN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Dein und mein Wille hie\u00df ein gleich gestimmter Chor.", "tokens": ["Dein", "und", "mein", "Wil\u00b7le", "hie\u00df", "ein", "gleich", "ge\u00b7stimm\u00b7ter", "Chor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "KON", "PPOSAT", "NN", "VVFIN", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Was hab ich nicht bey dir vor Zucker-Lust genossen,", "tokens": ["Was", "hab", "ich", "nicht", "bey", "dir", "vor", "Zu\u00b7cke\u00b7rLust", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PTKNEG", "APPR", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ich stelle sie mir noch durch Angedencken vor.", "tokens": ["Ich", "stel\u00b7le", "sie", "mir", "noch", "durch", "An\u00b7ge\u00b7den\u00b7cken", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Jedoch um alles di\u00df bin ich nunmehr gekommen,", "tokens": ["Je\u00b7doch", "um", "al\u00b7les", "di\u00df", "bin", "ich", "nun\u00b7mehr", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIS", "PDS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Mein Freuden-Stern verkehrt sich in Cometen Schein,", "tokens": ["Mein", "Freu\u00b7den\u00b7S\u00b7tern", "ver\u00b7kehrt", "sich", "in", "Co\u00b7me\u00b7ten", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+---+-+++-+", "measure": "dactylic.init"}, "line.31": {"text": "Der Schmuck wird meinen Haupt auf einmahl abgenommen,", "tokens": ["Der", "Schmuck", "wird", "mei\u00b7nen", "Haupt", "auf", "ein\u00b7mahl", "ab\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "APPR", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ich soll durch Sturm und Wind nunmehr entbl\u00e4ttert seyn.", "tokens": ["Ich", "soll", "durch", "Sturm", "und", "Wind", "nun\u00b7mehr", "ent\u00b7bl\u00e4t\u00b7tert", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "KON", "NN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ihr Sternen! sucht ihr denn mein g\u00e4ntzliches Verderben?", "tokens": ["Ihr", "Ster\u00b7nen", "!", "sucht", "ihr", "denn", "mein", "g\u00e4ntz\u00b7li\u00b7ches", "Ver\u00b7der\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ja sucht es immerhin, ich bin darzu bereit.", "tokens": ["Ja", "sucht", "es", "im\u00b7mer\u00b7hin", ",", "ich", "bin", "dar\u00b7zu", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ihr seht ja, wie sich Mund und Wangen schon verf\u00e4rben,", "tokens": ["Ihr", "seht", "ja", ",", "wie", "sich", "Mund", "und", "Wan\u00b7gen", "schon", "ver\u00b7f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "PRF", "NN", "KON", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und wie der Harm und Gram mir als ein M\u00f6rder dr\u00e4ut.", "tokens": ["Und", "wie", "der", "Harm", "und", "Gram", "mir", "als", "ein", "M\u00f6r\u00b7der", "dr\u00e4ut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "KON", "NN", "PPER", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ach la\u00df mich Seladon dich bald von neuen k\u00fcssen,", "tokens": ["Ach", "la\u00df", "mich", "Se\u00b7la\u00b7don", "dich", "bald", "von", "neu\u00b7en", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVIMP", "PPER", "NN", "PPER", "ADV", "APPR", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Eh vollends F\u00e4ul und Wurm der Lippen-Pracht zerfleischt.", "tokens": ["Eh", "vol\u00b7lends", "F\u00e4ul", "und", "Wurm", "der", "Lip\u00b7pen\u00b7Pracht", "zer\u00b7fleischt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "KON", "NN", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.39": {"text": "Ich kan und mag mich nicht von dir entfernet wissen,", "tokens": ["Ich", "kan", "und", "mag", "mich", "nicht", "von", "dir", "ent\u00b7fer\u00b7net", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "PPER", "PTKNEG", "APPR", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Weil Lieb und Sehnsucht nun von mir dergleichen heischt.", "tokens": ["Weil", "Lieb", "und", "Sehn\u00b7sucht", "nun", "von", "mir", "derg\u00b7lei\u00b7chen", "heischt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ADV", "APPR", "PPER", "PIS", "VVFIN", 