{"textgrid.poem.54413": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "24. Gedichte", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In deinem sch\u00f6nen Werk, sieht man erstaunend an,", "tokens": ["In", "dei\u00b7nem", "sch\u00f6\u00b7nen", "Werk", ",", "sieht", "man", "er\u00b7stau\u00b7nend", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "PIS", "VVPP", "PTKVZ", "$,"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Was Nachsinn, und Verstand, was Kunst und Flei\u00df gethan.", "tokens": ["Was", "Nach\u00b7sinn", ",", "und", "Ver\u00b7stand", ",", "was", "Kunst", "und", "Flei\u00df", "ge\u00b7than", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "KON", "NN", "$,", "PWS", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Br\u00fccken Bau und Ri\u00df, zeigt nicht nur Wunderwerke,", "tokens": ["Der", "Br\u00fc\u00b7cken", "Bau", "und", "Ri\u00df", ",", "zeigt", "nicht", "nur", "Wun\u00b7der\u00b7wer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,", "VVFIN", "PTKNEG", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Man sp\u00fcrt auf jedem Blat der Wissenschaften St\u00e4rke.", "tokens": ["Man", "sp\u00fcrt", "auf", "je\u00b7dem", "Blat", "der", "Wis\u00b7sen\u00b7schaf\u00b7ten", "St\u00e4r\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Lehrer, und auch der, so ihm zu F\u00fcssen sitzt,", "tokens": ["Der", "Leh\u00b7rer", ",", "und", "auch", "der", ",", "so", "ihm", "zu", "F\u00fcs\u00b7sen", "sitzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ADV", "ART", "$,", "ADV", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bewundert Satz vor Satz, und wozu jeder n\u00fctzt.", "tokens": ["Be\u00b7wun\u00b7dert", "Satz", "vor", "Satz", ",", "und", "wo\u00b7zu", "je\u00b7der", "n\u00fctzt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "$,", "KON", "PWAV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Kurz, die gelehrte Welt nennt es ein Meisterst\u00fccke,", "tokens": ["Kurz", ",", "die", "ge\u00b7lehr\u00b7te", "Welt", "nennt", "es", "ein", "Meis\u00b7ter\u00b7st\u00fc\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und denkt nach sp\u00e4ter Zeit an dich mit Lust zur\u00fccke?", "tokens": ["Und", "denkt", "nach", "sp\u00e4\u00b7ter", "Zeit", "an", "dich", "mit", "Lust", "zu\u00b7r\u00fc\u00b7cke", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "APPR", "PRF", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Ewigkeit schreibt selbst, Freund, deinen Namen auf.", "tokens": ["Die", "E\u00b7wig\u00b7keit", "schreibt", "selbst", ",", "Freund", ",", "dei\u00b7nen", "Na\u00b7men", "auf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "NN", "$,", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-++-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Dein Wissen reizet sie; Monarchen merken drauf.", "tokens": ["Dein", "Wis\u00b7sen", "rei\u00b7zet", "sie", ";", "Mon\u00b7ar\u00b7chen", "mer\u00b7ken", "drauf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$.", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dein K\u00f6nig gab Befehl, was kann man anders lesen,", "tokens": ["Dein", "K\u00f6\u00b7nig", "gab", "Be\u00b7fehl", ",", "was", "kann", "man", "an\u00b7ders", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$,", "PWS", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Als ein vollkomnes Buch nach seinem innern Wesen?", "tokens": ["Als", "ein", "voll\u00b7kom\u00b7nes", "Buch", "nach", "sei\u00b7nem", "in\u00b7nern", "We\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}