{"dta.poem.11748": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "34.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sprach: \u201eDer Liebe Rausch verstehn nur trunkne Sinne;\u201c", "tokens": ["Ich", "sprach", ":", "\u201e", "Der", "Lie\u00b7be", "Rausch", "ver\u00b7stehn", "nur", "trunk\u00b7ne", "Sin\u00b7ne", ";", "\u201c"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ART", "NN", "NN", "VVFIN", "ADV", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und da\u00df ich recht es sprach, werd' ich mit Freuden inne.", "tokens": ["Und", "da\u00df", "ich", "recht", "es", "sprach", ",", "werd'", "ich", "mit", "Freu\u00b7den", "in\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich freu' mich, da\u00df mich nicht die N\u00fcchternen verstehn,", "tokens": ["Ich", "freu'", "mich", ",", "da\u00df", "mich", "nicht", "die", "N\u00fcch\u00b7ter\u00b7nen", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nur die Trunknen sich mit mir im Reigen drehn.", "tokens": ["Und", "nur", "die", "Trunk\u00b7nen", "sich", "mit", "mir", "im", "Rei\u00b7gen", "dreh