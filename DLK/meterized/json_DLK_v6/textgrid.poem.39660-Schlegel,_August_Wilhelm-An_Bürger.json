{"textgrid.poem.39660": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An B\u00fcrger", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es t\u00f6nten die ersten zerstreuten Kl\u00e4nge", "tokens": ["Es", "t\u00f6n\u00b7ten", "die", "ers\u00b7ten", "zer\u00b7streu\u00b7ten", "Kl\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Des g\u00f6ttlichsten der Liebesges\u00e4nge", "tokens": ["Des", "g\u00f6tt\u00b7lichs\u00b7ten", "der", "Lie\u00b7bes\u00b7ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ART", "NN"], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Von deinem Munde mir in's Ohr", "tokens": ["Von", "dei\u00b7nem", "Mun\u00b7de", "mir", "in's", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da lauscht' ich, wie an Aganippen", "tokens": ["Da", "lauscht'", "ich", ",", "wie", "an", "A\u00b7gan\u00b7ip\u00b7pen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Auf jeden Hauch von Ph\u00f6bus Lippen", "tokens": ["Auf", "je\u00b7den", "Hauch", "von", "Ph\u00f6\u00b7bus", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der hohen Pierinnen Chor.", "tokens": ["Der", "ho\u00b7hen", "Pie\u00b7rin\u00b7nen", "Chor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Nun aber sollen, ihr ewigen G\u00f6tter!", "tokens": ["Nun", "a\u00b7ber", "sol\u00b7len", ",", "ihr", "e\u00b7wi\u00b7gen", "G\u00f6t\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "All' diese T\u00f6ne, wie die Bl\u00e4tter", "tokens": ["All'", "die\u00b7se", "T\u00f6\u00b7ne", ",", "wie", "die", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "PDAT", "NN", "$,", "PWAV", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Der hochbegeisterten Jungfrau, verwehn?", "tokens": ["Der", "hoch\u00b7be\u00b7geis\u00b7ter\u00b7ten", "Jung\u00b7frau", ",", "ver\u00b7wehn", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nein! sammle deines Geistes St\u00e4rke,", "tokens": ["Nein", "!", "samm\u00b7le", "dei\u00b7nes", "Geis\u00b7tes", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Um \u00fcber deiner Jugend Werke", "tokens": ["Um", "\u00fc\u00b7ber", "dei\u00b7ner", "Ju\u00b7gend", "Wer\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hellgl\u00e4nzend dieses zu erh\u00f6hn!", "tokens": ["Hell\u00b7gl\u00e4n\u00b7zend", "die\u00b7ses", "zu", "er\u00b7h\u00f6hn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O k\u00f6nnt' ich schweben auf Adlersschwingen!", "tokens": ["O", "k\u00f6nnt'", "ich", "schwe\u00b7ben", "auf", "Ad\u00b7lers\u00b7schwin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich wollte zum hohen Olympus dringen,", "tokens": ["Ich", "woll\u00b7te", "zum", "ho\u00b7hen", "O\u00b7lym\u00b7pus", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "ADJA", "NE", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und Nektar dort entwenden f\u00fcr dich;", "tokens": ["Und", "Nek\u00b7tar", "dort", "ent\u00b7wen\u00b7den", "f\u00fcr", "dich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ich wollte dich mit Nektar tr\u00e4nken,", "tokens": ["Ich", "woll\u00b7te", "dich", "mit", "Nek\u00b7tar", "tr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und Kraft in deine Adern senken,", "tokens": ["Und", "Kraft", "in", "dei\u00b7ne", "A\u00b7dern", "sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bis diese matte K\u00e4lte wich.", "tokens": ["Bis", "die\u00b7se", "mat\u00b7te", "K\u00e4l\u00b7te", "wich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch mir ward nichts als diese Leier,", "tokens": ["Doch", "mir", "ward", "nichts", "als", "die\u00b7se", "Lei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIS", "KOKOM", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und in der Brust ein unsterbliches Feuer,", "tokens": ["Und", "in", "der", "Brust", "ein", "uns\u00b7terb\u00b7li\u00b7ches", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das hei\u00df f\u00fcr alles Sch\u00f6ne gl\u00fcht.", "tokens": ["Das", "hei\u00df", "f\u00fcr", "al\u00b7les", "Sch\u00f6\u00b7ne", "gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Leier hab' ich dir gespielet,", "tokens": ["Die", "Lei\u00b7er", "hab'", "ich", "dir", "ge\u00b7spie\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und habe bald mich stolz gef\u00fchlet;", "tokens": ["Und", "ha\u00b7be", "bald", "mich", "stolz", "ge\u00b7f\u00fch\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Denn dir gefiel mein sch\u00fcchtern Lied.", "tokens": ["Denn", "dir", "ge\u00b7fiel", "mein", "sch\u00fcch\u00b7tern", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sag', kann ein Ton von mir dich freuen?", "tokens": ["Sag'", ",", "kann", "ein", "Ton", "von", "mir", "dich", "freu\u00b7en", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "ART", "NN", "APPR", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So will ich alle Ges\u00e4nge dir weihen,", "tokens": ["So", "will", "ich", "al\u00b7le", "Ge\u00b7s\u00e4n\u00b7ge", "dir", "wei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die meiner Leier verliehen sind.", "tokens": ["Die", "mei\u00b7ner", "Lei\u00b7er", "ver\u00b7lie\u00b7hen", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich will, gelagert unter Rosen,", "tokens": ["Ich", "will", ",", "ge\u00b7la\u00b7gert", "un\u00b7ter", "Ro\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Von holden Abenteuern kosen,", "tokens": ["Von", "hol\u00b7den", "A\u00b7bent\u00b7eu\u00b7ern", "ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und preisen Venus goldenes Kind.", "tokens": ["Und", "prei\u00b7sen", "Ve\u00b7nus", "gol\u00b7de\u00b7nes", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Und sind mir dann alle die Weisen gelungen,", "tokens": ["Und", "sind", "mir", "dann", "al\u00b7le", "die", "Wei\u00b7sen", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PIS", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und haben die Saiten dann ausgeklungen,", "tokens": ["Und", "ha\u00b7ben", "die", "Sai\u00b7ten", "dann", "aus\u00b7ge\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und ist mein K\u00f6cher ganz geleert:", "tokens": ["Und", "ist", "mein", "K\u00f6\u00b7cher", "ganz", "ge\u00b7leert", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann mu\u00dft auch du mit Orpheus-H\u00e4nden", "tokens": ["Dann", "mu\u00dft", "auch", "du", "mit", "Or\u00b7pheus\u00b7H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Saiten schlagen, das vollenden,", "tokens": ["Die", "Sai\u00b7ten", "schla\u00b7gen", ",", "das", "voll\u00b7en\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wornach mein Sinn und Herz begehrt.", "tokens": ["Wor\u00b7nach", "mein", "Sinn", "und", "Herz", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Dann werd' ich sinnend, voll Entz\u00fccken,", "tokens": ["Dann", "werd'", "ich", "sin\u00b7nend", ",", "voll", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach deiner Dichterstirne blicken,", "tokens": ["Nach", "dei\u00b7ner", "Dich\u00b7ter\u00b7stir\u00b7ne", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und jeden Nachhall in mich ziehn.", "tokens": ["Und", "je\u00b7den", "Nach\u00b7hall", "in", "mich", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zerschmettern werd' ich meine Leier:", "tokens": ["Zer\u00b7schmet\u00b7tern", "werd'", "ich", "mei\u00b7ne", "Lei\u00b7er", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch meines Busens starkes Feuer", "tokens": ["Doch", "mei\u00b7nes", "Bu\u00b7sens", "star\u00b7kes", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Soll, weil ich lebe, nie vergl\u00fchn!", "tokens": ["Soll", ",", "weil", "ich", "le\u00b7be", ",", "nie", "ver\u00b7gl\u00fchn", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}