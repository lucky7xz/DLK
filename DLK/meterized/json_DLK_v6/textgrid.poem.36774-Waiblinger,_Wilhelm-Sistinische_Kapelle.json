{"textgrid.poem.36774": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Sistinische Kapelle", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun so seh' ich doch endlich einmal die ber\u00fchmte Kapelle,", "tokens": ["Nun", "so", "seh'", "ich", "doch", "end\u00b7lich", "ein\u00b7mal", "die", "be\u00b7r\u00fchm\u00b7te", "Ka\u00b7pel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Aber das n\u00e4rrische Zeug, dort an der Decke, was ist's?", "tokens": ["A\u00b7ber", "das", "n\u00e4r\u00b7ri\u00b7sche", "Zeug", ",", "dort", "an", "der", "De\u00b7cke", ",", "was", "ist's", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,", "PWS", "VAFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "\u00bbwie? ich verstehe Sie nicht, das sind Angelo's gro\u00dfe Propheten!\u00ab \u2013", "tokens": ["\u00bb", "wie", "?", "ich", "ver\u00b7ste\u00b7he", "Sie", "nicht", ",", "das", "sind", "An\u00b7ge\u00b7lo's", "gro\u00b7\u00dfe", "Pro\u00b7phe\u00b7ten", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PWAV", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PDS", "VAFIN", "NE", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Spa\u00df bei Seite, mein Freund, w\u00e4r' das der Michel im Ernst?", "tokens": ["Spa\u00df", "bei", "Sei\u00b7te", ",", "mein", "Freund", ",", "w\u00e4r'", "das", "der", "Mi\u00b7chel", "im", "Ernst", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "PPOSAT", "NN", "$,", "VAFIN", "PDS", "ART", "NE", "APPRART", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}}}}}