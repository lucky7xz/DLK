{"textgrid.poem.57632": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Sommernacht", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht ist stumm und d\u00fcster,", "tokens": ["Die", "Nacht", "ist", "stumm", "und", "d\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Mond gibt geizigen Schein,", "tokens": ["Der", "Mond", "gibt", "gei\u00b7zi\u00b7gen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Des Rohres leises Gefl\u00fcster", "tokens": ["Des", "Roh\u00b7res", "lei\u00b7ses", "Ge\u00b7fl\u00fcs\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Rauscht in den Park hinein.", "tokens": ["Rauscht", "in", "den", "Park", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "In meinem Herzen ist's dunkel,", "tokens": ["In", "mei\u00b7nem", "Her\u00b7zen", "ist's", "dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Kein karger, geiziger Schein,", "tokens": ["Kein", "kar\u00b7ger", ",", "gei\u00b7zi\u00b7ger", "Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Des Mondes kaltes Gefunkel", "tokens": ["Des", "Mon\u00b7des", "kal\u00b7tes", "Ge\u00b7fun\u00b7kel"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dringt nicht in das Herz hinein.", "tokens": ["Dringt", "nicht", "in", "das", "Herz", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Viel duftende Blumen bl\u00fchen \u2013", "tokens": ["Viel", "duf\u00b7ten\u00b7de", "Blu\u00b7men", "bl\u00fc\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Es jubelt die Nachtigall,", "tokens": ["Es", "ju\u00b7belt", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich m\u00f6chte dem Jubel entfliehen \u2013", "tokens": ["Ich", "m\u00f6ch\u00b7te", "dem", "Ju\u00b7bel", "ent\u00b7flie\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Doch h\u00f6r' ich ihn \u00fcberall.", "tokens": ["Doch", "h\u00f6r'", "ich", "ihn", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "H\u00f6r' auf mit deinem Balzen \u2013", "tokens": ["H\u00f6r'", "auf", "mit", "dei\u00b7nem", "Bal\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich schmerzt der jubelnde Schall \u2013", "tokens": ["Mich", "schmerzt", "der", "ju\u00b7beln\u00b7de", "Schall", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Das Sprudeln, Kollern und Balzen,", "tokens": ["Das", "Spru\u00b7deln", ",", "Kol\u00b7lern", "und", "Bal\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Du dumme Nachtigall!", "tokens": ["Du", "dum\u00b7me", "Nach\u00b7ti\u00b7gall", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}