{"textgrid.poem.46315": {"metadata": {"author": {"name": "Spee, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[so nur mein Heyland steht bey mir]", "genre": "verse", "period": "N.A.", "pub_year": 1613, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So nur mein Heyland steht bey mir,", "tokens": ["So", "nur", "mein", "Hey\u00b7land", "steht", "bey", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00df mirs doch all gelingen:", "tokens": ["Mu\u00df", "mirs", "doch", "all", "ge\u00b7lin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "ADV", "PIAT", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Drumb will ich, \u00f4 mein Gott, mit dir", "tokens": ["Drumb", "will", "ich", ",", "\u00f4", "mein", "Gott", ",", "mit", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "$,", "FM", "PPOSAT", "NN", "$,", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch vber Mauren springen.", "tokens": ["Auch", "vber", "Mau\u00b7ren", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "vnd widerumb im 124 psalm:", "tokens": ["vnd", "wi\u00b7de\u00b7rumb", "im", "124", "psalm", ":"], "token_info": ["word", "word", "word", "number", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "CARD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die sich auff Gott so gantz vnd gar", "tokens": ["Die", "sich", "auff", "Gott", "so", "gantz", "vnd", "gar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "NN", "ADV", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit fester hoffnung legen,", "tokens": ["Mit", "fes\u00b7ter", "hoff\u00b7nung", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die bleiben st\u00e4ts vnwandelbar,", "tokens": ["Die", "blei\u00b7ben", "st\u00e4ts", "vn\u00b7wan\u00b7del\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Vnd lahn sich nie bewegen.", "tokens": ["Vnd", "lahn", "sich", "nie", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Vnd widerumb im 17 psalm:", "tokens": ["Vnd", "wi\u00b7de\u00b7rumb", "im", "17", "psalm", ":"], "token_info": ["word", "word", "word", "number", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "CARD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Die wider mich auff leinen sich,", "tokens": ["Die", "wi\u00b7der", "mich", "auff", "lei\u00b7nen", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "APPR", "ADJA", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Da\u00df sie mein Seel ermorden,", "tokens": ["Da\u00df", "sie", "mein", "Seel", "er\u00b7mor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Hastu geworffen vnter mich,", "tokens": ["Has\u00b7tu", "ge\u00b7worf\u00b7fen", "vn\u00b7ter", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "APPR", "PPER", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.14": {"text": "Feldfl\u00fcchtig seind sie worden.", "tokens": ["Feld\u00b7fl\u00fcch\u00b7tig", "seind", "sie", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "VAPP", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Es ist ein w\u00fcrckung deiner macht,", "tokens": ["Es", "ist", "ein", "w\u00fcr\u00b7ckung", "dei\u00b7ner", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Da\u00df sie verlohren gaben;", "tokens": ["Da\u00df", "sie", "ver\u00b7loh\u00b7ren", "ga\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Vmbs leben seind von dir gebracht,", "tokens": ["Vmbs", "le\u00b7ben", "seind", "von", "dir", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVINF", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Die mich beneidet haben.", "tokens": ["Die", "mich", "be\u00b7nei\u00b7det", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Gott rettet mich von feinden mein,", "tokens": ["Gott", "ret\u00b7tet", "mich", "von", "fein\u00b7den", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "ADJA", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Wan ich bin vberladen:", "tokens": ["Wan", "ich", "bin", "vber\u00b7la\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.21": {"text": "Wie sehr sie auch ergrimmet sein,", "tokens": ["Wie", "sehr", "sie", "auch", "er\u00b7grim\u00b7met", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Doch m\u00f6gens mir nicht schaden.", "tokens": ["Doch", "m\u00f6\u00b7gens", "mir", "nicht", "scha\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "vnd widerumb im 19 psalm.", "tokens": ["vnd", "wi\u00b7de\u00b7rumb", "im", "19", "psalm", "."], "token_info": ["word", "word", "word", "number", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "CARD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Wan schon der feind vermessentlich", "tokens": ["Wan", "schon", "der", "feind", "ver\u00b7mes\u00b7sent\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.25": {"text": "Auff seine ro\u00df vnd wagen,", "tokens": ["Auff", "sei\u00b7ne", "ro\u00df", "vnd", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "KON", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Vnd auff die st\u00e4rck verlasset sich,", "tokens": ["Vnd", "auff", "die", "st\u00e4rck", "ver\u00b7las\u00b7set", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Will ich doch nimmer zagen:", "tokens": ["Will", "ich", "doch", "nim\u00b7mer", "za\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Wan ich bin voller traurigkeit,", "tokens": ["Wan", "ich", "bin", "vol\u00b7ler", "trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Will ichs dem Herren klagen;", "tokens": ["Will", "ichs", "dem", "Her\u00b7ren", "kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Er kompt, er komt zu seiner zeit,", "tokens": ["Er", "kompt", ",", "er", "komt", "zu", "sei\u00b7ner", "zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Mags wol in warheit sagen.", "tokens": ["Mags", "wol", "in", "war\u00b7heit", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Vnd widerumb im 9 psalm:", "tokens": ["Vnd", "wi\u00b7de\u00b7rumb", "im", "9", "psalm", ":"], "token_info": ["word", "word", "word", "number", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "CARD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Es werden sich mit festem mut", "tokens": ["Es", "wer\u00b7den", "sich", "mit", "fes\u00b7tem", "mut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Auff dich, o Herr, verlassen,", "tokens": ["Auff", "dich", ",", "o", "Herr", ",", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Die deinen tewren namen gut", "tokens": ["Die", "dei\u00b7nen", "tew\u00b7ren", "na\u00b7men", "gut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Erkennen rechter massen;", "tokens": ["Er\u00b7ken\u00b7nen", "rech\u00b7ter", "mas\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Dieweil noch nie zu keiner frist,", "tokens": ["Die\u00b7weil", "noch", "nie", "zu", "kei\u00b7ner", "frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Wer sich auff dich geleinet,", "tokens": ["Wer", "sich", "auff", "dich", "ge\u00b7lei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.39": {"text": "Von dir zuletzt verlassen ist,", "tokens": ["Von", "dir", "zu\u00b7letzt", "ver\u00b7las\u00b7sen", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Wan ers mit trew gemeinet.", "tokens": ["Wan", "ers", "mit", "trew", "ge\u00b7mei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ADJD", "VVPP", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}