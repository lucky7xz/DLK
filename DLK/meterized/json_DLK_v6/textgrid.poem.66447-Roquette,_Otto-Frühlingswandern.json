{"textgrid.poem.66447": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingswandern", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Berg ergeht ein Rufen,", "tokens": ["Vom", "Berg", "er\u00b7geht", "ein", "Ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Antwort schallt im Thal,", "tokens": ["Und", "Ant\u00b7wort", "schallt", "im", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da springen von gr\u00fcnen Stufen", "tokens": ["Da", "sprin\u00b7gen", "von", "gr\u00fc\u00b7nen", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Quellen allzumal.", "tokens": ["Die", "Quel\u00b7len", "all\u00b7zu\u00b7mal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und Eines ruft's dem Andern,", "tokens": ["Und", "Ei\u00b7nes", "ruft's", "dem", "An\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das klinget fern und nah:", "tokens": ["Das", "klin\u00b7get", "fern", "und", "nah", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die rechte Zeit zum Wandern,", "tokens": ["Die", "rech\u00b7te", "Zeit", "zum", "Wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Fr\u00fchlingszeit ist da!", "tokens": ["Die", "Fr\u00fch\u00b7lings\u00b7zeit", "ist", "da", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O du holdselig Weben", "tokens": ["O", "du", "hold\u00b7se\u00b7lig", "We\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "ADJD", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "In Wald und Thal und H\u00f6hn!", "tokens": ["In", "Wald", "und", "Thal", "und", "H\u00f6hn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun athmet Alles Leben,", "tokens": ["Nun", "ath\u00b7met", "Al\u00b7les", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und findet's gut und sch\u00f6n.", "tokens": ["Und", "fin\u00b7det's", "gut", "und", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nun mit der Lerche steige,", "tokens": ["Nun", "mit", "der", "Ler\u00b7che", "stei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Mein Wandersang, empor,", "tokens": ["Mein", "Wan\u00b7der\u00b7sang", ",", "em\u00b7por", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und klinge laut, und zeige", "tokens": ["Und", "klin\u00b7ge", "laut", ",", "und", "zei\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "$,", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So frisch dich wie zuvor!", "tokens": ["So", "frisch", "dich", "wie", "zu\u00b7vor", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "KOKOM", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Durch all die Windeswellen,", "tokens": ["Durch", "all", "die", "Win\u00b7des\u00b7wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch all die Fr\u00fchlingszeit", "tokens": ["Durch", "all", "die", "Fr\u00fch\u00b7lings\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun wandern, wie die Quellen,", "tokens": ["Nun", "wan\u00b7dern", ",", "wie", "die", "Quel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Will ich mit Freudigkeit.", "tokens": ["Will", "ich", "mit", "Freu\u00b7dig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie jene rieselnd schweifen", "tokens": ["Wie", "je\u00b7ne", "rie\u00b7selnd", "schwei\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PDS", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durch Schlucht und Halden viel,", "tokens": ["Durch", "Schlucht", "und", "Hal\u00b7den", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Verirren sich und streifen,", "tokens": ["Ver\u00b7ir\u00b7ren", "sich", "und", "strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sie kommen doch an's Ziel.", "tokens": ["Sie", "kom\u00b7men", "doch", "an's", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "W\u00e4hlst du dir zum Begleiter", "tokens": ["W\u00e4hlst", "du", "dir", "zum", "Be\u00b7glei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "APPRART", "NN"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Den goldnen Lebensmuth,", "tokens": ["Den", "gold\u00b7nen", "Le\u00b7bens\u00b7muth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie findest du so heiter", "tokens": ["Wie", "fin\u00b7dest", "du", "so", "hei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Welt, wie sch\u00f6n und gut.", "tokens": ["Die", "Welt", ",", "wie", "sch\u00f6n", "und", "gut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wagst du k\u00fchn zu irren,", "tokens": ["Und", "wagst", "du", "k\u00fchn", "zu", "ir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So dr\u00fcckst du einst mit Lust,", "tokens": ["So", "dr\u00fcckst", "du", "einst", "mit", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mag auch der Weg sich wirren,", "tokens": ["Mag", "auch", "der", "Weg", "sich", "wir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Erf\u00fcllung an die Brust.", "tokens": ["Er\u00b7f\u00fcl\u00b7lung", "an", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}