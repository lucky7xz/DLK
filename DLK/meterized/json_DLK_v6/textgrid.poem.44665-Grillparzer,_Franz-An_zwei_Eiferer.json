{"textgrid.poem.44665": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "An zwei Eiferer", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich je schrieb, wie du Luzind geschrieben,", "tokens": ["Wenn", "ich", "je", "schrieb", ",", "wie", "du", "Lu\u00b7zind", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,", "PWAV", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auch in der Irre, G\u00f6tz, gleich dir herumgetrieben,", "tokens": ["Auch", "in", "der", "Ir\u00b7re", ",", "G\u00f6tz", ",", "gleich", "dir", "her\u00b7um\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "NE", "$,", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So wollt ich jetzt auch b\u00fc\u00dfen wie nun ihr.", "tokens": ["So", "wollt", "ich", "jetzt", "auch", "b\u00fc\u00b7\u00dfen", "wie", "nun", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "KOKOM", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verzeih ich ", "tokens": ["Ver\u00b7zeih", "ich"], "token_info": ["word", "word"], "pos": ["VVIMP", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}}}}}