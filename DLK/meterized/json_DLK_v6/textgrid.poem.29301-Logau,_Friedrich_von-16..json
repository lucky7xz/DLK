{"textgrid.poem.29301": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie dann, wann man Advocaten ihre heisse H\u00e4nde schmieret,", "tokens": ["Wie", "dann", ",", "wann", "man", "Ad\u00b7vo\u00b7ca\u00b7ten", "ih\u00b7re", "heis\u00b7se", "H\u00e4n\u00b7de", "schmie\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "PWAV", "PIS", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Da\u00df davon nicht so die H\u00e4nde wie die Zunge wol sich r\u00fchret?", "tokens": ["Da\u00df", "da\u00b7von", "nicht", "so", "die", "H\u00e4n\u00b7de", "wie", "die", "Zun\u00b7ge", "wol", "sich", "r\u00fch\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PAV", "PTKNEG", "ADV", "ART", "NN", "KOKOM", "ART", "NN", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Gelbes wird und etwas weisses, wie man sagt, dazu genummen;", "tokens": ["Gel\u00b7bes", "wird", "und", "et\u00b7was", "weis\u00b7ses", ",", "wie", "man", "sagt", ",", "da\u00b7zu", "ge\u00b7num\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KON", "ADV", "ADJA", "$,", "PWAV", "PIS", "VVFIN", "$,", "PAV", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Dieses soll zumal dem Hertzen und Gehirne wol bekummen.", "tokens": ["Die\u00b7ses", "soll", "zu\u00b7mal", "dem", "Hert\u00b7zen", "und", "Ge\u00b7hir\u00b7ne", "wol", "be\u00b7kum\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ART", "NN", "KON", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}