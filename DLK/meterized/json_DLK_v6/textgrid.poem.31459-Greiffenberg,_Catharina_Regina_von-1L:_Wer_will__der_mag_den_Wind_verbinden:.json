{"textgrid.poem.31459": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer will / der mag den Wind verbinden:", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer will / der mag den Wind verbinden:", "tokens": ["Wer", "will", "/", "der", "mag", "den", "Wind", "ver\u00b7bin\u00b7den", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$(", "ART", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er unterfangt Vnm\u00fcglichkeit.", "tokens": ["Er", "un\u00b7ter\u00b7fangt", "Vn\u00b7m\u00fcg\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Geist ist doch von mir nicht weit /", "tokens": ["Der", "Geist", "ist", "doch", "von", "mir", "nicht", "weit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "PPER", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "macht man schon Ort und Port verschwinden.", "tokens": ["macht", "man", "schon", "Ort", "und", "Port", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "NN", "KON", "NE", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Er bleibt nicht / bleib schon ich / dahinden:", "tokens": ["Er", "bleibt", "nicht", "/", "bleib", "schon", "ich", "/", "da\u00b7hin\u00b7den", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$(", "VVFIN", "ADV", "PPER", "$(", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "ist st\u00e4ts mit Gaben mir bereit /", "tokens": ["ist", "st\u00e4ts", "mit", "Ga\u00b7ben", "mir", "be\u00b7reit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "mich zu erquicken jederzeit.", "tokens": ["mich", "zu", "er\u00b7qui\u00b7cken", "je\u00b7der\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er l\u00e4st kein Creutz sich \u00fcberwinden.", "tokens": ["Er", "l\u00e4st", "kein", "Creutz", "sich", "\u00fc\u00b7berw\u00b7in\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Mein standhaffts Herz ist seine Klufft /", "tokens": ["Mein", "stand\u00b7haffts", "Herz", "ist", "sei\u00b7ne", "Klufft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "wo Er der liebsten Seelen rufft.", "tokens": ["wo", "Er", "der", "liebs\u00b7ten", "See\u00b7len", "rufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Jauchz' ich schon nicht mit etlich tausend:", "tokens": ["Jauch\u00b7z'", "ich", "schon", "nicht", "mit", "et\u00b7lich", "tau\u00b7send", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PTKNEG", "APPR", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Ey so vertritt sein' Allheit mich /", "tokens": ["Ey", "so", "ver\u00b7tritt", "sein'", "All\u00b7heit", "mich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPOSAT", "NN", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "so unaussprechlich seufz-und sausend /", "tokens": ["so", "un\u00b7aus\u00b7sprech\u00b7lich", "seuf\u00b7z\u00b7und", "sau\u00b7send", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "ADJD", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "da\u00df ich sie sp\u00fcre regen sich!", "tokens": ["da\u00df", "ich", "sie", "sp\u00fc\u00b7re", "re\u00b7gen", "sich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "ADJA", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}