{"textgrid.poem.46753": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[sonne, gehst du nieder]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sonne, gehst du nieder,", "tokens": ["Son\u00b7ne", ",", "gehst", "du", "nie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und der Lerche Lieder", "tokens": ["Und", "der", "Ler\u00b7che", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Jubeln nach mit Schall", "tokens": ["Ju\u00b7beln", "nach", "mit", "Schall"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Deinem Feuerball!", "tokens": ["Dei\u00b7nem", "Feu\u00b7er\u00b7ball", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn Du aufgehst wieder,", "tokens": ["Wenn", "Du", "auf\u00b7gehst", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Jubeln Lerchenlieder", "tokens": ["Ju\u00b7beln", "Ler\u00b7chen\u00b7lie\u00b7der"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Deinem Feuerball", "tokens": ["Dei\u00b7nem", "Feu\u00b7er\u00b7ball"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wieder zu mit Schall.", "tokens": ["Wie\u00b7der", "zu", "mit", "Schall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Ob du gehest nieder,", "tokens": ["Ob", "du", "ge\u00b7hest", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Oder aufgehst wieder,", "tokens": ["O\u00b7der", "auf\u00b7gehst", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nie den sch\u00f6nsten Schall", "tokens": ["Nie", "den", "sch\u00f6ns\u00b7ten", "Schall"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "H\u00f6rst du, goldner Ball.", "tokens": ["H\u00f6rst", "du", ",", "gold\u00b7ner", "Ball", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Wenn die Lerchenlieder", "tokens": ["Wenn", "die", "Ler\u00b7chen\u00b7lie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Schweigen hin und wieder,", "tokens": ["Schwei\u00b7gen", "hin", "und", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wird die Nachtigall", "tokens": ["Wird", "die", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Laut am Wasserfall.", "tokens": ["Laut", "am", "Was\u00b7ser\u00b7fall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Wenn das Tagsgefieder", "tokens": ["Wenn", "das", "Tags\u00b7ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Schl\u00e4ft im Nachtduftflieder,", "tokens": ["Schl\u00e4ft", "im", "Nacht\u00b7duft\u00b7flie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Weckt die Nachtigall", "tokens": ["Weckt", "die", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Seelenwiderhall.", "tokens": ["See\u00b7len\u00b7wi\u00b7der\u00b7hall", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Sonne, niemals wieder", "tokens": ["Son\u00b7ne", ",", "nie\u00b7mals", "wie\u00b7der"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADV", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sing' ich andre Lieder", "tokens": ["Sing'", "ich", "and\u00b7re", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Als Nachtseufzerhall", "tokens": ["Als", "Nach\u00b7tseuf\u00b7zer\u00b7hall"], "token_info": ["word", "word"], "pos": ["KOUS", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Unter Thr\u00e4nenfall.", "tokens": ["Un\u00b7ter", "Thr\u00e4\u00b7nen\u00b7fall", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}