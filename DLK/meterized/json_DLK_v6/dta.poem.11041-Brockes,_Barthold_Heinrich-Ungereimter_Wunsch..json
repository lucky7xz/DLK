{"dta.poem.11041": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ungereimter Wunsch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie ich, aus einem s\u00fcssen Traum, j\u00fcngst fr\u00fch erwachte,", "tokens": ["Wie", "ich", ",", "aus", "ei\u00b7nem", "s\u00fcs\u00b7sen", "Traum", ",", "j\u00fcngst", "fr\u00fch", "er\u00b7wach\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und, welch ein gro\u00dfes Gl\u00fcck es w\u00e4r, im Ernst gedachte,", "tokens": ["Und", ",", "welch", "ein", "gro\u00b7\u00dfes", "Gl\u00fcck", "es", "w\u00e4r", ",", "im", "Ernst", "ge\u00b7dach\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAT", "ART", "ADJA", "NN", "PPER", "VAFIN", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn man sich selber Tr\u00e4ume machen, dieselbige willk\u00fchr-", "tokens": ["Wenn", "man", "sich", "sel\u00b7ber", "Tr\u00e4u\u00b7me", "ma\u00b7chen", ",", "die\u00b7sel\u00b7bi\u00b7ge", "will\u00b7k\u00fchr"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PIS", "PRF", "ADV", "NN", "VVINF", "$,", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+---", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Und sie zusammen setzen k\u00f6nnte: O welch Vergn\u00fcgen", "tokens": ["Und", "sie", "zu\u00b7sam\u00b7men", "set\u00b7zen", "k\u00f6nn\u00b7te", ":", "O", "welch", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "VVINF", "VMFIN", "$.", "NE", "PIAT", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "W\u00fcrd einer doch sich selbst verschaffen! was w\u00fcrd er sehn,", "tokens": ["W\u00fcrd", "ei\u00b7ner", "doch", "sich", "selbst", "ver\u00b7schaf\u00b7fen", "!", "was", "w\u00fcrd", "er", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADV", "PRF", "ADV", "VVINF", "$.", "PWS", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "F\u00fcr Lieblichkeiten der Natur! Was w\u00fcrd er h\u00f6ren,", "tokens": ["F\u00fcr", "Lieb\u00b7lich\u00b7kei\u00b7ten", "der", "Na\u00b7tur", "!", "Was", "w\u00fcrd", "er", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$.", "PWS", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In einem angenehmen Wald, in hellen Ch\u00f6ren,", "tokens": ["In", "ei\u00b7nem", "an\u00b7ge\u00b7neh\u00b7men", "Wald", ",", "in", "hel\u00b7len", "Ch\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Von Singev\u00f6geln, Nachtigallen, f\u00fcr ein Get\u00f6n!", "tokens": ["Von", "Sin\u00b7ge\u00b7v\u00f6\u00b7geln", ",", "Nach\u00b7ti\u00b7gal\u00b7len", ",", "f\u00fcr", "ein", "Ge\u00b7t\u00f6n", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Wie w\u00fcrde mancher an der Sch\u00f6nheit, und andern Sch\u00e4tzen,", "tokens": ["Wie", "w\u00fcr\u00b7de", "man\u00b7cher", "an", "der", "Sch\u00f6n\u00b7heit", ",", "und", "an\u00b7dern", "Sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PIS", "APPR", "ART", "NN", "$,", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Von einer jungen sch\u00f6nen Frauen, sich nicht ergetzen!", "tokens": ["Von", "ei\u00b7ner", "jun\u00b7gen", "sch\u00f6\u00b7nen", "Frau\u00b7en", ",", "sich", "nicht", "er\u00b7get\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Was w\u00fcrd er nicht in Speis und Trank, f\u00fcr Lieblichkeiten,", "tokens": ["Was", "w\u00fcrd", "er", "nicht", "in", "Speis", "und", "Trank", ",", "f\u00fcr", "Lieb\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PTKNEG", "APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Durch vorgestellete Jdeen, sich selbst bereiten!", "tokens": ["Durch", "vor\u00b7ge\u00b7stel\u00b7le\u00b7te", "Jdeen", ",", "sich", "selbst", "be\u00b7rei\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRF", "ADV", "VVINF", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.13": {"text": "Allein indem ich also denke: So f\u00e4llt mir bey,", "tokens": ["Al\u00b7lein", "in\u00b7dem", "ich", "al\u00b7so", "den\u00b7ke", ":", "So", "f\u00e4llt", "mir", "bey", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "--+--+----+-+", "measure": "anapaest.di.plus"}, "line.14": {"text": "Da\u00df dieser mein so kluger Wunsch, recht n\u00e4rrisch sey.", "tokens": ["Da\u00df", "die\u00b7ser", "mein", "so", "klu\u00b7ger", "Wunsch", ",", "recht", "n\u00e4r\u00b7risch", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "PPOSAT", "ADV", "ADJA", "NN", "$,", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wir haben ja dergleichen Macht, indem wir wachen,", "tokens": ["Wir", "ha\u00b7ben", "ja", "derg\u00b7lei\u00b7chen", "Macht", ",", "in\u00b7dem", "wir", "wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "NN", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wir k\u00f6nnen uns von ungez\u00e4hlt-und s\u00fcssen Sachen,", "tokens": ["Wir", "k\u00f6n\u00b7nen", "uns", "von", "un\u00b7ge\u00b7z\u00e4hl\u00b7t\u00b7und", "s\u00fcs\u00b7sen", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Jm Denken tausendfach Vergn\u00fcgen, Jdeen machen,", "tokens": ["Jm", "Den\u00b7ken", "tau\u00b7send\u00b7fach", "Ver\u00b7gn\u00fc\u00b7gen", ",", "Jdeen", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Vergn\u00fcgliche zusammen setzen, ein ganzes Heer,", "tokens": ["Ver\u00b7gn\u00fcg\u00b7li\u00b7che", "zu\u00b7sam\u00b7men", "set\u00b7zen", ",", "ein", "gan\u00b7zes", "Heer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVINF", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Von holden Phantaseyen zeugen; ja was noch mehr,", "tokens": ["Von", "hol\u00b7den", "Phan\u00b7ta\u00b7se\u00b7yen", "zeu\u00b7gen", ";", "ja", "was", "noch", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$.", "ADV", "PWS", "ADV", "ADV", "$,"], "meter": "-+--+-++-+--+", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Wir k\u00f6nnen nicht nur in Gedanken, durch kluges F\u00fcgen,", "tokens": ["Wir", "k\u00f6n\u00b7nen", "nicht", "nur", "in", "Ge\u00b7dan\u00b7ken", ",", "durch", "klu\u00b7ges", "F\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "APPR", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.21": {"text": "Der sch\u00f6nsten Bilder des Gem\u00fcths, uns hier vergn\u00fcgen;", "tokens": ["Der", "sch\u00f6ns\u00b7ten", "Bil\u00b7der", "des", "Ge\u00b7m\u00fcths", ",", "uns", "hier", "ver\u00b7gn\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wir haben, von den allermeisten, die Wirklichkeit.", "tokens": ["Wir", "ha\u00b7ben", ",", "von", "den", "al\u00b7ler\u00b7meis\u00b7ten", ",", "die", "Wirk\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPR", "ART", "ADJA", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Von welcher unbegreiflichen Beschaffenheit", "tokens": ["Von", "wel\u00b7cher", "un\u00b7be\u00b7greif\u00b7li\u00b7chen", "Be\u00b7schaf\u00b7fen\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ist denn das menschliche Geschlecht! Da, sonder Licht,", "tokens": ["Ist", "denn", "das", "menschli\u00b7che", "Ge\u00b7schlecht", "!", "Da", ",", "son\u00b7der", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$.", "ADV", "$,", "KON", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Vernunft und Wirklichkeit und Wahrheit, wir was verlangen,", "tokens": ["Ver\u00b7nunft", "und", "Wirk\u00b7lich\u00b7keit", "und", "Wahr\u00b7heit", ",", "wir", "was", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$,", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Zu sehn, zu h\u00f6ren, zu geniessen, und zu empfangen.", "tokens": ["Zu", "sehn", ",", "zu", "h\u00f6\u00b7ren", ",", "zu", "ge\u00b7nies\u00b7sen", ",", "und", "zu", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-++-+-", "measure": "unknown.measure.septa"}, "line.27": {"text": "Hat man nun wirklich Licht, Vernunft, Geh\u00f6r, Gesicht,", "tokens": ["Hat", "man", "nun", "wirk\u00b7lich", "Licht", ",", "Ver\u00b7nunft", ",", "Ge\u00b7h\u00f6r", ",", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADJD", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Zu sehn, zu h\u00f6ren, sich zu freuen: So thut mans nicht.", "tokens": ["Zu", "sehn", ",", "zu", "h\u00f6\u00b7ren", ",", "sich", "zu", "freu\u00b7en", ":", "So", "thut", "mans", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,", "PRF", "PTKZU", "VVINF", "$.", "ADV", "VVFIN", "PIS", "PTKNEG", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}