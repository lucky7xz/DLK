{"textgrid.poem.58235": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "273. Hohes Licht tut's noch nicht", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es haben manche wohl von hohen St\u00e4nden Licht", "tokens": ["Es", "ha\u00b7ben", "man\u00b7che", "wohl", "von", "ho\u00b7hen", "St\u00e4n\u00b7den", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIS", "ADV", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und haben lange noch das wahre Wesen nicht;", "tokens": ["Und", "ha\u00b7ben", "lan\u00b7ge", "noch", "das", "wah\u00b7re", "We\u00b7sen", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Licht ist eine Blum', die leichtlich kann verwehen,", "tokens": ["Das", "Licht", "ist", "ei\u00b7ne", "Blum'", ",", "die", "leicht\u00b7lich", "kann", "ver\u00b7we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schau, was f\u00fcr Frucht in dir nach K\u00e4lt' und Sturm bleibt stehen!", "tokens": ["Schau", ",", "was", "f\u00fcr", "Frucht", "in", "dir", "nach", "K\u00e4lt'", "und", "Sturm", "bleibt", "ste\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "NN", "APPR", "PPER", 