{"textgrid.poem.49141": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Bey Genie\u00dfung des heiligen Abendmahls", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Jesu, heiligs Gottes-Lamm", "tokens": ["O", "Je\u00b7su", ",", "hei\u00b7ligs", "Got\u00b7tes\u00b7Lamm"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das an des bittern Kreutzes-Stamm", "tokens": ["Das", "an", "des", "bit\u00b7tern", "Kreut\u00b7zes\u00b7Stamm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr uns so schmertzlich lie\u00df das Leben,", "tokens": ["F\u00fcr", "uns", "so", "schmertz\u00b7lich", "lie\u00df", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fcr die Schuld der gantzen Welt", "tokens": ["Und", "f\u00fcr", "die", "Schuld", "der", "gant\u00b7zen", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das unsch\u00e4tzbare S\u00fchne-Geld", "tokens": ["Das", "un\u00b7sch\u00e4tz\u00b7ba\u00b7re", "S\u00fch\u00b7ne\u00b7Geld"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "An seinen Vater hat gegeben,", "tokens": ["An", "sei\u00b7nen", "Va\u00b7ter", "hat", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ach, es verirrt sich mein Gem\u00fcth", "tokens": ["Ach", ",", "es", "ver\u00b7irrt", "sich", "mein", "Ge\u00b7m\u00fcth"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Im Abgrund aller deiner G\u00fct.", "tokens": ["Im", "Ab\u00b7grund", "al\u00b7ler", "dei\u00b7ner", "G\u00fct", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du k\u00f6ntest nicht vergn\u00fcget seyn,", "tokens": ["Du", "k\u00f6n\u00b7test", "nicht", "ver\u00b7gn\u00fc\u00b7get", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df mit so gro\u00dfer Qual und Pein", "tokens": ["Da\u00df", "mit", "so", "gro\u00b7\u00dfer", "Qual", "und", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADV", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein theurer Leib geopffert w\u00fcrde;", "tokens": ["Dein", "theu\u00b7rer", "Leib", "ge\u00b7opf\u00b7fert", "w\u00fcr\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bestriemt, durchnagelt, auffgeritzt", "tokens": ["Be\u00b7striemt", ",", "durch\u00b7na\u00b7gelt", ",", "auff\u00b7ge\u00b7ritzt"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sein Blut hat \u00fcberall geschwitzt", "tokens": ["Sein", "Blut", "hat", "\u00fc\u00b7be\u00b7rall", "ge\u00b7schwitzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und endlich starb in seiner B\u00fcrde;", "tokens": ["Und", "end\u00b7lich", "starb", "in", "sei\u00b7ner", "B\u00fcr\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie solches uns recht heylsam w\u00e4r',", "tokens": ["Wie", "sol\u00b7ches", "uns", "recht", "heyl\u00b7sam", "w\u00e4r'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Herr, dies bedachtest du weit mehr.", "tokens": ["Herr", ",", "dies", "be\u00b7dach\u00b7test", "du", "weit", "mehr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VVFIN", "PPER", "ADJD", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "So setztest du ein Gastmahl ein,", "tokens": ["So", "setz\u00b7test", "du", "ein", "Gast\u00b7mahl", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vermachtest hiezu Brodt und Wein,", "tokens": ["Ver\u00b7mach\u00b7test", "hie\u00b7zu", "Brodt", "und", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur einen Trank und eine Speise,", "tokens": ["Nur", "ei\u00b7nen", "Trank", "und", "ei\u00b7ne", "Spei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die aber uns so vielfach gut.", "tokens": ["Die", "a\u00b7ber", "uns", "so", "viel\u00b7fach", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was, Jesu, selbst dein Fleisch und Blut?", "tokens": ["Was", ",", "Je\u00b7su", ",", "selbst", "dein", "Fleisch", "und", "Blut", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "NE", "$,", "ADV", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach, eine h\u00f6chst geheime Weise,", "tokens": ["Ach", ",", "ei\u00b7ne", "h\u00f6chst", "ge\u00b7hei\u00b7me", "Wei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Auff die wir, liebster Heyland, dein", "tokens": ["Auff", "die", "wir", ",", "liebs\u00b7ter", "Hey\u00b7land", ",", "dein"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "PRELS", "PPER", "$,", "ADJA", "NN", "$,", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und deines Leidens theilhafft seyn.", "tokens": ["Und", "dei\u00b7nes", "Lei\u00b7dens", "theil\u00b7hafft", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der letzte Tag kam schon herbey,", "tokens": ["Der", "letz\u00b7te", "Tag", "kam", "schon", "her\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dran du, o Bild der Lieb' und Treu,", "tokens": ["Dran", "du", ",", "o", "Bild", "der", "Lieb'", "und", "Treu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "$,", "FM", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr deine Knechte wollest sterben;", "tokens": ["F\u00fcr", "dei\u00b7ne", "Knech\u00b7te", "wol\u00b7lest", "ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So machtest du dein Testament", "tokens": ["So", "mach\u00b7test", "du", "dein", "Tes\u00b7ta\u00b7ment"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und setztest uns, den wir gesch\u00e4ndt,", "tokens": ["Und", "setz\u00b7test", "uns", ",", "den", "wir", "ge\u00b7sch\u00e4ndt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu aller deiner G\u00fcter Erben,", "tokens": ["Zu", "al\u00b7ler", "dei\u00b7ner", "G\u00fc\u00b7ter", "Er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und da\u00df kein Zweiffel hie w\u00e4r' an,", "tokens": ["Und", "da\u00df", "kein", "Zweif\u00b7fel", "hie", "w\u00e4r'", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIAT", "NN", "ADV", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hingst du dich selbst zum Siegel dran.", "tokens": ["Hingst", "du", "dich", "selbst", "zum", "Sie\u00b7gel", "dran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O Wunder-Tisch, o seltnes Fest,", "tokens": ["O", "Wun\u00b7der\u00b7Tisch", ",", "o", "selt\u00b7nes", "Fest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da sich der Wirth aufftragen l\u00e4st", "tokens": ["Da", "sich", "der", "Wirth", "auff\u00b7tra\u00b7gen", "l\u00e4st"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "VVIZU", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und selber hei\u00dfet zu genie\u00dfen,", "tokens": ["Und", "sel\u00b7ber", "hei\u00b7\u00dfet", "zu", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der so viel hundert tausend nehrt", "tokens": ["Der", "so", "viel", "hun\u00b7dert", "tau\u00b7send", "nehrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "CARD", "CARD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und dennoch nimmer wird verzehrt.", "tokens": ["Und", "den\u00b7noch", "nim\u00b7mer", "wird", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nichts kanstu, \u00dcberwitz, hie wi\u00dfen,", "tokens": ["Nichts", "kans\u00b7tu", ",", "\u00dc\u00b7ber\u00b7witz", ",", "hie", "wi\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "$,", "NE", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Einfalt, die dem h\u00f6chsten gl\u00e4ubt,", "tokens": ["Die", "Ein\u00b7falt", ",", "die", "dem", "h\u00f6chs\u00b7ten", "gl\u00e4ubt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die ist es, der der Krantz verbleibt.", "tokens": ["Die", "ist", "es", ",", "der", "der", "Krantz", "ver\u00b7bleibt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Kommt, ruffstu, Jesu, Alle her,", "tokens": ["Kommt", ",", "ruffs\u00b7tu", ",", "Je\u00b7su", ",", "Al\u00b7le", "her", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "NE", "$,", "PIS", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Die ihr mit Kummer und Beschwer", "tokens": ["Die", "ihr", "mit", "Kum\u00b7mer", "und", "Be\u00b7schwer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des S\u00fcnden-Joches seyd beladen,", "tokens": ["Des", "S\u00fcn\u00b7den\u00b7Jo\u00b7ches", "seyd", "be\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hie wird die Last euch abgethan.", "tokens": ["Hie", "wird", "die", "Last", "euch", "ab\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auff dies dein Wort komm' ich auch an,", "tokens": ["Auff", "dies", "dein", "Wort", "komm'", "ich", "auch", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht wehrt zwar, Liebster, deiner Gnaden,", "tokens": ["Nicht", "wehrt", "zwar", ",", "Liebs\u00b7ter", ",", "dei\u00b7ner", "Gna\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ADV", "$,", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Versag mir doch nicht deine G\u00fct',", "tokens": ["Ver\u00b7sag", "mir", "doch", "nicht", "dei\u00b7ne", "G\u00fct'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Denn selbst mein Unwerth mich herzieht.", "tokens": ["Denn", "selbst", "mein", "Un\u00b7werth", "mich", "her\u00b7zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die Stoltzen, die in ihrem Sinn", "tokens": ["Die", "Stolt\u00b7zen", ",", "die", "in", "ih\u00b7rem", "Sinn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schon mehr als heilig sind vorhin,", "tokens": ["Schon", "mehr", "als", "hei\u00b7lig", "sind", "vor\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "KOKOM", "ADJD", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verachten, Herr, mit dir zu speisen;", "tokens": ["Ver\u00b7ach\u00b7ten", ",", "Herr", ",", "mit", "dir", "zu", "spei\u00b7sen", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Arm' und Lahme von der Ga\u00df", "tokens": ["Der", "Arm'", "und", "Lah\u00b7me", "von", "der", "Ga\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und der am Zaun der Tr\u00fcbsal sa\u00df,", "tokens": ["Und", "der", "am", "Zaun", "der", "Tr\u00fcb\u00b7sal", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der l\u00e4\u00dfet gern zu dir sich weisen,", "tokens": ["Der", "l\u00e4\u00b7\u00dfet", "gern", "zu", "dir", "sich", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und der verhungert und verd\u00fcrst", "tokens": ["Und", "der", "ver\u00b7hun\u00b7gert", "und", "ver\u00b7d\u00fcrst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sucht deine Taffel, Lebens-F\u00fcrst.", "tokens": ["Sucht", "dei\u00b7ne", "Taf\u00b7fel", ",", "Le\u00b7bens\u00b7F\u00fcrst", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Auch ich bin aus derselben Zahl,", "tokens": ["Auch", "ich", "bin", "aus", "der\u00b7sel\u00b7ben", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zwar vor von dir schon mannigmal", "tokens": ["Zwar", "vor", "von", "dir", "schon", "man\u00b7nig\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "APPR", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Au\u00df meinem Elend' auffgenommen,", "tokens": ["Au\u00df", "mei\u00b7nem", "E\u00b7lend'", "auff\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kaum aber, da\u00df du mich geweidt,", "tokens": ["Kaum", "a\u00b7ber", ",", "da\u00df", "du", "mich", "ge\u00b7weidt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "i