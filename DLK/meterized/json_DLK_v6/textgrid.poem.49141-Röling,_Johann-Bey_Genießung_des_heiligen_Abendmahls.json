"-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und mach mich unbefleckt und rein,", "tokens": ["Und", "mach", "mich", "un\u00b7be\u00b7fleckt", "und", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da du in mir gezeugt wilst seyn.", "tokens": ["Da", "du", "in", "mir", "ge\u00b7zeugt", "wilst", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVPP", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Der du f\u00fcr uns ertrugst den Tod,", "tokens": ["Der", "du", "f\u00fcr", "uns", "er\u00b7trugst", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ert\u00f6dte meine Todes-Noth,", "tokens": ["Er\u00b7t\u00f6d\u00b7te", "mei\u00b7ne", "To\u00b7des\u00b7Noth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der du au\u00df deinem Grab entstanden,", "tokens": ["Der", "du", "au\u00df", "dei\u00b7nem", "Grab", "ent\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erheb mich auch aus meinem Grab,", "tokens": ["Er\u00b7heb", "mich", "auch", "aus", "mei\u00b7nem", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"tex