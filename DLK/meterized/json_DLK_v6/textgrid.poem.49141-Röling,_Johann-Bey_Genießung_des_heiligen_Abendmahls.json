okens": ["Voll", "dei\u00b7nes", "Him\u00b7mels", ",", "dei\u00b7nes", "Le\u00b7bens", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In wei\u00dfer Leinwand wurdest du,", "tokens": ["In", "wei\u00b7\u00dfer", "Lein\u00b7wand", "wur\u00b7dest", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Heyl, gebracht zu deiner Ruh,", "tokens": ["Mein", "Heyl", ",", "ge\u00b7bracht", "zu", "dei\u00b7ner", "Ruh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich wei\u00df, da\u00df dieses nicht vergebens;", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "die\u00b7ses", "nicht", "ver\u00b7ge\u00b7bens", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PDS", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Auch mein Hertz mu\u00df von solchem Schein,", "tokens": ["Auch", "mein", "Hertz", "mu\u00df", "von", "sol\u00b7chem", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "APPR", "PIAT", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Solst du darinnen ruhen, seyn.", "tokens": ["Solst", "du", "da\u00b7rin\u00b7nen", "ru\u00b7hen", ",", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$,", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "So komm, o Jesu, komm denn ein,", "tokens": ["So", "komm", ",", "o", "Je\u00b7su", ",", "komm", "denn", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "FM", "NE", "$,", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gib, liebes Brodt, gib, lieber Wein,", "tokens": ["Gib", ",", "lie\u00b7bes", "Brodt", ",", "gib", ",", "lie\u00b7ber", "Wein", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADJA", "NN", "$,", "VVIMP", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gib her, den meine Seele liebet;", "tokens": ["Gib", "her", ",", "den", "mei\u00b7ne", "See\u00b7le", "lie\u00b7bet", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich bin nunmehro nicht mehr ich,", "tokens": ["Ich", "bin", "nun\u00b7meh\u00b7ro", "nicht", "mehr", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ADV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir tauschen, Jesu; Ich f\u00fcr dich.", "tokens": ["Wir", "tau\u00b7schen", ",", "Je\u00b7su", ";", "Ich", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NE", "$.", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach, da\u00df ein S\u00fcnder Gott sich giebet", "tokens": ["Ach", ",", "da\u00df", "ein", "S\u00fcn\u00b7der", "Gott", "sich", "gie\u00b7bet"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "ART", "NN", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und Gott mit gr\u00f6\u00dferer Begier", "tokens": ["Und", "Gott", "mit", "gr\u00f6\u00b7\u00dfe\u00b7rer", "Be\u00b7gier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sich doch demselben schenkt daf\u00fcr.", "tokens": ["Sich", "doch", "dem\u00b7sel\u00b7ben", "schenkt", "da\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVINF", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}