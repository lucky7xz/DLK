{"dta.poem.9362": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Ii .  \n Ein Muster der Neuen Troch\u00e4ischen  \n Art/ da sich die strophe mit 2. Weiblichen 13-syl-  \n bigen anf\u00e4ht/ und mit 2. M\u00e4nnlichen 12-  \n sybigen schlie\u00dfet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Soll ich armer schweigen nun/ oder sol ichs sagen?", "tokens": ["Soll", "ich", "ar\u00b7mer", "schwei\u00b7gen", "nun", "/", "o\u00b7der", "sol", "ichs", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "VVFIN", "ADV", "$(", "KON", "VMFIN", "PIS", "VVINF", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ach mein senftzen-volles hertz zwinget mich zu", "tokens": ["Ach", "mein", "senft\u00b7zen\u00b7vol\u00b7les", "hertz", "zwin\u00b7get", "mich", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "PTKZU"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "klagen/", "tokens": ["kla\u00b7gen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Ach wo ist mein Seelen-Artzt/ Ach wo ist er hin!", "tokens": ["Ach", "wo", "ist", "mein", "See\u00b7len\u00b7Artzt", "/", "Ach", "wo", "ist", "er", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWAV", "VAFIN", "PPOSAT", "NN", "$(", "ITJ", "PWAV", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Sein verzug alleine mier raubet muth und Sinn;", "tokens": ["Sein", "ver\u00b7zug", "al\u00b7lei\u00b7ne", "mier", "rau\u00b7bet", "muth", "und", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Mancher Artzt und manch balbier haben mir gedienet/", "tokens": ["Man\u00b7cher", "Artzt", "und", "manch", "bal\u00b7bier", "ha\u00b7ben", "mir", "ge\u00b7die\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "VAFIN", "PPER", "VVPP", "$("], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "manche blume/ manches kraut so im garten gr\u00fcnet", "tokens": ["man\u00b7che", "blu\u00b7me", "/", "man\u00b7ches", "kraut", "so", "im", "gar\u00b7ten", "gr\u00fc\u00b7net"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$(", "PIS", "VVFIN", "ADV", "APPRART", "VVFIN", "VVFIN"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "und was mehr vor artzeney diese welt \u00fcmbschr\u00e4nckt;", "tokens": ["und", "was", "mehr", "vor", "art\u00b7ze\u00b7ney", "die\u00b7se", "welt", "\u00fcmbsc\u00b7hr\u00e4nckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "APPR", "NN", "PDAT", "NN", "VVPP", "$."], "meter": "----+----+-+", "measure": "unknown.measure.tri"}, "line.9": {"text": "nur du warest nicht dabey welches mich sehr kr\u00e4nckt/", "tokens": ["nur", "du", "wa\u00b7rest", "nicht", "da\u00b7bey", "wel\u00b7ches", "mich", "sehr", "kr\u00e4nckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PTKNEG", "PAV", "PRELS", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Du gerechter Samarith/ Du ", "tokens": ["Du", "ge\u00b7rech\u00b7ter", "Sa\u00b7ma\u00b7rith", "/", "Du"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "ADJD", "NE", "$(", "PPER"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.11": {"text": "Du mein Laabsaal und mein Heil/ meine Lebens-Son-", "tokens": ["Du", "mein", "La\u00b7ab\u00b7saal", "und", "mein", "Heil", "/", "mei\u00b7ne", "Le\u00b7bens\u00b7Son"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$(", "PPOSAT", "TRUNC"], "meter": "-++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.12": {"text": "dr\u00fcmb war alles nur \u00fcmbsonst; nur \u00fcmbsonst es ist/", "tokens": ["dr\u00fcmb", "war", "al\u00b7les", "nur", "\u00fcm\u00b7bsonst", ";", "nur", "\u00fcm\u00b7bsonst", "es", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PIS", "ADV", "ADV", "$.", "ADV", "ADV", "PPER", "VAFIN", "$("], "meter": "--+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "was ich hofft\u2019 un\u0303 hoffe noch/ was ich l\u00e4ngst vermisst/", "tokens": ["was", "ich", "hofft'", "u\u00f1", "hof\u00b7fe", "noch", "/", "was", "ich", "l\u00e4ngst", "ver\u00b7misst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "KON", "VVFIN", "ADV", "$(", "PWS", "PPER", "ADV", "VVFIN", "$("], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "dieses mangelt mir allein: ach wie seyn verblichen", "tokens": ["die\u00b7ses", "man\u00b7gelt", "mir", "al\u00b7lein", ":", "ach", "wie", "seyn", "ver\u00b7bli\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$.", "ADV", "KOKOM", "PPOSAT", "ADJA"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.15": {"text": "meine Wangen und der Mund dem die rosen wichen/", "tokens": ["mei\u00b7ne", "Wan\u00b7gen", "und", "der", "Mund", "dem", "die", "ro\u00b7sen", "wi\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ART", "NN", "ART", "ART", "ADJA", "VVFIN", "$("], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "kennstu/ Gott/ auch dein gesch\u00f6pf/ kennstu mich auch", "tokens": ["kenns\u00b7tu", "/", "Gott", "/", "auch", "dein", "ge\u00b7sch\u00f6pf", "/", "kenns\u00b7tu", "mich", "auch"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$(", "NN", "$(", "ADV", "PPOSAT", "NN", "$(", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.17": {"text": "der ich t\u00e4glich zihen mu\u00df ein so schweres joch. (noch?", "tokens": ["der", "ich", "t\u00e4g\u00b7lich", "zi\u00b7hen", "mu\u00df", "ein", "so", "schwe\u00b7res", "joch", ".", "(", "noch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVINF", "VMFIN", "ART", "ADV", "ADJA", "NN", "$.", "$(", "ADV", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "Meine zunge todten-bla\u00df kaum sich mehr erreget/", "tokens": ["Mei\u00b7ne", "zun\u00b7ge", "tod\u00b7ten\u00b7bla\u00df", "kaum", "sich", "mehr", "er\u00b7re\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "PRF", "ADV", "VVFIN", "$("], "meter": "+-+-+-+----+-", "measure": "unknown.measure.penta"}, "line.19": {"text": "ja der gantz geschw\u00e4chte puls n\u00e4rlich n\u00e4rlich schl\u00e4get/", "tokens": ["ja", "der", "gantz", "ge\u00b7schw\u00e4ch\u00b7te", "puls", "n\u00e4r\u00b7lich", "n\u00e4r\u00b7lich", "schl\u00e4\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADV", "ADJA", "NN", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.20": {"text": "Meinem ", "tokens": ["Mei\u00b7nem"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+-", "measure": "trochaic.single"}, "line.21": {"text": "alle krafft ist auch hinweg; ich bin in gefahr/", "tokens": ["al\u00b7le", "krafft", "ist", "auch", "hin\u00b7weg", ";", "ich", "bin", "in", "ge\u00b7fahr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "PTKVZ", "$.", "PPER", "VAFIN", "APPR", "NN", "$("], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "marck und bein ist sterbe-kranck/ weil ich gantz verblasset", "tokens": ["marck", "und", "bein", "ist", "ster\u00b7be\u00b7kranck", "/", "weil", "ich", "gantz", "ver\u00b7blas\u00b7set"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADV", "VAFIN", "ADJD", "$(", "KOUS", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "schwach und lagerhafftig bin/ und der welt verhasset/", "tokens": ["schwach", "und", "la\u00b7ger\u00b7haff\u00b7tig", "bin", "/", "und", "der", "welt", "ver\u00b7has\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "$(", "KON", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "weil kein aug\u2019 in meinem aug\u2019 und kein hertze mehr", "tokens": ["weil", "kein", "aug'", "in", "mei\u00b7nem", "aug'", "und", "kein", "hert\u00b7ze", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "APPR", "PPOSAT", "NN", "KON", "PIAT", "NN", "ADV"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.25": {"text": "selbst in meinem hertzen ist/ das mich kr\u00e4ncket sehr.", "tokens": ["selbst", "in", "mei\u00b7nem", "hert\u00b7zen", "ist", "/", "das", "mich", "kr\u00e4n\u00b7cket", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VAFIN", "$(", "PRELS", "PPER", "VVFIN", "ADV", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.26": {"text": "Eyle/ weile nicht/ mein Heil/ komm mich zuverbinden/", "tokens": ["Ey\u00b7le", "/", "wei\u00b7le", "nicht", "/", "mein", "Heil", "/", "komm", "mich", "zu\u00b7ver\u00b7bin\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVFIN", "PTKNEG", "$(", "PPOSAT", "NN", "$(", "VVFIN", "PPER", "VVINF", "$("], "meter": "+-+----+-+-+-", "measure": "unknown.measure.penta"}, "line.27": {"text": "und gedencke nicht der schuld/ la\u00df mich gnade finden/", "tokens": ["und", "ge\u00b7den\u00b7cke", "nicht", "der", "schuld", "/", "la\u00df", "mich", "gna\u00b7de", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "ADJD", "$(", "VVIMP", "PPER", "VVFIN", "VVINF", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.28": {"text": "la\u00df mich seyn dein liebes kind/ hilf mir aus der noth/", "tokens": ["la\u00df", "mich", "seyn", "dein", "lie\u00b7bes", "kind", "/", "hilf", "mir", "aus", "der", "noth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VAINF", "PPOSAT", "ADJA", "NN", "$(", "VVIMP", "PPER", "APPR", "ART", "NN", "$("], "meter": "+---+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.29": {"text": "Ich verbleibe dier getreu/ Gott/ bi\u00df in den Todt.", "tokens": ["Ich", "ver\u00b7blei\u00b7be", "dier", "ge\u00b7treu", "/", "Gott", "/", "bi\u00df", "in", "den", "Todt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$(", "NN", "$(", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}}}}}