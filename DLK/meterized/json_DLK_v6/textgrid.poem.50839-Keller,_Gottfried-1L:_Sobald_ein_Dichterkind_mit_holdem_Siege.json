{"textgrid.poem.50839": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sobald ein Dichterkind mit holdem Siege", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sobald ein Dichterkind mit holdem Siege", "tokens": ["So\u00b7bald", "ein", "Dich\u00b7ter\u00b7kind", "mit", "hol\u00b7dem", "Sie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Augen aufschl\u00e4gt hier im Erdentale,", "tokens": ["Die", "Au\u00b7gen", "auf\u00b7schl\u00e4gt", "hier", "im", "Er\u00b7den\u00b7ta\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Stehn schon zwei Genien an seiner Wiege:", "tokens": ["Stehn", "schon", "zwei", "Ge\u00b7ni\u00b7en", "an", "sei\u00b7ner", "Wie\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "CARD", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}, "stanza.2": {"line.1": {"text": "Hell von Kristall h\u00e4lt dieser eine Schale,", "tokens": ["Hell", "von", "Kris\u00b7tall", "h\u00e4lt", "die\u00b7ser", "ei\u00b7ne", "Scha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "VVFIN", "PDAT", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Voll bis zum Rand von feuergoldnem Wein,", "tokens": ["Voll", "bis", "zum", "Rand", "von", "feu\u00b7er\u00b7gold\u00b7nem", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Belebt, durchwebt vom reinsten Sonnenstrahle;", "tokens": ["Be\u00b7lebt", ",", "durch\u00b7webt", "vom", "reins\u00b7ten", "Son\u00b7nen\u00b7strah\u00b7le", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Des andern Schal' ist dunkler Edelstein,", "tokens": ["Des", "an\u00b7dern", "Schal'", "ist", "dunk\u00b7ler", "E\u00b7del\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Rubin, und fa\u00dft des Mohnes dunkeln Saft,", "tokens": ["Ru\u00b7bin", ",", "und", "fa\u00dft", "des", "Moh\u00b7nes", "dun\u00b7keln", "Saft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Durchwoben von des Mondes Zitterschein.", "tokens": ["Durch\u00b7wo\u00b7ben", "von", "des", "Mon\u00b7des", "Zit\u00b7ter\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "In beiden Schalen ruht die Lebenskraft,", "tokens": ["In", "bei\u00b7den", "Scha\u00b7len", "ruht", "die", "Le\u00b7bens\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die ihm die treuen Genien rastlos schenken,", "tokens": ["Die", "ihm", "die", "treu\u00b7en", "Ge\u00b7ni\u00b7en", "rast\u00b7los", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die ihn durchwallt und seine Lieder schafft.", "tokens": ["Die", "ihn", "durch\u00b7wallt", "und", "sei\u00b7ne", "Lie\u00b7der", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Aus beiden Schalen str\u00f6mt sein Sein und Denken,", "tokens": ["Aus", "bei\u00b7den", "Scha\u00b7len", "str\u00f6mt", "sein", "Sein", "und", "Den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein Bl\u00fchn und Sehnen, flie\u00dfen Tag und Nacht,", "tokens": ["Sein", "Bl\u00fchn", "und", "Seh\u00b7nen", ",", "flie\u00b7\u00dfen", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein sonnig Schaun, ein tr\u00e4umerisch Versenken", "tokens": ["Ein", "son\u00b7nig", "Schaun", ",", "ein", "tr\u00e4u\u00b7me\u00b7risch", "Ver\u00b7sen\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "$,", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "In seine Seele, wie sie tr\u00e4umt und wacht.", "tokens": ["In", "sei\u00b7ne", "See\u00b7le", ",", "wie", "sie", "tr\u00e4umt", "und", "wacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Preis dem Dichter, wenn die Lebensbecher", "tokens": ["Und", "Preis", "dem", "Dich\u00b7ter", ",", "wenn", "die", "Le\u00b7bens\u00b7be\u00b7cher"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "ART", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihm reich erfunkeln und in gleicher Pracht!", "tokens": ["Ihm", "reich", "er\u00b7fun\u00b7keln", "und", "in", "glei\u00b7cher", "Pracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "KON", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Doch Halbpoet nur ist der trunkne Zecher,", "tokens": ["Doch", "Halb\u00b7poet", "nur", "ist", "der", "trunk\u00b7ne", "Ze\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-++-+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Der aus dem ", "tokens": ["Der", "aus", "dem"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Sein Herz wird krank, sein Lied allt\u00e4glich schw\u00e4cher. \u2013", "tokens": ["Sein", "Herz", "wird", "krank", ",", "sein", "Lied", "all\u00b7t\u00e4g\u00b7lich", "schw\u00e4\u00b7cher", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "ADJD", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Oh, wenn die Nacht mit ihren Sternen winkt,", "tokens": ["Oh", ",", "wenn", "die", "Nacht", "mit", "ih\u00b7ren", "Ster\u00b7nen", "winkt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dann leer die dunkle Schale bis zum Grunde,", "tokens": ["Dann", "leer", "die", "dunk\u00b7le", "Scha\u00b7le", "bis", "zum", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df der uralte Zauber in dich sinkt!", "tokens": ["Da\u00df", "der", "ur\u00b7al\u00b7te", "Zau\u00b7ber", "in", "dich", "sinkt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Doch naht mit heil'gem Wehn die Morgenstunde,", "tokens": ["Doch", "naht", "mit", "heil'\u00b7gem", "Wehn", "die", "Mor\u00b7gen\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "La\u00df dem Kristall den klaren Trank entquellen", "tokens": ["La\u00df", "dem", "Kris\u00b7tall", "den", "kla\u00b7ren", "Trank", "ent\u00b7quel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "ART", "ADJA", "NN", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und f\u00fchr, wie sie, der Wahrheit Gold im Munde!", "tokens": ["Und", "f\u00fchr", ",", "wie", "sie", ",", "der", "Wahr\u00b7heit", "Gold", "im", "Mun\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "$,", "ART", "NN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Tu auf dein Aug des Lichtes goldnen Wellen!", "tokens": ["Tu", "auf", "dein", "Aug", "des", "Lich\u00b7tes", "gold\u00b7nen", "Wel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "La\u00df liegen, die im t\u00f6dlichen Rausch versunken,", "tokens": ["La\u00df", "lie\u00b7gen", ",", "die", "im", "t\u00f6d\u00b7li\u00b7chen", "Rausch", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "$,", "PRELS", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die ewig auch den Tag zur Nachtgesellen! \u2013", "tokens": ["Die", "e\u00b7wig", "auch", "den", "Tag", "zur", "Nacht\u00b7ge\u00b7sel\u00b7len", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "ADV", "ART", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "So hast auch du die Zauberflut getrunken,", "tokens": ["So", "hast", "auch", "du", "die", "Zau\u00b7ber\u00b7flut", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Freiligrath! da\u00df Berg und Tal erklungen", "tokens": ["O", "Frei\u00b7li\u00b7gra\u00b7th", "!", "da\u00df", "Berg", "und", "Tal", "er\u00b7klun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "KOUS", "NN", "KON", "NN", "VVINF"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und sich die Elfen fr\u00f6hlich zugewunken!", "tokens": ["Und", "sich", "die", "El\u00b7fen", "fr\u00f6h\u00b7lich", "zu\u00b7ge\u00b7wun\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Vom Morgenland hast ahnend du gesungen;", "tokens": ["Vom", "Mor\u00b7gen\u00b7land", "hast", "ah\u00b7nend", "du", "ge\u00b7sun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn als der Morgen leuchtend vor dir stand,", "tokens": ["Denn", "als", "der", "Mor\u00b7gen", "leuch\u00b7tend", "vor", "dir", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da hast du aus den Rosen dich geschwungen", "tokens": ["Da", "hast", "du", "aus", "den", "Ro\u00b7sen", "dich", "ge\u00b7schwun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Frisch und gesund; und sieh! das Morgenland", "tokens": ["Frisch", "und", "ge\u00b7sund", ";", "und", "sieh", "!", "das", "Mor\u00b7gen\u00b7land"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "$.", "KON", "VVIMP", "$.", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Lag ausgebreitet da zu deinen F\u00fc\u00dfen:", "tokens": ["Lag", "aus\u00b7ge\u00b7brei\u00b7tet", "da", "zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kamele, Tiger, Sklaverei und Sand!", "tokens": ["Ka\u00b7me\u00b7le", ",", "Ti\u00b7ger", ",", "Skla\u00b7ve\u00b7rei", "und", "Sand", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.14": {"line.1": {"text": "Doch mitten aus der W\u00fcste Finsternissen", "tokens": ["Doch", "mit\u00b7ten", "aus", "der", "W\u00fcs\u00b7te", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erbl\u00fcht' der \u00bbMorgen, und vom Rhein\u00ab erklang's", "tokens": ["Er\u00b7bl\u00fcht'", "der", "\u00bb", "Mor\u00b7gen", ",", "und", "vom", "Rhein", "\u00ab", "er\u00b7klang's"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "ART", "$(", "NN", "$,", "KON", "APPRART", "NE", "$(", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Entgegen dir von hellen Freiheitsgr\u00fc\u00dfen!", "tokens": ["Ent\u00b7ge\u00b7gen", "dir", "von", "hel\u00b7len", "Frei\u00b7heits\u00b7gr\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Und jeder Mund im deutschen Lande sang's:", "tokens": ["Und", "je\u00b7der", "Mund", "im", "deut\u00b7schen", "Lan\u00b7de", "sang's", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbder Freiligrath hat sich zu uns geschlagen!\u00ab", "tokens": ["\u00bb", "der", "Frei\u00b7li\u00b7gra\u00b7th", "hat", "sich", "zu", "uns", "ge\u00b7schla\u00b7gen", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PRF", "APPR", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und jedes Ohr in fernen Gaun verschlang's,", "tokens": ["Und", "je\u00b7des", "Ohr", "in", "fer\u00b7nen", "Gaun", "ver\u00b7schlang's", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "So weit die deutsche Kunde ward getragen.", "tokens": ["So", "weit", "die", "deut\u00b7sche", "Kun\u00b7de", "ward", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch manchem wohl erklang dein Taglied schrill,", "tokens": ["Doch", "man\u00b7chem", "wohl", "er\u00b7klang", "dein", "Ta\u00b7glied", "schrill", ","], "token_info": ["word", "wo