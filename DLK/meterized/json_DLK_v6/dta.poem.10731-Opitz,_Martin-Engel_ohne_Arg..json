{"dta.poem.10731": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Engel ohne Arg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Gantz Engelisch bistu an gstalt vnd angeberden/", "tokens": ["Gantz", "En\u00b7ge\u00b7lisch", "bis\u00b7tu", "an", "gstalt", "vnd", "an\u00b7ge\u00b7ber\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "APPR", "NN", "KON", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dazu dein Nahme will/ wen er durchmenget ist/", "tokens": ["Da\u00b7zu", "dein", "Nah\u00b7me", "will", "/", "wen", "er", "durch\u00b7men\u00b7get", "ist", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "VMFIN", "$(", "PWS", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Auch bringen an das licht/ wer du inwendig bist/", "tokens": ["Auch", "brin\u00b7gen", "an", "das", "licht", "/", "wer", "du", "in\u00b7wen\u00b7dig", "bist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "APPR", "ART", "NN", "$(", "PWS", "PPER", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Engel ohn\u2019 all arg soltu genennet werden.", "tokens": ["Ein", "En\u00b7gel", "ohn'", "all", "arg", "sol\u00b7tu", "ge\u00b7nen\u00b7net", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "AD