{"dta.poem.12764": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der winter im sommer,  \n oder  \n Die unverl\u00f6schliche flamme der liebe,  \n Bey dem Raderischen hochzeit-feste,  \n  an.  1687.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die nacht besiegte kaum den starcken sonnenschein,", "tokens": ["Die", "nacht", "be\u00b7sieg\u00b7te", "kaum", "den", "star\u00b7cken", "son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die sterne waren nur am himmel aufgestiegen;", "tokens": ["Die", "ster\u00b7ne", "wa\u00b7ren", "nur", "am", "him\u00b7mel", "auf\u00b7ge\u00b7stie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da wolte mich der schlaf schon mit gedancken wiegen,", "tokens": ["Da", "wol\u00b7te", "mich", "der", "schlaf", "schon", "mit", "ge\u00b7dan\u00b7cken", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und pr\u00e4gte durch den traum mir diese fabel ein:", "tokens": ["Und", "pr\u00e4g\u00b7te", "durch", "den", "traum", "mir", "die\u00b7se", "fa\u00b7bel", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PPER", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich sah den gro\u00dfen saal der g\u00f6tter offen stehn;", "tokens": ["Ich", "sah", "den", "gro\u00b7\u00dfen", "saal", "der", "g\u00f6t\u00b7ter", "of\u00b7fen", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hier blitzte Jupiter mit scharffen donner-keilen;", "tokens": ["Hier", "blitz\u00b7te", "Ju\u00b7pi\u00b7ter", "mit", "scharf\u00b7fen", "don\u00b7ner\u00b7kei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dort letzte Mavors sich an schweren krieges-pfeilen,", "tokens": ["Dort", "letz\u00b7te", "Ma\u00b7vors", "sich", "an", "schwe\u00b7ren", "krie\u00b7ge\u00b7spfei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und lie\u00df den trommel-klang durch alle wolcken gehn.", "tokens": ["Und", "lie\u00df", "den", "trom\u00b7mel\u00b7klang", "durch", "al\u00b7le", "wol\u00b7cken", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nicht weit von diesem ab sa\u00df Venus und Vulcan,", "tokens": ["Nicht", "weit", "von", "die\u00b7sem", "ab", "sa\u00df", "Ve\u00b7nus", "und", "Vul\u00b7can", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "PDAT", "PTKVZ", "VVFIN", "NN", "KON", "NE", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Und konten ohne scheu sich mit einander k\u00fcssen;", "tokens": ["Und", "kon\u00b7ten", "oh\u00b7ne", "scheu", "sich", "mit", "ein\u00b7an\u00b7der", "k\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "ADJD", "PRF", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Doch gleich als Pallas kam den engen krei\u00df zu schliessen,", "tokens": ["Doch", "gleich", "als", "Pal\u00b7las", "kam", "den", "en\u00b7gen", "krei\u00df", "zu", "schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Kam Eris auch zugleich mit ihrem apffel an.", "tokens": ["Kam", "E\u00b7ris", "auch", "zu\u00b7gleich", "mit", "ih\u00b7rem", "apf\u00b7fel", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sie warff ihn unverhofft in himmels-circkel ein,", "tokens": ["Sie", "warff", "ihn", "un\u00b7ver\u00b7hofft", "in", "him\u00b7mels\u00b7cir\u00b7ckel", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und rieff vor gro\u00dfer lust und freuden-vollen lachen:", "tokens": ["Und", "rieff", "vor", "gro\u00b7\u00dfer", "lust", "und", "freu\u00b7den\u00b7vol\u00b7len", "la\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "KON", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wer, g\u00f6tter! kan von euch im winter sommer machen,", "tokens": ["Wer", ",", "g\u00f6t\u00b7ter", "!", "kan", "von", "euch", "im", "win\u00b7ter", "som\u00b7mer", "ma\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "ADJA", "$.", "VMFIN", "APPR", "PPER", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "De\u00df soll dis theure gold und dieser apffel seyn.", "tokens": ["De\u00df", "soll", "dis", "theu\u00b7re", "gold", "und", "die\u00b7ser", "apf\u00b7fel", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PDS", "ADJA", "NN", "KON", "PDAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Mir, mir! schrie Jupiter, mir steht der apffel zu!", "tokens": ["Mir", ",", "mir", "!", "schrie", "Ju\u00b7pi\u00b7ter", ",", "mir", "steht", "der", "apf\u00b7fel", "zu", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "$.", "VVFIN", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Was kan wol hitziger als meine keile blitzen?", "tokens": ["Was", "kan", "wol", "hit\u00b7zi\u00b7ger", "als", "mei\u00b7ne", "kei\u00b7le", "blit\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADJD", "KOKOM", "PPOSAT", "PIAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Mu\u00df nicht der winter selbst vor meinem donner schwitzen?", "tokens": ["Mu\u00df", "nicht", "der", "win\u00b7ter", "selbst", "vor", "mei\u00b7nem", "don\u00b7ner", "schwit\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "St\u00f6rt nicht mein wetter-sturm auch Zemblens kalte ruh?", "tokens": ["St\u00f6rt", "nicht", "mein", "wet\u00b7ter\u00b7sturm", "auch", "Zem\u00b7blens", "kal\u00b7te", "ruh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "NN", "ADV", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Welch hagel oder ei\u00df d\u00e4mpfft Aetnens hei\u00dfe gluth?", "tokens": ["Welch", "ha\u00b7gel", "o\u00b7der", "ei\u00df", "d\u00e4mpfft", "A\u00b7e\u00b7tnens", "hei\u00b7\u00dfe", "gluth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "ADJD", "VVFIN", "NE", "ADJA", "NN", "$."], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.22": {"text": "Wenn darff Encelades die flammen nicht bewegen?", "tokens": ["Wenn", "darff", "En\u00b7ce\u00b7la\u00b7des", "die", "flam\u00b7men", "nicht", "be\u00b7we\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "NN", "ART", "VVFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "Wenn mu\u00df der heckel-berg die flammen niederlegen?", "tokens": ["Wenn", "mu\u00df", "der", "he\u00b7ckel\u00b7berg", "die", "flam\u00b7men", "nie\u00b7der\u00b7le\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "ART", "NN", "ART", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wenn weicht Vesuvius der st\u00e4rcksten regenfluth?", "tokens": ["Wenn", "weicht", "Ve\u00b7su\u00b7vius", "der", "st\u00e4rcks\u00b7ten", "re\u00b7gen\u00b7fluth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "NE", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Was? sprach der k\u00fchne Mars; dis ist nur schatten-spiel,", "tokens": ["Was", "?", "sprach", "der", "k\u00fch\u00b7ne", "Mars", ";", "dis", "ist", "nur", "schat\u00b7ten\u00b7spiel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$.", "VVFIN", "ART", "ADJA", "NN", "$.", "PDS", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Bringt schwere bomben her und schw\u00e4rmende carcassen,", "tokens": ["Bringt", "schwe\u00b7re", "bom\u00b7ben", "her", "und", "schw\u00e4r\u00b7men\u00b7de", "car\u00b7cas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "VVFIN", "PTKVZ", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "La\u00dft m\u00f6rsel und carthaun erz\u00f6rntes feuer fassen,", "tokens": ["La\u00dft", "m\u00f6r\u00b7sel", "und", "car\u00b7thaun", "er\u00b7z\u00f6rn\u00b7tes", "feu\u00b7er", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NE", "KON", "ADJD", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.28": {"text": "Sengt, brennet, schiest und trefft in das gesetzte ziel;", "tokens": ["Sengt", ",", "bren\u00b7net", ",", "schiest", "und", "trefft", "in", "das", "ge\u00b7setz\u00b7te", "ziel", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Denn geht und schaut die frucht von meinem eifer an,", "tokens": ["Denn", "geht", "und", "schaut", "die", "frucht", "von", "mei\u00b7nem", "ei\u00b7fer", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und sagt, wenn jung und alt in warmen blute schwitzen:", "tokens": ["Und", "sagt", ",", "wenn", "jung", "und", "alt", "in", "war\u00b7men", "blu\u00b7te", "schwit\u00b7zen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ADJD", "KON", "ADJD", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ob nicht mein pulver mehr als alle donner blitzen,", "tokens": ["Ob", "nicht", "mein", "pul\u00b7ver", "mehr", "als", "al\u00b7le", "don\u00b7ner", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "NN", "PIAT", "KOKOM", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und selbst den strengen Nord in hitze setzen kan?", "tokens": ["Und", "selbst", "den", "stren\u00b7gen", "Nord", "in", "hit\u00b7ze", "set\u00b7zen", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die Pallas sch\u00fctzte sich mit gleicher tapfferkeit", "tokens": ["Die", "Pal\u00b7las", "sch\u00fctz\u00b7te", "sich", "mit", "glei\u00b7cher", "tapf\u00b7fer\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und sprach: Ein kluger geist, der keiner last gewichen,", "tokens": ["Und", "sprach", ":", "Ein", "klu\u00b7ger", "geist", ",", "der", "kei\u00b7ner", "last", "ge\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ART", "ADJA", "NN", "$,", "PRELS", "PIS", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und den die tugend selbst mit anmuth angestrichen,", "tokens": ["Und", "den", "die", "tu\u00b7gend", "selbst", "mit", "an\u00b7muth", "an\u00b7ge\u00b7stri\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ist auch bey winters-zeit von flammen nicht befreyt.", "tokens": ["Ist", "auch", "bey", "win\u00b7ter\u00b7szeit", "von", "flam\u00b7men", "nicht", "be\u00b7freyt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "APPR", "VVFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Er spielt als diamant mit strahlen in der welt,", "tokens": ["Er", "spielt", "als", "di\u00b7a\u00b7mant", "mit", "strah\u00b7len", "in", "der", "welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJD", "APPR", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Vermischet wind und sturm mit holden fr\u00fchlings-blicken,", "tokens": ["Ver\u00b7mi\u00b7schet", "wind", "und", "sturm", "mit", "hol\u00b7den", "fr\u00fch\u00b7lings\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "KON", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und wei\u00df das gl\u00fccke selbst mit k\u00fcnsten zu ber\u00fccken:", "tokens": ["Und", "wei\u00df", "das", "gl\u00fc\u00b7cke", "selbst", "mit", "k\u00fcns\u00b7ten", "zu", "be\u00b7r\u00fc\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "VVFIN", "ADV", "APPR", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Drum sag ich, da\u00df der sieg auf meine seite f\u00e4llt.", "tokens": ["Drum", "sag", "ich", ",", "da\u00df", "der", "sieg", "auf", "mei\u00b7ne", "sei\u00b7te", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ich wei\u00df nicht, was auch schon der