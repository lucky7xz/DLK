{"textgrid.poem.31671": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nach dem Tod das Leben geben / steht allein der Tugend zu /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach dem Tod das Leben geben / steht allein der Tugend zu /", "tokens": ["Nach", "dem", "Tod", "das", "Le\u00b7ben", "ge\u00b7ben", "/", "steht", "al\u00b7lein", "der", "Tu\u00b7gend", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$(", "VVFIN", "ADV", "ART", "NN", "PTKZU", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "die kan Ruh' im Leben schaffen / und das Leben in der Ruh.", "tokens": ["die", "kan", "Ruh'", "im", "Le\u00b7ben", "schaf\u00b7fen", "/", "und", "das", "Le\u00b7ben", "in", "der", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "NN", "APPRART", "NN", "VVINF", "$(", "KON", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Leuchtet hier ihr Strahl in uns: nach dem Tod wird unser Nahme", "tokens": ["Leuch\u00b7tet", "hier", "ihr", "Strahl", "in", "uns", ":", "nach", "dem", "Tod", "wird", "un\u00b7ser", "Nah\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "APPR", "PPER", "$.", "APPR", "ART", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "ewiglich in ihr erhellt / als ein ungeleschte Flamme.", "tokens": ["e\u00b7wig\u00b7lich", "in", "ihr", "er\u00b7hellt", "/", "als", "ein", "un\u00b7ge\u00b7leschte", "Flam\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVFIN", "$(", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.5": {"text": "Ist demnach / des Tugend Lebens / Hydrens-Art die rechte Prob:", "tokens": ["Ist", "dem\u00b7nach", "/", "des", "Tu\u00b7gend", "Le\u00b7bens", "/", "Hyd\u00b7rens\u00b7Art", "die", "rech\u00b7te", "Prob", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "$(", "ART", "NN", "NN", "$(", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "zwey aus eines Tod entspringen: dort der Seele / hier das Lob.", "tokens": ["zwey", "aus", "ei\u00b7nes", "Tod", "ent\u00b7sprin\u00b7gen", ":", "dort", "der", "See\u00b7le", "/", "hier", "das", "Lob", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "APPR", "ART", "NN", "VVINF", "$.", "ADV", "ART", "NN", "$(", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}