{"dta.poem.12844": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Schifffarth der liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die liebe schiffte durch den Sund,", "tokens": ["Die", "lie\u00b7be", "schiff\u00b7te", "durch", "den", "Sund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr Pharus war der wohllust kertze,", "tokens": ["Ihr", "Pha\u00b7rus", "war", "der", "wohl\u00b7lust", "kert\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die muschel Amarillens mund,", "tokens": ["Die", "mu\u00b7schel", "A\u00b7ma\u00b7ril\u00b7lens", "mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die anfuhrt mein getreues hertze.", "tokens": ["Die", "an\u00b7fuhrt", "mein", "ge\u00b7treu\u00b7es", "hert\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}