{"textgrid.poem.32570": {"metadata": {"author": {"name": "Wieland, Christoph Martin", "birth": "N.A.", "death": "N.A."}, "title": "Ode. Klagen und Beruhigung", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, die unsterblich ist, der Seraphinen Schwester,", "tokens": ["Du", ",", "die", "uns\u00b7terb\u00b7lich", "ist", ",", "der", "Se\u00b7ra\u00b7phi\u00b7nen", "Schwes\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VAFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du, deren angebornen Glanz", "tokens": ["Du", ",", "de\u00b7ren", "an\u00b7ge\u00b7bor\u00b7nen", "Glanz"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch dunkler Stoff, des Todes Kleid verh\u00fcllet,", "tokens": ["Noch", "dunk\u00b7ler", "Stoff", ",", "des", "To\u00b7des", "Kleid", "ver\u00b7h\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Itzt Tier, doch einst ein Gott!", "tokens": ["Itzt", "Tier", ",", "doch", "einst", "ein", "Gott", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "F\u00fchlst du, o Seele, dich? Bebst du in deinen Banden", "tokens": ["F\u00fchlst", "du", ",", "o", "See\u00b7le", ",", "dich", "?", "Bebst", "du", "in", "dei\u00b7nen", "Ban\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "FM", "NN", "$,", "PPER", "$.", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Vom Thron gest\u00fcrzte K\u00f6nigin? \u2013", "tokens": ["Vom", "Thron", "ge\u00b7st\u00fcrz\u00b7te", "K\u00f6\u00b7ni\u00b7gin", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Oft angelockt, get\u00e4uscht und nie befriedigt", "tokens": ["Oft", "an\u00b7ge\u00b7lockt", ",", "ge\u00b7t\u00e4uscht", "und", "nie", "be\u00b7frie\u00b7digt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "$,", "VVPP", "KON", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wankt dein \u00e4therscher Blick", "tokens": ["Wankt", "dein", "\u00e4t\u00b7her\u00b7scher", "Blick"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Auf dieser Erd umher, dem Vaterland des Irrtums,", "tokens": ["Auf", "die\u00b7ser", "Erd", "um\u00b7her", ",", "dem", "Va\u00b7ter\u00b7land", "des", "Irr\u00b7tums", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKVZ", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Lasters m\u00f6rderischer Gruft!", "tokens": ["Des", "Las\u00b7ters", "m\u00f6r\u00b7de\u00b7ri\u00b7scher", "Gruft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo Traum und Wahrheit nur die Sonn entscheidet,", "tokens": ["Wo", "Traum", "und", "Wahr\u00b7heit", "nur", "die", "Sonn", "ent\u00b7schei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und selbst die Liebe ha\u00dft, \u2013", "tokens": ["Und", "selbst", "die", "Lie\u00b7be", "ha\u00dft", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mein zarter Wille bebt vorm Anblick der Verw\u00fcstung,", "tokens": ["Mein", "zar\u00b7ter", "Wil\u00b7le", "bebt", "vorm", "An\u00b7blick", "der", "Ver\u00b7w\u00fcs\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die dich einst, sch\u00f6ne Erd, entstellt.", "tokens": ["Die", "dich", "einst", ",", "sch\u00f6\u00b7ne", "Erd", ",", "ent\u00b7stellt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "ADV", "$,", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er, an der Brust der holden Lieb erzogen,", "tokens": ["Er", ",", "an", "der", "Brust", "der", "hol\u00b7den", "Lieb", "er\u00b7zo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Tr\u00e4gt nicht des Hasses Blick!", "tokens": ["Tr\u00e4gt", "nicht", "des", "Has\u00b7ses", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ach! die der Geister Herr zu gleichen Seligkeiten,", "tokens": ["Ach", "!", "die", "der", "Geis\u00b7ter", "Herr", "zu", "glei\u00b7chen", "Se\u00b7lig\u00b7kei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "ART", "NN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Tugend zu genie\u00dfen, schuf,", "tokens": ["Die", "Tu\u00b7gend", "zu", "ge\u00b7nie\u00b7\u00dfen", ",", "schuf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die jagt der Durst nach ungl\u00fcckselgen G\u00fctern", "tokens": ["Die", "jagt", "der", "Durst", "nach", "un\u00b7gl\u00fcck\u00b7sel\u00b7gen", "G\u00fc\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In einen ewgen Krieg!", "tokens": ["In", "ei\u00b7nen", "ew\u00b7gen", "Krieg", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wie wird dir, Seele, dann, wenn du voll Menschenliebe", "tokens": ["Wie", "wird", "dir", ",", "See\u00b7le", ",", "dann", ",", "wenn", "du", "voll", "Men\u00b7schen\u00b7lie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "NN", "$,", "ADV", "$,", "KOUS", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Herab auf dein Geschlechte blickst?", "tokens": ["Her\u00b7ab", "auf", "dein", "Ge\u00b7schlech\u00b7te", "blickst", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie, die du gern mit deinem Schmerz begl\u00fccktest,", "tokens": ["Sie", ",", "die", "du", "gern", "mit", "dei\u00b7nem", "Schmerz", "be\u00b7gl\u00fcck\u00b7test", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie sind des Elends Raub!", "tokens": ["Sie", "sind", "des", "E\u00b7lends", "Raub", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Du weinst! \u2013 Du breitest dich mit allen deinen W\u00fcnschen", "tokens": ["Du", "weinst", "!", "\u2013", "Du", "brei\u00b7test", "dich", "mit", "al\u00b7len", "dei\u00b7nen", "W\u00fcn\u00b7schen"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "PPER", "VVFIN", "PRF", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum g\u00f6ttlichsten Gesch\u00e4fte aus,", "tokens": ["Zum", "g\u00f6tt\u00b7lichs\u00b7ten", "Ge\u00b7sch\u00e4f\u00b7te", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Zum Wohltun! Der Gedank, Gl\u00fcck um dich her zu schaffen,", "tokens": ["Zum", "Wohl\u00b7tun", "!", "Der", "Ge\u00b7dank", ",", "Gl\u00fcck", "um", "dich", "her", "zu", "schaf\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "ART", "NN", "$,", "NN", "APPR", "PRF", "APZR", "PTKZU", "VVINF", "$,"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Schon der belohnte dich!", "tokens": ["Schon", "der", "be\u00b7lohn\u00b7te", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VVFIN", "PPER", "$."], "meter": "++-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und ach! was hindert dich? dein Wille bleibt nur W\u00fcnschen!", "tokens": ["Und", "ach", "!", "was", "hin\u00b7dert", "dich", "?", "dein", "Wil\u00b7le", "bleibt", "nur", "W\u00fcn\u00b7schen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PWS", "VVFIN", "PPER", "$.", "PPOSAT", "NN", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du, die des Himmels Grenzen selbst", "tokens": ["Du", ",", "die", "des", "Him\u00b7mels", "Gren\u00b7zen", "selbst"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit der Gedanken Seraphs-Flug erreichet,", "tokens": ["Mit", "der", "Ge\u00b7dan\u00b7ken", "Se\u00b7ra\u00b7phs\u00b7Flug", "er\u00b7rei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Bist arm an \u00e4u\u00dfrer Kraft.", "tokens": ["Bist", "arm", "an", "\u00e4u\u00df\u00b7rer", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Besch\u00e4mt, best\u00fcrzt, verirrt im Labyrinth des Schicksals", "tokens": ["Be\u00b7sch\u00e4mt", ",", "be\u00b7st\u00fcrzt", ",", "ver\u00b7irrt", "im", "La\u00b7by\u00b7rinth", "des", "Schick\u00b7sals"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "$,", "VVPP", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entfliehst du in dich selbst \u2013 doch ach!", "tokens": ["Ent\u00b7fliehst", "du", "in", "dich", "selbst", "\u2013", "doch", "ach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "ADV", "$(", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch in dich selbst, auch in den Sitz der Liebe", "tokens": ["Auch", "in", "dich", "selbst", ",", "auch", "in", "den", "Sitz", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "ADV", "$,", "ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verfolgt der Kummer dich!", "tokens": ["Ver\u00b7folgt", "der", "Kum\u00b7mer", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ich f\u00fchl es, wie du oft die leichtern Fl\u00fcgel schwingest", "tokens": ["Ich", "f\u00fchl", "es", ",", "wie", "du", "oft", "die", "leich\u00b7tern", "Fl\u00fc\u00b7gel", "schwin\u00b7gest"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In reinre Gegenden empor,", "tokens": ["In", "rein\u00b7re", "Ge\u00b7gen\u00b7den", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch dr\u00fcckt ein innrer Hang, der Feind des Wesens,", "tokens": ["Doch", "dr\u00fcckt", "ein", "inn\u00b7rer", "Hang", ",", "der", "Feind", "des", "We\u00b7sens", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein Nichts, dich stets zur\u00fcck! \u2013", "tokens": ["Dein", "Nichts", ",", "dich", "stets", "zu\u00b7r\u00fcck", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "O Sch\u00f6pfer! ist denn hier der Wunsch der Ruh Verbrechen?", "tokens": ["O", "Sch\u00f6p\u00b7fer", "!", "ist", "denn", "hier", "der", "Wunsch", "der", "Ruh", "Ver\u00b7bre\u00b7chen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VAFIN", "ADV", "ADV", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie? Oder ist der Freude Fu\u00df", "tokens": ["Wie", "?", "O\u00b7der", "ist", "der", "Freu\u00b7de", "Fu\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "NE", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur an des Himmels goldne Flur geheftet,", "tokens": ["Nur", "an", "des", "Him\u00b7mels", "gold\u00b7ne", "Flur", "ge\u00b7hef\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo du sie l\u00e4chelnd schufst?", "tokens": ["Wo", "du", "sie", "l\u00e4\u00b7chelnd", "schufst", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Soll auch der Edlere den die Syrenen Kehle", "tokens": ["Soll", "auch", "der", "Ed\u00b7le\u00b7re", "den", "die", "Sy\u00b7re\u00b7nen", "Keh\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN", "ART", "ART", "ADJA", "NN"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Der Erden Freude nicht entz\u00fcckt", "tokens": ["Der", "Er\u00b7den", "Freu\u00b7de", "nicht", "ent\u00b7z\u00fcckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PTKNEG", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der, sich bewu\u00dft, tief unter seinen W\u00fcnschen", "tokens": ["Der", ",", "sich", "be\u00b7wu\u00dft", ",", "tief", "un\u00b7ter", "sei\u00b7nen", "W\u00fcn\u00b7schen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "PRF", "ADJD", "$,", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Thronen Schimmer sieht,", "tokens": ["Der", "Thro\u00b7nen", "Schim\u00b7mer", "sieht", ","], "token_info": ["word", "word", "word