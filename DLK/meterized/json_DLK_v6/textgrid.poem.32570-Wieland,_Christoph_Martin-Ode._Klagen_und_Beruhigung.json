], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In deinem Umgang, z\u00e4rtliche Serena,", "tokens": ["In", "dei\u00b7nem", "Um\u00b7gang", ",", "z\u00e4rt\u00b7li\u00b7che", "Se\u00b7re\u00b7na", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJA", "NE", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Dir und dem Himmel nur!", "tokens": ["Dir", "und", "dem", "Him\u00b7mel", "nur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "NN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.23": {"line.1": {"text": "Zwar von der Welt getrennt, in einer holden Wildnis", "tokens": ["Zwar", "von", "der", "Welt", "ge\u00b7trennt", ",", "in", "ei\u00b7ner", "hol\u00b7den", "Wild\u00b7nis"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "(gleich dem begl\u00fcckten Aufenthalt,", "tokens": ["(", "gleich", "dem", "be\u00b7gl\u00fcck\u00b7ten", "Auf\u00b7ent\u00b7halt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo Billeter mein Herz mir abgewonnen,", "tokens": ["Wo", "Bil\u00b7le\u00b7ter", "mein", "Herz", "mir", "ab\u00b7ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo deine Unschuld mich,", "tokens": ["Wo", "dei\u00b7ne", "Un\u00b7schuld", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.24": {"line.1": {"text": "O Daphne, still entz\u00fcckt, wo ich den Freund gesegnet,", "tokens": ["O", "Daph\u00b7ne", ",", "still", "ent\u00b7z\u00fcckt", ",", "wo", "ich", "den", "Freund", "ge\u00b7seg\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJD", "VVPP", "$,", "PWAV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den dein geliebtes Herz begl\u00fcckt \u2013)", "tokens": ["Den", "dein", "ge\u00b7lieb\u00b7tes", "Herz", "be\u00b7gl\u00fcckt", "\u2013", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVPP", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch nicht allein! oft von den jungen Nymphen", "tokens": ["Doch", "nicht", "al\u00b7lein", "!", "oft", "von", "den", "jun\u00b7gen", "Nym\u00b7phen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ADV", "$.", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+---", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Des Eichenwalds gesehn", "tokens": ["Des", "Ei\u00b7chen\u00b7walds", "ge\u00b7sehn"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.25": {"line.1": {"text": "Zuweilen gingen wir mit Siphas holden T\u00f6chtern", "tokens": ["Zu\u00b7wei\u00b7len", "gin\u00b7gen", "wir", "mit", "Si\u00b7phas", "hol\u00b7den", "T\u00f6ch\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am See, der Evens Spiegel war;", "tokens": ["Am", "See", ",", "der", "E\u00b7vens", "Spie\u00b7gel", "war", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "NE", "NE", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald macht ein Freund, ein He\u00df, uns mit der Weisheit", "tokens": ["Bald", "macht", "ein", "Freund", ",", "ein", "He\u00df", ",", "uns", "mit", "der", "Weis\u00b7heit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch sein Gespr\u00e4ch vertraut.", "tokens": ["Durch", "sein", "Ge\u00b7spr\u00e4ch", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.26": {"line.1": {"text": "Ja unsre Seligkeit entlockte selbst die Engel", "tokens": ["Ja", "uns\u00b7re", "Se\u00b7lig\u00b7keit", "ent\u00b7lock\u00b7te", "selbst", "die", "En\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gott n\u00e4hern Welten, uns zu sehn;", "tokens": ["Gott", "n\u00e4\u00b7hern", "Wel\u00b7ten", ",", "uns", "zu", "sehn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir h\u00f6rten denn in hellen Mittern\u00e4chten", "tokens": ["Wir", "h\u00f6r\u00b7ten", "denn", "in", "hel\u00b7len", "Mit\u00b7ter\u00b7n\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word