{"dta.poem.812": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XlIX.  \n Ewige Frewde der Au\u00dferwehlten.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.71", "pl:0.28"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "O! wo bin ich! O was seh' ich! wach ich! treumt mir? wie ", "tokens": ["O", "!", "wo", "bin", "ich", "!", "O", "was", "seh'", "ich", "!", "wach", "ich", "!", "treumt", "mir", "?", "wie"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "$.", "PWAV", "VAFIN", "PPER", "$.", "ITJ", "PWS", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "$.", "PWAV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "wird mir ", "tokens": ["wird", "mir"], "token_info": ["word", "word"], "pos": ["VAFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "IeSV! welcher wollust Meer/ \u00fcberschwemmt mein fr\u00f6- ", "tokens": ["IeSV", "!", "wel\u00b7cher", "wol\u00b7lust", "Meer", "/", "\u00fc\u00b7bersc\u00b7hwemmt", "mein", "fr\u00f6"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "PWAT", "VMFIN", "NN", "$(", "VVFIN", "PPOSAT", "TRUNC"], "meter": "-+-+-+----+", "measure": "unknown.measure.tetra"}, "line.4": {"text": "lich Hertz/ ", "tokens": ["lich", "Hertz", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Welt Ade! gl\u00fcck zu mein trost! gutte Nacht todt angst vnd", "tokens": ["Welt", "A\u00b7de", "!", "gl\u00fcck", "zu", "mein", "trost", "!", "gut\u00b7te", "Nacht", "todt", "angst", "vnd"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "$.", "NN", "APPR", "PPOSAT", "NN", "$.", "ADJA", "NN", "ADJD", "VVPP", "KON"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "schmertz/ ", "tokens": ["schmertz", "/"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Ich find alles. alles lern ich! alles schaw\u2019 ich HERR in die", "tokens": ["Ich", "find", "al\u00b7les", ".", "al\u00b7les", "lern", "ich", "!", "al\u00b7les", "scha\u00b7w'", "ich", "HeRR", "in", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "$.", "PIS", "VVFIN", "PPER", "$.", "PIS", "VVFIN", "PPER", "NN", "APPR", "ART"], "meter": "-+--+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich zuschmeltz in lautter wonne! ", "tokens": ["Ich", "zu\u00b7schmeltz", "in", "laut\u00b7ter", "won\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O wie herrlich ists hier seyn! Erde deine Frewd ist schertz!", "tokens": ["O", "wie", "herr\u00b7lich", "ists", "hier", "seyn", "!", "Er\u00b7de", "dei\u00b7ne", "Frewd", "ist", "schertz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VAFIN", "ADV", "VAINF", "$.", "NN", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "IeSu! ewig-gl\u00e4ntzend Licht! (tunckel ist der So&#241;en kertz!) ", "tokens": ["Ie\u00b7Su", "!", "e\u00b7wig\u00b7gl\u00e4nt\u00b7zend", "Licht", "!", "(", "tun\u00b7ckel", "ist", "der", "So", "&#241;", "en", "kertz", "!", ")"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "XML_entity", "word", "word", "punct", "punct"], "pos": ["NE", "$.", "ADJD", "NN", "$.", "$(", "ADJD", "VAFIN", "ART", "ADV", "$(", "FM", "FM", "$.", "$("], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Ach! wie funckeln deine Scharen! Sternen fliht! hier schim-", "tokens": ["Ach", "!", "wie", "fun\u00b7ckeln", "dei\u00b7ne", "Scha\u00b7ren", "!", "Ster\u00b7nen", "fliht", "!", "hier", "schim"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$.", "PWAV", "VVFIN", "PPOSAT", "NN", "$.", "NN", "VVFIN", "$.", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "mern wir. ", "tokens": ["mern", "wir", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.4": {"line.1": {"text": "Jhr die jhr glutt v\u00f1 Schwerd verlacht! ob schon ewer Leib", "tokens": ["Ihr", "die", "jhr", "glutt", "v\u00f1", "Schwerd", "ver\u00b7lacht", "!", "ob", "schon", "e\u00b7wer", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ART", "PPER", "ADJD", "ADJA", "NN", "VVPP", "$.", "KOUS", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-++-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "wurd staub vnd aschen/ ", "tokens": ["wurd", "staub", "vnd", "asc\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Jhr die jhr ewer reines kleid habt in dem Blutt de\u00df Lambs", "tokens": ["Ihr", "die", "jhr", "e\u00b7wer", "rei\u00b7nes", "kleid", "habt", "in", "dem", "Blutt", "de\u00df", "Lambs"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "PPER", "PPOSAT", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "gewaschen/ ", "tokens": ["ge\u00b7wa\u00b7schen", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Rufft Halleluja! Halleluja! frewd vnd leben! ", "tokens": ["Rufft", "Hal\u00b7le\u00b7lu\u00b7ja", "!", "Hal\u00b7le\u00b7lu\u00b7ja", "!", "frewd", "vnd", "le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$.", "ITJ", "$.", "NN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Dir dreymal einig Ewigkeit; die alles in allen allm\u00e4chtig", "tokens": ["Dir", "drey\u00b7mal", "ei\u00b7nig", "E\u00b7wig\u00b7keit", ";", "die", "al\u00b7les", "in", "al\u00b7len", "all\u00b7m\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJD", "NN", "$.", "ART", "PIS", "APPR", "PIAT", "ADJD"], "meter": "-+-+-+-+-+--+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "regiret: ", "tokens": ["re\u00b7gi\u00b7ret", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Sey vnau\u00dfsprechlich Lob vnd ruhm/ vnd Ehre die dir nur", "tokens": ["Sey", "vn\u00b7au\u00df\u00b7sprech\u00b7lich", "Lob", "vnd", "ruhm", "/", "vnd", "Eh\u00b7re", "die", "dir", "nur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "NN", "KON", "NN", "$(", "KON", "NN", "ART", "PPER", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "alleine geb\u00fchret. ", "tokens": ["al\u00b7lei\u00b7ne", "ge\u00b7b\u00fch\u00b7ret", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Dir/ die sich ewig/ (Halleluja!) vns wil geben. ", "tokens": ["Dir", "/", "die", "sich", "e\u00b7wig", "/", "(", "Hal\u00b7le\u00b7lu\u00b7ja", "!", ")", "vns", "wil", "ge\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PRELS", "PRF", "ADJD", "$(", "$(", "ITJ", "$.", "$(", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}