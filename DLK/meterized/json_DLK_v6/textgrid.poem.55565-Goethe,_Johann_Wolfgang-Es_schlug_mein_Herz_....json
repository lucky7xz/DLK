{"textgrid.poem.55565": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Es schlug mein Herz ...", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es schlug mein Herz, geschwind zu Pferd!", "tokens": ["Es", "schlug", "mein", "Herz", ",", "ge\u00b7schwind", "zu", "Pferd", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und fort! wild, wie ein Held zur Schlacht.", "tokens": ["Und", "fort", "!", "wild", ",", "wie", "ein", "Held", "zur", "Schlacht", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "$.", "ADJD", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Abend wiegte schon die Erde,", "tokens": ["Der", "A\u00b7bend", "wieg\u00b7te", "schon", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und an den Bergen hing die Nacht;", "tokens": ["Und", "an", "den", "Ber\u00b7gen", "hing", "die", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schon stund im Nebelkleid die Eiche", "tokens": ["Schon", "stund", "im", "Ne\u00b7bel\u00b7kleid", "die", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie ein get\u00fcrmter Riese da,", "tokens": ["Wie", "ein", "ge\u00b7t\u00fcrm\u00b7ter", "Rie\u00b7se", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo Finsternis aus dem Gestr\u00e4uche", "tokens": ["Wo", "Fins\u00b7ter\u00b7nis", "aus", "dem", "Ge\u00b7str\u00e4u\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit hundert schwarzen Augen sah.", "tokens": ["Mit", "hun\u00b7dert", "schwar\u00b7zen", "Au\u00b7gen", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Mond von einem Wolkenh\u00fcgel", "tokens": ["Der", "Mond", "von", "ei\u00b7nem", "Wol\u00b7ken\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sah schl\u00e4frig aus dem Duft hervor.", "tokens": ["Sah", "schl\u00e4f\u00b7rig", "aus", "dem", "Duft", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}