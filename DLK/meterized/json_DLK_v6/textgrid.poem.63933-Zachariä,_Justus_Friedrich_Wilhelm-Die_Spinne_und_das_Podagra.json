{"textgrid.poem.63933": {"metadata": {"author": {"name": "Zachari\u00e4, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Spinne und das Podagra", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Podagra und eine Spinne,", "tokens": ["Das", "Po\u00b7da\u00b7gra", "und", "ei\u00b7ne", "Spin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gef\u00fchrt von ihrem Eigensinne,", "tokens": ["Ge\u00b7f\u00fchrt", "von", "ih\u00b7rem", "Ei\u00b7gen\u00b7sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Entschlossen sich, die Welt zu sehn", "tokens": ["Ent\u00b7schlos\u00b7sen", "sich", ",", "die", "Welt", "zu", "sehn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "$,", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Abenteuern nachzugehn.", "tokens": ["Und", "A\u00b7bent\u00b7eu\u00b7ern", "nach\u00b7zu\u00b7gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie trafen unterwegs sich an,", "tokens": ["Sie", "tra\u00b7fen", "un\u00b7ter\u00b7wegs", "sich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und gr\u00fc\u00dften sich, da sie sich sahn,", "tokens": ["Und", "gr\u00fc\u00df\u00b7ten", "sich", ",", "da", "sie", "sich", "sahn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So leicht, so artig und galant,", "tokens": ["So", "leicht", ",", "so", "ar\u00b7tig", "und", "ga\u00b7lant", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als h\u00e4tten sie sich l\u00e4ngst gekannt.", "tokens": ["Als", "h\u00e4t\u00b7ten", "sie", "sich", "l\u00e4ngst", "ge\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ich d\u00e4chte (sprach das Podagra),", "tokens": ["Ich", "d\u00e4ch\u00b7te", "(", "sprach", "das", "Po\u00b7da\u00b7gra", ")", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$(", "VVFIN", "ART", "NN", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wir setzten nach dem Dorfe da", "tokens": ["Wir", "setz\u00b7ten", "nach", "dem", "Dor\u00b7fe", "da"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Zusammen unsre Reise fort.", "tokens": ["Zu\u00b7sam\u00b7men", "uns\u00b7re", "Rei\u00b7se", "fort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Es scheint ein wohlgelegner Ort,", "tokens": ["Es", "scheint", "ein", "wohl\u00b7ge\u00b7leg\u00b7ner", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und sind Madam so m\u00fcd' als ich,", "tokens": ["Und", "sind", "Ma\u00b7dam", "so", "m\u00fcd'", "als", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "ADV", "ADJD", "KOKOM", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "So wird uns Beiden, sicherlich,", "tokens": ["So", "wird", "uns", "Bei\u00b7den", ",", "si\u00b7cher\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Jedwede Herberg, gro\u00df und klein,", "tokens": ["Jed\u00b7we\u00b7de", "Her\u00b7berg", ",", "gro\u00df", "und", "klein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.16": {"text": "Auf diese Nacht willkommen seyn.", "tokens": ["Auf", "die\u00b7se", "Nacht", "will\u00b7kom\u00b7men", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Der Spinne war das eben recht.", "tokens": ["Der", "Spin\u00b7ne", "war", "das", "e\u00b7ben", "recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PDS", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Sie kamen an das Dorf. Geschw\u00e4cht,", "tokens": ["Sie", "ka\u00b7men", "an", "das", "Dorf", ".", "Ge\u00b7schw\u00e4cht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$.", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Hinf\u00e4llig, kraftlos und halb lahm", "tokens": ["Hin\u00b7f\u00e4l\u00b7lig", ",", "kraft\u00b7los", "und", "halb", "lahm"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "PTKVZ"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Erlag das Podagra und nahm,", "tokens": ["Er\u00b7lag", "das", "Po\u00b7da\u00b7gra", "und", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "So bald als m\u00f6glich, voll Begier,", "tokens": ["So", "bald", "als", "m\u00f6g\u00b7lich", ",", "voll", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOKOM", "ADJD", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Beim ersten Bauer das Quartier.", "tokens": ["Beim", "ers\u00b7ten", "Bau\u00b7er", "das", "Quar\u00b7tier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.23": {"text": "Die Spinne hielt sich f\u00fcr gescheuter", "tokens": ["Die", "Spin\u00b7ne", "hielt", "sich", "f\u00fcr", "ge\u00b7scheu\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Und nahm den Weg noch etwas weiter,", "tokens": ["Und", "nahm", "den", "Weg", "noch", "et\u00b7was", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Bis zu des Edelmannes Haus;", "tokens": ["Bis", "zu", "des", "E\u00b7del\u00b7man\u00b7nes", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Hier w\u00e4hlt' sie einen Saal sich aus,", "tokens": ["Hier", "w\u00e4hlt'", "sie", "ei\u00b7nen", "Saal", "sich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "In welchem man mit gro\u00dfem Prachte", "tokens": ["In", "wel\u00b7chem", "man", "mit", "gro\u00b7\u00dfem", "Prach\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PIS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Zu einem Gastmahl Anstalt machte.", "tokens": ["Zu", "ei\u00b7nem", "Gast\u00b7mahl", "An\u00b7stalt", "mach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Sogleich nahm sie nach ihrem Witz", "tokens": ["Sog\u00b7leich", "nahm", "sie", "nach", "ih\u00b7rem", "Witz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Von einem Fensterrahm Besitz;", "tokens": ["Von", "ei\u00b7nem", "Fens\u00b7ter\u00b7rahm", "Be\u00b7sitz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Hub an, mit emsigem Bestreben,", "tokens": ["Hub", "an", ",", "mit", "em\u00b7si\u00b7gem", "Be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Viel ihrer F\u00e4den anzukleben.", "tokens": ["Viel", "ih\u00b7rer", "F\u00e4\u00b7den", "an\u00b7zu\u00b7kle\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Doch eh' ihr Netz noch fertig war,", "tokens": ["Doch", "eh'", "ihr", "Netz", "noch", "fer\u00b7tig", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Nimmt eine Stubenmagd es wahr,", "tokens": ["Nimmt", "ei\u00b7ne", "Stu\u00b7ben\u00b7magd", "es", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Die mit dem Besen d'r\u00fcber f\u00e4hrt", "tokens": ["Die", "mit", "dem", "Be\u00b7sen", "d'\u00b7r\u00fcber", "f\u00e4hrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.36": {"text": "Und unbarmherzig es zerst\u00f6rt.", "tokens": ["Und", "un\u00b7barm\u00b7her\u00b7zig", "es", "zer\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Die Spinne hub von Neuem an", "tokens": ["Die", "Spin\u00b7ne", "hub", "von", "Neu\u00b7em", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Zu weben, wie sie erst gethan;", "tokens": ["Zu", "we\u00b7ben", ",", "wie", "sie", "erst", "ge\u00b7than", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PWAV", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Da ward der Saal voll Herr'n und Damen,", "tokens": ["Da", "ward", "der", "Saal", "voll", "Herr'n", "und", "Da\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Mit denen viel Lakaien kamen.", "tokens": ["Mit", "de\u00b7nen", "viel", "La\u00b7kai\u00b7en", "ka\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Ein naseweiser Bursche sah", "tokens": ["Ein", "na\u00b7se\u00b7wei\u00b7ser", "Bur\u00b7sche", "sah"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Der Spinne Netz, und rief: Sieh' da,", "tokens": ["Der", "Spin\u00b7ne", "Netz", ",", "und", "rief", ":", "Sieh'", "da", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "KON", "VVFIN", "$.", "VVIMP", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Was machst du hier? und stie\u00df sogleich", "tokens": ["Was", "machst", "du", "hier", "?", "und", "stie\u00df", "sog\u00b7leich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$.", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Den Hut quer durch ihr Fadenreich.", "tokens": ["Den", "Hut", "quer", "durch", "ihr", "Fa\u00b7den\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Die Spinne lie\u00df sich's nicht verdrie\u00dfen,", "tokens": ["Die", "Spin\u00b7ne", "lie\u00df", "sich's", "nicht", "ver\u00b7drie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Und heftete mit muntern F\u00fc\u00dfen", "tokens": ["Und", "hef\u00b7te\u00b7te", "mit", "mun\u00b7tern", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Ihr hangend halbzerst\u00f6rtes Nest", "tokens": ["Ihr", "han\u00b7gend", "halb\u00b7zer\u00b7st\u00f6r\u00b7tes", "Nest"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Zum dritten Mal am Fenster fest.", "tokens": ["Zum", "drit\u00b7ten", "Mal", "am", "Fens\u00b7ter", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Da trat ein junges Fr\u00e4ulein her,", "tokens": ["Da", "trat", "ein", "jun\u00b7ges", "Fr\u00e4u\u00b7lein", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Das sah am Fenster ungef\u00e4hr", "tokens": ["Das", "sah", "am", "Fens\u00b7ter", "un\u00b7ge\u00b7f\u00e4hr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Die Spinne hangen, und schrie laut:", "tokens": ["Die", "Spin\u00b7ne", "han\u00b7gen", ",", "und", "schrie", "laut", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Ach, Herr Baron, mir graut, mir graut!", "tokens": ["Ach", ",", "Herr", "Ba\u00b7ron", ",", "mir", "graut", ",", "mir", "graut", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "NN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Und wies mit Schrecken auf die Spinne.", "tokens": ["Und", "wies", "mit", "Schre\u00b7cken", "auf", "die", "Spin\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "Kaum ward der Herr Baron sie inne,", "tokens": ["Kaum", "ward", "der", "Herr", "Ba\u00b7ron", "sie", "in\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.55": {"text": "So zog er wie ein Held den Degen,", "tokens": ["So", "zog", "er", "wie", "ein", "Held", "den", "De\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.56": {"text": "Fing an im Netz herumzufegen,", "tokens": ["Fing", "an", "im", "Netz", "her\u00b7um\u00b7zu\u00b7fe\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.57": {"text": "So da\u00df mit Noth die Spinn' entkam", "tokens": ["So", "da\u00df", "mit", "Noth", "die", "Spinn'", "ent\u00b7kam"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "APPR", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.58": {"text": "Und aus dem Saal den Abschied nahm.", "tokens": ["Und", "aus", "dem", "Saal", "den", "Ab\u00b7schied", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dem Podagra ging's fast auch so,", "tokens": ["Dem", "Po\u00b7da\u00b7gra", "ging's", "fast", "auch", "so", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es ward der Herberg wenig froh.", "tokens": ["Es", "ward", "der", "Her\u00b7berg", "we\u00b7nig", "froh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nachdem es lange g'nug gesessen,", "tokens": ["Nach\u00b7dem", "es", "lan\u00b7ge", "g'\u00b7nug", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sprach es: Ich m\u00f6cht' ein wenig essen!", "tokens": ["Sprach", "es", ":", "Ich", "m\u00f6cht'", "ein", "we\u00b7nig", "es\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPER", "VMFIN", "ART", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Bauer brachte trocken Brod,", "tokens": ["Der", "Bau\u00b7er", "brach\u00b7te", "tro\u00b7cken", "Brod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zum Trunk dazu kalt Wasser bot;", "tokens": ["Zum", "Trunk", "da\u00b7zu", "kalt", "Was\u00b7ser", "bot", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PAV", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dies waren nach so langen Reisen", "tokens": ["Dies", "wa\u00b7ren", "nach", "so", "lan\u00b7gen", "Rei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "F\u00fcr's Podagra sehr schlechte Speisen.", "tokens": ["F\u00fcr's", "Po\u00b7da\u00b7gra", "sehr", "schlech\u00b7te", "Spei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Es a\u00df nicht viel, trank kaum dazu,", "tokens": ["Es", "a\u00df", "nicht", "viel", ",", "trank", "kaum", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$,", "VVFIN", "ADV", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und sprach betr\u00fcbt: Bringt mich zur Ruh'!", "tokens": ["Und", "sprach", "be\u00b7tr\u00fcbt", ":", "Bringt", "mich", "zur", "Ruh'", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVPP", "$.", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Da wies der Bauer ihm zum Bette", "tokens": ["Da", "wies", "der", "Bau\u00b7er", "ihm", "zum", "Bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Gar eine harte Lagerst\u00e4tte,", "tokens": ["Gar", "ei\u00b7ne", "har\u00b7te", "La\u00b7ger\u00b7st\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Worauf ein wenig Stroh nur lag.", "tokens": ["Wo\u00b7rauf", "ein", "we\u00b7nig", "Stroh", "nur", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Hier lag es kl\u00e4glich, bis der Tag", "tokens": ["Hier", "lag", "es", "kl\u00e4g\u00b7lich", ",", "bis", "der", "Tag"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Im Osten an zu grauen fing,", "tokens": ["Im", "Os\u00b7ten", "an", "zu", "grau\u00b7en", "fing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und seufzend es von dannen ging.", "tokens": ["Und", "seuf\u00b7zend", "es", "von", "dan\u00b7nen", "ging", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es traf die Spinne wieder an,", "tokens": ["Es", "traf", "die", "Spin\u00b7ne", "wie\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die auch kein Auge zugethan;", "tokens": ["Die", "auch", "kein", "Au\u00b7ge", "zu\u00b7ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und alle Beide klagten sich,", "tokens": ["Und", "al\u00b7le", "Bei\u00b7de", "klag\u00b7ten", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PIS", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie elend und wie j\u00e4mmerlich", "tokens": ["Wie", "e\u00b7lend", "und", "wie", "j\u00e4m\u00b7mer\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "PWAV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie beiderseits die vor'ge Nacht", "tokens": ["Sie", "bei\u00b7der\u00b7seits", "die", "vor'\u00b7ge", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Furcht und Sorgen zugebracht;", "tokens": ["In", "Furcht", "und", "Sor\u00b7gen", "zu\u00b7ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich seh' wohl, wo der Knoten sitzt,", "tokens": ["Ich", "seh'", "wohl", ",", "wo", "der", "Kno\u00b7ten", "sitzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "(sprach d'rauf das Podagra). Dir n\u00fctzt", "tokens": ["(", "sprach", "d'\u00b7rauf", "das", "Po\u00b7da\u00b7gra", ")", ".", "Dir", "n\u00fctzt"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["$(", "VVFIN", "PAV", "ART", "NN", "$(", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Zum Aufenthalte kein Palast;", "tokens": ["Zum", "Auf\u00b7ent\u00b7hal\u00b7te", "kein", "Pa\u00b7last", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So wie ich niemals Ruh' und Rast", "tokens": ["So", "wie", "ich", "nie\u00b7mals", "Ruh'", "und", "Rast"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PPER", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Bei schlechten Bauern finden kann.", "tokens": ["Bei", "schlech\u00b7ten", "Bau\u00b7ern", "fin\u00b7den", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "D'rum geh' du zu dem armen Mann,", "tokens": ["D'\u00b7rum", "geh'", "du", "zu", "dem", "ar\u00b7men", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Und ich will deinen Junker sehn,", "tokens": ["Und", "ich", "will", "dei\u00b7nen", "Jun\u00b7ker", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "So soll das Ding wohl besser gehn.", "tokens": ["So", "soll", "das", "Ding", "wohl", "bes\u00b7ser", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dies waren Beide wohl zufrieden,", "tokens": ["Dies", "wa\u00b7ren", "Bei\u00b7de", "wohl", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Beide gingen nun verschieden", "tokens": ["Und", "Bei\u00b7de", "gin\u00b7gen", "nun", "ver\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Weg, so wie der Abend kam.", "tokens": ["Den", "Weg", ",", "so", "wie", "der", "A\u00b7bend", "kam", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Podagra, voll Hoffnung, nahm", "tokens": ["Das", "Po\u00b7da\u00b7gra", ",", "voll", "Hoff\u00b7nung", ",", "nahm"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ADJD", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zum Schlo\u00df des Junkers seinen Gang.", "tok