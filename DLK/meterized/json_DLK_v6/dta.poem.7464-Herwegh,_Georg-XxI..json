{"dta.poem.7464": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XxI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Freiheit, Freiheit! Nicht wo Hymnen schallen,", "tokens": ["O", "Frei\u00b7heit", ",", "Frei\u00b7heit", "!", "Nicht", "wo", "Hym\u00b7nen", "schal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$.", "PTKNEG", "PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In reichgeschm\u00fcckten f\u00fcrstlichen Arkaden \u2014", "tokens": ["In", "reich\u00b7ge\u00b7schm\u00fcck\u00b7ten", "f\u00fcrst\u00b7li\u00b7chen", "Ar\u00b7ka\u00b7den"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Freiheit! Du wohnst an einsamen Gestaden,", "tokens": ["Frei\u00b7heit", "!", "Du", "wohnst", "an", "ein\u00b7sa\u00b7men", "Ge\u00b7sta\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Und liebst die Stille, wie die Nachtigallen.", "tokens": ["Und", "liebst", "die", "Stil\u00b7le", ",", "wie", "die", "Nach\u00b7ti\u00b7gal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du fliehest das Ger\u00e4usch der Marmorhallen,", "tokens": ["Du", "flie\u00b7hest", "das", "Ge\u00b7r\u00e4usch", "der", "Mar\u00b7mo\u00b7rhal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo trunkne Schlemmer sich im Weine baden,", "tokens": ["Wo", "trunk\u00b7ne", "Schlem\u00b7mer", "sich", "im", "Wei\u00b7ne", "ba\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du l\u00e4\u00dft in H\u00fctten dich zu Gaste laden,", "tokens": ["Du", "l\u00e4\u00dft", "in", "H\u00fct\u00b7ten", "dich", "zu", "Gas\u00b7te", "la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo Thr\u00e4nen in die leeren Becher fallen.", "tokens": ["Wo", "Thr\u00e4\u00b7nen", "in", "die", "lee\u00b7ren", "Be\u00b7cher", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein Engel nahst Du bei verschlo\u00dfnen Th\u00fcren,", "tokens": ["Ein", "En\u00b7gel", "nahst", "Du", "bei", "ver\u00b7schlo\u00df\u00b7nen", "Th\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Stellst l\u00e4chelnd Dich an Deiner Treuen Bette,", "tokens": ["Stellst", "l\u00e4\u00b7chelnd", "Dich", "an", "Dei\u00b7ner", "Treu\u00b7en", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und horchst der himmlischen Musik der Kette.", "tokens": ["Und", "horchst", "der", "himm\u00b7li\u00b7schen", "Mu\u00b7sik", "der", "Ket\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nicht stolze Tempel wollen Dir geb\u00fchren,", "tokens": ["Nicht", "stol\u00b7ze", "Tem\u00b7pel", "wol\u00b7len", "Dir", "ge\u00b7b\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "VMFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Drin wir als Opfer unsern Stolz Dir bieten \u2014", "tokens": ["Drin", "wir", "als", "Op\u00b7fer", "un\u00b7sern", "Stolz", "Dir", "bie\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "KOUS", "NN", "PPOSAT", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}