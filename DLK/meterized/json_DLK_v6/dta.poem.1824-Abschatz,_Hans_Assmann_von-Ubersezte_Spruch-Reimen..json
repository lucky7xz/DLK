{"dta.poem.1824": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wer ges\u00fcndigt/ wird zuerst hassen was er hat verbr", "tokens": ["Wer", "ge\u00b7s\u00fcn\u00b7digt", "/", "wird", "zu\u00b7erst", "has\u00b7sen", "was", "er", "hat", "ver\u00b7br"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVPP", "$(", "VAFIN", "ADV", "VVINF", "PWS", "PPER", "VAFIN", "NE"], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "chen/", "tokens": ["chen", "/"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Dringt die Gunst beym Richtstuhl durch/ er ist drum nicht lo", "tokens": ["Dringt", "die", "Gunst", "beym", "Richt\u00b7stuhl", "durch", "/", "er", "ist", "drum", "nicht", "lo"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "APPR", "$(", "PPER", "VAFIN", "PAV", "PTKNEG", "NE"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.4": {"text": "gesprochen/", "tokens": ["ge\u00b7spro\u00b7chen", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Sein Gewissen/ das sich f\u00fchlt/ steht am ersten wider ihn/", "tokens": ["Sein", "Ge\u00b7wis\u00b7sen", "/", "das", "sich", "f\u00fchlt", "/", "steht", "am", "ers\u00b7ten", "wi\u00b7der", "ihn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "PRF", "VVFIN", "$(", "VVFIN", "APPRART", "ADJA", "APPR", "PPER", "$("], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Eigne Reu und Scham und Angst mu\u00df ihn zur Bestraffu", "tokens": ["Eig\u00b7ne", "Reu", "und", "Scham", "und", "Angst", "mu\u00df", "ihn", "zur", "Be\u00b7straf\u00b7fu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NE", "KON", "NN", "KON", "NN", "VMFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "zichn.", "tokens": ["zichn", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}}}}}