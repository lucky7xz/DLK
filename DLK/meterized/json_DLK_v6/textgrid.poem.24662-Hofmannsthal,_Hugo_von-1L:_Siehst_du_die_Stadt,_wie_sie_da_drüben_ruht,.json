{"textgrid.poem.24662": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Siehst du die Stadt, wie sie da dr\u00fcben ruht,", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siehst du die Stadt, wie sie da dr\u00fcben ruht,", "tokens": ["Siehst", "du", "die", "Stadt", ",", "wie", "sie", "da", "dr\u00fc\u00b7ben", "ruht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sich fl\u00fcsternd schmieget in das Kleid der Nacht?", "tokens": ["Sich", "fl\u00fcs\u00b7ternd", "schmie\u00b7get", "in", "das", "Kleid", "der", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es gie\u00dft der Mond der Silberseide Flut", "tokens": ["Es", "gie\u00dft", "der", "Mond", "der", "Sil\u00b7ber\u00b7sei\u00b7de", "Flut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf sie herab in zauberischer Pracht.", "tokens": ["Auf", "sie", "her\u00b7ab", "in", "zau\u00b7be\u00b7ri\u00b7scher", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der laue Nachtwind weht ihr Atmen her,", "tokens": ["Der", "lau\u00b7e", "Nacht\u00b7wind", "weht", "ihr", "At\u00b7men", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So geisterhaft, verl\u00f6schend leisen Klang:", "tokens": ["So", "geis\u00b7ter\u00b7haft", ",", "ver\u00b7l\u00f6\u00b7schend", "lei\u00b7sen", "Klang", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "VVPP", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie weint im Traum, sie atmet tief und schwer,", "tokens": ["Sie", "weint", "im", "Traum", ",", "sie", "at\u00b7met", "tief", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie lispelt, r\u00e4tselvoll, verlockend bang ...", "tokens": ["Sie", "lis\u00b7pelt", ",", "r\u00e4t\u00b7sel\u00b7voll", ",", "ver\u00b7lo\u00b7ckend", "bang", "..."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "$,", "VVPP", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die dunkle Stadt, sie schl\u00e4ft im Herzen mein", "tokens": ["Die", "dunk\u00b7le", "Stadt", ",", "sie", "schl\u00e4ft", "im", "Her\u00b7zen", "mein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN", "PPOSAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Glanz und Glut, mit qualvoll bunter Pracht:", "tokens": ["Mit", "Glanz", "und", "Glut", ",", "mit", "qual\u00b7voll", "bun\u00b7ter", "Pracht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch schmeichelnd schwebt um dich ihr Widerschein,", "tokens": ["Doch", "schmei\u00b7chelnd", "schwebt", "um", "dich", "ihr", "Wi\u00b7der\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ged\u00e4mpft zum Fl\u00fcstern, gleitend durch die Nacht.", "tokens": ["Ge\u00b7d\u00e4mpft", "zum", "Fl\u00fcs\u00b7tern", ",", "glei\u00b7tend", "durch", "die", "Nacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}