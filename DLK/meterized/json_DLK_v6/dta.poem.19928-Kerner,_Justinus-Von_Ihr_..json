{"dta.poem.19928": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Von Ihr .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sonnenblume die in Wonne", "tokens": ["Son\u00b7nen\u00b7blu\u00b7me", "die", "in", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich nach gold'ner Sonne sehnet,", "tokens": ["Sich", "nach", "gold'\u00b7ner", "Son\u00b7ne", "seh\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird zum Bild der klaren Sonne", "tokens": ["Wird", "zum", "Bild", "der", "kla\u00b7ren", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihre Liebe sie versch\u00f6net.", "tokens": ["Ih\u00b7re", "Lie\u00b7be", "sie", "ver\u00b7sch\u00f6\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schein ich gut dir, s\u00fc\u00df Verlangen!", "tokens": ["Schein", "ich", "gut", "dir", ",", "s\u00fc\u00df", "Ver\u00b7lan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "PPER", "$,", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie das Herz so gerne w\u00e4hnet,", "tokens": ["Wie", "das", "Herz", "so", "ger\u00b7ne", "w\u00e4h\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist von Augen dein und Wangen", "tokens": ["Ist", "von", "Au\u00b7gen", "dein", "und", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NN", "PPOSAT", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ruhe auf mich \u00fcbergangen,", "tokens": ["Ru\u00b7he", "auf", "mich", "\u00fc\u00b7ber\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schein der Glorie, die dich kr\u00f6net.", "tokens": ["Schein", "der", "Glo\u00b7rie", ",", "die", "dich", "kr\u00f6\u00b7net", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}