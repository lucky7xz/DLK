{"textgrid.poem.67693": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "We\u00df Auge blind ist, \u00e4rgert sich", "tokens": ["We\u00df", "Au\u00b7ge", "blind", "ist", ",", "\u00e4r\u00b7gert", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "ADJD", "VAFIN", "$,", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kann es doch nicht sehen;", "tokens": ["Und", "kann", "es", "doch", "nicht", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "We\u00df Seele taub ist, h\u00f6ret es", "tokens": ["We\u00df", "See\u00b7le", "taub", "ist", ",", "h\u00f6\u00b7ret", "es"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "ADJD", "VAFIN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kann es nicht verstehen.", "tokens": ["Und", "kann", "es", "nicht", "ver\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wo aber Gott spricht: \u00bbLicht!\u00ab", "tokens": ["Wo", "a\u00b7ber", "Gott", "spricht", ":", "\u00bb", "Licht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["PWAV", "ADV", "NN", "VVFIN", "$.", "$(", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sieh, da ist Licht!", "tokens": ["Sieh", ",", "da", "ist", "Licht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VAFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Nun kannst Du W\u00fcst' und Leer'", "tokens": ["Nun", "kannst", "Du", "W\u00fcst'", "und", "Leer'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "KON", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und Sturm auf dunklem Meer,", "tokens": ["Und", "Sturm", "auf", "dunk\u00b7lem", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nun kannst Du Alles sehen.", "tokens": ["Nun", "kannst", "Du", "Al\u00b7les", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}