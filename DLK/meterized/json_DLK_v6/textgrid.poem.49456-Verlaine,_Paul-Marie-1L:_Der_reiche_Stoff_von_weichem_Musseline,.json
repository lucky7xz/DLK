{"textgrid.poem.49456": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der reiche Stoff von weichem Musseline,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der reiche Stoff von weichem Musseline,", "tokens": ["Der", "rei\u00b7che", "Stoff", "von", "wei\u00b7chem", "Mus\u00b7se\u00b7li\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo bleich der Schein der n\u00e4cht'gen Ampel wachte", "tokens": ["Wo", "bleich", "der", "Schein", "der", "n\u00e4cht'\u00b7gen", "Am\u00b7pel", "wach\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und die opalgleich schimmernde Gardine", "tokens": ["Und", "die", "o\u00b7pal\u00b7gleich", "schim\u00b7mern\u00b7de", "Gar\u00b7di\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Geheimnisvoll im Schatten fliessen machte,", "tokens": ["Ge\u00b7heim\u00b7nis\u00b7voll", "im", "Schat\u00b7ten", "flies\u00b7sen", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der reiche Stoff am Bett von Adeline", "tokens": ["Der", "rei\u00b7che", "Stoff", "am", "Bett", "von", "A\u00b7de\u00b7li\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "APPR", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vernahm, wie Klaras Silberstimme lachte,", "tokens": ["Ver\u00b7nahm", ",", "wie", "Kla\u00b7ras", "Sil\u00b7ber\u00b7stim\u00b7me", "lach\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So s\u00fcss, als ob sie nur der Liebe diene,", "tokens": ["So", "s\u00fcss", ",", "als", "ob", "sie", "nur", "der", "Lie\u00b7be", "die\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOKOM", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis sie ein heisser Ton zum Schweigen brachte.", "tokens": ["Bis", "sie", "ein", "heis\u00b7ser", "Ton", "zum", "Schwei\u00b7gen", "brach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O lieben! lieben! sagten eure Stimmen,", "tokens": ["O", "lie\u00b7ben", "!", "lie\u00b7ben", "!", "sag\u00b7ten", "eu\u00b7re", "Stim\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVINF", "$.", "VVINF", "$.", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Klara und Adeline, eng umschlungen,", "tokens": ["Kla\u00b7ra", "und", "A\u00b7de\u00b7li\u00b7ne", ",", "eng", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$,", "ADJD", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "O edle Herzen, die ihr Wunsch bezwungen.", "tokens": ["O", "ed\u00b7le", "Her\u00b7zen", ",", "die", "ihr", "Wunsch", "be\u00b7zwun\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Liebt, liebt, Vereinsamte! Wenn auch in schlimmen", "tokens": ["Liebt", ",", "liebt", ",", "Ver\u00b7ein\u00b7sam\u00b7te", "!", "Wenn", "auch", "in", "schlim\u00b7men"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$.", "KOUS", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und trauerreichen, \u00f6den Ungl\u00fcckstagen", "tokens": ["Und", "trau\u00b7er\u00b7rei\u00b7chen", ",", "\u00f6\u00b7den", "Un\u00b7gl\u00fccks\u00b7ta\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr das erhabne Brandmal still m\u00fcsst tragen.", "tokens": ["Ihr", "das", "er\u00b7hab\u00b7ne", "Brand\u00b7mal", "still", "m\u00fcsst", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}