{"dta.poem.22495": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "43. Da\u00df Oster Lamb.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Der Juden Oster Lamb war Fleisch und Blutt von", "tokens": ["Der", "Ju\u00b7den", "Os\u00b7ter", "Lamb", "war", "Fleisch", "und", "Blutt", "von"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "NN", "KON", "ADJD", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Thieren:", "tokens": ["Thie\u00b7ren", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und dennoch konte sie der W\u00fcrger nicht ber\u00fchren:", "tokens": ["Und", "den\u00b7noch", "kon\u00b7te", "sie", "der", "W\u00fcr\u00b7ger", "nicht", "be\u00b7r\u00fch\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ess\u2019 ich mein Oster Lamb/ und zeichne mich mit Blut/", "tokens": ["Ess'", "ich", "mein", "Os\u00b7ter", "Lamb", "/", "und", "zeich\u00b7ne", "mich", "mit", "Blut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "ADJA", "NN", "$(", "KON", "VVFIN", "PRF", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df sein verwundter Leib/ f\u00fcr mich vergissen thut:", "tokens": ["Da\u00df", "sein", "ver\u00b7wund\u00b7ter", "Leib", "/", "f\u00fcr", "mich", "ver\u00b7gis\u00b7sen", "thut", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$(", "APPR", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So ess\u2019 ich meinen HErrn/ GOtt/ Bruder/ Br\u00e4ut-", "tokens": ["So", "ess'", "ich", "mei\u00b7nen", "Herrn", "/", "Gott", "/", "Bru\u00b7der", "/", "Br\u00e4ut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$(", "NN", "$(", "NN", "$(", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "gam/ B\u00fcrgen:", "tokens": ["gam", "/", "B\u00fcr\u00b7gen", ":"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$(", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Wer ist dann nu der mich kan schlagen un\u0303 erw\u00fcrgen?", "tokens": ["Wer", "ist", "dann", "nu", "der", "mich", "kan", "schla\u00b7gen", "u\u00f1", "er\u00b7w\u00fcr\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "ART", "PPER", "VMFIN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}