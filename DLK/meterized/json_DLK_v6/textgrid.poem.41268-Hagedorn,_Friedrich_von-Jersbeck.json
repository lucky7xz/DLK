{"textgrid.poem.41268": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Jersbeck", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier seh' ich mehr als das, was jenen Kaiser trieb,", "tokens": ["Hier", "seh'", "ich", "mehr", "als", "das", ",", "was", "je\u00b7nen", "Kai\u00b7ser", "trieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "KOKOM", "PDS", "$,", "PWS", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der R\u00fcckkehr zu dem Thron die G\u00e4rten vorzusetzen:", "tokens": ["Der", "R\u00fcck\u00b7kehr", "zu", "dem", "Thron", "die", "G\u00e4r\u00b7ten", "vor\u00b7zu\u00b7set\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Reich, das er gepflanzt, wo Freiheit voll Erg\u00f6tzen,", "tokens": ["Ein", "Reich", ",", "das", "er", "ge\u00b7pflanzt", ",", "wo", "Frei\u00b7heit", "voll", "Er\u00b7g\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "PWAV", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zum t\u00e4glichen Triumph, sein Sieg im Alter blieb.", "tokens": ["Zum", "t\u00e4g\u00b7li\u00b7chen", "Tri\u00b7umph", ",", "sein", "Sieg", "im", "Al\u00b7ter", "blieb", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Hier herrschet diese Lust im w\u00fcrdigsten Gebiete:", "tokens": ["Hier", "herr\u00b7schet", "die\u00b7se", "Lust", "im", "w\u00fcr\u00b7digs\u00b7ten", "Ge\u00b7bie\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Gro\u00df ist die Anmuth hier, die jede Gegend schm\u00fcckt,", "tokens": ["Gro\u00df", "ist", "die", "An\u00b7muth", "hier", ",", "die", "je\u00b7de", "Ge\u00b7gend", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ADV", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gro\u00df jedes Werk der Kunst, und durch die Wahl begl\u00fcckt;", "tokens": ["Gro\u00df", "je\u00b7des", "Werk", "der", "Kunst", ",", "und", "durch", "die", "Wahl", "be\u00b7gl\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "ART", "NN", "$,", "KON", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch gr\u00f6\u00dfer des Besitzers G\u00fcte.", "tokens": ["Doch", "gr\u00f6\u00b7\u00dfer", "des", "Be\u00b7sit\u00b7zers", "G\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}