{"dta.poem.19545": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Alles war ein Spiel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "In diesen Liedern suche du", "tokens": ["In", "die\u00b7sen", "Lie\u00b7dern", "su\u00b7che", "du"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach keinem ernsten Ziel!", "tokens": ["Nach", "kei\u00b7nem", "erns\u00b7ten", "Ziel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein wenig Schmerz, ein wenig Lust", "tokens": ["Ein", "we\u00b7nig", "Schmerz", ",", "ein", "we\u00b7nig", "Lust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "$,", "ART", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Alles war ein Spiel.", "tokens": ["Und", "Al\u00b7les", "war", "ein", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Besonders forsche nicht danach,", "tokens": ["Be\u00b7son\u00b7ders", "for\u00b7sche", "nicht", "da\u00b7nach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Welch Antlitz mir gefiel,", "tokens": ["Welch", "Ant\u00b7litz", "mir", "ge\u00b7fiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohl leuchten Augen viele drin,", "tokens": ["Wohl", "leuch\u00b7ten", "Au\u00b7gen", "vie\u00b7le", "drin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch Alles war ein Spiel.", "tokens": ["Doch", "Al\u00b7les", "war", "ein", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und ob verstohlen auf ein Blatt", "tokens": ["Und", "ob", "ver\u00b7stoh\u00b7len", "auf", "ein", "Blatt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch eine Thr\u00e4ne fiel,", "tokens": ["Auch", "ei\u00b7ne", "Thr\u00e4\u00b7ne", "fiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Getrocknet ist die Thr\u00e4ne l\u00e4ngst", "tokens": ["Ge\u00b7trock\u00b7net", "ist", "die", "Thr\u00e4\u00b7ne", "l\u00e4ngst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Alles war ein Spiel.", "tokens": ["Und", "Al\u00b7les", "war", "ein", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}