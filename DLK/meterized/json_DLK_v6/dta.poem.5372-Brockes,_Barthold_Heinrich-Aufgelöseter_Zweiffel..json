{"dta.poem.5372": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Aufgel\u00f6seter Zweiffel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich habe, leider! oft, wie ich bereits erzehlet,", "tokens": ["Ich", "ha\u00b7be", ",", "lei\u00b7der", "!", "oft", ",", "wie", "ich", "be\u00b7reits", "er\u00b7zeh\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADV", "$.", "ADV", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit einem Zweiffel mich gequ\u00e4let:", "tokens": ["Mit", "ei\u00b7nem", "Zweif\u00b7fel", "mich", "ge\u00b7qu\u00e4\u00b7let", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ob f\u00fcr den Sch\u00f6pfer aller Dinge,", "tokens": ["Ob", "f\u00fcr", "den", "Sch\u00f6p\u00b7fer", "al\u00b7ler", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht aller Menschen Danck und Ehre, zu geringe,", "tokens": ["Nicht", "al\u00b7ler", "Men\u00b7schen", "Danck", "und", "Eh\u00b7re", ",", "zu", "ge\u00b7rin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "NN", "KON", "NN", "$,", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zu elend, zu ver\u00e4chtlich w\u00e4ren?", "tokens": ["Zu", "e\u00b7lend", ",", "zu", "ver\u00b7\u00e4cht\u00b7lich", "w\u00e4\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$,", "PTKA", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Jtzt w\u00fcnsch\u2019 ich, dir und mir noch ferner zu erkl\u00e4ren,", "tokens": ["Jtzt", "w\u00fcn\u00b7sch'", "ich", ",", "dir", "und", "mir", "noch", "fer\u00b7ner", "zu", "er\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "KON", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "Da\u00df diese Zweifels-Last, wie starck sie scheint, wie fest;", "tokens": ["Da\u00df", "die\u00b7se", "Zwei\u00b7fels\u00b7Last", ",", "wie", "starck", "sie", "scheint", ",", "wie", "fest", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$,", "PWAV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Durch die Vernunft sich dennoch heben l\u00e4\u00dft.", "tokens": ["Durch", "die", "Ver\u00b7nunft", "sich", "den\u00b7noch", "he\u00b7ben", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wie k\u00fcnstlich unser Leib von aussen und von innen", "tokens": ["Wie", "k\u00fcnst\u00b7lich", "un\u00b7ser", "Leib", "von", "aus\u00b7sen", "und", "von", "in\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "APPR", "VVINF", "KON", "APPR", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Gebildet sey, ist klar; da\u00df dessen Kunst die Sinnen", "tokens": ["Ge\u00b7bil\u00b7det", "sey", ",", "ist", "klar", ";", "da\u00df", "des\u00b7sen", "Kunst", "die", "Sin\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "$,", "VAFIN", "ADJD", "$.", "KOUS", "PRELAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Zu ihrem Endzweck hab\u2019, ist auch nicht minder wahr;", "tokens": ["Zu", "ih\u00b7rem", "End\u00b7zweck", "hab'", ",", "ist", "auch", "nicht", "min\u00b7der", "wahr", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "$,", "VAFIN", "ADV", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df durch dieselben nun die Seelen", "tokens": ["Da\u00df", "durch", "die\u00b7sel\u00b7ben", "nun", "die", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PDAT", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Sich mit der Creatur verm\u00e4hlen,", "tokens": ["Sich", "mit", "der", "Crea\u00b7tur", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Ist gleichfals sonder Streit. Die\u00df scheint der Endzweck", "tokens": ["Ist", "gleich\u00b7fals", "son\u00b7der", "Streit", ".", "Die\u00df", "scheint", "der", "End\u00b7zweck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJA", "NN", "$.", "PDS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "nun,", "tokens": ["nun", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-", "measure": "single.down"}, "line.16": {"text": "Die Absicht und der Grund. Da\u00df, von der Menschen", "tokens": ["Die", "Ab\u00b7sicht", "und", "der", "Grund", ".", "Da\u00df", ",", "von", "der", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "$.", "KOUS", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Thun", "tokens": ["Thun"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.18": {"text": "So herrlich nichts, als die Gedancken, seyn;", "tokens": ["So", "herr\u00b7lich", "nichts", ",", "als", "die", "Ge\u00b7dan\u00b7cken", ",", "seyn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "PIS", "$,", "KOUS", "ART", "NN", "$,", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Stimmt mit Erfahrung \u00fcberein:", "tokens": ["Stimmt", "mit", "Er\u00b7fah\u00b7rung", "\u00fc\u00b7be\u00b7re\u00b7in", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.20": {"text": "Da alle Dinge nun uns ferner \u00fcberzeugen,", "tokens": ["Da", "al\u00b7le", "Din\u00b7ge", "nun", "uns", "fer\u00b7ner", "\u00fc\u00b7berz\u00b7eu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Da\u00df die Gedancken h\u00f6her steigen,", "tokens": ["Da\u00df", "die", "Ge\u00b7dan\u00b7cken", "h\u00f6\u00b7her", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Als das was C\u00f6rperlich;", "tokens": ["Als", "das", "was", "C\u00f6r\u00b7per\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "PRELS", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "So zeigt von selbsten sich,", "tokens": ["So", "zeigt", "von", "selbs\u00b7ten", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Da\u00df die Gedancken ja von uns das gr\u00f6ste,", "tokens": ["Da\u00df", "die", "Ge\u00b7dan\u00b7cken", "ja", "von", "uns", "das", "gr\u00f6s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "APPR", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Das herrlichste, das beste.", "tokens": ["Das", "herr\u00b7lichs\u00b7te", ",", "das", "bes\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Das Beste nun von einem jeden Wesen", "tokens": ["Das", "Bes\u00b7te", "nun", "von", "ei\u00b7nem", "je\u00b7den", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Scheint f\u00fcr die GOttheit ja mit allem Recht erlesen.", "tokens": ["Scheint", "f\u00fcr", "die", "Got\u00b7theit", "ja", "mit", "al\u00b7lem", "Recht", "er\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "APPR", "PIS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}