{"textgrid.poem.38271": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Reit du und der Teufel", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Schiffmann f\u00e4hrt zum Lande,", "tokens": ["Der", "Schiff\u00b7mann", "f\u00e4hrt", "zum", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wem l\u00e4utet man so sehr?", "tokens": ["Wem", "l\u00e4u\u00b7tet", "man", "so", "sehr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIS", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wem singt man also sanfte,", "tokens": ["Wem", "singt", "man", "al\u00b7so", "sanf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu seiner letzten Ehr?", "tokens": ["Zu", "sei\u00b7ner", "letz\u00b7ten", "Ehr", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Jungfern sieht er heben", "tokens": ["Die", "Jung\u00b7fern", "sieht", "er", "he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl einen sch\u00f6nen Kranz,", "tokens": ["Wohl", "ei\u00b7nen", "sch\u00f6\u00b7nen", "Kranz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zum Sterben oder Leben,", "tokens": ["Zum", "Ster\u00b7ben", "o\u00b7der", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es war ein schwerer Gang.", "tokens": ["Es", "war", "ein", "schwe\u00b7rer", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Gang der war so schwere,", "tokens": ["Der", "Gang", "der", "war", "so", "schwe\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu seiner Liebsten Haus;", "tokens": ["Zu", "sei\u00b7ner", "Liebs\u00b7ten", "Haus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob sie gestorben w\u00e4re,", "tokens": ["Ob", "sie", "ge\u00b7stor\u00b7ben", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Oder eins andern Braut.", "tokens": ["O\u00b7der", "eins", "an\u00b7dern", "Braut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er fand sie auf dem K\u00e4mmerlein,", "tokens": ["Er", "fand", "sie", "auf", "dem", "K\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da sie die Haar aufbund;", "tokens": ["Da", "sie", "die", "Haar", "auf\u00b7bund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gott gr\u00fc\u00df dich, o mein Engelein,", "tokens": ["Gott", "gr\u00fc\u00df", "dich", ",", "o", "mein", "En\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich dich seh gesund.", "tokens": ["Da\u00df", "ich", "dich", "seh", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich hab mir lassen sagen,", "tokens": ["Ich", "hab", "mir", "las\u00b7sen", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du n\u00e4hmst den B\u00e4ndersknab;", "tokens": ["Du", "n\u00e4hmst", "den", "B\u00e4n\u00b7ders\u00b7knab", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So gieb du mir die Treuheit,", "tokens": ["So", "gieb", "du", "mir", "die", "Treu\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die ich dir geben hab.", "tokens": ["Die", "ich", "dir", "ge\u00b7ben", "hab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVINF", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich wei\u00df von keiner Treuheit,", "tokens": ["Ich", "wei\u00df", "von", "kei\u00b7ner", "Treu\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich wei\u00df von keinem Geld;", "tokens": ["Ich", "wei\u00df", "von", "kei\u00b7nem", "Geld", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Reiter soll mich holen,", "tokens": ["Der", "Rei\u00b7ter", "soll", "mich", "ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn ich von Treuheit wei\u00df.", "tokens": ["Wenn", "ich", "von", "Treu\u00b7heit", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da stehts an bis den dritten Tag,", "tokens": ["Da", "stehts", "an", "bis", "den", "drit\u00b7ten", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als da die Hochzeit war,", "tokens": ["Als", "da", "die", "Hoch\u00b7zeit", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da kam ein stolzer Reiter,", "tokens": ["Da", "kam", "ein", "stol\u00b7zer", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der setzt sich oben an.", "tokens": ["Der", "setzt", "sich", "o\u00b7ben", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbnun e\u00dft und trinkt ihr Jungfern,", "tokens": ["\u00bb", "nun", "e\u00dft", "und", "trinkt", "ihr", "Jung\u00b7fern", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich kann nicht fr\u00f6hlich seyn.\u00ab", "tokens": ["Ich", "kann", "nicht", "fr\u00f6h\u00b7lich", "seyn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Trompeten und Schalmeyen", "tokens": ["Trom\u00b7pe\u00b7ten", "und", "Schal\u00b7me\u00b7yen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die gehen insgemein.", "tokens": ["Die", "ge\u00b7hen", "ins\u00b7ge\u00b7mein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Das erste, das er th\u00e4te,", "tokens": ["Das", "ers\u00b7te", ",", "das", "er", "th\u00e4\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Tanz wohl mit der Braut;", "tokens": ["Den", "Tanz", "wohl", "mit", "der", "Braut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er schwenkt sie dreimal r'umme,", "tokens": ["Er", "schwenkt", "sie", "drei\u00b7mal", "r'\u00b7um\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Damit zur Th\u00fcr' hinaus.", "tokens": ["Da\u00b7mit", "zur", "Th\u00fcr'", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Sie kamen \u00fcber ein' Haide,", "tokens": ["Sie", "ka\u00b7men", "\u00fc\u00b7ber", "ein'", "Hai\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein Land, es war wohl breit.", "tokens": ["Ein", "Land", ",", "es", "war", "wohl", "breit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Hals war ihr zerbrochen,", "tokens": ["Der", "Hals", "war", "ihr", "zer\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Seel war eigen sein.", "tokens": ["Die", "Seel", "war", "ei\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}