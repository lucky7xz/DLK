{"dta.poem.1350": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Himmel-Schl\u00fcssel  \n oder  \n Geistliche Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Gott birgt sein helles Licht in Wolck und Schatten ein/", "tokens": ["Gott", "birgt", "sein", "hel\u00b7les", "Licht", "in", "Wolck", "und", "Schat\u00b7ten", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "NN", "KON", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Menschen bl\u00f6des Aug\u2019 erblindet f\u00fcr dem Schein", "tokens": ["Des", "Men\u00b7schen", "bl\u00f6\u00b7des", "Aug'", "er\u00b7blin\u00b7det", "f\u00fcr", "dem", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der g\u00f6ttlichen Gericht und uns verborgnen Wege;", "tokens": ["Der", "g\u00f6tt\u00b7li\u00b7chen", "Ge\u00b7richt", "und", "uns", "ver\u00b7borg\u00b7nen", "We\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Drum wohl mir/ wenn ich mich zu seinen F\u00fcssen lege/", "tokens": ["Drum", "wohl", "mir", "/", "wenn", "ich", "mich", "zu", "sei\u00b7nen", "F\u00fcs\u00b7sen", "le\u00b7ge", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PPER", "$(", "KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und mehr nicht/ als verg\u00f6nnt/ zu wissen bin bedacht/", "tokens": ["Und", "mehr", "nicht", "/", "als", "ver\u00b7g\u00f6nnt", "/", "zu", "wis\u00b7sen", "bin", "be\u00b7dacht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "$(", "KOKOM", "VVFIN", "$(", "PTKZU", "VVINF", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denn f\u00fchl ich seinen Strahl in tieffster Mitternacht.", "tokens": ["Denn", "f\u00fchl", "ich", "sei\u00b7nen", "Strahl", "in", "tieffs\u00b7ter", "Mit\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}