{"textgrid.poem.47498": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[nicht kirre mich, o Scheich, mit Betkorallen!]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht kirre mich, o Scheich, mit Betkorallen!", "tokens": ["Nicht", "kir\u00b7re", "mich", ",", "o", "Scheich", ",", "mit", "Bet\u00b7ko\u00b7ral\u00b7len", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "$,", "FM", "ADJD", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich werde nicht in deine Netze fallen;", "tokens": ["Ich", "wer\u00b7de", "nicht", "in", "dei\u00b7ne", "Net\u00b7ze", "fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn ich geh\u00f6re zu der Ketzersekte", "tokens": ["Denn", "ich", "ge\u00b7h\u00f6\u00b7re", "zu", "der", "Ket\u00b7zer\u00b7sek\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der rosenhauchberauschten Nachtigallen.", "tokens": ["Der", "ro\u00b7sen\u00b7hauch\u00b7be\u00b7rauschten", "Nach\u00b7ti\u00b7gal\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}