{"textgrid.poem.64668": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es s\u00fcndigten mit ihr die K\u00f6nige der Erden,", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es s\u00fcndigten mit ihr die K\u00f6nige der Erden,", "tokens": ["Es", "s\u00fcn\u00b7dig\u00b7ten", "mit", "ihr", "die", "K\u00f6\u00b7ni\u00b7ge", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Durch ihren Zauberkelch ward alle Welt beth\u00f6rt;", "tokens": ["Durch", "ih\u00b7ren", "Zau\u00b7ber\u00b7kelch", "ward", "al\u00b7le", "Welt", "be\u00b7th\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gefallen nun! bewohnt von Teufeln und zerst\u00f6rt!", "tokens": ["Ge\u00b7fal\u00b7len", "nun", "!", "be\u00b7wohnt", "von", "Teu\u00b7feln", "und", "zer\u00b7st\u00f6rt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "VVPP", "APPR", "NN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Um Blut, das sie vergo\u00df, verdammt, nie froh zu werden!", "tokens": ["Um", "Blut", ",", "das", "sie", "ver\u00b7go\u00df", ",", "ver\u00b7dammt", ",", "nie", "froh", "zu", "wer\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVPP", "$,", "ADV", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das Bild, das sich dem Seher wies,", "tokens": ["Das", "Bild", ",", "das", "sich", "dem", "Se\u00b7her", "wies", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "War Babel ihm, ist uns -", "tokens": ["War", "Ba\u00b7bel", "ihm", ",", "ist", "uns"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "NE", "PPER", "$,", "VAFIN", "PPER", "$("], "meter": "-+-++-", "measure": "unknown.measure.tri"}}}}}