{"dta.poem.10748": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Pastoral  \n  Vff Herrn Johan Zinckgrefen vnd Fraw Ma-  \n ria Wildeisin Wittiben Hochzeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Der Sch\u00f6ffer Philop&#339;men", "tokens": ["Der", "Sch\u00f6f\u00b7fer", "Phi\u00b7lop", "&#339;", "men"], "token_info": ["word", "word", "word", "XML_entity", "word"], "pos": ["ART", "NN", "NE", "$(", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Hoch auff der Hardt thet stehn/", "tokens": ["Hoch", "auff", "der", "Hardt", "thet", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Einsmorgens/ als die Str\u00e4men", "tokens": ["Eins\u00b7mor\u00b7gens", "/", "als", "die", "Str\u00e4\u00b7men"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$(", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Sonn vff wolten gehn/", "tokens": ["Der", "Sonn", "vff", "wol\u00b7ten", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "VMFIN", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er sah sie an mit frewden/", "tokens": ["Er", "sah", "sie", "an", "mit", "frew\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie sie so kleblichblicktr", "tokens": ["Wie", "sie", "so", "kleb\u00b7lich\u00b7blicktr"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vber die breite Heiden/", "tokens": ["Vber", "die", "brei\u00b7te", "Hei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Die in dem Reinthal ligt/", "tokens": ["Die", "in", "dem", "Rein\u00b7thal", "ligt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie sch\u00f6n das Wei\u00df vnd Rote:", "tokens": ["Wie", "sch\u00f6n", "das", "Wei\u00df", "vnd", "Ro\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am Himmel sich verj\u00fcngt:", "tokens": ["Am", "Him\u00b7mel", "sich", "ver\u00b7j\u00fcngt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er sprach: mir dieser Botte", "tokens": ["Er", "sprach", ":", "mir", "die\u00b7ser", "Bot\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Anm\u00fcthig Zeitung bringt/", "tokens": ["An\u00b7m\u00fct\u00b7hig", "Zei\u00b7tung", "bringt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Wie wan\u0303 vieleicht mein Sonne:", "tokens": ["Wie", "wa\u00f1", "vie\u00b7leicht", "mein", "Son\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mir jenseit auch vffgieng/", "tokens": ["Mir", "jen\u00b7seit", "auch", "vff\u00b7gieng", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "NE", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Was brechten mir f\u00fcr wonne", "tokens": ["Was", "brech\u00b7ten", "mir", "f\u00fcr", "won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Meim Hertzen diese ding?", "tokens": ["Meim", "Hert\u00b7zen", "die\u00b7se", "ding", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dann wann ich thue betrachten:", "tokens": ["Dann", "wann", "ich", "thue", "be\u00b7trach\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Alten Sch\u00f6ffer sag/", "tokens": ["Der", "Al\u00b7ten", "Sch\u00f6f\u00b7fer", "sag", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So sein nit zuverachten", "tokens": ["So", "sein", "nit", "zu\u00b7ve\u00b7rach\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAINF", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der G\u00f6tter Zeichen Tag/", "tokens": ["Der", "G\u00f6t\u00b7ter", "Zei\u00b7chen", "Tag", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er thete sich vffmachen/", "tokens": ["Er", "the\u00b7te", "sich", "vffma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Wolt suchen Abenthewr/", "tokens": ["Wolt", "su\u00b7chen", "A\u00b7ben\u00b7thewr", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sein Sch\u00e4flin zubewachen/", "tokens": ["Sein", "Sch\u00e4f\u00b7lin", "zu\u00b7be\u00b7wa\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$("], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Befahl er seim Nachbeur.", "tokens": ["Be\u00b7fahl", "er", "seim", "Nach\u00b7beur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er setzet vber Rheine", "tokens": ["Er", "set\u00b7zet", "vber", "Rhei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Den K\u00f6nig aller Fl\u00fc\u00df/", "tokens": ["Den", "K\u00f6\u00b7nig", "al\u00b7ler", "Fl\u00fc\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auff da\u00df er s\u00e4ndt das seine/", "tokens": ["Auff", "da\u00df", "er", "s\u00e4ndt", "das", "sei\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VVFIN", "ART", "PPOSAT", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ermit f\u0