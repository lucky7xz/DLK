, "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach schatten stundt sein will/", "tokens": ["Nach", "schat\u00b7ten", "stundt", "sein", "will", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJD", "VAINF", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er sucht die sch\u00f6nste weiden/", "tokens": ["Er", "sucht", "die", "sch\u00f6ns\u00b7te", "wei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die stunden am Gestadt/", "tokens": ["Die", "stun\u00b7den", "am", "Ge\u00b7stadt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPRART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Darunder er wolt beiden/", "tokens": ["Da\u00b7run\u00b7der", "er", "wolt", "bei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VMFIN", "PIAT", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vnd bey sich suchen rath.", "tokens": ["Vnd", "bey", "sich", "su\u00b7chen", "rath", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRF", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Gar bald Er that ersehen", "tokens": ["Gar", "bald", "Er", "that", "er\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Isis die Sch\u00e4ferin/", "tokens": ["I\u00b7sis", "die", "Sch\u00e4\u00b7fe\u00b7rin", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Nit fern im Busche stehen/", "tokens": ["Nit", "fern", "im", "Bu\u00b7sche", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPRART", "NN", "VVFIN", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Mit gantz betr\u00fcbtem Sinn/", "tokens": ["Mit", "gantz", "be\u00b7tr\u00fcb\u00b7tem", "Sinn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihrs Sch\u00e4fers Todt sie klaget/", "tokens": ["Ihrs", "Sch\u00e4\u00b7fers", "Todt", "sie", "kla\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Leide selbst sie ziert/", "tokens": ["Das", "Lei\u00b7de", "selbst", "sie", "ziert", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Manch trostlo\u00df Wort sie saget/", "tokens": ["Manch", "trost\u00b7lo\u00df", "Wort", "sie", "sa\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Manch seufftzen sie da f\u00fchrt.", "tokens": ["Manch", "seufft\u00b7zen", "sie", "da", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Er sah sie an/ vnd h\u00f6ret", "tokens": ["Er", "sah", "sie", "an", "/", "vnd", "h\u00f6\u00b7ret"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"],