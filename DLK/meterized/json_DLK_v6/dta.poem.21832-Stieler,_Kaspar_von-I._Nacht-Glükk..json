{"dta.poem.21832": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "I.  \n  Nacht-Gl\u00fckk.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Lyeus hatte mir den Sinn", "tokens": ["Ly\u00b7eus", "hat\u00b7te", "mir", "den", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "durch seines Safftes Zug benommen/", "tokens": ["durch", "sei\u00b7nes", "Saff\u00b7tes", "Zug", "be\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "ich gieng und wuste nicht/ wohin/", "tokens": ["ich", "gieng", "und", "wus\u00b7te", "nicht", "/", "wo\u00b7hin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKNEG", "$(", "PWAV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "indehm war ich zuweit gekommen.", "tokens": ["in\u00b7dehm", "war", "ich", "zu\u00b7weit", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der bleiche Monden hatte zwar", "tokens": ["Der", "blei\u00b7che", "Mon\u00b7den", "hat\u00b7te", "zwar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "doch wust\u2019 ich recht nicht/ wo ich war/", "tokens": ["doch", "wust'", "ich", "recht", "nicht", "/", "wo", "ich", "war", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "$(", "PWAV", "PPER", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Jhr G\u00f6tter/ habet Dank/ da\u00df ihr", "tokens": ["Ihr", "G\u00f6t\u00b7ter", "/", "ha\u00b7bet", "Dank", "/", "da\u00df", "ihr"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "VAFIN", "NN", "$(", "KOUS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mich bracht zu diesem sch\u00f6nen Kinde/", "tokens": ["mich", "bracht", "zu", "die\u00b7sem", "sch\u00f6\u00b7nen", "Kin\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "ich sach die himmlische Dorinde.", "tokens": ["ich", "sach", "die", "himm\u00b7li\u00b7sche", "Do\u00b7rin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie h\u00e4tt\u2019 ihr auffgel\u00f6stes Haubt", "tokens": ["Sie", "h\u00e4tt'", "ihr", "auff\u00b7ge\u00b7l\u00f6s\u00b7tes", "Haubt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "unachtsam auff dem Arme liegen/", "tokens": ["u\u00b7nacht\u00b7sam", "auff", "dem", "Ar\u00b7me", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "das Haar/ das meinen Sinn geraubt", "tokens": ["das", "Haar", "/", "das", "mei\u00b7nen", "Sinn", "ge\u00b7raubt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie zog den s\u00fcssen Zimmet-Geist", "tokens": ["Sie", "zog", "den", "s\u00fcs\u00b7sen", "Zim\u00b7met\u00b7Geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "bald ein/ bald haucht sie ihn zur\u00fckke/", "tokens": ["bald", "ein", "/", "bald", "haucht", "sie", "ihn", "zu\u00b7r\u00fck\u00b7ke", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$(", "ADV", "VVFIN", "PPER", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "was sch\u00f6n und liebwehrt ist und heist", "tokens": ["was", "sch\u00f6n", "und", "lieb\u00b7wehrt", "ist", "und", "heist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "KON", "VVFIN", "VAFIN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So mein\u2019 ich/ war Andromade", "tokens": ["So", "mein'", "ich", "/", "war", "And\u00b7ro\u00b7ma\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$(", "VAFIN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Als Perseus ihr zu H\u00fclffe kahme", "tokens": ["Als", "Per\u00b7seus", "ihr", "zu", "H\u00fclf\u00b7fe", "kah\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "PPER", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So die entbl\u00f6\u00dfte Zyprie", "tokens": ["So", "die", "ent\u00b7bl\u00f6\u00df\u00b7te", "Zyp\u00b7rie"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "als sie den g\u00f6ldnen Apfel nahme.", "tokens": ["als", "sie", "den", "g\u00f6ld\u00b7nen", "Ap\u00b7fel", "nah\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Diane hatte selbsten Lust", "tokens": ["Di\u00b7a\u00b7ne", "hat\u00b7te", "selbs\u00b7ten", "Lust"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mit dieser Sch\u00f6nheit beyzuschlaffen/", "tokens": ["mit", "die\u00b7ser", "Sch\u00f6n\u00b7heit", "bey\u00b7zu\u00b7schlaf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "die auch das Helffenbein kan straffen.", "tokens": ["die", "auch", "das", "Helf\u00b7fen\u00b7bein", "kan", "straf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Hie stritte bey mir die Begier/", "tokens": ["Hie", "strit\u00b7te", "bey", "mir", "die", "Be\u00b7gier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die Schaam und br\u00fcnstiges Verlangen:", "tokens": ["die", "Schaam", "und", "br\u00fcns\u00b7ti\u00b7ges", "Ver\u00b7lan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Der hohe Geist und Ernstligkeit/", "tokens": ["Der", "ho\u00b7he", "Geist", "und", "Ernst\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die schlaffend auch nicht von ihr schieden/", "tokens": ["die", "schlaf\u00b7fend", "auch", "nicht", "von", "ihr", "schie\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "PTKNEG", "APPR", "PPOSAT", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "die machten/ da\u00df ich lange Zeit", "tokens": ["die", "mach\u00b7ten", "/", "da\u00df", "ich", "lan\u00b7ge", "Zeit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "VVFIN", "$(", "KOUS", "PPER", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "allein mit Ansehn war zu frieden.", "tokens": ["al\u00b7lein", "mit", "An\u00b7sehn", "war", "zu", "frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Nicht Argus gab so eben acht", "tokens": ["Nicht", "Ar\u00b7gus", "gab", "so", "e\u00b7ben", "acht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "NE", "VVFIN", "ADV", "ADV", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "auff die ihm auvertraute Kuhe/", "tokens": ["auff", "die", "ihm", "au\u00b7ver\u00b7trau\u00b7te", "Ku\u00b7he", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "die er mit hundert Augen wacht\u2019:", "tokens": ["die", "er", "mit", "hun\u00b7dert", "Au\u00b7gen", "wacht'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "als ich auff ihre s\u00fcsse Ruhe.", "tokens": ["als", "ich", "auff", "ih\u00b7re", "s\u00fcs\u00b7se", "Ru\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Wie offt scholt\u2019 ich den Traum-Gott au\u00df", "tokens": ["Wie", "offt", "scholt'", "ich", "den", "Traum\u00b7Gott", "au\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wenn sie lie\u00df einen Seuffzer h\u00f6ren/", "tokens": ["wenn", "sie", "lie\u00df", "ei\u00b7nen", "Seuff\u00b7zer", "h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "bef\u00f6rchtend da\u00df durch einen Grau\u00df", "tokens": ["be\u00b7f\u00f6rch\u00b7tend", "da\u00df", "durch", "ei\u00b7nen", "Grau\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "er ihre Ruhe m\u00f6chte st\u00f6ren.", "tokens": ["er", "ih\u00b7re", "Ru\u00b7he", "m\u00f6ch\u00b7te", "st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Doch liesse mich die Liebe nicht", "tokens": ["Doch", "lies\u00b7se", "mich", "die", "Lie\u00b7be", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "den guten Vorteil so verseumen/", "tokens": ["den", "gu\u00b7ten", "Vor\u00b7teil", "so", "ver\u00b7seu\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "da\u00df ich ihr Liljen Angesicht", "tokens": ["da\u00df", "ich", "ihr", "Lil\u00b7jen", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "nicht r\u00fchren solt\u2019 in ihren Tr\u00e4umen.", "tokens": ["nicht", "r\u00fch\u00b7ren", "solt'", "in", "ih\u00b7ren", "Tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Dann \u00f6ffnet\u2019 ich den Busen ihr", "tokens": ["Dann", "\u00f6ff\u00b7net'", "ich", "den", "Bu\u00b7sen", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und weil der Schlaff sie noch umschlossen/", "tokens": ["und", "weil", "der", "Schlaff", "sie", "noch", "um\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "hab\u2019 ich ein K\u00fc\u00dfchen oder vier", "tokens": ["hab'", "ich", "ein", "K\u00fc\u00df\u00b7chen", "o\u00b7der", "vier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "KON", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "in solcher stillen Nacht genossen.", "tokens": ["in", "sol\u00b7cher", "stil\u00b7len", "Nacht", "ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Di\u00df sach der Eyffer-volle Mohn", "tokens": ["Di\u00df", "sach", "der", "Eyf\u00b7fer\u00b7vol\u00b7le", "Mohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und ward entr\u00fcst ob meinen Freuden.", "tokens": ["und", "ward", "ent\u00b7r\u00fcst", "ob", "mei\u00b7nen", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So sch\u00f6ner Liebe reicher Lohn", "tokens": ["So", "sch\u00f6\u00b7ner", "Lie\u00b7be", "rei\u00b7cher", "Lohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "macht auch die Sterne selber neiden.", "tokens": ["macht", "auch", "die", "Ster\u00b7ne", "sel\u00b7ber", "nei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.15": {"line.1": {"text": "Er scho\u00df\u2019 ihr einen Demant straal", "tokens": ["Er", "scho\u00df'", "ihr", "ei\u00b7nen", "De\u00b7mant", "straal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "in die verschlo\u00dfnen Augen-lieder/", "tokens": ["in", "die", "ver\u00b7schlo\u00df\u00b7nen", "Au\u00b7gen\u00b7lie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "darob erseuffzte sie einmahl", "tokens": ["da\u00b7rob", "er\u00b7seuffz\u00b7te", "sie", "ein\u00b7mahl"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und r\u00fchret\u2019 ihre Marmor-glieder.", "tokens": ["und", "r\u00fch\u00b7ret'", "ih\u00b7re", "Mar\u00b7mor\u00b7glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Sie schlug\u2019 die m\u00fcden Lichter auff/", "tokens": ["Sie", "schlug'", "die", "m\u00fc\u00b7den", "Lich\u00b7ter", "auff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die auch die Sonne k\u00f6nnen h\u00f6nen/", "tokens": ["die", "auch", "die", "Son\u00b7ne", "k\u00f6n\u00b7nen", "h\u00f6\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich dachte schon auff Flucht und Lauff", "tokens": ["Ich", "dach\u00b7te", "schon", "auff", "Flucht", "und", "Lauff"], "token_info": ["word", "word", "word", "word", "word", "word",