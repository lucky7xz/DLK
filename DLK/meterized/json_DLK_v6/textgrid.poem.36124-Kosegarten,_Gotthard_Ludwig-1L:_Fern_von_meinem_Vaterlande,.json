{"textgrid.poem.36124": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fern von meinem Vaterlande,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fern von meinem Vaterlande,", "tokens": ["Fern", "von", "mei\u00b7nem", "Va\u00b7ter\u00b7lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fern vom Ort, der mich gebar;", "tokens": ["Fern", "vom", "Ort", ",", "der", "mich", "ge\u00b7bar", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "PRELS", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weilt mein Fu\u00df in fremdem Lande,", "tokens": ["Weilt", "mein", "Fu\u00df", "in", "frem\u00b7dem", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo der Meinen keiner war;", "tokens": ["Wo", "der", "Mei\u00b7nen", "kei\u00b7ner", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PIS", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fern von allen meinen Lieben,", "tokens": ["Fern", "von", "al\u00b7len", "mei\u00b7nen", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Spuken Larven um mich her,", "tokens": ["Spu\u00b7ken", "Lar\u00b7ven", "um", "mich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Seelelos, und von den Trieben", "tokens": ["See\u00b7le\u00b7los", ",", "und", "von", "den", "Trie\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Warmer Menschenfreundschaft leer.", "tokens": ["War\u00b7mer", "Men\u00b7schen\u00b7freund\u00b7schaft", "leer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O! wo seyd ihr nun, ihr s\u00fc\u00dfen", "tokens": ["O", "!", "wo", "seyd", "ihr", "nun", ",", "ihr", "s\u00fc\u00b7\u00dfen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$.", "PWAV", "VAFIN", "PPER", "ADV", "$,", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heimischen und stillen H\u00f6h'n,", "tokens": ["Hei\u00b7mi\u00b7schen", "und", "stil\u00b7len", "H\u00f6h'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ich meinen Morgen sprie\u00dfen,", "tokens": ["Wo", "ich", "mei\u00b7nen", "Mor\u00b7gen", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und in seinem Bl\u00fch'n geseh'n,", "tokens": ["Und", "in", "sei\u00b7nem", "Bl\u00fch'n", "ge\u00b7seh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo mir in der Lieben Mitte", "tokens": ["Wo", "mir", "in", "der", "Lie\u00b7ben", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.6": {"text": "Jeder Tag, ein Fest, verschwand,", "tokens": ["Je\u00b7der", "Tag", ",", "ein", "Fest", ",", "ver\u00b7schwand", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und auf jedem meiner Schritte", "tokens": ["Und", "auf", "je\u00b7dem", "mei\u00b7ner", "Schrit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ein vertraulich M\u00e4dchen stand?", "tokens": ["Ein", "ver\u00b7trau\u00b7lich", "M\u00e4d\u00b7chen", "stand", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Rein, wie euer reiner Himmel,", "tokens": ["Rein", ",", "wie", "eu\u00b7er", "rei\u00b7ner", "Him\u00b7mel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sanft, wie eure sanfte Luft,", "tokens": ["Sanft", ",", "wie", "eu\u00b7re", "sanf\u00b7te", "Luft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6rt' ich da kein Stadtgewimmel,", "tokens": ["H\u00f6rt'", "ich", "da", "kein", "Stadt\u00b7ge\u00b7wim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hauchte keinen Gr\u00e4berduft.", "tokens": ["Hauch\u00b7te", "kei\u00b7nen", "Gr\u00e4\u00b7ber\u00b7duft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kannte nicht die wilden Freuden,", "tokens": ["Kann\u00b7te", "nicht", "die", "wil\u00b7den", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Drin die Thorheit sich vergn\u00fcgt;", "tokens": ["Drin", "die", "Thor\u00b7heit", "sich", "ver\u00b7gn\u00fcgt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nicht der Unschuld heimlich Leiden,", "tokens": ["Nicht", "der", "Un\u00b7schuld", "heim\u00b7lich", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn die Frechheit sie bekriegt.", "tokens": ["Wenn", "die", "Frech\u00b7heit", "sie", "be\u00b7kriegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nein, in euren stillen Gr\u00fcnden", "tokens": ["Nein", ",", "in", "eu\u00b7ren", "stil\u00b7len", "Gr\u00fcn\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hauchte Sch\u00f6nheit und Natur.", "tokens": ["Hauch\u00b7te", "Sch\u00f6n\u00b7heit", "und", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zwischen dunkelgr\u00fcnen Linden", "tokens": ["Zwi\u00b7schen", "dun\u00b7kel\u00b7gr\u00fc\u00b7nen", "Lin\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wohnt' ich auf vertrauter Flur.", "tokens": ["Wohnt'", "ich", "auf", "ver\u00b7trau\u00b7ter", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Heiter fand mich jeder Morgen,", "tokens": ["Hei\u00b7ter", "fand", "mich", "je\u00b7der", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ruhig jedes Mittags Pracht,", "tokens": ["Ru\u00b7hig", "je\u00b7des", "Mit\u00b7tags", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Jede D\u00e4mm'rung, frei von Sorgen,", "tokens": ["Je\u00b7de", "D\u00e4m\u00b7m'\u00b7rung", ",", "frei", "von", "Sor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Leis' entschlummert jede Nacht.", "tokens": ["Leis'", "ent\u00b7schlum\u00b7mert", "je\u00b7de", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Fr\u00fch ergriff ich meine Fl\u00f6te,", "tokens": ["Fr\u00fch", "er\u00b7griff", "ich", "mei\u00b7ne", "Fl\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wandert' durch bethaute Au'n,", "tokens": ["Wan\u00b7dert'", "durch", "be\u00b7thau\u00b7te", "Au'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sah des Morgens erste R\u00f6the", "tokens": ["Sah", "des", "Mor\u00b7gens", "ers\u00b7te", "R\u00f6\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hinter ", "tokens": ["Hin\u00b7ter"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Sah den j\u00fcngsten Strahl der Sonne,", "tokens": ["Sah", "den", "j\u00fcng\u00b7sten", "Strahl", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie er mild die Flur bestrich,", "tokens": ["Wie", "er", "mild", "die", "Flur", "be\u00b7strich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und des Morgens ganze Wonne", "tokens": ["Und", "des", "Mor\u00b7gens", "gan\u00b7ze", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Go\u00df sich segnend \u00fcber mich.", "tokens": ["Go\u00df", "sich", "seg\u00b7nend", "\u00fc\u00b7ber", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "ADJD", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Tages auf den bunten Matten,", "tokens": ["Ta\u00b7ges", "auf", "den", "bun\u00b7ten", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwischen Wiesen, Hain und Bach,", "tokens": ["Zwi\u00b7schen", "Wie\u00b7sen", ",", "Hain", "und", "Bach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sp\u00fcrte ich auf leisen Schritten,", "tokens": ["Sp\u00fcr\u00b7te", "ich", "auf", "lei\u00b7sen", "Schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freundliche Natur, dir nach,", "tokens": ["Freund\u00b7li\u00b7che", "Na\u00b7tur", ",", "dir", "nach", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Trank an deinem Mutterbusen", "tokens": ["Trank", "an", "dei\u00b7nem", "Mut\u00b7ter\u00b7bu\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Reine Weisheit, echten Ruhm;", "tokens": ["Rei\u00b7ne", "Weis\u00b7heit", ",", "ech\u00b7ten", "Ruhm", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die lieblichste der Musen", "tokens": ["Und", "die", "lieb\u00b7lichs\u00b7te", "der", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Zog mich in ihr Heiligthum.", "tokens": ["Zog", "mich", "in", "ihr", "Hei\u00b7lig\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Blickten dann die Abendsterne", "tokens": ["Blick\u00b7ten", "dann", "die", "A\u00b7bends\u00b7ter\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieblich aus den blauen H\u00f6h'n,", "tokens": ["Lieb\u00b7lich", "aus", "den", "blau\u00b7en", "H\u00f6h'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O, wie lauscht' ich da so gerne", "tokens": ["O", ",", "wie", "lauscht'", "ich", "da", "so", "ger\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "VVFIN", "PPER", "ADV", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf des Unbekannten Weh'n,", "tokens": ["Auf", "des", "Un\u00b7be\u00b7kann\u00b7ten", "Weh'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Horchte, wie, gleich Waldesstimme,", "tokens": ["Horch\u00b7te", ",", "wie", ",", "gleich", "Wal\u00b7dess\u00b7tim\u00b7me", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seine Stimm' die Flur beschlich,", "tokens": ["Sei\u00b7ne", "Stimm'", "die", "Flur", "be\u00b7schlich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und mir ahndete, als schw\u00fcmme", "tokens": ["Und", "mir", "ahn\u00b7de\u00b7te", ",", "als", "schw\u00fcm\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Leise Gottheit rings um mich,", "tokens": ["Lei\u00b7se", "Got\u00b7theit", "rings", "um", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und ein heimlich hohes Grauen", "tokens": ["Und", "ein", "heim\u00b7lich", "ho\u00b7hes", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zuckte mir durch Mark und Bein.", "tokens": ["Zuck\u00b7te", "mir", "durch", "Mark", "und", "Bein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich entfloh den offnen Auen", "tokens": ["Ich", "ent\u00b7floh", "den", "off\u00b7nen", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dem hellen Mondenschein,", "tokens": ["Und", "dem", "hel\u00b7len", "Mon\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Warf mich hin am dunklen Se\u00eb,", "tokens": ["Warf", "mich", "hin", "am", "dunk\u00b7len", "Se\u00eb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo der Wind den Schilf belief,", "tokens": ["Wo", "der", "Wind", "den", "Schilf", "be\u00b7lief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ihr s\u00fc\u00dfes Klagewehe,", "tokens": ["Und", "ihr", "s\u00fc\u00b7\u00dfes", "Kla\u00b7ge\u00b7we\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nachtigall und Unke rief.", "tokens": ["Nach\u00b7ti\u00b7gall", "und", "Un\u00b7ke", "rief", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Da entquollen leise T\u00f6ne", "tokens": ["Da", "ent\u00b7quol\u00b7len", "lei\u00b7se", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner Fl\u00f6te, wie dem Bach", "tokens": ["Mei\u00b7ner", "Fl\u00f6\u00b7te", ",", "wie", "dem", "Bach"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wellen, und dem Klag'get\u00f6ne", "tokens": ["Wel\u00b7len", ",", "und", "dem", "Klag'\u00b7ge\u00b7t\u00f6\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Floh'n die Abendgeister nach.", "tokens": ["Floh'n", "die", "A\u00b7bend\u00b7geis\u00b7ter", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "L\u00e4ngs dem Schilfe h\u00f6rt' ich's s\u00e4useln", "tokens": ["L\u00e4ngs", "dem", "Schil\u00b7fe", "h\u00f6rt'", "ich's", "s\u00e4u\u00b7seln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kl\u00e4rer, als ein Zephyr saus't,", "tokens": ["Kl\u00e4\u00b7rer", ",", "als", "ein", "Ze\u00b7phyr", "saus't", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sah die Fluth des Sees sich kr\u00e4useln", "tokens": ["Sah", "die", "Fluth", "des", "Sees", "sich", "kr\u00e4u\u00b7seln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sch\u00f6ner, als ein West sie kraus't.", "tokens": ["Sch\u00f6\u00b7ner", ",", "als", "ein", "West", "sie", "kraus'", "t."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Heller glitschte von dem Se\u00eb", "tokens": ["Hel\u00b7ler", "g