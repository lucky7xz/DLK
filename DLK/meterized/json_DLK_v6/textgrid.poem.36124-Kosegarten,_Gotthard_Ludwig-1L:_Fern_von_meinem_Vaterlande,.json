, "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Donner sein Trompetenhall,", "tokens": ["Don\u00b7ner", "sein", "Trom\u00b7pe\u00b7ten\u00b7hall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Blitz die Rosse, die ihn tragen,", "tokens": ["Blitz", "die", "Ros\u00b7se", ",", "die", "ihn", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und sein Tro\u00df des Hagels Fall. \u2013 \u2013 \u2013", "tokens": ["Und", "sein", "Tro\u00df", "des", "Ha\u00b7gels", "Fall", ".", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "NN", "$.", "$(", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Ach, den Frieden, der so milde", "tokens": ["Ach", ",", "den", "Frie\u00b7den", ",", "der", "so", "mil\u00b7de"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "NN", "$,", "PRELS", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dort auf meine Seele flo\u00df,", "tokens": ["Dort", "auf", "mei\u00b7ne", "See\u00b7le", "flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ferne scheucht ihn die\u00df Gefilde,", "tokens": ["Fer\u00b7ne", "scheucht", "ihn", "die\u00df", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PDS", "NN", "$,"], "meter": "+