{"dta.poem.960": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "\u00dc BERSCHRIFTEN", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Lieder wie ich gern sie s&#228;nge               ", "tokens": ["Lie\u00b7der", "wie", "ich", "gern", "sie", "s", "&#228;", "nge"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["NN", "KOKOM", "PPER", "ADV", "PPER", "NE", "$(", "VVFIN"], "meter": "+--++--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Darf ich freunde noch nicht singen               ", "tokens": ["Darf", "ich", "freun\u00b7de", "noch", "nicht", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVFIN", "ADV", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur dies fl&#252;chtige gedr&#228;nge               ", "tokens": ["Nur", "dies", "fl", "&#252;", "ch\u00b7ti\u00b7ge", "ge\u00b7dr", "&#228;", "nge"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "XML_entity", "word"], "pos": ["ADV", "PDS", "NN", "$(", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Scheuer reime will gelingen             ", "tokens": ["Scheu\u00b7er", "rei\u00b7me", "will", "ge\u00b7lin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hinter reben oder hinter               ", "tokens": ["Hin\u00b7ter", "re\u00b7ben", "o\u00b7der", "hin\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVINF", "KON", "APPR"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stillen mauern zu kredenzen               ", "tokens": ["Stil\u00b7len", "mau\u00b7ern", "zu", "kre\u00b7den\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVINF", "PTKZU", "VVINF"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Zur erheitrung weisser winter               ", "tokens": ["Zur", "er\u00b7heit\u00b7rung", "weis\u00b7ser", "win\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und zum trost in fahlen lenzen             ", "tokens": ["Und", "zum", "trost", "in", "fah\u00b7len", "len\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was ich nach den harten fehden               ", "tokens": ["Was", "ich", "nach", "den", "har\u00b7ten", "feh\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den schooss des friedens bette               ", "tokens": ["In", "den", "scho\u00b7oss", "des", "frie\u00b7dens", "bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "VVFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und aus reicher jugend eden               ", "tokens": ["Und", "aus", "rei\u00b7cher", "ju\u00b7gend", "e\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJD", "ADJD", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In das leben &#252;ber-rette.             ", "tokens": ["In", "das", "le\u00b7ben", "&#252;", "ber\u00b7ret\u00b7te", "."], "token_info": ["word", "word", "word", "XML_entity", "word", "punct"], "pos": ["APPR", "PDS", "VVINF", "$(", "NE", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}