{"dta.poem.9162": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "IiI.  \n An die junge und wunder-sch\u00f6ne Salibene.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Du alte Salibene/", "tokens": ["Du", "al\u00b7te", "Sa\u00b7li\u00b7be\u00b7ne", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit gunst da\u00df ich dich h\u00f6hne/", "tokens": ["Mit", "gunst", "da\u00df", "ich", "dich", "h\u00f6h\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "KOUS", "PPER", "PRF", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Weil du dich in den gr\u00fcnen", "tokens": ["Weil", "du", "dich", "in", "den", "gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So h\u00f6flich l\u00e4st bedienen.", "tokens": ["So", "h\u00f6f\u00b7lich", "l\u00e4st", "be\u00b7die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}