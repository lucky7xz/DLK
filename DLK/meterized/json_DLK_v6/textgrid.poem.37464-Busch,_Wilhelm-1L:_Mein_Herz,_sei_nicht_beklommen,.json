{"textgrid.poem.37464": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Herz, sei nicht beklommen,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Herz, sei nicht beklommen,", "tokens": ["Mein", "Herz", ",", "sei", "nicht", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch wird die Welt nicht alt.", "tokens": ["Noch", "wird", "die", "Welt", "nicht", "alt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Fr\u00fchling ist wiedergekommen,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "ist", "wie\u00b7der\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Frisch gr\u00fcnt der deutsche Wald.", "tokens": ["Frisch", "gr\u00fcnt", "der", "deut\u00b7sche", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Seit Ururv\u00e4tertagen", "tokens": ["Seit", "U\u00b7rur\u00b7v\u00e4\u00b7ter\u00b7ta\u00b7gen"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Stehen die Eichen am See,", "tokens": ["Ste\u00b7hen", "die", "Ei\u00b7chen", "am", "See", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Die Nachtigallen schlagen,", "tokens": ["Die", "Nach\u00b7ti\u00b7gal\u00b7len", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zur Tr\u00e4nke kommt das Reh.", "tokens": ["Zur", "Tr\u00e4n\u00b7ke", "kommt", "das", "Reh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Sonne geht auf und unter", "tokens": ["Die", "Son\u00b7ne", "geht", "auf", "und", "un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "APPR"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schon lange vieltausendmal,", "tokens": ["Schon", "lan\u00b7ge", "viel\u00b7tau\u00b7send\u00b7mal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Noch immer eilen so munter", "tokens": ["Noch", "im\u00b7mer", "ei\u00b7len", "so", "mun\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ADV", "ADJD"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die B\u00e4chlein ins bl\u00fchende Tal.", "tokens": ["Die", "B\u00e4ch\u00b7lein", "ins", "bl\u00fc\u00b7hen\u00b7de", "Tal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Hier lieg ich im weichen Moose", "tokens": ["Hier", "lieg", "ich", "im", "wei\u00b7chen", "Moo\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Unter dem rauschenden Baum,", "tokens": ["Un\u00b7ter", "dem", "rau\u00b7schen\u00b7den", "Baum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Die Zeit, die wesenlose,", "tokens": ["Die", "Zeit", ",", "die", "we\u00b7sen\u00b7lo\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verschwindet als wie ein Traum.", "tokens": ["Ver\u00b7schwin\u00b7det", "als", "wie", "ein", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "KOKOM", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Von k\u00fchlen Schatten umd\u00e4mmert,", "tokens": ["Von", "k\u00fch\u00b7len", "Schat\u00b7ten", "um\u00b7d\u00e4m\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Versink ich in selige Ruh;", "tokens": ["Ver\u00b7sink", "ich", "in", "se\u00b7li\u00b7ge", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ein Specht, der lustig h\u00e4mmert,", "tokens": ["Ein", "Specht", ",", "der", "lus\u00b7tig", "h\u00e4m\u00b7mert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nickt mir vertraulich zu.", "tokens": ["Nickt", "mir", "ver\u00b7trau\u00b7lich", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Mir ist, als ob er riefe:", "tokens": ["Mir", "ist", ",", "als", "ob", "er", "rie\u00b7fe", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Heija, mein guter Gesell,", "tokens": ["Hei\u00b7ja", ",", "mein", "gu\u00b7ter", "Ge\u00b7sell", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "F\u00fcr ewig aus dunkler Tiefe", "tokens": ["F\u00fcr", "e\u00b7wig", "aus", "dunk\u00b7ler", "Tie\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sprudelt der Lebensquell.", "tokens": ["Spru\u00b7delt", "der", "Le\u00b7bens\u00b7quell", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}