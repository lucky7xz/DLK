{"textgrid.poem.52675": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Traurigkeit, du sterblich nie,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Traurigkeit, du sterblich nie,", "tokens": ["O", "Trau\u00b7rig\u00b7keit", ",", "du", "sterb\u00b7lich", "nie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Recht bittre du, nichts werthe,", "tokens": ["Recht", "bitt\u00b7re", "du", ",", "nichts", "wert\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nicht s\u00fc\u00dfe sanfte Melancholie,", "tokens": ["Nicht", "s\u00fc\u00b7\u00dfe", "sanf\u00b7te", "Me\u00b7lan\u00b7cho\u00b7lie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bist unser Lebensgef\u00e4hrte.", "tokens": ["Bist", "un\u00b7ser", "Le\u00b7bens\u00b7ge\u00b7f\u00e4hr\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ach Alles vergeht!", "tokens": ["Ach", "Al\u00b7les", "ver\u00b7geht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "PIS", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Was aber besteht?", "tokens": ["Was", "a\u00b7ber", "be\u00b7steht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Allzeit?", "tokens": ["All\u00b7zeit", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Der alte Schmerz, das ewge Leid.", "tokens": ["Der", "al\u00b7te", "Schmerz", ",", "das", "ew\u00b7ge", "Leid", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Freudentag, ein Rausch des Gl\u00fccks", "tokens": ["Ein", "Freu\u00b7den\u00b7tag", ",", "ein", "Rausch", "des", "Gl\u00fccks"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Freundeskreise vor\u00fcber!", "tokens": ["Im", "Freun\u00b7des\u00b7krei\u00b7se", "vor\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Lust eines dauernden \u2013 Augenblicks:", "tokens": ["Lust", "ei\u00b7nes", "dau\u00b7ern\u00b7den", "\u2013", "Au\u00b7gen\u00b7blicks", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "ADJA", "$(", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Dann um so stiller und tr\u00fcber.", "tokens": ["Dann", "um", "so", "stil\u00b7ler", "und", "tr\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ach Alles vergeht!", "tokens": ["Ach", "Al\u00b7les", "ver\u00b7geht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "PIS", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Was aber besteht?", "tokens": ["Was", "a\u00b7ber", "be\u00b7steht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Allzeit?", "tokens": ["All\u00b7zeit", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Der alte Schmerz, das ewge Leid.", "tokens": ["Der", "al\u00b7te", "Schmerz", ",", "das", "ew\u00b7ge", "Leid", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Geruht an g\u00f6ttlicher Liebe Brust,", "tokens": ["Ge\u00b7ruht", "an", "g\u00f6tt\u00b7li\u00b7cher", "Lie\u00b7be", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Maistunde jauchzender Wonne,", "tokens": ["Mais\u00b7tun\u00b7de", "jauch\u00b7zen\u00b7der", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Genossen der Treue Reiz und Lust \u2013", "tokens": ["Ge\u00b7nos\u00b7sen", "der", "Treu\u00b7e", "Reiz", "und", "Lust", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "KON", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zum Abend neiget die Sonne.", "tokens": ["Zum", "A\u00b7bend", "nei\u00b7get", "die", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ach Alles vergeht!", "tokens": ["Ach", "Al\u00b7les", "ver\u00b7geht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "PIS", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Was aber besteht?", "tokens": ["Was", "a\u00b7ber", "be\u00b7steht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Allzeit?", "tokens": ["All\u00b7zeit", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Der alte Schmerz, das ewge Leid.", "tokens": ["Der", "al\u00b7te", "Schmerz", ",", "das", "ew\u00b7ge", "Leid", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Geerntet, was einst mit Muth ges\u00e4t,", "tokens": ["Ge\u00b7ern\u00b7tet", ",", "was", "einst", "mit", "Muth", "ge\u00b7s\u00e4t", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRELS", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Arbeit lohnende Fr\u00fcchte \u2013", "tokens": ["Der", "Ar\u00b7beit", "loh\u00b7nen\u00b7de", "Fr\u00fcch\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Empfindung rein! doch, o so sp\u00e4t,", "tokens": ["Emp\u00b7fin\u00b7dung", "rein", "!", "doch", ",", "o", "so", "sp\u00e4t", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "ADV", "$,", "FM", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nur, damit sie fl\u00fcchte.", "tokens": ["Und", "nur", ",", "da\u00b7mit", "sie", "fl\u00fcch\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ach Alles vergeht!", "tokens": ["Ach", "Al\u00b7les", "ver\u00b7geht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "PIS", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Was aber besteht?", "tokens": ["Was", "a\u00b7ber", "be\u00b7steht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Allzeit?", "tokens": ["All\u00b7zeit", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Der alte Schmerz, das ewge Leid.", "tokens": ["Der", "al\u00b7te", "Schmerz", ",", "das", "ew\u00b7ge", "Leid", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Genesung, holdes r\u00fchrendes Gl\u00fcck,", "tokens": ["Ge\u00b7ne\u00b7sung", ",", "hol\u00b7des", "r\u00fch\u00b7ren\u00b7des", "Gl\u00fcck", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Nach schweren kranken Tagen,", "tokens": ["Nach", "schwe\u00b7ren", "kran\u00b7ken", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du f\u00fchrest den N\u00fcchternen \u2013 zur\u00fcck", "tokens": ["Du", "f\u00fch\u00b7rest", "den", "N\u00fcch\u00b7ter\u00b7nen", "\u2013", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "PTKVZ"], "meter": "-+--+---+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zu eiteln Sorgen und Plagen.", "tokens": ["Zu", "ei\u00b7teln", "Sor\u00b7gen", "und", "Pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ach Alles vergeht!", "tokens": ["Ach", "Al\u00b7les", "ver\u00b7geht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "PIS", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Was aber besteht?", "tokens": ["Was", "a\u00b7ber", "be\u00b7steht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Allzeit?", "tokens": ["All\u00b7zeit", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Der alte Schmerz, das ewge Leid.", "tokens": ["Der", "al\u00b7te", "Schmerz", ",", "das", "ew\u00b7ge", "Leid", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Religion, du Hochgef\u00fchl von dem", "tokens": ["Re\u00b7li\u00b7gi\u00b7on", ",", "du", "Hoch\u00b7ge\u00b7f\u00fchl", "von", "dem"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "NN", "APPR", "ART"], "meter": "+-+--+-+--", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Was wahr, gut, ewig und sch\u00f6n ist,", "tokens": ["Was", "wahr", ",", "gut", ",", "e\u00b7wig", "und", "sch\u00f6n", "ist", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wohl labest du oft, fern von System,", "tokens": ["Wohl", "la\u00b7best", "du", "oft", ",", "fern", "von", "Sys\u00b7tem", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ADJD", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Seele, der Leidges geschehn ist \u2013", "tokens": ["Die", "See\u00b7le", ",", "der", "Leid\u00b7ges", "ge\u00b7schehn", "ist", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVPP", "VAFIN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Ach Alles \u2013 vergeht;", "tokens": ["Ach", "Al\u00b7les", "\u2013", "ver\u00b7geht", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ITJ", "PIS", "$(", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Was aber besteht?", "tokens": ["Was", "a\u00b7ber", "be\u00b7steht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Allzeit?", "tokens": ["All\u00b7zeit", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Der alte Schmerz, das ewge Leid.", "tokens": ["Der", "al\u00b7te", "Schmerz", ",", "das", "ew\u00b7ge", "Leid", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"l