{"textgrid.poem.28033": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die sterbens-freye tausend-Steuer", "tokens": ["Die", "ster\u00b7bens\u00b7freye", "tau\u00b7sen\u00b7dSteu\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ist, d\u00fcnkt mich, \u00fcbersichtig heuer;", "tokens": ["Ist", ",", "d\u00fcnkt", "mich", ",", "\u00fc\u00b7bers\u00b7ich\u00b7tig", "heu\u00b7er", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "VVFIN", "PPER", "$,", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie siht nur auff das Haupt, das steht,", "tokens": ["Sie", "siht", "nur", "auff", "das", "Haupt", ",", "das", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht aber, auff was F\u00fc\u00df es geht.", "tokens": ["Nicht", "a\u00b7ber", ",", "auff", "was", "F\u00fc\u00df", "es", "geht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "APPR", "PRELS", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}