{"textgrid.poem.34703": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich aber lag am Rande des Schiffes,", "tokens": ["Ich", "a\u00b7ber", "lag", "am", "Ran\u00b7de", "des", "Schif\u00b7fes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und schaute, tr\u00e4umenden Auges,", "tokens": ["Und", "schau\u00b7te", ",", "tr\u00e4u\u00b7men\u00b7den", "Au\u00b7ges", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hinab in das spiegelklare Wasser,", "tokens": ["Hin\u00b7ab", "in", "das", "spie\u00b7gel\u00b7kla\u00b7re", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und schaute tiefer und tiefer \u2013", "tokens": ["Und", "schau\u00b7te", "tie\u00b7fer", "und", "tie\u00b7fer", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Bis tief, im Meeresgrunde,", "tokens": ["Bis", "tief", ",", "im", "Mee\u00b7res\u00b7grun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJD", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Anfangs wie d\u00e4mmernde Nebel,", "tokens": ["An\u00b7fangs", "wie", "d\u00e4m\u00b7mern\u00b7de", "Ne\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Jedoch allm\u00e4hlich farbenbestimmter,", "tokens": ["Je\u00b7doch", "all\u00b7m\u00e4h\u00b7lich", "far\u00b7ben\u00b7be\u00b7stimm\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Kirchenkuppel und T\u00fcrme sich zeigten,", "tokens": ["Kir\u00b7chen\u00b7kup\u00b7pel", "und", "T\u00fcr\u00b7me", "sich", "zeig\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Und endlich, sonnenklar, eine ganze Stadt,", "tokens": ["Und", "end\u00b7lich", ",", "son\u00b7nen\u00b7klar", ",", "ei\u00b7ne", "gan\u00b7ze", "Stadt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Altert\u00fcmlich niederl\u00e4ndisch,", "tokens": ["Al\u00b7ter\u00b7t\u00fcm\u00b7lich", "nie\u00b7der\u00b7l\u00e4n\u00b7disch", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und menschenbelebt.", "tokens": ["Und", "men\u00b7schen\u00b7be\u00b7lebt", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "Bed\u00e4chtige M\u00e4nner, schwarzbem\u00e4ntelt,", "tokens": ["Be\u00b7d\u00e4ch\u00b7ti\u00b7ge", "M\u00e4n\u00b7ner", ",", "schwarz\u00b7be\u00b7m\u00e4n\u00b7telt", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Mit wei\u00dfen Halskrausen und Ehrenketten", "tokens": ["Mit", "wei\u00b7\u00dfen", "Hals\u00b7krau\u00b7sen", "und", "Eh\u00b7ren\u00b7ket\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.14": {"text": "Und langen Degen und langen Gesichtern,", "tokens": ["Und", "lan\u00b7gen", "De\u00b7gen", "und", "lan\u00b7gen", "Ge\u00b7sich\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Schreiten, \u00fcber den wimmelnden Marktplatz,", "tokens": ["Schrei\u00b7ten", ",", "\u00fc\u00b7ber", "den", "wim\u00b7meln\u00b7den", "Markt\u00b7platz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.16": {"text": "Nach dem treppenhohen Rathaus,", "tokens": ["Nach", "dem", "trep\u00b7pen\u00b7ho\u00b7hen", "Rat\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Wo steinerne Kaiserbilder", "tokens": ["Wo", "stei\u00b7ner\u00b7ne", "Kai\u00b7ser\u00b7bil\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "Wacht halten mit Zepter und Schwert.", "tokens": ["Wacht", "hal\u00b7ten", "mit", "Zep\u00b7ter", "und", "Schwert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.19": {"text": "Unferne, vor langen H\u00e4userreihn,", "tokens": ["Un\u00b7fer\u00b7ne", ",", "vor", "lan\u00b7gen", "H\u00e4u\u00b7ser\u00b7reihn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Wo spiegelblanke Fenster", "tokens": ["Wo", "spie\u00b7gel\u00b7blan\u00b7ke", "Fens\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Und pyramidisch beschnittene Linden,", "tokens": ["Und", "py\u00b7ra\u00b7mi\u00b7disch", "be\u00b7schnit\u00b7te\u00b7ne", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NE", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Wandeln seidenrauschende Jungfern,", "tokens": ["Wan\u00b7deln", "sei\u00b7den\u00b7rau\u00b7schen\u00b7de", "Jung\u00b7fern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.23": {"text": "Schlanke Leibchen, die Blumengesichter", "tokens": ["Schlan\u00b7ke", "Leib\u00b7chen", ",", "die", "Blu\u00b7men\u00b7ge\u00b7sich\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ART", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.24": {"text": "Sittsam umschlossen von schwarzen M\u00fctzchen", "tokens": ["Sitt\u00b7sam", "um\u00b7schlos\u00b7sen", "von", "schwar\u00b7zen", "M\u00fctz\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "APPR", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.25": {"text": "Und hervorquellendem Goldhaar.", "tokens": ["Und", "her\u00b7vor\u00b7quel\u00b7len\u00b7dem", "Gold\u00b7haar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+--++", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Bunte Gesellen, in spanischer Tracht,", "tokens": ["Bun\u00b7te", "Ge\u00b7sel\u00b7len", ",", "in", "spa\u00b7ni\u00b7scher", "Tracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.27": {"text": "Stolzieren vor\u00fcber und nicken.", "tokens": ["Stol\u00b7zie\u00b7ren", "vor\u00b7\u00fc\u00b7ber", "und", "ni\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.28": {"text": "Bejahrte Frauen,", "tokens": ["Be\u00b7jahr\u00b7te", "Frau\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.29": {"text": "In braunen, verschollnen Gew\u00e4ndern,", "tokens": ["In", "brau\u00b7nen", ",", "ver\u00b7scholl\u00b7nen", "Ge\u00b7w\u00e4n\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.30": {"text": "Gesangbuch und Rosenkranz in der Hand,", "tokens": ["Ge\u00b7sang\u00b7buch", "und", "Ro\u00b7sen\u00b7kranz", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "Eilen, trippelnden Schritts,", "tokens": ["Ei\u00b7len", ",", "trip\u00b7peln\u00b7den", "Schritts", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.32": {"text": "Nach dem gro\u00dfen Dome,", "tokens": ["Nach", "dem", "gro\u00b7\u00dfen", "Do\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.33": {"text": "Getrieben von Glockengel\u00e4ute", "tokens": ["Ge\u00b7trie\u00b7ben", "von", "Glo\u00b7cken\u00b7ge\u00b7l\u00e4u\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "APPR", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.34": {"text": "Und rauschendem Orgelton.", "tokens": ["Und", "rau\u00b7schen\u00b7dem", "Or\u00b7gel\u00b7ton", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Mich selbst ergreift des fernen Klangs", "tokens": ["Mich", "selbst", "er\u00b7greift", "des", "fer\u00b7nen", "Klangs"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geheimnisvoller Schauer!", "tokens": ["Ge\u00b7heim\u00b7nis\u00b7vol\u00b7ler", "Schau\u00b7er", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Unendliches Sehnen, tiefe Wehmut", "tokens": ["Un\u00b7end\u00b7li\u00b7ches", "Seh\u00b7nen", ",", "tie\u00b7fe", "Weh\u00b7mut"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Beschleicht mein Herz,", "tokens": ["Be\u00b7schleicht", "mein", "Herz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Mein kaum geheiltes Herz; \u2013", "tokens": ["Mein", "kaum", "ge\u00b7heil\u00b7tes", "Herz", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADV", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mir ist, als w\u00fcrden seine Wunden", "tokens": ["Mir", "ist", ",", "als", "w\u00fcr\u00b7den", "sei\u00b7ne", "Wun\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Von lieben Lippen aufgek\u00fc\u00dft,", "tokens": ["Von", "lie\u00b7ben", "Lip\u00b7pen", "auf\u00b7ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und t\u00e4ten wieder bluten \u2013", "tokens": ["Und", "t\u00e4\u00b7ten", "wie\u00b7der", "blu\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Hei\u00dfe, rote Tropfen,", "tokens": ["Hei\u00b7\u00dfe", ",", "ro\u00b7te", "Trop\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Die lang und langsam niederfall'n", "tokens": ["Die", "lang", "und", "lang\u00b7sam", "nie\u00b7der\u00b7fall'n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Auf ein altes Haus, dort unten", "tokens": ["Auf", "ein", "al\u00b7tes", "Haus", ",", "dort", "un\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "In der tiefen Meerstadt,", "tokens": ["In", "der", "tie\u00b7fen", "Meer\u00b7stadt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Auf ein altes, hochgegiebeltes Haus,", "tokens": ["Auf", "ein", "al\u00b7tes", ",", "hoch\u00b7ge\u00b7gie\u00b7bel\u00b7tes", "Haus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.14": {"text": "Das melancholisch menschenleer ist,", "tokens": ["Das", "me\u00b7lan\u00b7cho\u00b7lisch", "men\u00b7schen\u00b7leer", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJD", "VAFIN", "$,"], "meter": "+-+-+---+", "measure": "unknown.measure.tetra"}, "line.15": {"text": "Nur da\u00df am untern Fenster", "tokens": ["Nur", "da\u00df", "am", "un\u00b7tern", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Ein M\u00e4dchen sitzt,", "tokens": ["Ein", "M\u00e4d\u00b7chen", "sitzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Den Kopf auf den Arm gest\u00fctzt,", "tokens": ["Den", "Kopf", "auf", "den", "Arm", "ge\u00b7st\u00fctzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "Wie ein armes, vergessenes Kind \u2013", "tokens": ["Wie", "ein", "ar\u00b7mes", ",", "ver\u00b7ges\u00b7se\u00b7nes", "Kind", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.19": {"text": "Und ich kenne dich, armes, vergessenes Kind!", "tokens": ["Und", "ich", "ken\u00b7ne", "dich", ",", "ar\u00b7mes", ",", "ver\u00b7ges\u00b7se\u00b7nes", "Kind", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.3": {"line.1": {"text": "So tief, meertief also", "tokens": ["So", "tief", ",", "meer\u00b7tief", "al\u00b7so"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Verstecktest du dich vor mir,", "tokens": ["Ver\u00b7steck\u00b7test", "du", "dich", "vor", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPER", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Aus kindischer Laune,", "tokens": ["Aus", "kin\u00b7di\u00b7scher", "Lau\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und konntest nicht mehr herauf,", "tokens": ["Und", "konn\u00b7test", "nicht", "mehr", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und sa\u00dfest fremd unter fremden Leuten,", "tokens": ["Und", "sa\u00b7\u00dfest", "fremd", "un\u00b7ter", "frem\u00b7den", "Leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Jahrhundertelang,", "tokens": ["Jahr\u00b7hun\u00b7der\u00b7te\u00b7lang", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Derweilen ich, die Seele voll Gram,", "tokens": ["Der\u00b7wei\u00b7len", "ich", ",", "die", "See\u00b7le", "voll", "Gram", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Auf der ganzen Erde dich suchte,", "tokens": ["Auf", "der", "gan\u00b7zen", "Er\u00b7de", "dich", "such\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Und immer dich suchte,", "tokens": ["Und", "im\u00b7mer", "dich", "such\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Du Immergeliebte,", "tokens": ["Du", "Im\u00b7mer\u00b7ge\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Du L\u00e4ngstverlorene,", "tokens": ["Du", "L\u00e4ngst\u00b7ver\u00b7lo\u00b7re\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.12": {"text": "Du Endlichgefundene \u2013", "tokens": ["Du", "End\u00b7lich\u00b7ge\u00b7fun\u00b7de\u00b7ne", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$("], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.13": {"text": "Ich hab dich gefunden und schaue wieder", "tokens": ["Ich", "hab", "dich", "ge\u00b7fun\u00b7den", "und", "schau\u00b7e", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "KON", "VVFIN", "ADV"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Dein s\u00fc\u00dfes Gesicht,", "tokens": ["Dein", "s\u00fc\u00b7\u00dfes", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Die klugen, treuen Augen,", "tokens": ["Die", "klu\u00b7gen", ",", "treu\u00b7en", "Au\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das liebe L\u00e4cheln \u2013", "tokens": ["Das", "lie\u00b7be", "L\u00e4\u00b7cheln", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und nimmer will ich dich wieder verlassen,", "tokens": ["Und", "nim\u00b7mer", "will", "ich", "dich", "wie\u00b7der", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und ich komme hinab zu dir,", "tokens": ["Und", "ich", "kom\u00b7me", "hin\u00b7ab", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und mit ausgebreiteten Armen", "tokens": ["Und", "mit", "aus\u00b7ge\u00b7brei\u00b7te\u00b7ten", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "St\u00fcrz ich hinab an dein Herz \u2013", "tokens": ["St\u00fcrz", "ich", "hin\u00b7ab", "an", "dein", "Herz", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Aber zur rechten Zeit noch", "tokens": ["A\u00b7ber", "zur", "rech\u00b7ten", "Zeit", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "ADV"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Ergriff mich beim Fu\u00df der Kapit\u00e4n,", "tokens": ["Er\u00b7griff", "mich", "beim", "Fu\u00df", "der", "Ka\u00b7pi\u00b7t\u00e4n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und zog mich vom Schiffsrand,", "tokens": ["Und", "zog", "mich", "vom", "Schiffs\u00b7rand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und rief, \u00e4rgerlich lachend:", "tokens": ["Und", "rief", ",", "\u00e4r\u00b7ger\u00b7lich", "la\u00b7chend", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJD", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbdoktor, sind Sie des Teufels?\u00ab", "tokens": ["\u00bb", "dok\u00b7tor", ",", "sind", "Sie", "des", "Teu\u00b7fels", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM.la", "$,", "VAFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}