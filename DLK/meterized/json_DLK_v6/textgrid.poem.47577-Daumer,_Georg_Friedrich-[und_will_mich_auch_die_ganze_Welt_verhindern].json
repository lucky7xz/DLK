{"textgrid.poem.47577": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[und will mich auch die ganze Welt verhindern]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und will mich auch die ganze Welt verhindern,", "tokens": ["Und", "will", "mich", "auch", "die", "gan\u00b7ze", "Welt", "ver\u00b7hin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geist und Gem\u00fcth zu opfern sch\u00f6nen Kindern,", "tokens": ["Geist", "und", "Ge\u00b7m\u00fcth", "zu", "op\u00b7fern", "sch\u00f6\u00b7nen", "Kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKZU", "VVINF", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Nie wird sie doch in mir die Stimme Gottes", "tokens": ["Nie", "wird", "sie", "doch", "in", "mir", "die", "Stim\u00b7me", "Got\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und meinen freudigen Gehorsam mindern.", "tokens": ["Und", "mei\u00b7nen", "freu\u00b7di\u00b7gen", "Ge\u00b7hor\u00b7sam", "min\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}