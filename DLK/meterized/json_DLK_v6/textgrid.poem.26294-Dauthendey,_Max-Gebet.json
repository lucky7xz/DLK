{"textgrid.poem.26294": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Gebet", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Allmachtgott, du naher,", "tokens": ["All\u00b7macht\u00b7gott", ",", "du", "na\u00b7her", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Seit ich zu dir halte,", "tokens": ["Seit", "ich", "zu", "dir", "hal\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wird mein Kummer leichter,", "tokens": ["Wird", "mein", "Kum\u00b7mer", "leich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Glatt die Sorgenfalte.", "tokens": ["Glatt", "die", "Sor\u00b7gen\u00b7fal\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Gott der hellen H\u00f6he,", "tokens": ["Gott", "der", "hel\u00b7len", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Gott der klaren Tiefe,", "tokens": ["Gott", "der", "kla\u00b7ren", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Selig machst du alle.", "tokens": ["Se\u00b7lig", "machst", "du", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PIS", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Da\u00df dich jeder riefe!", "tokens": ["Da\u00df", "dich", "je\u00b7der", "rie\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Bist die Lebensruhe", "tokens": ["Bist", "die", "Le\u00b7bens\u00b7ru\u00b7he"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "An der wilden Stra\u00dfe.", "tokens": ["An", "der", "wil\u00b7den", "Stra\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Bist die Lebenswelle", "tokens": ["Bist", "die", "Le\u00b7bens\u00b7wel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "In der toten Masse.", "tokens": ["In", "der", "to\u00b7ten", "Mas\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Bin dir voll ergeben.", "tokens": ["Bin", "dir", "voll", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Gl\u00fccklich macht dein Wille.", "tokens": ["Gl\u00fcck\u00b7lich", "macht", "dein", "Wil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Dein Wunsch ist mir Freude,", "tokens": ["Dein", "Wunsch", "ist", "mir", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Gott der Lebensf\u00fclle.", "tokens": ["Gott", "der", "Le\u00b7bens\u00b7f\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}