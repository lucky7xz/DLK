{"dta.poem.18985": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "12.  \n  Vnendliche Liebs pein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Ich brenn au\u00df lieb vnd lust/ doch kan der brunst", "tokens": ["Ich", "brenn", "au\u00df", "lieb", "vnd", "lust", "/", "doch", "kan", "der", "brunst"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJD", "KON", "VVFIN", "$(", "ADV", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "verdru\u00df", "tokens": ["ver\u00b7dru\u00df"], "token_info": ["word"], "pos": ["NN"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Meines haupts feuchtigkeit vnd thr\u00e4nen nicht", "tokens": ["Mei\u00b7nes", "haupts", "feuch\u00b7tig\u00b7keit", "vnd", "thr\u00e4\u00b7nen", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN", "PTKNEG"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "verz\u00f6hren:", "tokens": ["ver\u00b7z\u00f6h\u00b7ren", ":"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Ich wein au\u00df lieb vnd layd/ doch kan mein zeher-", "tokens": ["Ich", "wein", "au\u00df", "lieb", "vnd", "layd", "/", "doch", "kan", "mein", "ze\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "APPR", "ADJD", "KON", "NN", "$(", "ADV", "VMFIN", "PPOSAT", "TRUNC"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "flu\u00df", "tokens": ["flu\u00df"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Meiner brust grossen brunst vnd flammen gar", "tokens": ["Mei\u00b7ner", "brust", "gros\u00b7sen", "brunst", "vnd", "flam\u00b7men", "gar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "KON", "VVFIN", "ADV"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "nicht w\u00f6hren.", "tokens": ["nicht", "w\u00f6h\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Ja/ vilmehr pfleget stehts meiner brunst vberflu\u00df", "tokens": ["Ja", "/", "vil\u00b7mehr", "pfle\u00b7get", "stehts", "mei\u00b7ner", "brunst", "vberf\u00b7lu\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$(", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den quellen meines layds die nahr\u0169g zu besch\u00f6r\u1ebd:", "tokens": ["Den", "quel\u00b7len", "mei\u00b7nes", "layds", "die", "nahr\u0169g", "zu", "be\u00b7sch\u00f6r\u1ebd", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Ja/ vilmehr pfleget stehts meines layds zehergu\u00df", "tokens": ["Ja", "/", "vil\u00b7mehr", "pfle\u00b7get", "stehts", "mei\u00b7nes", "layds", "ze\u00b7her\u00b7gu\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$(", "ADV", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die flam\u0303en meiner Lieb zu st\u00f6rcken vn\u0303 zu n\u00f6hren.", "tokens": ["Die", "flam\u0303en", "mei\u00b7ner", "Lieb", "zu", "st\u00f6r\u00b7cken", "v\u00f1", "zu", "n\u00f6h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "In dem mein weinen nu/ in dem nu meine brunst", "tokens": ["In", "dem", "mein", "wei\u00b7nen", "nu", "/", "in", "dem", "nu", "mei\u00b7ne", "brunst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PPOSAT", "ADJA", "ADV", "$(", "APPR", "ART", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Einander jhre hilff zu wechslen nicht verneinen/", "tokens": ["Ein\u00b7an\u00b7der", "jhre", "hilff", "zu", "wechs\u00b7len", "nicht", "ver\u00b7nei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "PTKNEG", "VVINF", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So leyd ich dise lieb/ vnd lieb das layd vmbsunst.", "tokens": ["So", "leyd", "ich", "di\u00b7se", "lieb", "/", "vnd", "lieb", "das", "layd", "vmbsunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$(", "KON", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dan findend in dem fewr/ das ewiglich mu\u00df scheine\u0304/", "tokens": ["Dan", "fin\u00b7dend", "in", "dem", "fewr", "/", "das", "e\u00b7wig\u00b7lich", "mu\u00df", "schein\u0113", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$(", "PDS", "ADV", "VMFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd in dem stehten flu\u00df der zehern keine gunst/", "tokens": ["Vnd", "in", "dem", "steh\u00b7ten", "flu\u00df", "der", "ze\u00b7hern", "kei\u00b7ne", "gunst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So mu\u00df (O schmertz!) mein hertz stehts brennen", "tokens": ["So", "mu\u00df", "(", "O", "schmertz", "!", ")", "mein", "hertz", "stehts", "bren\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "$(", "FM.xy", "FM.xy", "$.", "$(", "PPOSAT", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "vnd stehts weinen.", "tokens": ["vnd", "stehts", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}