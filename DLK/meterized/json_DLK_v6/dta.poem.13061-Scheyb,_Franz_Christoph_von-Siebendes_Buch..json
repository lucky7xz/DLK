{"dta.poem.13061": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ejedoch ich tr\u00f6ste mich. Es scheinet bey der Sach", "tokens": ["\u201e", "je\u00b7doch", "ich", "tr\u00f6s\u00b7te", "mich", ".", "Es", "schei\u00b7net", "bey", "der", "Sach"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "PPER", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "550\u201dNoch guter Rath zu seyn. Ich forsche besser nach.", "tokens": ["\"", "Noch", "gu\u00b7ter", "Rath", "zu", "seyn", ".", "Ich", "for\u00b7sche", "bes\u00b7ser", "nach", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJA", "NN", "PTKZU", "VAINF", "$.", "PPER", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201evernehmt, was ich vermein! ..... Es lie\u00df an einer Seite,", "tokens": ["\u201e", "ver\u00b7nehmt", ",", "was", "ich", "ver\u00b7mein", "!", ".....", "Es", "lie\u00df", "an", "ei\u00b7ner", "Sei\u00b7te", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "PWS", "PPER", "PTKVZ", "$.", "$(", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als wann man dorten sich schon wiederum entzweyte;", "tokens": ["Als", "wann", "man", "dor\u00b7ten", "sich", "schon", "wie\u00b7de\u00b7rum", "ent\u00b7zwey\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "PIS", "ADV", "PRF", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Allein man sp\u00fchrte nur den Vorhang einer Th\u00fcr:", "tokens": ["Al\u00b7lein", "man", "sp\u00fchr\u00b7te", "nur", "den", "Vor\u00b7hang", "ei\u00b7ner", "Th\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Durch dessen Oeffnung ward der alte Redner irr.", "tokens": ["Durch", "des\u00b7sen", "Oeff\u00b7nung", "ward", "der", "al\u00b7te", "Red\u00b7ner", "irr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "VAFIN", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "555Ich nahme wahr, da\u00df er in Mi\u00dfvergn\u00fcgen seye,", "tokens": ["nah\u00b7me", "wahr", ",", "da\u00df", "er", "in", "Mi\u00df\u00b7ver\u00b7gn\u00fc\u00b7gen", "se\u00b7ye", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "KOUS", "PPER", "APPR", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.8": {"text": "Da\u00df man durch das Ger\u00e4usch die Achtsamkeit zerstreue.", "tokens": ["Da\u00df", "man", "durch", "das", "Ge\u00b7r\u00e4usch", "die", "Acht\u00b7sam\u00b7keit", "zer\u00b7streu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Weil es den Augenblick in seiner Miene lie\u00df,", "tokens": ["Weil", "es", "den", "Au\u00b7gen\u00b7blick", "in", "sei\u00b7ner", "Mie\u00b7ne", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Gleich als ob er Verdru\u00df und Wiederwillen wies.", "tokens": ["Gleich", "als", "ob", "er", "Ver\u00b7dru\u00df", "und", "Wie\u00b7der\u00b7wil\u00b7len", "wies", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "KOUS", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Er wandte das Gesicht, die finstern Augen-Lieder,", "tokens": ["Er", "wand\u00b7te", "das", "Ge\u00b7sicht", ",", "die", "fins\u00b7tern", "Au\u00b7gen\u00b7Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "560Der Blicke Regungen, die Stellung hin und wieder.", "tokens": ["Bli\u00b7cke", "Re\u00b7gun\u00b7gen", ",", "die", "Stel\u00b7lung", "hin", "und", "wie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ART", "NN", "PTKVZ", "KON", "ADV", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.13": {"text": "Sieh da! welch\u2019 angenehm\u2019 und reizende Person", "tokens": ["Sieh", "da", "!", "welch'", "an\u00b7ge\u00b7nehm'", "und", "rei\u00b7zen\u00b7de", "Per\u00b7son"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$.", "PWAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Kam ungef\u00e4hr daher, und gieng fast bi\u00df zum Thron?", "tokens": ["Kam", "un\u00b7ge\u00b7f\u00e4hr", "da\u00b7her", ",", "und", "gieng", "fast", "bi\u00df", "zum", "Thron", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PAV", "$,", "KON", "VVFIN", "ADV", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sie wies holds\u00e4lige, liebreich\u2019 und sanfte Mienen,", "tokens": ["Sie", "wies", "hold\u00b7s\u00e4\u00b7li\u00b7ge", ",", "lieb\u00b7reich'", "und", "sanf\u00b7te", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "$,", "VVFIN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Aus denen Sittsamkeit und Tugend-Reiz erschienen.", "tokens": ["Aus", "de\u00b7nen", "Sitt\u00b7sam\u00b7keit", "und", "Tu\u00b7gen\u00b7dReiz", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "565Ein Zepter-gleicher Stab in ihrer zarten Hand", "tokens": ["Zep\u00b7ter\u00b7glei\u00b7cher", "Stab", "in", "ih\u00b7rer", "zar\u00b7ten", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.18": {"text": "War, was die Gegenwart vor wunderbar befand:", "tokens": ["War", ",", "was", "die", "Ge\u00b7gen\u00b7wart", "vor", "wun\u00b7der\u00b7bar", "be\u00b7fand", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PRELS", "ART", "NN", "APPR", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Auf dessen Spize stund ein Aug in scharffen Blizen,", "tokens": ["Auf", "des\u00b7sen", "Spi\u00b7ze", "stund", "ein", "Aug", "in", "scharf\u00b7fen", "Bli\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Als wann es diesen Rath im Kreise s\u00e4he sizen.", "tokens": ["Als", "wann", "es", "die\u00b7sen", "Rath", "im", "Krei\u00b7se", "s\u00e4\u00b7he", "si\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "PPER", "PDAT", "NN", "APPRART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Sonst aber hatte sie nur auf sich selber acht,", "tokens": ["Sonst", "a\u00b7ber", "hat\u00b7te", "sie", "nur", "auf", "sich", "sel\u00b7ber", "acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "APPR", "PRF", "ADV", "CARD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "570Bi\u00df endlich sie, warum sie kame, vorgebracht.", "tokens": ["end\u00b7lich", "sie", ",", "wa\u00b7rum", "sie", "ka\u00b7me", ",", "vor\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.23": {"text": "Das Wort, so man vernahm, aus ihren Lippen fliessen,", "tokens": ["Das", "Wort", ",", "so", "man", "ver\u00b7nahm", ",", "aus", "ih\u00b7ren", "Lip\u00b7pen", "flies\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PIS", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Schien ihrer Stimme Klang und Anmuth zu vers\u00fcssen.", "tokens": ["Schien", "ih\u00b7rer", "Stim\u00b7me", "Klang", "und", "An\u00b7muth", "zu", "ver\u00b7s\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}