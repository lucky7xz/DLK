{"textgrid.poem.59695": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Heimweh nach R\u00fcgen", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Land der dunklen Haine,", "tokens": ["O", "Land", "der", "dunk\u00b7len", "Hai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Glanz der blauen See,", "tokens": ["O", "Glanz", "der", "blau\u00b7en", "See", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Eiland, das ich meine,", "tokens": ["O", "Ei\u00b7land", ",", "das", "ich", "mei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie tut's nach dir mir weh!", "tokens": ["Wie", "tut's", "nach", "dir", "mir", "weh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPR", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nach Fluchten und nach Z\u00fcgen", "tokens": ["Nach", "Fluch\u00b7ten", "und", "nach", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Weit \u00fcber Land und Meer,", "tokens": ["Weit", "\u00fc\u00b7ber", "Land", "und", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mein trautes L\u00e4ndchen R\u00fcgen,", "tokens": ["Mein", "trau\u00b7tes", "L\u00e4nd\u00b7chen", "R\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie mahnst du mich so sehr!", "tokens": ["Wie", "mahnst", "du", "mich", "so", "sehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O wie, mit goldnen S\u00e4umen", "tokens": ["O", "wie", ",", "mit", "gold\u00b7nen", "S\u00e4u\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PWAV", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Fl\u00fcgel rings umwebt,", "tokens": ["Die", "Fl\u00fc\u00b7gel", "rings", "um\u00b7webt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit M\u00e4rchen und mit Tr\u00e4umen", "tokens": ["Mit", "M\u00e4r\u00b7chen", "und", "mit", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erinnrung zu mir schwebt!", "tokens": ["E\u00b7rinn\u00b7rung", "zu", "mir", "schwebt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie hebt von grauen Jahren", "tokens": ["Sie", "hebt", "von", "grau\u00b7en", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den dunkeln Schleier auf,", "tokens": ["Den", "dun\u00b7keln", "Schlei\u00b7er", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Von Wiegen und von Bahren,", "tokens": ["Von", "Wie\u00b7gen", "und", "von", "Bah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und Tr\u00e4nen fallen drauf.", "tokens": ["Und", "Tr\u00e4\u00b7nen", "fal\u00b7len", "drauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O Eiland gr\u00fcner K\u00fcsten!", "tokens": ["O", "Ei\u00b7land", "gr\u00fc\u00b7ner", "K\u00fcs\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O bunter Himmelschein!", "tokens": ["O", "bun\u00b7ter", "Him\u00b7mel\u00b7schein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie schlief an deinen Br\u00fcsten", "tokens": ["Wie", "schlief", "an", "dei\u00b7nen", "Br\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Knabe selig ein!", "tokens": ["Der", "Kna\u00b7be", "se\u00b7lig", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Wiegenlieder sangen", "tokens": ["Die", "Wie\u00b7gen\u00b7lie\u00b7der", "san\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Wellen aus der See,", "tokens": ["Die", "Wel\u00b7len", "aus", "der", "See", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und Engelharfen klangen", "tokens": ["Und", "En\u00b7gel\u00b7har\u00b7fen", "klan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Hernieder aus der H\u00f6h'.", "tokens": ["Her\u00b7nie\u00b7der", "aus", "der", "H\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und deine Heldenm\u00e4ler", "tokens": ["Und", "dei\u00b7ne", "Hel\u00b7den\u00b7m\u00e4\u00b7ler"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit moosgewobnem Kleid,", "tokens": ["Mit", "moos\u00b7ge\u00b7wob\u00b7nem", "Kleid", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was k\u00fcnden sie, Erz\u00e4hler", "tokens": ["Was", "k\u00fcn\u00b7den", "sie", ",", "Er\u00b7z\u00e4h\u00b7ler"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus tapfrer V\u00e4ter Zeit,", "tokens": ["Aus", "tapf\u00b7rer", "V\u00e4\u00b7ter", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von edler Tode Ehren", "tokens": ["Von", "ed\u00b7ler", "To\u00b7de", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auf fl\u00fccht'gem Segelro\u00df,", "tokens": ["Auf", "fl\u00fccht'\u00b7gem", "Se\u00b7gel\u00b7ro\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Von Schwertern und von Speeren", "tokens": ["Von", "Schwer\u00b7tern", "und", "von", "Spee\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und Schildesklang und -sto\u00df?", "tokens": ["Und", "Schil\u00b7des\u00b7klang", "und", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So locken deine Minnen", "tokens": ["So", "lo\u00b7cken", "dei\u00b7ne", "Min\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit l\u00e4ngst verklungnem Gl\u00fcck", "tokens": ["Mit", "l\u00e4ngst", "ver\u00b7klung\u00b7nem", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den grauen Tr\u00e4umer hinnen", "tokens": ["Den", "grau\u00b7en", "Tr\u00e4u\u00b7mer", "hin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In alter Lust zur\u00fcck.", "tokens": ["In", "al\u00b7ter", "Lust", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O hei\u00dfes Herzenssehnen!", "tokens": ["O", "hei\u00b7\u00dfes", "Her\u00b7zens\u00b7seh\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "O goldner Tage Schein", "tokens": ["O", "gold\u00b7ner", "Ta\u00b7ge", "Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Von Liebe reich und Tr\u00e4nen!", "tokens": ["Von", "Lie\u00b7be", "reich", "und", "Tr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Schon liegt mein Grab am Rhein.", "tokens": ["Schon", "liegt", "mein", "Grab", "am", "Rhein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Fern, fern vom Heimatlande", "tokens": ["Fern", ",", "fern", "vom", "Hei\u00b7mat\u00b7lan\u00b7de"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "APPRART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Liegt Haus und Grab am Rhein.", "tokens": ["Liegt", "Haus", "und", "Grab", "am", "Rhein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "APPRART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nie werd' an deinem Strande", "tokens": ["Nie", "werd'", "an", "dei\u00b7nem", "Stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich wieder Pilger sein.", "tokens": ["Ich", "wie\u00b7der", "Pil\u00b7ger", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drum gr\u00fc\u00df' ich aus der Ferne", "tokens": ["Drum", "gr\u00fc\u00df'", "ich", "aus", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dich, Eiland lieb und gr\u00fcn:", "tokens": ["Dich", ",", "Ei\u00b7land", "lieb", "und", "gr\u00fcn", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sollst unterm besten Sterne", "tokens": ["Sollst", "un\u00b7term", "bes\u00b7ten", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Himmels ewig bl\u00fchn!", "tokens": ["Des", "Him\u00b7mels", "e\u00b7wig", "bl\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}