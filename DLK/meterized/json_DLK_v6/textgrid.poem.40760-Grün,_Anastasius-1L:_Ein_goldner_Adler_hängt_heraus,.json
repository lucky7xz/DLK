{"textgrid.poem.40760": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein goldner Adler h\u00e4ngt heraus,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein goldner Adler h\u00e4ngt heraus,", "tokens": ["Ein", "gold\u00b7ner", "Ad\u00b7ler", "h\u00e4ngt", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch nennt man nicht nach ihm das Haus;", "tokens": ["Doch", "nennt", "man", "nicht", "nach", "ihm", "das", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PTKNEG", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer d\u00e4chte noch ans Au\u00dfenschild,", "tokens": ["Wer", "d\u00e4ch\u00b7te", "noch", "ans", "Au\u00b7\u00dfen\u00b7schild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn drin solch lieblich Frauenbild?", "tokens": ["Wenn", "drin", "solch", "lieb\u00b7lich", "Frau\u00b7en\u00b7bild", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbzur sch\u00f6nen Wirthin\u00ab hei\u00dft das Haus,", "tokens": ["\u00bb", "zur", "sch\u00f6\u00b7nen", "Wirt\u00b7hin", "\u00ab", "hei\u00dft", "das", "Haus", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "ADJA", "NN", "$(", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Manch schmucker Gast ging ein und aus", "tokens": ["Manch", "schmu\u00b7cker", "Gast", "ging", "ein", "und", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PTKVZ", "KON", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Auge trank mehr als der Mund,", "tokens": ["Das", "Au\u00b7ge", "trank", "mehr", "als", "der", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er schlich davon, im Herzen wund.", "tokens": ["Er", "schlich", "da\u00b7von", ",", "im", "Her\u00b7zen", "wund", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PAV", "$,", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Noch kannt' ich Wirthin nicht und Haus;", "tokens": ["Noch", "kannt'", "ich", "Wirt\u00b7hin", "nicht", "und", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "PTKNEG", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch sprach man nur den Namen aus,", "tokens": ["Doch", "sprach", "man", "nur", "den", "Na\u00b7men", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da klang mir wie Musik die Luft,", "tokens": ["Da", "klang", "mir", "wie", "Mu\u00b7sik", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "NN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Da sog ich Rebenbl\u00fcthenduft.", "tokens": ["Da", "sog", "ich", "Re\u00b7ben\u00b7bl\u00fc\u00b7then\u00b7duft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich sah's im Geist, ihr holdes Bild", "tokens": ["Ich", "sah's", "im", "Geist", ",", "ihr", "hol\u00b7des", "Bild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Macht Wides sanft und Rauhes mild,", "tokens": ["Macht", "Wi\u00b7des", "sanft", "und", "Rau\u00b7hes", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Becherprall, der Zechersang", "tokens": ["Der", "Be\u00b7cher\u00b7prall", ",", "der", "Ze\u00b7cher\u00b7sang"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ward Finkenschlag und Harfenklang.", "tokens": ["Ward", "Fin\u00b7ken\u00b7schlag", "und", "Har\u00b7fen\u00b7klang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Einst trat ich selbst in das Gela\u00df,", "tokens": ["Einst", "trat", "ich", "selbst", "in", "das", "Ge\u00b7la\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein M\u00fctterlein am Rocken sa\u00df;", "tokens": ["Ein", "M\u00fct\u00b7ter\u00b7lein", "am", "Ro\u00b7cken", "sa\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbwo ist die sch\u00f6ne Wirthin, wo?\u00ab", "tokens": ["\u00bb", "wo", "ist", "die", "sch\u00f6\u00b7ne", "Wirt\u00b7hin", ",", "wo", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "PWAV", "VAFIN", "ART", "ADJA", "NN", "$,", "PWAV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Wehmut sprach's: \u00bbIch hie\u00df einst so.\u00ab", "tokens": ["In", "Weh\u00b7mut", "sprach's", ":", "\u00bb", "Ich", "hie\u00df", "einst", "so", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "VVFIN", "$.", "$(", "PPER", "VVFIN", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Am Fenstersims ein Rosenpaar", "tokens": ["Am", "Fens\u00b7ter\u00b7sims", "ein", "Ro\u00b7sen\u00b7paar"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das sagte mir, wie einst es war;", "tokens": ["Das", "sag\u00b7te", "mir", ",", "wie", "einst", "es", "war", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "PWAV", "ADV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Kranz, der welk am Spiegel hing,", "tokens": ["Ein", "Kranz", ",", "der", "welk", "am", "Spie\u00b7gel", "hing", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erz\u00e4hlte still, wie's weiter ging.", "tokens": ["Er\u00b7z\u00e4hl\u00b7te", "still", ",", "wie's", "wei\u00b7ter", "ging", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nach Jahren wieder trat ich ein:", "tokens": ["Nach", "Jah\u00b7ren", "wie\u00b7der", "trat", "ich", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbwo mag die sch\u00f6ne Wirthin sein?\u00ab", "tokens": ["\u00bb", "wo", "mag", "die", "sch\u00f6\u00b7ne", "Wirt\u00b7hin", "sein", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "VMFIN", "ART", "ADJA", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vierschr\u00f6tig hob vom Schanktisch sich", "tokens": ["Vier\u00b7schr\u00f6\u00b7tig", "hob", "vom", "Schank\u00b7tisch", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPRART", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein feister Kerl: \u00bbDas bin jetzt ich.\u00ab", "tokens": ["Ein", "feis\u00b7ter", "Kerl", ":", "\u00bb", "Das", "bin", "jetzt", "ich", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$(", "PDS", "VAFIN", "ADV", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Als er das Haupt mich sch\u00fctteln sah,", "tokens": ["Als", "er", "das", "Haupt", "mich", "sch\u00fct\u00b7teln", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hinaus durchs Fenster wies er da,", "tokens": ["Hin\u00b7aus", "durchs", "Fens\u00b7ter", "wies", "er", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dorthin wo viele Kreuze stehn", "tokens": ["Dor\u00b7thin", "wo", "vie\u00b7le", "Kreu\u00b7ze", "stehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hohe Gr\u00e4ser dr\u00fcber wehn.", "tokens": ["Und", "ho\u00b7he", "Gr\u00e4\u00b7ser", "dr\u00fc\u00b7ber", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Die Zeit verstrich. Verfall und Graus,", "tokens": ["Die", "Zeit", "ver\u00b7strich", ".", "Ver\u00b7fall", "und", "Graus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$.", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gar wildes Volk zog seit durchs Haus;", "tokens": ["Gar", "wil\u00b7des", "Volk", "zog", "seit", "durchs", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Name blieb, denn Mensch und Flur", "tokens": ["Der", "Na\u00b7me", "blieb", ",", "denn", "Mensch", "und", "Flur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beh\u00fcten treu der Sch\u00f6nheit Spur.", "tokens": ["Be\u00b7h\u00fc\u00b7ten", "treu", "der", "Sch\u00f6n\u00b7heit", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "\u00bbzur sch\u00f6nen Wirthin\u00ab hei\u00dft das Haus,", "tokens": ["\u00bb", "zur", "sch\u00f6\u00b7nen", "Wirt\u00b7hin", "\u00ab", "hei\u00dft", "das", "Haus", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "ADJA", "NN", "$(", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch sprichst du heut den Namen aus,", "tokens": ["Doch", "sprichst", "du", "heut", "den", "Na\u00b7men", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Umschwebt ein Hauch den w\u00fcsten Ort", "tokens": ["Um\u00b7schwebt", "ein", "Hauch", "den", "w\u00fcs\u00b7ten", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So fromm, als st\u00fcnd' ein Kirchlein dort.", "tokens": ["So", "fromm", ",", "als", "st\u00fcnd'", "ein", "Kirch\u00b7lein", "dort", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Das ist kein Rebenbl\u00fcthenduft,", "tokens": ["Das", "ist", "kein", "Re\u00b7ben\u00b7bl\u00fc\u00b7then\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das zieht wie Weihrauch durch die Luft,", "tokens": ["Das", "zieht", "wie", "Weih\u00b7rauch", "durch", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zur Orgel ward der Rundgesang,", "tokens": ["Zur", "Or\u00b7gel", "ward", "der", "Rund\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Glockenschall der Becherklang;", "tokens": ["Zum", "Glo\u00b7cken\u00b7schall", "der", "Be\u00b7cher\u00b7klang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Das klingt dir wie Musik ins Ohr,", "tokens": ["Das", "klingt", "dir", "wie", "Mu\u00b7sik", "ins", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "KOKOM", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und auferweckt zum Maienflor", "tokens": ["Und", "au\u00b7fer\u00b7weckt", "zum", "Mai\u00b7en\u00b7flor"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Beginnen aus des Angers Gr\u00fcn", "tokens": ["Be\u00b7gin\u00b7nen", "aus", "des", "An\u00b7gers", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verwelkte Rosen aufzubl\u00fchn.", "tokens": ["Ver\u00b7welk\u00b7te", "Ro\u00b7sen", "auf\u00b7zu\u00b7bl\u00fchn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Dein leiblich Aug' sah sie noch nie,", "tokens": ["Dein", "leib\u00b7lich", "Aug'", "sah", "sie", "noch", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jetzt siehst du zwischen Rosen sie,", "tokens": ["Jetzt", "siehst", "du", "zwi\u00b7schen", "Ro\u00b7sen", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Frauenbildni\u00df wunderbar,", "tokens": ["Ein", "Frau\u00b7en\u00b7bild\u00b7ni\u00df", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur sch\u00f6ner noch, als je sie war.", "tokens": ["Nur", "sch\u00f6\u00b7ner", "noch", ",", "als", "je", "sie", "war", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "$,", "KOUS", "ADV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}