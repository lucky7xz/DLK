{"textgrid.poem.28146": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "61.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist ein reiner Mensch, O Jesu Christ, geboren,", "tokens": ["Du", "bist", "ein", "rei\u00b7ner", "Mensch", ",", "O", "Je\u00b7su", "Christ", ",", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "NE", "NE", "NE", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich verdammter Mensch durch dich sey unverloren.", "tokens": ["Da\u00df", "ich", "ver\u00b7damm\u00b7ter", "Mensch", "durch", "dich", "sey", "un\u00b7ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "APPR", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hilff, da\u00df auch ich nun dich behalte f\u00fcr und f\u00fcr,", "tokens": ["Hilff", ",", "da\u00df", "auch", "ich", "nun", "dich", "be\u00b7hal\u00b7te", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ADV", "PPER", "ADV", "PPER", "VVFIN", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dir nicht verliere mich, noch dich verliere mir!", "tokens": ["Dir", "nicht", "ver\u00b7lie\u00b7re", "mich", ",", "noch", "dich", "ver\u00b7lie\u00b7re", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVFIN", "PPER", "$,", "ADV", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}