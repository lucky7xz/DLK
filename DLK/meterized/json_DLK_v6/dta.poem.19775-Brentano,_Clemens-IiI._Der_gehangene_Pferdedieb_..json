{"dta.poem.19775": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "IiI.   Der gehangene Pferdedieb .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.85", "da:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der edle Thedel Unversehrt                 ", "tokens": ["Der", "ed\u00b7le", "The\u00b7del", "Un\u00b7ver\u00b7sehrt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach Braunschweig eilt auf seinem Pferd,", "tokens": ["Nach", "Braun\u00b7schweig", "eilt", "auf", "sei\u00b7nem", "Pferd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu Herzog Heinrichs Ehgemahl,", "tokens": ["Zu", "Her\u00b7zog", "Hein\u00b7richs", "Eh\u00b7ge\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihren Kindern sprach im Saal:", "tokens": ["Und", "ih\u00b7ren", "Kin\u00b7dern", "sprach", "im", "Saal", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u201eder Herzog w\u00fcnscht euch so viel gute Nacht", "tokens": ["\u201e", "der", "Her\u00b7zog", "w\u00fcnscht", "euch", "so", "viel", "gu\u00b7te", "Nacht"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "VVFIN", "PPER", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "\u201eals manch roth M\u00fcndlein in dem Jahre lacht,", "tokens": ["\u201e", "als", "manch", "roth", "M\u00fcnd\u00b7lein", "in", "dem", "Jah\u00b7re", "lacht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PIAT", "ADJD", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "\u201eso viel als gr\u00fcne Grasstiel sind,", "tokens": ["\u201e", "so", "viel", "als", "gr\u00fc\u00b7ne", "Gras\u00b7stiel", "sind", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PIAT", "KOKOM", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u201edie man am Weg zum Grabe findt,", "tokens": ["\u201e", "die", "man", "am", "Weg", "zum", "Gra\u00b7be", "findt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "PIS", "APPRART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "\u201evon wo er diese Briefe sandt,", "tokens": ["\u201e", "von", "wo", "er", "die\u00b7se", "Brie\u00b7fe", "sandt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PWAV", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "\u201edie \u00fcbergiebt euch meine Hand.\u201c", "tokens": ["\u201e", "die", "\u00fc\u00b7berg\u00b7iebt", "euch", "mei\u00b7ne", "Hand", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die F\u00fcrstin k\u00fc\u00dft die Brief f\u00fcrwahr,", "tokens": ["Die", "F\u00fcrs\u00b7tin", "k\u00fc\u00dft", "die", "Brief", "f\u00fcr\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Mit Weinen, Seufzen spricht sie dar:", "tokens": ["Mit", "Wei\u00b7nen", ",", "Seuf\u00b7zen", "spricht", "sie", "dar", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "\u201egott lohn es dir, mein edler Herr,", "tokens": ["\u201e", "gott", "lohn", "es", "dir", ",", "mein", "ed\u00b7ler", "Herr", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "\u201eich glaubt ihn todt und weinte sehr,", "tokens": ["\u201e", "ich", "glaubt", "ihn", "todt", "und", "wein\u00b7te", "sehr", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "\u201eaus seinen Schreiben ich befind,", "tokens": ["\u201e", "aus", "sei\u00b7nen", "Schrei\u00b7ben", "ich", "be\u00b7find", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "\u201ewohl wie sie zupetschieret sind,", "tokens": ["\u201e", "wohl", "wie", "sie", "zu\u00b7pet\u00b7schie\u00b7ret", "sind", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "KOKOM", "PPER", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "\u201edu sollst hier trinken und auch essen", "tokens": ["\u201e", "du", "sollst", "hier", "trin\u00b7ken", "und", "auch", "es\u00b7sen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VMFIN", "ADV", "VVINF", "KON", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "\u201enach Nothdurft, bis wir sie gelesen.\u201c", "tokens": ["\u201e", "nach", "Noth\u00b7durft", ",", "bis", "wir", "sie", "ge\u00b7le\u00b7sen", ".", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "NN", "$,", "KOUS", "PPER", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Die F\u00fcrstin war sehr guter Ding,", "tokens": ["Die", "F\u00fcrs\u00b7tin", "war", "sehr", "gu\u00b7ter", "Ding", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Lie\u00df bringen einen goldnen Ring,", "tokens": ["Lie\u00df", "brin\u00b7gen", "ei\u00b7nen", "gold\u00b7nen", "Ring", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Auch einen Kranz von Golde gut,", "tokens": ["Auch", "ei\u00b7nen", "Kranz", "von", "Gol\u00b7de", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Der sa\u00df auf einem neuen Huth,", "tokens": ["Der", "sa\u00df", "auf", "ei\u00b7nem", "neu\u00b7en", "Huth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Sie wurd gereitzt zur Fr\u00f6hligkeit,", "tokens": ["Sie", "wurd", "ge\u00b7reitzt", "zur", "Fr\u00f6h\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Da\u00df sie ihm gab ein neues Kleid,", "tokens": ["Da\u00df", "sie", "ihm", "gab", "ein", "neu\u00b7es", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "All das dem Thedel zum Geschenk,", "tokens": ["All", "das", "dem", "The\u00b7del", "zum", "Ge\u00b7schenk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PDS", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Da\u00df er ihr Gnaden bey gedenk.", "tokens": ["Da\u00df", "er", "ihr", "Gna\u00b7den", "bey", "ge\u00b7denk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Dann sagt sie ihm:\u201eEin gutes Pferd", "tokens": ["Dann", "sagt", "sie", "ihm", ":", "\u201e", "Ein", "gu\u00b7tes", "Pferd"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$.", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "\u201em\u00fc\u00dft ihr wohl haben Unverfehrt,", "tokens": ["\u201e", "m\u00fc\u00dft", "ihr", "wohl", "ha\u00b7ben", "Un\u00b7ver\u00b7fehrt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "ADV", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "\u201eda\u00df ihr in zweyen Tagen hier? \u2014", "tokens": ["\u201e", "da\u00df", "ihr", "in", "zwe\u00b7yen", "Ta\u00b7gen", "hier", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "PPER", "APPR", "VVFIN", "NN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "\u201edaf\u00fcr gebt Gott die Ehr, nicht mir!\u201c", "tokens": ["\u201e", "da\u00b7f\u00fcr", "gebt", "Gott", "die", "Ehr", ",", "nicht", "mir", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PAV", "VVFIN", "NN", "ART", "NN", "$,", "PTKNEG", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Die F\u00fcrstin gab ihm ihre Hand,", "tokens": ["Die", "F\u00fcrs\u00b7tin", "gab", "ihm", "ih\u00b7re", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Eh dann sie ihn von dannen sandt,", "tokens": ["Eh", "dann", "sie", "ihn", "von", "dan\u00b7nen", "sandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "PPER", "APPR", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Der Thedel in die Herberg ging,", "tokens": ["Der", "The\u00b7del", "in", "die", "Her\u00b7berg", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Zu sagen also gleich anfing:", "tokens": ["Zu", "sa\u00b7gen", "al\u00b7so", "gleich", "an\u00b7fing", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "\u201eihr Knechte, da\u00df wir reiten, trachtet,", "tokens": ["\u201e", "ihr", "Knech\u00b7te", ",", "da\u00df", "wir", "rei\u00b7ten", ",", "trach\u00b7tet", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "\u201eherr Wirth genau die Rechnung machet.\u201c", "tokens": ["\u201e", "herr", "Wirth", "ge\u00b7nau", "die", "Rech\u00b7nung", "ma\u00b7chet", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "NN", "ADJD", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Der Wirth sprach:\u201eZieht in Gottes Geleit,", "tokens": ["Der", "Wirth", "sprach", ":", "\u201e", "Zieht", "in", "Got\u00b7tes", "Ge\u00b7leit", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "VVIMP", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.38": {"text": "\u201edie F\u00fcrstin hat bezahlet heut.\u201c", "tokens": ["\u201e", "die", "F\u00fcrs\u00b7tin", "hat", "be\u00b7zah\u00b7let", "heut", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Da nahm er g\u00fctlich sein Abschied", "tokens": ["Da", "nahm", "er", "g\u00fct\u00b7lich", "sein", "Ab\u00b7schied"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.40": {"text": "Zum Graf von Schladen er hinritt,", "tokens": ["Zum", "Graf", "von", "Schla\u00b7den", "er", "hin\u00b7ritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Doch fand er ihn nicht gleich zu Haus,", "tokens": ["Doch", "fand", "er", "ihn", "nicht", "gleich", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKNEG", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Er mu\u00dfte vor das Thor hinaus,", "tokens": ["Er", "mu\u00df\u00b7te", "vor", "das", "Thor", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "APZR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Gericht ward da gesprochen,", "tokens": ["Ge\u00b7richt", "ward", "da", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "Der Stab war schon gebrochen.", "tokens": ["Der", "Stab", "war", "schon", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.45": {"text": "\u201eder Pferdedieb ist schon gehangen,", "tokens": ["\u201e", "der", "Pfer\u00b7de\u00b7di\u00b7eb", "ist", "schon", "ge\u00b7han\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.46": {"text": "\u201ela\u00dft euch um euer sch\u00f6n Pferd nicht bangen.\u201c", "tokens": ["\u201e", "la\u00dft", "euch", "um", "eu\u00b7er", "sch\u00f6n", "Pferd", "nicht", "ban\u00b7gen", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "PPER", "APPR", "PPOSAT", "ADJD", "NN", "PTKNEG", "VVINF", "$.", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.47": {"text": "Der Graf ihn f\u00fchrt zu seinem Schlo\u00df,", "tokens": ["Der", "Graf", "ihn", "f\u00fchrt", "zu", "sei\u00b7nem", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Und freut sich \u00fcbers schwarze Ro\u00df.", "tokens": ["Und", "freut", "sich", "\u00fc\u00b7bers", "schwar\u00b7ze", "Ro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Das schwarze Ro\u00df, Herr Thedel spricht,", "tokens": ["Das", "schwar\u00b7ze", "Ro\u00df", ",", "Herr", "The\u00b7del", "spricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Das f\u00fcrcht selbst h\u00f6llsches Feuer nicht.", "tokens": ["Das", "f\u00fcrcht", "selbst", "h\u00f6ll\u00b7sches", "Feu\u00b7er", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Es ist wie ich, ich mach kein Kreutz", "tokens": ["Es", "ist", "wie", "ich", ",", "ich", "mach", "kein", "Kreutz"], "token_info": ["word", "word", "word", "word", "punct", "wo