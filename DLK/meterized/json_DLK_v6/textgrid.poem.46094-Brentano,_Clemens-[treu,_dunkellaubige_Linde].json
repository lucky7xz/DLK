{"textgrid.poem.46094": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[treu, dunkellaubige Linde]", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Treu, dunkellaubige Linde,", "tokens": ["Treu", ",", "dun\u00b7kel\u00b7lau\u00b7bi\u00b7ge", "Lin\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wenn rings die Windsbraut tobt,", "tokens": ["Wenn", "rings", "die", "Winds\u00b7braut", "tobt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein S\u00e4useln lieblich linde", "tokens": ["Dein", "S\u00e4u\u00b7seln", "lieb\u00b7lich", "lin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Frieden Gottes lobt.", "tokens": ["Den", "Frie\u00b7den", "Got\u00b7tes", "lobt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Treu, dunkellaubige Linde,", "tokens": ["Treu", ",", "dun\u00b7kel\u00b7lau\u00b7bi\u00b7ge", "Lin\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie f\u00e4hrt all Gut und Blut", "tokens": ["Wie", "f\u00e4hrt", "all", "Gut", "und", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Fort, fort im Sturm geschwinde,", "tokens": ["Fort", ",", "fort", "im", "Sturm", "ge\u00b7schwin\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PTKVZ", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur du hegst festen Mut,", "tokens": ["Nur", "du", "hegst", "fes\u00b7ten", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Treu, dunkellaubige Linde,", "tokens": ["Treu", ",", "dun\u00b7kel\u00b7lau\u00b7bi\u00b7ge", "Lin\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie bist du stark und gut,", "tokens": ["Wie", "bist", "du", "stark", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohl dem, der mit dem Kinde", "tokens": ["Wohl", "dem", ",", "der", "mit", "dem", "Kin\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bei dir im H\u00fcglein ruht.", "tokens": ["Bei", "dir", "im", "H\u00fcg\u00b7lein", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}