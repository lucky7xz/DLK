{"textgrid.poem.43257": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Warum der Liebelose wiederkam,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum der Liebelose wiederkam,", "tokens": ["Wa\u00b7rum", "der", "Lie\u00b7be\u00b7lo\u00b7se", "wie\u00b7der\u00b7kam", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das fragt mich oft Dein bang-beredter Blick,", "tokens": ["Das", "fragt", "mich", "oft", "Dein", "bang\u00b7be\u00b7red\u00b7ter", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ver\u00f6dung f\u00fchrte mich vielleicht zur\u00fcck,", "tokens": ["Ve\u00b7r\u00f6\u00b7dung", "f\u00fchr\u00b7te", "mich", "viel\u00b7leicht", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vielleicht ein Selbstgericht, vielleicht auch \u2013 Scham.", "tokens": ["Viel\u00b7leicht", "ein", "Selbst\u00b7ge\u00b7richt", ",", "viel\u00b7leicht", "auch", "\u2013", "Scham", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ADV", "$(", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn oft frug ich in dumpfer schw\u00fcler Nacht,", "tokens": ["Denn", "oft", "frug", "ich", "in", "dum\u00b7pfer", "schw\u00fc\u00b7ler", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was Dich, Du gro\u00dfes willenloses Kind,", "tokens": ["Was", "Dich", ",", "Du", "gro\u00b7\u00dfes", "wil\u00b7len\u00b7lo\u00b7ses", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "$,", "PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An mich gekettet einst so wahr, so blind", "tokens": ["An", "mich", "ge\u00b7ket\u00b7tet", "einst", "so", "wahr", ",", "so", "blind"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "VVPP", "ADV", "ADV", "ADJD", "$,", "ADV", "ADJD"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "An mich, der hart geredet und gedacht.", "tokens": ["An", "mich", ",", "der", "hart", "ge\u00b7re\u00b7det", "und", "ge\u00b7dacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "ADJD", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und forschend weilt' ich bei der Frage lang ...", "tokens": ["Und", "for\u00b7schend", "weilt'", "ich", "bei", "der", "Fra\u00b7ge", "lang", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bald wuchs an Dich der Glaube unbewu\u00dft,", "tokens": ["Bald", "wuchs", "an", "Dich", "der", "Glau\u00b7be", "un\u00b7be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und in der leeren, sturmzerw\u00fchlten Brust", "tokens": ["Und", "in", "der", "lee\u00b7ren", ",", "sturm\u00b7zer\u00b7w\u00fchl\u00b7ten", "Brust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gar mahnungsvoll Dein altes Lied erklang.", "tokens": ["Gar", "mah\u00b7nungs\u00b7voll", "Dein", "al\u00b7tes", "Lied", "er\u00b7klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}