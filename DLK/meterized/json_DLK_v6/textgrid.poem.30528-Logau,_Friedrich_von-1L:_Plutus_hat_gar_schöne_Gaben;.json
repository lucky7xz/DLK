{"textgrid.poem.30528": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Plutus hat gar sch\u00f6ne Gaben;", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Plutus hat gar sch\u00f6ne Gaben;", "tokens": ["Plu\u00b7tus", "hat", "gar", "sch\u00f6\u00b7ne", "Ga\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ieder wil sie von ihm haben;", "tokens": ["Ie\u00b7der", "wil", "sie", "von", "ihm", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kan sie, wann er wil, verschencken,", "tokens": ["Kan", "sie", ",", "wann", "er", "wil", ",", "ver\u00b7schen\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$,", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pflegt sich aber zu bedencken.", "tokens": ["Pflegt", "sich", "a\u00b7ber", "zu", "be\u00b7den\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}