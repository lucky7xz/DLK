{"textgrid.poem.25329": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: B\u00e4ume wei\u00df ich, fr\u00fchlingsstarke B\u00e4ume,", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "B\u00e4ume wei\u00df ich, fr\u00fchlingsstarke B\u00e4ume,", "tokens": ["B\u00e4u\u00b7me", "wei\u00df", "ich", ",", "fr\u00fch\u00b7lings\u00b7star\u00b7ke", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "denen g\u00e4rend der Jugend Saft", "tokens": ["de\u00b7nen", "g\u00e4\u00b7rend", "der", "Ju\u00b7gend", "Saft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "ART", "NN", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "durch gl\u00fchende Adern singt.", "tokens": ["durch", "gl\u00fc\u00b7hen\u00b7de", "A\u00b7dern", "singt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die lechzend verlangen", "tokens": ["Die", "lech\u00b7zend", "ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "nach dem Rausche der Erf\u00fcllung.", "tokens": ["nach", "dem", "Rau\u00b7sche", "der", "Er\u00b7f\u00fcl\u00b7lung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber noch starren sie kahl und stumm.", "tokens": ["A\u00b7ber", "noch", "star\u00b7ren", "sie", "kahl", "und", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Harte Schorfe ketten die vorschwellenden Triebe.", "tokens": ["Har\u00b7te", "Schor\u00b7fe", "ket\u00b7ten", "die", "vor\u00b7schwel\u00b7len\u00b7den", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Und in wilden Tr\u00e4umen nur langen sie empor", "tokens": ["Und", "in", "wil\u00b7den", "Tr\u00e4u\u00b7men", "nur", "lan\u00b7gen", "sie", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "ADV", "ADJA", "PPER", "PTKVZ"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "zu dem schaffenden Licht,", "tokens": ["zu", "dem", "schaf\u00b7fen\u00b7den", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.10": {"text": "da\u00df es sie bade in Glanz und Glut.", "tokens": ["da\u00df", "es", "sie", "ba\u00b7de", "in", "Glanz", "und", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Weiten sich ihre \u00c4ste, da\u00df gierig sie eins\u00f6gen", "tokens": ["Wei\u00b7ten", "sich", "ih\u00b7re", "\u00c4s\u00b7te", ",", "da\u00df", "gie\u00b7rig", "sie", "ein\u00b7s\u00f6\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "$,", "KOUS", "ADJD", "PPER", "VVINF"], "meter": "+--+-+--+-+-+-", "measure": "iambic.hexa.invert"}, "line.12": {"text": "den zauberstarken Most lauen Sommerregens,", "tokens": ["den", "zau\u00b7ber\u00b7star\u00b7ken", "Most", "lau\u00b7en", "Som\u00b7mer\u00b7re\u00b7gens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "zu erbl\u00fchen und zu leben gleich ihren Br\u00fcdern.", "tokens": ["zu", "er\u00b7bl\u00fc\u00b7hen", "und", "zu", "le\u00b7ben", "gleich", "ih\u00b7ren", "Br\u00fc\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "Denn noch kennen sie nicht den Sommerrausch", "tokens": ["Denn", "noch", "ken\u00b7nen", "sie", "nicht", "den", "Som\u00b7mer\u00b7rausch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.15": {"text": "der Erf\u00fcllung. Aber krachend durchw\u00fchlt ihren Leib", "tokens": ["der", "Er\u00b7f\u00fcl\u00b7lung", ".", "A\u00b7ber", "kra\u00b7chend", "durch\u00b7w\u00fchlt", "ih\u00b7ren", "Leib"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "KON", "ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "der Lenzstrom der Ahnung.", "tokens": ["der", "Lenz\u00b7strom", "der", "Ah\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Wanderer ziehen vor\u00fcber,", "tokens": ["Wan\u00b7de\u00b7rer", "zie\u00b7hen", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.18": {"text": "und also spricht einer zum anderen:", "tokens": ["und", "al\u00b7so", "spricht", "ei\u00b7ner", "zum", "an\u00b7de\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "APPRART", "ADJA", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.19": {"text": "\u00bbsehet die B\u00e4ume dort,", "tokens": ["\u00bb", "se\u00b7het", "die", "B\u00e4u\u00b7me", "dort", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.20": {"text": "wie kahl sie stehen und stumm!", "tokens": ["wie", "kahl", "sie", "ste\u00b7hen", "und", "stumm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.21": {"text": "Kalt schleppt sich ihr Blut, und m\u00fcrrisch fliehen", "tokens": ["Kalt", "schleppt", "sich", "ihr", "Blut", ",", "und", "m\u00fcr\u00b7risch", "flie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "PPOSAT", "NN", "$,", "KON", "ADJD", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "sie des Lenzes sanft wirkende Kraft.", "tokens": ["sie", "des", "Len\u00b7zes", "sanft", "wir\u00b7ken\u00b7de", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.23": {"text": "Lasset sie im Dunkeln, die Finstern! ...\u00ab", "tokens": ["Las\u00b7set", "sie", "im", "Dun\u00b7keln", ",", "die", "Fins\u00b7tern", "!", "...", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "$,", "ART", "NN", "$.", "$(", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.24": {"text": "So sprechen sie und gehen vorbei. \u2013", "tokens": ["So", "spre\u00b7chen", "sie", "und", "ge\u00b7hen", "vor\u00b7bei", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.25": {"text": "Und nicht einer,", "tokens": ["Und", "nicht", "ei\u00b7ner", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.26": {"text": "der s\u00e4he die st\u00fcrmenden Flammen der Sehnsucht,", "tokens": ["der", "s\u00e4\u00b7he", "die", "st\u00fcr\u00b7men\u00b7den", "Flam\u00b7men", "der", "Sehn\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.27": {"text": "die gierend aus ihren Augen lodern", "tokens": ["die", "gie\u00b7rend", "aus", "ih\u00b7ren", "Au\u00b7gen", "lo\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "und verzehrend \u00fcber ihnen zusammengluten ...", "tokens": ["und", "ver\u00b7zeh\u00b7rend", "\u00fc\u00b7ber", "ih\u00b7nen", "zu\u00b7sam\u00b7men\u00b7glu\u00b7ten", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "APPR", "PPER", "VVFIN", "$("], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}}}}}