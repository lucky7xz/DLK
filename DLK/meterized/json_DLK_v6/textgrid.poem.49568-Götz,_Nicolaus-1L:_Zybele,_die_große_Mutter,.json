{"textgrid.poem.49568": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zybele, die gro\u00dfe Mutter,", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zybele, die gro\u00dfe Mutter,", "tokens": ["Zy\u00b7be\u00b7le", ",", "die", "gro\u00b7\u00dfe", "Mut\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gab in ihrem goldnen Haus", "tokens": ["Gab", "in", "ih\u00b7rem", "gold\u00b7nen", "Haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "J\u00fcngst den G\u00f6ttern, ihren Kindern,", "tokens": ["J\u00fcngst", "den", "G\u00f6t\u00b7tern", ",", "ih\u00b7ren", "Kin\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen Ball und einen Schmaus!", "tokens": ["Ei\u00b7nen", "Ball", "und", "ei\u00b7nen", "Schmaus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nach den ernstlichsten Gespr\u00e4chen", "tokens": ["Nach", "den", "ernst\u00b7lichs\u00b7ten", "Ge\u00b7spr\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Von Regierung dieser Welt,", "tokens": ["Von", "Re\u00b7gie\u00b7rung", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ward in gro\u00dfen goldnen Schalen", "tokens": ["Ward", "in", "gro\u00b7\u00dfen", "gold\u00b7nen", "Scha\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00fc\u00dfer Nektar aufgestellt.", "tokens": ["S\u00fc\u00b7\u00dfer", "Nek\u00b7tar", "auf\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Trunken zankten alle G\u00f6tter", "tokens": ["Trun\u00b7ken", "zank\u00b7ten", "al\u00b7le", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit belebter Fantasey", "tokens": ["Mit", "be\u00b7leb\u00b7ter", "Fan\u00b7ta\u00b7sey"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob mein M\u00e4dchen, oder Zypris", "tokens": ["Ob", "mein", "M\u00e4d\u00b7chen", ",", "o\u00b7der", "Zyp\u00b7ris"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "KON", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Reizender und sch\u00f6ner sey?", "tokens": ["Rei\u00b7zen\u00b7der", "und", "sch\u00f6\u00b7ner", "sey", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Du bekamst der Sch\u00f6nheit Apfel,", "tokens": ["Du", "be\u00b7kamst", "der", "Sch\u00f6n\u00b7heit", "Ap\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mutter Amors, vom Vulkan!", "tokens": ["Mut\u00b7ter", "A\u00b7mors", ",", "vom", "Vul\u00b7kan", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "APPRART", "NE", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Momus und die andern G\u00f6tter", "tokens": ["Mo\u00b7mus", "und", "die", "an\u00b7dern", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ART", "ADJA", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Trugen ihn dem M\u00e4dchen an.", "tokens": ["Tru\u00b7gen", "ihn", "dem", "M\u00e4d\u00b7chen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}