{"textgrid.poem.45000": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Milit\u00e4rischer Staat", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit den Waffen in den H\u00e4nden,", "tokens": ["Mit", "den", "Waf\u00b7fen", "in", "den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das Volk zugleich ein Heer,", "tokens": ["Und", "das", "Volk", "zu\u00b7gleich", "ein", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Scheint die Knechtschaft leicht zu enden,", "tokens": ["Scheint", "die", "Knecht\u00b7schaft", "leicht", "zu", "en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Freiheit nicht so schwer.", "tokens": ["Und", "die", "Frei\u00b7heit", "nicht", "so", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch was kr\u00e4ftig macht nach au\u00dfen,", "tokens": ["Doch", "was", "kr\u00e4f\u00b7tig", "macht", "nach", "au\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJD", "VVFIN", "APPR", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist nach einw\u00e4rts auch vom Schlimmen,", "tokens": ["Ist", "nach", "ein\u00b7w\u00e4rts", "auch", "vom", "Schlim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kriegers Losung hei\u00dft Gehorsam,", "tokens": ["Krie\u00b7gers", "Lo\u00b7sung", "hei\u00dft", "Ge\u00b7hor\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heere d\u00fcrfen niemals stimmen.", "tokens": ["Hee\u00b7re", "d\u00fcr\u00b7fen", "nie\u00b7mals", "stim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}