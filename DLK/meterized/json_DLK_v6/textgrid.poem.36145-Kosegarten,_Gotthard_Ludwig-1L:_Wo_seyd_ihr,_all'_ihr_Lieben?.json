{"textgrid.poem.36145": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo seyd ihr, all' ihr Lieben?", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo seyd ihr, all' ihr Lieben?", "tokens": ["Wo", "seyd", "ihr", ",", "all'", "ihr", "Lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "PIAT", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Sturm und hei\u00dfem Drang,", "tokens": ["Mit", "Sturm", "und", "hei\u00b7\u00dfem", "Drang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Seufzen und bangem Sehnen,", "tokens": ["Mit", "Seuf\u00b7zen", "und", "ban\u00b7gem", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ruft euch mein Wehgesang.", "tokens": ["Ruft", "euch", "mein", "Weh\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo seyd ihr hin, ihr Edlen?", "tokens": ["Wo", "seyd", "ihr", "hin", ",", "ihr", "Ed\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was t\u00e4uscht ihr mich so sehr?", "tokens": ["Was", "t\u00e4uscht", "ihr", "mich", "so", "sehr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ihr habt mich sonst geliebet,", "tokens": ["Ihr", "habt", "mich", "sonst", "ge\u00b7lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und liebt mich jetzt nicht mehr?", "tokens": ["Und", "liebt", "mich", "jetzt", "nicht", "mehr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich steh' auf hohem Felsen,", "tokens": ["Ich", "steh'", "auf", "ho\u00b7hem", "Fel\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und rolle mein Aug' umher,", "tokens": ["Und", "rol\u00b7le", "mein", "Aug'", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und krauses Regengew\u00f6lke,", "tokens": ["Und", "krau\u00b7ses", "Re\u00b7gen\u00b7ge\u00b7w\u00f6l\u00b7ke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Umdunkelt Luft und Meer.", "tokens": ["Um\u00b7dun\u00b7kelt", "Luft", "und", "Meer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich st\u00fcrme durch Gebirg' und Thale,", "tokens": ["Ich", "st\u00fcr\u00b7me", "durch", "Ge\u00b7bir\u00b7g'", "und", "Tha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Durch Morast und Dorngestr\u00e4uch,", "tokens": ["Durch", "Mo\u00b7rast", "und", "Dorn\u00b7ge\u00b7str\u00e4uch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Durch Distel und Donnernessel,", "tokens": ["Durch", "Dis\u00b7tel", "und", "Don\u00b7ner\u00b7nes\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Und d\u00fcrst' und schrei' nach euch,", "tokens": ["Und", "d\u00fcr\u00b7st'", "und", "schrei'", "nach", "euch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "KON", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Eichwald steht entbl\u00e4ttert,", "tokens": ["Der", "Eich\u00b7wald", "steht", "ent\u00b7bl\u00e4t\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und todt und starr die Au',", "tokens": ["Und", "todt", "und", "starr", "die", "Au'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der feuchte Herbstwind sauset", "tokens": ["Der", "feuch\u00b7te", "Herbst\u00b7wind", "sau\u00b7set"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ueber Stoppeln kalt und rauh \u2013", "tokens": ["Ue\u00b7ber", "Stop\u00b7peln", "kalt", "und", "rauh", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was trau'rst du so, o Aue,", "tokens": ["Was", "trau'rst", "du", "so", ",", "o", "Au\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was heulst du so, o Hain?", "tokens": ["Was", "heulst", "du", "so", ",", "o", "Hain", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "FM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Seyd ihr auch von den Lieben verlassen?", "tokens": ["Seyd", "ihr", "auch", "von", "den", "Lie\u00b7ben", "ver\u00b7las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ADV", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-----+--+-", "measure": "iambic.di.relaxed"}, "line.8": {"text": "Seyd ihr auch, wie ich, allein?", "tokens": ["Seyd", "ihr", "auch", ",", "wie", "ich", ",", "al\u00b7lein", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAIMP", "PPER", "ADV", "$,", "PWAV", "PPER", "$,", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich bin allein, in fremdem", "tokens": ["Ich", "bin", "al\u00b7lein", ",", "in", "frem\u00b7dem"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$,", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein\u00f6den Land allein.", "tokens": ["Ein\u00b7\u00f6\u00b7den", "Land", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch sah ich hier noch Sonnen-,", "tokens": ["Noch", "sah", "ich", "hier", "noch", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "TRUNC", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch Mond-, noch Sternenschein.", "tokens": ["Noch", "Mon\u00b7d", ",", "noch", "Ster\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "TRUNC", "$,", "ADV", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die Luft ist irrer Nebel,", "tokens": ["Die", "Luft", "ist", "ir\u00b7rer", "Ne\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und nackter Fels die Au',", "tokens": ["Und", "nack\u00b7ter", "Fels", "die", "Au'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und klapperndes Gerippe", "tokens": ["Und", "klap\u00b7pern\u00b7des", "Ge\u00b7rip\u00b7pe"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Frost, und Reif der Thau.", "tokens": ["Der", "Frost", ",", "und", "Reif", "der", "Thau", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Mein Aug ist ausgeweinet,", "tokens": ["Mein", "Aug", "ist", "aus\u00b7ge\u00b7wei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Angesicht verbleicht.", "tokens": ["Mein", "An\u00b7ge\u00b7sicht", "ver\u00b7bleicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da kommt kein freundlicher Wandrer,", "tokens": ["Da", "kommt", "kein", "freund\u00b7li\u00b7cher", "Wand\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der mir die H\u00e4nde reicht.", "tokens": ["Der", "mir", "die", "H\u00e4n\u00b7de", "reicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Meine Klage \u00fcberhallt die Felder,", "tokens": ["Mei\u00b7ne", "Kla\u00b7ge", "\u00fc\u00b7berh\u00b7allt", "die", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Die Berge, den Forst, und laut", "tokens": ["Die", "Ber\u00b7ge", ",", "den", "Forst", ",", "und", "laut"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "KON", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Gibt mir das Echo Antwort,", "tokens": ["Gibt", "mir", "das", "E\u00b7cho", "Ant\u00b7wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Doch keines Menschen Laut.", "tokens": ["Doch", "kei\u00b7nes", "Men\u00b7schen", "Laut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich bin allein. Es schweben", "tokens": ["Ich", "bin", "al\u00b7lein", ".", "Es", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Phantome sichtbarlich,", "tokens": ["Phan\u00b7to\u00b7me", "sicht\u00b7bar\u00b7lich", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und tanzen im Wolkenwirbel,", "tokens": ["Und", "tan\u00b7zen", "im", "Wol\u00b7ken\u00b7wir\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und schrei'n im Sturm um mich.", "tokens": ["Und", "schrei'n", "im", "Sturm", "um", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Sturm br\u00fcllt lauter. Das Weltmeer", "tokens": ["Der", "Sturm", "br\u00fcllt", "lau\u00b7ter", ".", "Das", "Welt\u00b7meer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$.", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "W\u00e4lzt berghoch seine Fluth,", "tokens": ["W\u00e4lzt", "berg\u00b7hoch", "sei\u00b7ne", "Fluth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Und Felsenvesten erheben", "tokens": ["Und", "Fel\u00b7sen\u00b7ves\u00b7ten", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Vor des stolzen Orkans Wuth.", "tokens": ["Vor", "des", "stol\u00b7zen", "Or\u00b7kans", "Wuth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Br\u00fclle nur, o Sturm, und schreie", "tokens": ["Br\u00fcl\u00b7le", "nur", ",", "o", "Sturm", ",", "und", "schrei\u00b7e"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "$,", "FM", "NN", "$,", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie tausend Donner laut.", "tokens": ["Wie", "tau\u00b7send", "Don\u00b7ner", "laut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir hallt dein d\u00fcstres Schreien,", "tokens": ["Mir", "hallt", "dein", "d\u00fcst\u00b7res", "Schrei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie Morgengru\u00df der Braut.", "tokens": ["Wie", "Mor\u00b7gen\u00b7gru\u00df", "der", "Braut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "St\u00fcrme nur, und peitsch' die Berge,", "tokens": ["St\u00fcr\u00b7me", "nur", ",", "und", "peit\u00b7sch'", "die", "Ber\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Hochstolzer Ocean,", "tokens": ["Hoch\u00b7stol\u00b7zer", "O\u00b7cean", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Ich h\u00f6r' dein Wogenget\u00fcmmel", "tokens": ["Ich", "h\u00f6r'", "dein", "Wo\u00b7gen\u00b7ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Wie Fl\u00f6tenwirbel an.", "tokens": ["Wie", "Fl\u00f6\u00b7ten\u00b7wir\u00b7bel", "an", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Was soll mir Fr\u00fchlingsmilde", "tokens": ["Was", "soll", "mir", "Fr\u00fch\u00b7lings\u00b7mil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und laues Westgeweh?", "tokens": ["Und", "lau\u00b7es", "West\u00b7ge\u00b7weh", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es wiegt des Verla\u00dfnen Seele", "tokens": ["Es", "wiegt", "des", "Ver\u00b7la\u00df\u00b7nen", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nur in noch hei\u00df'res Weh!", "tokens": ["Nur", "in", "noch", "hei\u00df'\u00b7res", "Weh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was soll mir Hainges\u00e4usel,", "tokens": ["Was", "soll", "mir", "Hain\u00b7ge\u00b7s\u00e4u\u00b7sel", ","], "token_info": ["word", "wo