{"textgrid.poem.45063": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Historisches Drama", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er stellt sich gar so heimisch dar,", "tokens": ["Er", "stellt", "sich", "gar", "so", "hei\u00b7misch", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ein wackrer, alter Bekannter.", "tokens": ["Wie", "ein", "wack\u00b7rer", ",", "al\u00b7ter", "Be\u00b7kann\u00b7ter", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das St\u00fcck ist Geschichte ganz und gar,", "tokens": ["Das", "St\u00fcck", "ist", "Ge\u00b7schich\u00b7te", "ganz", "und", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "ADV", "KON", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nur etwas ennuyanter.", "tokens": ["Nur", "et\u00b7was", "en\u00b7nu\u00b7yan\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}