{"textgrid.poem.56507": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Finsternis raschelt wie ein Gewand,", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Finsternis raschelt wie ein Gewand,", "tokens": ["Die", "Fins\u00b7ter\u00b7nis", "ra\u00b7schelt", "wie", "ein", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die B\u00e4ume torkeln am Himmelsrand.", "tokens": ["Die", "B\u00e4u\u00b7me", "tor\u00b7keln", "am", "Him\u00b7mels\u00b7rand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Rette dich in das Herz der Nacht,", "tokens": ["Ret\u00b7te", "dich", "in", "das", "Herz", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Grabe dich schnell in das Dunkele ein,", "tokens": ["Gra\u00b7be", "dich", "schnell", "in", "das", "Dun\u00b7ke\u00b7le", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Wie in Waben. Mache dich klein,", "tokens": ["Wie", "in", "Wa\u00b7ben", ".", "Ma\u00b7che", "dich", "klein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "$.", "VVFIN", "PPER", "ADJD", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Steige aus deinem Bette.", "tokens": ["Stei\u00b7ge", "aus", "dei\u00b7nem", "Bet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Etwas will \u00fcber die Br\u00fccken,", "tokens": ["Et\u00b7was", "will", "\u00fc\u00b7ber", "die", "Br\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Es scharret mit Hufen krumm,", "tokens": ["Es", "schar\u00b7ret", "mit", "Hu\u00b7fen", "krumm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Sterne erschraken so wei\u00df.", "tokens": ["Die", "Ster\u00b7ne", "er\u00b7schra\u00b7ken", "so", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Und der Mond wie ein Greis", "tokens": ["Und", "der", "Mond", "wie", "ein", "Greis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KOKOM", "ART", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Watschelt oben herum", "tokens": ["Wat\u00b7schelt", "o\u00b7ben", "he\u00b7rum"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "APZR"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Mit dem h\u00f6ckrigen R\u00fccken.", "tokens": ["Mit", "dem", "h\u00f6ck\u00b7ri\u00b7gen", "R\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}}}}