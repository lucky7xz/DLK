{"textgrid.poem.49524": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Was von ohngef\u00e4hr geschehen k\u00f6nnte", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von ohngef\u00e4hr gefiel mir Amarille;", "tokens": ["Von", "ohn\u00b7ge\u00b7f\u00e4hr", "ge\u00b7fiel", "mir", "A\u00b7ma\u00b7ril\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVFIN", "PPER", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von ohngef\u00e4hr gefiel ich ihr.", "tokens": ["Von", "ohn\u00b7ge\u00b7f\u00e4hr", "ge\u00b7fiel", "ich", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Getreu zu seyn ist itzt mein ernster Wille \u2013 \u2013", "tokens": ["Ge\u00b7treu", "zu", "seyn", "ist", "itzt", "mein", "erns\u00b7ter", "Wil\u00b7le", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "PTKZU", "VAINF", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch, Amor, h\u00f6r'! und sage mir:", "tokens": ["Doch", ",", "A\u00b7mor", ",", "h\u00f6r'", "!", "und", "sa\u00b7ge", "mir", ":"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NE", "$,", "VVFIN", "$.", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verziehestu's, wenn ich von ohngef\u00e4hr", "tokens": ["Ver\u00b7zie\u00b7he\u00b7stu's", ",", "wenn", "ich", "von", "ohn\u00b7ge\u00b7f\u00e4hr"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "APPR", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ihr untreu w\u00e4r'?", "tokens": ["Ihr", "un\u00b7treu", "w\u00e4r'", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}