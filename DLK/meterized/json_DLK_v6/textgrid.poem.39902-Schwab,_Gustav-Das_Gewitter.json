{"textgrid.poem.39902": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Das Gewitter", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Urahne, Gro\u00dfmutter, Mutter und Kind", "tokens": ["Ur\u00b7ah\u00b7ne", ",", "Gro\u00df\u00b7mut\u00b7ter", ",", "Mut\u00b7ter", "und", "Kind"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "In dumpfer Stube beisammen sind;", "tokens": ["In", "dum\u00b7pfer", "Stu\u00b7be", "bei\u00b7sam\u00b7men", "sind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es spielet das Kind, die Mutter sich schm\u00fcckt,", "tokens": ["Es", "spie\u00b7let", "das", "Kind", ",", "die", "Mut\u00b7ter", "sich", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gro\u00dfmutter spinnet, Urahne geb\u00fcckt", "tokens": ["Gro\u00df\u00b7mut\u00b7ter", "spin\u00b7net", ",", "Ur\u00b7ah\u00b7ne", "ge\u00b7b\u00fcckt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "NN", "VVPP"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Sitzt hinter dem Ofen im Pf\u00fchl \u2013", "tokens": ["Sitzt", "hin\u00b7ter", "dem", "O\u00b7fen", "im", "Pf\u00fchl", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Wie wehen die L\u00fcfte so schw\u00fcl!", "tokens": ["Wie", "we\u00b7hen", "die", "L\u00fcf\u00b7te", "so", "schw\u00fcl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Das Kind spricht: \u00bbMorgen ist's Feiertag,", "tokens": ["Das", "Kind", "spricht", ":", "\u00bb", "Mor\u00b7gen", "ist's", "Fei\u00b7er\u00b7tag", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie will ich spielen im gr\u00fcnen Hag,", "tokens": ["Wie", "will", "ich", "spie\u00b7len", "im", "gr\u00fc\u00b7nen", "Hag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Wie will ich springen durch Thal und H\u00f6h'n,", "tokens": ["Wie", "will", "ich", "sprin\u00b7gen", "durch", "Thal", "und", "H\u00f6h'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Wie will ich pfl\u00fccken viel Blumen sch\u00f6n;", "tokens": ["Wie", "will", "ich", "pfl\u00fc\u00b7cken", "viel", "Blu\u00b7men", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "PIAT", "NN", "ADJD", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Dem Anger, dem bin ich hold!\u00ab \u2013", "tokens": ["Dem", "An\u00b7ger", ",", "dem", "bin", "ich", "hold", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VAFIN", "PPER", "ADJD", "$.", "$(", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "H\u00f6rt ihr's, wie der Donner grollt?", "tokens": ["H\u00f6rt", "ih\u00b7r's", ",", "wie", "der", "Don\u00b7ner", "grollt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Die Mutter spricht: \u00bbMorgen ist's Feiertag,", "tokens": ["Die", "Mut\u00b7ter", "spricht", ":", "\u00bb", "Mor\u00b7gen", "ist's", "Fei\u00b7er\u00b7tag", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "NN", "VAFIN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da halten wir alle fr\u00f6hlich Gelag,", "tokens": ["Da", "hal\u00b7ten", "wir", "al\u00b7le", "fr\u00f6h\u00b7lich", "Ge\u00b7lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "ADJD", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich selber ich r\u00fcste mein Feierkleid;", "tokens": ["Ich", "sel\u00b7ber", "ich", "r\u00fcs\u00b7te", "mein", "Fei\u00b7er\u00b7kleid", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Das Leben es hat auch Lust nach Leid,", "tokens": ["Das", "Le\u00b7ben", "es", "hat", "auch", "Lust", "nach", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VAFIN", "ADV", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Dann scheint die Sonne wie Gold!\u00ab \u2013", "tokens": ["Dann", "scheint", "die", "Son\u00b7ne", "wie", "Gold", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KOKOM", "NN", "$.", "$(", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "H\u00f6rt ihr's, wie der Donner grollt?", "tokens": ["H\u00f6rt", "ih\u00b7r's", ",", "wie", "der", "Don\u00b7ner", "grollt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}}, "stanza.4": {"line.1": {"text": "Gro\u00dfmutter spricht: \u00bbMorgen ist's Feiertag,", "tokens": ["Gro\u00df\u00b7mut\u00b7ter", "spricht", ":", "\u00bb", "Mor\u00b7gen", "ist's", "Fei\u00b7er\u00b7tag", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "$(", "NN", "VAFIN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Gro\u00dfmutter hat keinen Feiertag,", "tokens": ["Gro\u00df\u00b7mut\u00b7ter", "hat", "kei\u00b7nen", "Fei\u00b7er\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie kochet das Mahl, sie spinnet das Kleid,", "tokens": ["Sie", "ko\u00b7chet", "das", "Mahl", ",", "sie", "spin\u00b7net", "das", "Kleid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Leben ist Sorg' und viel Arbeit;", "tokens": ["Das", "Le\u00b7ben", "ist", "Sor\u00b7g'", "und", "viel", "Ar\u00b7beit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wohl dem, der that, was er sollt'!\u00ab \u2013", "tokens": ["Wohl", "dem", ",", "der", "that", ",", "was", "er", "sollt'", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$.", "$(", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "H\u00f6rt ihr's, wie der Donner grollt?", "tokens": ["H\u00f6rt", "ih\u00b7r's", ",", "wie", "der", "Don\u00b7ner", "grollt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}}, "stanza.5": {"line.1": {"text": "Urahne spricht: \u00bbMorgen ist's Feiertag,", "tokens": ["Ur\u00b7ah\u00b7ne", "spricht", ":", "\u00bb", "Mor\u00b7gen", "ist's", "Fei\u00b7er\u00b7tag", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "$(", "NN", "VAFIN", "NN", "$,"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Am liebsten morgen ich sterben mag:", "tokens": ["Am", "liebs\u00b7ten", "mor\u00b7gen", "ich", "ster\u00b7ben", "mag", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ADV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich kann nicht singen und scherzen mehr,", "tokens": ["Ich", "kann", "nicht", "sin\u00b7gen", "und", "scher\u00b7zen", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich kann nicht sorgen und schaffen schwer,", "tokens": ["Ich", "kann", "nicht", "sor\u00b7gen", "und", "schaf\u00b7fen", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "NN", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Was thu' ich noch auf der Welt?\u00ab \u2013", "tokens": ["Was", "thu'", "ich", "noch", "auf", "der", "Welt", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$.", "$(", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Seht ihr, wie der Blitz dort f\u00e4llt?", "tokens": ["Seht", "ihr", ",", "wie", "der", "Blitz", "dort", "f\u00e4llt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sie h\u00f6ren's nicht, sie sehen's nicht,", "tokens": ["Sie", "h\u00f6\u00b7ren's", "nicht", ",", "sie", "se\u00b7hen's", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es flammet die Stube wie lauter Licht:", "tokens": ["Es", "flam\u00b7met", "die", "Stu\u00b7be", "wie", "lau\u00b7ter", "Licht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOKOM", "PIAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Urahne, Gro\u00dfmutter, Mutter und Kind", "tokens": ["Ur\u00b7ah\u00b7ne", ",", "Gro\u00df\u00b7mut\u00b7ter", ",", "Mut\u00b7ter", "und", "Kind"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Vom Stral miteinander getroffen sind,", "tokens": ["Vom", "Stral", "mi\u00b7tein\u00b7an\u00b7der", "ge\u00b7trof\u00b7fen", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Vier Leben endet ein Schlag \u2013", "tokens": ["Vier", "Le\u00b7ben", "en\u00b7det", "ein", "Schlag", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Und morgen ist's Feiertag.", "tokens": ["Und", "mor\u00b7gen", "ist's", "Fei\u00b7er\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}