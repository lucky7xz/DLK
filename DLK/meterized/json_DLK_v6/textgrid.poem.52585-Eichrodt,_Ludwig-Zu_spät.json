{"textgrid.poem.52585": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Zu sp\u00e4t", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir sind gar oft mit Schweigen", "tokens": ["Wir", "sind", "gar", "oft", "mit", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aneinander vor\u00fcbergegangen,", "tokens": ["An\u00b7ein\u00b7an\u00b7der", "vor\u00b7\u00fc\u00b7ber\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Wir wollten einander nicht zeigen", "tokens": ["Wir", "woll\u00b7ten", "ein\u00b7an\u00b7der", "nicht", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "PTKNEG", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Des Herzens Sehnen und Bangen \u2013", "tokens": ["Des", "Her\u00b7zens", "Seh\u00b7nen", "und", "Ban\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und haben uns doch geliebt!", "tokens": ["Und", "ha\u00b7ben", "uns", "doch", "ge\u00b7liebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Wir haben zusammen gesprochen,", "tokens": ["Wir", "ha\u00b7ben", "zu\u00b7sam\u00b7men", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Uns tief in die Augen geschaut,", "tokens": ["Uns", "tief", "in", "die", "Au\u00b7gen", "ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Das Herz ist schier gebrochen,", "tokens": ["Das", "Herz", "ist", "schier", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wir haben uns ", "tokens": ["Wir", "ha\u00b7ben", "uns"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und haben uns doch geliebt!", "tokens": ["Und", "ha\u00b7ben", "uns", "doch", "ge\u00b7liebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Es schwebt auf unsern Zungen", "tokens": ["Es", "schwebt", "auf", "un\u00b7sern", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das holde das s\u00fc\u00dfe Gestehen,", "tokens": ["Das", "hol\u00b7de", "das", "s\u00fc\u00b7\u00dfe", "Ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wir waren so innig durchdrungen,", "tokens": ["Wir", "wa\u00b7ren", "so", "in\u00b7nig", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wir lie\u00dfen die Stunde verwehen \u2013", "tokens": ["Wir", "lie\u00b7\u00dfen", "die", "Stun\u00b7de", "ver\u00b7we\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und haben uns doch geliebt!", "tokens": ["Und", "ha\u00b7ben", "uns", "doch", "ge\u00b7liebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Wir haben das ", "tokens": ["Wir", "ha\u00b7ben", "das"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ART"], "meter": "-+--", "measure": "dactylic.init"}, "line.2": {"text": "Das Heil uns bescheeret und Frieden,", "tokens": ["Das", "Heil", "uns", "be\u00b7schee\u00b7ret", "und", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Nun bluten zeitlebens die Wunden,", "tokens": ["Nun", "blu\u00b7ten", "zeit\u00b7le\u00b7bens", "die", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nun sind wir auf immer geschieden \u2013", "tokens": ["Nun", "sind", "wir", "auf", "im\u00b7mer", "ge\u00b7schie\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ADV", "VVPP", "$("], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und haben uns doch geliebt!", "tokens": ["Und", "ha\u00b7ben", "uns", "doch", "ge\u00b7liebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}