{"dta.poem.982": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "M. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Wie unsre glorreichen himmel bruder im stolz                 ", "tokens": ["Wie", "uns\u00b7re", "glor\u00b7rei\u00b7chen", "him\u00b7mel", "bru\u00b7der", "im", "stolz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "NN", "APPRART", "NN"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So breitet dein gl&#228;nzendes gelb und wie reifender lohn                 ", "tokens": ["So", "brei\u00b7tet", "dein", "gl", "&#228;", "nzen\u00b7des", "gelb", "und", "wie", "rei\u00b7fen\u00b7der", "lohn"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "FM", "FM", "FM", "ADJD", "KON", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Es zittern in deinem lila und wehen gr&#252;n                 ", "tokens": ["Es", "zit\u00b7tern", "in", "dei\u00b7nem", "li\u00b7la", "und", "we\u00b7hen", "gr", "&#252;", "n"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJD", "KON", "VVFIN", "XY", "XY", "XY"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Gestaltlose stunden mit ihrem m&#252;hsamen rinnen                 ", "tokens": ["Ge\u00b7stalt\u00b7lo\u00b7se", "stun\u00b7den", "mit", "ih\u00b7rem", "m", "&#252;", "hsa\u00b7men", "rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$(", "VVINF", "VVINF"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.5": {"text": "Und lange seufzer aus kerkern ohne erhebung                 ", "tokens": ["Und", "lan\u00b7ge", "seuf\u00b7zer", "aus", "ker\u00b7kern", "oh\u00b7ne", "er\u00b7he\u00b7bung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Dein strahlendes blau umkleidet die wunschlosen g\u00f6tter                 ", "tokens": ["Dein", "strah\u00b7len\u00b7des", "blau", "um\u00b7klei\u00b7det", "die", "wun\u00b7schlo\u00b7sen", "g\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "In deinem veilchendunkel voll purpurner scheine                 ", "tokens": ["In", "dei\u00b7nem", "veil\u00b7chen\u00b7dun\u00b7kel", "voll", "pur\u00b7pur\u00b7ner", "schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "ADJA", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ist unser t\u00f6tliches sehnen bruder im leid.               ", "tokens": ["Ist", "un\u00b7ser", "t\u00f6t\u00b7li\u00b7ches", "seh\u00b7nen", "bru\u00b7der", "im", "leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "ADJA", "NN", "APPRART", "ADJD", "$."], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}}}}}