{"dta.poem.20876": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Der Socrates fing an zu lachen/", "tokens": ["Der", "So\u00b7cra\u00b7tes", "fing", "an", "zu", "la\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$("], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und lie\u00df der Augen hellen Schein/", "tokens": ["Und", "lie\u00df", "der", "Au\u00b7gen", "hel\u00b7len", "Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach seiner Art recht freudig seyn/", "tokens": ["Nach", "sei\u00b7ner", "Art", "recht", "freu\u00b7dig", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Antrieb dieser hohen Sachen.", "tokens": ["Aus", "An\u00b7trieb", "die\u00b7ser", "ho\u00b7hen", "Sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er sagte/ was du f\u00fcrgebracht/", "tokens": ["Er", "sag\u00b7te", "/", "was", "du", "f\u00fcr\u00b7ge\u00b7bracht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Steckt freylich noch in tieffer Nacht/", "tokens": ["Steckt", "frey\u00b7lich", "noch", "in", "tief\u00b7fer", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "da\u00df ich itzt werd erkl\u00e4ren m\u00fcssen.", "tokens": ["da\u00df", "ich", "itzt", "werd", "er\u00b7kl\u00e4\u00b7ren", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie habt ihr denn/ als ich gelehrt/", "tokens": ["Wie", "habt", "ihr", "denn", "/", "als", "ich", "ge\u00b7lehrt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$(", "KOUS", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und ihr mit Eyfer zugeh\u00f6rt/", "tokens": ["Und", "ihr", "mit", "Ey\u00b7fer", "zu\u00b7ge\u00b7h\u00f6rt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mich nicht den Zweiffel lassen wissen?", "tokens": ["Mich", "nicht", "den", "Zweif\u00b7fel", "las\u00b7sen", "wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer besser Art zu reden hat/", "tokens": ["Wer", "bes\u00b7ser", "Art", "zu", "re\u00b7den", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "NN", "PTKZU", "VVINF", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der solte meine Worte stehlen/", "tokens": ["Der", "sol\u00b7te", "mei\u00b7ne", "Wor\u00b7te", "steh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und bald erweisen in der That/", "tokens": ["Und", "bald", "er\u00b7wei\u00b7sen", "in", "der", "That", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie offters unsre Schulen fehlen/", "tokens": ["Wie", "off\u00b7ters", "uns\u00b7re", "Schu\u00b7len", "feh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dann der Gedancken ist wol werth/", "tokens": ["Dann", "der", "Ge\u00b7dan\u00b7cken", "ist", "wol", "werth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df man ihn mit Verstand erkl\u00e4rt/", "tokens": ["Da\u00df", "man", "ihn", "mit", "Ver\u00b7stand", "er\u00b7kl\u00e4rt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und diese Wolcken lauter mache/", "tokens": ["Und", "die\u00b7se", "Wol\u00b7cken", "lau\u00b7ter", "ma\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dieweil der Schein in dieser Sache", "tokens": ["Die\u00b7weil", "der", "Schein", "in", "die\u00b7ser", "Sa\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Offt f\u00e4lschlich zu der Warheit kehrt.", "tokens": ["Offt", "f\u00e4lschlich", "zu", "der", "War\u00b7heit", "kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "So wisse dann/ da\u00df auf den Stegen/", "tokens": ["So", "wis\u00b7se", "dann", "/", "da\u00df", "auf", "den", "Ste\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$(", "KOUS", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch ehe man die Sachen r\u00fchrt/", "tokens": ["Doch", "e\u00b7he", "man", "die", "Sa\u00b7chen", "r\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und diesen Fall wil wiederlegen/", "tokens": ["Und", "die\u00b7sen", "Fall", "wil", "wie\u00b7der\u00b7le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So mu\u00df ich doch zuvor erwegen/", "tokens": ["So", "mu\u00df", "ich", "doch", "zu\u00b7vor", "er\u00b7we\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was Cebes vor Gedancken f\u00fchrt/", "tokens": ["Was", "Ce\u00b7bes", "vor", "Ge\u00b7dan\u00b7cken", "f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf da\u00df ich so in allen Dingen/", "tokens": ["Auf", "da\u00df", "ich", "so", "in", "al\u00b7len", "Din\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADV", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So es verg\u00f6nt der kurtze Tag/", "tokens": ["So", "es", "ver\u00b7g\u00f6nt", "der", "kurt\u00b7ze", "Tag", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und mir mein Wunsch noch soll gelingen/", "tokens": ["Und", "mir", "mein", "Wunsch", "noch", "soll", "ge\u00b7lin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Euch alle vergen\u00fcgen mag/", "tokens": ["Euch", "al\u00b7le", "ver\u00b7ge\u00b7n\u00fc\u00b7gen", "mag", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "In allem was ich f\u00fcr wil bringen.", "tokens": ["In", "al\u00b7lem", "was", "ich", "f\u00fcr", "wil", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PWS", "PPER", "APPR", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}