{"textgrid.poem.65357": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Kinderfr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wollt euch nicht so schnell belauben,", "tokens": ["Wollt", "euch", "nicht", "so", "schnell", "be\u00b7lau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4lder, und mir wieder rauben", "tokens": ["W\u00e4l\u00b7der", ",", "und", "mir", "wie\u00b7der", "rau\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Diesen lieben Sonnenschein,", "tokens": ["Die\u00b7sen", "lie\u00b7ben", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Den so lang' ich mu\u00dfte missen,", "tokens": ["Den", "so", "lang'", "ich", "mu\u00df\u00b7te", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis die Schleier er zerrissen,", "tokens": ["Bis", "die", "Schlei\u00b7er", "er", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die den Himmel h\u00fcllten ein.", "tokens": ["Die", "den", "Him\u00b7mel", "h\u00fcll\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwischen knospenvollen Zweigen", "tokens": ["Zwi\u00b7schen", "knos\u00b7pen\u00b7vol\u00b7len", "Zwei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seh' ich auf und nieder steigen", "tokens": ["Seh'", "ich", "auf", "und", "nie\u00b7der", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kleiner V\u00f6glein buntes Heer,", "tokens": ["Klei\u00b7ner", "V\u00f6\u00b7glein", "bun\u00b7tes", "Heer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seh' sie schn\u00e4beln, seh' sie picken,", "tokens": ["Seh'", "sie", "schn\u00e4\u00b7beln", ",", "seh'", "sie", "pi\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "$,", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die schwanken Reiser nicken,", "tokens": ["Und", "die", "schwan\u00b7ken", "Rei\u00b7ser", "ni\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denen ihre Last zu schwer.", "tokens": ["De\u00b7nen", "ih\u00b7re", "Last", "zu", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und der klare blaue Himmel", "tokens": ["Und", "der", "kla\u00b7re", "blau\u00b7e", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Breitet hinter dem Gewimmel", "tokens": ["Brei\u00b7tet", "hin\u00b7ter", "dem", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich in stillem Frieden aus.", "tokens": ["Sich", "in", "stil\u00b7lem", "Frie\u00b7den", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie durch kleine Fenstergitter", "tokens": ["Wie", "durch", "klei\u00b7ne", "Fens\u00b7ter\u00b7git\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Spielt die Sonne mit Gezitter", "tokens": ["Spielt", "die", "Son\u00b7ne", "mit", "Ge\u00b7zit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch der Zweige Flechtenhaus.", "tokens": ["Durch", "der", "Zwei\u00b7ge", "Flech\u00b7ten\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Halbbegr\u00fcnet stehn die Hecken,", "tokens": ["Halb\u00b7be\u00b7gr\u00fc\u00b7net", "stehn", "die", "He\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Nachbarskinder necken", "tokens": ["Und", "die", "Nach\u00b7bars\u00b7kin\u00b7der", "ne\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch die d\u00fcrren L\u00fccken sich,", "tokens": ["Durch", "die", "d\u00fcr\u00b7ren", "L\u00fc\u00b7cken", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis das M\u00e4dchen r\u00f6ther gl\u00fchet", "tokens": ["Bis", "das", "M\u00e4d\u00b7chen", "r\u00f6\u00b7ther", "gl\u00fc\u00b7het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und zu dichtern Stellen fliehet", "tokens": ["Und", "zu", "dich\u00b7tern", "Stel\u00b7len", "flie\u00b7het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vor dem Knaben j\u00fcngferlich.", "tokens": ["Vor", "dem", "Kna\u00b7ben", "j\u00fcng\u00b7fer\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Fr\u00fchling, heute noch ein Knabe,", "tokens": ["Fr\u00fch\u00b7ling", ",", "heu\u00b7te", "noch", "ein", "Kna\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Treibet auf des Winters Grabe", "tokens": ["Trei\u00b7bet", "auf", "des", "Win\u00b7ters", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit den Kindern seinen Scherz,", "tokens": ["Mit", "den", "Kin\u00b7dern", "sei\u00b7nen", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis der Gott der s\u00fc\u00dfen Triebe", "tokens": ["Bis", "der", "Gott", "der", "s\u00fc\u00b7\u00dfen", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit dem Flammenpfeil der Liebe", "tokens": ["Mit", "dem", "Flam\u00b7men\u00b7pfeil", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihm durchbohrt das kleine Herz.", "tokens": ["Ihm", "durch\u00b7bohrt", "das", "klei\u00b7ne", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}