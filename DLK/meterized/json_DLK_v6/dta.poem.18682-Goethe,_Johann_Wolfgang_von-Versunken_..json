{"dta.poem.18682": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Versunken .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Voll Locken kraus ein Haupt so rund! \u2014", "tokens": ["Voll", "Lo\u00b7cken", "kraus", "ein", "Haupt", "so", "rund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "NN", "ADV", "ART", "NN", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und darf ich dann in solchen ", "tokens": ["Und", "darf", "ich", "dann", "in", "sol\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "APPR", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit vollen H\u00e4nden hin und wieder fahren", "tokens": ["Mit", "vol\u00b7len", "H\u00e4n\u00b7den", "hin", "und", "wie\u00b7der", "fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "KON", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da f\u00fchl\u2019 ich mich von Herzensgrund gesund.", "tokens": ["Da", "f\u00fchl'", "ich", "mich", "von", "Her\u00b7zens\u00b7grund", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und k\u00fcss\u2019 ich Stirne, Bogen, Auge, Mund,", "tokens": ["Und", "k\u00fcss'", "ich", "Stir\u00b7ne", ",", "Bo\u00b7gen", ",", "Au\u00b7ge", ",", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dann bin ich frisch und immer wieder wund.", "tokens": ["Dann", "bin", "ich", "frisch", "und", "im\u00b7mer", "wie\u00b7der", "wund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der f\u00fcnfgezackte Kamm wo sollt\u2019 er stocken?", "tokens": ["Der", "f\u00fcnf\u00b7ge\u00b7zack\u00b7te", "Kamm", "wo", "sollt'", "er", "sto\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PWAV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Er kehrt schon wieder zu den Locken.", "tokens": ["Er", "kehrt", "schon", "wie\u00b7der", "zu", "den", "Lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Das Ohr versagt sich nicht dem Spiel,", "tokens": ["Das", "Ohr", "ver\u00b7sagt", "sich", "nicht", "dem", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Hier ist nicht Fleisch, hier ist nicht Haut,", "tokens": ["Hier", "ist", "nicht", "Fleisch", ",", "hier", "ist", "nicht", "Haut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "NN", "$,", "ADV", "VAFIN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "So zart zum Scherz so liebeviel!", "tokens": ["So", "zart", "zum", "Scherz", "so", "lie\u00b7be\u00b7viel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Doch wie man auf dem K\u00f6pfchen kraut,", "tokens": ["Doch", "wie", "man", "auf", "dem", "K\u00f6pf\u00b7chen", "kraut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Man wird in solchen reichen Haaren", "tokens": ["Man", "wird", "in", "sol\u00b7chen", "rei\u00b7chen", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "F\u00fcr ewig auf und nieder fahren.", "tokens": ["F\u00fcr", "e\u00b7wig", "auf", "und", "nie\u00b7der", "fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "PTKVZ", "KON", "PTKVZ", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "So hast du Hafis auch gethan,", "tokens": ["So", "hast", "du", "Ha\u00b7fis", "auch", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Wir fangen es von vornen an.", "tokens": ["Wir", "fan\u00b7gen", "es", "von", "vor\u00b7nen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}