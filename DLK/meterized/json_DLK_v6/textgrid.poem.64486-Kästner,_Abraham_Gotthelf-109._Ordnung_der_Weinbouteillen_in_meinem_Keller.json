{"textgrid.poem.64486": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "109. Ordnung der Weinbouteillen in meinem Keller", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier liegen sie, die Flaschen, in Dekaden,", "tokens": ["Hier", "lie\u00b7gen", "sie", ",", "die", "Fla\u00b7schen", ",", "in", "De\u00b7ka\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sammeln sich, nur in Hekatontaden;", "tokens": ["Und", "sam\u00b7meln", "sich", ",", "nur", "in", "He\u00b7ka\u00b7ton\u00b7ta\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$,", "ADV", "APPR", "NE", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Aetherisch, hoch einher, w\u00fcrd' erst mein Vers erklingen,", "tokens": ["A\u00b7e\u00b7the\u00b7risch", ",", "hoch", "ein\u00b7her", ",", "w\u00fcrd'", "erst", "mein", "Vers", "er\u00b7klin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "PTKVZ", "$,", "VAFIN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "K\u00f6nnt' er von Myriaden singen!", "tokens": ["K\u00f6nnt'", "er", "von", "My\u00b7ria\u00b7den", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}