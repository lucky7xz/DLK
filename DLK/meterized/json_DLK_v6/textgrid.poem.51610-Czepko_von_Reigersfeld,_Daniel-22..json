{"textgrid.poem.51610": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit ihm dem C\u00f6rper hat die Seele nichts gemein,", "tokens": ["Mit", "ihm", "dem", "C\u00f6r\u00b7per", "hat", "die", "See\u00b7le", "nichts", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VAFIN", "ART", "NN", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mu\u00df, o Wunder! doch in ihm verschlossen seyn.", "tokens": ["Und", "mu\u00df", ",", "o", "Wun\u00b7der", "!", "doch", "in", "ihm", "ver\u00b7schlos\u00b7sen", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$,", "FM", "NN", "$.", "ADV", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}