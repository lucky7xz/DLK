{"textgrid.poem.41516": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am Feste der hl. drei K\u00f6nige", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch die Nacht drei Wandrer ziehn,", "tokens": ["Durch", "die", "Nacht", "drei", "Wand\u00b7rer", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "CARD", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Um die Stirnen Purpurbinden,", "tokens": ["Um", "die", "Stir\u00b7nen", "Pur\u00b7pur\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tiefgebr\u00e4unt von hei\u00dfen Winden", "tokens": ["Tief\u00b7ge\u00b7br\u00e4unt", "von", "hei\u00b7\u00dfen", "Win\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der langen Reise M\u00fchn;", "tokens": ["Und", "der", "lan\u00b7gen", "Rei\u00b7se", "M\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch der Palmen s\u00e4uselnd Gr\u00fcn", "tokens": ["Durch", "der", "Pal\u00b7men", "s\u00e4u\u00b7selnd", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Folgt der Diener Schar von weiten;", "tokens": ["Folgt", "der", "Die\u00b7ner", "Schar", "von", "wei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "APPR", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Von der Dromedare Seiten", "tokens": ["Von", "der", "Dro\u00b7me\u00b7da\u00b7re", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Goldene Kleinode gl\u00fchn.", "tokens": ["Gol\u00b7de\u00b7ne", "Klein\u00b7o\u00b7de", "gl\u00fchn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.9": {"text": "Wie sie klirrend vorw\u00e4rts schreiten,", "tokens": ["Wie", "sie", "klir\u00b7rend", "vor\u00b7w\u00e4rts", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "S\u00fc\u00dfe Wohlger\u00fcche fliehn.", "tokens": ["S\u00fc\u00b7\u00dfe", "Wohl\u00b7ge\u00b7r\u00fc\u00b7che", "fliehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Finsternis h\u00fcllt schwarz und dicht", "tokens": ["Fins\u00b7ter\u00b7nis", "h\u00fcllt", "schwarz", "und", "dicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was die Gegend mag enthalten;", "tokens": ["Was", "die", "Ge\u00b7gend", "mag", "ent\u00b7hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Riesig drohen die Gestalten:", "tokens": ["Rie\u00b7sig", "dro\u00b7hen", "die", "Ge\u00b7stal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wandrer f\u00fcrchtet ihr euch nicht?", "tokens": ["Wand\u00b7rer", "f\u00fcrch\u00b7tet", "ihr", "euch", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch ob tausend Schleier flicht", "tokens": ["Doch", "ob", "tau\u00b7send", "Schlei\u00b7er", "flicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "CARD", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Los' und leicht die Wolkenaue:", "tokens": ["Los'", "und", "leicht", "die", "Wol\u00b7ke\u00b7nau\u00b7e", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Siegreich durch das zarte Graue", "tokens": ["Sieg\u00b7reich", "durch", "das", "zar\u00b7te", "Grau\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sich ein funkelnd Sternlein bricht,", "tokens": ["Sich", "ein", "fun\u00b7kelnd", "Stern\u00b7lein", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Langsam wallt es durch das Blaue,", "tokens": ["Lang\u00b7sam", "wallt", "es", "durch", "das", "Blau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und der Zug folgt seinem Licht.", "tokens": ["Und", "der", "Zug", "folgt", "sei\u00b7nem", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Horch, die Diener fl\u00fcstern leis:", "tokens": ["Horch", ",", "die", "Die\u00b7ner", "fl\u00fcs\u00b7tern", "leis", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Will noch nicht die Stadt erscheinen,", "tokens": ["Will", "noch", "nicht", "die", "Stadt", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit den Tempeln und den Hainen,", "tokens": ["Mit", "den", "Tem\u00b7peln", "und", "den", "Hai\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie der schweren M\u00fche Preis?", "tokens": ["Sie", "der", "schwe\u00b7ren", "M\u00fc\u00b7he", "Preis", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob die W\u00fcste brannte hei\u00df,", "tokens": ["Ob", "die", "W\u00fcs\u00b7te", "brann\u00b7te", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ob die Nattern uns umschlangen,", "tokens": ["Ob", "die", "Nat\u00b7tern", "uns", "um\u00b7schlan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Uns die Tiger nachgegangen,", "tokens": ["Uns", "die", "Ti\u00b7ger", "nach\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ob der Glutwind d\u00f6rrt' den Schwei\u00df:", "tokens": ["Ob", "der", "Glut\u00b7wind", "d\u00f6rrt'", "den", "Schwei\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Augen an den Gaben hangen", "tokens": ["Au\u00b7gen", "an", "den", "Ga\u00b7ben", "han\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "F\u00fcr den K\u00f6nig stark und weis'.", "tokens": ["F\u00fcr", "den", "K\u00f6\u00b7nig", "stark", "und", "weis'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sonder Sorge, sonder Acht,", "tokens": ["Son\u00b7der", "Sor\u00b7ge", ",", "son\u00b7der", "Acht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "KON", "CARD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie drei stille Monde ziehen", "tokens": ["Wie", "drei", "stil\u00b7le", "Mon\u00b7de", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "CARD", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Um des Sonnensternes Gl\u00fchen,", "tokens": ["Um", "des", "Son\u00b7nens\u00b7ter\u00b7nes", "Gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ziehn die Dreie durch die Nacht.", "tokens": ["Ziehn", "die", "Drei\u00b7e", "durch", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn die Staublawine kracht,", "tokens": ["Wenn", "die", "Stau\u00b7bla\u00b7wi\u00b7ne", "kracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn mit grausig sch\u00f6nen Flecken", "tokens": ["Wenn", "mit", "grau\u00b7sig", "sch\u00f6\u00b7nen", "Fle\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sich der W\u00fcste Blumen strecken:", "tokens": ["Sich", "der", "W\u00fcs\u00b7te", "Blu\u00b7men", "stre\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schaun sie still auf jene Macht,", "tokens": ["Schaun", "sie", "still", "auf", "je\u00b7ne", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Die sie sicher wird bedecken,", "tokens": ["Die", "sie", "si\u00b7cher", "wird", "be\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VAFIN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Die den Stern hat angefacht.", "tokens": ["Die", "den", "Stern", "hat", "an\u00b7ge\u00b7facht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "O ihr hohen heil'gen Drei!", "tokens": ["O", "ihr", "ho\u00b7hen", "heil'\u00b7gen", "Drei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Finsternis geboren,", "tokens": ["In", "der", "Fins\u00b7ter\u00b7nis", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat euch kaum ein Strahl erkoren,", "tokens": ["Hat", "euch", "kaum", "ein", "Strahl", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ihr folgt so fromm und treu!", "tokens": ["Und", "ihr", "folgt", "so", "fromm", "und", "treu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und du meine Seele, frei", "tokens": ["Und", "du", "mei\u00b7ne", "See\u00b7le", ",", "frei"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "PPOSAT", "NN", "$,", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schwelgend in der Gnade Wogen,", "tokens": ["Schwel\u00b7gend", "in", "der", "Gna\u00b7de", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit Gewalt ans Licht gezogen,", "tokens": ["Mit", "Ge\u00b7walt", "ans", "Licht", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Suchst die Finsternis aufs neu!", "tokens": ["Suchst", "die", "Fins\u00b7ter\u00b7nis", "aufs", "neu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "O wie hast du dich betrogen;", "tokens": ["O", "wie", "hast", "du", "dich", "be\u00b7tro\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Tr\u00e4nen blieben dir und Reu'!", "tokens": ["Tr\u00e4\u00b7nen", "blie\u00b7ben", "dir", "und", "Reu'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KON", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Dennoch, Seele, fasse Mut!", "tokens": ["Den\u00b7noch", ",", "See\u00b7le", ",", "fas\u00b7se", "Mut", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Magst du nimmer gleich ergr\u00fcnden,", "tokens": ["Magst", "du", "nim\u00b7mer", "gleich", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie du kannst Vergebung finden:", "tokens": ["Wie", "du", "kannst", "Ver\u00b7ge\u00b7bung", "fin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gott ist \u00fcber alles gut!", "tokens": ["Gott", "ist", "\u00fc\u00b7ber", "al\u00b7les", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PIS", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hast du in der Reue Flut", "tokens": ["Hast", "du", "in", "der", "Reu\u00b7e", "Flut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dich gerettet aus der Menge,", "tokens": ["Dich", "ge\u00b7ret\u00b7tet", "aus", "der", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ob sie dir das Mark versenge", "tokens": ["Ob", "sie", "dir", "das", "Mark", "ver\u00b7sen\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Siedend in geheimer Glut:", "tokens": ["Sie\u00b7dend", "in", "ge\u00b7hei\u00b7mer", "Glut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "L\u00e4\u00dft dich nimmer dem Gedr\u00e4nge", "tokens": ["L\u00e4\u00dft", "dich", "nim\u00b7mer", "dem", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Der dich warb mit seinem Blut.", "tokens": ["Der", "dich", "warb", "mit", "sei\u00b7nem", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Einen Strahl bin ich nicht wert,", "tokens": ["Ei\u00b7nen", "Strahl", "bin", "ich", "nicht", "wert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht den kleinsten Schein von oben.", "tokens": ["Nicht", "den", "kleins\u00b7ten", "Schein", "von", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "APPR", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Herr, ich will dich freudig loben,", "tokens": ["Herr", ",", "ich", "will", "dich", "freu\u00b7dig", "lo\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was dein Wille mir beschert!", "tokens": ["Was", "dein", "Wil\u00b7le", "mir", "be\u00b7schert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sei es Gram, der mich verzehrt,", "tokens": ["Sei", "es", "Gram", ",", "der", "mich", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Soll mein Liebstes ich verlieren,", "tokens": ["Soll", "mein", "Liebs\u00b7tes", "ich", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Soll ich keine Tr\u00f6stung sp\u00fcren,", "tokens": ["Soll", "ich", "kei\u00b7ne", "Tr\u00f6s\u00b7tung", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sei mir kein Gebet erh\u00f6rt:", "tokens": ["Sei", "mir", "kein", "Ge\u00b7bet", "er\u00b7h\u00f6rt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Kann es nur zu dir mich f\u00fchren,", "tokens": ["Kann", "es", "nur", "zu", "dir", "mich", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Dann willkommen Flamm' und Schwert!", "tokens": ["Dann", "will\u00b7kom\u00b7men", "Flamm'", "und", "Schwert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}