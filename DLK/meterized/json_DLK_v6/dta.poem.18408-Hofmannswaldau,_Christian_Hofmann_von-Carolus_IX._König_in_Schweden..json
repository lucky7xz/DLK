{"dta.poem.18408": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Carolus  IX.  K\u00f6nig in Schweden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vor deiner v\u00e4ter schwerd/ dem stets der sieg geblieben/", "tokens": ["Vor", "dei\u00b7ner", "v\u00e4\u00b7ter", "schwerd", "/", "dem", "stets", "der", "sieg", "ge\u00b7blie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$(", "ART", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zog manch ber\u00fchmtes land die stoltzen gr\u00e4ntzen ein:", "tokens": ["Zog", "manch", "be\u00b7r\u00fchm\u00b7tes", "land", "die", "stolt\u00b7zen", "gr\u00e4nt\u00b7zen", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "L\u00e4st du dir ebenfalls noch diese kunst belieben/", "tokens": ["L\u00e4st", "du", "dir", "e\u00b7ben\u00b7falls", "noch", "die\u00b7se", "kunst", "be\u00b7lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "PDS", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So la\u00df doch Franckreich itzt das meisterst\u00fccke seyn.", "tokens": ["So", "la\u00df", "doch", "Fran\u00b7ck\u00b7reich", "itzt", "das", "meis\u00b7ter\u00b7st\u00fc\u00b7cke", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "p