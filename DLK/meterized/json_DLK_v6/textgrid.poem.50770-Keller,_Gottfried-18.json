{"textgrid.poem.50770": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "18", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es schneit und eist den ganzen Tag,", "tokens": ["Es", "schneit", "und", "eist", "den", "gan\u00b7zen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Frost umf\u00e4ngt mich scharf und blank;", "tokens": ["Der", "Frost", "um\u00b7f\u00e4ngt", "mich", "scharf", "und", "blank", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie ich mich geb\u00e4rden mag \u2013", "tokens": ["Und", "wie", "ich", "mich", "ge\u00b7b\u00e4r\u00b7den", "mag", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun liegt sie wirklich ernsthaft krank!", "tokens": ["Nun", "liegt", "sie", "wirk\u00b7lich", "ernst\u00b7haft", "krank", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ver\u00f6det ist das Paradies,", "tokens": ["Ver\u00b7\u00f6\u00b7det", "ist", "das", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das sonst auf ihrem Angesicht;", "tokens": ["Das", "sonst", "auf", "ih\u00b7rem", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur zitternd blieb und ungewi\u00df", "tokens": ["Nur", "zit\u00b7ternd", "blieb", "und", "un\u00b7ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Augen mildes Sternenlicht.", "tokens": ["Der", "Au\u00b7gen", "mil\u00b7des", "Ster\u00b7nen\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nur wenn ich alle Tag einmal", "tokens": ["Nur", "wenn", "ich", "al\u00b7le", "Tag", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "PIAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An ihrem Krankenlager bin,", "tokens": ["An", "ih\u00b7rem", "Kran\u00b7ken\u00b7la\u00b7ger", "bin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So f\u00e4llt ein heitrer, klarer Strahl", "tokens": ["So", "f\u00e4llt", "ein", "hei\u00b7trer", ",", "kla\u00b7rer", "Strahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf meine feuchten Augen hin.", "tokens": ["Auf", "mei\u00b7ne", "feuch\u00b7ten", "Au\u00b7gen", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wenn wir so beisammen sind,", "tokens": ["Und", "wenn", "wir", "so", "bei\u00b7sam\u00b7men", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann lieb ich still sie anzuschaun", "tokens": ["Dann", "lieb", "ich", "still", "sie", "an\u00b7zu\u00b7schaun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "VMFIN", "PPER", "VVIZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und tr\u00e4umend ob dem lieben Kind", "tokens": ["Und", "tr\u00e4u\u00b7mend", "ob", "dem", "lie\u00b7ben", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Fr\u00fchling wieder aufzubaun!", "tokens": ["Den", "Fr\u00fch\u00b7ling", "wie\u00b7der", "auf\u00b7zu\u00b7baun", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Noch ziert den Mund ein leichtes Rot", "tokens": ["Noch", "ziert", "den", "Mund", "ein", "leich\u00b7tes", "Rot"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und immer eines Kusses wert \u2013", "tokens": ["Und", "im\u00b7mer", "ei\u00b7nes", "Kus\u00b7ses", "wert", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie l\u00e4\u00dft's geschehen, weil die Not", "tokens": ["Sie", "l\u00e4\u00dft's", "ge\u00b7sche\u00b7hen", ",", "weil", "die", "Not"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "VVPP", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Menschenkinder beten lehrt.", "tokens": ["Die", "Men\u00b7schen\u00b7kin\u00b7der", "be\u00b7ten", "lehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbich lieb nicht deinen feinen Mund,", "tokens": ["\u00bb", "ich", "lieb", "nicht", "dei\u00b7nen", "fei\u00b7nen", "Mund", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJD", "PTKNEG", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur deine Seele ganz allein \u2013", "tokens": ["Nur", "dei\u00b7ne", "See\u00b7le", "ganz", "al\u00b7lein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Fr\u00fchling wollen wir gesund", "tokens": ["Im", "Fr\u00fch\u00b7ling", "wol\u00b7len", "wir", "ge\u00b7sund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und beide wieder fr\u00f6hlich sein!\u00ab", "tokens": ["Und", "bei\u00b7de", "wie\u00b7der", "fr\u00f6h\u00b7lich", "sein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIS", "ADV", "ADJD", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und wenn der Arzt kommt, l\u00fcgen wir", "tokens": ["Und", "wenn", "der", "Arzt", "kommt", ",", "l\u00fc\u00b7gen", "wir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn tr\u00f6stlich voller Hoffnung an;", "tokens": ["Ihn", "tr\u00f6st\u00b7lich", "vol\u00b7ler", "Hoff\u00b7nung", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch hab ich heimlich neben ihr", "tokens": ["Doch", "hab", "ich", "heim\u00b7lich", "ne\u00b7ben", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "APPR", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Gott manch hei\u00df Gebet getan.", "tokens": ["Zu", "Gott", "manch", "hei\u00df", "Ge\u00b7bet", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Das ist der erste Kummer, so", "tokens": ["Das", "ist", "der", "ers\u00b7te", "Kum\u00b7mer", ",", "so"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir schwer und ernst ins Leben bricht;", "tokens": ["Mir", "schwer", "und", "ernst", "ins", "Le\u00b7ben", "bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie werd ich wieder leicht und froh,", "tokens": ["Wie", "werd", "ich", "wie\u00b7der", "leicht", "und", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn ihm der Lenz das Urteil spricht!", "tokens": ["Wenn", "ihm", "der", "Lenz", "das", "Ur\u00b7teil", "spricht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}