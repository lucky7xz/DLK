{"textgrid.poem.49550": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Das Anmuthsvolle und Holde", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist so anmuthsvoll und hold?", "tokens": ["Was", "ist", "so", "an\u00b7muths\u00b7voll", "und", "hold", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich kr\u00f6nt bey Tag, in schweren Zweigen,", "tokens": ["Mich", "kr\u00f6nt", "bey", "Tag", ",", "in", "schwe\u00b7ren", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die sich zu mir herunter neigen,", "tokens": ["Die", "sich", "zu", "mir", "her\u00b7un\u00b7ter", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPER", "APZR", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Pomeranzenbaum mit Gold.", "tokens": ["Der", "Po\u00b7me\u00b7ran\u00b7zen\u00b7baum", "mit", "Gold", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was ist so anmuthsvoll und hold?", "tokens": ["Was", "ist", "so", "an\u00b7muths\u00b7voll", "und", "hold", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was ist so anmuthsvoll und hold?", "tokens": ["Was", "ist", "so", "an\u00b7muths\u00b7voll", "und", "hold", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Abends seh' ich Lunen rollen,", "tokens": ["Des", "A\u00b7bends", "seh'", "ich", "Lu\u00b7nen", "rol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und mir verliebt ein M\u00e4ulgen zollen,", "tokens": ["Und", "mir", "ver\u00b7liebt", "ein", "M\u00e4ul\u00b7gen", "zol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sie Endymion gezollt.", "tokens": ["Wie", "sie", "En\u00b7dy\u00b7mi\u00b7on", "ge\u00b7zollt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was ist so anmuthsvoll und hold?", "tokens": ["Was", "ist", "so", "an\u00b7muths\u00b7voll", "und", "hold", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was ist so anmuthsvoll und hold?", "tokens": ["Was", "ist", "so", "an\u00b7muths\u00b7voll", "und", "hold", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich seh' in ihrer Purpurwiegen", "tokens": ["Ich", "seh'", "in", "ih\u00b7rer", "Pur\u00b7pur\u00b7wie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Morgens dort die Sonne liegen,", "tokens": ["Des", "Mor\u00b7gens", "dort", "die", "Son\u00b7ne", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So gl\u00fchend wie ein Trunkenbold.", "tokens": ["So", "gl\u00fc\u00b7hend", "wie", "ein", "Trun\u00b7ken\u00b7bold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was ist so anmuthsvoll und hold?", "tokens": ["Was", "ist", "so", "an\u00b7muths\u00b7voll", "und", "hold", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}