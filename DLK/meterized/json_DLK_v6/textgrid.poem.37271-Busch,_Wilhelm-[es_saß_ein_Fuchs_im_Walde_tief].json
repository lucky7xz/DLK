{"textgrid.poem.37271": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[es sa\u00df ein Fuchs im Walde tief]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sa\u00df ein Fuchs im Walde tief.", "tokens": ["Es", "sa\u00df", "ein", "Fuchs", "im", "Wal\u00b7de", "tief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da schrieb ihm der Bauer einen Brief:", "tokens": ["Da", "schrieb", "ihm", "der", "Bau\u00b7er", "ei\u00b7nen", "Brief", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So und so, und er sollte nur kommen,", "tokens": ["So", "und", "so", ",", "und", "er", "soll\u00b7te", "nur", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "$,", "KON", "PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "'s w\u00e4r alles verziehn, was \u00fcbel genommen.", "tokens": ["'s", "w\u00e4r", "al\u00b7les", "ver\u00b7ziehn", ",", "was", "\u00fc\u00b7bel", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "VVINF", "$,", "PRELS", "ADJD", "VVPP", "$."], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Der Hahn, die H\u00fchner und G\u00e4nse lie\u00dfen", "tokens": ["Der", "Hahn", ",", "die", "H\u00fch\u00b7ner", "und", "G\u00e4n\u00b7se", "lie\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ihn alle zusammen auch vielmals gr\u00fc\u00dfen.", "tokens": ["Ihn", "al\u00b7le", "zu\u00b7sam\u00b7men", "auch", "viel\u00b7mals", "gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Und wann ihn denn erwarten sollte", "tokens": ["Und", "wann", "ihn", "denn", "er\u00b7war\u00b7ten", "soll\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVINF", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sein guter, treuer Krischan Bolte.", "tokens": ["Sein", "gu\u00b7ter", ",", "treu\u00b7er", "Krisc\u00b7han", "Bol\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Drauf schrieb der Fuchs mit G\u00e4nseblut:", "tokens": ["Drauf", "schrieb", "der", "Fuchs", "mit", "G\u00e4n\u00b7seb\u00b7lut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NE", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Kann nicht gut.", "tokens": ["Kann", "nicht", "gut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Meine Alte mal wieder", "tokens": ["Mei\u00b7ne", "Al\u00b7te", "mal", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "ADV"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.12": {"text": "Gekommen nieder!", "tokens": ["Ge\u00b7kom\u00b7men", "nie\u00b7der", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Im \u00fcbrigen von ganzer Seele", "tokens": ["Im", "\u00fcb\u00b7ri\u00b7gen", "von", "gan\u00b7zer", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Dein Fuchs in der H\u00f6hle.", "tokens": ["Dein", "Fuchs", "in", "der", "H\u00f6h\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "APPR", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}