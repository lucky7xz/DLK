{"dta.poem.11428": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Verl\u00e4ugnung der welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer sich verl\u00e4ugnen will, und diese welt verlassen,", "tokens": ["Wer", "sich", "ver\u00b7l\u00e4ug\u00b7nen", "will", ",", "und", "die\u00b7se", "welt", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "VVINF", "VMFIN", "$,", "KON", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der mu\u00df, was menschlich ist, und nicht die menschen hassen.", "tokens": ["Der", "mu\u00df", ",", "was", "menschlich", "ist", ",", "und", "nicht", "die", "men\u00b7schen", "has\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "KON", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "++-+---+--+-", "measure": "trochaic.penta.relaxed"}}}}}