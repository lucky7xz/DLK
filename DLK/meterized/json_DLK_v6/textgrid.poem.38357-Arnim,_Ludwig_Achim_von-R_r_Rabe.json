{"textgrid.poem.38357": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "R r Rabe", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Rab thut t\u00e4glich singen, sein groben rauhen Ba\u00df.", "tokens": ["Der", "Rab", "thut", "t\u00e4g\u00b7lich", "sin\u00b7gen", ",", "sein", "gro\u00b7ben", "rau\u00b7hen", "Ba\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "VVINF", "$,", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Heut will ihm nichts gelingen, drum singt er cras, cras, cras,", "tokens": ["Heut", "will", "ihm", "nichts", "ge\u00b7lin\u00b7gen", ",", "drum", "singt", "er", "cras", ",", "cras", ",", "cras", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "VVINF", "$,", "PAV", "VVFIN", "PPER", "NE", "$,", "FM.fr", "$,", "FM.fr", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wer alles schiebt auf morgen, und nichts gerichtet heut,", "tokens": ["Wer", "al\u00b7les", "schiebt", "auf", "mor\u00b7gen", ",", "und", "nichts", "ge\u00b7rich\u00b7tet", "heut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "APPR", "ADV", "$,", "KON", "PIS", "VVPP", "ADV", "$,"], "meter": "-+--+---+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der mu\u00df stets seyn in Sorgen, da\u00df es ihm fehle weit.", "tokens": ["Der", "mu\u00df", "stets", "seyn", "in", "Sor\u00b7gen", ",", "da\u00df", "es", "ihm", "feh\u00b7le", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "VAINF", "APPR", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}