{"textgrid.poem.62373": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Am Rhein", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin herausgekommen,", "tokens": ["Ich", "bin", "her\u00b7aus\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Worms, der alten Stadt,", "tokens": ["Von", "Worms", ",", "der", "al\u00b7ten", "Stadt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich habe wohl vernommen,", "tokens": ["Ich", "ha\u00b7be", "wohl", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df es gerufen hat.", "tokens": ["Da\u00df", "es", "ge\u00b7ru\u00b7fen", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Am Ufer dort, am rechten,", "tokens": ["Am", "U\u00b7fer", "dort", ",", "am", "rech\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erscheint ein M\u00e4dchenpaar;", "tokens": ["Er\u00b7scheint", "ein", "M\u00e4d\u00b7chen\u00b7paar", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da weht in langen Flechten", "tokens": ["Da", "weht", "in", "lan\u00b7gen", "Flech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein goldnes Lockenhaar.", "tokens": ["Ein", "gold\u00b7nes", "Lo\u00b7cken\u00b7haar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und hier am gr\u00fcnen Flusse", "tokens": ["Und", "hier", "am", "gr\u00fc\u00b7nen", "Flus\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Stadt so wonnesam,", "tokens": ["Die", "Stadt", "so", "won\u00b7ne\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu der mit mildem Gru\u00dfe", "tokens": ["Zu", "der", "mit", "mil\u00b7dem", "Gru\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der milde Siegfried kam.", "tokens": ["Der", "mil\u00b7de", "Sieg\u00b7fried", "kam", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Was hat mich denn gezogen?", "tokens": ["Was", "hat", "mich", "denn", "ge\u00b7zo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was klang in ferner Luft?", "tokens": ["Was", "klang", "in", "fer\u00b7ner", "Luft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O meldet, liebe Wogen,", "tokens": ["O", "mel\u00b7det", ",", "lie\u00b7be", "Wo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo ist Sie, die mich ruft?", "tokens": ["Wo", "ist", "Sie", ",", "die", "mich", "ruft", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nicht h\u00fcben und nicht dr\u00fcben,", "tokens": ["Nicht", "h\u00fc\u00b7ben", "und", "nicht", "dr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "KON", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von unten klingt's herauf;", "tokens": ["Von", "un\u00b7ten", "klingt's", "her\u00b7auf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das W\u00fcnschen und das Lieben", "tokens": ["Das", "W\u00fcn\u00b7schen", "und", "das", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nimmt hier nur tiefen Lauf.", "tokens": ["Nimmt", "hier", "nur", "tie\u00b7fen", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du hast es ja geh\u00f6ret", "tokens": ["Du", "hast", "es", "ja", "ge\u00b7h\u00f6\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Lied nach weiser Kunst,", "tokens": ["Das", "Lied", "nach", "wei\u00b7ser", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie Siegfried ward beth\u00f6ret", "tokens": ["Wie", "Sieg\u00b7fried", "ward", "be\u00b7th\u00f6\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NE", "VAFIN", "VVFIN"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Von s\u00fc\u00dfer Frauengunst.", "tokens": ["Von", "s\u00fc\u00b7\u00dfer", "Frau\u00b7en\u00b7gunst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Um holden Schatz zu werben", "tokens": ["Um", "hol\u00b7den", "Schatz", "zu", "wer\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kam er mit seinem Schatz,", "tokens": ["Kam", "er", "mit", "sei\u00b7nem", "Schatz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu werben und zu sterben", "tokens": ["Zu", "wer\u00b7ben", "und", "zu", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kam er an diesen Platz.", "tokens": ["Kam", "er", "an", "die\u00b7sen", "Platz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "Tief unten in dem Grunde,", "tokens": ["Tief", "un\u00b7ten", "in", "dem", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am feuchten, k\u00fchlen Ort,", "tokens": ["Am", "feuch\u00b7ten", ",", "k\u00fch\u00b7len", "Ort", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da ruht noch diese Stunde", "tokens": ["Da", "ruht", "noch", "die\u00b7se", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Nibelungenhort.", "tokens": ["Der", "Ni\u00b7be\u00b7lun\u00b7gen\u00b7hort", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "So flie\u00dfet nun ihr Wellen,", "tokens": ["So", "flie\u00b7\u00dfet", "nun", "ihr", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und deckt ihn ferner zu,", "tokens": ["Und", "deckt", "ihn", "fer\u00b7ner", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn Herzen sehnend schwellen,", "tokens": ["Wenn", "Her\u00b7zen", "seh\u00b7nend", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Singt sie in stille Ruh.", "tokens": ["Singt", "sie", "in", "stil\u00b7le", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ich trag' ihn fort im Herzen", "tokens": ["Ich", "trag'", "ihn", "fort", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den rechten treuen Schatz,", "tokens": ["Den", "rech\u00b7ten", "treu\u00b7en", "Schatz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da finden Lust und Schmerzen", "tokens": ["Da", "fin\u00b7den", "Lust", "und", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr lange Jahre Platz.", "tokens": ["F\u00fcr", "lan\u00b7ge", "Jah\u00b7re", "Platz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Mich wird sie nicht verderben", "tokens": ["Mich", "wird", "sie", "nicht", "ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die s\u00fc\u00dfe Frauengunst,", "tokens": ["Die", "s\u00fc\u00b7\u00dfe", "Frau\u00b7en\u00b7gunst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch gerne will ich sterben", "tokens": ["Doch", "ger\u00b7ne", "will", "ich", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In heil'ger Liebesbrunst.", "tokens": ["In", "heil'\u00b7ger", "Lie\u00b7bes\u00b7brunst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}