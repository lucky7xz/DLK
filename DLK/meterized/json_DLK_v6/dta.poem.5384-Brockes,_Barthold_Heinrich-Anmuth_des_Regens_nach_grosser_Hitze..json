{"dta.poem.5384": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Anmuth des Regens nach grosser Hitze.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es kochte gleichsam j\u00fcngst der schw\u00fchlen L\u00fcfte Kreis", "tokens": ["Es", "koch\u00b7te", "gleich\u00b7sam", "j\u00fcngst", "der", "schw\u00fch\u00b7len", "L\u00fcf\u00b7te", "Kreis"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch strenger Sonnen-Strahlen Blitze.", "tokens": ["Durch", "stren\u00b7ger", "Son\u00b7nen\u00b7Strah\u00b7len", "Blit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht nur das Land, das Wasser selbst war hei\u00df.", "tokens": ["Nicht", "nur", "das", "Land", ",", "das", "Was\u00b7ser", "selbst", "war", "hei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "$,", "ART", "NN", "ADV", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es lechtzte Gras und Laub vom Druck der schwehren Hitze;", "tokens": ["Es", "lechtz\u00b7te", "Gras", "und", "Laub", "vom", "Druck", "der", "schweh\u00b7ren", "Hit\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als unverhoft ein k\u00fchler Regen fiel,", "tokens": ["Als", "un\u00b7ver\u00b7hoft", "ein", "k\u00fch\u00b7ler", "Re\u00b7gen", "fiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So starck, da\u00df alles rauscht\u2019 und zischte,", "tokens": ["So", "starck", ",", "da\u00df", "al\u00b7les", "rauscht'", "und", "zischte", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wodurch sich Luft und Land und Wald und Feld erfrischte.", "tokens": ["Wo\u00b7durch", "sich", "Luft", "und", "Land", "und", "Wald", "und", "Feld", "er\u00b7frischte", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was erst durchhitzet war ward allgem\u00e4hlig k\u00fchl.", "tokens": ["Was", "erst", "durch\u00b7hit\u00b7zet", "war", "ward", "all\u00b7ge\u00b7m\u00e4h\u00b7lig", "k\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "VAFIN", "VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich sahe die\u00df bedachtsam an, und fand,", "tokens": ["Ich", "sa\u00b7he", "die\u00df", "be\u00b7dacht\u00b7sam", "an", ",", "und", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "ADJD", "PTKVZ", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Da\u00df Nutz und Lust hieraus, zu GOttes Ruhm entstand.", "tokens": ["Da\u00df", "Nutz", "und", "Lust", "hier\u00b7aus", ",", "zu", "Got\u00b7tes", "Ruhm", "ent\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PAV", "$,", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mich daucht\u2019 ob sehen meine Augen,", "tokens": ["Mich", "daucht'", "ob", "se\u00b7hen", "mei\u00b7ne", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Blumen, Kr\u00e4uter, Laub und Gras", "tokens": ["Die", "Blu\u00b7men", ",", "Kr\u00e4u\u00b7ter", ",", "Laub", "und", "Gras"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das lang\u2019 erseufzte laue Na\u00df", "tokens": ["Das", "lang'", "er\u00b7seufz\u00b7te", "lau\u00b7e", "Na\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit tausend kleinen M\u00fcnden, saugen.", "tokens": ["Mit", "tau\u00b7send", "klei\u00b7nen", "M\u00fcn\u00b7den", ",", "sau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mich deucht\u2019, ich k\u00f6nne, GOtt zu ehren,", "tokens": ["Mich", "deucht'", ",", "ich", "k\u00f6n\u00b7ne", ",", "Gott", "zu", "eh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der ihnen neues Labsal schenckt,", "tokens": ["Der", "ih\u00b7nen", "neu\u00b7es", "Lab\u00b7sal", "schenckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der sie so liebreich n\u00e4hrt und tr\u00e4nckt,", "tokens": ["Der", "sie", "so", "lieb\u00b7reich", "n\u00e4hrt", "und", "tr\u00e4nckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Jhr sanftes Schmatzen gleichsam h\u00f6ren.", "tokens": ["Ihr", "sanf\u00b7tes", "Schmat\u00b7zen", "gleich\u00b7sam", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ein jeglich welck-, bestaubtes Blatt", "tokens": ["Ein", "jeg\u00b7lich", "welck", ",", "be\u00b7staub\u00b7tes", "Blatt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PIAT", "TRUNC", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Versch\u00f6nert sich, wird frisch und glatt.", "tokens": ["Ver\u00b7sch\u00f6\u00b7nert", "sich", ",", "wird", "frisch", "und", "glatt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Man sieht ein angenehm und holdes Dunckel-Gr\u00fcn", "tokens": ["Man", "sieht", "ein", "an\u00b7ge\u00b7nehm", "und", "hol\u00b7des", "Dun\u00b7ckel\u00b7Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "ADJD", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So Feld als Garten \u00fcberziehn.", "tokens": ["So", "Feld", "als", "Gar\u00b7ten", "\u00fc\u00b7ber\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ja da des Himmels neues Licht", "tokens": ["Ja", "da", "des", "Him\u00b7mels", "neu\u00b7es", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Sich an der nassen Gl\u00e4tte bricht,", "tokens": ["Sich", "an", "der", "nas\u00b7sen", "Gl\u00e4t\u00b7te", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Kann man, auf allen Bl\u00e4ttern, sch\u00f6n", "tokens": ["Kann", "man", ",", "auf", "al\u00b7len", "Bl\u00e4t\u00b7tern", ",", "sch\u00f6n"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VMFIN", "PIS", "$,", "APPR", "PIAT", "NN", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ein Silber gleichsam gl\u00e4ntzen sehn.", "tokens": ["Ein", "Sil\u00b7ber", "gleich\u00b7sam", "gl\u00e4nt\u00b7zen", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Die schlaffen Stengel steiffen sich,", "tokens": ["Die", "schlaf\u00b7fen", "Sten\u00b7gel", "steif\u00b7fen", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Es richten sich fast sichtbarlich", "tokens": ["Es", "rich\u00b7ten", "sich", "fast", "sicht\u00b7bar\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Bl\u00e4tter in die H\u00f6h, es strotzet Laub und Kraut.", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "in", "die", "H\u00f6h", ",", "es", "strot\u00b7zet", "Laub", "und", "Kraut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dadurch nun, da\u00df die Luft sich lieblich abgek\u00fchlet,", "tokens": ["Da\u00b7durch", "nun", ",", "da\u00df", "die", "Luft", "sich", "lieb\u00b7lich", "ab\u00b7ge\u00b7k\u00fch\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "$,", "KOUS", "ART", "NN", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wird auch mit Lust von unsrer Haut", "tokens": ["Wird", "auch", "mit", "Lust", "von", "uns\u00b7rer", "Haut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein s\u00fcsser Schauder oft gef\u00fchlet,", "tokens": ["Ein", "s\u00fcs\u00b7ser", "Schau\u00b7der", "oft", "ge\u00b7f\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der selber unsern Geist ergetzet", "tokens": ["Der", "sel\u00b7ber", "un\u00b7sern", "Geist", "er\u00b7get\u00b7zet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ihn, wenn ers erwegt, in ein Vergn\u00fcgen setzet,", "tokens": ["Und", "ihn", ",", "wenn", "ers", "er\u00b7wegt", ",", "in", "ein", "Ver\u00b7gn\u00fc\u00b7gen", "set\u00b7zet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "KOUS", "PIS", "VVFIN", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das wahrlich nicht gemein.", "tokens": ["Das", "wahr\u00b7lich", "nicht", "ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Mir fiel hiebey die\u00df Danck-Lied ein:", "tokens": ["Mir", "fiel", "hie\u00b7bey", "die\u00df", "Dan\u00b7ck\u00b7Lied", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PDS", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach GOtt! der du uns diesen Regen,", "tokens": ["Ach", "Gott", "!", "der", "du", "uns", "die\u00b7sen", "Re\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "ART", "PPER", "PRF", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und, in demselben, so viel Seegen", "tokens": ["Und", ",", "in", "dem\u00b7sel\u00b7ben", ",", "so", "viel", "See\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "APPR", "PDAT", "$,", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der d\u00fcrr- und matten Welt geschenckt,", "tokens": ["Der", "d\u00fcrr", "und", "mat\u00b7ten", "Welt", "ge\u00b7schenckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der du das durst\u2019ge Feld getr\u00e4nckt,", "tokens": ["Der", "du", "das", "dur\u00b7st'\u00b7ge", "Feld", "ge\u00b7tr\u00e4nckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der du der welcken Pflantzen Heer,", "tokens": ["Der", "du", "der", "wel\u00b7cken", "Pflant\u00b7zen", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu unserm Nutz, gen\u00e4hrt, erqvicket,", "tokens": ["Zu", "un\u00b7serm", "Nutz", ",", "ge\u00b7n\u00e4hrt", ",", "er\u00b7qvic\u00b7ket", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und auch zugleich die Welt geschm\u00fccket,", "tokens": ["Und", "auch", "zu\u00b7gleich", "die", "Welt", "ge\u00b7schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dir sey daf\u00fcr Lob, Preis und Ehr!", "tokens": ["Dir", "sey", "da\u00b7f\u00fcr", "Lob", ",", "Preis", "und", "Ehr", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}