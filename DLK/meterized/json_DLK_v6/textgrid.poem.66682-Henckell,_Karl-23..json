{"textgrid.poem.66682": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bis dahin Kampf der n\u00e4chsten Augenblicke,", "tokens": ["Bis", "da\u00b7hin", "Kampf", "der", "n\u00e4chs\u00b7ten", "Au\u00b7gen\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PAV", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die hart versagen, was das Bild erf\u00fcllt,", "tokens": ["Die", "hart", "ver\u00b7sa\u00b7gen", ",", "was", "das", "Bild", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bis dahin Rebellion dem Weltgeschicke,", "tokens": ["Bis", "da\u00b7hin", "Re\u00b7bel\u00b7lion", "dem", "Welt\u00b7ge\u00b7schi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PAV", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das grausam sich in dichte Schleier h\u00fcllt,", "tokens": ["Das", "grau\u00b7sam", "sich", "in", "dich\u00b7te", "Schlei\u00b7er", "h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bis dahin mit erhobenem Genicke", "tokens": ["Bis", "da\u00b7hin", "mit", "er\u00b7ho\u00b7be\u00b7nem", "Ge\u00b7ni\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Emp\u00f6rung, ob der Chor der H\u00f6lle br\u00fcllt,", "tokens": ["Em\u00b7p\u00f6\u00b7rung", ",", "ob", "der", "Chor", "der", "H\u00f6l\u00b7le", "br\u00fcllt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Gen Not und Niedertracht und Nebeldrachen", "tokens": ["Gen", "Not", "und", "Nie\u00b7der\u00b7tracht", "und", "Ne\u00b7bel\u00b7dra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit weltersch\u00fctterndem Ver\u00e4chterlachen!", "tokens": ["Mit", "wel\u00b7ter\u00b7sch\u00fct\u00b7tern\u00b7dem", "Ver\u00b7\u00e4ch\u00b7ter\u00b7la\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und ist's der Trotz des t\u00f6richten Titanen,", "tokens": ["Und", "ist's", "der", "Trotz", "des", "t\u00f6\u00b7rich\u00b7ten", "Ti\u00b7ta\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des Ohnmacht dr\u00e4uend sich gen Himmel reckt,", "tokens": ["Des", "Ohn\u00b7macht", "dr\u00e4u\u00b7end", "sich", "gen", "Him\u00b7mel", "reckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu st\u00f6ren das Gestirn in seinen Bahnen,", "tokens": ["Zu", "st\u00f6\u00b7ren", "das", "Ge\u00b7stirn", "in", "sei\u00b7nen", "Bah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das B\u00f6s und Gut mit gleichem Licht bedeckt \u2013", "tokens": ["Das", "B\u00f6s", "und", "Gut", "mit", "glei\u00b7chem", "Licht", "be\u00b7deckt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wohlan, zum Hohn den Welterschaffungsahnen", "tokens": ["Wo\u00b7hlan", ",", "zum", "Hohn", "den", "Wel\u00b7ter\u00b7schaf\u00b7fun\u00b7gsah\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und ihrem Werk, das sie von Grund befleckt,", "tokens": ["Und", "ih\u00b7rem", "Werk", ",", "das", "sie", "von", "Grund", "be\u00b7fleckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Zum Hohn der Schicksalsschlange Trug und Glei\u00dfen,", "tokens": ["Zum", "Hohn", "der", "Schick\u00b7sals\u00b7schlan\u00b7ge", "Trug", "und", "Glei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Aus ihrer Bahn will ich die Sonne rei\u00dfen.", "tokens": ["Aus", "ih\u00b7rer", "Bahn", "will", "ich", "die", "Son\u00b7ne", "rei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}