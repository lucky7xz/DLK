{"textgrid.poem.61202": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bei Gott, ich z\u00e4hle nicht zu den Verwegnen,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bei Gott, ich z\u00e4hle nicht zu den Verwegnen,", "tokens": ["Bei", "Gott", ",", "ich", "z\u00e4h\u00b7le", "nicht", "zu", "den", "Ver\u00b7weg\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die um ein Nichts ein schwer Verh\u00e4ngnis fodern,", "tokens": ["Die", "um", "ein", "Nichts", "ein", "schwer", "Ver\u00b7h\u00e4ng\u00b7nis", "fo\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "PIS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch besser, als am innern Krebs vermodern,", "tokens": ["Doch", "bes\u00b7ser", ",", "als", "am", "in\u00b7nern", "Krebs", "ver\u00b7mo\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Deucht mir's, dem Feind auf blut'gem Feld begegnen.", "tokens": ["Deucht", "mir's", ",", "dem", "Feind", "auf", "blut'\u00b7gem", "Feld", "be\u00b7geg\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ja, dreifach will ich jetzt die Stunde segnen,", "tokens": ["Ja", ",", "drei\u00b7fach", "will", "ich", "jetzt", "die", "Stun\u00b7de", "seg\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PAV", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo ihrer Scheiden bar die Schwerter lodern,", "tokens": ["Wo", "ih\u00b7rer", "Schei\u00b7den", "bar", "die", "Schwer\u00b7ter", "lo\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wo an euern Moseln, euern Odern", "tokens": ["Und", "wo", "an", "eu\u00b7ern", "Mo\u00b7seln", ",", "eu\u00b7ern", "O\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWAV", "APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Statt ew'ger Zankesworte Kugeln regnen.", "tokens": ["Statt", "ew'\u00b7ger", "Zan\u00b7kes\u00b7wor\u00b7te", "Ku\u00b7geln", "reg\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "O s\u00e4h' ich morgen schon den Sonnenschein", "tokens": ["O", "s\u00e4h'", "ich", "mor\u00b7gen", "schon", "den", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sich spiegeln auf den Helmen der Geschwader!", "tokens": ["Sich", "spie\u00b7geln", "auf", "den", "Hel\u00b7men", "der", "Ge\u00b7schwa\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ging's morgen schon in Feindes Land hinein!", "tokens": ["Ging's", "mor\u00b7gen", "schon", "in", "Fein\u00b7des", "Land", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "APPR", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Krieg! Krieg! Gebt einen Krieg uns f\u00fcr den Hader,", "tokens": ["Krieg", "!", "Krieg", "!", "Gebt", "ei\u00b7nen", "Krieg", "uns", "f\u00fcr", "den", "Ha\u00b7der", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "VVIMP", "ART", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der uns das Mark versenget im Gebein! -", "tokens": ["Der", "uns", "das", "Mark", "ver\u00b7sen\u00b7get", "im", "Ge\u00b7bein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Deutschland ist todkrank - schlagt ihm eine Ader!", "tokens": ["Deutschland", "ist", "tod\u00b7krank", "schlagt", "ihm", "ei\u00b7ne", "A\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "$(", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}