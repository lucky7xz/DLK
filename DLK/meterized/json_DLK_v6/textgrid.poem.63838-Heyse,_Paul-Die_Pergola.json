{"textgrid.poem.63838": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Die Pergola", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vier schlanke Pfeiler im Geviert, dar\u00fcber", "tokens": ["Vier", "schlan\u00b7ke", "Pfei\u00b7ler", "im", "Ge\u00b7viert", ",", "da\u00b7r\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["CARD", "ADJA", "NN", "APPRART", "NN", "$,", "PAV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von braunem Holz ein leichtes Sparrenwerk,", "tokens": ["Von", "brau\u00b7nem", "Holz", "ein", "leich\u00b7tes", "Spar\u00b7ren\u00b7werk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der offne Bau von M\u00e4uerchen ums\u00e4umt", "tokens": ["Der", "off\u00b7ne", "Bau", "von", "M\u00e4u\u00b7er\u00b7chen", "um\u00b7s\u00e4umt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und zierlichen Balustern nach dem See,", "tokens": ["Und", "zier\u00b7li\u00b7chen", "Ba\u00b7lus\u00b7tern", "nach", "dem", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So steht an meines Gartens Uferrand", "tokens": ["So", "steht", "an", "mei\u00b7nes", "Gar\u00b7tens", "Uf\u00b7er\u00b7rand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Pergola.", "tokens": ["Die", "Per\u00b7go\u00b7la", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Noch klettern lustig nicht", "tokens": ["Noch", "klet\u00b7tern", "lus\u00b7tig", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die Bangsiarosen bis zum Dach empor.", "tokens": ["Die", "Bang\u00b7si\u00b7a\u00b7ro\u00b7sen", "bis", "zum", "Dach", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Doch \u00fcbers Jahr schon w\u00f6lben ihre Ranken", "tokens": ["Doch", "\u00fc\u00b7bers", "Jahr", "schon", "w\u00f6l\u00b7ben", "ih\u00b7re", "Ran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ein luftig Schattendach, das mir den Brand", "tokens": ["Ein", "luf\u00b7tig", "Schat\u00b7ten\u00b7dach", ",", "das", "mir", "den", "Brand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "$,", "PRELS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Der Maiensonne d\u00e4mpft. Und auf der Bank", "tokens": ["Der", "Mai\u00b7en\u00b7son\u00b7ne", "d\u00e4mpft", ".", "Und", "auf", "der", "Bank"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Darunter sitzend, kann hin\u00fcber ich", "tokens": ["Da\u00b7run\u00b7ter", "sit\u00b7zend", ",", "kann", "hin\u00b7\u00fc\u00b7ber", "ich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVPP", "$,", "VMFIN", "ADV", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Zum Greisenhaupt des Monte Baldo schau'n,", "tokens": ["Zum", "Grei\u00b7sen\u00b7haupt", "des", "Mon\u00b7te", "Bal\u00b7do", "schau'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und an die Brustwehr tr\u00e4umend hingelehnt", "tokens": ["Und", "an", "die", "Brust\u00b7wehr", "tr\u00e4u\u00b7mend", "hin\u00b7ge\u00b7lehnt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Dem Pl\u00e4tschern lauschen der kristallnen Flut,", "tokens": ["Dem", "Pl\u00e4t\u00b7schern", "lau\u00b7schen", "der", "kris\u00b7tall\u00b7nen", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "So klar durchsichtig, da\u00df ich spielen seh'", "tokens": ["So", "klar", "durch\u00b7sich\u00b7tig", ",", "da\u00df", "ich", "spie\u00b7len", "seh'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJD", "$,", "KOUS", "PPER", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Die Fischlein drunten \u00fcberm Kieselgrund,", "tokens": ["Die", "Fisc\u00b7hlein", "drun\u00b7ten", "\u00fc\u00b7berm", "Kie\u00b7sel\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Blitzend wie lautres Silber.", "tokens": ["Blit\u00b7zend", "wie", "laut\u00b7res", "Sil\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.19": {"text": "Hier zu ruhn", "tokens": ["Hier", "zu", "ruhn"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PTKZU", "VVINF"], "meter": "+-+", "measure": "trochaic.di"}, "line.20": {"text": "Nach hei\u00dfem Tagwerk in der Abendk\u00fchle", "tokens": ["Nach", "hei\u00b7\u00dfem", "Tag\u00b7werk", "in", "der", "A\u00b7bend\u00b7k\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Wird k\u00f6stlich sein. Und noch willkommner einst", "tokens": ["Wird", "k\u00f6st\u00b7lich", "sein", ".", "Und", "noch", "will\u00b7komm\u00b7ner", "einst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "VAINF", "$.", "KON", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.23": {"text": "So hoff' ich! Dann jedoch, statt eingepfercht", "tokens": ["So", "hoff'", "ich", "!", "Dann", "je\u00b7doch", ",", "statt", "ein\u00b7ge\u00b7pfercht"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ADV", "ADV", "$,", "KOUI", "VVPP"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.24": {"text": "In eines Friedhofs Mauerring, mein Haupt", "tokens": ["In", "ei\u00b7nes", "Fried\u00b7hofs", "Mau\u00b7er\u00b7ring", ",", "mein", "Haupt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "NE", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Hier frei zu betten w\u00e4r' ein tr\u00f6stlicher", "tokens": ["Hier", "frei", "zu", "bet\u00b7ten", "w\u00e4r'", "ein", "tr\u00f6st\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "VAFIN", "ART", "ADJA"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.26": {"text": "Gedank', und hier, wenn noch ein Abgeschiedner", "tokens": ["Ge\u00b7dank'", ",", "und", "hier", ",", "wenn", "noch", "ein", "Ab\u00b7ge\u00b7schied\u00b7ner"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "ADV", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "In seiner Nacht des Lebens inne wird,", "tokens": ["In", "sei\u00b7ner", "Nacht", "des", "Le\u00b7bens", "in\u00b7ne", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "PTKVZ", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Das droben weiterbraust, vernehm' ich wohl", "tokens": ["Das", "dro\u00b7ben", "wei\u00b7ter\u00b7braust", ",", "ver\u00b7nehm'", "ich", "wohl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADV", "VVFIN", "$,", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Im Traum, wie Enkel und Urenkel fr\u00f6hlich", "tokens": ["Im", "Traum", ",", "wie", "En\u00b7kel", "und", "Ur\u00b7en\u00b7kel", "fr\u00f6h\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "NN", "KON", "NN", "ADJD"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Im Garten spielen. Unter ihnen wandelt", "tokens": ["Im", "Gar\u00b7ten", "spie\u00b7len", ".", "Un\u00b7ter", "ih\u00b7nen", "wan\u00b7delt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVINF", "$.", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Mit ernstem L\u00e4cheln dann die teure Frau,", "tokens": ["Mit", "erns\u00b7tem", "L\u00e4\u00b7cheln", "dann", "die", "teu\u00b7re", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Die mich vermi\u00dft, wenn all den andern schon", "tokens": ["Die", "mich", "ver\u00b7mi\u00dft", ",", "wenn", "all", "den", "an\u00b7dern", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "$,", "KOUS", "PIAT", "ART", "ADJA", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Mein Bild verblich. O liebe, liebliche,", "tokens": ["Mein", "Bild", "ver\u00b7blich", ".", "O", "lie\u00b7be", ",", "lieb\u00b7li\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$.", "NE", "VVFIN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Ewig Geliebte, dein Ged\u00e4chtnis wird,", "tokens": ["E\u00b7wig", "Ge\u00b7lieb\u00b7te", ",", "dein", "Ge\u00b7d\u00e4cht\u00b7nis", "wird", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.35": {"text": "Solang ein Ton von meiner Leier noch", "tokens": ["So\u00b7lang", "ein", "Ton", "von", "mei\u00b7ner", "Lei\u00b7er", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Die Welt durchzittert, nie vergehn! Und die", "tokens": ["Die", "Welt", "durch\u00b7zit\u00b7tert", ",", "nie", "ver\u00b7gehn", "!", "Und", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "ADV", "VVINF", "$.", "KON", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Vor\u00fcberschiffen auf dem See, sie deuten", "tokens": ["Vor\u00b7\u00fc\u00b7bers\u00b7chif\u00b7fen", "auf", "dem", "See", ",", "sie", "deu\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Auf dich und sprechen: 's ist des Dichters Frau,", "tokens": ["Auf", "dich", "und", "spre\u00b7chen", ":", "'s", "ist", "des", "Dich\u00b7ters", "Frau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "VVINF", "$.", "PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.39": {"text": "Der hier gewohnt und diesen See geliebt", "tokens": ["Der", "hier", "ge\u00b7wohnt", "und", "die\u00b7sen", "See", "ge\u00b7liebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVPP", "KON", "PDAT", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Und nun den letzten Schlummer schl\u00e4ft im Schatten", "tokens": ["Und", "nun", "den", "letz\u00b7ten", "Schlum\u00b7mer", "schl\u00e4ft", "im", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Der Pergola.", "tokens": ["Der", "Per\u00b7go\u00b7la", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}