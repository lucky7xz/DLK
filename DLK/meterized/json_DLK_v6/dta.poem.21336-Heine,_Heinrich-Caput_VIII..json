{"dta.poem.21336": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput VIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Menschen schauten so geisterhaft", "tokens": ["Die", "Men\u00b7schen", "schau\u00b7ten", "so", "geis\u00b7ter\u00b7haft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In alter Erinn\u2019rung verloren \u2013", "tokens": ["In", "al\u00b7ter", "Er\u00b7inn'\u00b7rung", "ver\u00b7lo\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der imperiale M\u00e4hrchentraum", "tokens": ["Der", "im\u00b7pe\u00b7ri\u00b7a\u00b7le", "M\u00e4hr\u00b7chen\u00b7traum"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "War wieder herauf beschworen.", "tokens": ["War", "wie\u00b7der", "her\u00b7auf", "be\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich weinte an jenem Tag. Mir sind", "tokens": ["Ich", "wein\u00b7te", "an", "je\u00b7nem", "Tag", ".", "Mir", "sind"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "$.", "PPER", "VAFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Thr\u00e4nen in\u2019s Auge gekommen,", "tokens": ["Die", "Thr\u00e4\u00b7nen", "in's", "Au\u00b7ge", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Als ich den verschollenen Liebesruf,", "tokens": ["Als", "ich", "den", "ver\u00b7schol\u00b7le\u00b7nen", "Lie\u00b7bes\u00b7ruf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}}}}}