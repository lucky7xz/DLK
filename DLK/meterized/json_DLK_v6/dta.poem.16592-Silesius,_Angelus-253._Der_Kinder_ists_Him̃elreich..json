{"dta.poem.16592": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "253.  Der Kinder ists Him\u0303elreich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Christ so du kanst ein Kind von gantzem Hertzen werden/", "tokens": ["Christ", "so", "du", "kanst", "ein", "Kind", "von", "gant\u00b7zem", "Hert\u00b7zen", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "VMFIN", "ART", "NN", "APPR", "ADJA", "NN", "VAINF", "$("], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "So ist das Himmelreich schon deine hier auf Erden.", "tokens": ["So", "ist", "das", "Him\u00b7mel\u00b7reich", "schon", "dei\u00b7ne", "hier", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "PPOSAT", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}