{"textgrid.poem.41928": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mancher mu\u00df die Heimath meiden,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mancher mu\u00df die Heimath meiden,", "tokens": ["Man\u00b7cher", "mu\u00df", "die", "Hei\u00b7math", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch die Liebe schaut ihm nach;", "tokens": ["Doch", "die", "Lie\u00b7be", "schaut", "ihm", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber fremd zur Fremde scheiden,", "tokens": ["A\u00b7ber", "fremd", "zur", "Frem\u00b7de", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist der tr\u00fcbste Scheidetag.", "tokens": ["Ist", "der", "tr\u00fcbs\u00b7te", "Schei\u00b7de\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Heiter gl\u00e4nzt es mir entgegen,", "tokens": ["Hei\u00b7ter", "gl\u00e4nzt", "es", "mir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, das vielgeliebte Haus;", "tokens": ["Ach", ",", "das", "viel\u00b7ge\u00b7lieb\u00b7te", "Haus", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber ich, auf dunkeln Wegen", "tokens": ["A\u00b7ber", "ich", ",", "auf", "dun\u00b7keln", "We\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wandl' ich in die Nacht hinaus.", "tokens": ["Wandl'", "ich", "in", "die", "Nacht", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Anders ist in andern Tagen", "tokens": ["An\u00b7ders", "ist", "in", "an\u00b7dern", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Menschensinn und Angesicht.", "tokens": ["Men\u00b7schen\u00b7sinn", "und", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ich Lust und Leid getragen,", "tokens": ["Wo", "ich", "Lust", "und", "Leid", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da gedenkt man meiner nicht.", "tokens": ["Da", "ge\u00b7denkt", "man", "mei\u00b7ner", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPOSAT", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Fremde haben's leicht genommen,", "tokens": ["Frem\u00b7de", "ha\u00b7ben's", "leicht", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was ich suchte lang und schwer;", "tokens": ["Was", "ich", "such\u00b7te", "lang", "und", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist die fl\u00fccht'ge Gunst verronnen,", "tokens": ["Ist", "die", "fl\u00fccht'\u00b7ge", "Gunst", "ver\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denkt auch ihrer keiner mehr.", "tokens": ["Denkt", "auch", "ih\u00b7rer", "kei\u00b7ner", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "PIS", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und so ziehn auch sie zur Ferne,", "tokens": ["Und", "so", "ziehn", "auch", "sie", "zur", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unbeklagt und ungekr\u00e4nkt,", "tokens": ["Un\u00b7be\u00b7klagt", "und", "un\u00b7ge\u00b7kr\u00e4nkt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Suchen fr\u00f6hlich neue Sterne,", "tokens": ["Su\u00b7chen", "fr\u00f6h\u00b7lich", "neu\u00b7e", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil ihr Herz an keinen h\u00e4ngt.", "tokens": ["Weil", "ihr", "Herz", "an", "kei\u00b7nen", "h\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PIAT", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ewig mu\u00df die Woge branden,", "tokens": ["E\u00b7wig", "mu\u00df", "die", "Wo\u00b7ge", "bran\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimmer steht der Nachen still;", "tokens": ["Nim\u00b7mer", "steht", "der", "Na\u00b7chen", "still", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gl\u00fccklich ist, wer nimmer landen,", "tokens": ["Gl\u00fcck\u00b7lich", "ist", ",", "wer", "nim\u00b7mer", "lan\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "PWS", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und nur schnell vor\u00fcber will.", "tokens": ["Und", "nur", "schnell", "vor\u00b7\u00fc\u00b7ber", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ADV", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}