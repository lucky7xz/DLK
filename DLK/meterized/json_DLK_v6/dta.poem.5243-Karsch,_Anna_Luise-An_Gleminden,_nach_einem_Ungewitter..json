{"dta.poem.5243": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An Gleminden,  \n nach einem Ungewitter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht von den Fl\u00fcgeln starker Winde", "tokens": ["Nicht", "von", "den", "Fl\u00fc\u00b7geln", "star\u00b7ker", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Heraufgebracht, kam es daher", "tokens": ["Her\u00b7auf\u00b7ge\u00b7bracht", ",", "kam", "es", "da\u00b7her"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Ungewitter, o Gleminde!", "tokens": ["Das", "Un\u00b7ge\u00b7wit\u00b7ter", ",", "o", "Gle\u00b7min\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es w\u00e4lzte sich herauf, so f\u00fcrchterlich, so schwer,", "tokens": ["Es", "w\u00e4lz\u00b7te", "sich", "her\u00b7auf", ",", "so", "f\u00fcrch\u00b7ter\u00b7lich", ",", "so", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als wenn in grossen Menschenkriegen,", "tokens": ["Als", "wenn", "in", "gros\u00b7sen", "Men\u00b7schen\u00b7krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zwey Heere langsam ziehn, itzt an einander stehn,", "tokens": ["Zwey", "Hee\u00b7re", "lang\u00b7sam", "ziehn", ",", "itzt", "an", "ein\u00b7an\u00b7der", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "VVINF", "$,", "ADV", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und Kugeln durch die Luft mit Feuerfl\u00fcgeln gehn;", "tokens": ["Und", "Ku\u00b7geln", "durch", "die", "Luft", "mit", "Feu\u00b7er\u00b7fl\u00fc\u00b7geln", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So br\u00fcllten Donner fort! die V\u00f6gel alle schwiegen,", "tokens": ["So", "br\u00fcll\u00b7ten", "Don\u00b7ner", "fort", "!", "die", "V\u00f6\u00b7gel", "al\u00b7le", "schwie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKVZ", "$.", "ART", "NN", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Nachtigallen krochen tief", "tokens": ["Die", "Nach\u00b7ti\u00b7gal\u00b7len", "kro\u00b7chen", "tief"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "In dunkler Hecken Laub, und keine Wachtel rief", "tokens": ["In", "dunk\u00b7ler", "He\u00b7cken", "Laub", ",", "und", "kei\u00b7ne", "Wach\u00b7tel", "rief"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN", "$,", "KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der andern z\u00e4rtlich zu, in Furchen, wo der Weizen", "tokens": ["Der", "an\u00b7dern", "z\u00e4rt\u00b7lich", "zu", ",", "in", "Fur\u00b7chen", ",", "wo", "der", "Wei\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJD", "PTKVZ", "$,", "APPR", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Den Mund hatt\u2019 aufgethan, um Regen einzugeizen.", "tokens": ["Den", "Mund", "hatt'", "auf\u00b7ge\u00b7than", ",", "um", "Re\u00b7gen", "ein\u00b7zu\u00b7gei\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "KOUI", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dem Sch\u00e4fer, der im Schatten schlief,", "tokens": ["Dem", "Sch\u00e4\u00b7fer", ",", "der", "im", "Schat\u00b7ten", "schlief", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Fuhr Schrecken in das Ohr, und in die Brust.", "tokens": ["Fuhr", "Schre\u00b7cken", "in", "das", "Ohr", ",", "und", "in", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "$,", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Er h\u00f6rte", "tokens": ["Er", "h\u00f6r\u00b7te"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.16": {"text": "Das Brausen in dem Wolkenzug.", "tokens": ["Das", "Brau\u00b7sen", "in", "dem", "Wol\u00b7ken\u00b7zug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Dem Pfl\u00fcger sank die Hand an seinem schweren Pflug;", "tokens": ["Dem", "Pfl\u00fc\u00b7ger", "sank", "die", "Hand", "an", "sei\u00b7nem", "schwe\u00b7ren", "Pflug", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und in der St\u00e4dte Zimmer st\u00f6hrte", "tokens": ["Und", "in", "der", "St\u00e4d\u00b7te", "Zim\u00b7mer", "st\u00f6hr\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Das Wetter Gastmahl, Tanz, und Spiel.", "tokens": ["Das", "Wet\u00b7ter", "Gast\u00b7mahl", ",", "Tanz", ",", "und", "Spiel", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "$,", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Der Muth zur Uebelthat entfiel", "tokens": ["Der", "Muth", "zur", "Ue\u00b7belt\u00b7hat", "ent\u00b7fiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Dem S\u00fcnder, der sie itzt versteckt begehen wollte;", "tokens": ["Dem", "S\u00fcn\u00b7der", ",", "der", "sie", "itzt", "ver\u00b7steckt", "be\u00b7ge\u00b7hen", "woll\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Denn hohl, und unaufh\u00f6rlich rollte", "tokens": ["Denn", "hohl", ",", "und", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "roll\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Des Donners schreckliche Gewalt.", "tokens": ["Des", "Don\u00b7ners", "schreck\u00b7li\u00b7che", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Dir aber, Freundin, ward das Herz nicht schauerkalt,", "tokens": ["Dir", "a\u00b7ber", ",", "Freun\u00b7din", ",", "ward", "das", "Herz", "nicht", "schau\u00b7er\u00b7kalt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "$,", "VAFIN", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Du danktest einem Gott, der gro\u00df in Ungewittern", "tokens": ["Du", "dank\u00b7test", "ei\u00b7nem", "Gott", ",", "der", "gro\u00df", "in", "Un\u00b7ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Nach d\u00fcrrer Luft, und d\u00fcrrer Zeit", "tokens": ["Nach", "d\u00fcr\u00b7rer", "Luft", ",", "und", "d\u00fcr\u00b7rer", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Kommt, seinen Erdkrei\u00df zu ersch\u00fcttern,", "tokens": ["Kommt", ",", "sei\u00b7nen", "Erd\u00b7krei\u00df", "zu", "er\u00b7sch\u00fct\u00b7tern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Hervorzubringen Fruchtbarkeit.", "tokens": ["Her\u00b7vor\u00b7zu\u00b7brin\u00b7gen", "Frucht\u00b7bar\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}