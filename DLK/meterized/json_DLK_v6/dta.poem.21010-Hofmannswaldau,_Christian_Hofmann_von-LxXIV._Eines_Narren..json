{"dta.poem.21010": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "LxXIV.  \n Eines Narren.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Mit Lachen sol dein Mund die kurtzen Worte lesen/", "tokens": ["Mit", "La\u00b7chen", "sol", "dein", "Mund", "die", "kurt\u00b7zen", "Wor\u00b7te", "le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier liegt ein lustig Haupt/ so vielen lieb gewesen.", "tokens": ["Hier", "liegt", "ein", "lus\u00b7tig", "Haupt", "/", "so", "vie\u00b7len", "lieb", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN", "$(", "ADV", "PIAT", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Schellen kan dein Kopff kein rechter Erbe seyn/", "tokens": ["Der", "Schel\u00b7len", "kan", "dein", "Kopff", "kein", "rech\u00b7ter", "Er\u00b7be", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "NN", "PIAT", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Mangel ist bey dir/ sie sind vor dich zu klein.", "tokens": ["Der", "Man\u00b7gel", "ist", "bey", "dir", "/", "sie", "sind", "vor", "dich", "zu", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "$(", "PPER", "VAFIN", "APPR", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}}}}}