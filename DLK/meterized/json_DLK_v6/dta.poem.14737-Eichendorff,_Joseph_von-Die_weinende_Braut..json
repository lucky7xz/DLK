{"dta.poem.14737": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die weinende Braut.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Die Fr\u00fchlingslieder neu beginnen \u2014", "tokens": ["Die", "Fr\u00fch\u00b7lings\u00b7lie\u00b7der", "neu", "be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du kehrst nach manchem Jahr zur\u00fcck,", "tokens": ["Du", "kehrst", "nach", "man\u00b7chem", "Jahr", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und stehest still, Dich zu besinnen,", "tokens": ["Und", "ste\u00b7hest", "still", ",", "Dich", "zu", "be\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie auf ein l\u00e4ngstvergang'nes Gl\u00fcck.", "tokens": ["Wie", "auf", "ein", "l\u00e4ngst\u00b7ver\u00b7gang'\u00b7nes", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch w\u00fcstverwachsen liegt der Garten,", "tokens": ["Doch", "w\u00fcst\u00b7ver\u00b7wach\u00b7sen", "liegt", "der", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Haus steht lange still und leer,", "tokens": ["Das", "Haus", "steht", "lan\u00b7ge", "still", "und", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kein Lieb' will Dein am Fenster warten,", "tokens": ["Kein", "Lieb'", "will", "Dein", "am", "Fens\u00b7ter", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPOSAT", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und Dich und mich kennt Niemand mehr.", "tokens": ["Und", "Dich", "und", "mich", "kennt", "Nie\u00b7mand", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KON", "PPER", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}