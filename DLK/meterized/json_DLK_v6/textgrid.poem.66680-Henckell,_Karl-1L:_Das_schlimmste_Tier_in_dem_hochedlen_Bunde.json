{"textgrid.poem.66680": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das schlimmste Tier in dem hochedlen Bunde", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das schlimmste Tier in dem hochedlen Bunde", "tokens": ["Das", "schlimms\u00b7te", "Tier", "in", "dem", "ho\u00b7ched\u00b7len", "Bun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hei\u00dft Mammonitis, wulstig aufgebl\u00e4ht,", "tokens": ["Hei\u00dft", "Mam\u00b7mo\u00b7ni\u00b7tis", ",", "wuls\u00b7tig", "auf\u00b7ge\u00b7bl\u00e4ht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Wucherin mit dem gemeinsten Pfunde,", "tokens": ["Die", "Wu\u00b7che\u00b7rin", "mit", "dem", "ge\u00b7meins\u00b7ten", "Pfun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die einzig nach dem ewigen \u00bbMehr\u00ab-Wert sp\u00e4ht.", "tokens": ["Die", "ein\u00b7zig", "nach", "dem", "e\u00b7wi\u00b7gen", "\u00bb", "Mehr", "\u00ab", "sp\u00e4ht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "ADJA", "$(", "PIAT", "$(", "NE", "VVFIN", "$."], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Sie betet mit dem Geldschrankschl\u00fcsselbunde", "tokens": ["Sie", "be\u00b7tet", "mit", "dem", "Geld\u00b7schrank\u00b7schl\u00fcs\u00b7sel\u00b7bun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Vor einem Altar, dran ein Gauner kr\u00e4ht,", "tokens": ["Vor", "ei\u00b7nem", "Al\u00b7tar", ",", "dran", "ein", "Gau\u00b7ner", "kr\u00e4ht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Verkappt in ehrlichen Verdienstes Robe:", "tokens": ["Ver\u00b7kappt", "in", "ehr\u00b7li\u00b7chen", "Ver\u00b7diens\u00b7tes", "Ro\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "\u00bbdem Mammon Heil, den heiligen Mammon lobe!\u00ab", "tokens": ["\u00bb", "dem", "Mam\u00b7mon", "Heil", ",", "den", "hei\u00b7li\u00b7gen", "Mam\u00b7mon", "lo\u00b7be", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Was adlig atmet, herzrein sich gestaltet,", "tokens": ["Was", "ad\u00b7lig", "at\u00b7met", ",", "herz\u00b7rein", "sich", "ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "$,", "ADV", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was geistesk\u00fchn zum Sonnenlichte dr\u00e4ngt,", "tokens": ["Was", "geis\u00b7tes\u00b7k\u00fchn", "zum", "Son\u00b7nen\u00b7lich\u00b7te", "dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was liebend sich in freier Luft entfaltet", "tokens": ["Was", "lie\u00b7bend", "sich", "in", "frei\u00b7er", "Luft", "ent\u00b7fal\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "PRF", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und freudeschwellend seine Knospen sprengt,", "tokens": ["Und", "freu\u00b7de\u00b7schwel\u00b7lend", "sei\u00b7ne", "Knos\u00b7pen", "sprengt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Worin der Genius der Wahrheit waltet,", "tokens": ["Wo\u00b7rin", "der", "Ge\u00b7nius", "der", "Wahr\u00b7heit", "wal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der niemals niederm Stoffe sich vermengt,", "tokens": ["Der", "nie\u00b7mals", "nie\u00b7derm", "Stof\u00b7fe", "sich", "ver\u00b7mengt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIS", "VVFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das wird verleugnet nur und preisgeboten", "tokens": ["Das", "wird", "ver\u00b7leug\u00b7net", "nur", "und", "preis\u00b7ge\u00b7bo\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "VVFIN", "ADV", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Von Geldessklaven und Besitzdespoten.", "tokens": ["Von", "Gel\u00b7dess\u00b7kla\u00b7ven", "und", "Be\u00b7sitz\u00b7des\u00b7po\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was frommt's, Besitz und Geldeswert verdammen?", "tokens": ["Was", "frommt's", ",", "Be\u00b7sitz", "und", "Gel\u00b7des\u00b7wert", "ver\u00b7dam\u00b7men", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Mensch hat Selbstsucht, w\u00fcnscht sein eigen Teil.", "tokens": ["Der", "Mensch", "hat", "Selbst\u00b7sucht", ",", "w\u00fcnscht", "sein", "ei\u00b7gen", "Teil", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Weil wir von Tieren, nicht von Engeln stammen,", "tokens": ["Weil", "wir", "von", "Tie\u00b7ren", ",", "nicht", "von", "En\u00b7geln", "stam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "$,", "PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "H\u00e4lt Beutelust selbst Bruderliebe feil.", "tokens": ["H\u00e4lt", "Beu\u00b7te\u00b7lust", "selbst", "Bru\u00b7der\u00b7lie\u00b7be", "feil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Raff denn, wer mag, sein H\u00e4uflein hier zusammen,", "tokens": ["Raff", "denn", ",", "wer", "mag", ",", "sein", "H\u00e4uf\u00b7lein", "hier", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "$,", "PWS", "VMFIN", "$,", "PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Liegt in Gemeinschaft auch das h\u00f6here Heil \u2013", "tokens": ["Liegt", "in", "Ge\u00b7mein\u00b7schaft", "auch", "das", "h\u00f6\u00b7he\u00b7re", "Heil", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "++-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.7": {"text": "Vor \u00dcbermenschlichem bleibt Adam stutzig,", "tokens": ["Vor", "\u00dc\u00b7ber\u00b7menschli\u00b7chem", "bleibt", "A\u00b7dam", "stut\u00b7zig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NN", "ADJD", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Doch Geldgesinnung nenn' ich schlechthin schmutzig.", "tokens": ["Doch", "Geld\u00b7ge\u00b7sin\u00b7nung", "nenn'", "ich", "schlecht\u00b7hin", "schmut\u00b7zig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie ist's, die eisig Mensch vom Menschen scheidet", "tokens": ["Sie", "ist's", ",", "die", "ei\u00b7sig", "Mensch", "vom", "Men\u00b7schen", "schei\u00b7det"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "ADJD", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und d\u00fcnkelhaft sich spreizt in eklem Wahn,", "tokens": ["Und", "d\u00fcn\u00b7kel\u00b7haft", "sich", "spreizt", "in", "ek\u00b7lem", "Wahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die sich am Bild der Armut heimlich weidet", "tokens": ["Die", "sich", "am", "Bild", "der", "Ar\u00b7mut", "heim\u00b7lich", "wei\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPRART", "NN", "ART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und tief geringsch\u00e4tzt, die gering sich nahn.", "tokens": ["Und", "tief", "ge\u00b7ring\u00b7sch\u00e4tzt", ",", "die", "ge\u00b7ring", "sich", "nahn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$,", "PRELS", "ADJD", "PRF", "ADJA", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dem Volk hat sie der Arbeit Lohn verleidet", "tokens": ["Dem", "Volk", "hat", "sie", "der", "Ar\u00b7beit", "Lohn", "ver\u00b7lei\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und Ha\u00df ges\u00e4t, wo V\u00e4ter Liebe sahn:", "tokens": ["Und", "Ha\u00df", "ge\u00b7s\u00e4t", ",", "wo", "V\u00e4\u00b7ter", "Lie\u00b7be", "sahn", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$,", "PWAV", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Hochm\u00fctig h\u00fctet sie die saubern H\u00e4nde", "tokens": ["Hoch\u00b7m\u00fc\u00b7tig", "h\u00fc\u00b7tet", "sie", "die", "sau\u00b7bern", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Vor der Besudlung durch die \u00bbuntern St\u00e4nde\u00ab.", "tokens": ["Vor", "der", "Be\u00b7sud\u00b7lung", "durch", "die", "\u00bb", "un\u00b7tern", "St\u00e4n\u00b7de", "\u00ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "$(", "ADJA", "NN", "$(", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Brutal setzt sie den Fu\u00df auf jener Nacken,", "tokens": ["Bru\u00b7tal", "setzt", "sie", "den", "Fu\u00df", "auf", "je\u00b7ner", "Na\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Die sich im Schwei\u00df des Angesichtes m\u00fchn,", "tokens": ["Die", "sich", "im", "Schwei\u00df", "des", "An\u00b7ge\u00b7sich\u00b7tes", "m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im \u00dcberflu\u00df geneigt, noch abzuzwacken", "tokens": ["Im", "\u00dc\u00b7berf\u00b7lu\u00df", "ge\u00b7neigt", ",", "noch", "ab\u00b7zu\u00b7zwa\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "$,", "ADV", "VVIZU"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom sauren Lohn, des Saaten sp\u00e4rlich bl\u00fchn.", "tokens": ["Vom", "sau\u00b7ren", "Lohn", ",", "des", "Saa\u00b7ten", "sp\u00e4r\u00b7lich", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie l\u00e4\u00dft f\u00fcr sich wohl h\u00e4mmern, weben, hacken,", "tokens": ["Sie", "l\u00e4\u00dft", "f\u00fcr", "sich", "wohl", "h\u00e4m\u00b7mern", ",", "we\u00b7ben", ",", "ha\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ADV", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den Acker bauen und das Eisen gl\u00fchn,", "tokens": ["Den", "A\u00b7cker", "bau\u00b7en", "und", "das", "Ei\u00b7sen", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch gilt's, zu sorgen f\u00fcr die \u00bbniedern Klassen\u00ab,", "tokens": ["Doch", "gilt's", ",", "zu", "sor\u00b7gen", "f\u00fcr", "die", "\u00bb", "nie\u00b7dern", "Klas\u00b7sen", "\u00ab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "NN", "APPR", "ART", "$(", "ADJA", "NN", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Glaubt sie sich bestenfalls \u2013 herabzulassen.", "tokens": ["Glaubt", "sie", "sich", "bes\u00b7ten\u00b7falls", "\u2013", "her\u00b7ab\u00b7zu\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Trumpf ist der Geldsackstolz im Spiel der Tage,", "tokens": ["Trumpf", "ist", "der", "Geld\u00b7sack\u00b7stolz", "im", "Spiel", "der", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Darin ich lebe. K\u00fcmmerliche Schau!", "tokens": ["Da\u00b7rin", "ich", "le\u00b7be", ".", "K\u00fcm\u00b7mer\u00b7li\u00b7che", "Schau", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVFIN", "$.", "ADJA", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Der Wert des Menschen selbst scheint eine Sage", "tokens": ["Der", "Wert", "des", "Men\u00b7schen", "selbst", "scheint", "ei\u00b7ne", "Sa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Trotz Unfallsicherung und Kirchenbau.", "tokens": ["Trotz", "Un\u00b7fall\u00b7si\u00b7che\u00b7rung", "und", "Kir\u00b7chen\u00b7bau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sch\u00e4tzung der Schaffenden mit gleicher Wage,", "tokens": ["Sch\u00e4t\u00b7zung", "der", "Schaf\u00b7fen\u00b7den", "mit", "glei\u00b7cher", "Wa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Darin das Z\u00fcnglein Recht zeigt haargenau,", "tokens": ["Da\u00b7rin", "das", "Z\u00fcn\u00b7glein", "Recht", "zeigt", "haar\u00b7ge\u00b7nau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mu\u00df ich trotz Bros\u00e4mlein vom Herrschaftskuchen", "tokens": ["Mu\u00df", "ich", "trotz", "Bro\u00b7s\u00e4m\u00b7lein", "vom", "Herr\u00b7schafts\u00b7ku\u00b7ch