{"dta.poem.2287": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Das Gel\u00fcbde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo ich morgen oder heute", "tokens": ["Wo", "ich", "mor\u00b7gen", "o\u00b7der", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "KON", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Doris wieder finde;", "tokens": ["Mei\u00b7ne", "Do\u00b7ris", "wie\u00b7der", "fin\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ich etwa dort am Ufer", "tokens": ["Wo", "ich", "et\u00b7wa", "dort", "am", "U\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADV", "APPRART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Ihre Spur und Sie entdekke;", "tokens": ["Ih\u00b7re", "Spur", "und", "Sie", "ent\u00b7dek\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo ich sie vieleicht im Schatten", "tokens": ["Wo", "ich", "sie", "vie\u00b7leicht", "im", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Unter Rosen schlummern sehe;", "tokens": ["Un\u00b7ter", "Ro\u00b7sen", "schlum\u00b7mern", "se\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo ich morgen oder heute", "tokens": ["Wo", "ich", "mor\u00b7gen", "o\u00b7der", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "KON", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihren zweiten Ku\u00df empfinde;", "tokens": ["Ih\u00b7ren", "zwei\u00b7ten", "Ku\u00df", "emp\u00b7fin\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Da will ich zum ersten male", "tokens": ["Da", "will", "ich", "zum", "ers\u00b7ten", "ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Da will ich, vernimms o Liebe!", "tokens": ["Da", "will", "ich", ",", "ver\u00b7nimms", "o", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "FM", "FM", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Da will ich, du solt es sehen!", "tokens": ["Da", "will", "ich", ",", "du", "solt", "es", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Dreist nach ihrem Busen schielen.", "tokens": ["Dreist", "nach", "ih\u00b7rem", "Bu\u00b7sen", "schie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}