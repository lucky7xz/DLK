{"dta.poem.7621": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mond am Himmel ist nicht sch\u00f6n im leeren Raum,", "tokens": ["Der", "Mond", "am", "Him\u00b7mel", "ist", "nicht", "sch\u00f6n", "im", "lee\u00b7ren", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VAFIN", "PTKNEG", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Mondschein lieblich nicht auf Fluren ohne Baum.", "tokens": ["Der", "Mond\u00b7schein", "lieb\u00b7lich", "nicht", "auf", "Flu\u00b7ren", "oh\u00b7ne", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKNEG", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Entweder mu\u00df sein Glanz aus lichten Wolken steigen,", "tokens": ["Ent\u00b7we\u00b7der", "mu\u00df", "sein", "Glanz", "aus", "lich\u00b7ten", "Wol\u00b7ken", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Oder gebrochen sanft erscheinen zwischen Zweigen.", "tokens": ["O\u00b7der", "ge\u00b7bro\u00b7chen", "sanft", "er\u00b7schei\u00b7nen", "zwi\u00b7schen", "Zwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "ADJD", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So nimmt die Sch\u00f6nheit selbst bald einen Schleier vor,", "tokens": ["So", "nimmt", "die", "Sch\u00f6n\u00b7heit", "selbst", "bald", "ei\u00b7nen", "Schlei\u00b7er", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bald schauet man zu ihr durch einen auch empor.", "tokens": ["Bald", "schau\u00b7et", "man", "zu", "ihr", "durch", "ei\u00b7nen", "auch", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "PPER", "APPR", "ART", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}