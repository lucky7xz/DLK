{"textgrid.poem.60041": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da\u00df unter tausend, tausend Liedern,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df unter tausend, tausend Liedern,", "tokens": ["Da\u00df", "un\u00b7ter", "tau\u00b7send", ",", "tau\u00b7send", "Lie\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPR", "CARD", "$,", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn jede Muse singt, wenn, voller Seligkeit,", "tokens": ["Wenn", "je\u00b7de", "Mu\u00b7se", "singt", ",", "wenn", ",", "vol\u00b7ler", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "$,", "KOUS", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die V\u00f6lker den Gesang erwiedern \u2013", "tokens": ["Die", "V\u00f6l\u00b7ker", "den", "Ge\u00b7sang", "er\u00b7wie\u00b7dern", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df unter tausend, tausend Liedern,", "tokens": ["Da\u00df", "un\u00b7ter", "tau\u00b7send", ",", "tau\u00b7send", "Lie\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPR", "CARD", "$,", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hervor aus seiner Dunkelheit,", "tokens": ["Her\u00b7vor", "aus", "sei\u00b7ner", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Neides hohle Stimme schreyt \u2013", "tokens": ["Des", "Nei\u00b7des", "hoh\u00b7le", "Stim\u00b7me", "schreyt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O Chloe! soll uns dies in unserm Gl\u00fccke st\u00f6ren?", "tokens": ["O", "Chloe", "!", "soll", "uns", "dies", "in", "un\u00b7serm", "Gl\u00fc\u00b7cke", "st\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "VMFIN", "PPER", "PDS", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Gedenke nur an jenen Hain,", "tokens": ["Ge\u00b7den\u00b7ke", "nur", "an", "je\u00b7nen", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "An jenen Fr\u00fchlings-Sonnenschein!", "tokens": ["An", "je\u00b7nen", "Fr\u00fch\u00b7lings\u00b7Son\u00b7nen\u00b7schein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Da giengen wir, von Nachtigallen-Ch\u00f6ren", "tokens": ["Da", "gien\u00b7gen", "wir", ",", "von", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7Ch\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Das erste Maylied anzuh\u00f6ren;", "tokens": ["Das", "ers\u00b7te", "May\u00b7lied", "an\u00b7zu\u00b7h\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und o wie lieblich sangen sie \u2013", "tokens": ["Und", "o", "wie", "lieb\u00b7lich", "san\u00b7gen", "sie", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "KOKOM", "ADJD", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Als pl\u00f6tzlich unter ihren Ch\u00f6ren,", "tokens": ["Als", "pl\u00f6tz\u00b7lich", "un\u00b7ter", "ih\u00b7ren", "Ch\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Versteckt im Holz, ein heisrer Heher schrie!", "tokens": ["Ver\u00b7steckt", "im", "Holz", ",", "ein", "heis\u00b7rer", "He\u00b7her", "schrie", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Wir aber lie\u00dfen uns nicht st\u00f6ren:", "tokens": ["Wir", "a\u00b7ber", "lie\u00b7\u00dfen", "uns", "nicht", "st\u00f6\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Die rauhen V\u00f6gel selbst geh\u00f6ren", "tokens": ["Die", "rau\u00b7hen", "V\u00f6\u00b7gel", "selbst", "ge\u00b7h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Zur gro\u00dfen Waldes-Harmonie!", "tokens": ["Zur", "gro\u00b7\u00dfen", "Wal\u00b7des\u00b7Har\u00b7mo\u00b7nie", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}