{"textgrid.poem.26659": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Klopstock und Wieland", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gewi\u00df! bin ich nur \u00fcberm Strome dr\u00fcben,", "tokens": ["Ge\u00b7wi\u00df", "!", "bin", "ich", "nur", "\u00fc\u00b7berm", "Stro\u00b7me", "dr\u00fc\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "VAFIN", "PPER", "ADV", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gewi\u00df will ich den Mann zur ", "tokens": ["Ge\u00b7wi\u00df", "will", "ich", "den", "Mann", "zur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "APPRART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr Menschen hat der ", "tokens": ["F\u00fcr", "Men\u00b7schen", "hat", "der"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ihn darf auch unsereiner lieben \u2013", "tokens": ["Ihn", "darf", "auch", "un\u00b7ser\u00b7ei\u00b7ner", "lie\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Komm, linker Mann! Ich k\u00fcsse dich.", "tokens": ["Komm", ",", "lin\u00b7ker", "Mann", "!", "Ich", "k\u00fcs\u00b7se", "dich", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}