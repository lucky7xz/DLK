{"textgrid.poem.33902": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Letzter Gru\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jungfraunbilder, fr\u00fch' erblichen,", "tokens": ["Jung\u00b7fraun\u00b7bil\u00b7der", ",", "fr\u00fch'", "er\u00b7bli\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem Haar den Myrthenkranz,", "tokens": ["In", "dem", "Haar", "den", "Myr\u00b7then\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "D\u00e4mmernd-schwebende Gestalten,", "tokens": ["D\u00e4m\u00b7mern\u00b7dschwe\u00b7ben\u00b7de", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Steigen auf bei Mondenglanz.", "tokens": ["Stei\u00b7gen", "auf", "bei", "Mon\u00b7den\u00b7glanz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wollt ihr mit den wei\u00dfen H\u00e4nden,", "tokens": ["Wollt", "ihr", "mit", "den", "wei\u00b7\u00dfen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die den Knaben nie gedr\u00fcckt,", "tokens": ["Die", "den", "Kna\u00b7ben", "nie", "ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Halb verwelkte Rosen brechen,", "tokens": ["Halb", "ver\u00b7welk\u00b7te", "Ro\u00b7sen", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil kein Fr\u00f6hlicher sie pfl\u00fcckt?", "tokens": ["Weil", "kein", "Fr\u00f6h\u00b7li\u00b7cher", "sie", "pfl\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wollt ihr mit den kalten Lippen,", "tokens": ["Wollt", "ihr", "mit", "den", "kal\u00b7ten", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die kein J\u00fcngling warm gek\u00fc\u00dft,", "tokens": ["Die", "kein", "J\u00fcng\u00b7ling", "warm", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus den Bl\u00fctenkelchen trinken,", "tokens": ["Aus", "den", "Bl\u00fc\u00b7ten\u00b7kel\u00b7chen", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Die der Schmetterling vergi\u00dft?", "tokens": ["Die", "der", "Schmet\u00b7ter\u00b7ling", "ver\u00b7gi\u00dft", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Oder wollt ihr still erkunden,", "tokens": ["O\u00b7der", "wollt", "ihr", "still", "er\u00b7kun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ihr, wie im Traum, euch zeigt,", "tokens": ["Wenn", "ihr", ",", "wie", "im", "Traum", ",", "euch", "zeigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "APPRART", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Ob euch aus dem treusten Herzen", "tokens": ["Ob", "euch", "aus", "dem", "treus\u00b7ten", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Noch ein letzter Seufzer steigt?", "tokens": ["Noch", "ein", "letz\u00b7ter", "Seuf\u00b7zer", "steigt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Eine tritt zu mir an's Lager,", "tokens": ["Ei\u00b7ne", "tritt", "zu", "mir", "an's", "La\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, ich tr\u00e4umte nicht von ihr,", "tokens": ["Ach", ",", "ich", "tr\u00e4um\u00b7te", "nicht", "von", "ihr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PTKNEG", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber, abendroth-umgossen,", "tokens": ["A\u00b7ber", ",", "a\u00b7bend\u00b7ro\u00b7th\u00b7um\u00b7gos\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["KON", "$,", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Steht sie jetzt, wie einst, vor mir.", "tokens": ["Steht", "sie", "jetzt", ",", "wie", "einst", ",", "vor", "mir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "ADV", "$,", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Immer l\u00e4chelnd, immer freundlich,", "tokens": ["Im\u00b7mer", "l\u00e4\u00b7chelnd", ",", "im\u00b7mer", "freund\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und erst in dem letzten Schmerz", "tokens": ["Und", "erst", "in", "dem", "letz\u00b7ten", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Pre\u00dfte sie, zusammen sinkend,", "tokens": ["Pre\u00df\u00b7te", "sie", ",", "zu\u00b7sam\u00b7men", "sin\u00b7kend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihre Hand auf's arme Herz!", "tokens": ["Ih\u00b7re", "Hand", "auf's", "ar\u00b7me", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ach, ihr Herz war wie ein Siegel:", "tokens": ["Ach", ",", "ihr", "Herz", "war", "wie", "ein", "Sie\u00b7gel", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "VAFIN", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Erst als es gebrochen war,", "tokens": ["Erst", "als", "es", "ge\u00b7bro\u00b7chen", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Wurde mir sein schaurig-s\u00fc\u00dfes,", "tokens": ["Wur\u00b7de", "mir", "sein", "schau\u00b7rig\u00b7s\u00fc\u00b7\u00dfes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Himmlisches Geheimni\u00df klar!", "tokens": ["Himm\u00b7li\u00b7sches", "Ge\u00b7heim\u00b7ni\u00df", "klar", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}