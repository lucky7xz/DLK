{"textgrid.poem.66687": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Welch krausen Zickzackweg bin ich gestiegen", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch krausen Zickzackweg bin ich gestiegen", "tokens": ["Welch", "krau\u00b7sen", "Zick\u00b7zack\u00b7weg", "bin", "ich", "ge\u00b7stie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu dieser Anh\u00f6h, die mich h\u00f6her ruft!", "tokens": ["Zu", "die\u00b7ser", "An\u00b7h\u00f6h", ",", "die", "mich", "h\u00f6\u00b7her", "ruft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In welchen W\u00fcnschen kann das Herz sich wiegen,", "tokens": ["In", "wel\u00b7chen", "W\u00fcn\u00b7schen", "kann", "das", "Herz", "sich", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VMFIN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis sich dem Blick das Bergziel abgestuft!", "tokens": ["Bis", "sich", "dem", "Blick", "das", "Berg\u00b7ziel", "ab\u00b7ge\u00b7stuft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Abgr\u00fcnde rings, drin Ungeheuer liegen!", "tokens": ["Ab\u00b7gr\u00fcn\u00b7de", "rings", ",", "drin", "Un\u00b7ge\u00b7heu\u00b7er", "lie\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADV", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Wer seid ihr, Genien, die ihr so mich schuft,", "tokens": ["Wer", "seid", "ihr", ",", "Ge\u00b7ni\u00b7en", ",", "die", "ihr", "so", "mich", "schuft", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "NN", "$,", "PRELS", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Da\u00df ich, schon manchem Drachen schier im Schlunde,", "tokens": ["Da\u00df", "ich", ",", "schon", "man\u00b7chem", "Dra\u00b7chen", "schier", "im", "Schlun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "PIAT", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Hier stehe heil \u2013 mit wem seid ihr im Bunde?", "tokens": ["Hier", "ste\u00b7he", "heil", "\u2013", "mit", "wem", "seid", "ihr", "im", "Bun\u00b7de", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$(", "APPR", "PWS", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Entwirr' ich dieses Kn\u00e4ul von Leidenschaften,", "tokens": ["Ent\u00b7wirr'", "ich", "die\u00b7ses", "Kn\u00e4ul", "von", "Lei\u00b7den\u00b7schaf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Drin Schritt auf Schritt mein Fu\u00df verstrickt sich sah,", "tokens": ["Drin", "Schritt", "auf", "Schritt", "mein", "Fu\u00df", "ver\u00b7strickt", "sich", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "PPOSAT", "NN", "VVFIN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie Schuldgespenster mich zu Boden rafften,", "tokens": ["Wie", "Schuld\u00b7ge\u00b7spens\u00b7ter", "mich", "zu", "Bo\u00b7den", "raff\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie durch Verblendung ich dem Wahnsinn nah,", "tokens": ["Wie", "durch", "Ver\u00b7blen\u00b7dung", "ich", "dem", "Wahn\u00b7sinn", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie durch vergeudend Unma\u00df mir erschlafften", "tokens": ["Wie", "durch", "ver\u00b7geu\u00b7dend", "Un\u00b7ma\u00df", "mir", "er\u00b7schlaff\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "VVPP", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des Geistes Muskeln, und es doch geschah,", "tokens": ["Des", "Geis\u00b7tes", "Mus\u00b7keln", ",", "und", "es", "doch", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "KON", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df es mich aufhob wie mit Adlersf\u00e4ngen,", "tokens": ["Da\u00df", "es", "mich", "auf\u00b7hob", "wie", "mit", "Ad\u00b7lers\u00b7f\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "KOKOM", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Glaub' ich bereitet mich zu Gipfelg\u00e4ngen.", "tokens": ["Glaub'", "ich", "be\u00b7rei\u00b7tet", "mich", "zu", "Gip\u00b7fel\u00b7g\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wer sagt: Durch eigne Kraft? Wer: Gottesm\u00e4chtig?", "tokens": ["Wer", "sagt", ":", "Durch", "eig\u00b7ne", "Kraft", "?", "Wer", ":", "Got\u00b7tes\u00b7m\u00e4ch\u00b7tig", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "$.", "APPR", "ADJA", "NN", "$.", "PWS", "$.", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verdienst und F\u00fcgung sind von Grund gemischt.", "tokens": ["Ver\u00b7dienst", "und", "F\u00fc\u00b7gung", "sind", "von", "Grund", "ge\u00b7mischt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als ich das Gift gesch\u00fcttelt schreckensn\u00e4chtig,", "tokens": ["Als", "ich", "das", "Gift", "ge\u00b7sch\u00fct\u00b7telt", "schre\u00b7cken\u00b7sn\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mir die Verzweiflung: \u00bbTrinke!\u00ab zugezischt,", "tokens": ["Mir", "die", "Ver\u00b7zwei\u00b7flung", ":", "\u00bb", "Trin\u00b7ke", "!", "\u00ab", "zu\u00b7ge\u00b7zischt", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "punct"], "pos": ["PPER", "ART", "NN", "$.", "$(", "VVFIN", "$.", "$(", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "\u00bbdie H\u00f6lle will's. Der Trug ist niedertr\u00e4chtig.", "tokens": ["\u00bb", "die", "H\u00f6l\u00b7le", "will'", "s.", "Der", "Trug", "ist", "nie\u00b7der\u00b7tr\u00e4ch\u00b7tig", "."], "token_info": ["punct", "word", "word", "word", "abbreviation", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VMFIN", "VVIMP", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das Fl\u00e4mmchen qu\u00e4lt sich. Besser, es erlischt.\u00ab", "tokens": ["Das", "Fl\u00e4mm\u00b7chen", "qu\u00e4lt", "sich", ".", "Bes\u00b7ser", ",", "es", "er\u00b7lischt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$.", "NN", "$,", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wer wirkte, da\u00df den Sud ich fortgegossen?", "tokens": ["Wer", "wirk\u00b7te", ",", "da\u00df", "den", "Sud", "ich", "fort\u00b7ge\u00b7gos\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wer hat den Faden mir ans Seil geschlossen?", "tokens": ["Wer", "hat", "den", "Fa\u00b7den", "mir", "ans", "Seil", "ge\u00b7schlos\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und als des Menschheitsglaubens Fr\u00fchlingsbl\u00fcte,", "tokens": ["Und", "als", "des", "Menschheits\u00b7glau\u00b7bens", "Fr\u00fch\u00b7lings\u00b7bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von Frost befallen, tief ihr Haupt gebeugt,", "tokens": ["Von", "Frost", "be\u00b7fal\u00b7len", ",", "tief", "ihr", "Haupt", "ge\u00b7beugt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "ADJD", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als Hagel schlug und winterlich Gew\u00fcte", "tokens": ["Als", "Ha\u00b7gel", "schlug", "und", "win\u00b7ter\u00b7lich", "Ge\u00b7w\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVFIN", "KON", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Lenz zerri\u00df, den ich im Traum erzeugt,", "tokens": ["Den", "Lenz", "zer\u00b7ri\u00df", ",", "den", "ich", "im", "Traum", "er\u00b7zeugt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Als eisiger Schauer Anhauch im Gem\u00fcte", "tokens": ["Als", "ei\u00b7si\u00b7ger", "Schau\u00b7er", "An\u00b7hauch", "im", "Ge\u00b7m\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "NN", "APPRART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Des Herzens Kinder mordete, ges\u00e4ugt", "tokens": ["Des", "Her\u00b7zens", "Kin\u00b7der", "mor\u00b7de\u00b7te", ",", "ge\u00b7s\u00e4ugt"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "$,", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mit meines Lebens liebevollsten S\u00e4ften,", "tokens": ["Mit", "mei\u00b7nes", "Le\u00b7bens", "lie\u00b7be\u00b7volls\u00b7ten", "S\u00e4f\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wer n\u00e4hrte mich mit welterprobtern Kr\u00e4ften?", "tokens": ["Wer", "n\u00e4hr\u00b7te", "mich", "mit", "wel\u00b7ter\u00b7prob\u00b7tern", "Kr\u00e4f\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "PWAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Geheimnis ganz. Frischg\u00e4rung aus Verderben.", "tokens": ["Ge\u00b7heim\u00b7nis", "ganz", ".", "Frischg\u00e4\u00b7rung", "aus", "Ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "NN", "APPR", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aufrichtung aus zerst\u00f6rtem Tempelgrund.", "tokens": ["Auf\u00b7rich\u00b7tung", "aus", "zer\u00b7st\u00f6r\u00b7tem", "Tem\u00b7pel\u00b7grund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mu\u00dft' ich in niederer Gestaltung sterben,", "tokens": ["Mu\u00dft'", "ich", "in", "nie\u00b7de\u00b7rer", "Ge\u00b7stal\u00b7tung", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um reif zu sein zu h\u00f6herem Wesensbund?", "tokens": ["Um", "reif", "zu", "sein", "zu", "h\u00f6\u00b7he\u00b7rem", "We\u00b7sens\u00b7bund", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "PTKZU", "VAINF", "PTKZU", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Brach das Geschirr des Geistes mir in Scherben,", "tokens": ["Brach", "das", "Ge\u00b7schirr", "des", "Geis\u00b7tes", "mir", "in", "Scher\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df es zu sch\u00f6nern Formen auferstund?", "tokens": ["Da\u00df", "es", "zu", "sch\u00f6\u00b7nern", "For\u00b7men", "auf\u00b7er\u00b7stund", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Darf sich mein winzig Schicksal fragend br\u00fcsten:", "tokens": ["Darf", "sich", "mein", "win\u00b7zig", "Schick\u00b7sal", "fra\u00b7gend", "br\u00fcs\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PPOSAT", "ADJA", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "\u00bbl\u00e4uft so das Lebensschiff von Todesk\u00fcsten?\u00ab", "tokens": ["\u00bb", "l\u00e4uft", "so", "das", "Le\u00b7bens\u00b7schiff", "von", "To\u00b7des\u00b7k\u00fcs\u00b7ten", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "ADV", "ART", "NN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wer wei\u00df, wohin mich diese Segel tragen?!", "tokens": ["Wer", "wei\u00df", ",", "wo\u00b7hin", "mich", "die\u00b7se", "Se\u00b7gel", "tra\u00b7gen", "?!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "PPER", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Augenblick zur Ewigkeit \u2013 hoiho!", "tokens": ["Vom", "Au\u00b7gen\u00b7blick", "zur", "E\u00b7wig\u00b7keit", "\u2013", "hoi\u00b7ho", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$(", "ITJ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein enges Sackloch soll mich f\u00fcrder plagen,", "tokens": ["Kein", "en\u00b7ges", "Sack\u00b7loch", "soll", "mich", "f\u00fcr\u00b7der", "pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich bin ein Mensch \u2013 Urgeist, du schufst mich so.", "tokens": ["Ich", "bin", "ein", "Mensch", "\u2013", "Ur\u00b7geist", ",", "du", "schufst", "mich", "so", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "An meiner Schwachheit will ich nicht verzagen,", "tokens": ["An", "mei\u00b7ner", "Schwach\u00b7heit", "will", "ich", "nicht", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der \u00dcberwindung werd' ich kr\u00e4ftig froh,", "tokens": ["Der", "\u00dc\u00b7berw\u00b7in\u00b7dung", "werd'", "ich", "kr\u00e4f\