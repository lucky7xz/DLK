{"textgrid.poem.47961": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Eine Dichterin", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An Meeresstrand bist du geboren,", "tokens": ["An", "Mee\u00b7res\u00b7strand", "bist", "du", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "umrauscht von seinem frischen Wind,", "tokens": ["um\u00b7rauscht", "von", "sei\u00b7nem", "fri\u00b7schen", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "erbl\u00fchtest du, der Welt verloren,", "tokens": ["er\u00b7bl\u00fch\u00b7test", "du", ",", "der", "Welt", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "der Freiheit unentwegtes Kind!", "tokens": ["der", "Frei\u00b7heit", "un\u00b7ent\u00b7weg\u00b7tes", "Kind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Wiegenlied schon sang der Wogen", "tokens": ["Dein", "Wie\u00b7gen\u00b7lied", "schon", "sang", "der", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "geheimnisvolle Melodie \u2013", "tokens": ["ge\u00b7heim\u00b7nis\u00b7vol\u00b7le", "Me\u00b7lo\u00b7die", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "so ward, in ihrem Hauch erzogen,", "tokens": ["so", "ward", ",", "in", "ih\u00b7rem", "Hauch", "er\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "dein Traum und Sinnen Poesie.", "tokens": ["dein", "Traum", "und", "Sin\u00b7nen", "Poe\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nun wogt die See durch deine Lieder,", "tokens": ["Nun", "wogt", "die", "See", "durch", "dei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ein unergr\u00fcndlich tiefes Meer:", "tokens": ["ein", "un\u00b7er\u00b7gr\u00fcnd\u00b7lich", "tie\u00b7fes", "Meer", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Welle flieht und kehret wieder", "tokens": ["Die", "Wel\u00b7le", "flieht", "und", "keh\u00b7ret", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und glitzernd spr\u00fcht der Schaum umher,", "tokens": ["und", "glit\u00b7zernd", "spr\u00fcht", "der", "Schaum", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "erbrausend schl\u00e4gt sie auf am Strande,", "tokens": ["er\u00b7brau\u00b7send", "schl\u00e4gt", "sie", "auf", "am", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "doch nur des Kenners Blick allein", "tokens": ["doch", "nur", "des", "Ken\u00b7ners", "Blick", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "ersp\u00e4ht im feuchten Ufersande", "tokens": ["er\u00b7sp\u00e4ht", "im", "feuch\u00b7ten", "U\u00b7fer\u00b7san\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "der Perle Glanz im Muschelschrein.", "tokens": ["der", "Per\u00b7le", "Glanz", "im", "Mu\u00b7schel\u00b7schrein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}