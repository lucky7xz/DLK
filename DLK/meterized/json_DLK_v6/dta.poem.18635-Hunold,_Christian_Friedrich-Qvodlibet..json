{"dta.poem.18635": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Qvodlibet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.85", "fr:0.14"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Du dumme Welt!", "tokens": ["Du", "dum\u00b7me", "Welt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wenn wirst du einmahl kl\u00fcger heissen?", "tokens": ["Wenn", "wirst", "du", "ein\u00b7mahl", "kl\u00fc\u00b7ger", "heis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was n\u00fctzt in Kasten alles Geld/", "tokens": ["Was", "n\u00fctzt", "in", "Kas\u00b7ten", "al\u00b7les", "Geld", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn du wilst Gr\u00fctz und Bonen speisen?", "tokens": ["Wenn", "du", "wilst", "Gr\u00fctz", "und", "Bo\u00b7nen", "spei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Thaler kriegen einen Rost/", "tokens": ["Die", "Tha\u00b7ler", "krie\u00b7gen", "ei\u00b7nen", "Rost", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und du nur B\u00e4renheuter-Kost.", "tokens": ["Und", "du", "nur", "B\u00e4\u00b7ren\u00b7heu\u00b7ter\u00b7Kost", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Reiche sind den Eseln gleich/", "tokens": ["Rei\u00b7che", "sind", "den", "E\u00b7seln", "gleich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Die offt Gold und Silber tragen/", "tokens": ["Die", "offt", "Gold", "und", "Sil\u00b7ber", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "KON", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Da\u00df sie Last und Sorgen plagen/", "tokens": ["Da\u00df", "sie", "Last", "und", "Sor\u00b7gen", "pla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Denn so sind die Esel reich.", "tokens": ["Denn", "so", "sind", "die", "E\u00b7sel", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Ein Reicher und ein fettes Schwein/", "tokens": ["Ein", "Rei\u00b7cher", "und", "ein", "fet\u00b7tes", "Schwein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die dienen sonst zu nichts/ als da\u00df man sie mu\u00df schlachten;", "tokens": ["Die", "die\u00b7nen", "sonst", "zu", "nichts", "/", "als", "da\u00df", "man", "sie", "mu\u00df", "schlach\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PTKA", "PIS", "$(", "KOKOM", "KOUS", "PIS", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dr\u00fcm Bruder/ wir wollen die Thorheit verpachten/", "tokens": ["Dr\u00fcm", "Bru\u00b7der", "/", "wir", "wol\u00b7len", "die", "Thor\u00b7heit", "ver\u00b7pach\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PPER", "VMFIN", "ART", "NN", "VVFIN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.14": {"text": "Wir trincken den Wein/", "tokens": ["Wir", "trin\u00b7cken", "den", "Wein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Und streichen die Thaler deswegen nur ein/", "tokens": ["Und", "strei\u00b7chen", "die", "Tha\u00b7ler", "des\u00b7we\u00b7gen", "nur", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PAV", "ADV", "ART", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.16": {"text": "Nicht da\u00df wir in ihrer Gefangenschafft liegen/", "tokens": ["Nicht", "da\u00df", "wir", "in", "ih\u00b7rer", "Ge\u00b7fan\u00b7gen\u00b7schafft", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.17": {"text": "Nein/ da\u00df sie uns m\u00fcssen als Sclaven vergn\u00fcgen.", "tokens": ["Nein", "/", "da\u00df", "sie", "uns", "m\u00fcs\u00b7sen", "als", "Scla\u00b7ven", "ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "KOUS", "PPER", "PPER", "VMFIN", "KOUS", "NN", "VVINF", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.18": {"text": "Ich Armer/ Ach! ich mu\u00df verderben/", "tokens": ["Ich", "Ar\u00b7mer", "/", "Ach", "!", "ich", "mu\u00df", "ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$(", "ITJ", "$.", "PPER", "VMFIN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Weil meine Sch\u00f6ne grausam ist/", "tokens": ["Weil", "mei\u00b7ne", "Sch\u00f6\u00b7ne", "grau\u00b7sam", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Sie selber spricht/ du must ersterben/", "tokens": ["Sie", "sel\u00b7ber", "spricht", "/", "du", "must", "ers\u00b7ter\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$(", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "Wie da\u00df du nicht dein Hencker bist?", "tokens": ["Wie", "da\u00df", "du", "nicht", "dein", "Hen\u00b7cker", "bist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "PTKNEG", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Ach lauffet und bringet den Wagen herein/", "tokens": ["Ach", "lauf\u00b7fet", "und", "brin\u00b7get", "den", "Wa\u00b7gen", "her\u00b7ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.23": {"text": "Und schmeisset den Narren ins Tollhau\u00df hinein.", "tokens": ["Und", "schmeis\u00b7set", "den", "Nar\u00b7ren", "ins", "Toll\u00b7hau\u00df", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.24": {"text": "Wer allzusehr verliebt bey einem M\u00e4dgen sieht/", "tokens": ["Wer", "all\u00b7zu\u00b7sehr", "ver\u00b7liebt", "bey", "ei\u00b7nem", "M\u00e4d\u00b7gen", "sieht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Dem wird sie es versagen/", "tokens": ["Dem", "wird", "sie", "es", "ver\u00b7sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Doch wer sich nicht \u00fcm sie bem\u00fcth/", "tokens": ["Doch", "wer", "sich", "nicht", "\u00fcm", "sie", "be\u00b7m\u00fcth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "PTKNEG", "VVFIN", "PPER", "VVFIN", "$("], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.27": {"text": "Dem pflegen sie es selber anzutragen.", "tokens": ["Dem", "pfle\u00b7gen", "sie", "es", "sel\u00b7ber", "an\u00b7zu\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Ach die M\u00e4dgen habens gerne/", "tokens": ["Ach", "die", "M\u00e4d\u00b7gen", "ha\u00b7bens", "ger\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "NN", "VAFIN", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Ob sie gleich was spr\u00f6de seyn:", "tokens": ["Ob", "sie", "gleich", "was", "spr\u00f6\u00b7de", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PWS", "VVFIN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Denn so gleich das Maul zu geben/", "tokens": ["Denn", "so", "gleich", "das", "Maul", "zu", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Und uns zu Gefallen leben/", "tokens": ["Und", "uns", "zu", "Ge\u00b7fal\u00b7len", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.32": {"text": "Trifft nicht mit dem Wolstand ein.", "tokens": ["Trifft", "nicht", "mit", "dem", "Wol\u00b7stand", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.33": {"text": "Das Rind-Vieh kommet heuer", "tokens": ["Das", "Rin\u00b7dVieh", "kom\u00b7met", "heu\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "In zimlich hohen Kauff/", "tokens": ["In", "zim\u00b7lich", "ho\u00b7hen", "Kauff", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.35": {"text": "Die Ochsen werden theuer/", "tokens": ["Die", "Och\u00b7sen", "wer\u00b7den", "theu\u00b7er", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Denn einer fri\u00dft den andern auff.", "tokens": ["Denn", "ei\u00b7ner", "fri\u00dft", "den", "an\u00b7dern", "auff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Neulich trieb die Muntrigkeit mich ins Gr\u00fcne zu spatzieren/", "tokens": ["Neu\u00b7lich", "trieb", "die", "Munt\u00b7rig\u00b7keit", "mich", "ins", "Gr\u00fc\u00b7ne", "zu", "spat\u00b7zie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.38": {"text": "Und ein angenehmer Freund war die beste Lust dabey:", "tokens": ["Und", "ein", "an\u00b7ge\u00b7neh\u00b7mer", "Freund", "war", "die", "bes\u00b7te", "Lust", "da\u00b7bey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "PAV", "$."], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.39": {"text": "Nun solt' uns der Rommeldeus auch die durstge Kehle schmieren", "tokens": ["Nun", "solt'", "uns", "der", "Rom\u00b7mel\u00b7deus", "auch", "die", "durst\u00b7ge", "Keh\u00b7le", "schmie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NE", "ADV", "ART", "ADJA", "NN", "VVINF"], "meter": "-+--+----+-+-+-", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "Doch der Appetit wie\u00df aus/ da\u00df das Wasser besser sey.", "tokens": ["Doch", "der", "Ap\u00b7pe\u00b7tit", "wie\u00df", "aus", "/", "da\u00df", "das", "Was\u00b7ser", "bes\u00b7ser", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$(", "KOUS", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.41": {"text": "Itzt k\u00f6mt ein Schiff von Stade/", "tokens": ["Itzt", "k\u00f6mt", "ein", "Schiff", "von", "Sta\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "Und f\u00fchret Treck-Pomade/", "tokens": ["Und", "f\u00fch\u00b7ret", "Treck\u00b7Po\u00b7ma\u00b7de", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.43": {"text": "Ein Wagen mit Kuhfladen/", "tokens": ["Ein", "Wa\u00b7gen", "mit", "Kuh\u00b7fla\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "Statt Pfl\u00e4stergen beladen/", "tokens": ["Statt", "Pfl\u00e4s\u00b7ter\u00b7gen", "be\u00b7la\u00b7den", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.45": {"text": "Ein Schiff mit steiffen Waden/", "tokens": ["Ein", "Schiff", "mit", "steif\u00b7fen", "Wa\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.46": {"text": "Und sind nun diese waren da/", "tokens": ["Und", "sind", "nun", "die\u00b7se", "wa\u00b7ren", "da", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PDS", "VAFIN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "So wird man gleich die Opera", "tokens": ["So", "wird", "man", "gleich", "die", "O\u00b7pe\u00b7ra"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "ART", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.48": {"text": "Von hundert tausend Jungfern spielen/", "tokens": ["Von", "hun\u00b7dert", "tau\u00b7send", "Jung\u00b7fern", "spie\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "CARD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Wenn eine Wespe sticht/", "tokens": ["Wenn", "ei\u00b7ne", "Wes\u00b7pe", "sticht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.50": {"text": "So pflegt man es zu fielen/", "tokens": ["So", "pflegt", "man", "es", "zu", "fie\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.51": {"text": "Die Zungen stiche Bluten nicht/", "tokens": ["Die", "Zun\u00b7gen", "sti\u00b7che", "Blu\u00b7ten", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Und machen doch viel gr\u00f6ssre Schmertzen.", "tokens": ["Und", "ma\u00b7chen", "doch", "viel", "gr\u00f6ss\u00b7re", "Schmert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "Mit grossen Herrn zu Schertzen/", "tokens": ["Mit", "gros