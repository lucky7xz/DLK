{"textgrid.poem.55377": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Der K\u00f6lner Mummenschanz", "genre": "verse", "period": "N.A.", "pub_year": 1825, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da das Alter, wie wir wissen,", "tokens": ["Da", "das", "Al\u00b7ter", ",", "wie", "wir", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht f\u00fcr Torheit helfen kann,", "tokens": ["Nicht", "f\u00fcr", "Tor\u00b7heit", "hel\u00b7fen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4r es ein gefundner Bissen", "tokens": ["W\u00e4r", "es", "ein", "ge\u00b7fund\u00b7ner", "Bis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einem heitern alten Mann,", "tokens": ["Ei\u00b7nem", "hei\u00b7tern", "al\u00b7ten", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da\u00df am Rhein, dem vielbeschwommnen,", "tokens": ["Da\u00df", "am", "Rhein", ",", "dem", "viel\u00b7be\u00b7schwomm\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NE", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mummenschar sich zum Gefecht", "tokens": ["Mum\u00b7men\u00b7schar", "sich", "zum", "Ge\u00b7fecht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PRF", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "R\u00fcstet gegen angekommnen", "tokens": ["R\u00fcs\u00b7tet", "ge\u00b7gen", "an\u00b7ge\u00b7komm\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Feind, zu sichern altes Recht.", "tokens": ["Feind", ",", "zu", "si\u00b7chern", "al\u00b7tes", "Recht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Auch dem Weisen f\u00fcgt beh\u00e4glich", "tokens": ["Auch", "dem", "Wei\u00b7sen", "f\u00fcgt", "be\u00b7h\u00e4g\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich die Torheit wohl zur Hand,", "tokens": ["Sich", "die", "Tor\u00b7heit", "wohl", "zur", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und so ist es gar vertr\u00e4glich,", "tokens": ["Und", "so", "ist", "es", "gar", "ver\u00b7tr\u00e4g\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn er sich mit euch verband.", "tokens": ["Wenn", "er", "sich", "mit", "euch", "ver\u00b7band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Selbst Erasmus ging den Spuren", "tokens": ["Selbst", "Er\u00b7as\u00b7mus", "ging", "den", "Spu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Ulrich Hutten mit ", "tokens": ["Ul\u00b7rich", "Hut\u00b7ten", "mit"], "token_info": ["word", "word", "word"], "pos": ["NE", "NN", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Derbe Lanzenkiele brach.", "tokens": ["Der\u00b7be", "Lan\u00b7zen\u00b7kie\u00b7le", "brach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "L\u00f6blich wird ein tolles Streben,", "tokens": ["L\u00f6b\u00b7lich", "wird", "ein", "tol\u00b7les", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn es kurz ist und mit Sinn;", "tokens": ["Wenn", "es", "kurz", "ist", "und", "mit", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heiterkeit zum Erdeleben", "tokens": ["Hei\u00b7ter\u00b7keit", "zum", "Er\u00b7de\u00b7le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei dem fl\u00fcchtigen Rausch Gewinn.", "tokens": ["Sei", "dem", "fl\u00fcch\u00b7ti\u00b7gen", "Rausch", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "H\u00e4ufet nur an diesem ", "tokens": ["H\u00e4u\u00b7fet", "nur", "an", "die\u00b7sem"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PDAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Kluger Torheit Vollgewicht,", "tokens": ["Klu\u00b7ger", "Tor\u00b7heit", "Voll\u00b7ge\u00b7wicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df mit uns die Nachwelt sage:", "tokens": ["Da\u00df", "mit", "uns", "die", "Nach\u00b7welt", "sa\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}