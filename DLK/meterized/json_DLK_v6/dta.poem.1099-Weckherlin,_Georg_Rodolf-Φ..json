{"dta.poem.1099": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "\u03a6.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie? wan ich ohn heucheley", "tokens": ["Wie", "?", "wan", "ich", "ohn", "heu\u00b7che\u00b7ley"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "PWAV", "PPER", "APPR", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mich wider ergib/ getrew", "tokens": ["Mich", "wi\u00b7der", "er\u00b7gib", "/", "ge\u00b7trew"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "APPR", "NE", "$(", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In deinem dienst mich zu \u00fcben/", "tokens": ["In", "dei\u00b7nem", "dienst", "mich", "zu", "\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Vnd keine mehr dan dich prei\u00df;", "tokens": ["Vnd", "kei\u00b7ne", "mehr", "dan", "dich", "prei\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADV", "ADV", "PPER", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wilt du mich nicht gleicherwei\u00df", "tokens": ["Wilt", "du", "mich", "nicht", "glei\u00b7cher\u00b7wei\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "PTKNEG", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wider/ vnd jhn nicht mehr lieben?", "tokens": ["Wi\u00b7der", "/", "vnd", "jhn", "nicht", "mehr", "lie\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KON", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}