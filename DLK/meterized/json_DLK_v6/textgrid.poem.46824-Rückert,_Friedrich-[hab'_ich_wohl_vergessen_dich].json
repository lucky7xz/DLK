{"textgrid.poem.46824": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[hab' ich wohl vergessen dich]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hab' ich wohl vergessen dich,", "tokens": ["Hab'", "ich", "wohl", "ver\u00b7ges\u00b7sen", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "VVPP", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil die Thr\u00e4nen wieder wallen,", "tokens": ["Weil", "die", "Thr\u00e4\u00b7nen", "wie\u00b7der", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn der Blick mir ist gefallen", "tokens": ["Wenn", "der", "Blick", "mir", "ist", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf dein Bild? O nein, f\u00fcr mich", "tokens": ["Auf", "dein", "Bild", "?", "O", "nein", ",", "f\u00fcr", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "NE", "PTKANT", "$,", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bist du ewig da im Innern,", "tokens": ["Bist", "du", "e\u00b7wig", "da", "im", "In\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch dein Bild mu\u00df mich erinnern,", "tokens": ["Doch", "dein", "Bild", "mu\u00df", "mich", "e\u00b7rin\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df du da warst \u00e4u\u00dferlich.", "tokens": ["Da\u00df", "du", "da", "warst", "\u00e4u\u00b7\u00dfer\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}