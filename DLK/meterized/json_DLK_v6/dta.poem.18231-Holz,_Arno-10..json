{"dta.poem.18231": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Die ihr so oft schon im Erdengew\u00fchle", "tokens": ["Die", "ihr", "so", "oft", "schon", "im", "Er\u00b7den\u00b7ge\u00b7w\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mir dieses Herz mit sich selber vers\u00f6hnt!", "tokens": ["Mir", "die\u00b7ses", "Herz", "mit", "sich", "sel\u00b7ber", "ver\u00b7s\u00f6hnt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "APPR", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Schwingt euch gen Himmel auf goldnem Gefieder,", "tokens": ["Schwingt", "euch", "gen", "Him\u00b7mel", "auf", "gold\u00b7nem", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Wandelt euch klingend in tr\u00f6stende Lieder,", "tokens": ["Wan\u00b7delt", "euch", "klin\u00b7gend", "in", "tr\u00f6s\u00b7ten\u00b7de", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Da\u00df ich vergesse, ", "tokens": ["Da\u00df", "ich", "ver\u00b7ges\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "O, sagt es niemand, da\u00df l\u00e4ngst eurem Dichter", "tokens": ["O", ",", "sagt", "es", "nie\u00b7mand", ",", "da\u00df", "l\u00e4ngst", "eu\u00b7rem", "Dich\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPER", "PIS", "$,", "KOUS", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Alle die sonnendurchfunkelten Lichter,", "tokens": ["Al\u00b7le", "die", "son\u00b7nen\u00b7durch\u00b7fun\u00b7kel\u00b7ten", "Lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Die euch umflimmern, erloschen sind!", "tokens": ["Die", "euch", "um\u00b7flim\u00b7mern", ",", "er\u00b7lo\u00b7schen", "sind", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "$,", "VVPP", "VAFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "T\u00e4nzelt und gaukelt wie Falter um Rosen,", "tokens": ["T\u00e4n\u00b7zelt", "und", "gau\u00b7kelt", "wie", "Fal\u00b7ter", "um", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "KOKOM", "NN", "APPR", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "La\u00dft euch von schmeichelnden L\u00fcften umkosen,", "tokens": ["La\u00dft", "euch", "von", "schmei\u00b7cheln\u00b7den", "L\u00fcf\u00b7ten", "um\u00b7ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Selber so fl\u00fcchtig wie Wetter und Wind!", "tokens": ["Sel\u00b7ber", "so", "fl\u00fcch\u00b7tig", "wie", "Wet\u00b7ter", "und", "Wind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "KOKOM", "NN", "KON", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Braucht doch die Welt nicht schon heute zu wissen,", "tokens": ["Braucht", "doch", "die", "Welt", "nicht", "schon", "heu\u00b7te", "zu", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKNEG", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Da\u00df sie erst gestern das Herz dir zerrissen,", "tokens": ["Da\u00df", "sie", "erst", "ge\u00b7stern", "das", "Herz", "dir", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Weil es zu rein und zu heilig ergl\u00fcht!", "tokens": ["Weil", "es", "zu", "rein", "und", "zu", "hei\u00b7lig", "er\u00b7gl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "ADJD", "KON", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Opfre dein Herzblut, o la\u00df, la\u00df es fluthen,", "tokens": ["Opf\u00b7re", "dein", "Herz\u00b7blut", ",", "o", "la\u00df", ",", "la\u00df", "es", "flut\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "FM", "FM", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Siehe, schon str\u00f6men die feurigen Gluthen,", "tokens": ["Sie\u00b7he", ",", "schon", "str\u00f6\u00b7men", "die", "feu\u00b7ri\u00b7gen", "Glu\u00b7then", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Freier und reiner nun wird dein Gem\u00fcth!", "tokens": ["Frei\u00b7er", "und", "rei\u00b7ner", "nun", "wird", "dein", "Ge\u00b7m\u00fcth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "H\u00f6her und h\u00f6her in lieblicher Feier", "tokens": ["H\u00f6\u00b7her", "und", "h\u00f6\u00b7her", "in", "lieb\u00b7li\u00b7cher", "Fei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "APPR", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Schwebt deine Seele, es fallen die Schleier,", "tokens": ["Schwebt", "dei\u00b7ne", "See\u00b7le", ",", "es", "fal\u00b7len", "die", "Schlei\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Und dir erscheint ein erhaben Gesicht.", "tokens": ["Und", "dir", "er\u00b7scheint", "ein", "er\u00b7ha\u00b7ben", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Unter dir siehst du der Erde Gebilde,", "tokens": ["Un\u00b7ter", "dir", "siehst", "du", "der", "Er\u00b7de", "Ge\u00b7bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Ueber dir schaust du des Himmels Gefilde,", "tokens": ["Ue\u00b7ber", "dir", "schaust", "du", "des", "Him\u00b7mels", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Um dich und in dir ein g\u00f6ttliches Licht.", "tokens": ["Um", "dich", "und", "in", "dir", "ein", "g\u00f6tt\u00b7li\u00b7ches", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "KON", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.5": {"line.1": {"text": "Juble nun, da\u00df du der Erde entronnen,", "tokens": ["Jub\u00b7le", "nun", ",", "da\u00df", "du", "der", "Er\u00b7de", "ent\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wenn du auch noch nicht den Himmel gewonnen,", "tokens": ["Wenn", "du", "auch", "noch", "nicht", "den", "Him\u00b7mel", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PTKNEG", "ART", "NN", "VVPP", "$,"], "meter": "----+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Schwebst du doch \u00fcber der Welt und dem Schein!", "tokens": ["Schwebst", "du", "doch", "\u00fc\u00b7ber", "der", "Welt", "und", "dem", "Schein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Aber zu bald nur erlahmen die Fl\u00fcgel,", "tokens": ["A\u00b7ber", "zu", "bald", "nur", "er\u00b7lah\u00b7men", "die", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+---+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Nahe und n\u00e4her schon winken die H\u00fcgel,", "tokens": ["Na\u00b7he", "und", "n\u00e4\u00b7her", "schon", "win\u00b7ken", "die", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Klaffend bedroht dich das spitze Gestein!", "tokens": ["Klaf\u00b7fend", "be\u00b7droht", "dich", "das", "spit\u00b7ze", "Ge\u00b7stein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach, und schon f\u00e4llst du, und wehe, da liegst du!", "tokens": ["Ach", ",", "und", "schon", "f\u00e4llst", "du", ",", "und", "we\u00b7he", ",", "da", "liegst", "du", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "ADV", "VVFIN", "PPER", "$,", "KON", "ADJD", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Aber was willst du und warum auch fliegst du?", "tokens": ["A\u00b7ber", "was", "willst", "du", "und", "wa\u00b7rum", "auch", "fliegst", "du", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "PPER", "KON", "PWAV", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Bleibe dahier, denn der Himmel ist hoch!", "tokens": ["Blei\u00b7be", "da\u00b7hier", ",", "denn", "der", "Him\u00b7mel", "ist", "hoch", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "$,", "KON", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Wandle auf Erden und krieche im Staube,", "tokens": ["Wand\u00b7le", "auf", "Er\u00b7den", "und", "krie\u00b7che", "im", "Stau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Denn dich beth\u00f6rt nun dein hoffender Glaube,", "tokens": ["Denn", "dich", "be\u00b7th\u00f6rt", "nun", "dein", "hof\u00b7fen\u00b7der", "Glau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wie dich dein Herz und die Liebe betrog! \u2014", "tokens": ["Wie", "dich", "dein", "Herz", "und", "die", "Lie\u00b7be", "be\u00b7trog", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "KON", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}