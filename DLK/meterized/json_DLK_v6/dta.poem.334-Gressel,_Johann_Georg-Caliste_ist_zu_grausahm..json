{"dta.poem.334": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Caliste  ist zu grausahm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Gewalt! Gewalt! H\u00fclff' ist vonn\u00f6hten/", "tokens": ["Ge\u00b7walt", "!", "Ge\u00b7walt", "!", "H\u00fclff'", "ist", "von\u00b7n\u00f6h\u00b7ten", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "VAFIN", "ADV", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr G\u00f6tter eilet doch herbey!", "tokens": ["Ihr", "G\u00f6t\u00b7ter", "ei\u00b7let", "doch", "her\u00b7bey", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Caliste will mich g\u00e4ntzlich t\u00f6dten/", "tokens": ["Ca\u00b7lis\u00b7te", "will", "mich", "g\u00e4ntz\u00b7lich", "t\u00f6d\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PRF", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ach macht mich von den Plagen frey!", "tokens": ["Ach", "macht", "mich", "von", "den", "Pla\u00b7gen", "frey", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Geist mu\u00df tausend Marter leiden.", "tokens": ["Mein", "Geist", "mu\u00df", "tau\u00b7send", "Mar\u00b7ter", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und darf doch nicht vom Leibe scheiden.", "tokens": ["Und", "darf", "doch", "nicht", "vom", "Lei\u00b7be", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Schnee-Berg ihrer sch\u00f6nen Br\u00fcste/", "tokens": ["Der", "Schnee\u00b7Berg", "ih\u00b7rer", "sch\u00f6\u00b7nen", "Br\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Der glatten Wangen Wunder Pracht/", "tokens": ["Der", "glat\u00b7ten", "Wan\u00b7gen", "Wun\u00b7der", "Pracht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Anblick dieser ", "tokens": ["Der", "An\u00b7blick", "die\u00b7ser"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "PDAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die haben mich zum Knecht gemacht.", "tokens": ["Die", "ha\u00b7ben", "mich", "zum", "Knecht", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich seuffze in den schweren Banden", "tokens": ["Ich", "seuff\u00b7ze", "in", "den", "schwe\u00b7ren", "Ban\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist kein Erretter nicht verhanden?", "tokens": ["Ist", "kein", "Er\u00b7ret\u00b7ter", "nicht", "ver\u00b7han\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach Nein! ich sehe kein Erretten/", "tokens": ["Ach", "Nein", "!", "ich", "se\u00b7he", "kein", "Er\u00b7ret\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PTKANT", "$.", "PPER", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich soll und mu\u00df zu Grunde gehn/", "tokens": ["Ich", "soll", "und", "mu\u00df", "zu", "Grun\u00b7de", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich darf mich in den schweren Ketten", "tokens": ["Ich", "darf", "mich", "in", "den", "schwe\u00b7ren", "Ket\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach keiner Linderung umsehn.", "tokens": ["Nach", "kei\u00b7ner", "Lin\u00b7de\u00b7rung", "um\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jemehr ich meine Marter klage/", "tokens": ["Je\u00b7mehr", "ich", "mei\u00b7ne", "Mar\u00b7ter", "kla\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Je gr\u00f6sser wird die Liebes-Plage.", "tokens": ["Je", "gr\u00f6s\u00b7ser", "wird", "die", "Lie\u00b7bes\u00b7Pla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Weich Sonne/ weich aus deinen Schrancken!", "tokens": ["Weich", "Son\u00b7ne", "/", "weich", "aus", "dei\u00b7nen", "Schran\u00b7cken", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$(", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Komm\u2019 und erweich ihr hartes Hertz/", "tokens": ["Komm'", "und", "er\u00b7weich", "ihr", "har\u00b7tes", "Hertz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KON", "ADJD", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach lencke sie auf die Gedancken", "tokens": ["Ach", "len\u00b7cke", "sie", "auf", "die", "Ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "So leget sich mein Liebes Schmertz.", "tokens": ["So", "le\u00b7get", "sich", "mein", "Lie\u00b7bes", "Schmertz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo nicht? so kommt ihr schwartzen Geister/", "tokens": ["Wo", "nicht", "?", "so", "kommt", "ihr", "schwart\u00b7zen", "Geis\u00b7ter", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$.", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und bringet mich zu euren Meister.", "tokens": ["Und", "brin\u00b7get", "mich", "zu", "eu\u00b7ren", "Meis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}