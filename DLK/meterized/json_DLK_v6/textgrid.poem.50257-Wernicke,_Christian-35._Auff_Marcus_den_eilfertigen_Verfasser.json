{"textgrid.poem.50257": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "35. Auff Marcus den eilfertigen Verfasser", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was Marcus ", "tokens": ["Was", "Mar\u00b7cus"], "token_info": ["word", "word"], "pos": ["PWS", "NE"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Und was ihm aus der Feder f\u00e4llt,", "tokens": ["Und", "was", "ihm", "aus", "der", "Fe\u00b7der", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist wie es f\u00e4llt, beliegen blieben:", "tokens": ["Ist", "wie", "es", "f\u00e4llt", ",", "be\u00b7lie\u00b7gen", "blie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "PPER", "VVFIN", "$,", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und fragst du, was die Ursach sey?", "tokens": ["Und", "fragst", "du", ",", "was", "die", "Ur\u00b7sach", "sey", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil er ", "tokens": ["Weil", "er"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Und ein ", "tokens": ["Und", "ein"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "-+", "measure": "iambic.single"}}}}}