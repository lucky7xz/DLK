{"dta.poem.3624": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "13.  Erziehung durch Genu\u00df .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Steh auf Nordwind,               ", "tokens": ["Steh", "auf", "Nord\u00b7wind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Und komm S\u00fcdwind!", "tokens": ["Und", "komm", "S\u00fcd\u00b7wind", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Weh mit deiner heilgen Luft", "tokens": ["Weh", "mit", "dei\u00b7ner", "heil\u00b7gen", "Luft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch den Garten,", "tokens": ["Durch", "den", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Ich will warten", "tokens": ["Ich", "will", "war\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF"], "meter": "--+-", "measure": "anapaest.init"}, "line.6": {"text": "Dein in meines Herzens Gruft;", "tokens": ["Dein", "in", "mei\u00b7nes", "Her\u00b7zens", "Gruft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "La\u00df dein Sausen", "tokens": ["La\u00df", "dein", "Sau\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Auf mich brausen,", "tokens": ["Auf", "mich", "brau\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Meine Seele nach dir ruft.", "tokens": ["Mei\u00b7ne", "See\u00b7le", "nach", "dir", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Steh auf Nordwind,", "tokens": ["Steh", "auf", "Nord\u00b7wind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Und komm S\u00fcdwind!", "tokens": ["Und", "komm", "S\u00fcd\u00b7wind", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Jag die schwarzen Wolken hin'", "tokens": ["Jag", "die", "schwar\u00b7zen", "Wol\u00b7ken", "hin'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mach das Dunkle,", "tokens": ["Mach", "das", "Dunk\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Da\u00df es funkle,", "tokens": ["Da\u00df", "es", "funk\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Alle Finsterni\u00df zerrinn!", "tokens": ["Al\u00b7le", "Fins\u00b7ter\u00b7ni\u00df", "zer\u00b7rinn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Finstre S\u00fcnden", "tokens": ["Finst\u00b7re", "S\u00fcn\u00b7den"], "token_info": ["word", "word"], "pos": ["NE", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "La\u00df verschwinden,", "tokens": ["La\u00df", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Und mach helle Herz und Sinn.", "tokens": ["Und", "mach", "hel\u00b7le", "Herz", "und", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "KON", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Steh auf Nordwind,", "tokens": ["Steh", "auf", "Nord\u00b7wind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Und komm S\u00fcdwind!", "tokens": ["Und", "komm", "S\u00fcd\u00b7wind", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mach mein kaltes Herze hei\u00df;", "tokens": ["Mach", "mein", "kal\u00b7tes", "Her\u00b7ze", "hei\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich zu lieben,", "tokens": ["Dich", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Das zu \u00fcben,", "tokens": ["Das", "zu", "\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Was gereicht zu deinem Preis.", "tokens": ["Was", "ge\u00b7reicht", "zu", "dei\u00b7nem", "Preis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sey mir g\u00fcnstig,", "tokens": ["Sey", "mir", "g\u00fcns\u00b7tig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Mach mich br\u00fcnstig,", "tokens": ["Mach", "mich", "br\u00fcns\u00b7tig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "In mein Herz die Liebe geu\u00df.", "tokens": ["In", "mein", "Herz", "die", "Lie\u00b7be", "geu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}