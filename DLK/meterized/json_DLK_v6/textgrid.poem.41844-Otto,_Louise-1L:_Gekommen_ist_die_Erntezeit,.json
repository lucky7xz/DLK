{"textgrid.poem.41844": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gekommen ist die Erntezeit,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gekommen ist die Erntezeit,", "tokens": ["Ge\u00b7kom\u00b7men", "ist", "die", "Ern\u00b7te\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schon wird das reife Korn gehauen,", "tokens": ["Schon", "wird", "das", "rei\u00b7fe", "Korn", "ge\u00b7hau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und garbenbindend steht bereit", "tokens": ["Und", "gar\u00b7ben\u00b7bin\u00b7dend", "steht", "be\u00b7reit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Schar der Schnitter und der Frauen.", "tokens": ["Die", "Schar", "der", "Schnit\u00b7ter", "und", "der", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie haben alle ihren Teil", "tokens": ["Sie", "ha\u00b7ben", "al\u00b7le", "ih\u00b7ren", "Teil"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am liederfrohen Erntefeste", "tokens": ["Am", "lie\u00b7der\u00b7fro\u00b7hen", "Ern\u00b7te\u00b7fes\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und rufen jubelnd: \u00bbDank und Heil!", "tokens": ["Und", "ru\u00b7fen", "ju\u00b7belnd", ":", "\u00bb", "Dank", "und", "Heil", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVPP", "$.", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Himmel gab dazu das Beste.\u00ab", "tokens": ["Der", "Him\u00b7mel", "gab", "da\u00b7zu", "das", "Bes\u00b7te", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbder Himmel segnete das M\u00fchn',", "tokens": ["\u00bb", "der", "Him\u00b7mel", "seg\u00b7ne\u00b7te", "das", "M\u00fchn'", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ackernd, s\u00e4end uns verbunden", "tokens": ["Das", "ac\u00b7kernd", ",", "s\u00e4\u00b7end", "uns", "ver\u00b7bun\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADJD", "$,", "KOUS", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Im Wettersturm, im Sonnengl\u00fchn \u2013", "tokens": ["Im", "Wet\u00b7ter\u00b7sturm", ",", "im", "Son\u00b7nen\u00b7gl\u00fchn", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun haben wir den Lohn gefunden!\u00ab", "tokens": ["Nun", "ha\u00b7ben", "wir", "den", "Lohn", "ge\u00b7fun\u00b7den", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sch\u00f6n war das frische Saatengr\u00fcn,", "tokens": ["Sch\u00f6n", "war", "das", "fri\u00b7sche", "Saa\u00b7ten\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sch\u00f6n war das Feld im Aehrenwallen,", "tokens": ["Sch\u00f6n", "war", "das", "Feld", "im", "A\u00b7eh\u00b7ren\u00b7wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Die Wiese sch\u00f6n im Blumenbl\u00fchn \u2013", "tokens": ["Die", "Wie\u00b7se", "sch\u00f6n", "im", "Blu\u00b7men\u00b7bl\u00fchn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch nun sind Bl\u00fct und Halm gefallen. \u2013", "tokens": ["Doch", "nun", "sind", "Bl\u00fct", "und", "Halm", "ge\u00b7fal\u00b7len", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "KON", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Baum, nur sp\u00e4rlich noch belaubt,", "tokens": ["Der", "Baum", ",", "nur", "sp\u00e4r\u00b7lich", "noch", "be\u00b7laubt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wirft statt der Bl\u00fcten Fr\u00fcchte nieder", "tokens": ["Wirft", "statt", "der", "Bl\u00fc\u00b7ten", "Fr\u00fcch\u00b7te", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und sch\u00fcttelt l\u00e4chelnd noch das Haupt,", "tokens": ["Und", "sch\u00fct\u00b7telt", "l\u00e4\u00b7chelnd", "noch", "das", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Singt man f\u00fcr ihn auch Erntelieder.", "tokens": ["Singt", "man", "f\u00fcr", "ihn", "auch", "Ern\u00b7te\u00b7lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So ist es wohl ein Hochgef\u00fchl", "tokens": ["So", "ist", "es", "wohl", "ein", "Hoch\u00b7ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu ernten selbst ges\u00e4te Saaten.", "tokens": ["Zu", "ern\u00b7ten", "selbst", "ge\u00b7s\u00e4\u00b7te", "Saa\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Arbeit Lohn, ein Kranz am Ziel,", "tokens": ["Der", "Ar\u00b7beit", "Lohn", ",", "ein", "Kranz", "am", "Ziel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn, was man pflegte, wohlgeraten.", "tokens": ["Wenn", ",", "was", "man", "pfleg\u00b7te", ",", "wohl\u00b7ge\u00b7ra\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "PIS", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Auch manchen, die nicht selbst ges\u00e4t,", "tokens": ["Auch", "man\u00b7chen", ",", "die", "nicht", "selbst", "ge\u00b7s\u00e4t", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "PRELS", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist Erntesegen doch gekommen;", "tokens": ["Ist", "Ern\u00b7te\u00b7se\u00b7gen", "doch", "ge\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie haben daf\u00fcr fr\u00fch und sp\u00e4t", "tokens": ["Sie", "ha\u00b7ben", "da\u00b7f\u00fcr", "fr\u00fch", "und", "sp\u00e4t"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PAV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein heilig Erbteil \u00fcbernommen.", "tokens": ["Ein", "hei\u00b7lig", "Erb\u00b7teil", "\u00fc\u00b7ber\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Sie m\u00f6gen nun mit voller Hand", "tokens": ["Sie", "m\u00f6\u00b7gen", "nun", "mit", "vol\u00b7ler", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Samen streun, den sie empfangen:", "tokens": ["Den", "Sa\u00b7men", "streun", ",", "den", "sie", "emp\u00b7fan\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des alten Werkes Segenspfand", "tokens": ["Des", "al\u00b7ten", "Wer\u00b7kes", "Se\u00b7gen\u00b7spfand"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Soll er zu neuer Frucht gelangen.", "tokens": ["Soll", "er", "zu", "neu\u00b7er", "Frucht", "ge\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und bleiben jetzt am Stoppelfeld", "tokens": ["Und", "blei\u00b7ben", "jetzt", "am", "Stop\u00b7pel\u00b7feld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wehm\u00fctig unsre Blicke hangen \u2013", "tokens": ["Weh\u00b7m\u00fc\u00b7tig", "uns\u00b7re", "Bli\u00b7cke", "han\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bald wieder wird es neu bestellt", "tokens": ["Bald", "wie\u00b7der", "wird", "es", "neu", "be\u00b7stellt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Gr\u00fcn der Wintersaaten prangen,", "tokens": ["Im", "Gr\u00fcn", "der", "Win\u00b7ter\u00b7saa\u00b7ten", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Scheint einmal alles still und tot,", "tokens": ["Scheint", "ein\u00b7mal", "al\u00b7les", "still", "und", "tot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "W\u00e4chst selbst noch unterm Schnee verborgen", "tokens": ["W\u00e4chst", "selbst", "noch", "un\u00b7term", "Schnee", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die junge Saat zum neuen Brot", "tokens": ["Die", "jun\u00b7ge", "Saat", "zum", "neu\u00b7en", "Brot"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kommt doch f\u00fcr sie ein Ostermorgen.", "tokens": ["Kommt", "doch", "f\u00fcr", "sie", "ein", "Os\u00b7ter\u00b7mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Drum frisch an's Werk und nicht verzagt!", "tokens": ["Drum", "frisch", "an's", "Werk", "und", "nicht", "ver\u00b7zagt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "APPRART", "NN", "KON", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beim Ernten denkt an's Samenstreuen!", "tokens": ["Beim", "Ern\u00b7ten", "denkt", "an's", "Sa\u00b7men\u00b7streu\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn wer da m\u00fc\u00dfig steht und klagt", "tokens": ["Denn", "wer", "da", "m\u00fc\u00b7\u00dfig", "steht", "und", "klagt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "ADJD", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kann keiner Ernte sich erfreuen!", "tokens": ["Kann", "kei\u00b7ner", "Ern\u00b7te", "sich", "er\u00b7freu\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}