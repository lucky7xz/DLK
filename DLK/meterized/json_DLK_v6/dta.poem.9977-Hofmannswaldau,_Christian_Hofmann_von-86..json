{"dta.poem.9977": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "86.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin angenehm und gut/ weil man mich noch nicht erkennet:", "tokens": ["Ich", "bin", "an\u00b7ge\u00b7nehm", "und", "gut", "/", "weil", "man", "mich", "noch", "nicht", "er\u00b7ken\u00b7net", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD", "$(", "KOUS", "PIS", "PRF", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Doch geschiehets nicht aus furcht/ da\u00df man anfangs mich nicht", "tokens": ["Doch", "ge\u00b7schie\u00b7hets", "nicht", "aus", "furcht", "/", "da\u00df", "man", "an\u00b7fangs", "mich", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PTKNEG", "APPR", "NN", "$(", "KOUS", "PIS", "ADV", "PPER", "PTKNEG"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "nennet/", "tokens": ["nen\u00b7net", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "--", "measure": "unknown.measure.zero"}, "line.4": {"text": "Der/ von dem ich her bin kommen/ es so von mir haben will/", "tokens": ["Der", "/", "von", "dem", "ich", "her", "bin", "kom\u00b7men", "/", "es", "so", "von", "mir", "ha\u00b7ben", "will", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "APPR", "PRELS", "PPER", "ADV", "VAFIN", "VVINF", "$(", "PPER", "ADV", "APPR", "PPER", "VAINF", "VMFIN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Jhr seht mich vor euch hier stehen/ und d\u00fcrfft erst nicht rathen", "tokens": ["Ihr", "seht", "mich", "vor", "euch", "hier", "ste\u00b7hen", "/", "und", "d\u00fcrfft", "erst", "nicht", "ra\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "ADV", "VVFIN", "$(", "KON", "VMFIN", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "viel.", "tokens": ["viel", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+", "measure": "single.up"}}}}}