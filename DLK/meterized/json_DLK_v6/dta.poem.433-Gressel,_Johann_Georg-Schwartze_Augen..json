{"dta.poem.433": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Schwartze Augen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Gleich wie das Sonnen-Licht aus schwartzen Wolcken", "tokens": ["Gleich", "wie", "das", "Son\u00b7nen\u00b7Licht", "aus", "schwart\u00b7zen", "Wol\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "prahlet", "tokens": ["prah\u00b7let"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und mit beliebten Blitz auf ", "tokens": ["Und", "mit", "be\u00b7lieb\u00b7ten", "Blitz", "auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So spielt die Anmuth auch aus schwartz-und sch\u00f6nen Augen/", "tokens": ["So", "spielt", "die", "An\u00b7muth", "auch", "aus", "schwartz\u00b7\u00b7und", "sch\u00f6\u00b7nen", "Au\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und l\u00e4st den Liebes-Blitz aus d\u00fcstern Peche saugen.", "tokens": ["Und", "l\u00e4st", "den", "Lie\u00b7bes\u00b7Blitz", "aus", "d\u00fcs\u00b7tern", "Pe\u00b7che", "sau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}