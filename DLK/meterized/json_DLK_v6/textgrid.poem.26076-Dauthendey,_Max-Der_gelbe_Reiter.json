{"textgrid.poem.26076": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Der gelbe Reiter", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schnell hinter dem Regen ritt fiebernd ein Reiter,", "tokens": ["Schnell", "hin\u00b7ter", "dem", "Re\u00b7gen", "ritt", "fie\u00b7bernd", "ein", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Hell hinter den Wolken auf graunassen Wegen.", "tokens": ["Hell", "hin\u00b7ter", "den", "Wol\u00b7ken", "auf", "grau\u00b7nas\u00b7sen", "We\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dem leuchteten gelb die Wangen, die blassen,", "tokens": ["Dem", "leuch\u00b7te\u00b7ten", "gelb", "die", "Wan\u00b7gen", ",", "die", "blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "ART", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie die Gelbbl\u00fcten, die an den B\u00fcschen sa\u00dfen.", "tokens": ["Wie", "die", "Gelb\u00b7bl\u00fc\u00b7ten", ",", "die", "an", "den", "B\u00fc\u00b7schen", "sa\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-++--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Und Mann mit Pferd flog eilend weiter,", "tokens": ["Und", "Mann", "mit", "Pferd", "flog", "ei\u00b7lend", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als w\u00e4re nur Ferne dem Fieber heilend,", "tokens": ["Als", "w\u00e4\u00b7re", "nur", "Fer\u00b7ne", "dem", "Fie\u00b7ber", "hei\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ADV", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Als ob ihn ein Feuer blind vorw\u00e4rts trieb,", "tokens": ["Als", "ob", "ihn", "ein", "Feu\u00b7er", "blind", "vor\u00b7w\u00e4rts", "trieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ART", "NN", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Auf allen B\u00fcschen wie Schwefel weilend", "tokens": ["Auf", "al\u00b7len", "B\u00fc\u00b7schen", "wie", "Schwe\u00b7fel", "wei\u00b7lend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KOKOM", "NN", "VVPP"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Am Weg des Reiters Bl\u00e4sse blieb.", "tokens": ["Am", "Weg", "des", "Rei\u00b7ters", "Bl\u00e4s\u00b7se", "blieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}