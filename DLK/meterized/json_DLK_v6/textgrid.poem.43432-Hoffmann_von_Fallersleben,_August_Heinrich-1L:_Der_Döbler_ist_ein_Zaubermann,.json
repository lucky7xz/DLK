{"textgrid.poem.43432": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der D\u00f6bler ist ein Zaubermann,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der D\u00f6bler ist ein Zaubermann,", "tokens": ["Der", "D\u00f6b\u00b7ler", "ist", "ein", "Zau\u00b7ber\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was der doch sch\u00f6ne Kunstst\u00fcck kann!", "tokens": ["Was", "der", "doch", "sch\u00f6\u00b7ne", "Kunst\u00b7st\u00fcck", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADV", "ADJA", "NN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Beispiel legt er auf den Tisch", "tokens": ["Zum", "Bei\u00b7spiel", "legt", "er", "auf", "den", "Tisch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Ei, das ganz gesund und frisch;", "tokens": ["Ein", "Ei", ",", "das", "ganz", "ge\u00b7sund", "und", "frisch", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er st\u00fclpet eine Glocke drauf \u2013", "tokens": ["Er", "st\u00fcl\u00b7pet", "ei\u00b7ne", "Glo\u00b7cke", "drauf", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was wird daraus? nun passet auf!", "tokens": ["Was", "wird", "da\u00b7raus", "?", "nun", "pas\u00b7set", "auf", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PAV", "$.", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Seht, wie er hebt die Glock' empor,", "tokens": ["Seht", ",", "wie", "er", "hebt", "die", "Glock'", "em\u00b7por", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da kommt ein General hervor,", "tokens": ["Da", "kommt", "ein", "Ge\u00b7ne\u00b7ral", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein General ganz wundernett", "tokens": ["Ein", "Ge\u00b7ne\u00b7ral", "ganz", "wun\u00b7der\u00b7nett"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Degen, Spor'n und Epaulettes.", "tokens": ["Mit", "De\u00b7gen", ",", "Spor'n", "und", "E\u00b7pau\u00b7let\u00b7tes", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der D\u00f6bler macht, o Teufelei!", "tokens": ["Der", "D\u00f6b\u00b7ler", "macht", ",", "o", "Teu\u00b7fe\u00b7lei", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Macht General' aus einem Ei.", "tokens": ["Macht", "Ge\u00b7ne\u00b7ral'", "aus", "ei\u00b7nem", "Ei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Der D\u00f6bler reicht den Korb herum,", "tokens": ["Der", "D\u00f6b\u00b7ler", "reicht", "den", "Korb", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Spricht: hochgeehrtes Publicum!", "tokens": ["Spricht", ":", "hoch\u00b7geehr\u00b7tes", "Pub\u00b7li\u00b7cum", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das ist f\u00fcrwahr gar keine Sach',", "tokens": ["Das", "ist", "f\u00fcr\u00b7wahr", "gar", "kei\u00b7ne", "Sach'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Kunstst\u00fcck macht mir jeder nach.", "tokens": ["Das", "Kunst\u00b7st\u00fcck", "macht", "mir", "je\u00b7der", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Herr, versuchen Sie's einmal!", "tokens": ["Mein", "Herr", ",", "ver\u00b7su\u00b7chen", "Sie's", "ein\u00b7mal", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In jedem Ei ein General.", "tokens": ["In", "je\u00b7dem", "Ei", "ein", "Ge\u00b7ne\u00b7ral", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es nimmt ein Herr ein Ei, gl\u00fcckauf!", "tokens": ["Es", "nimmt", "ein", "Herr", "ein", "Ei", ",", "gl\u00fcc\u00b7kauf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Legt's hin und st\u00fclpt die Glocke drauf.", "tokens": ["Legt's", "hin", "und", "st\u00fclpt", "die", "Glo\u00b7cke", "drauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er hebt die Glocke dann empor,", "tokens": ["Er", "hebt", "die", "Glo\u00b7cke", "dann", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was aber kommt, was kommt hervor?", "tokens": ["Was", "a\u00b7ber", "kommt", ",", "was", "kommt", "her\u00b7vor", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "PWS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dies Mal kein General es ist,", "tokens": ["Dies", "Mal", "kein", "Ge\u00b7ne\u00b7ral", "es", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "PIAT", "NN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "'s Ist ein gemeiner Polizist.", "tokens": ["'s", "Ist", "ein", "ge\u00b7mei\u00b7ner", "Po\u00b7li\u00b7zist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Drob wundert sich denn mancher Tropf,", "tokens": ["Drob", "wun\u00b7dert", "sich", "denn", "man\u00b7cher", "Tropf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sch\u00fcttelt drob gar sehr den Kopf.", "tokens": ["Und", "sch\u00fct\u00b7telt", "drob", "gar", "sehr", "den", "Kopf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der D\u00f6bler untersucht's und find't", "tokens": ["Der", "D\u00f6b\u00b7ler", "un\u00b7ter\u00b7sucht's", "und", "find't"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Heraus den wahren Grund geschwind,", "tokens": ["He\u00b7raus", "den", "wah\u00b7ren", "Grund", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Alles sperret auf das Maul,", "tokens": ["Und", "Al\u00b7les", "sper\u00b7ret", "auf", "das", "Maul", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als D\u00f6bler spricht: ", "tokens": ["Als", "D\u00f6b\u00b7ler", "spricht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}