{"dta.poem.8544": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Unbeschreiblich .", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dreitausend Schreiber auf Teppichen sa\u00dfen", "tokens": ["Drei\u00b7tau\u00b7send", "Schrei\u00b7ber", "auf", "Tep\u00b7pi\u00b7chen", "sa\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und r\u00fchrten den Bart mit der Feder;", "tokens": ["Und", "r\u00fchr\u00b7ten", "den", "Bart", "mit", "der", "Fe\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sie schrieben, schrieben so manchen Tag,", "tokens": ["Sie", "schrie\u00b7ben", ",", "schrie\u00b7ben", "so", "man\u00b7chen", "Tag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da\u00df grau geworden die B\u00e4rte,", "tokens": ["Da\u00df", "grau", "ge\u00b7wor\u00b7den", "die", "B\u00e4r\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VAPP", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Da\u00df tr\u00fcb geworden die Augen l\u00e4ngst", "tokens": ["Da\u00df", "tr\u00fcb", "ge\u00b7wor\u00b7den", "die", "Au\u00b7gen", "l\u00e4ngst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "VAPP", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und l\u00e4ngst erkrummet die Finger;", "tokens": ["Und", "l\u00e4ngst", "er\u00b7krum\u00b7met", "die", "Fin\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wer aber, was sie geschrieben lies\u2019t,", "tokens": ["Wer", "a\u00b7ber", ",", "was", "sie", "ge\u00b7schrie\u00b7ben", "lies't", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$,", "PRELS", "PPER", "VVPP", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und lies\u2019t das, was sie geschrieben,", "tokens": ["Und", "lies't", "das", ",", "was", "sie", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Der spricht: es ist ein Schatten wohl,", "tokens": ["Der", "spricht", ":", "es", "ist", "ein", "Schat\u00b7ten", "wohl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "PPER", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Oder ist es der Schatten des Schattens.", "tokens": ["O\u00b7der", "ist", "es", "der", "Schat\u00b7ten", "des", "Schat\u00b7tens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}}}}