{"textgrid.poem.33433": {"metadata": {"author": {"name": "Novalis", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich ihn nur habe,", "tokens": ["Wenn", "ich", "ihn", "nur", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VAFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wenn er mein nur ist,", "tokens": ["Wenn", "er", "mein", "nur", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADV", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn mein Herz bis hin zum Grabe", "tokens": ["Wenn", "mein", "Herz", "bis", "hin", "zum", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine Treue nie vergi\u00dft:", "tokens": ["Sei\u00b7ne", "Treu\u00b7e", "nie", "ver\u00b7gi\u00dft", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wei\u00df ich nichts von Leide,", "tokens": ["Wei\u00df", "ich", "nichts", "von", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "F\u00fchle nichts, als Andacht, Lieb' und Freude.", "tokens": ["F\u00fch\u00b7le", "nichts", ",", "als", "An\u00b7dacht", ",", "Lieb'", "und", "Freu\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "$,", "KOUS", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn ich ihn nur habe,", "tokens": ["Wenn", "ich", "ihn", "nur", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VAFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Lass' ich alles gern,", "tokens": ["Lass'", "ich", "al\u00b7les", "gern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIS", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Folg' an meinem Wanderstabe", "tokens": ["Fol\u00b7g'", "an", "mei\u00b7nem", "Wan\u00b7der\u00b7sta\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Treugesinnt nur meinem Herrn;", "tokens": ["Treu\u00b7ge\u00b7sinnt", "nur", "mei\u00b7nem", "Herrn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lasse still die Andern", "tokens": ["Las\u00b7se", "still", "die", "An\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Breite, lichte, volle Stra\u00dfen wandern.", "tokens": ["Brei\u00b7te", ",", "lich\u00b7te", ",", "vol\u00b7le", "Stra\u00b7\u00dfen", "wan\u00b7dern", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Wenn ich ihn nur habe,", "tokens": ["Wenn", "ich", "ihn", "nur", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VAFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Schlaf' ich fr\u00f6hlich ein,", "tokens": ["Schlaf'", "ich", "fr\u00f6h\u00b7lich", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ewig wird zu s\u00fc\u00dfer Labe", "tokens": ["E\u00b7wig", "wird", "zu", "s\u00fc\u00b7\u00dfer", "La\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seines Herzens Fluth mir seyn,", "tokens": ["Sei\u00b7nes", "Her\u00b7zens", "Fluth", "mir", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PPER", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die mit sanftem Zwingen", "tokens": ["Die", "mit", "sanf\u00b7tem", "Zwin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Alles wird erweichen und durchdringen.", "tokens": ["Al\u00b7les", "wird", "er\u00b7wei\u00b7chen", "und", "durch\u00b7drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "VVINF", "KON", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Wenn ich ihn nur habe,", "tokens": ["Wenn", "ich", "ihn", "nur", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VAFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Hab' ich auch die Welt;", "tokens": ["Hab'", "ich", "auch", "die", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Selig, wie ein Himmelsknabe,", "tokens": ["Se\u00b7lig", ",", "wie", "ein", "Him\u00b7mels\u00b7kna\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der der Jungfrau Schleyer h\u00e4lt.", "tokens": ["Der", "der", "Jung\u00b7frau", "Schle\u00b7yer", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Hingesenkt im Schauen", "tokens": ["Hin\u00b7ge\u00b7senkt", "im", "Schau\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Kann mir vor dem Irdischen nicht grauen.", "tokens": ["Kann", "mir", "vor", "dem", "Ir\u00b7di\u00b7schen", "nicht", "grau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Wo ich ihn nur habe,", "tokens": ["Wo", "ich", "ihn", "nur", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "VAFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ist mein Vaterland;", "tokens": ["Ist", "mein", "Va\u00b7ter\u00b7land", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und es f\u00e4llt mir jede Gabe", "tokens": ["Und", "es", "f\u00e4llt", "mir", "je\u00b7de", "Ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein Erbtheil in die Hand;", "tokens": ["Wie", "ein", "E\u00b7rbtheil", "in", "die", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "L\u00e4ngst vermi\u00dfte Br\u00fcder", "tokens": ["L\u00e4ngst", "ver\u00b7mi\u00df\u00b7te", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Find' ich nun in seinen J\u00fcngern wieder.", "tokens": ["Find'", "ich", "nun", "in", "sei\u00b7nen", "J\u00fcn\u00b7gern", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}