{"textgrid.poem.36501": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "26. Das Gem\u00e4lde und der K\u00e4ufer", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der gro\u00dfe Mengs hat mich gemalt!", "tokens": ["Der", "gro\u00b7\u00dfe", "Mengs", "hat", "mich", "ge\u00b7malt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach ein Gem\u00e4lde zu dem K\u00e4ufer.", "tokens": ["Sprach", "ein", "Ge\u00b7m\u00e4l\u00b7de", "zu", "dem", "K\u00e4u\u00b7fer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der K\u00e4ufer, der geriet in Eifer;", "tokens": ["Der", "K\u00e4u\u00b7fer", ",", "der", "ge\u00b7riet", "in", "Ei\u00b7fer", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gem\u00e4lde, sprach er, nicht geprahlt!", "tokens": ["Ge\u00b7m\u00e4l\u00b7de", ",", "sprach", "er", ",", "nicht", "ge\u00b7prahlt", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du warst so schmutzig, da\u00df ja keiner", "tokens": ["Du", "warst", "so", "schmut\u00b7zig", ",", "da\u00df", "ja", "kei\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "ADV", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dich kaufen wollte; reiner", "tokens": ["Dich", "kau\u00b7fen", "woll\u00b7te", ";", "rei\u00b7ner"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "VVINF", "VMFIN", "$.", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nur etwas bist du jetzt; ganz rein,", "tokens": ["Nur", "et\u00b7was", "bist", "du", "jetzt", ";", "ganz", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "PPER", "ADV", "$.", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}