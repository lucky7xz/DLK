{"textgrid.poem.45251": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Den Deutschen", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem Bergesgipfel naht ihr der Kultur,", "tokens": ["Dem", "Ber\u00b7ges\u00b7gip\u00b7fel", "naht", "ihr", "der", "Kul\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von Feldern und Pfaden l\u00e4ngst keine Spur,", "tokens": ["Von", "Fel\u00b7dern", "und", "Pfa\u00b7den", "l\u00e4ngst", "kei\u00b7ne", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "PIAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Das Knieholz f\u00e4ngt bereits schon an,", "tokens": ["Das", "Knie\u00b7holz", "f\u00e4ngt", "be\u00b7reits", "schon", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kaum kurzes Gras auf eurer Bahn,", "tokens": ["Kaum", "kur\u00b7zes", "Gras", "auf", "eu\u00b7rer", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Steigt ihr noch weiter, wie ich seh,", "tokens": ["Steigt", "ihr", "noch", "wei\u00b7ter", ",", "wie", "ich", "seh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erreicht ihr bald den ewigen Schnee.", "tokens": ["Er\u00b7reicht", "ihr", "bald", "den", "e\u00b7wi\u00b7gen", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}