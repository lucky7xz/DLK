{"dta.poem.3271": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Morgenlied der Sch\u00e4fer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Die d\u00fcstre Nacht ist hin,", "tokens": ["Die", "d\u00fcst\u00b7re", "Nacht", "ist", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Sonne kehret wieder.", "tokens": ["Die", "Son\u00b7ne", "keh\u00b7ret", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ermuntre dich, mein Sinn!", "tokens": ["Er\u00b7munt\u00b7re", "dich", ",", "mein", "Sinn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und dichte Freudenlieder.", "tokens": ["Und", "dich\u00b7te", "Freu\u00b7den\u00b7lie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die ihr, wann Hirten flehn,", "tokens": ["Die", "ihr", ",", "wann", "Hir\u00b7ten", "flehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ein willig Ohr gew\u00e4hret,", "tokens": ["Ein", "wil\u00b7lig", "Ohr", "ge\u00b7w\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Jhr G\u00f6tter! last geschehn,", "tokens": ["Ihr", "G\u00f6t\u00b7ter", "!", "last", "ge\u00b7schehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Was itzt mein Mund begehret.", "tokens": ["Was", "itzt", "mein", "Mund", "be\u00b7ge\u00b7hret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Gebt mir ein weises Herz,", "tokens": ["Gebt", "mir", "ein", "wei\u00b7ses", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das allen Gram verfluche;", "tokens": ["Das", "al\u00b7len", "Gram", "ver\u00b7flu\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und mehr den Jugendscherz,", "tokens": ["Und", "mehr", "den", "Ju\u00b7gend\u00b7scherz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Als Gold und Sorgen suche.", "tokens": ["Als", "Gold", "und", "Sor\u00b7gen", "su\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es rufe nie die Nacht", "tokens": ["Es", "ru\u00b7fe", "nie", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Den g\u00fcldnen Tag zu Grabe,", "tokens": ["Den", "g\u00fcld\u00b7nen", "Tag", "zu", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bis ich beym Wein gelacht,", "tokens": ["Bis", "ich", "beym", "Wein", "ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Das ist, gelebet habe.", "tokens": ["Das", "ist", ",", "ge\u00b7le\u00b7bet", "ha\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sch\u00fctzt Amors frohes Reich,", "tokens": ["Sch\u00fctzt", "A\u00b7mors", "fro\u00b7hes", "Reich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sch\u00fctzt unsre frohen Reben,", "tokens": ["Sch\u00fctzt", "uns\u00b7re", "fro\u00b7hen", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df Lieb und Wein zugleich", "tokens": ["Da\u00df", "Lieb", "und", "Wein", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Stets iedes Herz beleben.", "tokens": ["Stets", "ie\u00b7des", "Herz", "be\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wird Wasserbad und List", "tokens": ["Wird", "Was\u00b7ser\u00b7bad", "und", "List"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ly\u00e4ens Gottheit schw\u00e4chen;", "tokens": ["Ly\u00b7\u00e4\u00b7ens", "Got\u00b7theit", "schw\u00e4\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wird st\u00fcndlich nicht gek\u00fcsst:", "tokens": ["Wird", "st\u00fcnd\u00b7lich", "nicht", "ge\u00b7k\u00fcsst", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "So wollet ihr es r\u00e4chen!", "tokens": ["So", "wol\u00b7let", "ihr", "es", "r\u00e4\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nie m\u00fcss' ein artig Kind", "tokens": ["Nie", "m\u00fcss'", "ein", "ar\u00b7tig", "Kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die wilde Strenge lieben!", "tokens": ["Die", "wil\u00b7de", "Stren\u00b7ge", "lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nur die nicht artig sind,", "tokens": ["Nur", "die", "nicht", "ar\u00b7tig", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PTKNEG", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "La\u00dft Grausamkeiten \u00fcben!", "tokens": ["La\u00dft", "Grau\u00b7sam\u00b7kei\u00b7ten", "\u00fc\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auch segnet nun den May,", "tokens": ["Auch", "seg\u00b7net", "nun", "den", "May", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der manche z\u00e4rtlich machte;", "tokens": ["Der", "man\u00b7che", "z\u00e4rt\u00b7lich", "mach\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df keine Sch\u00f6ne sey,", "tokens": ["Da\u00df", "kei\u00b7ne", "Sch\u00f6\u00b7ne", "sey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die nicht nach K\u00fcssen schmachte.", "tokens": ["Die", "nicht", "nach", "K\u00fcs\u00b7sen", "schmach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wenn mancher, den ihr wisst,", "tokens": ["Wenn", "man\u00b7cher", ",", "den", "ihr", "wisst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sich doch verl\u00e4ugnen k\u00f6nnte,", "tokens": ["Sich", "doch", "ver\u00b7l\u00e4ug\u00b7nen", "k\u00f6nn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df, was ihm unn\u00fctz ist,", "tokens": ["Da\u00df", ",", "was", "ihm", "un\u00b7n\u00fctz", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er seinem N\u00e4chsten g\u00f6nnte!", "tokens": ["Er", "sei\u00b7nem", "N\u00e4chs\u00b7ten", "g\u00f6nn\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was soll der schwache Mann", "tokens": ["Was", "soll", "der", "schwa\u00b7che", "Mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Beym jungen Weibchen keichen?", "tokens": ["Beym", "jun\u00b7gen", "Weib\u00b7chen", "kei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Was er nicht brauchen kann,", "tokens": ["Was", "er", "nicht", "brau\u00b7chen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Das la\u00df er meines gleichen.", "tokens": ["Das", "la\u00df", "er", "mei\u00b7nes", "glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "So m\u00fcsse meine Brust", "tokens": ["So", "m\u00fcs\u00b7se", "mei\u00b7ne", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein ieder Tag entz\u00fccken,", "tokens": ["Ein", "ie\u00b7der", "Tag", "ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und eine frische Lust", "tokens": ["Und", "ei\u00b7ne", "fri\u00b7sche", "Lust"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit ieder Nacht begl\u00fccken!", "tokens": ["Mit", "ie\u00b7der", "Nacht", "be\u00b7gl\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bey M\u00e4dchen und bey Wein,", "tokens": ["Bey", "M\u00e4d\u00b7chen", "und", "bey", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mit Bluhmen um die Haare,", "tokens": ["Mit", "Bluh\u00b7men", "um", "die", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Will ich euch dankbar seyn,", "tokens": ["Will", "ich", "euch", "dank\u00b7bar", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Jm Fr\u00fchling meiner Jahre.", "tokens": ["Jm", "Fr\u00fch\u00b7ling", "mei\u00b7ner", "Jah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}