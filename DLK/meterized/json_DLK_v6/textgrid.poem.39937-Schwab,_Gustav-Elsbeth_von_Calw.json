{"textgrid.poem.39937": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Elsbeth von Calw", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdu hast geliebet meinen Knecht,", "tokens": ["\u00bb", "du", "hast", "ge\u00b7lie\u00b7bet", "mei\u00b7nen", "Knecht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du hast gesch\u00e4ndet mein Geschlecht;", "tokens": ["Du", "hast", "ge\u00b7sch\u00e4n\u00b7det", "mein", "Ge\u00b7schlecht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verheimlicht ist die tiefe Wunde,", "tokens": ["Ver\u00b7heim\u00b7licht", "ist", "die", "tie\u00b7fe", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er schmachtet in des Kerkers Grunde!\u00ab", "tokens": ["Er", "schmach\u00b7tet", "in", "des", "Ker\u00b7kers", "Grun\u00b7de", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und Elsbeth spricht: \u00bbIst er ein Knecht,", "tokens": ["Und", "Els\u00b7be\u00b7th", "spricht", ":", "\u00bb", "Ist", "er", "ein", "Knecht", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$.", "$(", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "So ist sein Sinn doch hoch und echt!", "tokens": ["So", "ist", "sein", "Sinn", "doch", "hoch", "und", "echt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es focht sein Arm in Ritterschlachten,", "tokens": ["Es", "focht", "sein", "Arm", "in", "Rit\u00b7ter\u00b7schlach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O Vater! la\u00df ihn nicht verschmachten!\u00ab", "tokens": ["O", "Va\u00b7ter", "!", "la\u00df", "ihn", "nicht", "ver\u00b7schmach\u00b7ten", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "$.", "VVIMP", "PPER", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bber schl\u00e4ft mir bald den langen Schlaf,", "tokens": ["\u00bb", "er", "schl\u00e4ft", "mir", "bald", "den", "lan\u00b7gen", "Schlaf", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und darum eil' ich,\u00ab spricht der Graf,", "tokens": ["Und", "da\u00b7rum", "eil'", "ich", ",", "\u00ab", "spricht", "der", "Graf", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "$,", "$(", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbdich, eh' es ruchbar wird hier oben,", "tokens": ["\u00bb", "dich", ",", "eh'", "es", "ruch\u00b7bar", "wird", "hier", "o\u00b7ben", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem edeln Nachbar zu verloben.", "tokens": ["Dem", "e\u00b7deln", "Nach\u00b7bar", "zu", "ver\u00b7lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Den Schl\u00fcssel zu dem Kerkerloch,", "tokens": ["Den", "Schl\u00fcs\u00b7sel", "zu", "dem", "Ker\u00b7ker\u00b7loch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nimm selbst ihn, Elsbeth \u2013 nimm ihn doch!", "tokens": ["Nimm", "selbst", "ihn", ",", "Els\u00b7be\u00b7th", "\u2013", "nimm", "ihn", "doch", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "$,", "NE", "$(", "VVIMP", "PPER", "ADV", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ich kann ihn andern nicht vertrauen,", "tokens": ["Ich", "kann", "ihn", "an\u00b7dern", "nicht", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf keines Dieners Treue bauen.", "tokens": ["Auf", "kei\u00b7nes", "Die\u00b7ners", "Treu\u00b7e", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du aber schw\u00f6rst mir, schwache Maid,", "tokens": ["Du", "a\u00b7ber", "schw\u00f6rst", "mir", ",", "schwa\u00b7che", "Maid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du schw\u00f6rst bei deiner Seligkeit:", "tokens": ["Du", "schw\u00f6rst", "bei", "dei\u00b7ner", "Se\u00b7lig\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht g\u00f6nn' ihm Licht, nicht g\u00f6nn' ihm Labe,", "tokens": ["Nicht", "g\u00f6nn'", "ihm", "Licht", ",", "nicht", "g\u00f6nn'", "ihm", "La\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "NN", "$,", "PTKNEG", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht Flucht zu Ro\u00df, nicht Flucht am Stabe.\u00ab", "tokens": ["Nicht", "Flucht", "zu", "Ro\u00df", ",", "nicht", "Flucht", "am", "Sta\u00b7be", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "NN", "APPR", "NN", "$,", "PTKNEG", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Den Schl\u00fcssel fa\u00dft die Jungfrau bleich,", "tokens": ["Den", "Schl\u00fcs\u00b7sel", "fa\u00dft", "die", "Jung\u00b7frau", "bleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als fa\u00dfte sie das Himmelreich;", "tokens": ["Als", "fa\u00df\u00b7te", "sie", "das", "Him\u00b7mel\u00b7reich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Blick schwingt sich zur Kerkerpforte,", "tokens": ["Ihr", "Blick", "schwingt", "sich", "zur", "Ker\u00b7ker\u00b7pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie schw\u00f6rt im Taumel alle Worte.", "tokens": ["Sie", "schw\u00f6rt", "im", "Tau\u00b7mel", "al\u00b7le", "Wor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Graf getrost besteigt sein Ro\u00df,", "tokens": ["Der", "Graf", "ge\u00b7trost", "be\u00b7steigt", "sein", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sprengt mit den Knappen aus dem Schlo\u00df,", "tokens": ["Sprengt", "mit", "den", "Knap\u00b7pen", "aus", "dem", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Im Staub verwallen ihre Schritte,", "tokens": ["Im", "Staub", "ver\u00b7wal\u00b7len", "ih\u00b7re", "Schrit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Thal verhallen ihre Tritte.", "tokens": ["Im", "Thal", "ver\u00b7hal\u00b7len", "ih\u00b7re", "Trit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Da stand die Jungfrau ganz allein", "tokens": ["Da", "stand", "die", "Jung\u00b7frau", "ganz", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im lichten, goldnen Sonnenschein,", "tokens": ["Im", "lich\u00b7ten", ",", "gold\u00b7nen", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Himmel \u00f6ffnet seine Bl\u00e4ue,", "tokens": ["Der", "Him\u00b7mel", "\u00f6ff\u00b7net", "sei\u00b7ne", "Bl\u00e4u\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00f6lbt seinen Arm f\u00fcr Liebestreue.", "tokens": ["W\u00f6lbt", "sei\u00b7nen", "Arm", "f\u00fcr", "Lie\u00b7be\u00b7streu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Hinauf zum hohen Thurm geschwind!", "tokens": ["Hin\u00b7auf", "zum", "ho\u00b7hen", "Thurm", "ge\u00b7schwind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es fa\u00dft ihr Kleid ein frischer Wind,", "tokens": ["Es", "fa\u00dft", "ihr", "Kleid", "ein", "fri\u00b7scher", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er saus't, als wollt' er fl\u00fcsternd fragen:", "tokens": ["Er", "saus't", ",", "als", "wollt'", "er", "fl\u00fcs\u00b7ternd", "fra\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wann darf ich euch von hinnen tragen?", "tokens": ["Wann", "darf", "ich", "euch", "von", "hin\u00b7nen", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Und vor der Th\u00fcr' auf Berg und Thal,", "tokens": ["Und", "vor", "der", "Th\u00fcr'", "auf", "Berg", "und", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf eine Welt voll Sonnenstral,", "tokens": ["Auf", "ei\u00b7ne", "Welt", "voll", "Son\u00b7nen\u00b7stral", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf sichre Burgen, feste Mauern", "tokens": ["Auf", "sich\u00b7re", "Bur\u00b7gen", ",", "fes\u00b7te", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Blickt sie hinab mit Hoffnungsschauern.", "tokens": ["Blickt", "sie", "hin\u00b7ab", "mit", "Hoff\u00b7nungs\u00b7schau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Die Arme streckt sie sehnlich aus,", "tokens": ["Die", "Ar\u00b7me", "streckt", "sie", "sehn\u00b7lich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Arme senket sie mit Graus;", "tokens": ["Die", "Ar\u00b7me", "sen\u00b7ket", "sie", "mit", "Graus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So steht sie vor des Kerkers Thore,", "tokens": ["So", "steht", "sie", "vor", "des", "Ker\u00b7kers", "Tho\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und nieder hallt's zu krankem Ohre:", "tokens": ["Und", "nie\u00b7der", "hallt's", "zu", "kran\u00b7kem", "Oh\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "\u00bbo Gottfried, hei\u00dfgeliebter Mann,", "tokens": ["\u00bb", "o", "Gott\u00b7fried", ",", "hei\u00df\u00b7ge\u00b7lieb\u00b7ter", "Mann", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu dir hinab, hinab ich kann;", "tokens": ["Zu", "dir", "hin\u00b7ab", ",", "hin\u00b7ab", "ich", "kann", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$,", "VVFIN", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Schl\u00fcssel hab' ich, steige nieder,", "tokens": ["Den", "Schl\u00fcs\u00b7sel", "hab'", "ich", ",", "stei\u00b7ge", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch nicht mit dir zu kommen wieder.\u00ab", "tokens": ["Doch", "nicht", "mit", "dir", "zu", "kom\u00b7men", "wie\u00b7der", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PTKNEG", "APPR", "PPER", "PTKZU", "VVINF", "ADV", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "\u00bbhast du den Schl\u00fcssel, komm herab,", "tokens": ["\u00bb", "hast", "du", "den", "Schl\u00fcs\u00b7sel", ",", "komm", "her\u00b7ab", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "ART", "NN", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bring' Licht und Leben mir in's Grab,", "tokens": ["Bring'", "Licht", "und", "Le\u00b7ben", "mir", "in's", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Hunger nagt an meinem Schlunde,", "tokens": ["Der", "Hun\u00b7ger", "nagt", "an", "mei\u00b7nem", "Schlun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-