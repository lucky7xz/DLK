{"textgrid.poem.62343": {"metadata": {"author": {"name": "Gessner, Salomon", "birth": "N.A.", "death": "N.A."}, "title": "Daphne und Chloe", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geh M\u00e4dgen, ich folge dir; geh weiter voraus, sonst schlagen die Ranken mir ins Gesicht.", "tokens": ["Geh", "M\u00e4d\u00b7gen", ",", "ich", "fol\u00b7ge", "dir", ";", "geh", "wei\u00b7ter", "vo\u00b7raus", ",", "sonst", "schla\u00b7gen", "die", "Ran\u00b7ken", "mir", "ins", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN", "PPER", "$.", "VVFIN", "ADV", "PTKVZ", "$,", "ADV", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+-+-+-+-++--+-++-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Wenn jemand k\u00f6mmt, wenn jemand uns sieht!", "tokens": ["Wenn", "je\u00b7mand", "k\u00f6mmt", ",", "wenn", "je\u00b7mand", "uns", "sieht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Munter und neubelebt bin ich. Was fangen wir an, wollen wir ein Liedgen singen?", "tokens": ["Mun\u00b7ter", "und", "neu\u00b7be\u00b7lebt", "bin", "ich", ".", "Was", "fan\u00b7gen", "wir", "an", ",", "wol\u00b7len", "wir", "ein", "Lied\u00b7gen", "sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "VAFIN", "PPER", "$.", "PWS", "VVFIN", "PPER", "PTKVZ", "$,", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.invert"}}, "stanza.4": {"line.1": {"text": "Einf\u00e4ltiges Kind! Singen, da\u00df man uns vom Ufer h\u00f6rt.", "tokens": ["Ein\u00b7f\u00e4l\u00b7ti\u00b7ges", "Kind", "!", "Sin\u00b7gen", ",", "da\u00df", "man", "uns", "vom", "U\u00b7fer", "h\u00f6rt", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NN", "$,", "KOUS", "PIS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+++-+-+-+-+", "measure": "unknown.measure.octa.plus"}}, "stanza.5": {"line.1": {"text": "So wollen wir fl\u00fcstern. Weissest du was? Erz\u00e4hle mir ein Geschichtgen.", "tokens": ["So", "wol\u00b7len", "wir", "fl\u00fcs\u00b7tern", ".", "Weis\u00b7sest", "du", "was", "?", "Er\u00b7z\u00e4h\u00b7le", "mir", "ein", "Ge\u00b7schicht\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$.", "VVFIN", "PPER", "PIS", "$.", "NN", "PPER", "ART", "NN", "$."], "meter": "-+--+-+-+--+-++-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.6": {"line.1": {"text": "Ein Geschichtgen?", "tokens": ["Ein", "Ge\u00b7schicht\u00b7gen", "?"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Ja, ein geheimes artiges Geschichtgen; du erz\u00e4hlest mir zuerst, und dann erz\u00e4hl ich dir.", "tokens": ["Ja", ",", "ein", "ge\u00b7hei\u00b7mes", "ar\u00b7ti\u00b7ges", "Ge\u00b7schicht\u00b7gen", ";", "du", "er\u00b7z\u00e4h\u00b7lest", "mir", "zu\u00b7erst", ",", "und", "dann", "er\u00b7z\u00e4hl", "ich", "dir", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "ADJA", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "ADV", "$,", "KON", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.8": {"line.1": {"text": "Ich wei\u00df wol eins, artig genug, aber \u2013 \u2013 \u2013", "tokens": ["Ich", "wei\u00df", "wol", "eins", ",", "ar\u00b7tig", "ge\u00b7nug", ",", "a\u00b7ber", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "$,", "ADJD", "ADV", "$,", "ADV", "$(", "$(", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Verschwiegen bin ich, wie diese Geb\u00fcsche.", "tokens": ["Ver\u00b7schwie\u00b7gen", "bin", "ich", ",", "wie", "die\u00b7se", "Ge\u00b7b\u00fc\u00b7sche", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "$,", "PWAV", "PDAT", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Aus dem Geheimsten meines Busens erz\u00e4hl ich dir dann wieder.", "tokens": ["Aus", "dem", "Ge\u00b7heims\u00b7ten", "mei\u00b7nes", "Bu\u00b7sens", "er\u00b7z\u00e4hl", "ich", "dir", "dann", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "PPER", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}}, "stanza.11": {"line.1": {"text": "W\u00fcrd ich mein geheimstes Geschichtgen dir erz\u00e4hlen, liebt ich dich nicht wie mich?", "tokens": ["W\u00fcrd", "ich", "mein", "ge\u00b7heims\u00b7tes", "Ge\u00b7schicht\u00b7gen", "dir", "er\u00b7z\u00e4h\u00b7len", ",", "liebt", "ich", "dich", "nicht", "wie", "mich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "PPER", "VVINF", "$,", "VVFIN", "PPER", "PRF", "PTKNEG", "PWAV", "PPER", "$."], "meter": "+-+-+--+-+-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}}}}}