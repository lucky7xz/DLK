{"dta.poem.19106": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "LxXXVIII.   Auf den grossen Grentz-Predi-  \n ger Joh. Christoph Schwedler.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Morgens von der Arbeit gehn, das ist eines Menschen\nWeise,", "tokens": ["Mor\u00b7gens", "von", "der", "Ar\u00b7beit", "gehn", ",", "das", "ist", "ei\u00b7nes", "Men\u00b7schen", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$,", "PDS", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Der sich, wie der ", "tokens": ["Der", "sich", ",", "wie", "der"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "PRF", "$,", "PWAV", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Jhm war Tag und Nacht bequem, GOtt zu Dienst, dem", "tokens": ["Jhm", "war", "Tag", "und", "Nacht", "be\u00b7quem", ",", "Gott", "zu", "Dienst", ",", "dem"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "ADJD", "$,", "NN", "APPR", "NN", "$,", "PRELS"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "K\u00fcrtzlich ist er eingeschlaffen, und liegt noch die Stunde still.", "tokens": ["K\u00fcrtz\u00b7lich", "ist", "er", "ein\u00b7ge\u00b7schlaf\u00b7fen", ",", "und", "liegt", "noch", "die", "Stun\u00b7de", "still", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "VVPP", "$,", "KON", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+--+--+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Ohne Zweiffel ist der Knecht unter seiner Last erlegen,", "tokens": ["Oh\u00b7ne", "Zweif\u00b7fel", "ist", "der", "Knecht", "un\u00b7ter", "sei\u00b7ner", "Last", "er\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Und der Schlaf wird lange w\u00e4hren, weil er eben viel gewacht.", "tokens": ["Und", "der", "Schlaf", "wird", "lan\u00b7ge", "w\u00e4h\u00b7ren", ",", "weil", "er", "e\u00b7ben", "viel", "ge\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "VVINF", "$,", "KOUS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Schlaf, du ", "tokens": ["Schlaf", ",", "du"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Deine Ruhe-Stunde schl\u00e4get. Schlaf! du hast genug ge-", "tokens": ["Dei\u00b7ne", "Ru\u00b7he\u00b7Stun\u00b7de", "schl\u00e4\u00b7get", ".", "Schlaf", "!", "du", "hast", "ge\u00b7nug", "ge"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "NN", "$.", "PPER", "VAFIN", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.3": {"line.1": {"text": "Wie denn? Hat sich Schwedlers Haupt endlich in den\nSchlaf gefunden,", "tokens": ["Wie", "denn", "?", "Hat", "sich", "Schwed\u00b7lers", "Haupt", "end\u00b7lich", "in", "den", "Schlaf", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$.", "VAFIN", "PRF", "NN", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "----+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Sinckt sein ausgeh\u00e4rtet Leben auch einmal zur Ruhe hin?", "tokens": ["Sinckt", "sein", "aus\u00b7ge\u00b7h\u00e4r\u00b7tet", "Le\u00b7ben", "auch", "ein\u00b7mal", "zur", "Ru\u00b7he", "hin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "VVFIN", "NN", "ADV", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Freylich! so hat auch der Held JEsus Christus \u00fcberwunden,", "tokens": ["Frey\u00b7lich", "!", "so", "hat", "auch", "der", "Held", "Je\u00b7sus", "Chris\u00b7tus", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "VAFIN", "ADV", "ART", "NN", "NE", "NE", "VVPP", "$,"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.4": {"text": "Er, der Arbeits-Leute ", "tokens": ["Er", ",", "der", "Ar\u00b7beits\u00b7Leu\u00b7te"], "token_info": ["word", "punct", "word", "word"], "pos": ["PPER", "$,", "ART", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Treuer Zeuge! deinen Dienst werden alle Alter loben;", "tokens": ["Treu\u00b7er", "Zeu\u00b7ge", "!", "dei\u00b7nen", "Dienst", "wer\u00b7den", "al\u00b7le", "Al\u00b7ter", "lo\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPOSAT", "NN", "VAFIN", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Und so oft die wahre Kirche ihre gr\u00f6ste S\u00e4ulen nennt;", "tokens": ["Und", "so", "oft", "die", "wah\u00b7re", "Kir\u00b7che", "ih\u00b7re", "gr\u00f6s\u00b7te", "S\u00e4u\u00b7len", "nennt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "So gedencket sie zugleich der Beweisung deiner Proben,", "tokens": ["So", "ge\u00b7den\u00b7cket", "sie", "zu\u00b7gleich", "der", "Be\u00b7wei\u00b7sung", "dei\u00b7ner", "Pro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Die in diesen unsern Tagen wenig ihres gleichen kennt.", "tokens": ["Die", "in", "die\u00b7sen", "un\u00b7sern", "Ta\u00b7gen", "we\u00b7nig", "ih\u00b7res", "glei\u00b7chen", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PDAT", "ADJA", "NN", "PIS", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Wenn ein leichter Feder-Kiel Centner Worte fassen k\u00f6nte,", "tokens": ["Wenn", "ein", "leich\u00b7ter", "Fe\u00b7der\u00b7Kiel", "Cent\u00b7ner", "Wor\u00b7te", "fas\u00b7sen", "k\u00f6n\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "NE", "NN", "VVINF", "VMFIN", "$,"], "meter": "--+-+-++-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "O wie stimmt ich deinen Thaten, ", "tokens": ["O", "wie", "stimmt", "ich", "dei\u00b7nen", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deinem Sinn, der Lichter-loh in der Liebe Christi brennte,", "tokens": ["Dei\u00b7nem", "Sinn", ",", "der", "Lich\u00b7ter\u00b7loh", "in", "der", "Lie\u00b7be", "Chris\u00b7ti", "brenn\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Und uns zeigte: ", "tokens": ["Und", "uns", "zeig\u00b7te", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$."], "meter": "--+-", "measure": "anapaest.init"}}, "stanza.6": {"line.1": {"text": "Feuer-Flammen! die ihr nie angeschlagen, ohne Z\u00fcnden,", "tokens": ["Feu\u00b7er\u00b7Flam\u00b7men", "!", "die", "ihr", "nie", "an\u00b7ge\u00b7schla\u00b7gen", ",", "oh\u00b7ne", "Z\u00fcn\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "PRELS", "PPER", "ADV", "VVPP", "$,", "KOUI", "NN", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Donner, Worte, die ihr alles schmettert, was ihr nur er-\nreicht,", "tokens": ["Don\u00b7ner", ",", "Wor\u00b7te", ",", "die", "ihr", "al\u00b7les", "schmet\u00b7tert", ",", "was", "ihr", "nur", "er", "reicht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PRELS", "PPER", "PIS", "VVFIN", "$,", "PWS", "PPER", "ADV", "TRUNC", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wie? versincket euer ", "tokens": ["Wie", "?", "ver\u00b7sin\u00b7cket", "eu\u00b7er"], "token_info": ["word", "punct", "word", "word"], "pos": ["PWAV", "$.", "VVFIN", "PPOSAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Liegt der tapfre Helden-Muth, welcher unsre Gr\u00e4ntzen\nsch\u00fctzte?", "tokens": ["Liegt", "der", "tapf\u00b7re", "Hel\u00b7den\u00b7Muth", ",", "wel\u00b7cher", "uns\u00b7re", "Gr\u00e4nt\u00b7zen", "sch\u00fctz\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Ist das leise ", "tokens": ["Ist", "das", "lei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Ist das ", "tokens": ["Ist", "das"], "token_info": ["word", "word"], "pos": ["VAFIN", "PDS"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Will der ", "tokens": ["Will", "der"], "token_info": ["word", "word"], "pos": ["VMFIN", "ART"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.8": {"line.1": {"text": "Unerforschlicher Magnet! Wer soll deine Z\u00fcge deu-\nten?Joh. 12.\nF\u00fcrst der Legionen GOttes, Hertzog \u00fcber unsern Bund,", "tokens": ["Un\u00b7er\u00b7for\u00b7schli\u00b7cher", "Mag\u00b7net", "!", "Wer", "soll", "dei\u00b7ne", "Z\u00fc\u00b7ge", "deu", "ten", "?", "Joh", ".", "12.", "F\u00fcrst", "der", "Le\u00b7gi\u00b7o\u00b7nen", "Got\u00b7tes", ",", "Hert\u00b7zog", "\u00fc\u00b7ber", "un\u00b7sern", "Bund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "ordinal", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PWS", "VMFIN", "PPOSAT", "NN", "FM", "FM", "$.", "NE", "$.", "ADJA", "NN", "ART", "NN", "NN", "$,", "NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-+-+--+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.2": {"text": "Wer ergreift das ", "tokens": ["Wer", "er\u00b7greift", "das"], "token_info": ["word", "word", "word"], "pos": ["PWS", "VVFIN", "ART"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.9": {"line.1": {"text": "Seht ein muntres Helden-Ro\u00df! GOtt und die Natur sind", "tokens": ["Seht", "ein", "mun\u00b7tres", "Hel\u00b7den\u00b7Ro\u00df", "!", "Gott", "und", "die", "Na\u00b7tur", "sind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "NN", "KON", "ART", "NN", "VAFIN"], "meter": "--+-+-++-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Also trug und also k\u00e4mpfte ", "tokens": ["Al\u00b7so", "trug", "und", "al\u00b7so", "k\u00e4mpf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Jesu! grosser Seelen-Mann! Deine Kirche liegt im\nStaube", "tokens": ["Je\u00b7su", "!", "gros\u00b7ser", "See\u00b7len\u00b7Mann", "!", "Dei\u00b7ne", "Kir\u00b7che", "liegt", "im", "Stau\u00b7be"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADJA", "NN", "$.", "PPOSAT", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Zu den F\u00fcssen hingestrecket, die f\u00fcr sie durchgraben sind;", "tokens": ["Zu", "den", "F\u00fcs\u00b7sen", "hin\u00b7ge\u00b7stre\u00b7cket", ",", "die", "f\u00fcr", "sie", "durch\u00b7gra\u00b7ben", "sind", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "PRELS", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Nimm den Demuths-vollen Ku\u00df, weil der unterth\u00e4n\u2019ge", "tokens": ["Nimm", "den", "De\u00b7muths\u00b7vol\u00b7len", "Ku\u00df", ",", "weil", "der", "un\u00b7tert\u00b7h\u00e4n'\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN", "$,", "KOUS", "ART", "ADJA"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Sich vor ", "tokens": ["Sich", "vor"], "token_info": ["word", "word"], "pos": ["PRF", "APPR"], "meter": "-+", "measure": "iambic.single"}}, "stanza.11": {"line.1": {"text": "Zeuch dahin, ", "tokens": ["Zeuch", "da\u00b7hin", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "PAV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Bete an den GOtt der G\u00f6tter, der dem Sohne zugericht\u2019t;", "tokens": ["Be\u00b7te", "an", "den", "Gott", "der", "G\u00f6t\u00b7ter", ",", "der", "dem", "Soh\u00b7ne", "zu\u00b7ge\u00b7richt't", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Sage: ", "tokens": ["Sa\u00b7ge", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Bracht ich an die Menschen-Seelen, aber Alle bring ich\nnicht.", "tokens": ["Bracht", "ich", "an", "die", "Men\u00b7schen\u00b7See\u00b7len", ",", "a\u00b7ber", "Al\u00b7le", "bring", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,", "ADV", "PIS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.12": {"line.1": {"text": "Knechte! die ihr hier und dar auf den Strassen G\u00e4ste ladet,", "tokens": ["Knech\u00b7te", "!", "die", "ihr", "hier", "und", "dar", "auf", "den", "Stras\u00b7sen", "G\u00e4s\u00b7te", "la\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PRELS", "PPER", "ADV", "KON", "PTKVZ", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.2": {"text": "H\u00f6rt ihrs? ", "tokens": ["H\u00f6rt", "ihrs", "?"], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Hat ihm nun der Feinde ", "tokens": ["Hat", "ihm", "nun", "der", "Fein\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.13": {"line.1": {"text": "Staupen-Schl\u00e4ge, Rad und Pfahl machen allzu viele Christen,", "tokens": ["Stau\u00b7pen\u00b7Schl\u00e4\u00b7ge", ",", "Rad", "und", "Pfahl", "ma\u00b7chen", "all\u00b7zu", "vie\u00b7le", "Chris\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVINF", "PTKA", "PIAT", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Darum l\u00e4\u00dfts der Erb-Feind bleiben, da\u00df er Christen mar-", "tokens": ["Da\u00b7rum", "l\u00e4\u00dfts", "der", "Er\u00b7bFeind", "blei\u00b7ben", ",", "da\u00df", "er", "Chris\u00b7ten", "ma\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN", "VVINF", "$,", "KOUS", "PPER", "ADJA", "TRUNC"], "meter": "--+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "Dient ihr noch der Menschen-Furcht, einer von den Fleisches-", 