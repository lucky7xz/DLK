{"textgrid.poem.36176": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Verehrte G\u00f6nner, eh vor euren Blicken", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verehrte G\u00f6nner, eh vor euren Blicken", "tokens": ["Ver\u00b7ehr\u00b7te", "G\u00f6n\u00b7ner", ",", "eh", "vor", "eu\u00b7ren", "Bli\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der B\u00fchne wechselnd Spiel sich heut' erregt,", "tokens": ["Der", "B\u00fch\u00b7ne", "wech\u00b7selnd", "Spiel", "sich", "heut'", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sey mir verg\u00f6nnt, zwar sch\u00fcchtern, auszudr\u00fccken,", "tokens": ["Sey", "mir", "ver\u00b7g\u00f6nnt", ",", "zwar", "sch\u00fcch\u00b7tern", ",", "aus\u00b7zu\u00b7dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "ADV", "VVINF", "$,", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was m\u00e4chtig unser aller Brust bewegt!", "tokens": ["Was", "m\u00e4ch\u00b7tig", "un\u00b7ser", "al\u00b7ler", "Brust", "be\u00b7wegt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPOSAT", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Geziemt es doch der B\u00fchne, sich zu schm\u00fccken", "tokens": ["Ge\u00b7ziemt", "es", "doch", "der", "B\u00fch\u00b7ne", ",", "sich", "zu", "schm\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit jeder Farbe, die das Leben tr\u00e4gt!", "tokens": ["Mit", "je\u00b7der", "Far\u00b7be", ",", "die", "das", "Le\u00b7ben", "tr\u00e4gt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mag Gram euch d\u00fcstern, Frohes euch erheitern;", "tokens": ["Mag", "Gram", "euch", "d\u00fcs\u00b7tern", ",", "Fro\u00b7hes", "euch", "er\u00b7hei\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PPER", "VVFIN", "$,", "ADJA", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Kunst geb\u00fchrt, die Leidenschaft zu l\u00e4utern!", "tokens": ["Der", "Kunst", "ge\u00b7b\u00fchrt", ",", "die", "Lei\u00b7den\u00b7schaft", "zu", "l\u00e4u\u00b7tern", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das gro\u00dfe R\u00e4thsel l\u00f6sten diese Tage;", "tokens": ["Das", "gro\u00b7\u00dfe", "R\u00e4th\u00b7sel", "l\u00f6s\u00b7ten", "die\u00b7se", "Ta\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gesprochen hat der Gott ein mahnend Wort;", "tokens": ["Ge\u00b7spro\u00b7chen", "hat", "der", "Gott", "ein", "mah\u00b7nend", "Wort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schicksalentscheidend klang die V\u00f6lkerwaage,", "tokens": ["Schick\u00b7sa\u00b7lent\u00b7schei\u00b7dend", "klang", "die", "V\u00f6l\u00b7ker\u00b7waa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und ihrem Klang' erbebten S\u00fcd und Nord.", "tokens": ["Und", "ih\u00b7rem", "Klang'", "er\u00b7beb\u00b7ten", "S\u00fcd", "und", "Nord", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie Windsbraut w\u00e4lzt die ungeheure Sage", "tokens": ["Wie", "Winds\u00b7braut", "w\u00e4lzt", "die", "un\u00b7ge\u00b7heu\u00b7re", "Sa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von Strom zu Strom, von Strand zu Strand sich fort.", "tokens": ["Von", "Strom", "zu", "Strom", ",", "von", "Strand", "zu", "Strand", "sich", "fort", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,", "APPR", "NN", "APPR", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wohin ihr lauscht, h\u00f6rt ihr die Losung schallen:", "tokens": ["Wo\u00b7hin", "ihr", "lauscht", ",", "h\u00f6rt", "ihr", "die", "Lo\u00b7sung", "schal\u00b7len", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Franken stolze Hauptstadt ist gefallen!", "tokens": ["Der", "Fran\u00b7ken", "stol\u00b7ze", "Haupt\u00b7stadt", "ist", "ge\u00b7fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie, deren Ring den Raub des Erdballs fa\u00dfte,", "tokens": ["Sie", ",", "de\u00b7ren", "Ring", "den", "Raub", "des", "Erd\u00b7balls", "fa\u00df\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie, die Europens K\u00f6niginn sich pries;", "tokens": ["Sie", ",", "die", "Eu\u00b7ro\u00b7pens", "K\u00f6\u00b7ni\u00b7ginn", "sich", "pries", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die in der V\u00f6lker Blut und Thr\u00e4nen prass'te,", "tokens": ["Die", "in", "der", "V\u00f6l\u00b7ker", "Blut", "und", "Thr\u00e4\u00b7nen", "prass'\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und jede Mahnung h\u00f6hnend von sich wies;", "tokens": ["Und", "je\u00b7de", "Mah\u00b7nung", "h\u00f6h\u00b7nend", "von", "sich", "wies", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die aus der H\u00fctte l\u00e4ngst und dem Pallaste", "tokens": ["Die", "aus", "der", "H\u00fct\u00b7te", "l\u00e4ngst", "und", "dem", "Pal\u00b7las\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den Glauben sammt der Liebe von sich stie\u00df;", "tokens": ["Den", "Glau\u00b7ben", "sammt", "der", "Lie\u00b7be", "von", "sich", "stie\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sie hat des herben Kelchs nun auch getrunken,", "tokens": ["Sie", "hat", "des", "her\u00b7ben", "Kelchs", "nun", "auch", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ihr prangend Haupt ist in den Staub gesunken.", "tokens": ["Ihr", "pran\u00b7gend", "Haupt", "ist", "in", "den", "Staub", "ge\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein H\u00f6herer hat \u00fcber sie gesprochen,", "tokens": ["Ein", "H\u00f6\u00b7he\u00b7rer", "hat", "\u00fc\u00b7ber", "sie", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein St\u00e4rkerer hat ihr den Raub geraubt.", "tokens": ["Ein", "St\u00e4r\u00b7ke\u00b7rer", "hat", "ihr", "den", "Raub", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Auch \u00fcber sie ward nun der Stab gebrochen,", "tokens": ["Auch", "\u00fc\u00b7ber", "sie", "ward", "nun", "der", "Stab", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die unantastbar sich bis jetzt geglaubt.", "tokens": ["Die", "un\u00b7an\u00b7tast\u00b7bar", "sich", "bis", "jetzt", "ge\u00b7glaubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wien und Berlin und Moskau sind gerochen;", "tokens": ["Wi\u00b7en", "und", "Ber\u00b7lin", "und", "Mos\u00b7kau", "sind", "ge\u00b7ro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "KON", "NE", "VAFIN", "VVPP", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Die alte Roma hebt ihr w\u00fcrdig Haupt.", "tokens": ["Die", "al\u00b7te", "Ro\u00b7ma", "hebt", "ihr", "w\u00fcr\u00b7dig", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die j\u00fcngst noch Herrinn hie\u00df, wird Magd gescholten,", "tokens": ["Die", "j\u00fcngst", "noch", "Her\u00b7rinn", "hie\u00df", ",", "wird", "Magd", "ge\u00b7schol\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "NN", "VVFIN", "$,", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und wie sie andern that, wird ihr vergolten!", "tokens": ["Und", "wie", "sie", "an\u00b7dern", "that", ",", "wird", "ihr", "ver\u00b7gol\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PIS", "VVFIN", "$,", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Vergeltung, traun! harrt jenseit jener Sterne;", "tokens": ["Ver\u00b7gel\u00b7tung", ",", "traun", "!", "harrt", "jen\u00b7seit", "je\u00b7ner", "Ster\u00b7ne", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVINF", "$.", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch auch hienieden wird vergolten schon!", "tokens": ["Doch", "auch", "hien\u00b7ie\u00b7den", "wird", "ver\u00b7gol\u00b7ten", "schon", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VAFIN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es greife nicht zu l\u00fcstern in die Ferne,", "tokens": ["Es", "grei\u00b7fe", "nicht", "zu", "l\u00fcs\u00b7tern", "in", "die", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es baue nicht in Wolken seinen Thron,", "tokens": ["Es", "bau\u00b7e", "nicht", "in", "Wol\u00b7ken", "sei\u00b7nen", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wen eines Weibes Schoo\u00df gebar! Es lerne", "tokens": ["Wen", "ei\u00b7nes", "Wei\u00b7bes", "Schoo\u00df", "ge\u00b7bar", "!", "Es", "ler\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "ART", "ADJA", "NN", "ADJD", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die h\u00f6hern M\u00e4chte scheu'n der Erde Sohn!", "tokens": ["Die", "h\u00f6\u00b7hern", "M\u00e4ch\u00b7te", "scheu'n", "der", "Er\u00b7de", "Sohn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nicht Sterblichen ziemt schrankenloses Schalten;", "tokens": ["Nicht", "Sterb\u00b7li\u00b7chen", "ziemt", "schran\u00b7ken\u00b7lo\u00b7ses", "Schal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Es ziemt allein den himmlischen Gewalten.", "tokens": ["Es", "ziemt", "al\u00b7lein", "den", "himm\u00b7li\u00b7schen", "Ge\u00b7wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Das wars, worauf die Ueberwinder bau'ten,", "tokens": ["Das", "wars", ",", "wo\u00b7rauf", "die", "Ue\u00b7ber\u00b7win\u00b7der", "bau'\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und was der Ueberwund'ne frech verlacht.", "tokens": ["Und", "was", "der", "Ue\u00b7ber\u00b7wun\u00b7d'\u00b7ne", "frech", "ver\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nicht wars die Zahl, d'rauf unsre Tapfern trau'ten,", "tokens": ["Nicht", "wars", "die", "Zahl", ",", "d'\u00b7rauf", "uns\u00b7re", "Tap\u00b7fern", "trau'\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "ART", "NN", "$,", "PAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Nicht ihrer Ross' und Wagen Uebermacht;", "tokens": ["Nicht", "ih\u00b7rer", "Ross'", "und", "Wa\u00b7gen", "Ue\u00b7ber\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es war der Frommen Hort, auf den sie schau'ten,", "tokens": ["Es", "war", "der", "From\u00b7men", "Hort", ",", "auf", "den", "sie", "schau'\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NE", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und der durch sie das gro\u00dfe Werk vollbracht.", "tokens": ["Und", "der", "durch", "sie", "das", "gro\u00b7\u00dfe", "Werk", "voll\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.pent