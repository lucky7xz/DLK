{"textgrid.poem.50057": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "13. Auf Marolphus und seine Mspta", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit alter Schrifft verschwendt Marolphus seine Zeit,", "tokens": ["Mit", "al\u00b7ter", "Schrifft", "ver\u00b7schwendt", "Ma\u00b7rol\u00b7phus", "sei\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "NE", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und denkt kein ", "tokens": ["Und", "denkt", "kein"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Ob gleich darinnen nur sich die ", "tokens": ["Ob", "gleich", "da\u00b7rin\u00b7nen", "nur", "sich", "die"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ADV", "PRF", "ART"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Durch manchen ", "tokens": ["Durch", "man\u00b7chen"], "token_info": ["word", "word"], "pos": ["APPR", "PIAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Indem der Staub zwar halb die Zeilen ausgethan,", "tokens": ["In\u00b7dem", "der", "Staub", "zwar", "halb", "die", "Zei\u00b7len", "aus\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Zeit die Bl\u00e4tter hat zerrieben,", "tokens": ["Die", "Zeit", "die", "Bl\u00e4t\u00b7ter", "hat", "zer\u00b7rie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Doch wo an derer stat die ", "tokens": ["Doch", "wo", "an", "de\u00b7rer", "stat", "die"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "PDS", "VVFIN", "ART"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Marolphus liesst es durch, und findt", "tokens": ["Ma\u00b7rol\u00b7phus", "liesst", "es", "durch", ",", "und", "findt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Er widmet ihm die beste Stunden,", "tokens": ["Er", "wid\u00b7met", "ihm", "die", "bes\u00b7te", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und denkt er sey ein ", "tokens": ["Und", "denkt", "er", "sey", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "VAFIN", "ART"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Dass er die ", "tokens": ["Dass", "er", "die"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Als h\u00e4tt' er erst zu Meyntz die ", "tokens": ["Als", "h\u00e4tt'", "er", "erst", "zu", "Meyntz", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "APPR", "NE", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}