{"textgrid.poem.33298": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "In das Stammbuch eines Reisenden", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mensch gleicht einer M\u00fcnze, Freund!", "tokens": ["Der", "Mensch", "gleicht", "ei\u00b7ner", "M\u00fcn\u00b7ze", ",", "Freund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist er von gutem Korn, und scheint", "tokens": ["Ist", "er", "von", "gu\u00b7tem", "Korn", ",", "und", "scheint"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir \u00e4cht sein Schlag, und ist noch scharf sein Rand,", "tokens": ["Dir", "\u00e4cht", "sein", "Schlag", ",", "und", "ist", "noch", "scharf", "sein", "Rand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PPOSAT", "NN", "$,", "KON", "VAFIN", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dann reich' ihm unverweilt die Hand:", "tokens": ["Dann", "reich'", "ihm", "un\u00b7ver\u00b7weilt", "die", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Griff aber schon die Welt zu sehr ihn ab,", "tokens": ["Griff", "a\u00b7ber", "schon", "die", "Welt", "zu", "sehr", "ihn", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ART", "NN", "APPR", "ADV", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So mindert das den Werth, den die Natur ihm gab;", "tokens": ["So", "min\u00b7dert", "das", "den", "Werth", ",", "den", "die", "Na\u00b7tur", "ihm", "gab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ART", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch hat ihn gar die Kunst beschnitten,", "tokens": ["Doch", "hat", "ihn", "gar", "die", "Kunst", "be\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dann, Freund, magst du vor ihm dich h\u00fcten.", "tokens": ["Dann", ",", "Freund", ",", "magst", "du", "vor", "ihm", "dich", "h\u00fc\u00b7ten", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VMFIN", "PPER", "APPR", "PPER", "PRF", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}