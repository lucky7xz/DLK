{"dta.poem.15836": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "41.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer nur das Kleinste thut, was recht ihm d\u00fcnkt und gut,", "tokens": ["Wer", "nur", "das", "Kleins\u00b7te", "thut", ",", "was", "recht", "ihm", "d\u00fcnkt", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "PPER", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird finden da\u00df ihm gut davon der Nachschmack thut.", "tokens": ["Wird", "fin\u00b7den", "da\u00df", "ihm", "gut", "da\u00b7von", "der", "Nach\u00b7schmack", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVINF", "KOUS", "PPER", "ADJD", "PAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du brauchst, was dir gelang, so hoch nicht anzuschlagen,", "tokens": ["Du", "brauchst", ",", "was", "dir", "ge\u00b7lang", ",", "so", "hoch", "nicht", "an\u00b7zu\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "ADV", "ADJD", "PTKNEG", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um doch ein freudiges Bewu\u00dftseyn mitzutragen.", "tokens": ["Um", "doch", "ein", "freu\u00b7di\u00b7ges", "Be\u00b7wu\u00df\u00b7tseyn", "mit\u00b7zu\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Vor dem, was droben ich soll thun, ist eitel Tand,", "tokens": ["Vor", "dem", ",", "was", "dro\u00b7ben", "ich", "soll", "thun", ",", "ist", "ei\u00b7tel", "Tand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADV", "PPER", "VMFIN", "VVINF", "$,", "VAFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ich hienieden that, doch ists ein Liebespfand,", "tokens": ["Was", "ich", "hien\u00b7ie\u00b7den", "that", ",", "doch", "ists", "ein", "Lie\u00b7be\u00b7spfand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$,", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das ich beim Abschied froh lass' in der Nachwelt Hand.", "tokens": ["Das", "ich", "beim", "Ab\u00b7schied", "froh", "lass'", "in", "der", "Nach\u00b7welt", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPRART", "NN", "ADJD", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}