{"dta.poem.8513": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "5.  \n  H\u00f6hlenfey .", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Siehst du dr&#252;ben, am hohlen Baum,", "tokens": ["Siehst", "du", "dr", "&#252;", "ben", ",", "am", "hoh\u00b7len", "Baum", ","], "token_info": ["word", "word", "word", "XML_entity", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$(", "VVPP", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "In\u2019s Gekl\u00fcfte die Schatten steigen,", "tokens": ["In's", "Ge\u00b7kl\u00fcf\u00b7te", "die", "Schat\u00b7ten", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ueber\u2019m Bord, ein blanker Saum,", "tokens": ["Ue\u00b7ber'm", "Bord", ",", "ein", "blan\u00b7ker", "Saum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Leises Quellengeriesel neigen?", "tokens": ["Lei\u00b7ses", "Quel\u00b7len\u00b7ge\u00b7rie\u00b7sel", "nei\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Das ist die Eiche von Bagneres,", "tokens": ["Das", "ist", "die", "Ei\u00b7che", "von", "Bag\u00b7ne\u00b7res", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das ist die H\u00f6hle Trou de fer,", "tokens": ["Das", "ist", "die", "H\u00f6h\u00b7le", "Trou", "de", "fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NE", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo sie tags in der Spalten Raum,", "tokens": ["Wo", "sie", "tags", "in", "der", "Spal\u00b7ten", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "N\u00e4chtlich wohnt in den surrenden Zweigen?", "tokens": ["N\u00e4cht\u00b7lich", "wohnt", "in", "den", "sur\u00b7ren\u00b7den", "Zwei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "O sie ist \u00fcberalt die Fey,", "tokens": ["O", "sie", "ist", "\u00fc\u00b7be\u00b7ralt", "die", "Fey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Laut Annalen, vor grauen Jahren,", "tokens": ["Laut", "An\u00b7na\u00b7len", ",", "vor", "grau\u00b7en", "Jah\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Zwei Jahrhunderten oder drei", "tokens": ["Zwei", "Jahr\u00b7hun\u00b7der\u00b7ten", "o\u00b7der", "drei"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "KON", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00dfte sie seltsam sich gebahren:", "tokens": ["Mu\u00df\u00b7te", "sie", "selt\u00b7sam", "sich", "ge\u00b7bah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PRF", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Bald als Eule mit Uhu,", "tokens": ["Bald", "als", "Eu\u00b7le", "mit", "U\u00b7hu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "APPR", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Bald als Katze und schwarze Kuh;", "tokens": ["Bald", "als", "Kat\u00b7ze", "und", "schwar\u00b7ze", "Kuh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "KON", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Auch als Wiesel mit seinem Schrei", "tokens": ["Auch", "als", "Wie\u00b7sel", "mit", "sei\u00b7nem", "Schrei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist sie \u00fcber die Kluft gefahren.", "tokens": ["Ist", "sie", "\u00fc\u00b7ber", "die", "Kluft", "ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Aber, wenn jetzt im Mondenschein", "tokens": ["A\u00b7ber", ",", "wenn", "jetzt", "im", "Mon\u00b7den\u00b7schein"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "ADV", "APPRART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Zarte Lichter den Grund bet\u00fcpfen,", "tokens": ["Zar\u00b7te", "Lich\u00b7ter", "den", "Grund", "be\u00b7t\u00fcp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Sieht mitunter man am Gestein", "tokens": ["Sieht", "mi\u00b7tun\u00b7ter", "man", "am", "Ge\u00b7stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PIS", "APPRART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Sie im schillernden Mantel h\u00fcpfen,", "tokens": ["Sie", "im", "schil\u00b7lern\u00b7den", "Man\u00b7tel", "h\u00fcp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "H\u00f6rt ihr Stimmchen, Ges\u00e4usel gleich;", "tokens": ["H\u00f6rt", "ihr", "Stimm\u00b7chen", ",", "Ge\u00b7s\u00e4u\u00b7sel", "gleich", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "NN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Aber nahst du, dann nickt der Zweig", "tokens": ["A\u00b7ber", "nahst", "du", ",", "dann", "nickt", "der", "Zweig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Und das Wasser wispert darein,", "tokens": ["Und", "das", "Was\u00b7ser", "wis\u00b7pert", "da\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PAV", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Und du siehst nur die Quelle schl\u00fcpfen.", "tokens": ["Und", "du", "siehst", "nur", "die", "Quel\u00b7le", "schl\u00fcp\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Reich an Gold ist der H\u00f6hle Grund,", "tokens": ["Reich", "an", "Gold", "ist", "der", "H\u00f6h\u00b7le", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "O wie Guinea und wie Bengalen!", "tokens": ["O", "wie", "Gui\u00b7nea", "und", "wie", "Ben\u00b7ga\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "NE", "KON", "PWAV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und man spricht vom bewachenden Hund,", "tokens": ["Und", "man", "spricht", "vom", "be\u00b7wa\u00b7chen\u00b7den", "Hund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Doch de\u00df melden nichts die Annalen.", "tokens": ["Doch", "de\u00df", "mel\u00b7den", "nichts", "die", "An\u00b7na\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "PIS", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Aber Mancher, der wundersam,", "tokens": ["A\u00b7ber", "Man\u00b7cher", ",", "der", "wun\u00b7der\u00b7sam", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Unbegreiflich zu Gelde kam,", "tokens": ["Un\u00b7be\u00b7greif\u00b7lich", "zu", "Gel\u00b7de", "kam", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Lie\u00df, so k\u00fcndet der Sage Mund,", "tokens": ["Lie\u00df", ",", "so", "k\u00fcn\u00b7det", "der", "Sa\u00b7ge", "Mund", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Es am Baum von Bagneres sich zahlen.", "tokens": ["Es", "am", "Baum", "von", "Bag\u00b7ne\u00b7res", "sich", "zah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "APPR", "NE", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Barg einen Beutel im Hohle breit,", "tokens": ["Barg", "ei\u00b7nen", "Beu\u00b7tel", "im", "Hoh\u00b7le", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Drin den neuen Liard, bed\u00e4chtig,", "tokens": ["Drin", "den", "neu\u00b7en", "Li\u00b7ard", ",", "be\u00b7d\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "ADJD", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Recht in der sengenden Mittagszeit,", "tokens": ["Recht", "in", "der", "sen\u00b7gen\u00b7den", "Mit\u00b7tags\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Die den Geistern wie mittern\u00e4chtig,", "tokens": ["Die", "den", "Geis\u00b7tern", "wie", "mit\u00b7ter\u00b7n\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "KOKOM", "ADJD", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Fand ihn abends mit Gold geschwellt, \u2014", "tokens": ["Fand", "ihn", "a\u00b7bends", "mit", "Gold", "ge\u00b7schwellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "VVPP", "$,", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "O kein Christ komme so zu Geld!", "tokens": ["O", "kein", "Christ", "kom\u00b7me", "so", "zu", "Geld", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Falsch war Feyengold jederzeit,", "tokens": ["Falsch", "war", "Fey\u00b7en\u00b7gold", "je\u00b7der\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "NN", "ADV", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Kurz das Leben, und Gott ist m\u00e4chtig.", "tokens": ["Kurz", "das", "Le\u00b7ben", ",", "und", "Gott", "ist", "m\u00e4ch\u00b7tig", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "KON", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Einmal nur, da\u00df mich de\u00df gedenkt,", "tokens": ["Ein\u00b7mal", "nur", ",", "da\u00df", "mich", "de\u00df", "ge\u00b7denkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "ART", "VVPP", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ist ein Mann an den Baum gegangen,", "tokens": ["Ist", "ein", "Mann", "an", "den", "Baum", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hat seinen Sack hineingesenkt,", "tokens": ["Hat", "sei\u00b7nen", "Sack", "hin\u00b7ein\u00b7ge\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gro\u00df eines K\u00f6nigs Schatz zu fangen;", "tokens": ["Gro\u00df", "ei\u00b7nes", "K\u00f6\u00b7nigs", "Schatz", "zu", "fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "\u2019s war ein Wucherer, war ein Filz,", "tokens": ["'s", "war", "ein", "Wu\u00b7che\u00b7rer", ",", "war", "ein", "Filz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ein von Thr\u00e4nen geschwellter Pilz,", "tokens": ["Ein", "von", "Thr\u00e4\u00b7nen", "ge\u00b7schwell\u00b7ter", "Pilz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Nun, er hat sich zuletzt gehenkt, \u2014", "tokens": ["Nun", ",", "er", "hat", "sich", "zu\u00b7letzt", "ge\u00b7henkt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "PPER", "VAFIN", "PRF", "ADV", "VVPP", "$,", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Besser h\u00e4tt\u2019 er schon da gehangen.", "tokens": ["Bes\u00b7ser", "h\u00e4tt'", "er", "schon", "da", "ge\u00b7han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Hielt die Lippen so fest geklemmt,", "tokens": ["Hielt", "die", "Lip\u00b7pen", "so", "fest", "ge\u00b7klemmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "\u2014 Denn Gefl\u00fcster nur, mu\u00dft du wissen, \u2014", "tokens": ["Denn", "Ge\u00b7fl\u00fcs\u00b7ter", "nur", ",", "mu\u00dft", "du", "wis\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "NN", "ADV", "$,", "VMFIN", "PPER", "VVINF", "$,", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das ist eben, was Alles hemmt,", "tokens": ["Das", "ist", "e\u00b7ben", ",", "was", "Al\u00b7les", "hemmt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Lieber h\u00e4tt\u2019 er die Zunge zerbissen; \u2014", "tokens": ["Lie\u00b7ber", "h\u00e4tt'", "er", "die", "Zun\u00b7ge", "zer\u00b7bis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ART", "NN", "VVPP", "$.", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Barfu\u00df kam er, auf schlechten Rath,", "tokens": ["Bar\u00b7fu\u00df", "kam", "er", ",", "auf", "schlech\u00b7ten", "Rath", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Und als da in die Scherb\u2019 er trat,", "tokens": ["Und", "als", "da", "in", "die", "Scherb'", "er", "trat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hat er sich nur an den Baum gestemmt", "tokens": ["Hat", "er", "sich", "nur", "an", "den", "Baum", "ge\u00b7stemmt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Und den Schart aus der Wunde gerissen.", "tokens": ["Und", "den", "Schart", "aus", "der", "Wun\u00b7de", "ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.8": {"line.1": {"text": "Doch als aus dem Gemoder scheu", "tokens": ["Doch", "als", "aus", "dem", "Ge\u00b7mo\u00b7der", "scheu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schl\u00fcpft \u2019ne Schlange ihm l\u00e4ngs den Haaren,", "tokens": ["Schl\u00fcpft", "'ne", "Schlan\u00b7ge", "ihm", "l\u00e4ngs", "den", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Da ist endlich ein kleiner Schrei,", "tokens": ["Da", "ist", "end\u00b7lich", "ein", "klei\u00b7ner", "Schrei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Nur ein winziger, ihm entfahren;", "tokens": ["Nur", "ein", "win\u00b7zi\u00b7ger", ",", "ihm", "ent\u00b7fah\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "$,", "PPER", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und am Abend verschwunden war", "tokens": ["Und", "am", "A\u00b7bend", "ver\u00b7schwun\u00b7den", "war"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVPP", "VAFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Gro\u00dfer Sack und neuer Liard.", "tokens": ["Gro\u00b7\u00dfer", "Sack", "und", "neu\u00b7er", "Li\u00b7ard", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "O verr\u00e4therisch ist die Fey!", "tokens": ["O", "ver\u00b7r\u00e4\u00b7the\u00b7risch", "ist", "die", "Fey", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Und es wachen der H\u00f6lle Schaaren.", "tokens": ["Und", "es", "wa\u00b7chen", "der", "H\u00f6l\u00b7le", "Schaa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}}}}