{"textgrid.poem.65903": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Das Gebet", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gewogner Hades, Spender alter Gnade,", "tokens": ["Ge\u00b7wog\u00b7ner", "Ha\u00b7des", ",", "Spen\u00b7der", "al\u00b7ter", "Gna\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Mensch, durch Not, zum Opfer tief bereit,", "tokens": ["Der", "Mensch", ",", "durch", "Not", ",", "zum", "Op\u00b7fer", "tief", "be\u00b7reit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "$,", "APPRART", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erfleht f\u00fcr eine G\u00f6ttin, wirr im Leid,", "tokens": ["Er\u00b7fleht", "f\u00fcr", "ei\u00b7ne", "G\u00f6t\u00b7tin", ",", "wirr", "im", "Leid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Tochter Wiederkunft auf schwerem Pfade.", "tokens": ["Der", "Toch\u00b7ter", "Wie\u00b7der\u00b7kunft", "auf", "schwe\u00b7rem", "Pfa\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gestatte Kore, von der Styx Gestade \u2013", "tokens": ["Ge\u00b7stat\u00b7te", "Ko\u00b7re", ",", "von", "der", "Styx", "Ge\u00b7sta\u00b7de", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erborgt aus feuriger Umwobenheit", "tokens": ["Er\u00b7borgt", "aus", "feu\u00b7ri\u00b7ger", "Um\u00b7wo\u00b7ben\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In deinem Heim \u2013 auf fromme Fr\u00fchlingszeit,", "tokens": ["In", "dei\u00b7nem", "Heim", "\u2013", "auf", "from\u00b7me", "Fr\u00fch\u00b7lings\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Erfreut zu sein durch Lerche und Zikade!", "tokens": ["Er\u00b7freut", "zu", "sein", "durch", "Ler\u00b7che", "und", "Zi\u00b7ka\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VAINF", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Geh\u00e4rmtem Weib, der Mutter unsrer Fluren,", "tokens": ["Ge\u00b7h\u00e4rm\u00b7tem", "Weib", ",", "der", "Mut\u00b7ter", "uns\u00b7rer", "Flu\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Begegne traut, bei W\u00e4rme kaum im Wind,", "tokens": ["Be\u00b7geg\u00b7ne", "traut", ",", "bei", "W\u00e4r\u00b7me", "kaum", "im", "Wind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "APPR", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Kindes Knisterschritt um Krokus-Spuren:", "tokens": ["Des", "Kin\u00b7des", "Knis\u00b7ter\u00b7schritt", "um", "Kro\u00b7kus\u00b7Spu\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Im Geiste doch erkenne dann geschwind", "tokens": ["Im", "Geis\u00b7te", "doch", "er\u00b7ken\u00b7ne", "dann", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Froh-Erschrockne ihrer Tochter Kommen:", "tokens": ["Die", "Froh\u00b7Er\u00b7schrock\u00b7ne", "ih\u00b7rer", "Toch\u00b7ter", "Kom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In guten Armen bleib sie aufgenommen!", "tokens": ["In", "gu\u00b7ten", "Ar\u00b7men", "bleib", "sie", "auf\u00b7ge\u00b7nom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}