{"textgrid.poem.41610": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wann, dunkle Sch\u00f6ne, einst du in der Gruft wirst rasten,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann, dunkle Sch\u00f6ne, einst du in der Gruft wirst rasten,", "tokens": ["Wann", ",", "dunk\u00b7le", "Sch\u00f6\u00b7ne", ",", "einst", "du", "in", "der", "Gruft", "wirst", "ras\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "ADJA", "NN", "$,", "ADV", "PPER", "APPR", "ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf der get\u00fcrmt und kalt ein schwarzer Marmor liegt,", "tokens": ["Auf", "der", "ge\u00b7t\u00fcrmt", "und", "kalt", "ein", "schwar\u00b7zer", "Mar\u00b7mor", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVPP", "KON", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wann du statt im Pf\u00fchl, in den du weich geschmiegt,", "tokens": ["Und", "wann", "du", "statt", "im", "Pf\u00fchl", ",", "in", "den", "du", "weich", "ge\u00b7schmiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "APPRART", "NN", "$,", "APPR", "ART", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In feuchter H\u00f6hle ruhst, im Grabe, im verha\u00dften,", "tokens": ["In", "feuch\u00b7ter", "H\u00f6h\u00b7le", "ruhst", ",", "im", "Gra\u00b7be", ",", "im", "ver\u00b7ha\u00df\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "APPRART", "NN", "$,", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wann die Bl\u00f6cke schwer auf banger Brust dir lasten", "tokens": ["Und", "wann", "die", "Bl\u00f6\u00b7cke", "schwer", "auf", "ban\u00b7ger", "Brust", "dir", "las\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ADJD", "APPR", "ADJA", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und auf den H\u00fcften dir, die lasse Anmut biegt,", "tokens": ["Und", "auf", "den", "H\u00fcf\u00b7ten", "dir", ",", "die", "las\u00b7se", "An\u00b7mut", "biegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wann l\u00e4nger nicht dein Herz verlangend pocht und fliegt,", "tokens": ["Wann", "l\u00e4n\u00b7ger", "nicht", "dein", "Herz", "ver\u00b7lan\u00b7gend", "pocht", "und", "fliegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PTKNEG", "PPOSAT", "NN", "VVPP", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die F\u00fc\u00dfe l\u00e4nger nicht nach Abenteuern hasten \u2013", "tokens": ["Die", "F\u00fc\u00b7\u00dfe", "l\u00e4n\u00b7ger", "nicht", "nach", "A\u00b7bent\u00b7eu\u00b7ern", "has\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKNEG", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dann wird das Grab, dem ich der Tr\u00e4ume Last vertraut,", "tokens": ["Dann", "wird", "das", "Grab", ",", "dem", "ich", "der", "Tr\u00e4u\u00b7me", "Last", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "NE", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u2013 Mich deucht, da\u00df nur das Grab des Dichters Sehnsucht ahne \u2013", "tokens": ["\u2013", "Mich", "deucht", ",", "da\u00df", "nur", "das", "Grab", "des", "Dich\u00b7ters", "Sehn\u00b7sucht", "ah\u00b7ne", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die langen N\u00e4chte, da kein Schlummer niedertaut,", "tokens": ["Die", "lan\u00b7gen", "N\u00e4ch\u00b7te", ",", "da", "kein", "Schlum\u00b7mer", "nie\u00b7der\u00b7taut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dir raunen: Was nun hilfts, gleichg\u00fcltge Courtisane,", "tokens": ["Dir", "rau\u00b7nen", ":", "Was", "nun", "hilfts", ",", "gleich\u00b7g\u00fclt\u00b7ge", "Cour\u00b7ti\u00b7sa\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$.", "PWS", "ADV", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df du, was Tote noch beweinen, nicht gewu\u00dft?", "tokens": ["Da\u00df", "du", ",", "was", "To\u00b7te", "noch", "be\u00b7wei\u00b7nen", ",", "nicht", "ge\u00b7wu\u00dft", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWS", "NN", "ADV", "VVINF", "$,", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und grimm wie Reue nagt der Wurm dir deine Brust.", "tokens": ["Und", "grimm", "wie", "Reu\u00b7e", "nagt", "der", "Wurm", "dir", "dei\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "NN", "VVFIN", "ART", "NN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}