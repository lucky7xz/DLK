{"textgrid.poem.66383": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer ist wohl auf der ganzen Welt", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer ist wohl auf der ganzen Welt", "tokens": ["Wer", "ist", "wohl", "auf", "der", "gan\u00b7zen", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vergn\u00fcgter als ein Bauer?", "tokens": ["Ver\u00b7gn\u00fcg\u00b7ter", "als", "ein", "Bau\u00b7er", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sein Haus, und Hof, und Ackerfeld", "tokens": ["Sein", "Haus", ",", "und", "Hof", ",", "und", "A\u00b7cker\u00b7feld"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KON", "NN", "$,", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Macht's Leben ihm nicht sauer,", "tokens": ["Macht's", "Le\u00b7ben", "ihm", "nicht", "sau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hat er ein Weibchen noch dazu:", "tokens": ["Hat", "er", "ein", "Weib\u00b7chen", "noch", "da\u00b7zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O Bauer, wie vergn\u00fcgt bist du!", "tokens": ["O", "Bau\u00b7er", ",", "wie", "ver\u00b7gn\u00fcgt", "bist", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWAV", "VVPP", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich hab' ein M\u00e4del \u2013 Dudeldum!", "tokens": ["Ich", "hab'", "ein", "M\u00e4\u00b7del", "\u2013", "Du\u00b7del\u00b7dum", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Gott, so zuckers\u00fc\u00dfe.", "tokens": ["O", "Gott", ",", "so", "zu\u00b7cker\u00b7s\u00fc\u00b7\u00dfe", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Dorf, und Stadt, und weit herum,", "tokens": ["Im", "Dorf", ",", "und", "Stadt", ",", "und", "weit", "he\u00b7rum", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "NN", "$,", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gibt's nichts wie meine Lise.", "tokens": ["Gibt's", "nichts", "wie", "mei\u00b7ne", "Li\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So jung und sch\u00f6n, so roth und braun,", "tokens": ["So", "jung", "und", "sch\u00f6n", ",", "so", "roth", "und", "braun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und immer von so guter Laun'.", "tokens": ["Und", "im\u00b7mer", "von", "so", "gu\u00b7ter", "Laun'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mein' Lisel ist mir herzlich gut,", "tokens": ["Mein'", "Li\u00b7sel", "ist", "mir", "herz\u00b7lich", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ich ihr gleicherweise.", "tokens": ["Und", "ich", "ihr", "glei\u00b7cher\u00b7wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie schenkt mir B\u00e4nder auf den Hut,", "tokens": ["Sie", "schenkt", "mir", "B\u00e4n\u00b7der", "auf", "den", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ich \u2013 ich schenk' ihr Str\u00e4u\u00dfe.", "tokens": ["Und", "ich", "\u2013", "ich", "schenk'", "ihr", "Str\u00e4u\u00b7\u00dfe", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nun Dudelsack, so tummle dich!", "tokens": ["Nun", "Du\u00b7del\u00b7sack", ",", "so", "tumm\u00b7le", "dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kein Mensch ist so vergn\u00fcgt wie ich.", "tokens": ["Kein", "Mensch", "ist", "so", "ver\u00b7gn\u00fcgt", "wie", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADJD", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}