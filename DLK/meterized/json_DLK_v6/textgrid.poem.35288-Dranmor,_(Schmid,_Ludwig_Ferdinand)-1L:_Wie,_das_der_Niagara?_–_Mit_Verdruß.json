{"textgrid.poem.35288": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie, das der Niagara? \u2013 Mit Verdru\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie, das der Niagara? \u2013 Mit Verdru\u00df", "tokens": ["Wie", ",", "das", "der", "Nia\u00b7ga\u00b7ra", "?", "\u2013", "Mit", "Ver\u00b7dru\u00df"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PWAV", "$,", "PRELS", "ART", "NE", "$.", "$(", "APPR", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Rief ich's hinunter von der Eisenbr\u00fccke. \u2013", "tokens": ["Rief", "ich's", "hin\u00b7un\u00b7ter", "von", "der", "Ei\u00b7sen\u00b7br\u00fc\u00b7cke", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dort in der Ferne der gespaltne Flu\u00df,", "tokens": ["Dort", "in", "der", "Fer\u00b7ne", "der", "ge\u00b7spalt\u00b7ne", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Thalschlucht hier, die kleinen Felsenst\u00fccke?", "tokens": ["Die", "Thal\u00b7schlucht", "hier", ",", "die", "klei\u00b7nen", "Fel\u00b7sen\u00b7st\u00fc\u00b7cke", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein Traum, das war ein ew'ger Wolkenbruch,", "tokens": ["Mein", "Traum", ",", "das", "war", "ein", "ew'\u00b7ger", "Wol\u00b7ken\u00b7bruch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das waren Str\u00f6me, die vom Himmel brausen.", "tokens": ["Das", "wa\u00b7ren", "Str\u00f6\u00b7me", ",", "die", "vom", "Him\u00b7mel", "brau\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich wollte wie durch einen Zauberspruch", "tokens": ["Ich", "woll\u00b7te", "wie", "durch", "ei\u00b7nen", "Zau\u00b7ber\u00b7spruch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "KOKOM", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hineinversetzt sein in der S\u00fcndflut Grausen.", "tokens": ["Hin\u00b7ein\u00b7ver\u00b7setzt", "sein", "in", "der", "S\u00fcnd\u00b7flut", "Grau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O Thorheit, was die Phantasie erschuf!", "tokens": ["O", "Thor\u00b7heit", ",", "was", "die", "Phan\u00b7ta\u00b7sie", "er\u00b7schuf", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das bange Herz bet\u00e4ubt von tausend Wettern,", "tokens": ["Das", "ban\u00b7ge", "Herz", "be\u00b7t\u00e4ubt", "von", "tau\u00b7send", "Wet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So wollt' ich, fliehend vor der H\u00f6lle Ruf,", "tokens": ["So", "wollt'", "ich", ",", "flie\u00b7hend", "vor", "der", "H\u00f6l\u00b7le", "Ruf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit Indianern \u00fcber Felsen klettern", "tokens": ["Mit", "In\u00b7di\u00b7a\u00b7nern", "\u00fc\u00b7ber", "Fel\u00b7sen", "klet\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und pl\u00f6tzlich vor dem Ungeheuern stehn,", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "vor", "dem", "Un\u00b7ge\u00b7heu\u00b7ern", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und dann \u2013 aus golddurchwirkten Wasserschleiern", "tokens": ["Und", "dann", "\u2013", "aus", "gold\u00b7durch\u00b7wirk\u00b7ten", "Was\u00b7ser\u00b7schlei\u00b7ern"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$(", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jehovahs Zeichen glorreich blitzen sehn", "tokens": ["Je\u00b7ho\u00b7vahs", "Zei\u00b7chen", "glor\u00b7reich", "blit\u00b7zen", "sehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADJD", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und zitternd meine Morgenandacht feiern.", "tokens": ["Und", "zit\u00b7ternd", "mei\u00b7ne", "Mor\u00b7gen\u00b7an\u00b7dacht", "fei\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ach, was ich h\u00f6rte, war ein schwacher Schall!", "tokens": ["Ach", ",", "was", "ich", "h\u00f6r\u00b7te", ",", "war", "ein", "schwa\u00b7cher", "Schall", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und blickt' ich auf die beiden Katarakte,", "tokens": ["Und", "blickt'", "ich", "auf", "die", "bei\u00b7den", "Ka\u00b7ta\u00b7rak\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und lauscht' ich ihrem majest\u00e4t'schen Fall,", "tokens": ["Und", "lauscht'", "ich", "ih\u00b7rem", "ma\u00b7je\u00b7st\u00e4t'\u00b7schen", "Fall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich f\u00fchlte nichts, was mich gewaltsam packte.", "tokens": ["Ich", "f\u00fchl\u00b7te", "nichts", ",", "was", "mich", "ge\u00b7walt\u00b7sam", "pack\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und doch \u2013 wie Selbsterkenntnis langsam nur", "tokens": ["Und", "doch", "\u2013", "wie", "Selbs\u00b7ter\u00b7kennt\u00b7nis", "lang\u00b7sam", "nur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$(", "KOKOM", "NE", "ADJD", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die eitle Menschenseele \u00fcberflutet,", "tokens": ["Die", "eit\u00b7le", "Men\u00b7schen\u00b7see\u00b7le", "\u00fc\u00b7berf\u00b7lu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Besiegte mich die Wahrheit der Natur", "tokens": ["Be\u00b7sieg\u00b7te", "mich", "die", "Wahr\u00b7heit", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und gab mir alles, was ich nicht vermutet.", "tokens": ["Und", "gab", "mir", "al\u00b7les", ",", "was", "ich", "nicht", "ver\u00b7mu\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "$,", "PWS", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}