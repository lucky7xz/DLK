{"dta.poem.237": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Sie beklaget den Verlust ihres  Amanten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "So mu\u00df sich nun mein Stern in einem Unstern kehren/", "tokens": ["So", "mu\u00df", "sich", "nun", "mein", "Stern", "in", "ei\u00b7nem", "Uns\u00b7tern", "keh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "ADV", "PPOSAT", "NN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein vormahls s\u00fcsses Licht wil ein ", "tokens": ["Sein", "vor\u00b7mahls", "s\u00fcs\u00b7ses", "Licht", "wil", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADV", "ADJA", "NN", "VMFIN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Himmel kanst du so ein treues Hertz beschwehren", "tokens": ["O", "Him\u00b7mel", "kanst", "du", "so", "ein", "treu\u00b7es", "Hertz", "be\u00b7schweh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit so viel ungemach und ", "tokens": ["Mit", "so", "viel", "un\u00b7ge\u00b7mach", "und"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADV", "ADJD", "KON"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Ach soll ich meinen Stern hinf\u00fchro nicht mehr sehn?", "tokens": ["Ach", "soll", "ich", "mei\u00b7nen", "Stern", "hin\u00b7f\u00fch\u00b7ro", "nicht", "mehr", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VMFIN", "PPER", "PPOSAT", "NN", "ADV", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So mu\u00df mein Freuden-Licht in Thr\u00e4nen untergehn.", "tokens": ["So", "mu\u00df", "mein", "Freu\u00b7den\u00b7Licht", "in", "Thr\u00e4\u00b7nen", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was bringt es dir vor Lust grausahmestes Verh\u00e4ngni\u00df?", "tokens": ["Was", "bringt", "es", "dir", "vor", "Lust", "grau\u00b7sah\u00b7mes\u00b7tes", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da\u00df du ein schwaches Hertz f\u00fchrst auf die Marter-Bahn/", "tokens": ["Da\u00df", "du", "ein", "schwa\u00b7ches", "Hertz", "f\u00fchrst", "auf", "die", "Mar\u00b7ter\u00b7Bahn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein sonst vergn\u00fcgter Geist h\u00e4lt nun sein Leich-Beg\u00e4ngni\u00df/", "tokens": ["Mein", "sonst", "ver\u00b7gn\u00fcg\u00b7ter", "Geist", "h\u00e4lt", "nun", "sein", "Leich\u00b7Be\u00b7g\u00e4ng\u00b7ni\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADJA", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und leget halb entseelt die Trauer-Kleider an.", "tokens": ["Und", "le\u00b7get", "halb", "ent\u00b7seelt", "die", "Trau\u00b7e\u00b7rKlei\u00b7der", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und was nur noch allein erquicket meine Brust/", "tokens": ["Und", "was", "nur", "noch", "al\u00b7lein", "er\u00b7quic\u00b7ket", "mei\u00b7ne", "Brust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADV", "ADV", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ist die Erinnerung der l\u00e4ngst verflo\u00dfnen Lust.", "tokens": ["Ist", "die", "E\u00b7rin\u00b7ne\u00b7rung", "der", "l\u00e4ngst", "ver\u00b7flo\u00df\u00b7nen", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Jedoch verzage nicht/ treu doch verfolgte Seele/", "tokens": ["Je\u00b7doch", "ver\u00b7za\u00b7ge", "nicht", "/", "treu", "doch", "ver\u00b7folg\u00b7te", "See\u00b7le", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "$(", "ADJD", "ADV", "ADJA", "NN", "$("], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Die Zeit ver\u00e4ndert sich in einem Augenblick/", "tokens": ["Die", "Zeit", "ver\u00b7\u00e4n\u00b7dert", "sich", "in", "ei\u00b7nem", "Au\u00b7gen\u00b7blick", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer jetzo ist versenckt in einer Trauer-H\u00f6le/", "tokens": ["Wer", "jet\u00b7zo", "ist", "ver\u00b7senckt", "in", "ei\u00b7ner", "Trau\u00b7e\u00b7rH\u00f6le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hat/ eh ers wohl vermeynt/ das allerh\u00f6chste Gl\u00fcck/", "tokens": ["Hat", "/", "eh", "ers", "wohl", "ver\u00b7meynt", "/", "das", "al\u00b7ler\u00b7h\u00f6chs\u00b7te", "Gl\u00fcck", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "KOUS", "PIS", "ADV", "VVPP", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vielleicht kan auch dein Stern/ der jetzt verschwunden ist/", "tokens": ["Viel\u00b7leicht", "kan", "auch", "dein", "Stern", "/", "der", "jetzt", "ver\u00b7schwun\u00b7den", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPOSAT", "NN", "$(", "ART", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dich wiederum erfreun/ wenn du best\u00e4ndig bist.", "tokens": ["Dich", "wie\u00b7de\u00b7rum", "er\u00b7freun", "/", "wenn", "du", "be\u00b7st\u00e4n\u00b7dig", "bist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$(", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}