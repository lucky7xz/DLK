{"textgrid.poem.48297": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Die Gans von Putlitz", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein neues Lied gesungen sei:", "tokens": ["Ein", "neu\u00b7es", "Lied", "ge\u00b7sun\u00b7gen", "sei", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach dem Winter, da kommt der Mai,", "tokens": ["Nach", "dem", "Win\u00b7ter", ",", "da", "kommt", "der", "Mai", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Das haben wir wohl vernommen;", "tokens": ["Das", "ha\u00b7ben", "wir", "wohl", "ver\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und da\u00df Kettr-Angerm\u00fcnde m\u00e4rkisch ward,", "tokens": ["Und", "da\u00df", "Kett\u00b7r\u00b7An\u00b7ger\u00b7m\u00fcn\u00b7de", "m\u00e4r\u00b7kisch", "ward", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Das soll dem Markgrafen frommen!", "tokens": ["Das", "soll", "dem", "Mark\u00b7gra\u00b7fen", "from\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "ADJA", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Johann von Briesen lie\u00df sich jagen", "tokens": ["Jo\u00b7hann", "von", "Brie\u00b7sen", "lie\u00df", "sich", "ja\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "NE", "VVFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Kettr-Angerm\u00fcnde bis Greifenhagen,", "tokens": ["Von", "Kett\u00b7r\u00b7An\u00b7ger\u00b7m\u00fcn\u00b7de", "bis", "Grei\u00b7fen\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "All' Mut war ihm gebrochen;", "tokens": ["All'", "Mut", "war", "ihm", "ge\u00b7bro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da ging er zu Hofe nach Alten-Stettin", "tokens": ["Da", "ging", "er", "zu", "Ho\u00b7fe", "nach", "Al\u00b7ten\u00b7S\u00b7tet\u00b7tin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "APPR", "NN"], "meter": "-+--+---+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und hat zu dem Herzog gesprochen:", "tokens": ["Und", "hat", "zu", "dem", "Her\u00b7zog", "ge\u00b7spro\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bbgn\u00e4d'ger Herre, was zu halten stand:", "tokens": ["\u00bb", "gn\u00e4d'\u00b7ger", "Her\u00b7re", ",", "was", "zu", "hal\u00b7ten", "stand", ":"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "PRELS", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Kettr-Angerm\u00fcnd und das Stolper Land,", "tokens": ["Kett\u00b7r\u00b7An\u00b7ger\u00b7m\u00fcnd", "und", "das", "Stol\u00b7per", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ist verloren und verdorben;", "tokens": ["Ist", "ver\u00b7lo\u00b7ren", "und", "ver\u00b7dor\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "KON", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Und doch hie\u00df es: er sei gestorben.\u00ab", "tokens": ["Und", "doch", "hie\u00df", "es", ":", "er", "sei", "ge\u00b7stor\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "VVPP", "$.", "$("], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Da lie\u00df der Herzog entbieten und holen", "tokens": ["Da", "lie\u00df", "der", "Her\u00b7zog", "ent\u00b7bie\u00b7ten", "und", "ho\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVFIN", "KON", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "All seine Mannschaft, Pommern und Polen,", "tokens": ["All", "sei\u00b7ne", "Mann\u00b7schaft", ",", "Pom\u00b7mern", "und", "Po\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "NN", "KON", "NE", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nach Vierraden ritt man zu Tische;", "tokens": ["Nach", "Vier\u00b7ra\u00b7den", "ritt", "man", "zu", "Ti\u00b7sche", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Da setzten sie sich und hielten Rat", "tokens": ["Da", "setz\u00b7ten", "sie", "sich", "und", "hiel\u00b7ten", "Rat"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "KON", "VVFIN", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und a\u00dfen s\u00fc\u00dfe Fische.", "tokens": ["Und", "a\u00b7\u00dfen", "s\u00fc\u00b7\u00dfe", "Fi\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dann ritten sie weiter, und kaum heran,", "tokens": ["Dann", "rit\u00b7ten", "sie", "wei\u00b7ter", ",", "und", "kaum", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KON", "ADV", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Alle haben dem Herzog geschworen,", "tokens": ["Al\u00b7le", "ha\u00b7ben", "dem", "Her\u00b7zog", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+--++-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und alle riefen: \u00bbStettin, Stettin!\u00ab", "tokens": ["Und", "al\u00b7le", "rie\u00b7fen", ":", "\u00bb", "Stet\u00b7tin", ",", "Stet\u00b7tin", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "PIS", "VVFIN", "$.", "$(", "NE", "$,", "NE", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Brandenburg war verloren.", "tokens": ["Und", "Bran\u00b7den\u00b7burg", "war", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Aber drau\u00dfen hinter Wall und Graben,", "tokens": ["A\u00b7ber", "drau\u00b7\u00dfen", "hin\u00b7ter", "Wall", "und", "Gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die M\u00e4rkischen schon sich gesammelt haben,", "tokens": ["Die", "M\u00e4r\u00b7ki\u00b7schen", "schon", "sich", "ge\u00b7sam\u00b7melt", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PRF", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vierhundert Reiter und Knechte;", "tokens": ["Vier\u00b7hun\u00b7dert", "Rei\u00b7ter", "und", "Knech\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "KON", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Zischend, auf da\u00df sie fechte.", "tokens": ["Zi\u00b7schend", ",", "auf", "da\u00df", "sie", "fech\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.7": {"line.1": {"text": "Ja, die ", "tokens": ["Ja", ",", "die"], "token_info": ["word", "punct", "word"], "pos": ["PTKANT", "$,", "PRELS"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Sie streckte zornig ihren Kragen", "tokens": ["Sie", "streck\u00b7te", "zor\u00b7nig", "ih\u00b7ren", "Kra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u00dcber die Pommern alle;", "tokens": ["\u00dc\u00b7ber", "die", "Pom\u00b7mern", "al\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIS", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Da schwebte der m\u00e4rkische Adler hoch,", "tokens": ["Da", "schweb\u00b7te", "der", "m\u00e4r\u00b7ki\u00b7sche", "Ad\u00b7ler", "hoch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und die Greifen kamen zu Falle.", "tokens": ["Und", "die", "Grei\u00b7fen", "ka\u00b7men", "zu", "Fal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Die Gans aber wuchs im Grimme noch,", "tokens": ["Die", "Gans", "a\u00b7ber", "wuchs", "im", "Grim\u00b7me", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPRART", "NN", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie schlug mit den Fl\u00fcgeln ein Brescheloch,", "tokens": ["Sie", "schlug", "mit", "den", "Fl\u00fc\u00b7geln", "ein", "Bre\u00b7sche\u00b7loch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und da stand sie nun zwischen den Steinen,", "tokens": ["Und", "da", "stand", "sie", "nun", "zwi\u00b7schen", "den", "Stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Und als sie bis zum Markte kam,", "tokens": ["Und", "als", "sie", "bis", "zum", "Mark\u00b7te", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Waren sie zehn gegen einen.", "tokens": ["Wa\u00b7ren", "sie", "zehn", "ge\u00b7gen", "ei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "CARD", "APPR", "ART", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.9": {"line.1": {"text": "Da gingen die Schwerter die Klinker die Klang,", "tokens": ["Da", "gin\u00b7gen", "die", "Schwer\u00b7ter", "die", "Klin\u00b7ker", "die", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Herr Detleff Schwerin mit dem Putlitz rang", "tokens": ["Herr", "Det\u00b7leff", "Schwe\u00b7rin", "mit", "dem", "Put\u00b7litz", "rang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "NE", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wollte den Preis erwerben;", "tokens": ["Und", "woll\u00b7te", "den", "Preis", "er\u00b7wer\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da mu\u00dfte Herr ", "tokens": ["Da", "mu\u00df\u00b7te", "Herr"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VMFIN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "F\u00fcr seinen Erbherrn sterben.", "tokens": ["F\u00fcr", "sei\u00b7nen", "Er\u00b7bherrn", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Das war des Herzogs schwerster Tag,", "tokens": ["Das", "war", "des", "Her\u00b7zogs", "schwers\u00b7ter", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als da Herr Detleff vor ihm lag,", "tokens": ["Als", "da", "Herr", "Det\u00b7leff", "vor", "ihm", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zerhackt, in Blut und Wunden,", "tokens": ["Zer\u00b7hackt", ",", "in", "Blut", "und", "Wun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und er rief: \u00bbO h\u00e4tt' ich \u00fcber den Damm", "tokens": ["Und", "er", "rief", ":", "\u00bb", "O", "h\u00e4tt'", "ich", "\u00fc\u00b7ber", "den", "Damm"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "$(", "NE", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Erst wieder zur\u00fccke gefunden!\u00ab", "tokens": ["Erst", "wie\u00b7der", "zu\u00b7r\u00fc\u00b7cke", "ge\u00b7fun\u00b7den", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "ADV", "VVPP", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.11": {"line.1": {"text": "Er sprach es und ritt im Zuge vorn,", "tokens": ["Er", "sprach", "es", "und", "ritt", "im", "Zu\u00b7ge", "vorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "APPRART", "NN", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er gab seinem Rosse Schlag und Sporn", "tokens": ["Er", "gab", "sei\u00b7nem", "Ros\u00b7se", "Schlag", "und", "Sporn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "KON", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und suchte die Z\u00fcgel zu fassen;", "tokens": ["Und", "such\u00b7te", "die", "Z\u00fc\u00b7gel", "zu", "fas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "So kam er bis an das \u00bbhohe Haus\u00ab,", "tokens": ["So", "kam", "er", "bis", "an", "das", "\u00bb", "ho\u00b7he", "Haus", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "$(", "ADJA", "NN", "$(", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Da ward er eingelassen.", "tokens": ["Da", "ward", "er", "ein\u00b7ge\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Das war zu Vierraden. Auf Schlosses Br\u00fcck'", "tokens": ["Das", "war", "zu", "Vier\u00b7ra\u00b7den", ".", "Auf", "Schlos\u00b7ses", "Br\u00fcck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Einmal noch sah er zur\u00fcck, zur\u00fcck,", "tokens": ["Ein\u00b7mal", "noch", "sah", "er", "zu\u00b7r\u00fcck", ",", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PTKVZ", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Im Herzen voll Weh und Leide:", "tokens": ["Im", "Her\u00b7zen", "voll", "Weh", "und", "Lei\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "\u00bbkettr-Angerm\u00fcnde, du vielgute Stadt,", "tokens": ["\u00bb", "kett\u00b7r\u00b7An\u00b7ger\u00b7m\u00fcn\u00b7de", ",", "du", "viel\u00b7gu\u00b7te", "Stadt", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Da\u00df so ich von dir scheide!\u00ab", "tokens": ["Da\u00df", "so", "ich", "von", "dir", "schei\u00b7de", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADV", "PPER", "APPR", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Der aber, der dies Lied euch sang,", "tokens": ["Der", "a\u00b7ber", ",", "der", "dies", "Lied", "euch", "sang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "PRELS", "PDS", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Schmiedeknecht ist er schon lang',", "tokens": ["Ein", "Schmie\u00b7de\u00b7knecht", "ist", "er", "schon", "lang'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sie nennen ihn K\u00f6ne Finken;", "tokens": ["Und", "sie", "nen\u00b7nen", "ihn", "K\u00f6\u00b7ne", "Fin\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und er f\u00fchrt ein H\u00e4mmerchen auf der Hand,", "tokens": ["Und", "er", "f\u00fchrt", "ein", "H\u00e4m\u00b7mer\u00b7chen", "auf", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und Gut-Bierchen mag er trinken.", "tokens": ["Und", "Gut\u00b7Bier\u00b7chen", "mag", "er", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}