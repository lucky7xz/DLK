{"textgrid.poem.47622": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[der M\u00e4rtyrer der Liebe]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der M\u00e4rtyrer der Liebe,", "tokens": ["Der", "M\u00e4r\u00b7ty\u00b7rer", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Maa\u00dflosem Harm erlegen,", "tokens": ["Maa\u00df\u00b7lo\u00b7sem", "Harm", "er\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie herrlich und wie hehr", "tokens": ["Wie", "herr\u00b7lich", "und", "wie", "hehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "PWAV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Im Tode rastet er!", "tokens": ["Im", "To\u00b7de", "ras\u00b7tet", "er", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der heilige Geist spricht ihm den Grabessegen.", "tokens": ["Der", "hei\u00b7li\u00b7ge", "Geist", "spricht", "ihm", "den", "Gra\u00b7bes\u00b7se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}}}}