{"textgrid.poem.40241": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nehmen wir Geschehn f\u00fcr Leben,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nehmen wir Geschehn f\u00fcr Leben,", "tokens": ["Neh\u00b7men", "wir", "Ge\u00b7schehn", "f\u00fcr", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "haben wir's nicht recht verstanden;", "tokens": ["ha\u00b7ben", "wir's", "nicht", "recht", "ver\u00b7stan\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PTKNEG", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Menschenleben ist das Leben", "tokens": ["Men\u00b7schen\u00b7le\u00b7ben", "ist", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "so nur, wie wir es empfanden \u2013", "tokens": ["so", "nur", ",", "wie", "wir", "es", "emp\u00b7fan\u00b7den", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "ja, so schw\u00e4rmt'ich seelentrunken.", "tokens": ["ja", ",", "so", "schw\u00e4rmt'\u00b7ich", "see\u00b7len\u00b7trun\u00b7ken", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie mir alles wohlbehagte,", "tokens": ["Wie", "mir", "al\u00b7les", "wohl\u00b7be\u00b7hag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "was ich f\u00fchlte, was ich sagte,", "tokens": ["was", "ich", "f\u00fchl\u00b7te", ",", "was", "ich", "sag\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "in mein Spiegelbild versunken!", "tokens": ["in", "mein", "Spie\u00b7gel\u00b7bild", "ver\u00b7sun\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch jetzt hei\u00dft es: mit den Zielen,", "tokens": ["Doch", "jetzt", "hei\u00dft", "es", ":", "mit", "den", "Zie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$.", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "mit den Wegen sich beraten.", "tokens": ["mit", "den", "We\u00b7gen", "sich", "be\u00b7ra\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zwar den J\u00fcngling ehrt sein F\u00fchlen,", "tokens": ["Zwar", "den", "J\u00fcng\u00b7ling", "ehrt", "sein", "F\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "doch dem Manne ziemen Thaten.", "tokens": ["doch", "dem", "Man\u00b7ne", "zie\u00b7men", "Tha\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Altgeschehnes, Neuerfahrnes,", "tokens": ["Alt\u00b7ge\u00b7scheh\u00b7nes", ",", "Neu\u00b7er\u00b7fahr\u00b7nes", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "dunkel dr\u00e4ngt es sich zusammen,", "tokens": ["dun\u00b7kel", "dr\u00e4ngt", "es", "sich", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und wir wissen nicht zu scheiden", "tokens": ["und", "wir", "wis\u00b7sen", "nicht", "zu", "schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "dieses Lodern seltner Flammen;", "tokens": ["die\u00b7ses", "Lo\u00b7dern", "selt\u00b7ner", "Flam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "denn darunter lebt ein Gl\u00fchen", "tokens": ["denn", "da\u00b7run\u00b7ter", "lebt", "ein", "Gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "seltenster Begebenheiten,", "tokens": ["sel\u00b7tens\u00b7ter", "Be\u00b7ge\u00b7ben\u00b7hei\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-----+-", "measure": "dactylic.init"}, "line.3": {"text": "und man f\u00fchlt ein still Bem\u00fchen,", "tokens": ["und", "man", "f\u00fchlt", "ein", "still", "Be\u00b7m\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "als ob Zeiten sich bereiten.", "tokens": ["als", "ob", "Zei\u00b7ten", "sich", "be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Nah schon, will der Sonnenwagen", "tokens": ["Nah", "schon", ",", "will", "der", "Son\u00b7nen\u00b7wa\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "VMFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wieder einen Kreis vollenden.", "tokens": ["wie\u00b7der", "ei\u00b7nen", "Kreis", "voll\u00b7en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird er durch den Steinbock jagen?", "tokens": ["Wird", "er", "durch", "den", "Stein\u00b7bock", "ja\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "wird er sich zum Krebse wenden?", "tokens": ["wird", "er", "sich", "zum", "Kreb\u00b7se", "wen\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Schaudernd scheint er still zu stehen", "tokens": ["Schau\u00b7dernd", "scheint", "er", "still", "zu", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "zwischen gleichen Finsternissen,", "tokens": ["zwi\u00b7schen", "glei\u00b7chen", "Fins\u00b7ter\u00b7nis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und nun scheint er sich zu drehen,", "tokens": ["und", "nun", "scheint", "er", "sich", "zu", "dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "aber Du \u2013 wirst mitgerissen.", "tokens": ["a\u00b7ber", "Du", "\u2013", "wirst", "mit\u00b7ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}