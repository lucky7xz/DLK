{"textgrid.poem.60075": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was hab' ich, gutes M\u00e4dchen!", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was hab' ich, gutes M\u00e4dchen!", "tokens": ["Was", "hab'", "ich", ",", "gu\u00b7tes", "M\u00e4d\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als jenes kleine Feld", "tokens": ["Als", "je\u00b7nes", "klei\u00b7ne", "Feld"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Um dein geliebtes St\u00e4dtchen,", "tokens": ["Um", "dein", "ge\u00b7lieb\u00b7tes", "St\u00e4dt\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir eine ganze Welt?", "tokens": ["Mir", "ei\u00b7ne", "gan\u00b7ze", "Welt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der andern acht' ich wenig;", "tokens": ["Der", "an\u00b7dern", "acht'", "ich", "we\u00b7nig", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da traur' ich, wie verbannt!", "tokens": ["Da", "traur'", "ich", ",", "wie", "ver\u00b7bannt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dein K\u00f6nig ist mein K\u00f6nig,", "tokens": ["Dein", "K\u00f6\u00b7nig", "ist", "mein", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dein Land mein Vaterland.", "tokens": ["Dein", "Land", "mein", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die ersten gr\u00fcnen Haine", "tokens": ["Die", "ers\u00b7ten", "gr\u00fc\u00b7nen", "Hai\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sind dort, wo Liebchen geht;", "tokens": ["Sind", "dort", ",", "wo", "Lieb\u00b7chen", "geht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "PWAV", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Luft ist erst die meine,", "tokens": ["Die", "Luft", "ist", "erst", "die", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "PPOSAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die sich um sie gedreht.", "tokens": ["Die", "sich", "um", "sie", "ge\u00b7dreht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-++--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "O, wann begr\u00fc\u00df' ich wieder", "tokens": ["O", ",", "wann", "be\u00b7gr\u00fc\u00df'", "ich", "wie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dein St\u00e4dtchen, meine Welt,", "tokens": ["Dein", "St\u00e4dt\u00b7chen", ",", "mei\u00b7ne", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und h\u00f6re Lerchen-Lieder", "tokens": ["Und", "h\u00f6\u00b7re", "Ler\u00b7chen\u00b7Lie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auf deinem kleinen Feld,", "tokens": ["Auf", "dei\u00b7nem", "klei\u00b7nen", "Feld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und sehe Morgen-Schimmer", "tokens": ["Und", "se\u00b7he", "Mor\u00b7gen\u00b7Schim\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bey dir, und hellen Tag?", "tokens": ["Bey", "dir", ",", "und", "hel\u00b7len", "Tag", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O denke nur, da\u00df immer,", "tokens": ["O", "den\u00b7ke", "nur", ",", "da\u00df", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,", "KOUS", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In jedem Glocken-Schlag,", "tokens": ["In", "je\u00b7dem", "Glo\u00b7cken\u00b7Schlag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Des Wiedersehns Minute", "tokens": ["Des", "Wie\u00b7der\u00b7sehns", "Mi\u00b7nu\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durch meine Seele schallt,", "tokens": ["Durch", "mei\u00b7ne", "See\u00b7le", "schallt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Weil, ach! in deinem Blute", "tokens": ["Weil", ",", "ach", "!", "in", "dei\u00b7nem", "Blu\u00b7te"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "$,", "ITJ", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein eignes Leben wallt!", "tokens": ["Mein", "eig\u00b7nes", "Le\u00b7ben", "wallt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}