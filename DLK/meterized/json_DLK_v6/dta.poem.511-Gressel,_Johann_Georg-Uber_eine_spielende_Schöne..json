{"dta.poem.511": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Uber eine spielende Sch\u00f6ne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Hertze mu\u00df durch euch der Liebe Wirckung f\u00fchlen/", "tokens": ["Mein", "Hert\u00b7ze", "mu\u00df", "durch", "euch", "der", "Lie\u00b7be", "Wir\u00b7ckung", "f\u00fch\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VMFIN", "APPR", "PPER", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn eure sch\u00f6ne Hand/ ", "tokens": ["Wenn", "eu\u00b7re", "sch\u00f6\u00b7ne", "Hand", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So wie ein ", "tokens": ["So", "wie", "ein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KOKOM", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Ich starre/ ich vergeh/ wenn nur der Schaal beginnt", "tokens": ["Ich", "star\u00b7re", "/", "ich", "ver\u00b7geh", "/", "wenn", "nur", "der", "Schaal", "be\u00b7ginnt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "$(", "KOUS", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nach meinen Ohr zu gehn. Was aber thun die Augen?", "tokens": ["Nach", "mei\u00b7nen", "Ohr", "zu", "gehn", ".", "Was", "a\u00b7ber", "thun", "die", "Au\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$.", "PWS", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die spielen Feur und Gluth/ die Brust stimmt auch mit ein.", "tokens": ["Die", "spie\u00b7len", "Feur", "und", "Gluth", "/", "die", "Brust", "stimmt", "auch", "mit", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$(", "ART", "NN", "VVFIN", "ADV", "APPR", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Weil meine Kr\u00e4ffte nun zum Wiederstand nicht taugen.", "tokens": ["Weil", "mei\u00b7ne", "Kr\u00e4ff\u00b7te", "nun", "zum", "Wie\u00b7der\u00b7stand", "nicht", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "APPRART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So st\u00fcrb\u2019 ich gar/ wenn ich eur Spiel mann wolte seyn.", "tokens": ["So", "st\u00fcrb'", "ich", "gar", "/", "wenn", "ich", "eur", "Spiel", "mann", "wol\u00b7te", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "KOUS", "PPER", "PPOSAT", "NN", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}