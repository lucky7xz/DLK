{"textgrid.poem.67945": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "13. Hochzeitgesang", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "ChOR DER J\u00dcNGLINGE:", "tokens": ["ChOR", "DeR", "J\u00fcNG\u00b7LIN\u00b7GE", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Auf! der Abend ist da! ihr J\u00fcnglinge auf! am Olympus", "tokens": ["Auf", "!", "der", "A\u00b7bend", "ist", "da", "!", "ihr", "J\u00fcng\u00b7lin\u00b7ge", "auf", "!", "am", "O\u00b7lym\u00b7pus"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "$.", "ART", "NN", "VAFIN", "ADV", "$.", "PPOSAT", "NN", "PTKVZ", "$.", "APPRART", "NE"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Hebt der langersehnete Stern sein funkelndes Haupt schon.", "tokens": ["Hebt", "der", "lan\u00b7ger\u00b7seh\u00b7ne\u00b7te", "Stern", "sein", "fun\u00b7keln\u00b7des", "Haupt", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "La\u00dft das triefende Mahl! es ist Zeit! es ist Zeit! denn im Nu wird", "tokens": ["La\u00dft", "das", "trie\u00b7fen\u00b7de", "Mahl", "!", "es", "ist", "Zeit", "!", "es", "ist", "Zeit", "!", "denn", "im", "Nu", "wird"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$.", "PPER", "VAFIN", "NN", "$.", "PPER", "VAFIN", "NN", "$.", "ADV", "APPRART", "ADV", "VAFIN"], "meter": "--+--+--+--+-+-+", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Kommen die Braut und soll der Hymen\u00e4us ert\u00f6nen.", "tokens": ["Kom\u00b7men", "die", "Braut", "und", "soll", "der", "Hy\u00b7me\u00b7n\u00e4us", "er\u00b7t\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Hymen o Hymen\u00e4us! Hymen komm Hymen\u00e4us.", "tokens": ["Hy\u00b7men", "o", "Hy\u00b7me\u00b7n\u00e4us", "!", "Hy\u00b7men", "komm", "Hy\u00b7me\u00b7n\u00e4us", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "$.", "NN", "VVFIN", "NE", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}, "stanza.2": {"line.1": {"text": "M\u00e4DCHEN:", "tokens": ["M\u00e4D\u00b7CHEN", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Jungfraun, schauet ihr nicht die J\u00fcnglinge? Ihnen entgegen,", "tokens": ["Jung\u00b7fraun", ",", "schau\u00b7et", "ihr", "nicht", "die", "J\u00fcng\u00b7lin\u00b7ge", "?", "Ih\u00b7nen", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$.", "PPER", "PTKVZ", "$,"], "meter": "+-+--+-++-+--+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Auf! der Bote der Nacht, er schwingt die himmlische Fackel.", "tokens": ["Auf", "!", "der", "Bo\u00b7te", "der", "Nacht", ",", "er", "schwingt", "die", "himm\u00b7li\u00b7sche", "Fa\u00b7ckel", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "ART", "NN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wahrlich! sehet ihr nicht, wie sie sich zum Kampf schon r\u00fcsten:", "tokens": ["Wahr\u00b7lich", "!", "se\u00b7het", "ihr", "nicht", ",", "wie", "sie", "sich", "zum", "Kampf", "schon", "r\u00fcs\u00b7ten", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "PRF", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "Nicht vergeblich r\u00fcsten! der Sieg im Gesange wird ihr seyn.", "tokens": ["Nicht", "ver\u00b7geb\u00b7lich", "r\u00fcs\u00b7ten", "!", "der", "Sieg", "im", "Ge\u00b7san\u00b7ge", "wird", "ihr", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVFIN", "$.", "ART", "NN", "APPRART", "NN", "VAFIN", "PPER", "VAINF", "$."], "meter": "+-+-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Hymen o Hymen\u00e4us! Hymen komm Hymen\u00e4us.", "tokens": ["Hy\u00b7men", "o", "Hy\u00b7me\u00b7n\u00e4us", "!", "Hy\u00b7men", "komm", "Hy\u00b7me\u00b7n\u00e4us", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "$.", "NN", "VVFIN", "NE", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}, "stanza.3": {"line.1": {"text": "J\u00fcNGLINGE:", "tokens": ["J\u00fcNG\u00b7LIN\u00b7GE", ":"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Br\u00fcder, es ist uns nicht so leicht die Palme verliehen!", "tokens": ["Br\u00fc\u00b7der", ",", "es", "ist", "uns", "nicht", "so", "leicht", "die", "Pal\u00b7me", "ver\u00b7lie\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPER", "PTKNEG", "ADV", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Seht, wie die Jungfraun dort nachsinnend suchen Ges\u00e4nge,", "tokens": ["Seht", ",", "wie", "die", "Jung\u00b7fraun", "dort", "nach\u00b7sin\u00b7nend", "su\u00b7chen", "Ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Nicht vergebens sinnen sie nach; sie suchen das Sch\u00f6nste,", "tokens": ["Nicht", "ver\u00b7ge\u00b7bens", "sin\u00b7nen", "sie", "nach", ";", "sie", "su\u00b7chen", "das", "Sch\u00f6ns\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.5": {"text": "Wohl das Sch\u00f6nste, da sie mit ganzer Seele sich m\u00fchen;", "tokens": ["Wohl", "das", "Sch\u00f6ns\u00b7te", ",", "da", "sie", "mit", "gan\u00b7zer", "See\u00b7le", "sich", "m\u00fc\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und wir schweifen umher, das Ohr, die Seele getheilet.", "tokens": ["Und", "wir", "schwei\u00b7fen", "um\u00b7her", ",", "das", "Ohr", ",", "die", "See\u00b7le", "ge\u00b7thei\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "$,", "ART", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Billig siegen sie denn: denn Sieg will M\u00fche! Wohlauf noch", "tokens": ["Bil\u00b7lig", "sie\u00b7gen", "sie", "denn", ":", "denn", "Sieg", "will", "M\u00fc\u00b7he", "!", "Wohl\u00b7auf", "noch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "$.", "KON", "NN", "VMFIN", "NN", "$.", "ADV", "ADV"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Itzt ihr Br\u00fcder, o ruft zum Gesang' die Seele zusammen.", "tokens": ["Itzt", "ihr", "Br\u00fc\u00b7der", ",", "o", "ruft", "zum", "Ge\u00b7sang'", "die", "See\u00b7le", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "FM", "VVFIN", "APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.9": {"text": "Sie beginnen im Nu; im Nu soll Antwort ert\u00f6nen.", "tokens": ["Sie", "be\u00b7gin\u00b7nen", "im", "Nu", ";", "im", "Nu", "soll", "Ant\u00b7wort", "er\u00b7t\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADV", "$.", "APPRART", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Hymen o Hymen\u00e4us! Hymen, komm Hymen\u00e4us.", "tokens": ["Hy\u00b7men", "o", "Hy\u00b7me\u00b7n\u00e4us", "!", "Hy\u00b7men", ",", "komm", "Hy\u00b7me\u00b7n\u00e4us", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "$.", "NN", "$,", "VVFIN", "NE", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}, "stanza.4": {"line.1": {"text": "M\u00e4DCHEN:", "tokens": ["M\u00e4D\u00b7CHEN", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Hesperus, blickt am Himmel wohl Ein grausamer Gestirn, als", "tokens": ["Hes\u00b7pe\u00b7rus", ",", "blickt", "am", "Him\u00b7mel", "wohl", "Ein", "grau\u00b7sa\u00b7mer", "Ge\u00b7stirn", ",", "als"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "$,", "VVFIN", "APPRART", "NN", "ADV", "ART", "ADJA", "NN", "$,", "KOUS"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Du, der Mutterarmen vermag die bl\u00fchende Tochter", "tokens": ["Du", ",", "der", "Mut\u00b7ter\u00b7ar\u00b7men", "ver\u00b7mag", "die", "bl\u00fc\u00b7hen\u00b7de", "Toch\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Zu entreissen, sie loszureissen dem Arm, der sie festh\u00e4lt", "tokens": ["Zu", "en\u00b7treis\u00b7sen", ",", "sie", "los\u00b7zu\u00b7reis\u00b7sen", "dem", "Arm", ",", "der", "sie", "fest\u00b7h\u00e4lt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "--+--+-+--+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und dem brennenden J\u00fcngling' ein keusches M\u00e4dchen zu geben.", "tokens": ["Und", "dem", "bren\u00b7nen\u00b7den", "J\u00fcng\u00b7ling'", "ein", "keu\u00b7sches", "M\u00e4d\u00b7chen", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Feind' in eroberter Stadt, was k\u00f6nnen sie h\u00e4rter beginnen?", "tokens": ["Feind'", "in", "er\u00b7o\u00b7ber\u00b7ter", "Stadt", ",", "was", "k\u00f6n\u00b7nen", "sie", "h\u00e4r\u00b7ter", "be\u00b7gin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "PWS", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "---+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Hymen o Hymen\u00e4us! Hymen komm Hymen\u00e4us.", "tokens": ["Hy\u00b7men", "o", "Hy\u00b7me\u00b7n\u00e4us", "!", "Hy\u00b7men", "komm", "Hy\u00b7me\u00b7n\u00e4us", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "$.", "NN", "VVFIN", "NE", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}, "stanza.5": {"line.1": {"text": "J\u00fcNGLINGE:", "tokens": ["J\u00fcNG\u00b7LIN\u00b7GE", ":"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Hesperus, ist am Himmel wohl ein holdseliger Stern, als", "tokens": ["Hes\u00b7pe\u00b7rus", ",", "ist", "am", "Him\u00b7mel", "wohl", "ein", "hold\u00b7se\u00b7li\u00b7ger", "Stern", ",", "als"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "$,", "VAFIN", "APPRART", "NN", "ADV", "ART", "ADJA", "NN", "$,", "KOUS"], "meter": "-+-+-+-+-+---+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Du, de\u00df Flamme den Bund der treuen Liebe nun festkn\u00fcpft,", "tokens": ["Du", ",", "de\u00df", "Flam\u00b7me", "den", "Bund", "der", "treu\u00b7en", "Lie\u00b7be", "nun", "fest\u00b7kn\u00fcpft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "ART", "NN", "ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Kn\u00fcpft das Band, das M\u00e4nner, das Eltern geschlungen und eh nicht", "tokens": ["Kn\u00fcpft", "das", "Band", ",", "das", "M\u00e4n\u00b7ner", ",", "das", "El\u00b7tern", "ge\u00b7schlun\u00b7gen", "und", "eh", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "VVPP", "KON", "KOUS", "PTKNEG"], "meter": "+-+-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Zuziehn konnten, bis dein segnendes Auge darauf blickt.", "tokens": ["Zu\u00b7ziehn", "konn\u00b7ten", ",", "bis", "dein", "seg\u00b7nen\u00b7des", "Au\u00b7ge", "da\u00b7rauf", "blickt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "PAV", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "K\u00f6nnen G\u00f6tter uns mehr verleihn als die gl\u00fcckliche Stunde?", "tokens": ["K\u00f6n\u00b7nen", "G\u00f6t\u00b7ter", "uns", "mehr", "ver\u00b7leihn", "als", "die", "gl\u00fcck\u00b7li\u00b7che", "Stun\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "ADV", "VVINF", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.7": {"text": "Hymen o Hymen\u00e4us! Hymen komm Hymen\u00e4us.", "tokens": ["Hy\u00b7men", "o", "Hy\u00b7me\u00b7n\u00e4us", "!", "Hy\u00b7men", "komm", "Hy\u00b7me\u00b7n\u00e4us", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "$.", "NN", "VVFIN", "NE", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}, "stanza.6": {"line.1": {"text": "M\u00e4DCHEN:", "tokens": ["M\u00e4D\u00b7CHEN", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Hesperus, ach ihr Schwestern, er hat uns Eine Gespielin", "tokens": ["Hes\u00b7pe\u00b7rus", ",", "ach", "ihr", "Schwes\u00b7tern", ",", "er", "hat", "uns", "Ei\u00b7ne", "Ge\u00b7spie\u00b7lin"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "XY", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Weggeraubet, der R\u00e4uber, dem jede Wache vergebens", "tokens": ["Weg\u00b7ge\u00b7rau\u00b7bet", ",", "der", "R\u00e4u\u00b7ber", ",", "dem", "je\u00b7de", "Wa\u00b7che", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "ADV"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Lauret, der die Diebe verbirgt und wenn er mit anderm", "tokens": ["Lau\u00b7ret", ",", "der", "die", "Die\u00b7be", "ver\u00b7birgt", "und", "wenn", "er", "mit", "an\u00b7derm"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "KON", "KOUS", "PPER", "APPR", "PIS"], "meter": "+---+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Namen", "tokens": ["Na\u00b7men"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.7": {"line.1": {"text": "J\u00fcNGLINGE:", "tokens": ["J\u00fcNG\u00b7LIN\u00b7GE", ":"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Hesperus, h\u00f6re sie nicht: sie singen gedichtete Klagen", "tokens": ["Hes