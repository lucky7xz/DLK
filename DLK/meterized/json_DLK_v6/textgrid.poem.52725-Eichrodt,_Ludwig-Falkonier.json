{"textgrid.poem.52725": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Falkonier", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fche Morgens in die Schenke", "tokens": ["Fr\u00fc\u00b7he", "Mor\u00b7gens", "in", "die", "Schen\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trat ein junger Wandrer ein,", "tokens": ["Trat", "ein", "jun\u00b7ger", "Wand\u00b7rer", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gr\u00fc\u00dfte sittig und verlangte", "tokens": ["Gr\u00fc\u00df\u00b7te", "sit\u00b7tig", "und", "ver\u00b7lang\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen klaren Becher Wein.", "tokens": ["Ei\u00b7nen", "kla\u00b7ren", "Be\u00b7cher", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch es sprach vom Haus die Tochter,", "tokens": ["Doch", "es", "sprach", "vom", "Haus", "die", "Toch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werther Gast, kein Wein ist oben,", "tokens": ["Wert\u00b7her", "Gast", ",", "kein", "Wein", "ist", "o\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PIAT", "NN", "VAFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gestern Abend bei dem Feste", "tokens": ["Ge\u00b7stern", "A\u00b7bend", "bei", "dem", "Fes\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "APPR", "ART", "NN"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Ist er ganz durchaus zerstoben.", "tokens": ["Ist", "er", "ganz", "durc\u00b7haus", "zer\u00b7sto\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "M\u00fc\u00dft euch eben was gedulden,", "tokens": ["M\u00fc\u00dft", "euch", "e\u00b7ben", "was", "ge\u00b7dul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis ich aus dem Keller komm,", "tokens": ["Bis", "ich", "aus", "dem", "Kel\u00b7ler", "komm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Um die Ecke, aus dem Keller", "tokens": ["Um", "die", "E\u00b7cke", ",", "aus", "dem", "Kel\u00b7ler"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unterm St. Georgendom.", "tokens": ["Un\u00b7term", "St.", "Ge\u00b7or\u00b7gen\u00b7dom", "."], "token_info": ["word", "abbreviation", "word", "punct"], "pos": ["NN", "NE", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Nun so gehe nicht zum Keller,", "tokens": ["Nun", "so", "ge\u00b7he", "nicht", "zum", "Kel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PTKNEG", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Drunten ist es k\u00fchl und feucht,", "tokens": ["Drun\u00b7ten", "ist", "es", "k\u00fchl", "und", "feucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem n\u00e4chtlichen Ged\u00e4mmer", "tokens": ["In", "dem", "n\u00e4cht\u00b7li\u00b7chen", "Ge\u00b7d\u00e4m\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da verderben Blumen leicht.", "tokens": ["Da", "ver\u00b7der\u00b7ben", "Blu\u00b7men", "leicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Silberfrische Labung bringe", "tokens": ["Sil\u00b7ber\u00b7fri\u00b7sche", "La\u00b7bung", "brin\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Drau\u00dfen von dem guten Bronnen,", "tokens": ["Drau\u00b7\u00dfen", "von", "dem", "gu\u00b7ten", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mild und freundlich lacht der Morgen", "tokens": ["Mild", "und", "freund\u00b7lich", "lacht", "der", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der warmen Fr\u00fchlingssonnen.", "tokens": ["In", "der", "war\u00b7men", "Fr\u00fch\u00b7lings\u00b7son\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und der Silberquell wird munden", "tokens": ["Und", "der", "Sil\u00b7ber\u00b7quell", "wird", "mun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Besser mir, denn goldner Wein,", "tokens": ["Bes\u00b7ser", "mir", ",", "denn", "gold\u00b7ner", "Wein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll er mir von deinen lieben", "tokens": ["Soll", "er", "mir", "von", "dei\u00b7nen", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "APPR", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4nden hold kredenzet sein!", "tokens": ["H\u00e4n\u00b7den", "hold", "kre\u00b7den\u00b7zet", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Auf dem Tische steht der Becher,", "tokens": ["Auf", "dem", "Ti\u00b7sche", "steht", "der", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor dem J\u00fcngling steht die Maid,", "tokens": ["Vor", "dem", "J\u00fcng\u00b7ling", "steht", "die", "Maid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er spricht im Schaun versunken,", "tokens": ["Und", "er", "spricht", "im", "Schaun", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Recht in Seelenseligkeit:", "tokens": ["Recht", "in", "See\u00b7len\u00b7se\u00b7lig\u00b7keit", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Goldes Werth hat nicht das Wasser,", "tokens": ["Gol\u00b7des", "Werth", "hat", "nicht", "das", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wei\u00dft du, was ich zahlen mu\u00df?", "tokens": ["Wei\u00dft", "du", ",", "was", "ich", "zah\u00b7len", "mu\u00df", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diesen Becher, liebstes M\u00e4dchen,", "tokens": ["Die\u00b7sen", "Be\u00b7cher", ",", "liebs\u00b7tes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zahl ich nur mit einem Ku\u00df!", "tokens": ["Zahl", "ich", "nur", "mit", "ei\u00b7nem", "Ku\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}