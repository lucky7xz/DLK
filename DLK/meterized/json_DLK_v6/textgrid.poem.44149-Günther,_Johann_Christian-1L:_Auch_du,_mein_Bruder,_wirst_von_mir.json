{"textgrid.poem.44149": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auch du, mein Bruder, wirst von mir", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch du, mein Bruder, wirst von mir", "tokens": ["Auch", "du", ",", "mein", "Bru\u00b7der", ",", "wirst", "von", "mir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "PPOSAT", "NN", "$,", "VAFIN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch diesen Abschiedsbrief gek\u00fc\u00dfet.", "tokens": ["Durch", "die\u00b7sen", "Ab\u00b7schieds\u00b7brief", "ge\u00b7k\u00fc\u00b7\u00dfet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Himmel will: Ich mu\u00df von hier,", "tokens": ["Der", "Him\u00b7mel", "will", ":", "Ich", "mu\u00df", "von", "hier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$.", "PPER", "VMFIN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Drum folg ich, wie sein Rath beschlie\u00dfet.", "tokens": ["Drum", "folg", "ich", ",", "wie", "sein", "Rath", "be\u00b7schlie\u00b7\u00dfet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vergieb mir, was durch mein Versehn", "tokens": ["Ver\u00b7gieb", "mir", ",", "was", "durch", "mein", "Ver\u00b7sehn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dir dann und wann zu nah geschehn,", "tokens": ["Dir", "dann", "und", "wann", "zu", "nah", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KON", "PWAV", "PTKA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und las mir allzeit deine G\u00fcte!", "tokens": ["Und", "las", "mir", "all\u00b7zeit", "dei\u00b7ne", "G\u00fc\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Verl\u00e4st mich Freund und Vaterland,", "tokens": ["Ver\u00b7l\u00e4st", "mich", "Freund", "und", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Behalt ich gar nichts in der Hand,", "tokens": ["Be\u00b7halt", "ich", "gar", "nichts", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Behalt ich doch ein treu Gem\u00fcthe.", "tokens": ["Be\u00b7halt", "ich", "doch", "ein", "treu", "Ge\u00b7m\u00fc\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kan meine schlechte Poesie", "tokens": ["Kan", "mei\u00b7ne", "schlech\u00b7te", "Poe\u00b7sie"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dir auch noch in der Fremde dienen,", "tokens": ["Dir", "auch", "noch", "in", "der", "Frem\u00b7de", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So soll hinfort durch ihre M\u00fch", "tokens": ["So", "soll", "hin\u00b7fort", "durch", "ih\u00b7re", "M\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Lob in meinen B\u00fcchern gr\u00fcnen.", "tokens": ["Dein", "Lob", "in", "mei\u00b7nen", "B\u00fc\u00b7chern", "gr\u00fc\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du kennst mein Elend, meine Noth;", "tokens": ["Du", "kennst", "mein", "E\u00b7lend", ",", "mei\u00b7ne", "Noth", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gott weis, ich w\u00fcntschte mir den Tod,", "tokens": ["Gott", "weis", ",", "ich", "w\u00fcnt\u00b7schte", "mir", "den", "Tod", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wofern ich nicht dabey ged\u00e4chte,", "tokens": ["Wo\u00b7fern", "ich", "nicht", "da\u00b7bey", "ge\u00b7d\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df bald ein be\u00dfrer Unbestand,", "tokens": ["Da\u00df", "bald", "ein", "be\u00df\u00b7rer", "Un\u00b7be\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wer weis durch welches fremde Land,", "tokens": ["Wer", "weis", "durch", "wel\u00b7ches", "frem\u00b7de", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKVZ", "APPR", "PWAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir noch den Gl\u00fccksstern zeigen m\u00f6chte.", "tokens": ["Mir", "noch", "den", "Gl\u00fccks\u00b7stern", "zei\u00b7gen", "m\u00f6ch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "P.S. Wiltu dich noch gut erweisen,", "tokens": ["P.", "S.", "Wil\u00b7tu", "dich", "noch", "gut", "er\u00b7wei\u00b7sen", ","], "token_info": ["abbreviation", "abbreviation", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O so gieb mir was zum Reisen.", "tokens": ["O", "so", "gieb", "mir", "was", "zum", "Rei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVIMP", "PPER", "PRELS", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}