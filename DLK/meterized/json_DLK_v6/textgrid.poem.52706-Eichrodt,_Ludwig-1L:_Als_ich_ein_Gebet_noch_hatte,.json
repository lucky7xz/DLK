{"textgrid.poem.52706": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als ich ein Gebet noch hatte,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich ein Gebet noch hatte,", "tokens": ["Als", "ich", "ein", "Ge\u00b7bet", "noch", "hat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ich Abends schlafen ging,", "tokens": ["Wenn", "ich", "A\u00b7bends", "schla\u00b7fen", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Sinn, der schlummermatte,", "tokens": ["Und", "der", "Sinn", ",", "der", "schlum\u00b7mer\u00b7mat\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch Gewissensgrillen fing,", "tokens": ["Noch", "Ge\u00b7wis\u00b7sens\u00b7gril\u00b7len", "fing", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als bei jedem Schritt und Tritte", "tokens": ["Als", "bei", "je\u00b7dem", "Schritt", "und", "Trit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich mein Herz belauscht und frug,", "tokens": ["Ich", "mein", "Herz", "be\u00b7lauscht", "und", "frug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hei\u00dfen Dank und hei\u00dfre Bitte.", "tokens": ["Hei\u00b7\u00dfen", "Dank", "und", "hei\u00df\u00b7re", "Bit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In die Sternenwelten trug,", "tokens": ["In", "die", "Ster\u00b7nen\u00b7wel\u00b7ten", "trug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Glaubet nicht, da\u00df mir ", "tokens": ["Glau\u00b7bet", "nicht", ",", "da\u00df", "mir"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Damals sanfter, leichter flo\u00df,", "tokens": ["Da\u00b7mals", "sanf\u00b7ter", ",", "leich\u00b7ter", "flo\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil ich still und gottergeben", "tokens": ["Weil", "ich", "still", "und", "got\u00b7ter\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedem Stolz die Brust verschlo\u00df,", "tokens": ["Je\u00b7dem", "Stolz", "die", "Brust", "ver\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil ich nie sich frei entfalten", "tokens": ["Weil", "ich", "nie", "sich", "frei", "ent\u00b7fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PRF", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lie\u00df Gef\u00fchl und Sinnes Kraft,", "tokens": ["Lie\u00df", "Ge\u00b7f\u00fchl", "und", "Sin\u00b7nes", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sich gestalten, siegreich walten", "tokens": ["Sich", "ge\u00b7stal\u00b7ten", ",", "sieg\u00b7reich", "wal\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PRF", "VVPP", "$,", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nimmermehr die Leidenschaft.", "tokens": ["Nim\u00b7mer\u00b7mehr", "die", "Lei\u00b7den\u00b7schaft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Heiter streu ich in die St\u00fcrme", "tokens": ["Hei\u00b7ter", "streu", "ich", "in", "die", "St\u00fcr\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heute des Verstandes Licht,", "tokens": ["Heu\u00b7te", "des", "Ver\u00b7stan\u00b7des", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df mich eine Kette schirme,", "tokens": ["Da\u00df", "mich", "ei\u00b7ne", "Ket\u00b7te", "schir\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00fcg ich heute wahrlich nicht!", "tokens": ["Tr\u00fcg", "ich", "heu\u00b7te", "wahr\u00b7lich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Heiter in der Lebenswellen", "tokens": ["Hei\u00b7ter", "in", "der", "Le\u00b7bens\u00b7wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wilden Aufruhr gie\u00df ich heut", "tokens": ["Wil\u00b7den", "Auf\u00b7ruhr", "gie\u00df", "ich", "heut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Oel der Weisheit, sanftes Quellen", "tokens": ["O\u00b7el", "der", "Weis\u00b7heit", ",", "sanf\u00b7tes", "Quel\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ewiger Vern\u00fcnftigkeit!", "tokens": ["E\u00b7wi\u00b7ger", "Ver\u00b7n\u00fcnf\u00b7tig\u00b7keit", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}