{"textgrid.poem.62718": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Zuschrift bey der andern Auflage", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich widme Dir \u2013 \u2013 doch nein;", "tokens": ["Ich", "wid\u00b7me", "Dir", "\u2013", "\u2013", "doch", "nein", ";"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "$(", "ADV", "PTKANT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die K\u00fchnheit ist zu gro\u00df; Es m\u00f6chte strafbar seyn,", "tokens": ["Die", "K\u00fchn\u00b7heit", "ist", "zu", "gro\u00df", ";", "Es", "m\u00f6ch\u00b7te", "straf\u00b7bar", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "$.", "PPER", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wofern ich mich so frey zum Purpur wagen solte,", "tokens": ["Wo\u00b7fern", "ich", "mich", "so", "frey", "zum", "Pur\u00b7pur", "wa\u00b7gen", "sol\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADJD", "APPRART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und meiner Niedrigkeit darbey vergessen wolte.", "tokens": ["Und", "mei\u00b7ner", "Nied\u00b7rig\u00b7keit", "dar\u00b7bey", "ver\u00b7ges\u00b7sen", "wol\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PAV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jedoch! was red ich jetzt? Darf sich die Poesie,", "tokens": ["Je\u00b7doch", "!", "was", "red", "ich", "jetzt", "?", "Darf", "sich", "die", "Poe\u00b7sie", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PWS", "VVFIN", "PPER", "ADV", "$.", "VMFIN", "PRF", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Vor kein gesalbtes Haupt, und grossen K\u00f6nigs Knie", "tokens": ["Vor", "kein", "ge\u00b7salb\u00b7tes", "Haupt", ",", "und", "gros\u00b7sen", "K\u00f6\u00b7nigs", "Knie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,", "KON", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In Unterth\u00e4nigkeit und Demuth niederwerfen?", "tokens": ["In", "Un\u00b7ter\u00b7th\u00e4\u00b7nig\u00b7keit", "und", "De\u00b7muth", "nie\u00b7der\u00b7wer\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Kein Dichter seinen Kiel zum Dienst des K\u00f6nigs sch\u00e4rfen?", "tokens": ["Kein", "Dich\u00b7ter", "sei\u00b7nen", "Kiel", "zum", "Dienst", "des", "K\u00f6\u00b7nigs", "sch\u00e4r\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPOSAT", "NN", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "O ja! so sehr man auch die reine Dichtkunst flieht,", "tokens": ["O", "ja", "!", "so", "sehr", "man", "auch", "die", "rei\u00b7ne", "Dicht\u00b7kunst", "flieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "ADV", "ADV", "PIS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So wenig man auch sonst auf ihre Sch\u00f6nheit sieht,", "tokens": ["So", "we\u00b7nig", "man", "auch", "sonst", "auf", "ih\u00b7re", "Sch\u00f6n\u00b7heit", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sie aus Verachtung schilt, und schimpflich gnug benennet;", "tokens": ["Sie", "aus", "Ver\u00b7ach\u00b7tung", "schilt", ",", "und", "schimpf\u00b7lich", "gnug", "be\u00b7nen\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVFIN", "$,", "KON", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So lebt doch mancher F\u00fcrst, der ihren Adel kennet.", "tokens": ["So", "lebt", "doch", "man\u00b7cher", "F\u00fcrst", ",", "der", "ih\u00b7ren", "A\u00b7del", "ken\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und dessen Helden-Ruhm bis an den Himmel steigt,", "tokens": ["Und", "des\u00b7sen", "Hel\u00b7den\u00b7Ruhm", "bis", "an", "den", "Him\u00b7mel", "steigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "APPR", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und noch im Tode lebt, hat bey vergn\u00fcgten Stunden", "tokens": ["Und", "noch", "im", "To\u00b7de", "lebt", ",", "hat", "bey", "ver\u00b7gn\u00fcg\u00b7ten", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "$,", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Auch an der Dichter Kunst nicht wenig Lust gefunden.", "tokens": ["Auch", "an", "der", "Dich\u00b7ter", "Kunst", "nicht", "we\u00b7nig", "Lust", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "PTKNEG", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Er wu\u00dfte selbsten schon, was rein und lieblich war,", "tokens": ["Er", "wu\u00df\u00b7te", "selbs\u00b7ten", "schon", ",", "was", "rein", "und", "lieb\u00b7lich", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "PRELS", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Drum kunt ein muntrer Kiel sein Lied ganz ohn Gefahr", "tokens": ["Drum", "kunt", "ein", "mun\u00b7trer", "Kiel", "sein", "Lied", "ganz", "ohn", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dem welt ber\u00fchmten Held vor Aug und Anlitz bringen,", "tokens": ["Dem", "welt", "be\u00b7r\u00fchm\u00b7ten", "Held", "vor", "Aug", "und", "An\u00b7litz", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und einen reinen Thon nach Art der Dichter singen.", "tokens": ["Und", "ei\u00b7nen", "rei\u00b7nen", "Thon", "nach", "Art", "der", "Dich\u00b7ter", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So sehr nun diesem Held ein Helden-Lied gefiel,", "tokens": ["So", "sehr", "nun", "die\u00b7sem", "Held", "ein", "Hel\u00b7den\u00b7Lied", "ge\u00b7fiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PDAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So s\u00fcsse kams Ihm vor, wenn man ein S\u00e4yten-Spiel", "tokens": ["So", "s\u00fcs\u00b7se", "kams", "Ihm", "vor", ",", "wenn", "man", "ein", "S\u00e4y\u00b7ten\u00b7Spiel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PIS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Nach muntrer Bergmanns-Art vor seine Ohren brachte,", "tokens": ["Nach", "mun\u00b7trer", "Berg\u00b7manns\u00b7Art", "vor", "sei\u00b7ne", "Oh\u00b7ren", "brach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und seinem hohen Geist dadurch Vergn\u00fcgen machte.", "tokens": ["Und", "sei\u00b7nem", "ho\u00b7hen", "Geist", "da\u00b7durch", "Ver\u00b7gn\u00fc\u00b7gen", "mach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "PAV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Hat nicht die\u00df grosse Haupt bey mancher Lustbarkeit", "tokens": ["Hat", "nicht", "die\u00df", "gros\u00b7se", "Haupt", "bey", "man\u00b7cher", "Lust\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "PDS", "ADJA", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ein Aufzug nach der Art der Bergmannschafft erfreut?", "tokens": ["Ein", "Auf\u00b7zug", "nach", "der", "Art", "der", "Berg\u00b7mann\u00b7schafft", "er\u00b7freut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wie hoch hat nicht sein Ohr die Berg-Music gesch\u00e4tzet,", "tokens": ["Wie", "hoch", "hat", "nicht", "sein", "Ohr", "die", "Ber\u00b7gMu\u00b7sic", "ge\u00b7sch\u00e4t\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PTKNEG", "PPOSAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die seinen tapfern Geist durch ihren Thon erg\u00f6tzet?", "tokens": ["Die", "sei\u00b7nen", "tap\u00b7fern", "Geist", "durch", "ih\u00b7ren", "Thon", "er\u00b7g\u00f6t\u00b7zet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Da\u00df deine K\u00f6nigs-Brust dem Vater \u00e4hnlich sey.", "tokens": ["Da\u00df", "dei\u00b7ne", "K\u00f6\u00b7nigs\u00b7Brust", "dem", "Va\u00b7ter", "\u00e4hn\u00b7lich", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Du hast des Vaters Thron wie seinen Geist bekommen.", "tokens": ["Du", "hast", "des", "Va\u00b7ters", "Thron", "wie", "sei\u00b7nen", "Geist", "be\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "KOKOM", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.31": {"text": "Und was ", "tokens": ["Und", "was"], "token_info": ["word", "word"], "pos": ["KON", "PWS"], "meter": "-+", "measure": "iambic.single"}, "line.32": {"text": "Da\u00df man Dich auch mit Recht ", "tokens": ["Da\u00df", "man", "Dich", "auch", "mit", "Recht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "ADV", "APPR", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.33": {"text": "Du bist mit Gro\u00dfmuth selbst wie jener ausgeschm\u00fccket/", "tokens": ["Du", "bist", "mit", "Gro\u00df\u00b7muth", "selbst", "wie", "je\u00b7ner", "aus\u00b7ge\u00b7schm\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADV", "KOKOM", "PDS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Dein Auge nicht allein auf hohe Edern blicket;", "tokens": ["Dein", "Au\u00b7ge", "nicht", "al\u00b7lein", "auf", "ho\u00b7he", "E\u00b7dern", "bli\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "O nein! ", "tokens": ["O", "nein", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "PTKANT", "$."], "meter": "-+", "measure": "iambic.single"}, "line.36": {"text": "Auch auf ein niedrig Laub, und kleinen Rosen-Strauch;", "tokens": ["Auch", "auf", "ein", "nied\u00b7rig", "Laub", ",", "und", "klei\u00b7nen", "Ro\u00b7sen\u00b7Strauch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJD", "NN", "$,", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ein jeder darf getrost Dein hohes Anlitz sehen,", "tokens": ["Ein", "je\u00b7der", "darf", "ge\u00b7trost", "Dein", "ho\u00b7hes", "An\u00b7litz", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "ADJD", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und kan von Dir, ", "tokens": ["Und", "kan", "von", "Dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.39": {"text": "Dis machet mich beherzt, da\u00df ich mein Dichter-Spiel", "tokens": ["Dis", "ma\u00b7chet", "mich", "be\u00b7herzt", ",", "da\u00df", "ich", "mein", "Dich\u00b7ter\u00b7Spiel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "In Unterth\u00e4nigkeit vor deine F\u00fcsse lege,", "tokens": ["In", "Un\u00b7ter\u00b7th\u00e4\u00b7nig\u00b7keit", "vor", "dei\u00b7ne", "F\u00fcs\u00b7se", "le\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.41": {"text": "Und keinen Zweifel nicht in meiner Seelen hege;", "tokens": ["Und", "kei\u00b7nen", "Zwei\u00b7fel", "nicht", "in", "mei\u00b7ner", "See\u00b7len", "he\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Als w\u00fcrdest du mein Blat verachten und verschm\u00e4hn.", "tokens": ["Als", "w\u00fcr\u00b7dest", "du", "mein", "Blat", "ver\u00b7ach\u00b7ten", "und", "ver\u00b7schm\u00e4hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PPOSAT", "NN", "VVFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Du wirst, ", "tokens": ["Du", "wirst", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.44": {"text": "Das Dir jetzt Ehrfurchts-voll dies Blat in Demuth sendet.", "tokens": ["Das", "Dir", "jetzt", "Ehr\u00b7furchts\u00b7voll", "dies", "Blat", "in", "De\u00b7muth", "sen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "NN", "PDS", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-++--", "measure": "unknown.measure.hexa"}, "line.45": {"text": "Du bist, ", "tokens": ["Du", "bist", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.46": {"text": "Kein Mensch ist in der Welt, der Dich nicht hoch verehrt.", "tokens": ["Kein", "Mensch", "ist", "in", "der", "Welt", ",", "der", "Dich", "nicht", "hoch", "ver\u00b7ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Ich kan Dir sonst, ", "tokens": ["Ich", "kan", "Dir", "sonst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.48": {"text": "Nichts als ein Ehrfurchts-Blat mit Reimen \u00fcbergeben.", "tokens": ["Nichts", "als", "ein", "Ehr\u00b7furchts\u00b7Blat", "mit", "Rei\u00b7men", "\u00fc\u00b7ber\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Verschm\u00e4h, ", "tokens": ["Ver\u00b7schm\u00e4h", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.50": {"text": "Ich bitte Demuths-voll: ", "tokens": ["Ich", "bit\u00b7te", "De\u00b7muths\u00b7voll", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.51": {"text": "Mit Gnaden auf dis Blat, so werd ich gl\u00fccklich heisen,", "tokens": ["Mit", "Gna\u00b7den", "auf", "dis", "Blat", ",", "so", "werd", "ich", "gl\u00fcck\u00b7lich", "hei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PDS", "NN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Und andre werden mich auch gleichfals gl\u00fccklich preisen.", "tokens": ["Und", "and\u00b7re", "wer\u00b7den", "mich", "auch", "gleich\u00b7fals", "gl\u00fcck\u00b7lich", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}