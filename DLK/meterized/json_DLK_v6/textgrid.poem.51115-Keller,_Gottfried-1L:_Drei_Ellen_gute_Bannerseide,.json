{"textgrid.poem.51115": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Drei Ellen gute Bannerseide,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drei Ellen gute Bannerseide,", "tokens": ["Drei", "El\u00b7len", "gu\u00b7te", "Ban\u00b7ner\u00b7sei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein H\u00e4uflein Volkes, ehrenwert,", "tokens": ["Ein", "H\u00e4uf\u00b7lein", "Vol\u00b7kes", ",", "eh\u00b7ren\u00b7wert", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit klarem Aug, im Sonntagskleide,", "tokens": ["Mit", "kla\u00b7rem", "Aug", ",", "im", "Sonn\u00b7tags\u00b7klei\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist alles, was mein Herz begehrt!", "tokens": ["Ist", "al\u00b7les", ",", "was", "mein", "Herz", "be\u00b7gehrt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So end ich mit der Morgenhelle", "tokens": ["So", "end", "ich", "mit", "der", "Mor\u00b7gen\u00b7hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Sommernacht beschr\u00e4nkte Ruh", "tokens": ["Der", "Som\u00b7mer\u00b7nacht", "be\u00b7schr\u00e4nk\u00b7te", "Ruh"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wandre rasch dem frischen Quelle", "tokens": ["Und", "wand\u00b7re", "rasch", "dem", "fri\u00b7schen", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der vaterl\u00e4nd'schen Freuden zu.", "tokens": ["Der", "va\u00b7ter\u00b7l\u00e4n\u00b7d'\u00b7schen", "Freu\u00b7den", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Schiffe fahren und die Wagen,", "tokens": ["Die", "Schif\u00b7fe", "fah\u00b7ren", "und", "die", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bekr\u00e4nzt, auf allen Pfaden her;", "tokens": ["Be\u00b7kr\u00e4nzt", ",", "auf", "al\u00b7len", "Pfa\u00b7den", "her", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die luft'ge Halle seh ich ragen,", "tokens": ["Die", "luft'\u00b7ge", "Hal\u00b7le", "seh", "ich", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Steinen nicht noch Sorgen schwer;", "tokens": ["Von", "Stei\u00b7nen", "nicht", "noch", "Sor\u00b7gen", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "ADV", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vom Rednersimse schimmert lieblich", "tokens": ["Vom", "Red\u00b7ner\u00b7sim\u00b7se", "schim\u00b7mert", "lieb\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des Festpokales Silberhort:", "tokens": ["Des", "Fest\u00b7po\u00b7ka\u00b7les", "Sil\u00b7ber\u00b7hort", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Heil uns, noch ist bei Freien \u00fcblich", "tokens": ["Heil", "uns", ",", "noch", "ist", "bei", "Frei\u00b7en", "\u00fcb\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "ADV", "VAFIN", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein leidenschaftlich freies Wort!", "tokens": ["Ein", "lei\u00b7den\u00b7schaft\u00b7lich", "frei\u00b7es", "Wort", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und Wort und Lied, von Mund zu Munde,", "tokens": ["Und", "Wort", "und", "Lied", ",", "von", "Mund", "zu", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Herz zu Herzen hallt es hin;", "tokens": ["Von", "Herz", "zu", "Her\u00b7zen", "hallt", "es", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So bl\u00fcht des Festes Rosenstunde", "tokens": ["So", "bl\u00fcht", "des", "Fes\u00b7tes", "Ro\u00b7sen\u00b7stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mu\u00df mit goldner Wende fliehn!", "tokens": ["Und", "mu\u00df", "mit", "gold\u00b7ner", "Wen\u00b7de", "fliehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und jede Pflicht hat sie erneuet,", "tokens": ["Und", "je\u00b7de", "Pflicht", "hat", "sie", "er\u00b7neu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und jede Kraft hat sie gest\u00e4hlt", "tokens": ["Und", "je\u00b7de", "Kraft", "hat", "sie", "ge\u00b7st\u00e4hlt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und eine K\u00f6rnersaat gestreuet,", "tokens": ["Und", "ei\u00b7ne", "K\u00f6r\u00b7ner\u00b7saat", "ge\u00b7streu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die nimmer ihre Frucht verhehlt.", "tokens": ["Die", "nim\u00b7mer", "ih\u00b7re", "Frucht", "ver\u00b7hehlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Drum weilet, wo im Feierkleide", "tokens": ["Drum", "wei\u00b7let", ",", "wo", "im", "Fei\u00b7er\u00b7klei\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "$,", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein r\u00fcstig Volk zum Feste geht", "tokens": ["Ein", "r\u00fcs\u00b7tig", "Volk", "zum", "Fes\u00b7te", "geht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und leis die feine Bannerseide", "tokens": ["Und", "leis", "die", "fei\u00b7ne", "Ban\u00b7ner\u00b7sei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hoch \u00fcber ihm zum Himmel weht!", "tokens": ["Hoch", "\u00fc\u00b7ber", "ihm", "zum", "Him\u00b7mel", "weht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In Vaterlandes Saus und Brause,", "tokens": ["In", "Va\u00b7ter\u00b7lan\u00b7des", "Saus", "und", "Brau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da ist die Freude s\u00fcndenrein,", "tokens": ["Da", "ist", "die", "Freu\u00b7de", "s\u00fcn\u00b7den\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und kehr nicht besser ich nach Hause,", "tokens": ["Und", "kehr", "nicht", "bes\u00b7ser", "ich", "nach", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADJD", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So werd ich auch nicht schlechter sein!", "tokens": ["So", "werd", "ich", "auch", "nicht", "schlech\u00b7ter", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}