{"textgrid.poem.58515": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "553. S\u00fc\u00dfigkeit aus dem Bittern", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach bete doch, ach schmiege dich, ach lerne s\u00fc\u00df dich schicken,", "tokens": ["Ach", "be\u00b7te", "doch", ",", "ach", "schmie\u00b7ge", "dich", ",", "ach", "ler\u00b7ne", "s\u00fc\u00df", "dich", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "$,", "ITJ", "VVFIN", "PPER", "$,", "ITJ", "VVFIN", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Wenn Freund' und Feinde \u00fcben dich und oft empfindlich dr\u00fccken!", "tokens": ["Wenn", "Freund'", "und", "Fein\u00b7de", "\u00fc\u00b7ben", "dich", "und", "oft", "emp\u00b7find\u00b7lich", "dr\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "PPER", "KON", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Du lebest noch, drum tut dir's weh; Gott braucht sie, liebe drum!", "tokens": ["Du", "le\u00b7best", "noch", ",", "drum", "tut", "dir's", "weh", ";", "Gott", "braucht", "sie", ",", "lie\u00b7be", "drum", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PAV", "VVFIN", "PIS", "PTKVZ", "$.", "NN", "VVFIN", "PPER", "$,", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Du denkst, du glaubst nicht, wie sie dich verwahr'n und machen fromm.", "tokens": ["Du", "denkst", ",", "du", "glaubst", "nicht", ",", "wie", "sie", "dich", "ver\u00b7wahr'n", "und", "ma\u00b7chen", "fromm", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "PRF", "VVINF", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}