{"textgrid.poem.46680": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[o du traurig winternde Thr\u00e4ne]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du traurig winternde Thr\u00e4ne,", "tokens": ["O", "du", "trau\u00b7rig", "win\u00b7tern\u00b7de", "Thr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Augenwimper-durchsinternde Thr\u00e4ne!", "tokens": ["Au\u00b7gen\u00b7wim\u00b7per\u00b7durch\u00b7sin\u00b7tern\u00b7de", "Thr\u00e4\u00b7ne", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Bald warmflutig die Schmerzerstarrung", "tokens": ["Bald", "warm\u00b7flu\u00b7tig", "die", "Schmer\u00b7zer\u00b7star\u00b7rung"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Am Einfrieren verhindernde Thr\u00e4ne!", "tokens": ["Am", "Ein\u00b7frie\u00b7ren", "ver\u00b7hin\u00b7dern\u00b7de", "Thr\u00e4\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Bald k\u00fchltropfig des Herzens hei\u00dfem", "tokens": ["Bald", "k\u00fchl\u00b7trop\u00b7fig", "des", "Her\u00b7zens", "hei\u00b7\u00dfem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Steine die Gluthen mindernde Thr\u00e4ne!", "tokens": ["Stei\u00b7ne", "die", "Glu\u00b7then", "min\u00b7dern\u00b7de", "Thr\u00e4\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Mir im Auge verwischest du theure", "tokens": ["Mir", "im", "Au\u00b7ge", "ver\u00b7wi\u00b7schest", "du", "theu\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "VVFIN", "PPER", "ADJA"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Bilder, o grausam lindernde Thr\u00e4ne!", "tokens": ["Bil\u00b7der", ",", "o", "grau\u00b7sam", "lin\u00b7dern\u00b7de", "Thr\u00e4\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "ADJD", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "O aus Kinderverlust entsprungne", "tokens": ["O", "aus", "Kin\u00b7der\u00b7ver\u00b7lust", "ent\u00b7sprung\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "NN", "VVFIN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Mich noch einmal entkindernde Thr\u00e4ne!", "tokens": ["Mich", "noch", "ein\u00b7mal", "ent\u00b7kin\u00b7dern\u00b7de", "Thr\u00e4\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}}}}