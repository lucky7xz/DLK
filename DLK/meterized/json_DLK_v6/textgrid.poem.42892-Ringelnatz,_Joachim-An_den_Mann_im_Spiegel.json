{"textgrid.poem.42892": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "An den Mann im Spiegel", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist ein krummer, dummer Hund!", "tokens": ["Du", "bist", "ein", "krum\u00b7mer", ",", "dum\u00b7mer", "Hund", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hast es doch so gut gehabt,", "tokens": ["Und", "hast", "es", "doch", "so", "gut", "ge\u00b7habt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bist gar nicht reich und bist gesund,", "tokens": ["Bist", "gar", "nicht", "reich", "und", "bist", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADJD", "KON", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch gro\u00dfenteils nicht unbegabt.", "tokens": ["Auch", "gro\u00b7\u00dfen\u00b7teils", "nicht", "un\u00b7be\u00b7gabt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du altes Schwein im Tr\u00fcffelbeet,", "tokens": ["Du", "al\u00b7tes", "Schwein", "im", "Tr\u00fcf\u00b7fel\u00b7beet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wei\u00dft du auch stets, wie gut's dir geht?", "tokens": ["Wei\u00dft", "du", "auch", "stets", ",", "wie", "gut's", "dir", "geht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "PWAV", "ADJA", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du, spring nicht \u00fcber Schranken,", "tokens": ["Du", ",", "spring", "nicht", "\u00fc\u00b7ber", "Schran\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die h\u00f6her, als du selbst bist, sind.", "tokens": ["Die", "h\u00f6\u00b7her", ",", "als", "du", "selbst", "bist", ",", "sind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "$,", "KOUS", "PPER", "ADV", "VAFIN", "$,", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vergi\u00df nie, t\u00e4glich wie ein Kind", "tokens": ["Ver\u00b7gi\u00df", "nie", ",", "t\u00e4g\u00b7lich", "wie", "ein", "Kind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$,", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr alles tief zu danken.", "tokens": ["F\u00fcr", "al\u00b7les", "tief", "zu", "dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}