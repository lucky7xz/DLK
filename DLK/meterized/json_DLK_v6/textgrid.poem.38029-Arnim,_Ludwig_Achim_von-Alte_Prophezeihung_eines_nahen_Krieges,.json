{"textgrid.poem.38029": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Alte Prophezeihung eines nahen Krieges,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wird am Sankt Mattheus Tag", "tokens": ["Es", "wird", "am", "Sankt", "Mat\u00b7theus", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "NE", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Sonne treten in die Wag',", "tokens": ["Die", "Son\u00b7ne", "tre\u00b7ten", "in", "die", "Wag'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des sey die Armuth recht beklagt;", "tokens": ["Des", "sey", "die", "Ar\u00b7muth", "recht", "be\u00b7klagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Friede wird ihr abgesagt,", "tokens": ["Der", "Frie\u00b7de", "wird", "ihr", "ab\u00b7ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und auch darzu dem ganzen Land.", "tokens": ["Und", "auch", "dar\u00b7zu", "dem", "gan\u00b7zen", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der alte Feind ist wohl bekannt,", "tokens": ["Der", "al\u00b7te", "Feind", "ist", "wohl", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er zieht daher von Mitternacht,", "tokens": ["Er", "zieht", "da\u00b7her", "von", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit gro\u00dfer Heereskraft und Macht,", "tokens": ["Mit", "gro\u00b7\u00dfer", "Hee\u00b7res\u00b7kraft", "und", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Quartier macht ihm ein wild Gesind,", "tokens": ["Quar\u00b7tier", "macht", "ihm", "ein", "wild", "Ge\u00b7sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "+-+-++-+", "measure": "unknown.measure.penta"}, "line.10": {"text": "Der kalte Regen der rauhe Wind,", "tokens": ["Der", "kal\u00b7te", "Re\u00b7gen", "der", "rau\u00b7he", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Dann fl\u00fcchte jeder in die Gruben", "tokens": ["Dann", "fl\u00fcch\u00b7te", "je\u00b7der", "in", "die", "Gru\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Kartoffeln, Kraut, gelb, wei\u00dfe Ruben.", "tokens": ["Kar\u00b7tof\u00b7feln", ",", "Kraut", ",", "gelb", ",", "wei\u00b7\u00dfe", "Ru\u00b7ben", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Mit Erd soll man die Reben decken,", "tokens": ["Mit", "Erd", "soll", "man", "die", "Re\u00b7ben", "de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PIS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Wenn Frosch und M\u00fccken sich verstecken;", "tokens": ["Wenn", "Frosch", "und", "M\u00fc\u00b7cken", "sich", "ver\u00b7ste\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Wenn Kr\u00f6t und Natter sich verkrochen", "tokens": ["Wenn", "Kr\u00f6t", "und", "Nat\u00b7ter", "sich", "ver\u00b7kro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Dann wird der wilde Feind anpochen.", "tokens": ["Dann", "wird", "der", "wil\u00b7de", "Feind", "an\u00b7po\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Vor ihm wird Storch und Schwalbe fliehen,", "tokens": ["Vor", "ihm", "wird", "Storch", "und", "Schwal\u00b7be", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.18": {"text": "Der tapfre Kranich weiter ziehen,", "tokens": ["Der", "tapf\u00b7re", "Kra\u00b7nich", "wei\u00b7ter", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Sein Vortrab k\u00f6mmt mit kalten Reifen,", "tokens": ["Sein", "Vor\u00b7trab", "k\u00f6mmt", "mit", "kal\u00b7ten", "Rei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Dann soll das Volk zur R\u00fcstung greifen,", "tokens": ["Dann", "soll", "das", "Volk", "zur", "R\u00fcs\u00b7tung", "grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Schnell Fenster und auch Ofen flicken,", "tokens": ["Schnell", "Fens\u00b7ter", "und", "auch", "O\u00b7fen", "fli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Die Stuben verstreichen und verzwiken,", "tokens": ["Die", "Stu\u00b7ben", "ver\u00b7strei\u00b7chen", "und", "ver\u00b7zwi\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Die Th\u00fcr mit Tuch und Filz beschlagen,", "tokens": ["Die", "Th\u00fcr", "mit", "Tuch", "und", "Filz", "be\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Die Federbetten herbei auch tragen,", "tokens": ["Die", "Fe\u00b7der\u00b7bet\u00b7ten", "her\u00b7bei", "auch", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Das Dach mit Stroh und Ziegel bessern,", "tokens": ["Das", "Dach", "mit", "Stroh", "und", "Zie\u00b7gel", "bes\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Kein Krebs mehr fangen in Gew\u00e4ssern,", "tokens": ["Kein", "Krebs", "mehr", "fan\u00b7gen", "in", "Ge\u00b7w\u00e4s\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Kein G'sell und Bub soll bei Ungnaden", "tokens": ["Kein", "G'\u00b7sell", "und", "Bub", "soll", "bei", "Ung\u00b7na\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN", "VMFIN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Sich f\u00fcrder mehr im Flusse baden.", "tokens": ["Sich", "f\u00fcr\u00b7der", "mehr", "im", "Flus\u00b7se", "ba\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Den Lustg\u00e4rtnern wird abgesagt,", "tokens": ["Den", "Lust\u00b7g\u00e4rt\u00b7nern", "wird", "ab\u00b7ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Barfus, Hemd\u00e4rmel wird verjagt,", "tokens": ["Bar\u00b7fus", ",", "Hemd\u00b7\u00e4r\u00b7mel", "wird", "ver\u00b7jagt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Die Nankinghosen ziehn ab zu Haufen,", "tokens": ["Die", "Nan\u00b7king\u00b7ho\u00b7sen", "ziehn", "ab", "zu", "Hau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "APPR", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "Die leinen Kittel auch entlaufen,", "tokens": ["Die", "lei\u00b7nen", "Kit\u00b7tel", "auch", "ent\u00b7lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Die Strohh\u00fct sind betr\u00fcbt und trauren,", "tokens": ["Die", "Stroh\u00b7h\u00fct", "sind", "be\u00b7tr\u00fcbt", "und", "trau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Und von den D\u00f6rfern knarrn die Bauren,", "tokens": ["Und", "von", "den", "D\u00f6r\u00b7fern", "knarrn", "die", "Bau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Auf Karren leis das Holz herzu;", "tokens": ["Auf", "Kar\u00b7ren", "leis", "das", "Holz", "her\u00b7zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Die K\u00f6hler halten auch kein Ruh,", "tokens": ["Die", "K\u00f6h\u00b7ler", "hal\u00b7ten", "auch", "kein", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Sie bringen gro\u00dfe Wagen voll Kohlen.", "tokens": ["Sie", "brin\u00b7gen", "gro\u00b7\u00dfe", "Wa\u00b7gen", "voll", "Koh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.38": {"text": "Dann zieht der Feind ganz unverholen", "tokens": ["Dann", "zieht", "der", "Feind", "ganz", "un\u00b7ver\u00b7ho\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Daher mit kaltschneidender Luft,", "tokens": ["Da\u00b7her", "mit", "kalt\u00b7schnei\u00b7den\u00b7der", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.40": {"text": "Wald und Heck stehn ganz in Duft,", "tokens": ["Wald", "und", "Heck", "stehn", "ganz", "in", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Ein Nacht schnell Wonn und Freud zerst\u00f6rt.", "tokens": ["Ein", "Nacht", "schnell", "Wonn", "und", "Freud", "zer\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Nun endlich wird das Volk emp\u00f6rt,", "tokens": ["Nun", "end\u00b7lich", "wird", "das", "Volk", "em\u00b7p\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Das Vieh fl\u00fcchten sie in die St\u00e4ll,", "tokens": ["Das", "Vieh", "fl\u00fcch\u00b7ten", "sie", "in", "die", "St\u00e4ll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.44": {"text": "Das Volk sich also wapnet schnell", "tokens": ["Das", "Volk", "sich", "al\u00b7so", "wap\u00b7net", "schnell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "ADV", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "In Pelz, Rauchm\u00fctzen und Filzsoken,", "tokens": ["In", "Pelz", ",", "Rauch\u00b7m\u00fct\u00b7zen", "und", "Filz\u00b7so\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.46": {"text": "Pelzschu, Handschu recht unerschrocken,", "tokens": ["Pelz\u00b7schu", ",", "Hand\u00b7schu", "recht", "un\u00b7er\u00b7schro\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "ADV", "ADJD", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.47": {"text": "Auch zieht es mit gro\u00dfem Heer", "tokens": ["Auch", "zieht", "es", "mit", "gro\u00b7\u00dfem", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.48": {"text": "Dem Winter zu thun Gegenwehr.", "tokens": ["Dem", "Win\u00b7ter", "zu", "thun", "Ge\u00b7gen\u00b7wehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Z\u00e4hnklappern, Zittern geht da los,", "tokens": ["Z\u00e4hn\u00b7klap\u00b7pern", ",", "Zit\u00b7tern", "geht", "da", "los", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Husch Husch, ist ein Geschreie gro\u00df,", "tokens": ["Husch", "Husch", ",", "ist", "ein", "Ge\u00b7schrei\u00b7e", "gro\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "In wei\u00df Montur kleidt sich der Nachen,", "tokens": ["In", "wei\u00df", "Mon\u00b7tur", "kleidt", "sich", "der", "Na\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.52": {"text": "In blau die M\u00e4uler, in roth die Nasen.", "tokens": ["In", "blau", "die", "M\u00e4u\u00b7ler", ",", "in", "roth", "die", "Na\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ART", "NN", "$,", "APPR", "ADJD", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.53": {"text": "Der Feind wirft einen gro\u00dfen Schnee,", "tokens": ["Der", "Feind", "wirft", "ei\u00b7nen", "gro\u00b7\u00dfen", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Sein Br\u00fccken schimmert auf Flu\u00df und See,", "tokens": ["S