{"textgrid.poem.62585": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Bu\u00df-Andacht \u00fcber den 6ten Psalm", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach Herr! gerechter Gott, mit sehnlicher Begier,", "tokens": ["Ach", "Herr", "!", "ge\u00b7rech\u00b7ter", "Gott", ",", "mit", "sehn\u00b7li\u00b7cher", "Be\u00b7gier", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "ADJD", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Andacht, Bu\u00df und Reu erscheine ich vor dir,", "tokens": ["Mit", "An\u00b7dacht", ",", "Bu\u00df", "und", "Reu", "er\u00b7schei\u00b7ne", "ich", "vor", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NE", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und bitte, straf mich nicht im Zorn nach meinen S\u00fcnden.", "tokens": ["Und", "bit\u00b7te", ",", "straf", "mich", "nicht", "im", "Zorn", "nach", "mei\u00b7nen", "S\u00fcn\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Herr! schone meiner Schuld", "tokens": ["Herr", "!", "scho\u00b7ne", "mei\u00b7ner", "Schuld"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit Gnade und Gedult,", "tokens": ["Mit", "Gna\u00b7de", "und", "Ge\u00b7dult", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und la\u00df mich deinen Zorn nicht nach Verdienst empfinden.", "tokens": ["Und", "la\u00df", "mich", "dei\u00b7nen", "Zorn", "nicht", "nach", "Ver\u00b7dienst", "emp\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PPOSAT", "NN", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Herr! z\u00fcchtge mich doch nicht in deinem grossen Grimm,", "tokens": ["Herr", "!", "z\u00fccht\u00b7ge", "mich", "doch", "nicht", "in", "dei\u00b7nem", "gros\u00b7sen", "Grimm", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "ADV", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sonst falle ich vor dir wie eine Blume \u00fcm,", "tokens": ["Sonst", "fal\u00b7le", "ich", "vor", "dir", "wie", "ei\u00b7ne", "Blu\u00b7me", "\u00fcm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "KOKOM", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und mu\u00df vor deiner Macht verderben und vergehen.", "tokens": ["Und", "mu\u00df", "vor", "dei\u00b7ner", "Macht", "ver\u00b7der\u00b7ben", "und", "ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PPOSAT", "NN", "VVFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vergieb mir meine S\u00fcnd,", "tokens": ["Ver\u00b7gieb", "mir", "mei\u00b7ne", "S\u00fcnd", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die deinen Zorn entz\u00fcndt,", "tokens": ["Die", "dei\u00b7nen", "Zorn", "ent\u00b7z\u00fcndt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und la\u00df mich deine Hand nicht f\u00fchlen, oder sehen.", "tokens": ["Und", "la\u00df", "mich", "dei\u00b7ne", "Hand", "nicht", "f\u00fch\u00b7len", ",", "o\u00b7der", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Herr! der du g\u00fctig heist! Herr! der du gn\u00e4dig bist,", "tokens": ["Herr", "!", "der", "du", "g\u00fc\u00b7tig", "heist", "!", "Herr", "!", "der", "du", "gn\u00e4\u00b7dig", "bist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "PPER", "ADJD", "VAFIN", "$.", "NN", "$.", "ART", "PPER", "ADJD", "VAFIN", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Sieh meine Seele an, die voll Betr\u00fcbni\u00df ist;", "tokens": ["Sieh", "mei\u00b7ne", "See\u00b7le", "an", ",", "die", "voll", "Be\u00b7tr\u00fcb\u00b7ni\u00df", "ist", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PTKVZ", "$,", "PRELS", "ADJD", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sieh Herr, wie schwach und matt ich jetzo vor dir liege.", "tokens": ["Sieh", "Herr", ",", "wie", "schwach", "und", "matt", "ich", "jet\u00b7zo", "vor", "dir", "lie\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "ADJD", "KON", "ADJD", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vergieb nach deiner Gnad", "tokens": ["Ver\u00b7gieb", "nach", "dei\u00b7ner", "Gnad"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mir meine Missethat;", "tokens": ["Mir", "mei\u00b7ne", "Mis\u00b7se\u00b7that", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sey gn\u00e4dig, da\u00df mein Geist bald wieder St\u00e4rke kriege.", "tokens": ["Sey", "gn\u00e4\u00b7dig", ",", "da\u00df", "mein", "Geist", "bald", "wie\u00b7der", "St\u00e4r\u00b7ke", "krie\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "ADV", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Herr! sieh mich g\u00fctig an! komm! heile meine Seel", "tokens": ["Herr", "!", "sieh", "mich", "g\u00fc\u00b7tig", "an", "!", "komm", "!", "hei\u00b7le", "mei\u00b7ne", "Seel"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "VVIMP", "PPER", "ADJD", "PTKVZ", "$.", "VVFIN", "$.", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In ihrem grossen Schmerz mit deinem Freuden-Oel!", "tokens": ["In", "ih\u00b7rem", "gros\u00b7sen", "Schmerz", "mit", "dei\u00b7nem", "Freu\u00b7den\u00b7O\u00b7el", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hilf meiner Schwachheit auf, denn ich bin sehr erschrocken.", "tokens": ["Hilf", "mei\u00b7ner", "Schwach\u00b7heit", "auf", ",", "denn", "ich", "bin", "sehr", "er\u00b7schro\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PTKVZ", "$,", "KON", "PPER", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zieh mich nicht vors Gericht,", "tokens": ["Zieh", "mich", "nicht", "vors", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Verstosse mich doch nicht;", "tokens": ["Ver\u00b7stos\u00b7se", "mich", "doch", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sieh, mein Gebeth sucht dir die Gnade abzulocken.", "tokens": ["Sieh", ",", "mein", "Ge\u00b7beth", "sucht", "dir", "die", "Gna\u00b7de", "ab\u00b7zu\u00b7lo\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ach wie erschrocken ist mein Herz, mein Geist und Sinn!", "tokens": ["Ach", "wie", "er\u00b7schro\u00b7cken", "ist", "mein", "Herz", ",", "mein", "Geist", "und", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "VVPP", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Angst und Bangigkeit wirft meine Seele hin,", "tokens": ["Die", "Angst", "und", "Ban\u00b7gig\u00b7keit", "wirft", "mei\u00b7ne", "See\u00b7le", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und seufzt und girrt und fleht nach deiner grossen Gnade.", "tokens": ["Und", "seufzt", "und", "girrt", "und", "fleht", "nach", "dei\u00b7ner", "gros\u00b7sen", "Gna\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Herr! sieh mich liebreich an;", "tokens": ["Herr", "!", "sieh", "mich", "lieb\u00b7reich", "an", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVIMP", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vergieb was ich gethan,", "tokens": ["Ver\u00b7gieb", "was", "ich", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Damit mir nicht der Bi\u00df der alten Schlange schade.", "tokens": ["Da\u00b7mit", "mir", "nicht", "der", "Bi\u00df", "der", "al\u00b7ten", "Schlan\u00b7ge", "scha\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ach du mein Lebens-F\u00fcrst! ach du mein Herr und Gott!", "tokens": ["Ach", "du", "mein", "Le\u00b7bens\u00b7F\u00fcrst", "!", "ach", "du", "mein", "Herr", "und", "Gott", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "PPOSAT", "NN", "$.", "XY", "PPER", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erbarm dich \u00fcber mich! ach sieh auf meine Noth!", "tokens": ["Er\u00b7barm", "dich", "\u00fc\u00b7ber", "mich", "!", "ach", "sieh", "auf", "mei\u00b7ne", "Noth", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PPER", "$.", "XY", "XY", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie lange wartest du? wie lange soll ich klagen?", "tokens": ["Wie", "lan\u00b7ge", "war\u00b7test", "du", "?", "wie", "lan\u00b7ge", "soll", "ich", "kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "$.", "PWAV", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie lange soll ich schreyn?", "tokens": ["Wie", "lan\u00b7ge", "soll", "ich", "schreyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach! komm und siehe drein!", "tokens": ["Ach", "!", "komm", "und", "sie\u00b7he", "drein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "KON", "VVIMP", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ach! komm! und la\u00df mich nicht in meiner Angst verzagen!", "tokens": ["Ach", "!", "komm", "!", "und", "la\u00df", "mich", "nicht", "in", "mei\u00b7ner", "Angst", "ver\u00b7za\u00b7gen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "$.", "KON", "VVIMP", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Herr! kehre deinen Zorn und deinen Grimm von hier;", "tokens": ["Herr", "!", "keh\u00b7re", "dei\u00b7nen", "Zorn", "und", "dei\u00b7nen", "Grimm", "von", "hier", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NE", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zeig mir dein Angesicht, und wende dich zu mir,", "tokens": ["Zeig", "mir", "dein", "An\u00b7ge\u00b7sicht", ",", "und", "wen\u00b7de", "dich", "zu", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PRF", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und sieh auf meinen Schmerz, und h\u00f6re auf mein Schreyen!", "tokens": ["Und", "sieh", "auf", "mei\u00b7nen", "Schmerz", ",", "und", "h\u00f6\u00b7re", "auf", "mein", "Schre\u00b7yen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Herr sey mir jetzt nicht fern!", "tokens": ["Herr", "sey", "mir", "jetzt", "nicht", "fern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du hilfst ja sonsten gern,", "tokens": ["Du", "hilfst", "ja", "sons\u00b7ten", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Drum wollest du mich auch mit deiner H\u00fclf erfreuen.", "tokens": ["Drum", "wol\u00b7lest", "du", "mich", "auch", "mit", "dei\u00b7ner", "H\u00fclf", "er\u00b7freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Du Allmachts-voller Gott! der du in Noth besch\u00fctzt,", "tokens": ["Du", "All\u00b7machts\u00b7vol\u00b7ler", "Gott", "!", "der", "du", "in", "Noth", "be\u00b7sch\u00fctzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "ART", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hilf meinem armen Geist, der in Betr\u00fcbni\u00df sitzt,", "tokens": ["Hilf", "mei\u00b7nem", "ar\u00b7men", "Geist", ",", "der", "in", "Be\u00b7tr\u00fcb\u00b7ni\u00df", "sitzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN",