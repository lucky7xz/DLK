{"textgrid.poem.38930": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie die Schatten gehn und kommen", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie die Schatten gehn und kommen", "tokens": ["Wie", "die", "Schat\u00b7ten", "gehn", "und", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVINF", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Sonne wechselnd blicket,", "tokens": ["Und", "die", "Son\u00b7ne", "wech\u00b7selnd", "bli\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist die trunkne Flur entz\u00fccket,", "tokens": ["Ist", "die", "trunk\u00b7ne", "Flur", "ent\u00b7z\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch von Schatten \u00fcberschwommen", "tokens": ["Doch", "von", "Schat\u00b7ten", "\u00fc\u00b7bersc\u00b7hwom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist der Glanz hinweggenommen", "tokens": ["Ist", "der", "Glanz", "hin\u00b7weg\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und es bleibt ein ernstes Gr\u00fcn:", "tokens": ["Und", "es", "bleibt", "ein", "erns\u00b7tes", "Gr\u00fcn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Also auch mein Herz und Sinn,", "tokens": ["Al\u00b7so", "auch", "mein", "Herz", "und", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Freude bald und stille Schmerzen", "tokens": ["Freu\u00b7de", "bald", "und", "stil\u00b7le", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wechseln im verborgnen Herzen,", "tokens": ["Wech\u00b7seln", "im", "ver\u00b7borg\u00b7nen", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wandeln her und wandeln hin.", "tokens": ["Wan\u00b7deln", "her", "und", "wan\u00b7deln", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Ist es Trauer? ist es Freude?", "tokens": ["Ist", "es", "Trau\u00b7er", "?", "ist", "es", "Freu\u00b7de", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$.", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Nein, es ist ein s\u00fc\u00df Ermatten,", "tokens": ["Nein", ",", "es", "ist", "ein", "s\u00fc\u00df", "Er\u00b7mat\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Wie das K\u00fchl' im Waldesschatten,", "tokens": ["Wie", "das", "K\u00fchl'", "im", "Wal\u00b7des\u00b7schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Wie die Blumen auf der Haide", "tokens": ["Wie", "die", "Blu\u00b7men", "auf", "der", "Hai\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Wenn sie mit begl\u00e4nztem Kleide", "tokens": ["Wenn", "sie", "mit", "be\u00b7gl\u00e4nz\u00b7tem", "Klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Ungewi\u00df im Strome spiegeln:", "tokens": ["Un\u00b7ge\u00b7wi\u00df", "im", "Stro\u00b7me", "spie\u00b7geln", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Wie von waldumwachsnen H\u00fcgeln", "tokens": ["Wie", "von", "wald\u00b7um\u00b7wachs\u00b7nen", "H\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Heimlich eine Quelle springt,", "tokens": ["Heim\u00b7lich", "ei\u00b7ne", "Quel\u00b7le", "springt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Ungesehn durch B\u00fcsche dringt", "tokens": ["Un\u00b7ge\u00b7sehn", "durch", "B\u00fc\u00b7sche", "dringt"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Mit kristallnen weichen Fl\u00fcgeln.", "tokens": ["Mit", "kris\u00b7tall\u00b7nen", "wei\u00b7chen", "Fl\u00fc\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Seht! wie s\u00fc\u00df der Fr\u00fchling pranget,", "tokens": ["Seht", "!", "wie", "s\u00fc\u00df", "der", "Fr\u00fch\u00b7ling", "pran\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PWAV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Wie die lauen L\u00fcfte spielen", "tokens": ["Wie", "die", "lau\u00b7en", "L\u00fcf\u00b7te", "spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "In bewegten Blumen w\u00fchlen,", "tokens": ["In", "be\u00b7weg\u00b7ten", "Blu\u00b7men", "w\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Wie der Baum voll Bl\u00fchten hanget,", "tokens": ["Wie", "der", "Baum", "voll", "Bl\u00fch\u00b7ten", "han\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Und den Schmetterling verlanget", "tokens": ["Und", "den", "Schmet\u00b7ter\u00b7ling", "ver\u00b7lan\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Und die Biene nach dem Glanze,", "tokens": ["Und", "die", "Bie\u00b7ne", "nach", "dem", "Glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Und die Wiese w\u00e4chst zum Kranze,", "tokens": ["Und", "die", "Wie\u00b7se", "w\u00e4chst", "zum", "Kran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Und die kleinen blauen Quellen", "tokens": ["Und", "die", "klei\u00b7nen", "blau\u00b7en", "Quel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Rennen mit den lust'gen Wellen", "tokens": ["Ren\u00b7nen", "mit", "den", "lust'\u00b7gen", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Eilig, eilig, wie zum Tanze.", "tokens": ["Ei\u00b7lig", ",", "ei\u00b7lig", ",", "wie", "zum", "Tan\u00b7ze", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "PWAV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Und die Waldung rauschet s\u00fc\u00dfe,", "tokens": ["Und", "die", "Wal\u00b7dung", "rau\u00b7schet", "s\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Alle gr\u00fcnen Bl\u00e4tter regen", "tokens": ["Al\u00b7le", "gr\u00fc\u00b7nen", "Bl\u00e4t\u00b7ter", "re\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Zur Umarmung sich entgegen,", "tokens": ["Zur", "Um\u00b7ar\u00b7mung", "sich", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "T\u00f6nen nur und fl\u00fcstern K\u00fcsse,", "tokens": ["T\u00f6\u00b7nen", "nur", "und", "fl\u00fcs\u00b7tern", "K\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Laut verk\u00fcnden die Gen\u00fcsse", "tokens": ["Laut", "ver\u00b7k\u00fcn\u00b7den", "die", "Ge\u00b7n\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Alle V\u00f6gel aus dem Wald,", "tokens": ["Al\u00b7le", "V\u00f6\u00b7gel", "aus", "dem", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Und das gr\u00fcne Dickicht schallt", "tokens": ["Und", "das", "gr\u00fc\u00b7ne", "Di\u00b7ckicht", "schallt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Von den Nachtigallges\u00e4ngen,", "tokens": ["Von", "den", "Nach\u00b7ti\u00b7gall\u00b7ge\u00b7s\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Da\u00df den wollustvollen Kl\u00e4ngen", "tokens": ["Da\u00df", "den", "wol\u00b7lust\u00b7vol\u00b7len", "Kl\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Rings das Echo wiederhallt.", "tokens": ["Rings", "das", "E\u00b7cho", "wie\u00b7der\u00b7hallt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Sind die Blumen nicht wie Sterne", "tokens": ["Sind", "die", "Blu\u00b7men", "nicht", "wie", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "KOKOM", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "In das gr\u00fcne Gras gesunken?", "tokens": ["In", "das", "gr\u00fc\u00b7ne", "Gras", "ge\u00b7sun\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Locken sie den Blick nicht trunken", "tokens": ["Lo\u00b7cken", "sie", "den", "Blick", "nicht", "trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Nach dem lichten Brande gerne?", "tokens": ["Nach", "dem", "lich\u00b7ten", "Bran\u00b7de", "ger\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Alles ist so nah und ferne;", "tokens": ["Al\u00b7les", "ist", "so", "nah", "und", "fer\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADJD", "KON", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "M\u00f6cht ich nicht, mich zu begl\u00fccken,", "tokens": ["M\u00f6cht", "ich", "nicht", ",", "mich", "zu", "be\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "An die Brust den Fr\u00fchling dr\u00fccken?", "tokens": ["An", "die", "Brust", "den", "Fr\u00fch\u00b7ling", "dr\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Und ihm sagen, wie ich f\u00fchle,", "tokens": ["Und", "ihm", "sa\u00b7gen", ",", "wie", "ich", "f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Da\u00df er diese Sehnsucht k\u00fchle,", "tokens": ["Da\u00df", "er", "die\u00b7se", "Sehn\u00b7sucht", "k\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Oder ende dies Entz\u00fccken.", "tokens": ["O\u00b7der", "en\u00b7de", "dies", "Ent\u00b7z\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PDS", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}