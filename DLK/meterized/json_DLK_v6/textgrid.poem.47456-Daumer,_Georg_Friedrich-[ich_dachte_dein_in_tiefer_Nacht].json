{"textgrid.poem.47456": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ich dachte dein in tiefer Nacht]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich dachte dein in tiefer Nacht;", "tokens": ["Ich", "dach\u00b7te", "dein", "in", "tie\u00b7fer", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da leuchtete mit heller Macht,", "tokens": ["Da", "leuch\u00b7te\u00b7te", "mit", "hel\u00b7ler", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit pl\u00f6tzlicher, die Finsterni\u00df,", "tokens": ["Mit", "pl\u00f6tz\u00b7li\u00b7cher", ",", "die", "Fins\u00b7ter\u00b7ni\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJD", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wurde klar, wie Morgenpracht.", "tokens": ["Und", "wur\u00b7de", "klar", ",", "wie", "Mor\u00b7gen\u00b7pracht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "$,", "PWAV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zu jener Stunde hat gewi\u00df,", "tokens": ["Zu", "je\u00b7ner", "Stun\u00b7de", "hat", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Auge, Liebchen, auch gewacht,", "tokens": ["Dein", "Au\u00b7ge", ",", "Lieb\u00b7chen", ",", "auch", "ge\u00b7wacht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu jener Stunde hat gewi\u00df,", "tokens": ["Zu", "je\u00b7ner", "Stun\u00b7de", "hat", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Liebe mein dein Herz gedacht.", "tokens": ["In", "Lie\u00b7be", "mein", "dein", "Herz", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}