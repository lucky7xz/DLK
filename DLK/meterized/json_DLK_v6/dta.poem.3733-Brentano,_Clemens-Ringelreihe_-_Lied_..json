{"dta.poem.3733": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Ringelreihe - Lied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ringel, Ringel, Reihe!               ", "tokens": ["Rin\u00b7gel", ",", "Rin\u00b7gel", ",", "Rei\u00b7he", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sind der Kinder dreie,", "tokens": ["Sind", "der", "Kin\u00b7der", "drei\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "CARD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sitzen auf dem Holderbusch,", "tokens": ["Sit\u00b7zen", "auf", "dem", "Hol\u00b7der\u00b7busch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schreien alle musch, musch, musch,", "tokens": ["Schrei\u00b7en", "al\u00b7le", "musch", ",", "musch", ",", "musch", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sitzt nieder.", "tokens": ["Sitzt", "nie\u00b7der", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Sitzt ne Frau im Ringelein,", "tokens": ["Sitzt", "ne", "Frau", "im", "Rin\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit sieben kleine Kinderlein,", "tokens": ["Mit", "sie\u00b7ben", "klei\u00b7ne", "Kin\u00b7der\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Was essen's gern?", "tokens": ["Was", "es\u00b7sen's", "gern", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Fischlein.", "tokens": ["Fisc\u00b7hlein", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Was trinken's gern?", "tokens": ["Was", "trin\u00b7ken's", "gern", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Rothen Wein.", "tokens": ["Ro\u00b7then", "Wein", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Sitzt nieder.", "tokens": ["Sitzt", "nie\u00b7der", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}