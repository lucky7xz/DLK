{"textgrid.poem.47325": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "6. Das ist dein Amt", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leucht', o flammendes Sonnenaug', \u00fcber die Welt; das ist dein Amt.", "tokens": ["Leucht'", ",", "o", "flam\u00b7men\u00b7des", "Son\u00b7ne\u00b7naug'", ",", "\u00fc\u00b7ber", "die", "Welt", ";", "das", "ist", "dein", "Amt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$.", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Lenz! mit bl\u00fchendem Rosentraum schm\u00fccke das Feld; das ist dein Amt.", "tokens": ["Lenz", "!", "mit", "bl\u00fc\u00b7hen\u00b7dem", "Ro\u00b7sen\u00b7traum", "schm\u00fc\u00b7cke", "das", "Feld", ";", "das", "ist", "dein", "Amt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$.", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Mond am Himmel! o schlafe nicht! denn hier auf Erden wollen sein", "tokens": ["Mond", "am", "Him\u00b7mel", "!", "o", "schla\u00b7fe", "nicht", "!", "denn", "hier", "auf", "Er\u00b7den", "wol\u00b7len", "sein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "$.", "FM", "VVFIN", "PTKNEG", "$.", "KON", "ADV", "APPR", "NN", "VMFIN", "PPOSAT"], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.4": {"text": "Liebesn\u00e4chte von deinem Strahl lieblich erhellt; das ist dein Amt.", "tokens": ["Lie\u00b7bes\u00b7n\u00e4ch\u00b7te", "von", "dei\u00b7nem", "Strahl", "lieb\u00b7lich", "er\u00b7hellt", ";", "das", "ist", "dein", "Amt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ADJD", "VVFIN", "$.", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.5": {"text": "Sing', o liebende Nachtigall, was du von Rosen-Sch\u00f6nheit wei\u00dft,", "tokens": ["Sing'", ",", "o", "lie\u00b7ben\u00b7de", "Nach\u00b7ti\u00b7gall", ",", "was", "du", "von", "Ro\u00b7sen\u00b7Sch\u00f6n\u00b7heit", "wei\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "ADJA", "NN", "$,", "PWS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.6": {"text": "Sing' und stirb im Gesang, zu Sang bist du bestellt; das ist dein Amt.", "tokens": ["Sing'", "und", "stirb", "im", "Ge\u00b7sang", ",", "zu", "Sang", "bist", "du", "be\u00b7stellt", ";", "das", "ist", "dein", "Amt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "APPRART", "NN", "$,", "APPR", "NN", "VAFIN", "PPER", "VVFIN", "$.", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.7": {"text": "Thr\u00e4ne meines verlassnen Augs! f\u00fcr ein geliebtes Bild, das hier", "tokens": ["Thr\u00e4\u00b7ne", "mei\u00b7nes", "ver\u00b7lass\u00b7nen", "Augs", "!", "f\u00fcr", "ein", "ge\u00b7lieb\u00b7tes", "Bild", ",", "das", "hier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$.", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ADV"], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.8": {"text": "Soll einkehren, mit duft'gem Flor schm\u00fccke das Zelt; das ist dein Amt.", "tokens": ["Soll", "ein\u00b7keh\u00b7ren", ",", "mit", "duft'\u00b7gem", "Flor", "schm\u00fc\u00b7cke", "das", "Zelt", ";", "das", "ist", "dein", "Amt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$.", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+-++--+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.9": {"text": "Bild der Sch\u00f6nheit! mit Himmelsglanz allen in Nacht Versunkenen", "tokens": ["Bild", "der", "Sch\u00f6n\u00b7heit", "!", "mit", "Him\u00b7mels\u00b7glanz", "al\u00b7len", "in", "Nacht", "Ver\u00b7sun\u00b7ke\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "APPR", "NN", "PIAT", "APPR", "NN", "NN"], "meter": "+-+--+--+--+-+--", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Vorzuleuchten, dazu hat uns Gott dich gesellt; das ist dein Amt.", "tokens": ["Vor\u00b7zu\u00b7leuch\u00b7ten", ",", "da\u00b7zu", "hat", "uns", "Gott", "dich", "ge\u00b7sellt", ";", "das", "ist", "dein", "Amt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PAV", "VAFIN", "PPER", "NN", "PPER", "VVPP", "$.", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+---+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Sag' zu deinem verkl\u00e4rten Blick: lege die goldne R\u00fcstung an,", "tokens": ["Sag'", "zu", "dei\u00b7nem", "ver\u00b7kl\u00e4r\u00b7ten", "Blick", ":", "le\u00b7ge", "die", "gold\u00b7ne", "R\u00fcs\u00b7tung", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$.", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.12": {"text": "Gr\u00fcnd' auf Erden der Liebe Reich, leuchtender Held! das ist dein Amt.", "tokens": ["Gr\u00fcnd'", "auf", "Er\u00b7den", "der", "Lie\u00b7be", "Reich", ",", "leuch\u00b7ten\u00b7der", "Held", "!", "das", "ist", "dein", "Amt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "NN", "NN", "$,", "ADJA", "NN", "$.", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "--+--+-++--+-+-+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Zu dem Bogen der Braue sprich: spanne dich stolz, da\u00df Pfeil auf Pfeil", "tokens": ["Zu", "dem", "Bo\u00b7gen", "der", "Brau\u00b7e", "sprich", ":", "span\u00b7ne", "dich", "stolz", ",", "da\u00df", "Pfeil", "auf", "Pfeil"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ADJD", "$.", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "NN", "APPR", "NN"], "meter": "--+--+--+--+-+-+", "measure": "anapaest.tetra.plus"}, "line.14": {"text": "Auf rebellischer Herzen Trotz werde geschnellt; das ist dein Amt.", "tokens": ["Auf", "re\u00b7bel\u00b7li\u00b7scher", "Her\u00b7zen", "Trotz", "wer\u00b7de", "ge\u00b7schnellt", ";", "das", "ist", "dein", "Amt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "VAFIN", "VVPP", "$.", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+-+---+-+-+", "measure": "trochaic.septa.relaxed"}, "line.15": {"text": "Da\u00df du flatternde Locke mich Flatternden fingest, dank' ich dir;", "tokens": ["Da\u00df", "du", "flat\u00b7tern\u00b7de", "Lo\u00b7cke", "mich", "Flat\u00b7tern\u00b7den", "fin\u00b7gest", ",", "dank'", "ich", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "PPER", "NN", "VVFIN", "$,", "VVFIN", "PPER", "PPER", "$."], "meter": "--+--+--+--+-+-+", "measure": "anapaest.tetra.plus"}, "line.16": {"text": "Immer neu sei dein reizendes Netz mir gestellt; das ist dein Amt.", "tokens": ["Im\u00b7mer", "neu", "sei", "dein", "rei\u00b7zen\u00b7des", "Netz", "mir", "ge\u00b7stellt", ";", "das", "ist", "dein", "Amt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "ADJA", "NN", "PPER", "VVPP", "$.", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.17": {"text": "O mein t\u00f6nendes Saitenspiel! weil das Geschick in meine Hand", "tokens": ["O", "mein", "t\u00f6\u00b7nen\u00b7des", "Sai\u00b7ten\u00b7spiel", "!", "weil", "das", "Ge\u00b7schick", "in", "mei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$.", "KOUS", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.18": {"text": "Dich gegeben, von Liebeshauch T\u00f6ne-geschwellt; das ist dein Amt.", "tokens": ["Dich", "ge\u00b7ge\u00b7ben", ",", "von", "Lie\u00b7bes\u00b7hauch", "T\u00f6\u00b7ne\u00b7ge\u00b7schwellt", ";", "das", "ist", "dein", "Amt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "APPR", "NN", "NN", "$.", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.19": {"text": "Lenk', o r\u00fcstiger Steuermann, diesen verlornen Nachen durch", "tokens": ["Lenk'", ",", "o", "r\u00fcs\u00b7ti\u00b7ger", "Steu\u00b7er\u00b7mann", ",", "die\u00b7sen", "ver\u00b7lor\u00b7nen", "Na\u00b7chen", "durch"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "FM", "ADJA", "NN", "$,", "PDAT", "ADJA", "NN", "APPR"], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.20": {"text": "Klipp' und Brandung und Wogendrang, bis er zerschellt; das ist dein Amt.", "tokens": ["Klipp'", "und", "Bran\u00b7dung", "und", "Wo\u00b7gen\u00b7drang", ",", "bis", "er", "zer\u00b7schellt", ";", "das", "ist", "dein", "Amt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "KON", "NN", "$,", "KOUS", "PPER", "VVPP", "$.", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.21": {"text": "La\u00df die heuchlerisch dumpfe Welt scheitern an ihrer Eigensucht.", "tokens": ["La\u00df", "die", "heuch\u00b7le\u00b7risch", "dump\u00b7fe", "Welt", "schei\u00b7tern", "an", "ih\u00b7rer", "Ei\u00b7gen\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJD", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "--+--+-++-+--+-+", "measure": "anapaest.di.plus"}, "line.22": {"text": "Lieb' aufrichtig und trink, Hafis! schw\u00e4rm' unverstellt; das ist dein Amt.", "tokens": ["Lieb'", "auf\u00b7rich\u00b7tig", "und", "trink", ",", "Ha\u00b7fis", "!", "schw\u00e4rm'", "un\u00b7ver\u00b7stellt", ";", "das", "ist", "dein", "Amt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "VVFIN", "$,", "NE", "$.", "VVFIN", "ADJD", "$.", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+--++--+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}}}}}