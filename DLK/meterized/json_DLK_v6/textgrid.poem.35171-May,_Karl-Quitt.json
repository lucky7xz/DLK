{"textgrid.poem.35171": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Quitt", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O, lacht doch nicht! Treibt ja nicht euern Spott", "tokens": ["O", ",", "lacht", "doch", "nicht", "!", "Treibt", "ja", "nicht", "eu\u00b7ern", "Spott"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "ADV", "PTKNEG", "$.", "VVFIN", "ADV", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit unserm Glauben, denn ihr l\u00e4stert Gott.", "tokens": ["Mit", "un\u00b7serm", "Glau\u00b7ben", ",", "denn", "ihr", "l\u00e4s\u00b7tert", "Gott", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht diesem Glauben, Gott gilt euer Lachen;", "tokens": ["Nicht", "die\u00b7sem", "Glau\u00b7ben", ",", "Gott", "gilt", "eu\u00b7er", "La\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PDAT", "NN", "$,", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich sage euch: Ihr habt es quitt zu machen!", "tokens": ["Ich", "sa\u00b7ge", "euch", ":", "Ihr", "habt", "es", "quitt", "zu", "ma\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O, lacht doch nicht! Treibt ja nicht euern Spott", "tokens": ["O", ",", "lacht", "doch", "nicht", "!", "Treibt", "ja", "nicht", "eu\u00b7ern", "Spott"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "ADV", "PTKNEG", "$.", "VVFIN", "ADV", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit unsrer Liebe, denn ihr l\u00e4stert Gott.", "tokens": ["Mit", "uns\u00b7rer", "Lie\u00b7be", ",", "denn", "ihr", "l\u00e4s\u00b7tert", "Gott", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein solches Lachen kann nur Schmerz bereiten;", "tokens": ["Ein", "sol\u00b7ches", "La\u00b7chen", "kann", "nur", "Schmerz", "be\u00b7rei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VMFIN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich sage euch: Ihr habt es quitt zu leiden!", "tokens": ["Ich", "sa\u00b7ge", "euch", ":", "Ihr", "habt", "es", "quitt", "zu", "lei\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O, lacht doch nicht! Treibt ja nicht euern Spott", "tokens": ["O", ",", "lacht", "doch", "nicht", "!", "Treibt", "ja", "nicht", "eu\u00b7ern", "Spott"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "ADV", "PTKNEG", "$.", "VVFIN", "ADV", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit unsrer Hoffnung, denn ihr l\u00e4stert Gott.", "tokens": ["Mit", "uns\u00b7rer", "Hoff\u00b7nung", ",", "denn", "ihr", "l\u00e4s\u00b7tert", "Gott", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dies Lachen wird einst theuer euch erscheinen;", "tokens": ["Dies", "La\u00b7chen", "wird", "einst", "theu\u00b7er", "euch", "er\u00b7schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "ADV", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich sage euch: Ihr habt es quitt zu weinen!", "tokens": ["Ich", "sa\u00b7ge", "euch", ":", "Ihr", "habt", "es", "quitt", "zu", "wei\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}