{"textgrid.poem.60392": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der Schmerz", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie lie\u00df sich \u00fcberraschen", "tokens": ["Sie", "lie\u00df", "sich", "\u00fc\u00b7berr\u00b7a\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von diesem Trauerwort,", "tokens": ["Von", "die\u00b7sem", "Trau\u00b7er\u00b7wort", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ihre Tr\u00e4nen waschen", "tokens": ["Und", "ih\u00b7re", "Tr\u00e4\u00b7nen", "wa\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die rote Schminke fort.", "tokens": ["Die", "ro\u00b7te", "Schmin\u00b7ke", "fort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das Leben t\u00e4uscht uns lange,", "tokens": ["Das", "Le\u00b7ben", "t\u00e4uscht", "uns", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du zeigst der Schminke bar", "tokens": ["Du", "zeigst", "der", "Schmin\u00b7ke", "bar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Lebens welke Wange,", "tokens": ["Des", "Le\u00b7bens", "wel\u00b7ke", "Wan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PWAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O Schmerz; wie bist du wahr!", "tokens": ["O", "Schmerz", ";", "wie", "bist", "du", "wahr", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAV", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}