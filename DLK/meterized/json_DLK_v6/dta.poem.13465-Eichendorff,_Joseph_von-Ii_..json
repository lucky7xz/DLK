{"dta.poem.13465": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Ii .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Er reitet Nachts auf einem braunen Ro\u00df,", "tokens": ["Er", "rei\u00b7tet", "Nachts", "auf", "ei\u00b7nem", "brau\u00b7nen", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er reitet vor\u00fcber an manchem Schlo\u00df:", "tokens": ["Er", "rei\u00b7tet", "vor\u00b7\u00fc\u00b7ber", "an", "man\u00b7chem", "Schlo\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Schlaf' droben, mein Kind, bis der Tag erscheint,", "tokens": ["Schlaf'", "dro\u00b7ben", ",", "mein", "Kind", ",", "bis", "der", "Tag", "er\u00b7scheint", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die finstre Nacht ist des Menschen Feind!", "tokens": ["Die", "finst\u00b7re", "Nacht", "ist", "des", "Men\u00b7schen", "Feind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Er reitet vor\u00fcber an einem Teich,", "tokens": ["Er", "rei\u00b7tet", "vor\u00b7\u00fc\u00b7ber", "an", "ei\u00b7nem", "Teich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da stehet ein sch\u00f6nes M\u00e4dchen bleich", "tokens": ["Da", "ste\u00b7het", "ein", "sch\u00f6\u00b7nes", "M\u00e4d\u00b7chen", "bleich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und singt, ihr Hemdlein flattert im Wind:", "tokens": ["Und", "singt", ",", "ihr", "Hemd\u00b7lein", "flat\u00b7tert", "im", "Wind", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Vor\u00fcber, vor\u00fcber, mir graut vor dem Kind!", "tokens": ["Vor\u00b7\u00fc\u00b7ber", ",", "vor\u00b7\u00fc\u00b7ber", ",", "mir", "graut", "vor", "dem", "Kind", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Er reitet vor\u00fcber an einem Flu\u00df,", "tokens": ["Er", "rei\u00b7tet", "vor\u00b7\u00fc\u00b7ber", "an", "ei\u00b7nem", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da ruft ihm der Wassermann seinen Gru\u00df,", "tokens": ["Da", "ruft", "ihm", "der", "Was\u00b7ser\u00b7mann", "sei\u00b7nen", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Taucht wieder unter dann mit Gesaus,", "tokens": ["Taucht", "wie\u00b7der", "un\u00b7ter", "dann", "mit", "Ge\u00b7saus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Und stille wird's \u00fcber dem k\u00fchlen Haus.", "tokens": ["Und", "stil\u00b7le", "wird's", "\u00fc\u00b7ber", "dem", "k\u00fch\u00b7len", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VAFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Wenn Tag und Nacht in verworrenem Streit,", "tokens": ["Wenn", "Tag", "und", "Nacht", "in", "ver\u00b7wor\u00b7re\u00b7nem", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schon H\u00e4hne kr\u00e4hen in D\u00f6rfern weit,", "tokens": ["Schon", "H\u00e4h\u00b7ne", "kr\u00e4\u00b7hen", "in", "D\u00f6r\u00b7fern", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da schauert sein Ro\u00df und w\u00fchlet hinab,", "tokens": ["Da", "schau\u00b7ert", "sein", "Ro\u00df", "und", "w\u00fch\u00b7let", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Scharret ihm schnaubend sein eigenes Grab.", "tokens": ["Schar\u00b7ret", "ihm", "schnau\u00b7bend", "sein", "ei\u00b7ge\u00b7nes", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}