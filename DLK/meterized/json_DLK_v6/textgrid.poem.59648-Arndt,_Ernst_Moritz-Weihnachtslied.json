{"textgrid.poem.59648": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Weihnachtslied", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erklinge, Lied, und werde Schall,", "tokens": ["Er\u00b7klin\u00b7ge", ",", "Lied", ",", "und", "wer\u00b7de", "Schall", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "KON", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kling gleich der hellsten Nachtigall,", "tokens": ["Kling", "gleich", "der", "hells\u00b7ten", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kling gleich dem hellsten Lerchenklang", "tokens": ["Kling", "gleich", "dem", "hells\u00b7ten", "Ler\u00b7chen\u00b7klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die ganze, weite Welt entlang.", "tokens": ["Die", "gan\u00b7ze", ",", "wei\u00b7te", "Welt", "ent\u00b7lang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kling, Lied, und kling im h\u00f6chsten Ton:", "tokens": ["Kling", ",", "Lied", ",", "und", "kling", "im", "h\u00f6chs\u00b7ten", "Ton", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es kommt der s\u00fc\u00dfe Gottessohn,", "tokens": ["Es", "kommt", "der", "s\u00fc\u00b7\u00dfe", "Got\u00b7tes\u00b7sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es kommt das helle Himmelskind", "tokens": ["Es", "kommt", "das", "hel\u00b7le", "Him\u00b7mel\u00b7skind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hernieder, wo die S\u00fcnder sind.", "tokens": ["Her\u00b7nie\u00b7der", ",", "wo", "die", "S\u00fcn\u00b7der", "sind", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er kehrt bei einer Jungfrau ein,", "tokens": ["Er", "kehrt", "bei", "ei\u00b7ner", "Jung\u00b7frau", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will eines Weibes S\u00e4ugling sein,", "tokens": ["Will", "ei\u00b7nes", "Wei\u00b7bes", "S\u00e4ug\u00b7ling", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der gro\u00dfe Herr der ganzen Welt,", "tokens": ["Der", "gro\u00b7\u00dfe", "Herr", "der", "gan\u00b7zen", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein W\u00fcrmlein auf die Erde f\u00e4llt.", "tokens": ["Ein", "W\u00fcrm\u00b7lein", "auf", "die", "Er\u00b7de", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein armes Kn\u00e4blein nackt und blo\u00df,", "tokens": ["Ein", "ar\u00b7mes", "Kn\u00e4\u00b7blein", "nackt", "und", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So liegt er in Marias Scho\u00df;", "tokens": ["So", "liegt", "er", "in", "Ma\u00b7rias", "Scho\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der alle Sterne lenken kann,", "tokens": ["Der", "al\u00b7le", "Ster\u00b7ne", "len\u00b7ken", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fleht eines Weibes Gnade an.", "tokens": ["Fleht", "ei\u00b7nes", "Wei\u00b7bes", "Gna\u00b7de", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der eh'r als Erd' und Himmel war,", "tokens": ["Der", "eh'r", "als", "Erd'", "und", "Him\u00b7mel", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "KOUS", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Wort des Vaters rein und klar,", "tokens": ["Das", "Wort", "des", "Va\u00b7ters", "rein", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Spricht lieb und freundlich bei uns ein", "tokens": ["Spricht", "lieb", "und", "freund\u00b7lich", "bei", "uns", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "APPR", "PPER", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und will der S\u00fcnder Bruder sein.", "tokens": ["Und", "will", "der", "S\u00fcn\u00b7der", "Bru\u00b7der", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So kommt die unerme\u00dfne Huld,", "tokens": ["So", "kommt", "die", "un\u00b7er\u00b7me\u00df\u00b7ne", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu tragen unsre schwere Schuld,", "tokens": ["Zu", "tra\u00b7gen", "uns\u00b7re", "schwe\u00b7re", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die ewige Liebe steigt von Gott", "tokens": ["Die", "e\u00b7wi\u00b7ge", "Lie\u00b7be", "steigt", "von", "Gott"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zu uns herab f\u00fcr Schmach und Spott.", "tokens": ["Zu", "uns", "her\u00b7ab", "f\u00fcr", "Schmach", "und", "Spott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Des solln wir alle fr\u00f6hlich sein", "tokens": ["Des", "solln", "wir", "al\u00b7le", "fr\u00f6h\u00b7lich", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "VMFIN", "PPER", "PIAT", "ADJD", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und singen mit den Engelein", "tokens": ["Und", "sin\u00b7gen", "mit", "den", "En\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und singen mit der Hirten Schar:", "tokens": ["Und", "sin\u00b7gen", "mit", "der", "Hir\u00b7ten", "Schar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Des solln wir alle fr\u00f6hlich sein,", "tokens": ["Des", "solln", "wir", "al\u00b7le", "fr\u00f6h\u00b7lich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VMFIN", "PPER", "PIAT", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Gott will unser Vater sein,", "tokens": ["Da\u00df", "Gott", "will", "un\u00b7ser", "Va\u00b7ter", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VMFIN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und da\u00df der s\u00fc\u00dfe Jesus Christ", "tokens": ["Und", "da\u00df", "der", "s\u00fc\u00b7\u00dfe", "Je\u00b7sus", "Christ"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Heut unser Bruder worden ist.", "tokens": ["Heut", "un\u00b7ser", "Bru\u00b7der", "wor\u00b7den", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}