{"dta.poem.2895": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "96.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.57", "en:0.42"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein ist nicht, was du hast; das was du thuest, ist", "tokens": ["Dein", "ist", "nicht", ",", "was", "du", "hast", ";", "das", "was", "du", "thu\u00b7est", ",", "ist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "VAFIN", "PTKNEG", "$,", "PWS", "PPER", "VAFIN", "$.", "PDS", "PRELS", "PPER", "VVFIN", "$,", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mehr dein; am meisten dein scheint, was du selber bist.", "tokens": ["Mehr", "dein", ";", "am", "meis\u00b7ten", "dein", "scheint", ",", "was", "du", "sel\u00b7ber", "bist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "$.", "PTKA", "PIS", "PPOSAT", "VVFIN", "$,", "PWS", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch bist du, was du bist, am wenigsten durch dich;", "tokens": ["Doch", "bist", "du", ",", "was", "du", "bist", ",", "am", "we\u00b7nigs\u00b7ten", "durch", "dich", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "$,", "PWS", "PPER", "VAFIN", "$,", "APPRART", "PIS", "APPR", "PPER", "$."], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.2": {"text": "Was, dich zu r\u00fchmen, bleibt dir eignes also? sprich!", "tokens": ["Was", ",", "dich", "zu", "r\u00fch\u00b7men", ",", "bleibt", "dir", "eig\u00b7nes", "al\u00b7so", "?", "sp