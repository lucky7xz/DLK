{"dta.poem.12855": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Gedancken bey einem gemahlten  \n Cupido mit einer verloschenen fackel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jst dir, o Liebe! doch die fackel ausgegangen:", "tokens": ["Ist", "dir", ",", "o", "Lie\u00b7be", "!", "doch", "die", "fa\u00b7ckel", "aus\u00b7ge\u00b7gan\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "FM", "NN", "$.", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat irgend Daphne dir den possen angethan?", "tokens": ["Hat", "ir\u00b7gend", "Daph\u00b7ne", "dir", "den", "pos\u00b7sen", "an\u00b7ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Geh, z\u00fcnde sie alsbald von ihren augen an!", "tokens": ["Geh", ",", "z\u00fcn\u00b7de", "sie", "als\u00b7bald", "von", "ih\u00b7ren", "au\u00b7gen", "an", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vielleichte kanst du noch dein hohes ziel erlangen;", "tokens": ["Viel\u00b7leich\u00b7te", "kanst", "du", "noch", "dein", "ho\u00b7hes", "ziel", "er\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Allein du folgest nicht, ich finde kein geh\u00f6r:", "tokens": ["Al\u00b7lein", "du", "fol\u00b7gest", "nicht", ",", "ich", "fin\u00b7de", "kein", "ge\u00b7h\u00f6r", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du l\u00e4ssest dich den blitz in ihren augen schrecken;", "tokens": ["Du", "l\u00e4s\u00b7sest", "dich", "den", "blitz", "in", "ih\u00b7ren", "au\u00b7gen", "schre\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch komme nicht zu mir, die fackel anzustecken,", "tokens": ["Doch", "kom\u00b7me", "nicht", "zu", "mir", ",", "die", "fa\u00b7ckel", "an\u00b7zu\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "PPER", "$,", "PRELS", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was asche worden ist, das giebt kein feuer mehr.", "tokens": ["Was", "asc\u00b7he", "wor\u00b7den", "ist", ",", "das", "giebt", "kein", "feu\u00b7er", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "VAPP", "VAFIN", "$,", "PDS", "VVFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}