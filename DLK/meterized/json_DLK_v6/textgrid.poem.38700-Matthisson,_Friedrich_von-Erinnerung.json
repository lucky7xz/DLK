{"textgrid.poem.38700": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Erinnerung", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Rosenschimmer leuchtet den Tag zur Ruh;", "tokens": ["Kein", "Ro\u00b7sen\u00b7schim\u00b7mer", "leuch\u00b7tet", "den", "Tag", "zur", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Abendnebel schwillt am Gestad' empor,", "tokens": ["Der", "A\u00b7bend\u00b7ne\u00b7bel", "schwillt", "am", "Ge\u00b7stad'", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wo durch verdorrte Felsengr\u00e4ser", "tokens": ["Wo", "durch", "ver\u00b7dorr\u00b7te", "Fel\u00b7sen\u00b7gr\u00e4\u00b7ser"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sterbender L\u00fcfte Ges\u00e4usel wandelt.", "tokens": ["Ster\u00b7ben\u00b7der", "L\u00fcf\u00b7te", "Ge\u00b7s\u00e4u\u00b7sel", "wan\u00b7delt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Nicht schwermutsvoller bebte des Herbstes Wehn", "tokens": ["Nicht", "schwer\u00b7muts\u00b7vol\u00b7ler", "beb\u00b7te", "des", "Herbs\u00b7tes", "Wehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Durchs todte Gras am sinkenden Rasenmal,", "tokens": ["Durchs", "tod\u00b7te", "Gras", "am", "sin\u00b7ken\u00b7den", "Ra\u00b7sen\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wo meines Jugendlieblings Asche", "tokens": ["Wo", "mei\u00b7nes", "Ju\u00b7gend\u00b7lieb\u00b7lings", "A\u00b7sche"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Unter der weinenden Weide schlummert.", "tokens": ["Un\u00b7ter", "der", "wei\u00b7nen\u00b7den", "Wei\u00b7de", "schlum\u00b7mert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Ihm Thr\u00e4nen opfern werd' ich beim Bl\u00e4tterfall,", "tokens": ["Ihm", "Thr\u00e4\u00b7nen", "op\u00b7fern", "werd'", "ich", "beim", "Bl\u00e4t\u00b7ter\u00b7fall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ihm, wann das Mailaub wieder den Hain umrauscht,", "tokens": ["Ihm", ",", "wann", "das", "Mai\u00b7laub", "wie\u00b7der", "den", "Hain", "um\u00b7rauscht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAV", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bis du dem freien Geist, o Erde,", "tokens": ["Bis", "du", "dem", "frei\u00b7en", "Geist", ",", "o", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Freundlich im Reigen der Sterne schimmerst.", "tokens": ["Freund\u00b7lich", "im", "Rei\u00b7gen", "der", "Ster\u00b7ne", "schim\u00b7merst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}