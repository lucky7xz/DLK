{"dta.poem.4023": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die K\u00fcrze .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewarum bist du so kurz? liebst du wie vormals", "tokens": ["\u201e", "wa\u00b7rum", "bist", "du", "so", "kurz", "?", "liebst", "du", "wie", "vor\u00b7mals"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "VAFIN", "PPER", "ADV", "ADJD", "$.", "VVFIN", "PPER", "KOKOM", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "denn", "tokens": ["denn"], "token_info": ["word"], "pos": ["KON"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "\u201enun nicht mehr den Gesang? fand'st du als", "tokens": ["\u201e", "nun", "nicht", "mehr", "den", "Ge\u00b7sang", "?", "fan\u00b7d'st", "du", "als"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADV", "PTKNEG", "ADV", "ART", "NN", "$.", "VVFIN", "PPER", "KOKOM"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "J\u00fcngling doch", "tokens": ["J\u00fcng\u00b7ling", "doch"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "\u201din den Tagen der Hoffnung,", "tokens": ["\"", "in", "den", "Ta\u00b7gen", "der", "Hoff\u00b7nung", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "\u201dwenn du sangest, das Ende nie?&#8220;", "tokens": ["\"", "wenn", "du", "san\u00b7gest", ",", "das", "En\u00b7de", "nie", "?", "&#8220;"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "XML_entity"], "pos": ["$(", "KOUS", "PPER", "VVFIN", "$,", "ART", "NN", "ADV", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Wie mein Gl\u00fcck ist mein Lied. \u2014 Willst du im", "tokens": ["Wie", "mein", "Gl\u00fcck", "ist", "mein", "Lied", ".", "Willst", "du", "im"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$.", "$(", "VMFIN", "PPER", "APPRART"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Abendroth", "tokens": ["A\u00b7ben\u00b7droth"], "token_info": ["word"], "pos": ["NN"], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "Froh dich baden? Hinweg ist's und die Erd' ist kalt,", "tokens": ["Froh", "dich", "ba\u00b7den", "?", "Hin\u00b7weg", "ist's", "und", "die", "Erd'", "ist", "kalt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVINF", "$.", "ADV", "VAFIN", "KON", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Und der Vogel der Nacht schwirrt", "tokens": ["Und", "der", "Vo\u00b7gel", "der", "Nacht", "schwirrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Unbequem vor das Auge dir.", "tokens": ["Un\u00b7be\u00b7quem", "vor", "das", "Au\u00b7ge", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}