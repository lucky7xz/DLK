{"textgrid.poem.35617": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Gesang des Abgeschiedenen", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Voll Harmonien ist der Flug der V\u00f6gel. Es haben die gr\u00fcnen W\u00e4lder", "tokens": ["Voll", "Har\u00b7mo\u00b7ni\u00b7en", "ist", "der", "Flug", "der", "V\u00f6\u00b7gel", ".", "Es", "ha\u00b7ben", "die", "gr\u00fc\u00b7nen", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VAFIN", "ART", "NN", "ART", "NN", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+--+--+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Am Abend sich zu stilleren H\u00fctten versammelt;", "tokens": ["Am", "A\u00b7bend", "sich", "zu", "stil\u00b7le\u00b7ren", "H\u00fct\u00b7ten", "ver\u00b7sam\u00b7melt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die kristallenen Weiden des Rehs.", "tokens": ["Die", "kris\u00b7tal\u00b7le\u00b7nen", "Wei\u00b7den", "des", "Rehs", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Dunkles bes\u00e4nftigt das Pl\u00e4tschern des Bachs, die feuchten Schatten", "tokens": ["Dunk\u00b7les", "be\u00b7s\u00e4nf\u00b7tigt", "das", "Pl\u00e4t\u00b7schern", "des", "Bachs", ",", "die", "feuch\u00b7ten", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+--+--+--+-+-+-", "measure": "elegiambus"}}, "stanza.2": {"line.1": {"text": "Und die Blumen des Sommers, die sch\u00f6n im Winde l\u00e4uten.", "tokens": ["Und", "die", "Blu\u00b7men", "des", "Som\u00b7mers", ",", "die", "sch\u00f6n", "im", "Win\u00b7de", "l\u00e4u\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "--+--+--+-+-+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Schon d\u00e4mmert die Stirne dem sinnenden Menschen.", "tokens": ["Schon", "d\u00e4m\u00b7mert", "die", "Stir\u00b7ne", "dem", "sin\u00b7nen\u00b7den", "Men\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Und es leuchtet ein L\u00e4mpchen, das Gute, in seinem Herzen", "tokens": ["Und", "es", "leuch\u00b7tet", "ein", "L\u00e4mp\u00b7chen", ",", "das", "Gu\u00b7te", ",", "in", "sei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "--+--+--+--+-+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Und der Frieden des Mahls; denn geheiligt ist Brot und Wein", "tokens": ["Und", "der", "Frie\u00b7den", "des", "Mahls", ";", "denn", "ge\u00b7hei\u00b7ligt", "ist", "Brot", "und", "Wein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "$.", "KON", "VVPP", "VAFIN", "NN", "KON", "NN"], "meter": "--+--+--+--+-+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Von Gottes H\u00e4nden, und es schaut aus n\u00e4chtigen Augen", "tokens": ["Von", "Got\u00b7tes", "H\u00e4n\u00b7den", ",", "und", "es", "schaut", "aus", "n\u00e4ch\u00b7ti\u00b7gen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "$,", "KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Stille dich der Bruder an, da\u00df er ruhe von dorniger Wanderschaft.", "tokens": ["Stil\u00b7le", "dich", "der", "Bru\u00b7der", "an", ",", "da\u00df", "er", "ru\u00b7he", "von", "dor\u00b7ni\u00b7ger", "Wan\u00b7der\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "PTKVZ", "$,", "KOUS", "PPER", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.5": {"text": "O das Wohnen in der beseelten Bl\u00e4ue der Nacht.", "tokens": ["O", "das", "Woh\u00b7nen", "in", "der", "be\u00b7seel\u00b7ten", "Bl\u00e4u\u00b7e", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+--+-+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Liebend auch umf\u00e4ngt das Schweigen im Zimmer die Schatten der Alten,", "tokens": ["Lie\u00b7bend", "auch", "um\u00b7f\u00e4ngt", "das", "Schwei\u00b7gen", "im", "Zim\u00b7mer", "die", "Schat\u00b7ten", "der", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+--+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Die purpurnen Martern, Klage eines gro\u00dfen Geschlechts,", "tokens": ["Die", "pur\u00b7pur\u00b7nen", "Mar\u00b7tern", ",", "Kla\u00b7ge", "ei\u00b7nes", "gro\u00b7\u00dfen", "Ge\u00b7schlechts", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-++-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.3": {"text": "Das fromm nun hingeht im einsamen Enkel.", "tokens": ["Das", "fromm", "nun", "hin\u00b7geht", "im", "ein\u00b7sa\u00b7men", "En\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADV", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-++-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Denn strahlender immer erwacht aus schwarzen Minuten des Wahnsinns", "tokens": ["Denn", "strah\u00b7len\u00b7der", "im\u00b7mer", "er\u00b7wacht", "aus", "schwar\u00b7zen", "Mi\u00b7nu\u00b7ten", "des", "Wahn\u00b7sinns"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVPP", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+--+--+-+--+--+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der Duldende an versteinerter Schwelle", "tokens": ["Der", "Dul\u00b7den\u00b7de", "an", "ver\u00b7stei\u00b7ner\u00b7ter", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und es umf\u00e4ngt ihn gewaltig die k\u00fchle Bl\u00e4ue und die leuchtende Neige des Herbstes,", "tokens": ["Und", "es", "um\u00b7f\u00e4ngt", "ihn", "ge\u00b7wal\u00b7tig", "die", "k\u00fch\u00b7le", "Bl\u00e4u\u00b7e", "und", "die", "leuch\u00b7ten\u00b7de", "Nei\u00b7ge", "des", "Herbs\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "---+--+--+-+-+-+--+--+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.6": {"line.1": {"text": "Das stille Haus und die Sagen des Waldes,", "tokens": ["Das", "stil\u00b7le", "Haus", "und", "die", "Sa\u00b7gen", "des", "Wal\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ma\u00df und Gesetz und die mondenen Pfade der Abgeschiedenen.", "tokens": ["Ma\u00df", "und", "Ge\u00b7setz", "und", "die", "mon\u00b7de\u00b7nen", "Pfa\u00b7de", "der", "Ab\u00b7ge\u00b7schie\u00b7de\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+--+--+-+--", "measure": "dactylic.tetra.plus"}}}}}