"token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "VVFIN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "un de't Melkfeewer \u0153werstahn nich kann,", "tokens": ["un", "de't", "Mel\u00b7kfee\u00b7wer", "\u0153wer\u00b7stahn", "nich", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "PTKNEG", "VMFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "tosam dor\u0153wer breckt un starben m\u00f6t", "tokens": ["to\u00b7sam", "dor\u0153wer", "breckt", "un", "star\u00b7ben", "m\u00f6t"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "VVFIN", "FM", "VVFIN", "NE"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.13": {"text": "un selig, still un gottgef\u00e4llig starwt:", "tokens": ["un", "se\u00b7lig", ",", "still", "un", "gott\u00b7ge\u00b7f\u00e4l\u00b7lig", "starwt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "ADJD", "$,", "FM", "FM", "ADJD", "VVFIN", "$."], "meter": "+--++--+-+", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Se weet, ehr Kind dat \u0153werl\u00e4wt ehr doch,", "tokens": ["Se", "weet", ",", "ehr", "Kind", "dat", "\u0153wer\u00b7l\u00e4wt", "ehr", "doch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "KOUS", "NN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "dat liggt vull L\u00e4ben in sin B\u00fcndeln dor,", "tokens": ["dat", "liggt", "vull", "L\u00e4\u00b7ben", "in", "sin", "B\u00fcn\u00b7deln", "dor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "as inne warme Weeg, in Fack un Timmer.", "tokens": ["as", "in\u00b7ne", "war\u00b7me", "Weeg", ",", "in", "Fack", "un", "Tim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "APPR", "NN", "FM", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "De Kronen trecken hoch dor \u0153wer'n See.", "tokens": ["De", "Kro\u00b7nen", "tre\u00b7cken", "hoch", "dor", "\u0153wer'n", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ADJD", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Klamm h\u00e4ngt dat Lof un matt an Telg un Twig;", "tokens": ["Klamm", "h\u00e4ngt", "dat", "Lof", "un", "matt", "an", "Telg", "un", "Twig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "FM", "FM", "APPR", "NN", "FM", "NE", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "de heel oll Wold steiht irnst un stumm un still,", "tokens": ["de", "heel", "oll", "Wold", "steiht", "irnst", "un", "stumm", "un", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "so still un trurig dor un in sick kihrt", "tokens": ["so", "still", "un", "tru\u00b7rig", "dor", "un", "in", "sick", "kihrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "FM", "FM", "FM", "FM", "FM", "FM", "VVFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "as een, up den mit eens dat Ungl\u00fcck kem", "tokens": ["as", "e\u00b7en", ",", "up", "den", "mit", "e\u00b7ens", "dat", "Un\u00b7gl\u00fcck", "kem"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVPP", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "un de nu noch sick nich to faten weet.", "tokens": ["un", "de", "nu", "noch", "sick", "nich", "to", "fa\u00b7ten", "weet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "ADV", "ADV", "VVIMP", "PTKNEG", "ADV", "VVINF", "VVFIN", "$."], "meter": "+----+-+-+", "measure": "dactylic.init"}, "line.7": {"text": "De S\u00fck is braken in sin hoges Hus;", "tokens": ["De", "S\u00fck", "is", "bra\u00b7ken", "in", "sin", "ho\u00b7ges", "Hus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "sin Kinner fallen v\u00f6r em een bi een", "tokens": ["sin", "Kin\u00b7ner", "fal\u00b7len", "v\u00f6r", "em", "e\u00b7en", "bi", "e\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+----", "measure": "unknown.measure.tetra"}, "line.9": {"text": "un ligg' nu stif, verschr\u00fcmpt as Liken vor,", "tokens": ["un", "lig\u00b7g'", "nu", "stif", ",", "ver\u00b7schr\u00fcmpt", "as", "Li\u00b7ken", "vor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "VVFIN", "ADV", "VVFIN", "$,", "VVFIN", "NE", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.10": {"text": "de unnod un vull Elenn storben s\u00fcnd.", "tokens": ["de", "un\u00b7nod", "un", "vull", "E\u00b7lenn", "stor\u00b7ben", "s\u00fcnd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Un wu sonn arm l\u00fctt Blatt noch gr\u00f6n sick h\u00f6llt,", "tokens": ["Un", "wu", "sonn", "arm", "l\u00fctt", "Blatt", "noch", "gr\u00f6n", "sick", "h\u00f6llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "ADJD", "VVFIN", "NN", "ADV", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "dat s\u00f6cht so \u00e4ngstlich nah de S\u00fcnn, as wenn't", "tokens": ["dat", "s\u00f6cht", "so", "\u00e4ngst\u00b7lich", "nah", "de", "S\u00fcnn", ",", "as", "wenn't"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "ADJD", "ADJD", "NE", "NE", "$,", "FM.la", "FM.la"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "verklagen wull de b\u00f6s' oll Nacht bi ehr,", "tokens": ["ver\u00b7kla\u00b7gen", "wull", "de", "b\u00f6s'", "oll", "Nacht", "bi", "ehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Ruhrip un Frost un Hagel, Storm un Wind,", "tokens": ["Ruh\u00b7rip", "un", "Frost", "un", "Ha\u00b7gel", ",", "Storm", "un", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "NN", "$,", "FM", "FM", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.15": {"text": "de em so n\u00fcrig nah de G\u00f6rgel fat'", "tokens": ["de", "em", "so", "n\u00fc\u00b7rig", "nah", "de", "G\u00f6r\u00b7gel", "fat'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.da", "FM.da", "ADV", "ADJD", "ADJD", "NE", "NE", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "as hungrig W\u00fclw dat Aulamm in de H\u00f6rd.", "tokens": ["as", "hung\u00b7rig", "W\u00fclw", "dat", "Au\u00b7lamm", "in", "de", "H\u00f6rd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "De Kronen trecken hoch dor \u0153wer'n See.", "tokens": ["De", "Kro\u00b7nen", "tre\u00b7cken", "hoch", "dor", "\u0153wer'n", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ADJD", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "De Grund de dampt, un de grag Dak de stiggt", "tokens": ["De", "Grund", "de", "dampt", ",", "un", "de", "grag", "Dak", "de", "stiggt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "FM", "FM", "FM", "FM", "FM", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "hochup un treckt dor \u0153wer'n Hellbarg hen,", "tokens": ["hoc\u00b7hup", "un", "treckt", "dor", "\u0153wer'n", "Hell\u00b7barg", "hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxe