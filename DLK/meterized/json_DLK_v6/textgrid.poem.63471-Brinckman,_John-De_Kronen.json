{"textgrid.poem.63471": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "De Kronen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "De Kronen trecken hoch dor \u0153wer'n See,", "tokens": ["De", "Kro\u00b7nen", "tre\u00b7cken", "hoch", "dor", "\u0153wer'n", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ADJD", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "de Metten fl\u00fcggt un spinnt de Stoppel in;", "tokens": ["de", "Met\u00b7ten", "fl\u00fcggt", "un", "spinnt", "de", "Stop\u00b7pel", "in", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "so witt is dor dat Feld vun, as wir't Rip", "tokens": ["so", "witt", "is", "dor", "dat", "Feld", "vun", ",", "as", "wir't", "Rip"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "FM", "NE", "ART", "NN", "VVINF", "$,", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "un Frost un Snee. Dat bl\u00e4nkert in de S\u00fcnn,", "tokens": ["un", "Frost", "un", "Snee", ".", "Dat", "bl\u00e4n\u00b7kert", "in", "de", "S\u00fcnn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "NN", "$.", "NN", "VVFIN", "APPR", "NE", "NE", "$,"], "meter": "+++--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "grad as witt Sid deit anne Dodenkron,", "tokens": ["grad", "as", "witt", "Sid", "deit", "an\u00b7ne", 