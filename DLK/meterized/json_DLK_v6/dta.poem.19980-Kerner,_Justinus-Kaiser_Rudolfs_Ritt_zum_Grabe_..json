{"dta.poem.19980": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Kaiser Rudolfs Ritt zum Grabe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf der Burg zu Germersheim,", "tokens": ["Auf", "der", "Burg", "zu", "Ger\u00b7mers\u00b7heim", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Stark am Geist, am Leibe schwach,", "tokens": ["Stark", "am", "Geist", ",", "am", "Lei\u00b7be", "schwach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sitzt der greise Kaiser Rudolf", "tokens": ["Sitzt", "der", "grei\u00b7se", "Kai\u00b7ser", "Ru\u00b7dolf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spielend das gewohnte Schach.", "tokens": ["Spie\u00b7lend", "das", "ge\u00b7wohn\u00b7te", "Schach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und er spricht: \u201eihr guten Meister!", "tokens": ["Und", "er", "spricht", ":", "\u201e", "ihr", "gu\u00b7ten", "Meis\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "$(", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aerzte! sagt mir ohne Zagen:", "tokens": ["A\u00b7erz\u00b7te", "!", "sagt", "mir", "oh\u00b7ne", "Za\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wann aus dem zerbrochenen Leib", "tokens": ["Wann", "aus", "dem", "zer\u00b7bro\u00b7che\u00b7nen", "Leib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Wird der Geist zu Gott getragen?\u201c", "tokens": ["Wird", "der", "Geist", "zu", "Gott", "ge\u00b7tra\u00b7gen", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und die Meister sprechen: \u201eHerr!", "tokens": ["Und", "die", "Meis\u00b7ter", "spre\u00b7chen", ":", "\u201e", "Herr", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$.", "$(", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohl noch heut erscheint die Stunde.\u201c", "tokens": ["Wohl", "noch", "heut", "er\u00b7scheint", "die", "Stun\u00b7de", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Freundlich l\u00e4chelnd spricht der Greis:", "tokens": ["Freund\u00b7lich", "l\u00e4\u00b7chelnd", "spricht", "der", "Greis", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201emeister! Dank f\u00fcr diese Kunde!\u201c", "tokens": ["\u201e", "meis\u00b7ter", "!", "Dank", "f\u00fcr", "die\u00b7se", "Kun\u00b7de", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$.", "NN", "APPR", "PDAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u201eauf nach Speyer! auf nach Speyer!\u201c", "tokens": ["\u201e", "auf", "nach", "Spey\u00b7er", "!", "auf", "nach", "Spey\u00b7er", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "APPR", "NE", "$.", "APPR", "APPR", "NE", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruft er, als das Spiel geendet,", "tokens": ["Ruft", "er", ",", "als", "das", "Spiel", "ge\u00b7en\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201ewo so mancher deutsche Held", "tokens": ["\u201e", "wo", "so", "man\u00b7cher", "deut\u00b7sche", "Held"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "ADV", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201eliegt begraben, sey's vollendet!", "tokens": ["\u201e", "liegt", "be\u00b7gra\u00b7ben", ",", "sey's", "voll\u00b7en\u00b7det", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "VVPP", "$,", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u201eblast die H\u00f6rner! bringt das Ro\u00df,", "tokens": ["\u201e", "blast", "die", "H\u00f6r\u00b7ner", "!", "bringt", "das", "Ro\u00df", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ART", "NN", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das mich oft zur Schlacht getragen!\u201c", "tokens": ["Das", "mich", "oft", "zur", "Schlacht", "ge\u00b7tra\u00b7gen", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "PPER", "ADV", "APPRART", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zaudernd stehn die Diener all',", "tokens": ["Zau\u00b7dernd", "stehn", "die", "Die\u00b7ner", "all'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PIS", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch er ruft: \u201efolgt ohne Zagen!\u201c", "tokens": ["Doch", "er", "ruft", ":", "\u201e", "folgt", "oh\u00b7ne", "Za\u00b7gen", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "$(", "VVFIN", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und das Schlachtro\u00df wird gebracht.", "tokens": ["Und", "das", "Schlach\u00b7tro\u00df", "wird", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u201enicht zum Kampf, zum ew'gen Frieden,", "tokens": ["\u201e", "nicht", "zum", "Kampf", ",", "zum", "ew'\u00b7gen", "Frie\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Spricht er, \u201etrage, treuer Freund!", "tokens": ["Spricht", "er", ",", "\u201e", "tra\u00b7ge", ",", "treu\u00b7er", "Freund", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "$(", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201ejezt den Herrn, den Lebensm\u00fcden!\u201c", "tokens": ["\u201e", "jezt", "den", "Herrn", ",", "den", "Le\u00b7bens\u00b7m\u00fc\u00b7den", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ART", "NN", "$,", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Weinend steht der Diener Schaar,", "tokens": ["Wei\u00b7nend", "steht", "der", "Die\u00b7ner", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Als der Greis auf hohem Rosse,", "tokens": ["Als", "der", "Greis", "auf", "ho\u00b7hem", "Ros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rechts und links ein Kapellan,", "tokens": ["Rechts", "und", "links", "ein", "Ka\u00b7pel\u00b7lan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zieht halb Leich' aus seinem Schlosse.", "tokens": ["Zieht", "halb", "Leich'", "aus", "sei\u00b7nem", "Schlos\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Traurend neigt des Schlosses Lind'", "tokens": ["Trau\u00b7rend", "neigt", "des", "Schlos\u00b7ses", "Lind'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor ihm ihre Aeste nieder,", "tokens": ["Vor", "ihm", "ih\u00b7re", "A\u00b7es\u00b7te", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "V\u00f6gel, die in ihrer Hut,", "tokens": ["V\u00f6\u00b7gel", ",", "die", "in", "ih\u00b7rer", "Hut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Singen wehmuthsvolle Lieder.", "tokens": ["Sin\u00b7gen", "weh\u00b7muths\u00b7vol\u00b7le", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Mancher eilt des Wegs daher,", "tokens": ["Man\u00b7cher", "eilt", "des", "Wegs", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der geh\u00f6rt die bange Sage,", "tokens": ["Der", "ge\u00b7h\u00f6rt", "die", "ban\u00b7ge", "Sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieht des Helden sterbend Bild", "tokens": ["Sieht", "des", "Hel\u00b7den", "ster\u00b7bend", "Bild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und bricht aus in laute Klage.", "tokens": ["Und", "bricht", "aus", "in", "lau\u00b7te", "Kla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Aber nur von Himmelslust", "tokens": ["A\u00b7ber", "nur", "von", "Him\u00b7mels\u00b7lust"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Spricht der Greis mit jenen Zweyen,", "tokens": ["Spricht", "der", "Greis", "mit", "je\u00b7nen", "Zwe\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "L\u00e4chelnd blickt sein Angesicht", "tokens": ["L\u00e4\u00b7chelnd", "blickt", "sein", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als ritt er zur Lust in Mayen.", "tokens": ["Als", "ritt", "er", "zur", "Lust", "in", "Ma\u00b7yen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPRART", "NN", "APPR", "NE", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.11": {"line.1": {"text": "Von dem hohen Dom zu Speyer", "tokens": ["Von", "dem", "ho\u00b7hen", "Dom", "zu", "Spey\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rt man dumpf die Glocken schallen.", "tokens": ["H\u00f6rt", "man", "dumpf", "die", "Glo\u00b7cken", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ritter, B\u00fcrger, zarte Fraun,", "tokens": ["Rit\u00b7ter", ",", "B\u00fcr\u00b7ger", ",", "zar\u00b7te", "Fraun", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weinend ihm entgegen wallen.", "tokens": ["Wei\u00b7nend", "ihm", "ent\u00b7ge\u00b7gen", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKVZ", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "In den hohen Kaisersaal", "tokens": ["In", "den", "ho\u00b7hen", "Kai\u00b7ser\u00b7saal"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist er rasch noch eingetreten;", "tokens": ["Ist", "er", "rasch", "noch", "ein\u00b7ge\u00b7tre\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sitzend dort auf goldnem Stuhl", "tokens": ["Sit\u00b7zend", "dort", "auf", "gold\u00b7nem", "Stuhl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6rt man f\u00fcr das Volk ihn beten.", "tokens": ["H\u00f6rt", "man", "f\u00fcr", "das", "Volk", "ihn", "be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Reichet mir den heil'gen Leib!", "tokens": ["Rei\u00b7chet", "mir", "den", "heil'\u00b7gen", "Leib", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Spricht er dann mit bleichem Munde,", "tokens": ["Spricht", "er", "dann", "mit", "blei\u00b7chem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Drauf verj\u00fcngt sich sein Gesicht,", "tokens": ["Drauf", "ver\u00b7j\u00fcngt", "sich", "sein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Um die mittern\u00e4cht'ge Stunde.", "tokens": ["Um", "die", "mit\u00b7ter\u00b7n\u00e4cht'\u00b7ge", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Da auf einmal wird der Saal", "tokens": ["Da", "auf", "ein\u00b7mal", "wird", "der", "Saal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADV", "VAFIN", "ART", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Hell von \u00fcberird'schem Lichte,", "tokens": ["Hell", "von", "\u00fc\u00b7ber\u00b7ird'\u00b7schem", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und verschieden sitzt der Held,", "tokens": ["Und", "ver\u00b7schie\u00b7den", "sitzt", "der", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text":