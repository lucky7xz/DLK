{"textgrid.poem.39319": {"metadata": {"author": {"name": "Lavater, Johann Kaspar", "birth": "N.A.", "death": "N.A."}, "title": "2. Wilhelm Tell", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nein! vor dem aufgestekten Hut,", "tokens": ["Nein", "!", "vor", "dem", "auf\u00b7ge\u00b7stek\u00b7ten", "Hut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du M\u00f6rderangesicht!", "tokens": ["Du", "M\u00f6r\u00b7de\u00b7ran\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "B\u00fckt sich kein Mann voll Heldenmuth,", "tokens": ["B\u00fckt", "sich", "kein", "Mann", "voll", "Hel\u00b7den\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIAT", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "B\u00fckt Wilhelm Tell sich nicht!", "tokens": ["B\u00fckt", "Wil\u00b7helm", "Tell", "sich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "PRF", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Knirsch immer, du Tyrannenzahn!", "tokens": ["Knirsch", "im\u00b7mer", ",", "du", "Ty\u00b7ran\u00b7nen\u00b7zahn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPER", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer frey ist, bleibet frey!", "tokens": ["Wer", "frey", "ist", ",", "blei\u00b7bet", "frey", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn er nichts mehr haben kann,", "tokens": ["Und", "wenn", "er", "nichts", "mehr", "ha\u00b7ben", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "ADV", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat er noch Muth und Treu!", "tokens": ["Hat", "er", "noch", "Muth", "und", "Treu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Landvogt voll von Raache schnaubt", "tokens": ["Der", "Land\u00b7vogt", "voll", "von", "Raa\u00b7che", "schnaubt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn an: \u00bbSchie\u00df deinem Kind", "tokens": ["Ihn", "an", ":", "\u00bb", "Schie\u00df", "dei\u00b7nem", "Kind"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "$.", "$(", "NN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schnell einen Apfel weg vom Haupt;", "tokens": ["Schnell", "ei\u00b7nen", "Ap\u00b7fel", "weg", "vom", "Haupt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sonst w\u00fcrg ich dich geschwind\u00ab!", "tokens": ["Sonst", "w\u00fcrg", "ich", "dich", "ge\u00b7schwind", "\u00ab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Tell h\u00f6rt und seufzt: \u00bbAch, der Tyrann!", "tokens": ["Tell", "h\u00f6rt", "und", "seufzt", ":", "\u00bb", "Ach", ",", "der", "Ty\u00b7rann", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "$.", "$(", "ITJ", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich sterbe, Sohn, f\u00fcr dich!", "tokens": ["Ich", "ster\u00b7be", ",", "Sohn", ",", "f\u00fcr", "dich", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch Sohn! ich schie\u00dfe, ja ich kann", "tokens": ["Doch", "Sohn", "!", "ich", "schie\u00b7\u00dfe", ",", "ja", "ich", "kann"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$.", "PPER", "VVFIN", "$,", "ADV", "PPER", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erretten dich und mich\u00ab!", "tokens": ["Er\u00b7ret\u00b7ten", "dich", "und", "mich", "\u00ab", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "KON", "PPER", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dr\u00fckt an die Brust ihn \u2013 welch ein Schmerz! \u2013", "tokens": ["Dr\u00fckt", "an", "die", "Brust", "ihn", "\u2013", "welch", "ein", "Schmerz", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "$(", "PWAT", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und lispelt ihm: \u00bbSteh still!", "tokens": ["Und", "lis\u00b7pelt", "ihm", ":", "\u00bb", "Steh", "still", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "$(", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Eh schl\u00e4gt nicht mehr mein Vaterherz,", "tokens": ["Eh", "schl\u00e4gt", "nicht", "mehr", "mein", "Va\u00b7ter\u00b7herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eh ich dich trefen will\u00ab!", "tokens": ["Eh", "ich", "dich", "tre\u00b7fen", "will", "\u00ab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und f\u00fchrt ihn sanft an einen Baum,", "tokens": ["Und", "f\u00fchrt", "ihn", "sanft", "an", "ei\u00b7nen", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dr\u00fckt ihm den Apfel auf", "tokens": ["Dr\u00fckt", "ihm", "den", "Ap\u00b7fel", "auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und legt den angewiesnen Raum", "tokens": ["Und", "legt", "den", "an\u00b7ge\u00b7wi\u00b7es\u00b7nen", "Raum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Zur\u00fck im schnellen Lauf,", "tokens": ["Zu\u00b7r\u00fck", "im", "schnel\u00b7len", "Lauf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Nimmt eilends Pfeil und Bogen, spannt,", "tokens": ["Nimmt", "ei\u00b7lends", "Pfeil", "und", "Bo\u00b7gen", ",", "spannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "KON", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Blikt scharf \u2013 fest steht der Knab \u2013", "tokens": ["Blikt", "scharf", "\u2013", "fest", "steht", "der", "Knab", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$(", "ADJD", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dr\u00fckt mit unbewegter Hand \u2013", "tokens": ["Und", "dr\u00fckt", "mit", "un\u00b7be\u00b7weg\u00b7ter", "Hand", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es kn\u00e4llt \u2013 den Apfel ab!", "tokens": ["Es", "kn\u00e4llt", "\u2013", "den", "Ap\u00b7fel", "ab", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Voll jugendlicher Munterkeit", "tokens": ["Voll", "ju\u00b7gend\u00b7li\u00b7cher", "Mun\u00b7ter\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sucht ihn der Knab; in Eil", "tokens": ["Sucht", "ihn", "der", "Knab", ";", "in", "Eil"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bringt er dem Vater voller Freud", "tokens": ["Bringt", "er", "dem", "Va\u00b7ter", "vol\u00b7ler", "Freud"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am Apfel seinen Pfeil.", "tokens": ["Am", "Ap\u00b7fel", "sei\u00b7nen", "Pfeil", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbh\u00e4tt der ihm nur ein Haar gefehlt,", "tokens": ["\u00bb", "h\u00e4tt", "der", "ihm", "nur", "ein", "Haar", "ge\u00b7fehlt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ART", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der zweyte tr\u00e4fe doch\u00ab!", "tokens": ["Der", "zwey\u00b7te", "tr\u00e4\u00b7fe", "doch", "\u00ab", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbwen?\u00ab \u00bbGe\u00dfler, dich! Du l\u00e4gst entseelt,", "tokens": ["\u00bb", "wen", "?", "\u00ab", "\u00bb", "Ge\u00df\u00b7ler", ",", "dich", "!", "Du", "l\u00e4gst", "ent\u00b7seelt", ","], "token_info": ["punct", "word", "punct", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "$.", "$(", "$(", "NE", "$,", "PPER", "$.", "PPER", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Tell w\u00e4r frey vom Joch\u00ab!", "tokens": ["Und", "Tell", "w\u00e4r", "frey", "vom", "Joch", "\u00ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "VAFIN", "ADJD", "APPRART", "NN", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Der Vogt von Raach und Wuth entflammt,", "tokens": ["Der", "Vogt", "von", "Raach", "und", "Wuth", "ent\u00b7flammt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bindt schnell ihm H\u00e4nd' und F\u00fc\u00df'", "tokens": ["Bindt", "schnell", "ihm", "H\u00e4nd'", "und", "F\u00fc\u00df'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sch\u00e4umt und stampfet und verdammt", "tokens": ["Und", "sch\u00e4umt", "und", "stamp\u00b7fet", "und", "ver\u00b7dammt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Tell zur Finsterni\u00df.", "tokens": ["Den", "Tell", "zur", "Fins\u00b7ter\u00b7ni\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Gebunden bleibt der Held ein Held,", "tokens": ["Ge\u00b7bun\u00b7den", "bleibt", "der", "Held", "ein", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Ketten Tell noch Tell.", "tokens": ["In", "Ket\u00b7ten", "Tell", "noch", "Tell", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gott, dem die Freyheit stets gef\u00e4llt,", "tokens": ["Gott", ",", "dem", "die", "Frey\u00b7heit", "stets", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Sieht ihn und hilft ihm schnell.", "tokens": ["Sieht", "ihn", "und", "hilft", "ihm", "schnell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Er ruft dem Sturm. Der Sturm braust her,", "tokens": ["Er", "ruft", "dem", "Sturm", ".", "Der", "Sturm", "braust", "her", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Schiffer stehn erbla\u00dft,", "tokens": ["Die", "Schif\u00b7fer", "stehn", "er\u00b7bla\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sehn bebend keine Rettung mehr,", "tokens": ["Sehn", "be\u00b7bend", "kei\u00b7ne", "Ret\u00b7tung", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PIAT", "NN", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wenn Tell das Steur nicht fa\u00dft.", "tokens": ["Wenn", "Tell", "das", "Steur", "nicht", "fa\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Des Helden losgebundner Arm", "tokens": ["Des", "Hel\u00b7den", "los\u00b7ge\u00b7bund\u00b7ner", "Arm"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Arbeitet fort zum Strand.", "tokens": ["Ar\u00b7bei\u00b7tet", "fort", "zum", "Strand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Tell springt und steht von Freyheit warm", "tokens": ["Tell", "springt", "und", "steht", "von", "Frey\u00b7heit", "warm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "(das Schiff prellt weg) am Land!", "tokens": ["(", "das", "Schiff", "prellt", "weg", ")", "am", "Land", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PTKVZ", "$(", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Die Wogen rauschen f\u00fcrchterlich", "tokens": ["Die", "Wo\u00b7gen", "rau\u00b7schen", "f\u00fcrch\u00b7ter\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In des Tyrannen Ohr,", "tokens": ["In", "des", "Ty\u00b7ran\u00b7nen", "Ohr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Tell sieht zu Gott auf, st\u00e4rket sich", "tokens": ["Tell", "sieht", "zu", "Gott", "auf", ",", "st\u00e4r\u00b7ket", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "NN", "PTKVZ", "$,", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und l\u00e4uft ihm schnell zuvor.", "tokens": ["Und", "l\u00e4uft", "ihm", "schnell", "zu\u00b