{"dta.poem.10349": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "X.  \n  Belsatzar .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Die Mitternacht zog n\u00e4her schon;", "tokens": ["Die", "Mit\u00b7ter\u00b7nacht", "zog", "n\u00e4\u00b7her", "schon", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In stummer Ruh lag Babylon.", "tokens": ["In", "stum\u00b7mer", "Ruh", "lag", "Ba\u00b7by\u00b7lon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nur oben, in des K\u00f6nigs Schlo\u00df,", "tokens": ["Nur", "o\u00b7ben", ",", "in", "des", "K\u00f6\u00b7nigs", "Schlo\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da flackert's, da l\u00e4rmt des K\u00f6nigs Tro\u00df,", "tokens": ["Da", "fla\u00b7ckert's", ",", "da", "l\u00e4rmt", "des", "K\u00f6\u00b7nigs", "Tro\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Dort oben, in dem K\u00f6nigssaal,", "tokens": ["Dort", "o\u00b7ben", ",", "in", "dem", "K\u00f6\u00b7nigs\u00b7saal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Belsatzar hielt sein K\u00f6nigsmahl.", "tokens": ["Bel\u00b7sat\u00b7zar", "hielt", "sein", "K\u00f6\u00b7nigs\u00b7mahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Die Knechte sa\u00dfen in schimmernden Reih'n,", "tokens": ["Die", "Knech\u00b7te", "sa\u00b7\u00dfen", "in", "schim\u00b7mern\u00b7den", "Reih'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und leerten die Becher mit funkelndem Wein.", "tokens": ["Und", "leer\u00b7ten", "die", "Be\u00b7cher", "mit", "fun\u00b7keln\u00b7dem", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Es klirrten die Becher, es jauchzten die Knecht';", "tokens": ["Es", "klirr\u00b7ten", "die", "Be\u00b7cher", ",", "es", "jauchz\u00b7ten", "die", "Knecht'", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "So klang es dem st\u00f6rrigen K\u00f6nige recht.", "tokens": ["So", "klang", "es", "dem", "st\u00f6r\u00b7ri\u00b7gen", "K\u00f6\u00b7ni\u00b7ge", "recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Des K\u00f6nigs Wangen leuchten Glut;", "tokens": ["Des", "K\u00f6\u00b7nigs", "Wan\u00b7gen", "leuch\u00b7ten", "Glut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Wein erwuchs ihm kecker Muth.", "tokens": ["Im", "Wein", "er\u00b7wuchs", "ihm", "ke\u00b7cker", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und blindlings rei\u00dft der Muth ihn fort;", "tokens": ["Und", "blind\u00b7lings", "rei\u00dft", "der", "Muth", "ihn", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und er l\u00e4stert die Gottheit mit s\u00fcndigem Wort.", "tokens": ["Und", "er", "l\u00e4s\u00b7tert", "die", "Got\u00b7theit", "mit", "s\u00fcn\u00b7di\u00b7gem", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.8": {"line.1": {"text": "Und er br\u00fcstet sich frech, und l\u00e4stert wild;", "tokens": ["Und", "er", "br\u00fcs\u00b7tet", "sich", "frech", ",", "und", "l\u00e4s\u00b7tert", "wild", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADJD", "$,", "KON", "VVFIN", "ADJD", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die Knechtenschaar ihm Beifall br\u00fcllt.", "tokens": ["Die", "Knech\u00b7ten\u00b7schaar", "ihm", "Bei\u00b7fall", "br\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Der K\u00f6nig rief mit stolzem Blick;", "tokens": ["Der", "K\u00f6\u00b7nig", "rief", "mit", "stol\u00b7zem", "Blick", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Diener eilt und kehrt zur\u00fcck.", "tokens": ["Der", "Die\u00b7ner", "eilt", "und", "kehrt", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Er trug viel g\u00fclden Ger\u00e4th auf dem Haupt;", "tokens": ["Er", "trug", "viel", "g\u00fcl\u00b7den", "Ge\u00b7r\u00e4\u00b7th", "auf", "dem", "Haupt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das war aus dem Tempel Jehovas geraubt.", "tokens": ["Das", "war", "aus", "dem", "Tem\u00b7pel", "Je\u00b7ho\u00b7vas", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ART", "NN", "NE", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.11": {"line.1": {"text": "Und der K\u00f6nig ergriff mit frevler Hand", "tokens": ["Und", "der", "K\u00f6\u00b7nig", "er\u00b7griff", "mit", "frev\u00b7ler", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Einen heiligen Becher, gef\u00fcllt bis am Rand'.", "tokens": ["Ei\u00b7nen", "hei\u00b7li\u00b7gen", "Be\u00b7cher", ",", "ge\u00b7f\u00fcllt", "bis", "am", "Rand'", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP", "APPR", "APPRART", "NN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.12": {"line.1": {"text": "Und er leert ihn hastig bis auf den Grund,", "tokens": ["Und", "er", "leert", "ihn", "has\u00b7tig", "bis", "auf", "den", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "ADV", "APPR", "ART", "NN", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und rufet laut mit sch\u00e4umendem Mund:", "tokens": ["Und", "ru\u00b7fet", "laut", "mit", "sch\u00e4u\u00b7men\u00b7dem", "Mund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.13": {"line.1": {"text": "Jehovah! dir k\u00fcnd' ich auf ewig Hohn, \u2014", "tokens": ["Je\u00b7ho\u00b7vah", "!", "dir", "k\u00fcnd'", "ich", "auf", "e\u00b7wig", "Hohn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "PPER", "APPR", "ADJD", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich bin der K\u00f6nig von Babylon!", "tokens": ["Ich", "bin", "der", "K\u00f6\u00b7nig", "von", "Ba\u00b7by\u00b7lon", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.14": {"line.1": {"text": "Doch kaum das grause Wort verklang,", "tokens": ["Doch", "kaum", "das", "grau\u00b7se", "Wort", "ver\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem K\u00f6nig ward's heimlich im Busen bang.", "tokens": ["Dem", "K\u00f6\u00b7nig", "ward's", "heim\u00b7lich", "im", "Bu\u00b7sen", "bang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPRART", "NN", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.15": {"line.1": {"text": "Das gellende Lachen verstummte zumal;", "tokens": ["Das", "gel\u00b7len\u00b7de", "La\u00b7chen", "ver\u00b7stumm\u00b7te", "zu\u00b7mal", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Es wurde leichenstill im Saal.", "tokens": ["Es", "wur\u00b7de", "lei\u00b7chen\u00b7still", "im", "Saal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Und sieh! und sieh! an wei\u00dfer Wand", "tokens": ["Und", "sieh", "!", "und", "sieh", "!", "an", "wei\u00b7\u00dfer", "Wand"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "$.", "KON", "VVIMP", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da kam's hervor wie Menschenhand;", "tokens": ["Da", "kam's", "her\u00b7vor", "wie", "Men\u00b7schen\u00b7hand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Und schrieb, und schrieb an wei\u00dfer Wand", "tokens": ["Und", "schrieb", ",", "und", "schrieb", "an", "wei\u00b7\u00dfer", "Wand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Buchstaben von Feuer, und schrieb und schwand.", "tokens": ["Buch\u00b7sta\u00b7ben", "von", "Feu\u00b7er", ",", "und", "schrieb", "und", "schwand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.18": {"line.1": {"text": "Der K\u00f6nig stieren Blicks da sa\u00df,", "tokens": ["Der", "K\u00f6\u00b7nig", "stie\u00b7ren", "Blicks", "da", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit schlotternden Knien und todtenbla\u00df.", "tokens": ["Mit", "schlot\u00b7tern\u00b7den", "Kni\u00b7en", "und", "tod\u00b7ten\u00b7bla\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.19": {"line.1": {"text": "Die Knechtenschaar sa\u00df kalt durchgraut,", "tokens": ["Die", "Knech\u00b7ten\u00b7schaar", "sa\u00df", "kalt", "durch\u00b7graut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sa\u00df gar still, gab keinen Laut.", "tokens": ["Und", "sa\u00df", "gar", "still", ",", "gab", "kei\u00b7nen", "Laut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$,", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "Die Magier kamen, doch keiner verstand", "tokens": ["Die", "Ma\u00b7gier", "ka\u00b7men", ",", "doch", "kei\u00b7ner", "ver\u00b7stand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "PIS", "VVFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zu deuten die Flammenschrift an der Wand.", "tokens": ["Zu", "deu\u00b7ten", "die", "Flam\u00b7men\u00b7schrift", "an", "der", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.21": {"line.1": {"text": "Belsatzar ward aber in selbiger Nacht", "tokens": ["Bel\u00b7sat\u00b7zar", "ward", "a\u00b7ber", "in", "sel\u00b7bi\u00b7ger", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Von seinen Knechten umgebracht.", "tokens": ["Von", "sei\u00b7nen", "Knech\u00b7ten", "um\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}