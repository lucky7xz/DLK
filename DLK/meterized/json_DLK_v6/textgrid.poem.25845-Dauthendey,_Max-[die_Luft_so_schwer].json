{"textgrid.poem.25845": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[die Luft so schwer]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Luft so schwer,", "tokens": ["Die", "Luft", "so", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wolken stehen wei\u00df und still,", "tokens": ["Wol\u00b7ken", "ste\u00b7hen", "wei\u00df", "und", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VVFIN", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Himmel hohl und aschenleer,", "tokens": ["Der", "Him\u00b7mel", "hohl", "und", "asc\u00b7hen\u00b7leer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Rabenschrei \u2013", "tokens": ["Ein", "Ra\u00b7ben\u00b7schrei", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und kreischt vorbei.", "tokens": ["Und", "kreischt", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Die B\u00e4ume stehen kalt umher,", "tokens": ["Die", "B\u00e4u\u00b7me", "ste\u00b7hen", "kalt", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es ist, als ob das letzte Herz gestorben sei.", "tokens": ["Es", "ist", ",", "als", "ob", "das", "letz\u00b7te", "Herz", "ge\u00b7stor\u00b7ben", "sei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "ART", "ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}