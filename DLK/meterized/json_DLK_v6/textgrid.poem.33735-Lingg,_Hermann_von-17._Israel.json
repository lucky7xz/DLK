{"textgrid.poem.33735": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "17. Israel", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bberbeb, o Israel, dein Haupt vom Steine!", "tokens": ["\u00bb", "er\u00b7beb", ",", "o", "Is\u00b7rael", ",", "dein", "Haupt", "vom", "Stei\u00b7ne", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "FM", "NE", "$,", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dein Tag in Zion wird dir wieder kommen,", "tokens": ["Dein", "Tag", "in", "Zi\u00b7on", "wird", "dir", "wie\u00b7der", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NE", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Schwert wird deiner Feinde Hand genommen", "tokens": ["Das", "Schwert", "wird", "dei\u00b7ner", "Fein\u00b7de", "Hand", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wird vom Herrn gegeben in die deine.", "tokens": ["Und", "wird", "vom", "Herrn", "ge\u00b7ge\u00b7ben", "in", "die", "dei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "VVPP", "APPR", "ART", "PPOSAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dann, Babylon, dann sink ins Knie und weine,", "tokens": ["Dann", ",", "Ba\u00b7by\u00b7lon", ",", "dann", "sink", "ins", "Knie", "und", "wei\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NE", "$,", "ADV", "ADJD", "APPRART", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann wird dein Himmel sein von Glut erglommen;", "tokens": ["Dann", "wird", "dein", "Him\u00b7mel", "sein", "von", "Glut", "er\u00b7glom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PPOSAT", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In deinen Stra\u00dfen, \u00f6d' und blutdurchschwommen,", "tokens": ["In", "dei\u00b7nen", "Stra\u00b7\u00dfen", ",", "\u00f6d'", "und", "blut\u00b7durch\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wird ausgestreut so Gold wie Edelsteine!\u00ab", "tokens": ["Wird", "aus\u00b7ge\u00b7streut", "so", "Gold", "wie", "E\u00b7del\u00b7stei\u00b7ne", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "ADV", "NN", "KOKOM", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So sprach am Euphrat der Prophet und streckte", "tokens": ["So", "sprach", "am", "Eu\u00b7ph\u00b7rat", "der", "Pro\u00b7phet", "und", "streck\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NE", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die H\u00e4nde z\u00fcrnend gegen Babels Tore,", "tokens": ["Die", "H\u00e4n\u00b7de", "z\u00fcr\u00b7nend", "ge\u00b7gen", "Ba\u00b7bels", "To\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo Schlaf und Mondlicht schon die Zinnen deckte.", "tokens": ["Wo", "Schlaf", "und", "Mond\u00b7licht", "schon", "die", "Zin\u00b7nen", "deck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Zu seinen F\u00fc\u00dfen lag das Volk \u2013 im Rohre", "tokens": ["Zu", "sei\u00b7nen", "F\u00fc\u00b7\u00dfen", "lag", "das", "Volk", "\u2013", "im", "Roh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$(", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Euphratufers sang der Wind und weckte", "tokens": ["Des", "Eu\u00b7phra\u00b7tu\u00b7fers", "sang", "der", "Wind", "und", "weck\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Lied von Zions Glanz im M\u00e4nnerchore.", "tokens": ["Ein", "Lied", "von", "Zi\u00b7ons", "Glanz", "im", "M\u00e4n\u00b7ner\u00b7cho\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}