{"textgrid.poem.37217": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Amors Ankunft in Cythere", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Amors Ankunft in Cythere", "tokens": ["A\u00b7mors", "An\u00b7kunft", "in", "Cy\u00b7the\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "APPR", "NE"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Wird ein allgemeines Fest,", "tokens": ["Wird", "ein", "all\u00b7ge\u00b7mei\u00b7nes", "Fest", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als sich Venus nicht die Ehre", "tokens": ["Als", "sich", "Ve\u00b7nus", "nicht", "die", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "NE", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihn zu stillen rauben l\u00e4\u00dft.", "tokens": ["Ihn", "zu", "stil\u00b7len", "rau\u00b7ben", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil er aber nur betrachtet", "tokens": ["Weil", "er", "a\u00b7ber", "nur", "be\u00b7trach\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und, schon Kind nicht mehr, allein", "tokens": ["Und", ",", "schon", "Kind", "nicht", "mehr", ",", "al\u00b7lein"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "$,", "ADV", "NN", "PTKNEG", "ADV", "$,", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Des Gef\u00e4\u00dfes Reizen schmachtet,", "tokens": ["Des", "Ge\u00b7f\u00e4\u00b7\u00dfes", "Rei\u00b7zen", "schmach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Will ihm keine Milch gedeihn.", "tokens": ["Will", "ihm", "kei\u00b7ne", "Milch", "ge\u00b7deihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Rath in solcher Noth gew\u00e4hren", "tokens": ["Rath", "in", "sol\u00b7cher", "Noth", "ge\u00b7w\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hei\u00dft die G\u00f6ttin ihren Hof:", "tokens": ["Hei\u00dft", "die", "G\u00f6t\u00b7tin", "ih\u00b7ren", "Hof", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Haben Amorn aufzun\u00e4hren,", "tokens": ["Ha\u00b7ben", "A\u00b7morn", "auf\u00b7zu\u00b7n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Andre doch vielleicht den Stoff.", "tokens": ["And\u00b7re", "doch", "viel\u00b7leicht", "den", "Stoff", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da den Vorzug zu gewinnen", "tokens": ["Da", "den", "Vor\u00b7zug", "zu", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Treten in gedr\u00e4ngter Zahl", "tokens": ["Tre\u00b7ten", "in", "ge\u00b7dr\u00e4ng\u00b7ter", "Zahl"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Heldent\u00f6chter und G\u00f6ttinnen", "tokens": ["Hel\u00b7den\u00b7t\u00f6ch\u00b7ter", "und", "G\u00f6t\u00b7tin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die Tugenden zur Wahl.", "tokens": ["Und", "die", "Tu\u00b7gen\u00b7den", "zur", "Wahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Manche G\u00f6tterbrust quillt Nahrung", "tokens": ["Man\u00b7che", "G\u00f6t\u00b7ter\u00b7brust", "quillt", "Nah\u00b7rung"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df man nicht die Wollust w\u00e4hlt,", "tokens": ["Da\u00df", "man", "nicht", "die", "Wol\u00b7lust", "w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Untersaget blo\u00df Erfahrung,", "tokens": ["Un\u00b7ter\u00b7sa\u00b7get", "blo\u00df", "Er\u00b7fah\u00b7rung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die der H\u00f6fe keinem fehlt.", "tokens": ["Die", "der", "H\u00f6\u00b7fe", "kei\u00b7nem", "fehlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Trocken findet man die Musen,", "tokens": ["Tro\u00b7cken", "fin\u00b7det", "man", "die", "Mu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ernsthaft die Vernunft und alt,", "tokens": ["Ernst\u00b7haft", "die", "Ver\u00b7nunft", "und", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bis ein Labsal ihm am Busen", "tokens": ["Bis", "ein", "Lab\u00b7sal", "ihm", "am", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der erkornen Hofnung wallt.", "tokens": ["Der", "er\u00b7kor\u00b7nen", "Hof\u00b7nung", "wallt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sich unziemlich \u00fcbergangen", "tokens": ["Sich", "un\u00b7ziem\u00b7lich", "\u00fc\u00b7ber\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PRF", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4hnt vor allen L\u00fcsternheit,", "tokens": ["W\u00e4hnt", "vor", "al\u00b7len", "L\u00fcs\u00b7tern\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Blickt auf Amorn mit Verlangen,", "tokens": ["Blickt", "auf", "A\u00b7morn", "mit", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf die Amme voller Neid,", "tokens": ["Auf", "die", "Am\u00b7me", "vol\u00b7ler", "Neid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und begehrt \u2013 die schlaue! siegen", "tokens": ["Und", "be\u00b7gehrt", "\u2013", "die", "schlau\u00b7e", "!", "sie\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "VVPP", "$(", "ART", "ADJA", "$.", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mu\u00df sie oder selbst vergehn! \u2013", "tokens": ["Mu\u00df", "sie", "o\u00b7der", "selbst", "ver\u00b7gehn", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "KON", "ADV", "VVINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Das erlauchte Kind zu wiegen", "tokens": ["Das", "er\u00b7lauch\u00b7te", "Kind", "zu", "wie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die Hofnung l\u00e4\u00dfts geschehn.", "tokens": ["Und", "die", "Hof\u00b7nung", "l\u00e4\u00dfts", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aber Amor ohn Erbarmen", "tokens": ["A\u00b7ber", "A\u00b7mor", "ohn", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlummert nie und plaget stets.", "tokens": ["Schlum\u00b7mert", "nie", "und", "pla\u00b7get", "stets", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie flehet: \u00bbweichern Armen", "tokens": ["Und", "sie", "fle\u00b7het", ":", "\u00bb", "wei\u00b7chern", "Ar\u00b7men"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "$(", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueberla\u00df ihn!\u00ab \u2013 und erflehts.", "tokens": ["Ue\u00b7berl\u00b7a\u00df", "ihn", "!", "\u00ab", "\u2013", "und", "er\u00b7flehts", "."], "token_info": ["word", "word", "punct", "punct", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$.", "$(", "$(", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zuckerbrot mit vollen H\u00e4nden", "tokens": ["Zu\u00b7cker\u00b7brot", "mit", "vol\u00b7len", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Reicht die Pflegerin ihm dar,", "tokens": ["Reicht", "die", "Pfle\u00b7ge\u00b7rin", "ihm", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und sein Leben schnell zu enden,", "tokens": ["Und", "sein", "Le\u00b7ben", "schnell", "zu", "en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "L\u00e4uft der l\u00fcsterne Gefahr.", "tokens": ["L\u00e4uft", "der", "l\u00fcs\u00b7ter\u00b7ne", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}