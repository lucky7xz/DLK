{"textgrid.poem.44460": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "7. Verwandlungen", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie bist du schaurig,", "tokens": ["Wie", "bist", "du", "schau\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Du dunkle Nacht!", "tokens": ["Du", "dunk\u00b7le", "Nacht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Hier waren Wiesen,", "tokens": ["Hier", "wa\u00b7ren", "Wie\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "War Farbenpracht.", "tokens": ["War", "Far\u00b7ben\u00b7pracht", "."], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Doch kaum zur R\u00fcste", "tokens": ["Doch", "kaum", "zur", "R\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Der Sonne Schein,", "tokens": ["Der", "Son\u00b7ne", "Schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "So sank zur W\u00fcste", "tokens": ["So", "sank", "zur", "W\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Das Eden ein.", "tokens": ["Das", "E\u00b7den", "ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Hier ist die Stelle,", "tokens": ["Hier", "ist", "die", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Hier stand das Haus.", "tokens": ["Hier", "stand", "das", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ich such, ich taste", "tokens": ["Ich", "such", ",", "ich", "tas\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und finds nicht aus.", "tokens": ["Und", "finds", "nicht", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}