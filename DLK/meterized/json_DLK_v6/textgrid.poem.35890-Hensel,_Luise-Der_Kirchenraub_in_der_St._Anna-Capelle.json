{"textgrid.poem.35890": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "Der Kirchenraub in der St. Anna-Capelle", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nnt' ich doch mit hellen Engelzungen", "tokens": ["K\u00f6nnt'", "ich", "doch", "mit", "hel\u00b7len", "En\u00b7gel\u00b7zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Allen guten, frommen Menschenseelen,", "tokens": ["Al\u00b7len", "gu\u00b7ten", ",", "from\u00b7men", "Men\u00b7schen\u00b7see\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Was mein Ohr vernommen, wieder sagen!", "tokens": ["Was", "mein", "Ohr", "ver\u00b7nom\u00b7men", ",", "wie\u00b7der", "sa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVPP", "$,", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ach! vielleicht, da\u00df noch ein Herz erwachte,", "tokens": ["Ach", "!", "viel\u00b7leicht", ",", "da\u00df", "noch", "ein", "Herz", "er\u00b7wach\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Noch ein Auge weinte wie das meine,", "tokens": ["Noch", "ein", "Au\u00b7ge", "wein\u00b7te", "wie", "das", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "KOKOM", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Noch ein Seufzer auf zum Himmel strebte.", "tokens": ["Noch", "ein", "Seuf\u00b7zer", "auf", "zum", "Him\u00b7mel", "streb\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Doch weil Alle Andres sich erz\u00e4hlen", "tokens": ["Doch", "weil", "Al\u00b7le", "And\u00b7res", "sich", "er\u00b7z\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIAT", "PIS", "PRF", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und weil Niemand da, der mich verst\u00e4nde,", "tokens": ["Und", "weil", "Nie\u00b7mand", "da", ",", "der", "mich", "ver\u00b7st\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Will ich's dir, mein stilles Blatt, vertrauen.", "tokens": ["Will", "ich's", "dir", ",", "mein", "stil\u00b7les", "Blatt", ",", "ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PIS", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Nicht gar fern vom sch\u00f6nen Strom des Rheines", "tokens": ["Nicht", "gar", "fern", "vom", "sch\u00f6\u00b7nen", "Strom", "des", "Rhei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ADJD", "APPRART", "ADJA", "NN", "ART", "NE"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Steht auf einer immer gr\u00fcnen Aue", "tokens": ["Steht", "auf", "ei\u00b7ner", "im\u00b7mer", "gr\u00fc\u00b7nen", "Au\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Eine reich geschm\u00fcckte Dorf-Capelle.", "tokens": ["Ei\u00b7ne", "reich", "ge\u00b7schm\u00fcck\u00b7te", "Dor\u00b7fCa\u00b7pel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Und darin ein lieblich Bild der Mutter,", "tokens": ["Und", "da\u00b7rin", "ein", "lieb\u00b7lich", "Bild", "der", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ART", "ADJD", "NN", "ART", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "So die heil'ge Jungfrau uns geboren,", "tokens": ["So", "die", "heil'\u00b7ge", "Jung\u00b7frau", "uns", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Die wir nimmer w\u00fcrdig preisen m\u00f6gen.", "tokens": ["Die", "wir", "nim\u00b7mer", "w\u00fcr\u00b7dig", "prei\u00b7sen", "m\u00f6\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Rings im Kirchlein stehn viel heil'ge Bilder,", "tokens": ["Rings", "im", "Kirch\u00b7lein", "stehn", "viel", "heil'\u00b7ge", "Bil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Reich geschm\u00fcckt mit lichten, bunten Steinen,", "tokens": ["Reich", "ge\u00b7schm\u00fcckt", "mit", "lich\u00b7ten", ",", "bun\u00b7ten", "Stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Auf den H\u00e4uptern zierlich goldne Kronen.", "tokens": ["Auf", "den", "H\u00e4up\u00b7tern", "zier\u00b7lich", "gold\u00b7ne", "Kro\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Und das Kirchlein birgt viel Goldgef\u00e4\u00dfe,", "tokens": ["Und", "das", "Kirch\u00b7lein", "birgt", "viel", "Gold\u00b7ge\u00b7f\u00e4\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die geweiht sind, Christi Leib zu fassen,", "tokens": ["Die", "ge\u00b7weiht", "sind", ",", "Chris\u00b7ti", "Leib", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVPP", "VAFIN", "$,", "NE", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn der Priester Brod und Wein gesegnet.", "tokens": ["Wenn", "der", "Pries\u00b7ter", "Brod", "und", "Wein", "ge\u00b7seg\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Einst hatt' unter frommen Christen mitten", "tokens": ["Einst", "hatt'", "un\u00b7ter", "from\u00b7men", "Chris\u00b7ten", "mit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sich ein teuflisch Herz hineingeschlichen,", "tokens": ["Sich", "ein", "teuf\u00b7lisch", "Herz", "hin\u00b7ein\u00b7ge\u00b7schli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Glaub' und Lieb' und fromme Demuth heuchelnd.", "tokens": ["Glaub'", "und", "Lieb'", "und", "from\u00b7me", "De\u00b7muth", "heu\u00b7chelnd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+--+--", "measure": "trochaic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Als der Priester unsern Herrn emporhob,", "tokens": ["Als", "der", "Pries\u00b7ter", "un\u00b7sern", "Herrn", "em\u00b7por\u00b7hob", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Da\u00df die Christenschaar Ihn betend ehre,", "tokens": ["Da\u00df", "die", "Chris\u00b7ten\u00b7schaar", "Ihn", "be\u00b7tend", "eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Hat ", "tokens": ["Hat"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "-", "measure": "single.down"}}, "stanza.10": {"line.1": {"text": "Als die Frommen ihren Herrn empfangen,", "tokens": ["Als", "die", "From\u00b7men", "ih\u00b7ren", "Herrn", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dankend dann zu ihren H\u00fctten kehrten,", "tokens": ["Dan\u00b7kend", "dann", "zu", "ih\u00b7ren", "H\u00fct\u00b7ten", "kehr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Da der Priester sprach das letzte Amen:", "tokens": ["Da", "der", "Pries\u00b7ter", "sprach", "das", "letz\u00b7te", "A\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Lag er noch wie im Gebet versunken,", "tokens": ["Lag", "er", "noch", "wie", "im", "Ge\u00b7bet", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KOKOM", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Sann am heil'gen Ort auf List und T\u00fccke,", "tokens": ["Sann", "am", "heil'\u00b7gen", "Ort", "auf", "List", "und", "T\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Heimlich lauschend nach der Hand des Priesters.", "tokens": ["Heim\u00b7lich", "lau\u00b7schend", "nach", "der", "Hand", "des", "Pries\u00b7ters", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Dieser, unter heiligen Gebeten,", "tokens": ["Die\u00b7ser", ",", "un\u00b7ter", "hei\u00b7li\u00b7gen", "Ge\u00b7be\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Legt, was \u00fcbrig war an heil'gen Hostien,", "tokens": ["Legt", ",", "was", "\u00fcb\u00b7rig", "war", "an", "heil'\u00b7gen", "Hos\u00b7ti\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ADJD", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "In den pr\u00e4chtigsten Pokal des Altars.", "tokens": ["In", "den", "pr\u00e4ch\u00b7tigs\u00b7ten", "Po\u00b7kal", "des", "Al\u00b7tars", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "--+---+--+", "measure": "iambic.tri.chol"}}, "stanza.13": {"line.1": {"text": "Ging dann, sich noch einmal tief verneigend,", "tokens": ["Ging", "dann", ",", "sich", "noch", "ein\u00b7mal", "tief", "ver\u00b7nei\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PRF", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Fromm sich segnend mit dem Kreuzeszeichen", "tokens": ["Fromm", "sich", "seg\u00b7nend", "mit", "dem", "Kreu\u00b7zes\u00b7zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und nahm Wasser aus dem Weihgef\u00e4\u00dfe.", "tokens": ["Und", "nahm", "Was\u00b7ser", "aus", "dem", "Weih\u00b7ge\u00b7f\u00e4\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.14": {"line.1": {"text": "Und auch jener, der so glei\u00dfend kniete,", "tokens": ["Und", "auch", "je\u00b7ner", ",", "der", "so", "glei\u00b7\u00dfend", "knie\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PDAT", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Kam, taucht' ein in's Gott geweihte Wasser,", "tokens": ["Kam", ",", "taucht'", "ein", "in's", "Gott", "ge\u00b7weih\u00b7te", "Was\u00b7ser", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ART", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Schlich dann heim in seine d\u00fcstre Wohnung.", "tokens": ["Schlich", "dann", "heim", "in", "sei\u00b7ne", "d\u00fcst\u00b7re", "Woh\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PTKVZ", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.15": {"line.1": {"text": "\u00bbnicht vertr\u00e4gt das helle Licht der Sonne,\u00ab", "tokens": ["\u00bb", "nicht", "ver\u00b7tr\u00e4gt", "das", "hel\u00b7le", "Licht", "der", "Son\u00b7ne", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKNEG", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sprach er bei sich selbst, \u00bbso finstre Werke,", "tokens": ["Sprach", "er", "bei", "sich", "selbst", ",", "\u00bb", "so", "finst\u00b7re", "Wer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PRF", "ADV", "$,", "$(", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wie ich heut zu thun beschlossen habe.\u00ab", "tokens": ["Wie", "ich", "heut", "zu", "thun", "be\u00b7schlos\u00b7sen", "ha\u00b7be", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "ADV", "PTKZU", "VVINF", "VVPP", "VAFIN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.16": {"line.1": {"text": "\u00bbdarum in den Stunden n\u00e4cht'gen Dunkels,", "tokens": ["\u00bb", "da\u00b7rum", "in", "den", "Stun\u00b7den", "n\u00e4cht'\u00b7gen", "Dun\u00b7kels", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PAV", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wenn die Gegend einsam ist und \u00f6de,", "tokens": ["Wenn", "die", "Ge\u00b7gend", "ein\u00b7sam", "ist", "und", "\u00f6\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VAFIN", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Nehm' ich mir, was mir mein Herz genommen.\u00ab", "tokens": ["Nehm'", "ich", "mir", ",", "was", "mir", "mein", "Herz", "ge\u00b7nom\u00b7men", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "PWS", "PPER", "PPOSAT", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.17": {"line.1": {"text": "Und er z\u00e4hlte ruhelos die Stunden,",