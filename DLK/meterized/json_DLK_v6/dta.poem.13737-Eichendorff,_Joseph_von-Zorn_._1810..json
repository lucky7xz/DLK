{"dta.poem.13737": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Zorn .  \n 1810.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und ein neu Geschlecht von Zwergen", "tokens": ["Und", "ein", "neu", "Ge\u00b7schlecht", "von", "Zwer\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwindelnd um die Felsen klettern,", "tokens": ["Schwin\u00b7delnd", "um", "die", "Fel\u00b7sen", "klet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Frech, wenn's sonnig auf den Bergen,", "tokens": ["Frech", ",", "wenn's", "son\u00b7nig", "auf", "den", "Ber\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Feige kr\u00fcmmend sich in Wettern,", "tokens": ["Fei\u00b7ge", "kr\u00fcm\u00b7mend", "sich", "in", "Wet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}