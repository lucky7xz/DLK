{"textgrid.poem.42696": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Kostnitz, die du einst von Deutschlands freyen St\u00e4dten", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Kostnitz, die du einst von Deutschlands freyen St\u00e4dten", "tokens": ["O", "Kost\u00b7nitz", ",", "die", "du", "einst", "von", "Deutschlands", "frey\u00b7en", "St\u00e4d\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PRELS", "PPER", "ADV", "APPR", "NE", "ADJA", "NN"], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit nichten die geringste warst,", "tokens": ["Mit", "nich\u00b7ten", "die", "ge\u00b7rings\u00b7te", "warst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Kr\u00e4mer, deren Gold der Erde Majest\u00e4ten", "tokens": ["Und", "Kr\u00e4\u00b7mer", ",", "de\u00b7ren", "Gold", "der", "Er\u00b7de", "Ma\u00b7jes\u00b7t\u00e4\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "PRELAT", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Geschmeidig huldigten, gebarst!", "tokens": ["Ge\u00b7schmei\u00b7dig", "hul\u00b7dig\u00b7ten", ",", "ge\u00b7barst", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dahin ist nun die Zeit, als aus der Fremden S\u00e4ckeln", "tokens": ["Da\u00b7hin", "ist", "nun", "die", "Zeit", ",", "als", "aus", "der", "Frem\u00b7den", "S\u00e4\u00b7ckeln"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ADV", "ART", "NN", "$,", "KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In deine Pforten Reichthum quoll,", "tokens": ["In", "dei\u00b7ne", "Pfor\u00b7ten", "Reicht\u00b7hum", "quoll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und laut bis in die Nacht von stolzer Wuchrer M\u00e4keln", "tokens": ["Und", "laut", "bis", "in", "die", "Nacht", "von", "stol\u00b7zer", "Wuch\u00b7rer", "M\u00e4\u00b7keln"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein l\u00e4rmerf\u00fcllter Marktplatz scholl.", "tokens": ["Dein", "l\u00e4r\u00b7mer\u00b7f\u00fcll\u00b7ter", "Markt\u00b7platz", "scholl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Verwelkt ist deine Zier. Der Erde V\u00f6lker wallen", "tokens": ["Ver\u00b7welkt", "ist", "dei\u00b7ne", "Zier", ".", "Der", "Er\u00b7de", "V\u00f6l\u00b7ker", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$.", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht mehr zu deinen Mauern hin:", "tokens": ["Nicht", "mehr", "zu", "dei\u00b7nen", "Mau\u00b7ern", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Tief, wie einst Tyrus fiel, tief, tief bist du gefallen,", "tokens": ["Tief", ",", "wie", "einst", "Ty\u00b7rus", "fiel", ",", "tief", ",", "tief", "bist", "du", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ADV", "NE", "VVFIN", "$,", "ADJD", "$,", "ADJD", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Des Bodensees Beherrscherinn!", "tokens": ["Des", "Bo\u00b7den\u00b7sees", "Be\u00b7herr\u00b7sche\u00b7rinn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie auf dem Weingebirg ein Winzerhaus im kalten", "tokens": ["Wie", "auf", "dem", "Wein\u00b7ge\u00b7birg", "ein", "Win\u00b7zer\u00b7haus", "im", "kal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Eismonde w\u00fcst und einsam steht,", "tokens": ["Eis\u00b7mon\u00b7de", "w\u00fcst", "und", "ein\u00b7sam", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "So stehst du \u00f6d' und leer, und bist gleich einer alten", "tokens": ["So", "stehst", "du", "\u00f6d'", "und", "leer", ",", "und", "bist", "gleich", "ei\u00b7ner", "al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,", "KON", "VAFIN", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Reitzlosen Buhlerinn verschm\u00e4ht.", "tokens": ["Reitz\u00b7lo\u00b7sen", "Buh\u00b7le\u00b7rinn", "ver\u00b7schm\u00e4ht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Auf deinen alternden entv\u00f6lkerten Geb\u00e4uden", "tokens": ["Auf", "dei\u00b7nen", "al\u00b7tern\u00b7den", "ent\u00b7v\u00f6l\u00b7ker\u00b7ten", "Ge\u00b7b\u00e4u\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Keimt traurig, wie auf einem Grab,", "tokens": ["Keimt", "trau\u00b7rig", ",", "wie", "auf", "ei\u00b7nem", "Grab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "PWAV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die d\u00fcrre Nessel auf: in deinen Gassen weiden", "tokens": ["Die", "d\u00fcr\u00b7re", "Nes\u00b7sel", "auf", ":", "in", "dei\u00b7nen", "Gas\u00b7sen", "wei\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$.", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Schaafe Heerden auf und ab.", "tokens": ["Der", "Schaa\u00b7fe", "Heer\u00b7den", "auf", "und", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Auf deinen Th\u00fcrmen wohnt ein banger Schwarm von Eulen,", "tokens": ["Auf", "dei\u00b7nen", "Th\u00fcr\u00b7men", "wohnt", "ein", "ban\u00b7ger", "Schwarm", "von", "Eu\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der jammernd dein Geschick beklagt:", "tokens": ["Der", "jam\u00b7mernd", "dein", "Ge\u00b7schick", "be\u00b7klagt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Volk ist rings zerstreut, wie Stoppeln, die mit Heulen", "tokens": ["Dein", "Volk", "ist", "rings", "zer\u00b7streut", ",", "wie", "Stop\u00b7peln", ",", "die", "mit", "Heu\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$,", "PWAV", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Nordwind in die W\u00fcste jagt.", "tokens": ["Der", "Nord\u00b7wind", "in", "die", "W\u00fcs\u00b7te", "jagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie herrlich warst du einst, als Priester und Leviten,", "tokens": ["Wie", "herr\u00b7lich", "warst", "du", "einst", ",", "als", "Pries\u00b7ter", "und", "Le\u00b7vi\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ADV", "$,", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die, Roms erhabnen Vatikan", "tokens": ["Die", ",", "Roms", "er\u00b7hab\u00b7nen", "Va\u00b7ti\u00b7kan"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "NE", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu r\u00e4chen, manchen Feind des Pfaffenstolzes brieten,", "tokens": ["Zu", "r\u00e4\u00b7chen", ",", "man\u00b7chen", "Feind", "des", "Pfaf\u00b7fen\u00b7stol\u00b7zes", "brie\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu Hussens Richtplatz dich ersahn!", "tokens": ["Zu", "Hus\u00b7sens", "Richt\u00b7platz", "dich", "er\u00b7sahn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Da drang durch deine Thor' ein Schwall von schwarzen B\u00e4uchen,", "tokens": ["Da", "drang", "durch", "dei\u00b7ne", "Thor'", "ein", "Schwall", "von", "schwar\u00b7zen", "B\u00e4u\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die sich vom Evangelium", "tokens": ["Die", "sich", "vom", "E\u00b7van\u00b7ge\u00b7li\u00b7um"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie F\u00fcrsten m\u00e4steten, herbey aus allen Reichen", "tokens": ["Wie", "F\u00fcrs\u00b7ten", "m\u00e4s\u00b7te\u00b7ten", ",", "her\u00b7bey", "aus", "al\u00b7len", "Rei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVINF", "$,", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zum heiligen Synedrium.", "tokens": ["Zum", "hei\u00b7li\u00b7gen", "Sy\u00b7ned\u00b7ri\u00b7um", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Da schwand der frohe Tag den \u00fcppigen Pr\u00e4laten.", "tokens": ["Da", "schwand", "der", "fro\u00b7he", "Tag", "den", "\u00fcp\u00b7pi\u00b7gen", "Pr\u00e4\u00b7la\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beym Trinkgelag: da ward die Nacht", "tokens": ["Beym", "Trink\u00b7ge\u00b7lag", ":", "da", "ward", "die", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$.", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In feiler Dirnen Schooss, der Zehnten und Annaten", "tokens": ["In", "fei\u00b7ler", "Dir\u00b7nen", "Scho\u00b7oss", ",", "der", "Zehn\u00b7ten", "und", "An\u00b7na\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN", "$,", "ART", "NN", "KON", "NN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Mit heisser Gier verschlang, durchwacht.", "tokens": ["Mit", "heis\u00b7ser", "Gier", "ver\u00b7schlang", ",", "durch\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Bejammernswerthe Stadt! seit diesen Tagen wandten,", "tokens": ["Be\u00b7jam\u00b7merns\u00b7wert\u00b7he", "Stadt", "!", "seit", "die\u00b7sen", "Ta\u00b7gen", "wand\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verscheucht von tr\u00e4ger Lustbegier", "tokens": ["Ver\u00b7scheucht", "von", "tr\u00e4\u00b7ger", "Lust\u00b7be\u00b7gier"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und von der Schwelgerey hochw\u00fcrdiger Bachanten,", "tokens": ["Und", "von", "der", "Schwel\u00b7ge\u00b7rey", "hoch\u00b7w\u00fcr\u00b7di\u00b7ger", "Ba\u00b7ch\u00b7an\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "---+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sich Fleiss und Wohlstand weg von dir.", "tokens": ["Sich", "Fleiss", "und", "Wohl\u00b7stand", "weg", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NE", "KON", "NN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Doch fasse neuen Muth! denn sieh! es str\u00f6mt in Schaaren", "tokens": ["Doch", "fas\u00b7se", "neu\u00b7en", "Muth", "!", "denn", "sieh", "!", "es", "str\u00f6mt", "in", "Schaa\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$.", "KON", "VVIMP", "$.", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein kunstge\u00fcbtes Volk herzu,", "tokens": ["Ein", "kunst\u00b7ge\u00b7\u00fcb\u00b7tes", "Volk", "her\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und suchet, fern von Genfs unr\u00fchmlichen Gefahren,", "tokens": ["Und", "su\u00b7chet", ",", "fern", "von", "Genfs", "un\u00b7r\u00fchm\u00b7li\u00b7chen", "Ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJD", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In dir die langentw\u00f6hnte Ruh.", "tokens": ["In", "dir", "die", "lan\u00b7gent\u00b7w\u00f6hn\u00b7te", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Es f\u00fchren im Triumph in fernen Afterzeiten", "tokens": ["Es", "f\u00fch\u00b7ren", "im", "Tri\u00b7umph", "in", "fer\u00b7nen", "Af\u00b7ter\u00b7zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Vielleicht noch einst das spr\u00f6de Gl\u00fcck,", "tokens": ["Viel\u00b7leicht", "noch", "einst", "das", "spr\u00f6\u00b7de", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit dem die J\u00fcnger Roms so lange dich entzweyten,", "tokens": ["Mit", "dem", "die", "J\u00fcn\u00b7ger", "Roms", "so", "lan\u00b7ge", "dich", "ent\u00b7zwey\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "NE", "ADV", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Z\u00f6glinge Kalvins zur\u00fcck.", "tokens": ["Die", "Z\u00f6g\u00b7lin\u00b7ge", "Kal\u00b7vins", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "PTKVZ", "$."], "meter": "-++-+--+", "measure": "iambic.tetra.chol"}}}}}