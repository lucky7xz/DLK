{"dta.poem.4": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n  angenehme Morgenr\u00f6the  \n Und  \n  das Vergn\u00fcgen  \n beim Aufgang der Sonne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Morgenr\u00f6the schimmernd Pran-\ngen", "tokens": ["Der", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "schim\u00b7mernd", "Pran", "gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "TRUNC", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verk\u00fcndiget des Tages Licht,", "tokens": ["Ver\u00b7k\u00fcn\u00b7di\u00b7get", "des", "Ta\u00b7ges", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sie mit ihren Rosenwangen,", "tokens": ["Da", "sie", "mit", "ih\u00b7ren", "Ro\u00b7sen\u00b7wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Nacht und Schatten freu-", "tokens": ["Durch", "Nacht", "und", "Schat\u00b7ten", "freu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie \u00f6fnet ihre g\u00fcldne Thoren;", "tokens": ["Sie", "\u00f6f\u00b7net", "ih\u00b7re", "g\u00fcld\u00b7ne", "Tho\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dadurch der Tag wird neu ge-", "tokens": ["Da\u00b7durch", "der", "Tag", "wird", "neu", "ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "VAFIN", "ADJD", "TRUNC"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und wirft den aufgeglomnen Strahl,", "tokens": ["Und", "wirft", "den", "auf\u00b7ge\u00b7glom\u00b7nen", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der in den dunklen Luftkreis schwimmet,", "tokens": ["Der", "in", "den", "dunk\u00b7len", "Luft\u00b7kreis", "schwim\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Jm weis und rothen Farben glimmet,", "tokens": ["Jm", "weis", "und", "ro\u00b7then", "Far\u00b7ben", "glim\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "PTKVZ", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Aus einen tieffen Abgrunds-Thal.", "tokens": ["Aus", "ei\u00b7nen", "tief\u00b7fen", "Ab\u00b7grunds\u00b7Thal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie dehnet ihre hellen Fl\u00fcgel,", "tokens": ["Sie", "deh\u00b7net", "ih\u00b7re", "hel\u00b7len", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und schwingt sich immer h\u00f6her auf,", "tokens": ["Und", "schwingt", "sich", "im\u00b7mer", "h\u00f6\u00b7her", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis sie der Erden hohe H\u00fcgel", "tokens": ["Bis", "sie", "der", "Er\u00b7den", "ho\u00b7he", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bestrahlt in ihren fr\u00fchen Lauf.", "tokens": ["Be\u00b7strahlt", "in", "ih\u00b7ren", "fr\u00fc\u00b7hen", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O! was vor angenehmes Gl\u00e4nzen,", "tokens": ["O", "!", "was", "vor", "an\u00b7ge\u00b7neh\u00b7mes", "Gl\u00e4n\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Berge Gipfel sind mit Kr\u00e4nzen,", "tokens": ["Der", "Ber\u00b7ge", "Gip\u00b7fel", "sind", "mit", "Kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Von Purpurrosen \u00fcberstreut,", "tokens": ["Von", "Pur\u00b7pur\u00b7ro\u00b7sen", "\u00fc\u00b7bers\u00b7treut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bald scheints, als wenn von ihren Spizzen,", "tokens": ["Bald", "scheints", ",", "als", "wenn", "von", "ih\u00b7ren", "Spiz\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOKOM", "KOUS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die funkelnden Rubinen blizzen,", "tokens": ["Die", "fun\u00b7keln\u00b7den", "Ru\u00b7bi\u00b7nen", "bliz\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "O! h\u00f6chstvergn\u00fcgte Morgenzeit.", "tokens": ["O", "!", "h\u00f6chst\u00b7ver\u00b7gn\u00fcg\u00b7te", "Mor\u00b7gen\u00b7zeit", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Aufgang der gestrahlten R\u00f6the,", "tokens": ["Der", "Auf\u00b7gang", "der", "ge\u00b7strahl\u00b7ten", "R\u00f6\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erhebt sich bis ins feuchte Feld,", "tokens": ["Er\u00b7hebt", "sich", "bis", "ins", "feuch\u00b7te", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo sie der Aekker Kunsttapete,", "tokens": ["Wo", "sie", "der", "A\u00b7ek\u00b7ker", "Kunst\u00b7ta\u00b7pe\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Die die Natur gewirkt, erhellt.", "tokens": ["Die", "die", "Na\u00b7tur", "ge\u00b7wirkt", ",", "er\u00b7hellt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die bleichen Farben von dem Lichte,", "tokens": ["Die", "blei\u00b7chen", "Far\u00b7ben", "von", "dem", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Falln wie ein Silber ins Gesichte;", "tokens": ["Falln", "wie", "ein", "Sil\u00b7ber", "ins", "Ge\u00b7sich\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn in den tieffen Thal und Au,", "tokens": ["Wenn", "in", "den", "tief\u00b7fen", "Thal", "und", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Licht die Finsternis verdrenget,", "tokens": ["Das", "Licht", "die", "Fins\u00b7ter\u00b7nis", "ver\u00b7dren\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sich in die dunklen Schatten menget,", "tokens": ["Sich", "in", "die", "dunk\u00b7len", "Schat\u00b7ten", "men\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Entsteht ein demmricht klares Grau.", "tokens": ["Ent\u00b7steht", "ein", "demm\u00b7richt", "kla\u00b7res", "Grau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der N\u00e4chte Flor wird weggezogen,", "tokens": ["Der", "N\u00e4ch\u00b7te", "Flor", "wird", "weg\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Je h\u00f6her das entglomne Licht,", "tokens": ["Je", "h\u00f6\u00b7her", "das", "ent\u00b7glom\u00b7ne", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An den verkl\u00e4rten Himmelsbogen,", "tokens": ["An", "den", "ver\u00b7kl\u00e4r\u00b7ten", "Him\u00b7mels\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erhebt sein feurigs Angesicht.", "tokens": ["Er\u00b7hebt", "sein", "feu\u00b7rigs", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Schleier, die den Wald verstekket,", "tokens": ["Die", "Schlei\u00b7er", ",", "die", "den", "Wald", "vers\u00b7tek\u00b7ket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ihren gr\u00fcnen Kranz bedekket,", "tokens": ["Und", "ih\u00b7ren", "gr\u00fc\u00b7nen", "Kranz", "be\u00b7dek\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die werden nunmehr abgethan,", "tokens": ["Die", "wer\u00b7den", "nun\u00b7mehr", "ab\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und was mit gr\u00fcnen Schmuk gebildet,", "tokens": ["Und", "was", "mit", "gr\u00fc\u00b7nen", "Schmuk", "ge\u00b7bil\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sieht man am Morgen wie verg\u00fcldet,", "tokens": ["Sieht", "man", "am", "Mor\u00b7gen", "wie", "ver\u00b7g\u00fcl\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "NN", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "In Strahlenreicher Wonne an.", "tokens": ["In", "Strah\u00b7len\u00b7rei\u00b7cher", "Won\u00b7ne", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O! welch ein \u00fcberirdisch Glimmen,", "tokens": ["O", "!", "welch", "ein", "\u00fc\u00b7be\u00b7rir\u00b7disch", "Glim\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAT", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Fl\u00fcsse nasses Element,", "tokens": ["Der", "Fl\u00fcs\u00b7se", "nas\u00b7ses", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sieht man in regen Lustfeur schwimmen,", "tokens": ["Sieht", "man", "in", "re\u00b7gen", "Lust\u00b7feur", "schwim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Als wie ein fliessend Gold das brennt:", "tokens": ["Als", "wie", "ein", "flies\u00b7send", "Gold", "das", "brennt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJD", "NN", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df, wenn sich in bewegten Fl\u00e4chen,", "tokens": ["Da\u00df", ",", "wenn", "sich", "in", "be\u00b7weg\u00b7ten", "Fl\u00e4\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Morgenr\u00f6the Strahlen brechen,", "tokens": ["Der", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "Strah\u00b7len", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mit tausendfachen Farben strahlt,", "tokens": ["Mit", "tau\u00b7send\u00b7fa\u00b7chen", "Far\u00b7ben", "strahlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und des verj\u00fcngten Lichtes Bronnen,", "tokens": ["Und", "des", "ver\u00b7j\u00fcng\u00b7ten", "Lich\u00b7tes", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Das Bild der aufgegangnen Sonnen,", "tokens": ["Das", "Bild", "der", "auf\u00b7ge\u00b7gang\u00b7nen", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "In einen Fluthen Spiegel mahlt.", "tokens": ["In", "ei\u00b7nen", "Flut\u00b7hen", "Spie\u00b7gel", "mahlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Auf B\u00fcrger der entschlafnen Erde,", "tokens": ["Auf", "B\u00fcr\u00b7ger", "der", "ent\u00b7schlaf\u00b7nen", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jezt k\u00f6nnt ihr mit Vergn\u00fcgen sehn;", "tokens": ["Jezt", "k\u00f6nnt", "ihr", "mit", "Ver\u00b7gn\u00fc\u00b7gen", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was durch das Allmachts-Wort: Es werde", "tokens": ["Was", "durch", "das", "All\u00b7machts\u00b7Wort", ":", "Es", "wer\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jm Anfang aller Ding geschehn,", "tokens": ["Jm", "An\u00b7fang", "al\u00b7ler", "Ding", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da aus den tr\u00fcben Finsternissen,", "tokens": ["Da", "aus", "den", "tr\u00fc\u00b7ben", "Fins\u00b7ter\u00b7nis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Licht sich offenbahren m\u00fcssen.", "tokens": ["Das", "Licht", "sich", "of\u00b7fen\