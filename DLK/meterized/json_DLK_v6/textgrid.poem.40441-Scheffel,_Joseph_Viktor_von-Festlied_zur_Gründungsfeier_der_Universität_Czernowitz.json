{"textgrid.poem.40441": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Festlied zur Gr\u00fcndungsfeier der Universit\u00e4t Czernowitz", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verwundert hebt der Pruth im Schilf", "tokens": ["Ver\u00b7wun\u00b7dert", "hebt", "der", "Pru\u00b7th", "im", "Schilf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sein Haupt, das flutumschwemmte,", "tokens": ["Sein", "Haupt", ",", "das", "flu\u00b7tum\u00b7schwemm\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn hoch zu Ro\u00df, im Fr\u00fchrotschein", "tokens": ["Denn", "hoch", "zu", "Ro\u00df", ",", "im", "Fr\u00fch\u00b7rot\u00b7schein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Naht eine hohe Fremde:", "tokens": ["Naht", "ei\u00b7ne", "ho\u00b7he", "Frem\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Einst ehrten Griechenland und Rom", "tokens": ["Einst", "ehr\u00b7ten", "Grie\u00b7chen\u00b7land", "und", "Rom"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "KON", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Himmlische, die Muse;", "tokens": ["Die", "Himm\u00b7li\u00b7sche", ",", "die", "Mu\u00b7se", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Jetzt h\u00e4lt sie vor des Ostens Strom", "tokens": ["Jetzt", "h\u00e4lt", "sie", "vor", "des", "Os\u00b7tens", "Strom"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und hebt die Hand zum Gru\u00dfe!", "tokens": ["Und", "hebt", "die", "Hand", "zum", "Gru\u00b7\u00dfe", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gl\u00fcckauf, mein bergsch\u00f6n Buchenland,", "tokens": ["Gl\u00fcc\u00b7kauf", ",", "mein", "berg\u00b7sch\u00f6n", "Bu\u00b7chen\u00b7land", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "O C\u00e4cina, wie gl\u00fchst du!", "tokens": ["O", "C\u00e4\u00b7ci\u00b7na", ",", "wie", "gl\u00fchst", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWAV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich komme mit dem Morgenrot,", "tokens": ["Ich", "kom\u00b7me", "mit", "dem", "Mor\u00b7gen\u00b7rot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hauptstadt am Pruth, nun bl\u00fchst du!", "tokens": ["Haupt\u00b7stadt", "am", "Pru\u00b7th", ",", "nun", "bl\u00fchst", "du", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Ich bring' euch, wie Aurora, Licht,", "tokens": ["Ich", "bring'", "euch", ",", "wie", "Au\u00b7ro\u00b7ra", ",", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "NE", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Denn Finsternis tut Schaden;", "tokens": ["Denn", "Fins\u00b7ter\u00b7nis", "tut", "Scha\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich bringe Licht und f\u00fcrchte nicht", "tokens": ["Ich", "brin\u00b7ge", "Licht", "und", "f\u00fcrch\u00b7te", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die W\u00f6lfe der Karpathen.", "tokens": ["Die", "W\u00f6l\u00b7fe", "der", "Kar\u00b7pa\u00b7then", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr sollt mit Gott- und Weltweisheit", "tokens": ["Ihr", "sollt", "mit", "Got\u00b7t", "und", "Welt\u00b7weis\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "TRUNC", "KON", "NN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Des Sch\u00f6pfers Lob bekunden,", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "Lob", "be\u00b7kun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als Richter \u00fcben Gerechtigkeit,", "tokens": ["Als", "Rich\u00b7ter", "\u00fc\u00b7ben", "Ge\u00b7rech\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als \u00c4rzte heilen die Wunden:", "tokens": ["Als", "\u00c4rz\u00b7te", "hei\u00b7len", "die", "Wun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und jugendfrisch mit Hall und Schall", "tokens": ["Und", "ju\u00b7gend\u00b7frisch", "mit", "Hall", "und", "Schall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den freien K\u00fcnsten dienen,", "tokens": ["Den", "frei\u00b7en", "K\u00fcns\u00b7ten", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sangfr\u00f6hlich wie die Nachtigall,", "tokens": ["Sang\u00b7fr\u00f6h\u00b7lich", "wie", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Treuflei\u00dfig wie die Bienen.", "tokens": ["Treu\u00b7flei\u00b7\u00dfig", "wie", "die", "Bie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Schau auf, schon zieht und braust daher,", "tokens": ["Schau", "auf", ",", "schon", "zieht", "und", "braust", "da\u00b7her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ADV", "VVFIN", "KON", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An deinem Ufer zu wohnen,", "tokens": ["An", "dei\u00b7nem", "U\u00b7fer", "zu", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In vollem Wichs mein flottes Heer", "tokens": ["In", "vol\u00b7lem", "Wichs", "mein", "flot\u00b7tes", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Koller und Kanonen,", "tokens": ["Mit", "Kol\u00b7ler", "und", "Ka\u00b7no\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ruthenisch, deutsch, rum\u00e4nisch Blut", "tokens": ["Ru\u00b7the\u00b7nisch", ",", "deutsch", ",", "ru\u00b7m\u00e4\u00b7nisch", "Blut"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ADJD", "$,", "ADJD", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Vielz\u00fcngig miteinander!", "tokens": ["Viel\u00b7z\u00fcn\u00b7gig", "mi\u00b7tein\u00b7an\u00b7der", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und staunend h\u00f6rt der Vater Pruth", "tokens": ["Und", "stau\u00b7nend", "h\u00f6rt", "der", "Va\u00b7ter", "Pru\u00b7th"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den ersten Salamander:", "tokens": ["Den", "ers\u00b7ten", "Sa\u00b7la\u00b7man\u00b7der", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbheil dir, gewaltig \u00d6sterreich,", "tokens": ["\u00bb", "heil", "dir", ",", "ge\u00b7wal\u00b7tig", "\u00d6s\u00b7ter\u00b7reich", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "$,", "ADJD", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Heil Wissen dir, im Osten,", "tokens": ["Heil", "Wis\u00b7sen", "dir", ",", "im", "Os\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In Sprachen bunt, im Geiste gleich", "tokens": ["In", "Spra\u00b7chen", "bunt", ",", "im", "Geis\u00b7te", "gleich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "$,", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ziehn wir am Pruth auf Posten:", "tokens": ["Ziehn", "wir", "am", "Pru\u00b7th", "auf", "Pos\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPR", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Nun bl\u00fche, j\u00fcngster Musensitz,", "tokens": ["Nun", "bl\u00fc\u00b7he", ",", "j\u00fcngs\u00b7ter", "Mu\u00b7sen\u00b7sitz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Francisco-Josephina!", "tokens": ["Fran\u00b7cis\u00b7co\u00b7Jo\u00b7se\u00b7phi\u00b7na", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Frau Muse lehrt in Czernowitz", "tokens": ["Frau", "Mu\u00b7se", "lehrt", "in", "Czer\u00b7no\u00b7witz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und schirmt die Bukowina!\u00ab", "tokens": ["Und", "schirmt", "die", "Bu\u00b7ko\u00b7wi\u00b7na", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "NE", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}