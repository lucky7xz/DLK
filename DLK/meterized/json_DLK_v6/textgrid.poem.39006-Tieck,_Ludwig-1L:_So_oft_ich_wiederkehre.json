{"textgrid.poem.39006": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: So oft ich wiederkehre", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So oft ich wiederkehre", "tokens": ["So", "oft", "ich", "wie\u00b7der\u00b7keh\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Rafaels hohen Werken,", "tokens": ["Von", "Ra\u00b7faels", "ho\u00b7hen", "Wer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fchl' ich mich reicher, kr\u00e4ftiger,", "tokens": ["F\u00fchl'", "ich", "mich", "rei\u00b7cher", ",", "kr\u00e4f\u00b7ti\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Muth des Herzens w\u00e4chst,", "tokens": ["Der", "Muth", "des", "Her\u00b7zens", "w\u00e4chst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und mein ist diese Herrlichkeit.", "tokens": ["Und", "mein", "ist", "die\u00b7se", "Herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VAFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bin ich entfernt,", "tokens": ["Bin", "ich", "ent\u00b7fernt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Brennt in mir wieder die Sehnsucht auf,", "tokens": ["Brennt", "in", "mir", "wie\u00b7der", "die", "Sehn\u00b7sucht", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Die Himmelsschrift der S\u00e4le zu lesen,", "tokens": ["Die", "Him\u00b7mels\u00b7schrift", "der", "S\u00e4\u00b7le", "zu", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Und n\u00e4her, verwandter,", "tokens": ["Und", "n\u00e4\u00b7her", ",", "ver\u00b7wand\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "W\u00e4chst in meiner Seele", "tokens": ["W\u00e4chst", "in", "mei\u00b7ner", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Die Sch\u00f6nheit frisch gr\u00fcnend \u00fcppig mir.", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "frisch", "gr\u00fc\u00b7nend", "\u00fcp\u00b7pig", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "ADJD", "PPER", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Wie so anders,", "tokens": ["Wie", "so", "an\u00b7ders", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Als der Kranke zum erstenmal,", "tokens": ["Als", "der", "Kran\u00b7ke", "zum", "ers\u00b7ten\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "ADV", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.14": {"text": "Mit Thr\u00e4nen der Wehmuth,", "tokens": ["Mit", "Thr\u00e4\u00b7nen", "der", "Weh\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+--++", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Ohnm\u00e4chtigen Gef\u00fchls", "tokens": ["Ohn\u00b7m\u00e4ch\u00b7ti\u00b7gen", "Ge\u00b7f\u00fchls"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Von dort hernieder stieg.", "tokens": ["Von", "dort", "her\u00b7nie\u00b7der", "stieg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Seid mir gegr\u00fc\u00dft, ihr Genien,", "tokens": ["Seid", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "ihr", "Ge\u00b7ni\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "VVPP", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.18": {"text": "Die ihr so huldreichen Sinnes", "tokens": ["Die", "ihr", "so", "huld\u00b7rei\u00b7chen", "Sin\u00b7nes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Freundlich den Schwachen", "tokens": ["Freund\u00b7lich", "den", "Schwa\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "Wieder aufnehmt in euern heitern Kreis.", "tokens": ["Wie\u00b7der", "auf\u00b7nehmt", "in", "eu\u00b7ern", "hei\u00b7tern", "Kreis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Wie viel Schmerz und Lust", "tokens": ["Wie", "viel", "Schmerz", "und", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "KON", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.22": {"text": "Dank ich nicht euch, Himmelsgeschwister,", "tokens": ["Dank", "ich", "nicht", "euch", ",", "Him\u00b7mels\u00b7ge\u00b7schwis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "PTKNEG", "PPER", "$,", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.23": {"text": "Kunst und Poesie!", "tokens": ["Kunst", "und", "Poe\u00b7sie", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}