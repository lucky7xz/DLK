{"dta.poem.13323": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da sagt\u2019 ich ihr: mein Geist ist so Verwundrungs-voll,", "tokens": ["Da", "sagt'", "ich", "ihr", ":", "mein", "Geist", "ist", "so", "Ver\u00b7wun\u00b7drungs\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er nicht wei\u00df, wo er das Werck beginnen soll.", "tokens": ["Da\u00df", "er", "nicht", "wei\u00df", ",", "wo", "er", "das", "Werck", "be\u00b7gin\u00b7nen", "soll", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "$,", "PWAV", "PPER", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201edu wirst die K\u00f6niginn auf einem Ehren-Wagen\u201e", "tokens": ["\u201e", "du", "wirst", "die", "K\u00f6\u00b7ni\u00b7ginn", "auf", "ei\u00b7nem", "Eh\u00b7ren\u00b7Wa\u00b7gen", "\u201e"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sprach sie, bi\u00df in den Krei\u00df der sp\u00e4tsten Nachwelt tragen.\u201e", "tokens": ["Sprach", "sie", ",", "bi\u00df", "in", "den", "Krei\u00df", "der", "sp\u00e4ts\u00b7ten", "Nach\u00b7welt", "tra\u00b7gen", ".", "\u201e"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Wer ist der, fuhr ich fort, der die\u00df vollbringen kann?", "tokens": ["Wer", "ist", "der", ",", "fuhr", "ich", "fort", ",", "der", "die\u00df", "voll\u00b7brin\u00b7gen", "kann", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "PRELS", "PDS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "470\u201dFa\u00df Muth\u201d, versezte sie, fang bey dem Nahmen an:", "tokens": ["\"", "Fa\u00df", "Muth", "\"", ",", "ver\u00b7sez\u00b7te", "sie", ",", "fang", "bey", "dem", "Nah\u00b7men", "an", ":"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NN", "$(", "$,", "VVFIN", "PPER", "$,", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+++-+-+", "measure": "unknown.measure.septa"}, "line.7": {"text": "\u201ddie Grosse K\u00f6niginn von Hungarn", "tokens": ["\"", "die", "Gros\u00b7se", "K\u00f6\u00b7ni\u00b7ginn", "von", "Hun\u00b7garn"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "und von B\u00f6hmen,", "tokens": ["und", "von", "B\u00f6h\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "\u201dder man ihr Eigenthum, ihr Erb-Recht", "tokens": ["\"", "der", "man", "ihr", "Ei\u00b7gen\u00b7thum", ",", "ihr", "Er\u00b7bRecht"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "ART", "PIS", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "wollte nehmen;", "tokens": ["woll\u00b7te", "neh\u00b7men", ";"], "token_info": ["word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "\u201derzherzoginn und Frau des Lands", "tokens": ["\"", "erz\u00b7her\u00b7zo\u00b7ginn", "und", "Frau", "des", "Lands"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "KON", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "von Oesterreich;", "tokens": ["von", "O\u00b7es\u00b7ter\u00b7reich", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NE", "$."], "meter": "--+-+", "measure": "anapaest.init"}, "line.13": {"text": "\u201dein Held, dem in der Welt kein Held,", "tokens": ["\"", "ein", "Held", ",", "dem", "in", "der", "Welt", "kein", "Held", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "kein K\u00f6nig gleich:", "tokens": ["kein", "K\u00f6\u00b7nig", "gleich", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "475\u201dDie, sag, sey dein Gesang! .... Hier sah' ich sie verschwinden.", "tokens": ["\"", "Die", ",", "sag", ",", "sey", "dein", "Ge\u00b7sang", "!", "....", "Hier", "sah'", "ich", "sie", "ver\u00b7schwin\u00b7den", "."], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "$,", "VVFIN", "$,", "VAFIN", "PPOSAT", "NN", "$.", "$(", "ADV", "VVFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wo konnt\u2019 ich Schw\u00e4chster nun so viele Kr\u00e4fte finden?", "tokens": ["Wo", "konnt'", "ich", "Schw\u00e4chs\u00b7ter", "nun", "so", "vie\u00b7le", "Kr\u00e4f\u00b7te", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "NN", "ADV", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich stund in Sorg und Furcht; so macht ich den Beschlu\u00df:", "tokens": ["Ich", "stund", "in", "Sorg", "und", "Furcht", ";", "so", "macht", "ich", "den", "Be\u00b7schlu\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$.", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Bey diesem Nahmen ist das Lob ein Ueberflu\u00df;", "tokens": ["Bey", "die\u00b7sem", "Nah\u00b7men", "ist", "das", "Lob", "ein", "Ue\u00b7berf\u00b7lu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wie kann ein zitterndes, verzagtes Singen klingen,", "tokens": ["Wie", "kann", "ein", "zit\u00b7tern\u00b7des", ",", "ver\u00b7zag\u00b7tes", "Sin\u00b7gen", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "ADJA", "$,", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "480Wo deine Thaten dich, Theresia! besingen?", "tokens": ["dei\u00b7ne", "Tha\u00b7ten", "dich", ",", "The\u00b7re\u00b7sia", "!", "be\u00b7sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "$,", "NE", "$.", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}}}}