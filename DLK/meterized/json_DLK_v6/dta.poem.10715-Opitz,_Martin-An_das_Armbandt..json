{"dta.poem.10715": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "An das Armbandt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "O Bandt/ O sch\u00f6nes Bandt/ geflochten von den Haaren/", "tokens": ["O", "Bandt", "/", "O", "sch\u00f6\u00b7nes", "Bandt", "/", "ge\u00b7floch\u00b7ten", "von", "den", "Haa\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "NE", "ADJA", "NN", "$(", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die vff der Liebsten Haubt zuvor gestanden waren/", "tokens": ["Die", "vff", "der", "Liebs\u00b7ten", "Haubt", "zu\u00b7vor", "ge\u00b7stan\u00b7den", "wa\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "An Golt vnd Perlen reich vmbbunden meiner Handt/", "tokens": ["An", "Golt", "vnd", "Per\u00b7len", "reich", "vmb\u00b7bun\u00b7den", "mei\u00b7ner", "Handt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "ADJD", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zum zeichen jhrer Trew/ zu jhrer liebe Pfandt/", "tokens": ["Zum", "zei\u00b7chen", "jhrer", "Trew", "/", "zu", "jhrer", "lie\u00b7be", "Pfandt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$(", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Du hast mir nit allein die schwache Faust vmbgeben/", "tokens": ["Du", "hast", "mir", "nit", "al\u00b7lein", "die", "schwa\u00b7che", "Faust", "vmb\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "ADV", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Durch dich ist auch bestrickt mein Sinn/ mein Hertz/ mein Leben/", "tokens": ["Durch", "dich", "ist", "auch", "be\u00b7strickt", "mein", "Sinn", "/", "mein", "Hertz", "/", "mein", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADV", "VVFIN", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "O werdes Edles Pfandt/ O B\u00fcrgin jhrer hold/", "tokens": ["O", "wer\u00b7des", "Ed\u00b7les", "Pfandt", "/", "O", "B\u00fcr\u00b7gin", "jhrer", "hold", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJA", "NN", "$(", "NE", "NN", "PPOSAT", "ADJD", "$("], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.8": {"text": "An dir ist vmb vnd vmb geringers nichts als Goldt.", "tokens": ["An", "dir", "ist", "vmb", "vnd", "vmb", "ge\u00b7rin\u00b7gers", "nichts", "als", "Goldt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "APPR", "KON", "APPR", "ADV", "PIS", "KOKOM", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}