{"dta.poem.9870": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Von der stunde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr gr\u00fcnen den gem\u00fcther/", "tokens": ["Ihr", "gr\u00fc\u00b7nen", "den", "ge\u00b7m\u00fc\u00b7ther", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erkennt die sch\u00f6nen g\u00fcter/", "tokens": ["Er\u00b7kennt", "die", "sch\u00f6\u00b7nen", "g\u00fc\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die euch die edle zeit", "tokens": ["Die", "euch", "die", "ed\u00b7le", "zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In eure schoo\u00df will legen/", "tokens": ["In", "eu\u00b7re", "schoo\u00df", "will", "le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVFIN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nehmt an des H\u00f6chsten seegen/", "tokens": ["Nehmt", "an", "des", "H\u00f6chs\u00b7ten", "see\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des gl\u00fcckes g\u00fctigkeit.", "tokens": ["Des", "gl\u00fc\u00b7ckes", "g\u00fc\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Verachtet keine stunde/", "tokens": ["Ver\u00b7ach\u00b7tet", "kei\u00b7ne", "stun\u00b7de", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die aus gelehrtem munde", "tokens": ["Die", "aus", "ge\u00b7lehr\u00b7tem", "mun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Pr\u00e4gt klugen ohren ein/", "tokens": ["Pr\u00e4gt", "klu\u00b7gen", "oh\u00b7ren", "ein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Durch auserwehlte lehren/", "tokens": ["Durch", "au\u00b7ser\u00b7wehl\u00b7te", "leh\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Was GOttes ruhm zu mehren/", "tokens": ["Was", "Got\u00b7tes", "ruhm", "zu", "meh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Der welt kan dienlich seyn.", "tokens": ["Der", "welt", "kan", "dien\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Denckt nicht/ wenn eitelkeiten", "tokens": ["Denckt", "nicht", "/", "wenn", "ei\u00b7tel\u00b7kei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$(", "KOUS", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Euch von der schul ableiten/", "tokens": ["Euch", "von", "der", "schul", "ab\u00b7lei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJD", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Was eine stunde sey/", "tokens": ["Was", "ei\u00b7ne", "stun\u00b7de", "sey", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Wenn nach und nach verschwunden/", "tokens": ["Wenn", "nach", "und", "nach", "ver\u00b7schwun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "KON", "APPR", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Mehr als acht tausend stunden/", "tokens": ["Mehr", "als", "acht", "tau\u00b7send", "stun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "CARD", "CARD", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Sey erst ein jahr vorbey.", "tokens": ["Sey", "erst", "ein", "jahr", "vor\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.19": {"text": "Kan nicht in vielen dingen", "tokens": ["Kan", "nicht", "in", "vie\u00b7len", "din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Euch eine stunde bringen", "tokens": ["Euch", "ei\u00b7ne", "stun\u00b7de", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Den nutzen langer zeit?", "tokens": ["Den", "nut\u00b7zen", "lan\u00b7ger", "zeit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Kan euch aus s\u00fcssem lachen", "tokens": ["Kan", "euch", "aus", "s\u00fcs\u00b7sem", "la\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Nicht eine stunde machen", "tokens": ["Nicht", "ei\u00b7ne", "stun\u00b7de", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Des lebens traurigkeit?", "tokens": ["Des", "le\u00b7bens", "trau\u00b7rig\u00b7keit", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.25": {"text": "Wer hohe wei\u00dfheit liebet/", "tokens": ["Wer", "ho\u00b7he", "wei\u00df\u00b7heit", "lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Der tugend sich ergiebet/", "tokens": ["Der", "tu\u00b7gend", "sich", "er\u00b7gie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "H\u00e4lt jede stunde werth/", "tokens": ["H\u00e4lt", "je\u00b7de", "stun\u00b7de", "werth", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": "Denckt in des hertzens grunde", "tokens": ["Denckt", "in", "des", "hert\u00b7zens", "grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.29": {"text": "Stets an die letzte stunde/", "tokens": ["Stets", "an", "die", "letz\u00b7te", "stun\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Die er von GOtt begehrt.", "tokens": ["Die", "er", "von", "Gott", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}