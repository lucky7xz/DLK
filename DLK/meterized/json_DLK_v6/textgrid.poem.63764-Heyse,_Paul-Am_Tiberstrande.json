{"textgrid.poem.63764": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Am Tiberstrande", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn aus dem Stadtl\u00e4rm in der Korsostunde", "tokens": ["Wenn", "aus", "dem", "Stadt\u00b7l\u00e4rm", "in", "der", "Kor\u00b7sos\u00b7tun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich an den \u00f6den Tiberstrand mich rette,", "tokens": ["Ich", "an", "den", "\u00f6\u00b7den", "Ti\u00b7ber\u00b7strand", "mich", "ret\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist mir's, als ob aus seinem alten Bette", "tokens": ["Ist", "mir's", ",", "als", "ob", "aus", "sei\u00b7nem", "al\u00b7ten", "Bet\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NE", "$,", "KOKOM", "KOUS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Flu\u00df mir rauschte schauerliche Kunde", "tokens": ["Der", "Flu\u00df", "mir", "rauschte", "schau\u00b7er\u00b7li\u00b7che", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-++-+-+-", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "Von V\u00f6lkern, die er tief im schlamm'gen Grunde", "tokens": ["Von", "V\u00f6l\u00b7kern", ",", "die", "er", "tief", "im", "schlam\u00b7m'\u00b7gen", "Grun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Begrub, von Greueln, die an dieser St\u00e4tte", "tokens": ["Be\u00b7grub", ",", "von", "Greu\u00b7eln", ",", "die", "an", "die\u00b7ser", "St\u00e4t\u00b7te"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "NN", "$,", "PRELS", "APPR", "PDAT", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Jahrtausende ver\u00fcbten in die Wette,", "tokens": ["Jahr\u00b7tau\u00b7sen\u00b7de", "ver\u00b7\u00fcb\u00b7ten", "in", "die", "Wet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da Macht mit Niedertracht so gern im Bunde.", "tokens": ["Da", "Macht", "mit", "Nie\u00b7der\u00b7tracht", "so", "gern", "im", "Bun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch ist denn nicht der Strom ein junger Wandrer,", "tokens": ["Doch", "ist", "denn", "nicht", "der", "Strom", "ein", "jun\u00b7ger", "Wand\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PTKNEG", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der frisch herabsteigt vom Gebirg, dies Rom", "tokens": ["Der", "frisch", "her\u00b7ab\u00b7steigt", "vom", "Ge\u00b7birg", ",", "dies", "Rom"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJD", "VVPP", "APPRART", "NN", "$,", "PDS", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit Neugierblick in seiner Flut zu spiegeln?", "tokens": ["Mit", "Neu\u00b7gier\u00b7blick", "in", "sei\u00b7ner", "Flut", "zu", "spie\u00b7geln", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Her\u00fcberdr\u00e4ut ein Wissender, ein andrer", "tokens": ["Her\u00b7\u00fc\u00b7berd\u00b7r\u00e4ut", "ein", "Wis\u00b7sen\u00b7der", ",", "ein", "an\u00b7drer"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Blutzeuge: des Apostels Riesendom,", "tokens": ["Blut\u00b7zeu\u00b7ge", ":", "des", "A\u00b7pos\u00b7tels", "Rie\u00b7sen\u00b7dom", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "NE", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Der nie ein Beichtgeheimnis darf entsiegeln.", "tokens": ["Der", "nie", "ein", "Beicht\u00b7ge\u00b7heim\u00b7nis", "darf", "ent\u00b7sie\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}