{"textgrid.poem.56045": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: F\u00fcrstlich verw\u00f6hnte Fenster sehen immer,", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcrstlich verw\u00f6hnte Fenster sehen immer,", "tokens": ["F\u00fcrst\u00b7lich", "ver\u00b7w\u00f6hn\u00b7te", "Fens\u00b7ter", "se\u00b7hen", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "+--+-+-+-++", "measure": "iambic.hexa.invert"}, "line.2": {"text": "was manchesmal uns zu bem\u00fchn geruht:", "tokens": ["was", "man\u00b7ches\u00b7mal", "uns", "zu", "be\u00b7m\u00fchn", "ge\u00b7ruht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "PTKZU", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "die Stadt, die immer wieder, wo ein Schimmer", "tokens": ["die", "Stadt", ",", "die", "im\u00b7mer", "wie\u00b7der", ",", "wo", "ein", "Schim\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADV", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "von Himmel trifft auf ein Gef\u00fchl von Flut,", "tokens": ["von", "Him\u00b7mel", "trifft", "auf", "ein", "Ge\u00b7f\u00fchl", "von", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "sich bildet ohne irgendwann zu sein.", "tokens": ["sich", "bil\u00b7det", "oh\u00b7ne", "ir\u00b7gend\u00b7wann", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein jeder Morgen mu\u00df ihr die Opale", "tokens": ["Ein", "je\u00b7der", "Mor\u00b7gen", "mu\u00df", "ihr", "die", "O\u00b7pa\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VMFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "erst zeigen, die sie gestern trug, und Reihn", "tokens": ["erst", "zei\u00b7gen", ",", "die", "sie", "ge\u00b7stern", "trug", ",", "und", "Reihn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVINF", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,", "KON", "NE"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "von Spiegelbildern ziehn aus dem Kanale", "tokens": ["von", "Spie\u00b7gel\u00b7bil\u00b7dern", "ziehn", "aus", "dem", "Ka\u00b7na\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "und sie erinnern an die andern Male:", "tokens": ["und", "sie", "e\u00b7rin\u00b7nern", "an", "die", "an\u00b7dern", "Ma\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "dann giebt sie sich erst zu und f\u00e4llt sich ein", "tokens": ["dann", "giebt", "sie", "sich", "erst", "zu", "und", "f\u00e4llt", "sich", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "PTKVZ", "KON", "VVFIN", "PRF", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "wie eine Nymphe, die den Zeus empfing.", "tokens": ["wie", "ei\u00b7ne", "Nym\u00b7phe", ",", "die", "den", "Zeus", "emp\u00b7fing", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Ohrgeh\u00e4ng erklingt an ihrem Ohre;", "tokens": ["Das", "Ohr\u00b7ge\u00b7h\u00e4ng", "er\u00b7klingt", "an", "ih\u00b7rem", "Oh\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "sie aber hebt San Giorgio Maggiore", "tokens": ["sie", "a\u00b7ber", "hebt", "San", "Gi\u00b7or\u00b7gio", "Mag\u00b7gio\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "NE", "NE", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und l\u00e4chelt l\u00e4ssig in das sch\u00f6ne Ding.", "tokens": ["und", "l\u00e4\u00b7chelt", "l\u00e4s\u00b7sig", "in", "das", "sch\u00f6\u00b7ne", "Ding", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}