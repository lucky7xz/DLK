{"dta.poem.5956": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Bau der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jedoch sind einige,", "tokens": ["Je\u00b7doch", "sind", "ei\u00b7ni\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "(da sie gedruckt durch andere)", "tokens": ["(", "da", "sie", "ge\u00b7druckt", "durch", "an\u00b7de\u00b7re", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "VVPP", "APPR", "ADJA", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verl\u00e4nget, wie ein Ey. Auf welche Weise", "tokens": ["Ver\u00b7l\u00e4n\u00b7get", ",", "wie", "ein", "Ey", ".", "Auf", "wel\u00b7che", "Wei\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "$.", "APPR", "PWAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auch gleich die W\u00fcrbel stehn; behalten ihre Creyse", "tokens": ["Auch", "gleich", "die", "W\u00fcr\u00b7bel", "stehn", ";", "be\u00b7hal\u00b7ten", "ih\u00b7re", "Crey\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "VVINF", "$.", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch einen Zwischenstand.", "tokens": ["Doch", "ei\u00b7nen", "Zwi\u00b7schen\u00b7stand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Allein es ist bekannt,", "tokens": ["Al\u00b7lein", "es", "ist", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und w\u00fcrde man sich sehr betr\u00fcgen,", "tokens": ["Und", "w\u00fcr\u00b7de", "man", "sich", "sehr", "be\u00b7tr\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zu glauben, da\u00df ein Leer dazwischen k\u00f6nne liegen.", "tokens": ["Zu", "glau\u00b7ben", ",", "da\u00df", "ein", "Leer", "da\u00b7zwi\u00b7schen", "k\u00f6n\u00b7ne", "lie\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "ART", "NN", "PAV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die drey geeckten Zwischen-Stellen", "tokens": ["Die", "drey", "ge\u00b7eck\u00b7ten", "Zwi\u00b7schen\u00b7Stel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sind immer angef\u00fcllt, durch kleine C\u00f6rperlein", "tokens": ["Sind", "im\u00b7mer", "an\u00b7ge\u00b7f\u00fcllt", ",", "durch", "klei\u00b7ne", "C\u00f6r\u00b7per\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "VVPP", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Vom Erst- und Zweyten Element,", "tokens": ["Vom", "Er\u00b7st", "und", "Zwey\u00b7ten", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "So von den Sph\u00e4ren, die beweglich, abgetrennt", "tokens": ["So", "von", "den", "Sph\u00e4\u00b7ren", ",", "die", "be\u00b7weg\u00b7lich", ",", "ab\u00b7ge\u00b7trennt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "$,", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und abgerissen seyn.", "tokens": ["Und", "ab\u00b7ge\u00b7ris\u00b7sen", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Die, sonder da\u00df dadurch das mindste Pl\u00e4tzgen leer,", "tokens": ["Die", ",", "son\u00b7der", "da\u00df", "da\u00b7durch", "das", "minds\u00b7te", "Pl\u00e4tz\u00b7gen", "leer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KON", "KOUS", "PAV", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und ungef\u00fcllet w\u00e4r,", "tokens": ["Und", "un\u00b7ge\u00b7f\u00fcl\u00b7let", "w\u00e4r", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "In ihrem schnellen Lauf geschickt stets weg zu fliegen,", "tokens": ["In", "ih\u00b7rem", "schnel\u00b7len", "Lauf", "ge\u00b7schickt", "stets", "weg", "zu", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und auch all\u2019 Augenblick sich r\u00fcckwerts zu verf\u00fcgen.", "tokens": ["Und", "auch", "all'", "Au\u00b7gen\u00b7blick", "sich", "r\u00fcck\u00b7werts", "zu", "ver\u00b7f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "PRF", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}