{"textgrid.poem.55343": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier sind wir denn vorerst ganz still zu Haus,", "tokens": ["Hier", "sind", "wir", "denn", "vor\u00b7erst", "ganz", "still", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von T\u00fcr zu T\u00fcre sieht es lieblich aus;", "tokens": ["Von", "T\u00fcr", "zu", "T\u00fc\u00b7re", "sieht", "es", "lieb\u00b7lich", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der K\u00fcnstler froh die stillen Blicke hegt,", "tokens": ["Der", "K\u00fcnst\u00b7ler", "froh", "die", "stil\u00b7len", "Bli\u00b7cke", "hegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo Leben sich zum Leben freundlich regt.", "tokens": ["Wo", "Le\u00b7ben", "sich", "zum", "Le\u00b7ben", "freund\u00b7lich", "reg\u00b7t."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PWAV", "NN", "PRF", "APPRART", "NN", "ADJD", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und wie wir auch durch fremde Lande ziehn,", "tokens": ["Und", "wie", "wir", "auch", "durch", "frem\u00b7de", "Lan\u00b7de", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da kommt es her, da kehrt es wieder hin;", "tokens": ["Da", "kommt", "es", "her", ",", "da", "kehrt", "es", "wie\u00b7der", "hin", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wir wenden uns, wie auch die Welt entz\u00fccke,", "tokens": ["Wir", "wen\u00b7den", "uns", ",", "wie", "auch", "die", "Welt", "ent\u00b7z\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Enge zu, die uns allein begl\u00fccke.", "tokens": ["Der", "En\u00b7ge", "zu", ",", "die", "uns", "al\u00b7lein", "be\u00b7gl\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}