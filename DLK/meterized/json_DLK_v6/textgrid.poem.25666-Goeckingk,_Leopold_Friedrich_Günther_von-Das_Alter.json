{"textgrid.poem.25666": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "Das Alter", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trax sagt, er sey erst zwei und drei\u00dfig Jahr;", "tokens": ["Trax", "sagt", ",", "er", "sey", "erst", "zwei", "und", "drei\u00b7\u00dfig", "Jahr", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "CARD", "KON", "CARD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich l\u00e4ugn' es, aber Er? Er will den Taufschein bringen.", "tokens": ["Ich", "l\u00e4ugn'", "es", ",", "a\u00b7ber", "Er", "?", "Er", "will", "den", "Tauf\u00b7schein", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "PPER", "$.", "PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So h\u00f6r' ich denn, da\u00df er noch nicht geboren war,", "tokens": ["So", "h\u00f6r'", "ich", "denn", ",", "da\u00df", "er", "noch", "nicht", "ge\u00b7bo\u00b7ren", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als er und ich schon in die Schule gingen.", "tokens": ["Als", "er", "und", "ich", "schon", "in", "die", "Schu\u00b7le", "gin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KON", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}