{"textgrid.poem.37315": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Affen", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Bauer sprach zu seinem Jungen:", "tokens": ["Der", "Bau\u00b7er", "sprach", "zu", "sei\u00b7nem", "Jun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Heut in der Stadt da wirst du gaffen.", "tokens": ["Heut", "in", "der", "Stadt", "da", "wirst", "du", "gaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wir fahren hin und seh'n die Affen.", "tokens": ["Wir", "fah\u00b7ren", "hin", "und", "seh'n", "die", "Af\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es ist gelungen", "tokens": ["Es", "ist", "ge\u00b7lun\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Und um sich schief zu lachen,", "tokens": ["Und", "um", "sich", "schief", "zu", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUI", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was die f\u00fcr Streiche machen", "tokens": ["Was", "die", "f\u00fcr", "Strei\u00b7che", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und f\u00fcr Gesichter", "tokens": ["Und", "f\u00fcr", "Ge\u00b7sich\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Wie rechte B\u00f6sewichter.", "tokens": ["Wie", "rech\u00b7te", "B\u00f6\u00b7se\u00b7wich\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Sie krauen sich,", "tokens": ["Sie", "krau\u00b7en", "sich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Sie zausen sich,", "tokens": ["Sie", "zau\u00b7sen", "sich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Sie hauen sich,", "tokens": ["Sie", "hau\u00b7en", "sich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Sie lausen sich,", "tokens": ["Sie", "lau\u00b7sen", "sich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Beschnuppern dies, beknuppern das,", "tokens": ["Be\u00b7schnup\u00b7pern", "dies", ",", "be\u00b7knup\u00b7pern", "das", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PDS", "$,", "VVFIN", "PDS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und keiner g\u00f6nnt dem andern was,", "tokens": ["Und", "kei\u00b7ner", "g\u00f6nnt", "dem", "an\u00b7dern", "was", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "ADJA", "PWS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und essen tun sie mit der Hand,", "tokens": ["Und", "es\u00b7sen", "tun", "sie", "mit", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und alles tun sie mit Verstand,", "tokens": ["Und", "al\u00b7les", "tun", "sie", "mit", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und jeder stiehlt als wie ein Rabe.", "tokens": ["Und", "je\u00b7der", "stiehlt", "als", "wie", "ein", "Ra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "KOUS", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Pa\u00df auf, das siehst du heute.", "tokens": ["Pa\u00df", "auf", ",", "das", "siehst", "du", "heu\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "O Vater, rief der Knabe,", "tokens": ["O", "Va\u00b7ter", ",", "rief", "der", "Kna\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Sind Affen denn auch Leute?", "tokens": ["Sind", "Af\u00b7fen", "denn", "auch", "Leu\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Der Vater sprach: Nun ja,", "tokens": ["Der", "Va\u00b7ter", "sprach", ":", "Nun", "ja", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Nicht ganz, doch so beinah.", "tokens": ["Nicht", "ganz", ",", "doch", "so", "bei\u00b7nah", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}