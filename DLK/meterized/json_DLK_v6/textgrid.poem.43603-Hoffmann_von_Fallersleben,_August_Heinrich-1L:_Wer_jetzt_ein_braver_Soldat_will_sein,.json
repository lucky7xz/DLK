{"textgrid.poem.43603": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer jetzt ein braver Soldat will sein,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer jetzt ein braver Soldat will sein,", "tokens": ["Wer", "jetzt", "ein", "bra\u00b7ver", "Sol\u00b7dat", "will", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "VMFIN", "VAINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der stelle sich flink in unsere Reih'n,", "tokens": ["Der", "stel\u00b7le", "sich", "flink", "in", "un\u00b7se\u00b7re", "Reih'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Rum bidi bum bum! rum bidi bum bum!", "tokens": ["Rum", "bi\u00b7di", "bum", "bum", "!", "rum", "bi\u00b7di", "bum", "bum", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$.", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der stelle sich flink in unsere Reih'n!", "tokens": ["Der", "stel\u00b7le", "sich", "flink", "in", "un\u00b7se\u00b7re", "Reih'n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die deutsche Fahne, die flattert voran,", "tokens": ["Die", "deut\u00b7sche", "Fah\u00b7ne", ",", "die", "flat\u00b7tert", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir folgen ihr Alle Mann f\u00fcr Mann.", "tokens": ["Wir", "fol\u00b7gen", "ihr", "Al\u00b7le", "Mann", "f\u00fcr", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "So ziehen wir aus ganz muthig und stolz,", "tokens": ["So", "zie\u00b7hen", "wir", "aus", "ganz", "mut\u00b7hig", "und", "stolz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und ist unser Schwert auch von Eschenholz.", "tokens": ["Und", "ist", "un\u00b7ser", "Schwert", "auch", "von", "E\u00b7schen\u00b7holz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Von Eisen ist ja unser Muth,", "tokens": ["Von", "Ei\u00b7sen", "ist", "ja", "un\u00b7ser", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein eiserner Muth das Beste thut.", "tokens": ["Ein", "ei\u00b7ser\u00b7ner", "Muth", "das", "Bes\u00b7te", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Die Trommel ger\u00fchrt, da\u00df es donnert und kracht!", "tokens": ["Die", "Trom\u00b7mel", "ge\u00b7r\u00fchrt", ",", "da\u00df", "es", "don\u00b7nert", "und", "kracht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Hurrah! hurrah! nun geht's in die Schlacht!", "tokens": ["Hur\u00b7rah", "!", "hur\u00b7rah", "!", "nun", "geht's", "in", "die", "Schlacht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ITJ", "$.", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Wer hat denn das neue Lied erdacht?", "tokens": ["Wer", "hat", "denn", "das", "neu\u00b7e", "Lied", "er\u00b7dacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das haben wir Kinder uns selber gemacht.", "tokens": ["Das", "ha\u00b7ben", "wir", "Kin\u00b7der", "uns", "sel\u00b7ber", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}