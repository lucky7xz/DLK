{"textgrid.poem.36620": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich baue, darum hei\u00df' ich Bauer,", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich baue, darum hei\u00df' ich Bauer,", "tokens": ["Ich", "bau\u00b7e", ",", "da\u00b7rum", "hei\u00df'", "ich", "Bau\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PAV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich baue mit gesunder Hand,", "tokens": ["Ich", "bau\u00b7e", "mit", "ge\u00b7sun\u00b7der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht einen Thurm auf eine Mauer,", "tokens": ["Nicht", "ei\u00b7nen", "Thurm", "auf", "ei\u00b7ne", "Mau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich baue Weitzen in das Land!", "tokens": ["Ich", "bau\u00b7e", "Weit\u00b7zen", "in", "das", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Herr des Landes ist der K\u00f6nig,", "tokens": ["Der", "Herr", "des", "Lan\u00b7des", "ist", "der", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der baut den Thurm, der mich besch\u00fctzt,", "tokens": ["Der", "baut", "den", "Thurm", ",", "der", "mich", "be\u00b7sch\u00fctzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In welchem, S\u00fcnden unterth\u00e4nig,", "tokens": ["In", "wel\u00b7chem", ",", "S\u00fcn\u00b7den", "un\u00b7ter\u00b7th\u00e4\u00b7nig", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PWAT", "$,", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verr\u00e4ther, und Betr\u00fcger sitzt.", "tokens": ["Ver\u00b7r\u00e4\u00b7ther", ",", "und", "Be\u00b7tr\u00fc\u00b7ger", "sitzt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich baue redlich, in Vertrauen", "tokens": ["Ich", "bau\u00b7e", "red\u00b7lich", ",", "in", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf meinen Gott! er sieht herab,", "tokens": ["Auf", "mei\u00b7nen", "Gott", "!", "er", "sieht", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus seinem Himmel, auf mein Bauen,", "tokens": ["Aus", "sei\u00b7nem", "Him\u00b7mel", ",", "auf", "mein", "Bau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und z\u00e4hlt ihm seine Segen ab.", "tokens": ["Und", "z\u00e4hlt", "ihm", "sei\u00b7ne", "Se\u00b7gen", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Zehnf\u00e4ltig hab ich sie versp\u00fcret,", "tokens": ["Zehn\u00b7f\u00e4l\u00b7tig", "hab", "ich", "sie", "ver\u00b7sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zehn Weitzenk\u00f6rner gab er mir", "tokens": ["Zehn", "Weit\u00b7zen\u00b7k\u00f6r\u00b7ner", "gab", "er", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr eines! Herz, davon ger\u00fchret,", "tokens": ["F\u00fcr", "ei\u00b7nes", "!", "Herz", ",", "da\u00b7von", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "$.", "NN", "$,", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dank ewig, ewig ihm daf\u00fcr!", "tokens": ["Dank", "e\u00b7wig", ",", "e\u00b7wig", "ihm", "da\u00b7f\u00fcr", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "$,", "ADJD", "PPER", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dank' ihm f\u00fcr seine sanfte Regen,", "tokens": ["Dank'", "ihm", "f\u00fcr", "sei\u00b7ne", "sanf\u00b7te", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr seine Winde, seinen Thau!", "tokens": ["F\u00fcr", "sei\u00b7ne", "Win\u00b7de", ",", "sei\u00b7nen", "Thau", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In ihnen gab er seinen Segen", "tokens": ["In", "ih\u00b7nen", "gab", "er", "sei\u00b7nen", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und seine Gnade meinem Bau!", "tokens": ["Und", "sei\u00b7ne", "Gna\u00b7de", "mei\u00b7nem", "Bau", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O du, mein Feld, du sollst ihm gr\u00fcnen,", "tokens": ["O", "du", ",", "mein", "Feld", ",", "du", "sollst", "ihm", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPOSAT", "NN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als wenn du Braut der Sonne w\u00e4rst;", "tokens": ["Als", "wenn", "du", "Braut", "der", "Son\u00b7ne", "w\u00e4rst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihm, und dem K\u00f6nig will ich dienen,", "tokens": ["Ihm", ",", "und", "dem", "K\u00f6\u00b7nig", "will", "ich", "die\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KON", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zwar beyden treu, doch ihm zuerst.", "tokens": ["Zwar", "bey\u00b7den", "treu", ",", "doch", "ihm", "zu\u00b7erst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJD", "$,", "ADV", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Denn er, mein Gott, erschuf den Bauer,", "tokens": ["Denn", "er", ",", "mein", "Gott", ",", "er\u00b7schuf", "den", "Bau\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erschuf den K\u00f6nig, er ist HERR", "tokens": ["Er\u00b7schuf", "den", "K\u00f6\u00b7nig", ",", "er", "ist", "HeRR"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PPER", "VAFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der K\u00f6nige, bestimmt die Dauer", "tokens": ["Der", "K\u00f6\u00b7ni\u00b7ge", ",", "be\u00b7stimmt", "die", "Dau\u00b7er"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der K\u00f6nigreiche, m\u00e4chtiger", "tokens": ["Der", "K\u00f6\u00b7nig\u00b7rei\u00b7che", ",", "m\u00e4ch\u00b7ti\u00b7ger"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ADJA"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "In seiner kleinsten That, als alle", "tokens": ["In", "sei\u00b7ner", "kleins\u00b7ten", "That", ",", "als", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "KOUS", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gewaltige der Erde, die,", "tokens": ["Ge\u00b7wal\u00b7ti\u00b7ge", "der", "Er\u00b7de", ",", "die", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Voll seines Lobes, hin erschalle", "tokens": ["Voll", "sei\u00b7nes", "Lo\u00b7bes", ",", "hin", "er\u00b7schal\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "PPOSAT", "NN", "$,", "ADV", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu seiner Himmel Harmonie!", "tokens": ["Zu", "sei\u00b7ner", "Him\u00b7mel", "Har\u00b7mo\u00b7nie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}