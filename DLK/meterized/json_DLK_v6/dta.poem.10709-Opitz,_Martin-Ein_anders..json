{"dta.poem.10709": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Ein anders.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Die Keusche Lieb ist dises Lebens Sonne/", "tokens": ["Die", "Keu\u00b7sche", "Lieb", "ist", "di\u00b7ses", "Le\u00b7bens", "Son\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PDAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So vnser Hertz erquickt mit frewd vnd wonne/", "tokens": ["So", "vn\u00b7ser", "Hertz", "er\u00b7quickt", "mit", "frewd", "vnd", "won\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "APPR", "NN", "KON", "PWAV", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Derrote Mundt ist jhr Altar: der Ku\u00df/", "tokens": ["Der\u00b7ro\u00b7te", "Mundt", "ist", "jhr", "Al\u00b7tar", ":", "der", "Ku\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$.", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Opffer/ so man jhr verehren mu\u00df.", "tokens": ["Das", "Opf\u00b7fer", "/", "so", "man", "jhr", "ver\u00b7eh\u00b7ren", "mu\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "PIS", "PPER", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}