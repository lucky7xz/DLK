{"dta.poem.3744": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Geh ,  du schwarze Amsel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann ich schon schwarz bin,               ", "tokens": ["Wann", "ich", "schon", "schwarz", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "++-+-", "measure": "iambic.di"}, "line.2": {"text": "Schuld ist nicht mein allein,", "tokens": ["Schuld", "ist", "nicht", "mein", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "PPOSAT", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schuld hat mein Mutter gehabt,", "tokens": ["Schuld", "hat", "mein", "Mut\u00b7ter", "ge\u00b7habt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "VAPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Weil sie mich nicht gewaschen hat,", "tokens": ["Weil", "sie", "mich", "nicht", "ge\u00b7wa\u00b7schen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da ich noch klein,", "tokens": ["Da", "ich", "noch", "klein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Da ich wunderwinzig bin gesein.", "tokens": ["Da", "ich", "wun\u00b7der\u00b7win\u00b7zig", "bin", "ge\u00b7sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}