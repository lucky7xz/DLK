{"textgrid.poem.40854": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gepflanzt hat eine Rebe der Winzer vor sein Haus,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gepflanzt hat eine Rebe der Winzer vor sein Haus,", "tokens": ["Ge\u00b7pflanzt", "hat", "ei\u00b7ne", "Re\u00b7be", "der", "Win\u00b7zer", "vor", "sein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Manch frisches saft'ges Bl\u00e4ttlein treibt sie im Lenz heraus,", "tokens": ["Manch", "fri\u00b7sches", "saft'\u00b7ges", "Bl\u00e4tt\u00b7lein", "treibt", "sie", "im", "Lenz", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der Spr\u00f6\u00dfling sch\u00fcttelt freudig des Laubes \u00fcppig Gr\u00fcn,", "tokens": ["Der", "Spr\u00f6\u00df\u00b7ling", "sch\u00fct\u00b7telt", "freu\u00b7dig", "des", "Lau\u00b7bes", "\u00fcp\u00b7pig", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und gr\u00fc\u00dfet Lenz und Erde, und Erd' und Lenz gr\u00fc\u00dft ihn;", "tokens": ["Und", "gr\u00fc\u00b7\u00dfet", "Lenz", "und", "Er\u00b7de", ",", "und", "Erd'", "und", "Lenz", "gr\u00fc\u00dft", "ihn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "$,", "KON", "NN", "KON", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Und Fr\u00fchling kommt auf Fr\u00fchling, der Stamm dringt k\u00fchn hervor,", "tokens": ["Und", "Fr\u00fch\u00b7ling", "kommt", "auf", "Fr\u00fch\u00b7ling", ",", "der", "Stamm", "dringt", "k\u00fchn", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "NN", "$,", "ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und h\u00f6her, immer h\u00f6her steigt sein Gezweig empor,", "tokens": ["Und", "h\u00f6\u00b7her", ",", "im\u00b7mer", "h\u00f6\u00b7her", "steigt", "sein", "Ge\u00b7zweig", "em\u00b7por", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und reicher, immer reicher schwillt seiner Bl\u00e4tter Schaar,", "tokens": ["Und", "rei\u00b7cher", ",", "im\u00b7mer", "rei\u00b7cher", "schwillt", "sei\u00b7ner", "Bl\u00e4t\u00b7ter", "Schaar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und beut mit gr\u00fcnen Armen die erste Traube dar.", "tokens": ["Und", "beut", "mit", "gr\u00fc\u00b7nen", "Ar\u00b7men", "die", "ers\u00b7te", "Trau\u00b7be", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Winzer setzt ihm St\u00e4be, dran er sich ranken kann,", "tokens": ["Der", "Win\u00b7zer", "setzt", "ihm", "St\u00e4\u00b7be", ",", "dran", "er", "sich", "ran\u00b7ken", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "$,", "PAV", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Doch frei in luft'gem Bogen schlingt er sich stolz hinan,", "tokens": ["Doch", "frei", "in", "luft'\u00b7gem", "Bo\u00b7gen", "schlingt", "er", "sich", "stolz", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "PPER", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und Fr\u00fchling folgt auf Fr\u00fchling und Laub in F\u00fcll' entquillt", "tokens": ["Und", "Fr\u00fch\u00b7ling", "folgt", "auf", "Fr\u00fch\u00b7ling", "und", "Laub", "in", "F\u00fcll'", "ent\u00b7quillt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und deckt die ganze H\u00fctte, gleichwie ein gr\u00fcner Schild;", "tokens": ["Und", "deckt", "die", "gan\u00b7ze", "H\u00fct\u00b7te", ",", "gleich\u00b7wie", "ein", "gr\u00fc\u00b7ner", "Schild", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Und w\u00f6lbt sich dicht zur Kuppel, dran Blatt an Blatt sich dr\u00e4ngt,", "tokens": ["Und", "w\u00f6lbt", "sich", "dicht", "zur", "Kup\u00b7pel", ",", "dran", "Blatt", "an", "Blatt", "sich", "dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPRART", "NN", "$,", "PAV", "NN", "APPR", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und w\u00f6lbt sich k\u00fchl zur Laube, dran Traub' an Traube h\u00e4ngt;", "tokens": ["Und", "w\u00f6lbt", "sich", "k\u00fchl", "zur", "Lau\u00b7be", ",", "dran", "Traub'", "an", "Trau\u00b7be", "h\u00e4ngt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPRART", "NN", "$,", "PAV", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Rings flattern V\u00f6gel, die singen ihr Liedlein dort so gern;", "tokens": ["Rings", "flat\u00b7tern", "V\u00f6\u00b7gel", ",", "die", "sin\u00b7gen", "ihr", "Lied\u00b7lein", "dort", "so", "gern", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "NN", "$,", "PRELS", "VVFIN", "PPOSAT", "NN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Denn wo die Reben gl\u00fchen, bleibt auch kein S\u00e4nger fern.", "tokens": ["Denn", "wo", "die", "Re\u00b7ben", "gl\u00fc\u00b7hen", ",", "bleibt", "auch", "kein", "S\u00e4n\u00b7ger", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVINF", "$,", "VVFIN", "ADV", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Ei, Winzerin und Winzer, wie mocht' euch's Wonne sein,", "tokens": ["Ei", ",", "Win\u00b7ze\u00b7rin", "und", "Win\u00b7zer", ",", "wie", "mocht'", "euch's", "Won\u00b7ne", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,", "PWAV", "VMFIN", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Als ihr so sch\u00f6n und kr\u00e4ftig den Spr\u00f6\u00dfling saht gedeihn!", "tokens": ["Als", "ihr", "so", "sch\u00f6n", "und", "kr\u00e4f\u00b7tig", "den", "Spr\u00f6\u00df\u00b7ling", "saht", "ge\u00b7deihn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "ADJD", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Es ruhen Freundschaft, Liebe und Fried' im Laubenhaus,", "tokens": ["Es", "ru\u00b7hen", "Freund\u00b7schaft", ",", "Lie\u00b7be", "und", "Fried'", "im", "Lau\u00b7ben\u00b7haus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und Gl\u00e4serklang und Psalmton und Jubel klingt heraus!", "tokens": ["Und", "Gl\u00e4\u00b7ser\u00b7klang", "und", "Psalm\u00b7ton", "und", "Ju\u00b7bel", "klingt", "he\u00b7raus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NE", "KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "O Leonor' und Friedrich, wie mocht' euch's Wonne sein,", "tokens": ["O", "Le\u00b7o\u00b7nor'", "und", "Fried\u00b7rich", ",", "wie", "mocht'", "euch's", "Won\u00b7ne", "sein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "NE", "$,", "PWAV", "VMFIN", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-++-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Als ihr so reich und herrlich den Sohn nun saht gedeihn!", "tokens": ["Als", "ihr", "so", "reich", "und", "herr\u00b7lich", "den", "Sohn", "nun", "saht", "ge\u00b7deihn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "ADJD", "ART", "NN", "ADV", "VVFIN", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie er vom Kind zum Knaben, zum J\u00fcngling dann erbl\u00fcht", "tokens": ["Wie", "er", "vom", "Kind", "zum", "Kna\u00b7ben", ",", "zum", "J\u00fcng\u00b7ling", "dann", "er\u00b7bl\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN", "APPRART", "NN", "$,", "APPRART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dem Lenz auf ros'ger Wange und Lenz im Herzen gl\u00fcht;", "tokens": ["Dem", "Lenz", "auf", "ros'\u00b7ger", "Wan\u00b7ge", "und", "Lenz", "im", "Her\u00b7zen", "gl\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "KON", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Und wie der Welt so sorglos der Knab' entgegenblickt,", "tokens": ["Und", "wie", "der", "Welt", "so", "sorg\u00b7los", "der", "Knab'", "ent\u00b7ge\u00b7gen\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Den an des Vaters Krone nichts als der Glanz entz\u00fcckt;", "tokens": ["Den", "an", "des", "Va\u00b7ters", "Kro\u00b7ne", "nichts", "als", "der", "Glanz", "ent\u00b7z\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "PIS", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie sich des Daseins R\u00e4thsel dem J\u00fcngling dann erschlo\u00df,", "tokens": ["Wie", "sich", "des", "Da\u00b7seins", "R\u00e4th\u00b7sel", "dem", "J\u00fcng\u00b7ling", "dann", "er\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und ihm an jener Krone das Kreuz nicht deutungslos;", "tokens": ["Und", "ihm", "an", "je\u00b7ner", "Kro\u00b7ne", "das", "Kreuz", "nicht", "deu\u00b7tungs\u00b7los", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PDAT", "NN", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Wie was ihm eingepflanzet Schulwitz und Unverstand", "tokens": ["Wie", "was", "ihm", "ein\u00b7ge\u00b7pflan\u00b7zet", "Schul\u00b7witz", "und", "Un\u00b7ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PWS", "PPER", "VVFIN", "NE", "KON", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ein unfruchtbares Felsst\u00fcck an seinem Busen fand;", "tokens": ["Ein", "un\u00b7frucht\u00b7ba\u00b7res", "Fels\u00b7st\u00fcck", "an", "sei\u00b7nem", "Bu\u00b7sen", "fand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie dem, was Licht und Weisheit und Recht in ihm ges\u00e4t,", "tokens": ["Wie", "dem", ",", "was", "Licht", "und", "Weis\u00b7heit", "und", "Recht", "in", "ihm", "ge\u00b7s\u00e4t", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "$,", "PWS", "NN", "KON", "NN", "KON", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sein Herz ein \u00fcppig Erdreich, das voller Saaten steht.", "tokens": ["Sein", "Herz", "ein", "\u00fcp\u00b7pig", "Er\u00b7dreich", ",", "das", "vol\u00b7ler", "Saa\u00b7ten", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJD", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Oft sah, sein Kahlhaupt sch\u00fcttelnd, bedenklich Friedrich drein,", "tokens": ["Oft", "sah", ",", "sein", "Kahl\u00b7haupt", "sch\u00fct\u00b7telnd", ",", "be\u00b7denk\u00b7lich", "Fried\u00b7rich", "drein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPOSAT", "NN", "VVPP", "$,", "ADJD", "NE", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Gleichwie ein Lahmer beim Tanze muthwill'ger Jugendreih'n;", "tokens": ["Gleich\u00b7wie", "ein", "Lah\u00b7mer", "beim", "Tan\u00b7ze", "muthwill'\u00b7ger", "Ju\u00b7gend\u00b7reih'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Lenorens Herz doch wogte nun stolz und freudenreich,", "tokens": ["Le\u00b7no\u00b7rens", "Herz", "doch", "wog\u00b7te", "nun", "stolz", "und", "freu\u00b7den\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Oft lispelt sie wohl heimlich: sei nie dem Vater gleich!", "tokens": ["Oft", "lis\u00b7pelt", "sie", "wohl", "heim\u00b7lich", ":", "sei", "nie", "dem", "Va\u00b7ter", "gleich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$.", "VAFIN", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.10": {"line.1": {"text": "Wie herrlich, F\u00fcrstens\u00f6hne, steht ihr im Leben da!", "tokens": ["Wie", "herr\u00b7lich", ",", "F\u00fcrs\u00b7ten\u00b7s\u00f6h\u00b7ne", ",", "steht", "ihr", "im", "Le\u00b7ben", "da", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "NN", "$,", "VVFIN", "PPER", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Vom Hoffnungsstrahl wird trunken, wer euch ins Auge sah;", "tokens": ["Vom", "Hoff\u00b7nungs\u00b7strahl", "wird", "trun\u00b7ken", ",", "wer", "euch", "ins", "Au\u00b7ge", "sah", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADJD", "$,", "PWS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die stolze Morgenwolke ist euer gl\u00e4nzend Bild,", "tokens": ["Die", "stol\u00b7ze", "Mor\u00b7gen\u00b7wol\u00b7ke", "ist", "eu\u00b7er", "gl\u00e4n\u00b7zend", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wenn sie das goldne Fr\u00fchroth verschleiernd noch umquillt.", "tokens": ["Wenn", "sie", "das", "gold\u00b7ne", "Fr\u00fch\u00b7roth", "ver\u00b7schlei\u00b7ernd", "noch", "um\u00b7quillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "ADV", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "Ein Lenz seid ihr voll Bl\u00fcthen, in Knospen noch gewiegt,", "tokens": ["Ein", "Lenz", "seid", "ihr", "voll", "Bl\u00fc\u00b7then", ",", "in", "Knos\u00b7pen", "noch", "ge\u00b7wiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "NN", "$,", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ein Himmel voller Sterne, noch vom Gew\u00f6lk' umschmiegt,", "tokens": ["Ein", "Him\u00b7mel", "vol\u00b7ler", "Ster\u00b7ne", ",", "noch", "vom", "Ge\u00b7w\u00f6l\u00b7k'", "um\u00b7schmiegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ein Meer seid ihr voll Perlen, bedeckt von Fluthennacht,", "tokens": ["Ein", "Meer", "seid", "ihr", "voll", "Per\u00b7len", ",", "be\u00b7deckt", "von", "Flut\u00b7hen\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "NN", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ein Berg von Diamanten, verborgen noch im Schacht.", "tokens": ["Ein", "Berg", "von", "Di\u00b7a\u00b7man\u00b7ten", ",", "ver\u00b7bor\u00b7gen", "noch", "im", "Schacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "VVPP", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.12": {"line.1": {"text": "Heil, wenn einst euer Tag ruft! Das Fr\u00fchroth flammt hervor,", "tokens": ["Heil", ",", "wenn", "einst", "eu\u00b7er", "Tag", "ruft", "!", "Das", "Fr\u00fch\u00b7roth", "flammt", "her\u00b7vor", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ADV", "PPOSAT", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Demanten, Sterne, Perlen und Bl\u00fcthen tauchen empor!", "tokens": ["De\u00b7man\u00b7ten", ",", "Ster\u00b7ne", ",", "Per\u00b7len", "und", "Bl\u00fc\u00b7then", "tau\u00b7chen", "em\u00b7por", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dann streut nicht als Almosen dem Volk eu'r Morgenlicht,", "tokens": ["Dann", "streut", "nicht", "als", "Al\u00b7mo\u00b7sen", "dem", "Volk", "eu'r", "Mor\u00b7gen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "KOUS", "NN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sein langes stilles Hoffen schuf euch's zur sch\u00f6nen Pflicht!", "tokens": ["Sein", "lan\u00b7ges", "stil\u00b7les", "Hof\u00b7fen", "schuf", "euch's", "zur", "sch\u00f6\u00b7nen", "Pflicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "VVFIN", "PIS", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}