{"dta.poem.18646": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "A ls eine kluge  D ame  A ebtissen  \n wurde/ im Nahmen eines andern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Wil Aue/ Berg und Thal die Pracht des Sommers k\u00fcssen/", "tokens": ["Wil", "Au\u00b7e", "/", "Berg", "und", "Thal", "die", "Pracht", "des", "Som\u00b7mers", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "$(", "NN", "KON", "NN", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Betritt Aurorens Fu\u00df die gr\u00fcne Fr\u00fchlings-Bahn/", "tokens": ["Be\u00b7tritt", "Au\u00b7ro\u00b7rens", "Fu\u00df", "die", "gr\u00fc\u00b7ne", "Fr\u00fch\u00b7lings\u00b7Bahn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kan Flora sich verm\u00e4hlt mit ihren Zephier wissen/", "tokens": ["Kan", "Flo\u00b7ra", "sich", "ver\u00b7m\u00e4hlt", "mit", "ih\u00b7ren", "Ze\u00b7phier", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PRF", "VVPP", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und strahlt der Sonnen Gold uns wieder lieblich an.", "tokens": ["Und", "strahlt", "der", "Son\u00b7nen", "Gold", "uns", "wie\u00b7der", "lieb\u00b7lich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So sag' ich di\u00df geschicht/ O Kleinod unsrer Erden!", "tokens": ["So", "sag'", "ich", "di\u00df", "ge\u00b7schicht", "/", "O", "Klei\u00b7nod", "uns\u00b7rer", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "VVPP", "$(", "NE", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df Himmel und Natur ihr selber dienstbar werden.", "tokens": ["Da\u00df", "Him\u00b7mel", "und", "Na\u00b7tur", "ihr", "sel\u00b7ber", "dienst\u00b7bar", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn was vom Himmel stammt/ mu\u00df auch der Himmel lieben/", "tokens": ["Denn", "was", "vom", "Him\u00b7mel", "stammt", "/", "mu\u00df", "auch", "der", "Him\u00b7mel", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "NN", "VVFIN", "$(", "VMFIN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Baum entzieht die Krafft den edlen Zweigen nicht.", "tokens": ["Ein", "Baum", "ent\u00b7zieht", "die", "Krafft", "den", "ed\u00b7len", "Zwei\u00b7gen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und dessen reiner Sinn stets himmlisch ist geblieben/", "tokens": ["Und", "des\u00b7sen", "rei\u00b7ner", "Sinn", "stets", "himm\u00b7lisch", "ist", "ge\u00b7blie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADJA", "NN", "ADV", "ADJD", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem gl\u00e4ntzt die Ehren-Cron und seiner Gnaden-Licht.", "tokens": ["Dem", "gl\u00e4ntzt", "die", "Eh\u00b7ren\u00b7Cron", "und", "sei\u00b7ner", "Gna\u00b7den\u00b7Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was Wunders/ wenn sich nun bey dero neuen Freuden", "tokens": ["Was", "Wun\u00b7ders", "/", "wenn", "sich", "nun", "bey", "de\u00b7ro", "neu\u00b7en", "Freu\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "$(", "KOUS", "PRF", "ADV", "APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-++-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "So Himmel als Natur mit frohen Schmucke Kleiden.", "tokens": ["So", "Him\u00b7mel", "als", "Na\u00b7tur", "mit", "fro\u00b7hen", "Schmu\u00b7cke", "Klei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOUS", "NN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dem folgt die Tugend nach/ und ruffet mit Vergn\u00fcgen:", "tokens": ["Dem", "folgt", "die", "Tu\u00b7gend", "nach", "/", "und", "ruf\u00b7fet", "mit", "Ver\u00b7gn\u00fc\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "$(", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schaut hier mein Ebenbild mit rechten Augen an/", "tokens": ["Schaut", "hier", "mein", "E\u00b7ben\u00b7bild", "mit", "rech\u00b7ten", "Au\u00b7gen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das durch die Trefflichkeit die Neider kan besiegen/", "tokens": ["Das", "durch", "die", "Treff\u00b7lich\u00b7keit", "die", "Nei\u00b7der", "kan", "be\u00b7sie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und welches/ so wie ich vollkommen heissen kan.", "tokens": ["Und", "wel\u00b7ches", "/", "so", "wie", "ich", "voll\u00b7kom\u00b7men", "heis\u00b7sen", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "$(", "ADV", "KOKOM", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum wer sich meiner Fahn will untergeben nennen/", "tokens": ["Drum", "wer", "sich", "mei\u00b7ner", "Fahn", "will", "un\u00b7ter\u00b7ge\u00b7ben", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PWS", "PRF", "PPOSAT", "NN", "VMFIN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.6": {"text": "Der la\u00df' in seiner Brust ein Freuden-Feuer brennen.", "tokens": ["Der", "la\u00df'", "in", "sei\u00b7ner", "Brust", "ein", "Freu\u00b7den\u00b7Feu\u00b7er", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVIMP", "APPR", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Klugheit kommt hierauf und legt die Kostbarkeiten/", "tokens": ["Die", "Klug\u00b7heit", "kommt", "hier\u00b7auf", "und", "legt", "die", "Kost\u00b7bar\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "KON", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was sie sch\u00e4tzbars hat/ zu ihren F\u00fcssen hin", "tokens": ["Und", "was", "sie", "sch\u00e4tz\u00b7bars", "hat", "/", "zu", "ih\u00b7ren", "F\u00fcs\u00b7sen", "hin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "ADV", "VAFIN", "$(", "APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hier spricht sie/ ehret recht der Sinnen Trefflichkeiten/", "tokens": ["Hier", "spricht", "sie", "/", "eh\u00b7ret", "recht", "der", "Sin\u00b7nen", "Treff\u00b7lich\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "VVFIN", "ADV", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und dencket/ da\u00df ich hier in meiner Hoff-Stadt bin.", "tokens": ["Und", "den\u00b7cket", "/", "da\u00df", "ich", "hier", "in", "mei\u00b7ner", "Hoff\u00b7Stadt", "bin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer so begl\u00fcckt will seyn/ mu\u00df so viel Klugheit zeigen/", "tokens": ["Wer", "so", "be\u00b7gl\u00fcckt", "will", "seyn", "/", "mu\u00df", "so", "viel", "Klug\u00b7heit", "zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VMFIN", "VAINF", "$(", "VMFIN", "ADV", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So kan er auf die Bahn der hohen Ehren steigen.", "tokens": ["So", "kan", "er", "auf", "die", "Bahn", "der", "ho\u00b7hen", "Eh\u00b7ren", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Das Kloster will sich auch mit gleichen Lorbern kr\u00f6nen/", "tokens": ["Das", "Klos\u00b7ter", "will", "sich", "auch", "mit", "glei\u00b7chen", "Lor\u00b7bern", "kr\u00f6\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df es an Fr\u00f6mmigkeit nun eine Esther k\u00fc\u00dft/", "tokens": ["Da\u00df", "es", "an", "Fr\u00f6m\u00b7mig\u00b7keit", "nun", "ei\u00b7ne", "E\u00b7sther", "k\u00fc\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das sich sein Gl\u00fcck und wohl auf feste S\u00e4ulen lehnen/", "tokens": ["Das", "sich", "sein", "Gl\u00fcck", "und", "wohl", "auf", "fes\u00b7te", "S\u00e4u\u00b7len", "leh\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "PPOSAT", "NN", "KON", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das seine St\u00fctze recht zum Atlas worden ist/", "tokens": ["Das", "sei\u00b7ne", "St\u00fct\u00b7ze", "recht", "zum", "At\u00b7las", "wor\u00b7den", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADJD", "APPRART", "NN", "VAPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Esspricht: Der Himmel l\u00e4st mir g\u00fcldne Zeiten sp\u00fcren/", "tokens": ["Es\u00b7spricht", ":", "Der", "Him\u00b7mel", "l\u00e4st", "mir", "g\u00fcld\u00b7ne", "Zei\u00b7ten", "sp\u00fc\u00b7ren", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VVFIN", "PPER", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weil so viel Tugenden anitzt das Scepter f\u00fchren.", "tokens": ["Weil", "so", "viel", "Tu\u00b7gen\u00b7den", "a\u00b7nitzt", "das", "Scep\u00b7ter", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der Neid tritt auch heran mit gantz erblasten Wangen/", "tokens": ["Der", "Neid", "tritt", "auch", "he\u00b7ran", "mit", "gantz", "er\u00b7blas\u00b7ten", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "APPR", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Schaam verstellet ihn/ und bringt die Worte raus:", "tokens": ["Die", "Schaam", "ver\u00b7stel\u00b7let", "ihn", "/", "und", "bringt", "die", "Wor\u00b7te", "raus", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So ist die Sonne doch durch Dufft und Nebel gangen.", "tokens": ["So", "ist", "die", "Son\u00b7ne", "doch", "durch", "Dufft", "und", "Ne\u00b7bel", "gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kein Sturm und Wetter l\u00f6scht so starcke Strahlen aus.", "tokens": ["Kein", "Sturm", "und", "Wet\u00b7ter", "l\u00f6scht", "so", "star\u00b7cke", "Strah\u00b7len", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "VVFIN", "ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kommt lernet Sterbliche/ die auch der Neid bekrieget/", "tokens": ["Kommt", "ler\u00b7net", "Sterb\u00b7li\u00b7che", "/", "die", "auch", "der", "Neid", "be\u00b7krie\u00b7get", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "NN", "$(", "PRELS", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df eurer Tugend-Macht wie diese dennoch sieget.", "tokens": ["Da\u00df", "eu\u00b7rer", "Tu\u00b7gen\u00b7dMacht", "wie", "die\u00b7se", "den\u00b7noch", "sie\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KOKOM", "PDS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Und denn will auch ein Knecht die tieffe Pflicht bezahlen/", "tokens": ["Und", "denn", "will", "auch", "ein", "Knecht", "die", "tief\u00b7fe", "Pflicht", "be\u00b7zah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ehret was man jetzt der Ehren w\u00fcrdig sch\u00e4tzt/", "tokens": ["Und", "eh\u00b7ret", "was", "man", "jetzt", "der", "Eh\u00b7ren", "w\u00fcr\u00b7dig", "sch\u00e4tzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "PIS", "ADV", "ART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er spricht: Kan mich ein Blick von der", "tokens": ["Er", "spricht", ":", "Kan", "mich", "ein", "Blick", "von", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "VMFIN", "PRF", "ART", "NN", "APPR", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So bin ich dem Gel\u00fcck schon in den Schoo\u00df gesetzt.", "tokens": ["So", "bin", "ich", "dem", "Ge\u00b7l\u00fcck", "schon", "in", "den", "Schoo\u00df", "ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word",