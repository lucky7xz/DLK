{"dta.poem.8171": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Eine Fremde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Stern am Frauen-Firmament;", "tokens": ["Ein", "Stern", "am", "Frau\u00b7en\u00b7Fir\u00b7ma\u00b7ment", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie sprach in unsres Volkes Weise,", "tokens": ["Sie", "sprach", "in", "uns\u00b7res", "Vol\u00b7kes", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur leis mit klagendem Accent.", "tokens": ["Nur", "leis", "mit", "kla\u00b7gen\u00b7dem", "Ac\u00b7cent", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du h\u00f6rtest niemals heim verlangen", "tokens": ["Du", "h\u00f6r\u00b7test", "nie\u00b7mals", "heim", "ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den stolzen Mund der sch\u00f6nen Frau;", "tokens": ["Den", "stol\u00b7zen", "Mund", "der", "sch\u00f6\u00b7nen", "Frau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur auf den s\u00fcdlich blassen Wangen", "tokens": ["Nur", "auf", "den", "s\u00fcd\u00b7lich", "blas\u00b7sen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und \u00fcber der gew\u00f6lbten Brau", "tokens": ["Und", "\u00fc\u00b7ber", "der", "ge\u00b7w\u00f6lb\u00b7ten", "Brau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Lag noch Granadas Mondenschimmer,", "tokens": ["Lag", "noch", "Gra\u00b7na\u00b7das", "Mon\u00b7den\u00b7schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NE", "NE", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Den sie vertauscht um unsern Strand;", "tokens": ["Den", "sie", "ver\u00b7tauscht", "um", "un\u00b7sern", "Strand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und ihre Augen dachten immer", "tokens": ["Und", "ih\u00b7re", "Au\u00b7gen", "dach\u00b7ten", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "An ihr begl\u00e4nztes Heimathland.", "tokens": ["An", "ihr", "be\u00b7gl\u00e4nz\u00b7tes", "Hei\u00b7mat\u00b7hland", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}