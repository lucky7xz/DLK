{"textgrid.poem.60100": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bis in des Landmanns Furchen sieht", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bis in des Landmanns Furchen sieht", "tokens": ["Bis", "in", "des", "Land\u00b7manns", "Fur\u00b7chen", "sieht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Lerch' im h\u00f6chsten Flug;", "tokens": ["Die", "Ler\u00b7ch'", "im", "h\u00f6chs\u00b7ten", "Flug", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Aus goldnen Wolken t\u00f6nt ihr Lied", "tokens": ["Aus", "gold\u00b7nen", "Wol\u00b7ken", "t\u00f6nt", "ihr", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Herab auf seinen Pflug.", "tokens": ["Her\u00b7ab", "auf", "sei\u00b7nen", "Pflug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Volkes-Muse schwebt, wie sie,", "tokens": ["Die", "Vol\u00b7kes\u00b7Mu\u00b7se", "schwebt", ",", "wie", "sie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vertraut zum Arbeitsmann,", "tokens": ["Ver\u00b7traut", "zum", "Ar\u00b7beits\u00b7mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und stimmt in leichter Melodie", "tokens": ["Und", "stimmt", "in", "leich\u00b7ter", "Me\u00b7lo\u00b7die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein frommes Loblieb an.", "tokens": ["Ein", "from\u00b7mes", "Lob\u00b7lieb", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Des frommen Lieds gedenken wir", "tokens": ["Des", "from\u00b7men", "Lieds", "ge\u00b7den\u00b7ken", "wir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beym heut'gen Becher-Klang,", "tokens": ["Beym", "heut'\u00b7gen", "Be\u00b7cher\u00b7Klang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und weihn den Kranz von Aehren hier", "tokens": ["Und", "weihn", "den", "Kranz", "von", "A\u00b7eh\u00b7ren", "hier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dem S\u00e4nger, der es sang.", "tokens": ["Dem", "S\u00e4n\u00b7ger", ",", "der", "es", "sang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}