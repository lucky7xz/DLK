{"textgrid.poem.36964": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was forscht ihr nur und gr\u00fcbelt und klaubt,", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was forscht ihr nur und gr\u00fcbelt und klaubt,", "tokens": ["Was", "forscht", "ihr", "nur", "und", "gr\u00fc\u00b7belt", "und", "klaubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ihr dummen, gelehrten Wichte!", "tokens": ["Ihr", "dum\u00b7men", ",", "ge\u00b7lehr\u00b7ten", "Wich\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Was uns fr\u00fcher Allergn\u00e4digst erlaubt", "tokens": ["Was", "uns", "fr\u00fc\u00b7her", "Al\u00b7lerg\u00b7n\u00e4\u00b7digst", "er\u00b7laubt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADJD", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im K\u00f6nigsstaub der Geschichte?", "tokens": ["Im", "K\u00f6\u00b7nigs\u00b7staub", "der", "Ge\u00b7schich\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wir wollen die Gnaden auf Eselsfell nicht!", "tokens": ["Wir", "wol\u00b7len", "die", "Gna\u00b7den", "auf", "E\u00b7sels\u00b7fell", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "NN", "PTKNEG", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Wir sind auch den Todten nicht Knechte!", "tokens": ["Wir", "sind", "auch", "den", "Tod\u00b7ten", "nicht", "Knech\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "PTKNEG", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Wir wollen, was uns der Himmel zuspricht:", "tokens": ["Wir", "wol\u00b7len", ",", "was", "uns", "der", "Him\u00b7mel", "zu\u00b7spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Unsere ewigen, g\u00f6ttlichen Rechte!", "tokens": ["Un\u00b7se\u00b7re", "e\u00b7wi\u00b7gen", ",", "g\u00f6tt\u00b7li\u00b7chen", "Rech\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Und w\u00e4re von Moses Zeiten auch her", "tokens": ["Und", "w\u00e4\u00b7re", "von", "Mo\u00b7ses", "Zei\u00b7ten", "auch", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "NE", "NN", "ADV", "APZR"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Despotenlauf die Geschichte,", "tokens": ["Ein", "Des\u00b7po\u00b7ten\u00b7lauf", "die", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wir wollten doch keine Despoten mehr,", "tokens": ["Wir", "woll\u00b7ten", "doch", "kei\u00b7ne", "Des\u00b7po\u00b7ten", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIAT", "NN", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ihr dummen, gelehrten Wichte!", "tokens": ["Ihr", "dum\u00b7men", ",", "ge\u00b7lehr\u00b7ten", "Wich\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wir k\u00f6nnen uns selber regieren schon,", "tokens": ["Wir", "k\u00f6n\u00b7nen", "uns", "sel\u00b7ber", "re\u00b7gie\u00b7ren", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Denn wir sind M\u00e4nner, nicht Kinder!", "tokens": ["Denn", "wir", "sind", "M\u00e4n\u00b7ner", ",", "nicht", "Kin\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "$,", "PTKNEG", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wir sind gescheidter als der ganze Thron", "tokens": ["Wir", "sind", "ge\u00b7scheid\u00b7ter", "als", "der", "gan\u00b7ze", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mit seinem erblichen S\u00fcnder.", "tokens": ["Mit", "sei\u00b7nem", "er\u00b7bli\u00b7chen", "S\u00fcn\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und st\u00e4nde vom freien Gedanken Nichts", "tokens": ["Und", "st\u00e4n\u00b7de", "vom", "frei\u00b7en", "Ge\u00b7dan\u00b7ken", "Nichts"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "In der langen Herrschergeschichte;", "tokens": ["In", "der", "lan\u00b7gen", "Herr\u00b7scher\u00b7ge\u00b7schich\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "W\u00e4r's finster gewesen vom Tage des Lichts,", "tokens": ["W\u00e4r's", "fins\u00b7ter", "ge\u00b7we\u00b7sen", "vom", "Ta\u00b7ge", "des", "Lichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VAPP", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ihr dummen, gelehrten Wichte:", "tokens": ["Ihr", "dum\u00b7men", ",", "ge\u00b7lehr\u00b7ten", "Wich\u00b7te", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wir wollten doch den Gedanken frei!", "tokens": ["Wir", "woll\u00b7ten", "doch", "den", "Ge\u00b7dan\u00b7ken", "frei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Mit den alten Freiheiten ist's vorbei;", "tokens": ["Mit", "den", "al\u00b7ten", "Frei\u00b7hei\u00b7ten", "ist's", "vor\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "PTKVZ", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Die Freiheit! die mu\u00df uns werden.", "tokens": ["Die", "Frei\u00b7heit", "!", "die", "mu\u00df", "uns", "wer\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PDS", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Wir h\u00e4ngen uns selber nun und nie", "tokens": ["Wir", "h\u00e4n\u00b7gen", "uns", "sel\u00b7ber", "nun", "und", "nie"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "KON", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Am Weltgericht der Geschichte! \u2013", "tokens": ["Am", "Welt\u00b7ge\u00b7richt", "der", "Ge\u00b7schich\u00b7te", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Denn wir hassen auch ihre Despotie,", "tokens": ["Denn", "wir", "has\u00b7sen", "auch", "ih\u00b7re", "Des\u00b7po\u00b7tie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Ihr dummen, gelehrten Wichte!", "tokens": ["Ihr", "dum\u00b7men", ",", "ge\u00b7lehr\u00b7ten", "Wich\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und wenn die vergilbten Bl\u00e4tter und Roll'n", "tokens": ["Und", "wenn", "die", "ver\u00b7gilb\u00b7ten", "Bl\u00e4t\u00b7ter", "und", "Roll'n"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "KON", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Unser Fordern historisch verw\u00fcrfen:", "tokens": ["Un\u00b7ser", "For\