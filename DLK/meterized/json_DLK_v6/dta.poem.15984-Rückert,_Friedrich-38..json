{"dta.poem.15984": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "38.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist die Sch\u00f6nheit, Herz? das Spiegelbild der Liebe.", "tokens": ["Was", "ist", "die", "Sch\u00f6n\u00b7heit", ",", "Herz", "?", "das", "Spie\u00b7gel\u00b7bild", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$,", "NN", "$.", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Liebe f\u00fchlte Schmerz, da\u00df ungeliebt sie bliebe.", "tokens": ["Die", "Lie\u00b7be", "f\u00fchl\u00b7te", "Schmerz", ",", "da\u00df", "un\u00b7ge\u00b7liebt", "sie", "blie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,", "KOUS", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Thr\u00e4ne, die ihr quoll, mu\u00dft' ihr zum Spiegel dienen;", "tokens": ["Die", "Thr\u00e4\u00b7ne", ",", "die", "ihr", "quoll", ",", "mu\u00dft'", "ihr", "zum", "Spie\u00b7gel", "die\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie kannte selbst sich nicht, wie sie sich drin erschienen.", "tokens": ["Sie", "kann\u00b7te", "selbst", "sich", "nicht", ",", "wie", "sie", "sich", "drin", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PRF", "PTKNEG", "$,", "PWAV", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sie rief: O sch\u00f6n! Und Sch\u00f6n hei\u00dft seitdem dieses Bild,", "tokens": ["Sie", "rief", ":", "O", "sch\u00f6n", "!", "Und", "Sch\u00f6n", "hei\u00dft", "seit\u00b7dem", "die\u00b7ses", "Bild", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NE", "ADJD", "$.", "KON", "NE", "VVFIN", "PAV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das aus dem feuchten Grund des Liebespiegels quillt.", "tokens": ["Das", "aus", "dem", "feuch\u00b7ten", "Grund", "des", "Lie\u00b7be\u00b7spie\u00b7gels", "quillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Spiegel und das Bild darin ist uns geblieben;", "tokens": ["Der", "Spie\u00b7gel", "und", "das", "Bild", "da\u00b7rin", "ist", "uns", "ge\u00b7blie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "PAV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wer die Sch\u00f6nheit sieht, der mu\u00df die Sch\u00f6nheit lieben.", "tokens": ["Und", "wer", "die", "Sch\u00f6n\u00b7heit", "sieht", ",", "der", "mu\u00df", "die", "Sch\u00f6n\u00b7heit", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$,", "ART", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}