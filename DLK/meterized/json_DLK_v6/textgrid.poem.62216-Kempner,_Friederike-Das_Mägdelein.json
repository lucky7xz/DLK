{"textgrid.poem.62216": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Das M\u00e4gdelein", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich traf einmal im fremden Land,", "tokens": ["Ich", "traf", "ein\u00b7mal", "im", "frem\u00b7den", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein M\u00e4gdlein zierlich und gewandt;", "tokens": ["Ein", "M\u00e4gd\u00b7lein", "zier\u00b7lich", "und", "ge\u00b7wandt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbwo kommst Du her, wo weilest Du?", "tokens": ["\u00bb", "wo", "kommst", "Du", "her", ",", "wo", "wei\u00b7lest", "Du", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich finde im fremden Land nicht Ruh'!\u00ab", "tokens": ["Ich", "fin\u00b7de", "im", "frem\u00b7den", "Land", "nicht", "Ruh'", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "PTKNEG", "NN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbbist Du ein deutsches M\u00e4gdelein?", "tokens": ["\u00bb", "bist", "Du", "ein", "deut\u00b7sches", "M\u00e4g\u00b7del\u00b7ein", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geboren an dem deutschen Rhein? \u2013\u00ab", "tokens": ["Ge\u00b7bo\u00b7ren", "an", "dem", "deut\u00b7schen", "Rhein", "?", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NE", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbmein Vater war ein Kriegersmann,", "tokens": ["\u00bb", "mein", "Va\u00b7ter", "war", "ein", "Krie\u00b7gers\u00b7mann", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Mutter keine Seide spann! \u2013\u00ab", "tokens": ["Die", "Mut\u00b7ter", "kei\u00b7ne", "Sei\u00b7de", "spann", "!", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VVFIN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbwie kamst Du in das fremde Land?\u00ab", "tokens": ["\u00bb", "wie", "kamst", "Du", "in", "das", "frem\u00b7de", "Land", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bban eines Fremden falscher Hand.\u00ab", "tokens": ["\u00bb", "an", "ei\u00b7nes", "Frem\u00b7den", "fal\u00b7scher", "Hand", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbdie Treue wohnt in Deutschland nur;", "tokens": ["\u00bb", "die", "Treu\u00b7e", "wohnt", "in", "Deutschland", "nur", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "APPR", "NE", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von ihr ist hier, ach, keine Spur! \u2013\u00ab", "tokens": ["Von", "ihr", "ist", "hier", ",", "ach", ",", "kei\u00b7ne", "Spur", "!", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADV", "$,", "ITJ", "$,", "PIAT", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}