{"textgrid.poem.36769": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Itali\u00e4nischer", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hurtig, Postillon! Cospetto die Bacco! Die Pferde", "tokens": ["Hur\u00b7tig", ",", "Pos\u00b7til\u00b7lon", "!", "Cos\u00b7pet\u00b7to", "die", "Bac\u00b7co", "!", "Die", "Pfer\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "$,", "NN", "$.", "NE", "ART", "NE", "$.", "ART", "NN"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Laufen wie M\u00e4hren und ich habe nicht l\u00e4nger Geduld.", "tokens": ["Lau\u00b7fen", "wie", "M\u00e4h\u00b7ren", "und", "ich", "ha\u00b7be", "nicht", "l\u00e4n\u00b7ger", "Ge\u00b7duld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "KON", "PPER", "VAFIN", "PTKNEG", "ADJD", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Das will Eile! Der H\u00fcgel in Rom sind sieben, und alle", "tokens": ["Das", "will", "Ei\u00b7le", "!", "Der", "H\u00fc\u00b7gel", "in", "Rom", "sind", "sie\u00b7ben", ",", "und", "al\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VMFIN", "NN", "$.", "ART", "NN", "APPR", "NE", "VAFIN", "CARD", "$,", "KON", "PIAT"], "meter": "-+---+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Mu\u00df ich haben, und noch jeden von hinten und vorn.", "tokens": ["Mu\u00df", "ich", "ha\u00b7ben", ",", "und", "noch", "je\u00b7den", "von", "hin\u00b7ten", "und", "vorn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VAFIN", "$,", "KON", "ADV", "PIAT", "APPR", "ADV", "KON", "ADV", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Hab' ich die Extrapost doch bezahlt! Drum hurtig, Herr Schwager,", "tokens": ["Hab'", "ich", "die", "Ext\u00b7ra\u00b7post", "doch", "be\u00b7zahlt", "!", "Drum", "hur\u00b7tig", ",", "Herr", "Schwa\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "VVPP", "$.", "PAV", "ADJD", "$,", "NN", "NN", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "So in Flug und Galopp nehm' ich das Ding mir hinweg.", "tokens": ["So", "in", "Flug", "und", "Ga\u00b7lopp", "nehm'", "ich", "das", "Ding", "mir", "hin\u00b7weg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "VVFIN", "PPER", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}