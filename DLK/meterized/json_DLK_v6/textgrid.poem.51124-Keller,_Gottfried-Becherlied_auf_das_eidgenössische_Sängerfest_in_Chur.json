{"textgrid.poem.51124": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Becherlied auf das eidgen\u00f6ssische S\u00e4ngerfest in Chur", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Traube Saft behagt dem Mund,", "tokens": ["Der", "Trau\u00b7be", "Saft", "be\u00b7hagt", "dem", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch M\u00fch erheischt der edle Wein,", "tokens": ["Doch", "M\u00fch", "er\u00b7heischt", "der", "ed\u00b7le", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und blitzt des Bechers k\u00f6stlich Rund:", "tokens": ["Und", "blitzt", "des", "Be\u00b7chers", "k\u00f6st\u00b7lich", "Rund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Silber will gegraben sein;", "tokens": ["Sein", "Sil\u00b7ber", "will", "ge\u00b7gra\u00b7ben", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann harret erst noch auf das Erz", "tokens": ["Dann", "har\u00b7ret", "erst", "noch", "auf", "das", "Erz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Schmiedes kunsterfahrne Hand;", "tokens": ["Des", "Schmie\u00b7des", "kuns\u00b7ter\u00b7fahr\u00b7ne", "Hand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So \u00e4hnlich reift des Mannes Herz", "tokens": ["So", "\u00e4hn\u00b7lich", "reift", "des", "Man\u00b7nes", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Entgegen seinem Vaterland.", "tokens": ["Ent\u00b7ge\u00b7gen", "sei\u00b7nem", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So schwebt das Lied wie Glockensang", "tokens": ["So", "schwebt", "das", "Lied", "wie", "Glo\u00b7cken\u00b7sang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KOKOM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch heitre Sommerluft einher", "tokens": ["Durch", "heit\u00b7re", "Som\u00b7mer\u00b7luft", "ein\u00b7her"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PTKVZ"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und k\u00fcndet laut, da\u00df winterlang", "tokens": ["Und", "k\u00fcn\u00b7det", "laut", ",", "da\u00df", "win\u00b7ter\u00b7lang"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "$,", "KOUS", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem S\u00e4nger keine M\u00fch zu schwer.", "tokens": ["Dem", "S\u00e4n\u00b7ger", "kei\u00b7ne", "M\u00fch", "zu", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum schafft, bis aus den Bechern blinkt", "tokens": ["Drum", "schafft", ",", "bis", "aus", "den", "Be\u00b7chern", "blinkt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "$,", "KOUS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der M\u00e4nner Ehre, sch\u00f6n verkl\u00e4rt,", "tokens": ["Der", "M\u00e4n\u00b7ner", "Eh\u00b7re", ",", "sch\u00f6n", "ver\u00b7kl\u00e4rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und keiner mehr aus Silber trinkt,", "tokens": ["Und", "kei\u00b7ner", "mehr", "aus", "Sil\u00b7ber", "trinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der nicht des Weins und Silbers wert!", "tokens": ["Der", "nicht", "des", "Weins", "und", "Sil\u00b7bers", "wert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ART", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie Gl\u00fcck und Glas so leicht zerbricht,", "tokens": ["Wie", "Gl\u00fcck", "und", "Glas", "so", "leicht", "zer\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur etwas sp\u00e4ter, bricht das Erz,", "tokens": ["Nur", "et\u00b7was", "sp\u00e4\u00b7ter", ",", "bricht", "das", "Erz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Schale schmilzt \u2013 die Seele nicht,", "tokens": ["Die", "Scha\u00b7le", "schmilzt", "\u2013", "die", "See\u00b7le", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie gl\u00fcht, bewegend Herz um Herz.", "tokens": ["Sie", "gl\u00fcht", ",", "be\u00b7we\u00b7gend", "Herz", "um", "Herz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die h\u00f6chsten Tempel st\u00fcrzen ein,", "tokens": ["Die", "h\u00f6chs\u00b7ten", "Tem\u00b7pel", "st\u00fcr\u00b7zen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Werkmanns reiche Hand verdorrt,", "tokens": ["Des", "Werk\u00b7manns", "rei\u00b7che", "Hand", "ver\u00b7dorrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Verwildert stirbt am Berg der Wein \u2013", "tokens": ["Ver\u00b7wil\u00b7dert", "stirbt", "am", "Berg", "der", "Wein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "APPRART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Doch alles lebt im Liede fort.", "tokens": ["Doch", "al\u00b7les", "lebt", "im", "Lie\u00b7de", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wo sein ferner, goldner Ton", "tokens": ["Und", "wo", "sein", "fer\u00b7ner", ",", "gold\u00b7ner", "Ton"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWAV", "PPOSAT", "ADV", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Tr\u00fcmmern neue V\u00f6lker hebt,", "tokens": ["Aus", "Tr\u00fcm\u00b7mern", "neu\u00b7e", "V\u00f6l\u00b7ker", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bl\u00fcht auch die neue Rebe schon,", "tokens": ["Bl\u00fcht", "auch", "die", "neu\u00b7e", "Re\u00b7be", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihre Ranke spinnt und webt;", "tokens": ["Und", "ih\u00b7re", "Ran\u00b7ke", "spinnt", "und", "webt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In W\u00e4ldern trinkt am Felsenquell", "tokens": ["In", "W\u00e4l\u00b7dern", "trinkt", "am", "Fel\u00b7sen\u00b7quell"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Hirtenkind aus hohler Hand,", "tokens": ["Das", "Hir\u00b7ten\u00b7kind", "aus", "hoh\u00b7ler", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bis wieder bringt aus Bechern hell", "tokens": ["Bis", "wie\u00b7der", "bringt", "aus", "Be\u00b7chern", "hell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Mann sein Hoch dem Vaterland!", "tokens": ["Der", "Mann", "sein", "Hoch", "dem", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}