{"textgrid.poem.67365": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die beiden Mexicaner", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei junge edle Mexicaner sahen", "tokens": ["Zwei", "jun\u00b7ge", "ed\u00b7le", "Me\u00b7xi\u00b7ca\u00b7ner", "sa\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "ADJA", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den R\u00e4uber ihres Vaterlandes, Cortez,", "tokens": ["Den", "R\u00e4u\u00b7ber", "ih\u00b7res", "Va\u00b7ter\u00b7lan\u00b7des", ",", "Cor\u00b7tez", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "NE", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Auf einer unerme\u00dflich hohen Zinn'", "tokens": ["Auf", "ei\u00b7ner", "un\u00b7er\u00b7me\u00df\u00b7lich", "ho\u00b7hen", "Zinn'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Am tiefen Abgrund stehn.", "tokens": ["Am", "tie\u00b7fen", "Ab\u00b7grund", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbwie, wenn wir mit ihm nieder", "tokens": ["\u00bb", "wie", ",", "wenn", "wir", "mit", "ihm", "nie\u00b7der"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "$,", "KOUS", "PPER", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Uns st\u00fcrzten!\u00ab sprach der Eine.", "tokens": ["Uns", "st\u00fcrz\u00b7ten", "!", "\u00ab", "sprach", "der", "Ei\u00b7ne", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u00bbso ist das Vaterland befreiet!\u00ab Schnell", "tokens": ["\u00bb", "so", "ist", "das", "Va\u00b7ter\u00b7land", "be\u00b7frei\u00b7et", "!", "\u00ab", "Schnell"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["$(", "ADV", "VAFIN", "ART", "NN", "VVFIN", "$.", "$(", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ergriffen sie ihn an den F\u00fc\u00dfen \u2013 doch", "tokens": ["Er\u00b7grif\u00b7fen", "sie", "ihn", "an", "den", "F\u00fc\u00b7\u00dfen", "\u2013", "doch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "$(", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Umsonst! Der R\u00e4uber war gerettet;", "tokens": ["Um\u00b7sonst", "!", "Der", "R\u00e4u\u00b7ber", "war", "ge\u00b7ret\u00b7tet", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sie st\u00fcrzen Beide in die Kluft, zerschmettert", "tokens": ["Sie", "st\u00fcr\u00b7zen", "Bei\u00b7de", "in", "die", "Kluft", ",", "zer\u00b7schmet\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ART", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "F\u00fcrs Vaterland. Ein ehrenvoller Tod!", "tokens": ["F\u00fcrs", "Va\u00b7ter\u00b7land", ".", "Ein", "eh\u00b7ren\u00b7vol\u00b7ler", "Tod", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}