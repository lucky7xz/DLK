{"dta.poem.9195": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "2.  Das erste Blickgen.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Doch halt mein geist/ besinne dich/", "tokens": ["Doch", "halt", "mein", "geist", "/", "be\u00b7sin\u00b7ne", "dich", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$(", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sieh auff/ das m\u00e4dgen zeiget sich/", "tokens": ["Sieh", "auff", "/", "das", "m\u00e4d\u00b7gen", "zei\u00b7get", "sich", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "$(", "ART", "ADJA", "VVFIN", "PRF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du must es doch betrachten.", "tokens": ["Du", "must", "es", "doch", "be\u00b7trach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich hab es vormahls nicht gewust/", "tokens": ["Ich", "hab", "es", "vor\u00b7mahls", "nicht", "ge\u00b7wust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Man darff dergleichen augen-lust", "tokens": ["Man", "darff", "derg\u00b7lei\u00b7chen", "au\u00b7gen\u00b7lust"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PIS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In warheit nicht verachten.", "tokens": ["In", "war\u00b7heit", "nicht", "ver\u00b7ach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}