{"dta.poem.18796": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "O! dass der Sinnen doch so viele sind!", "tokens": ["O", "!", "dass", "der", "Sin\u00b7nen", "doch", "so", "vie\u00b7le", "sind", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "KOUS", "ART", "NN", "ADV", "ADV", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verwirrung bringen sie in\u2019s Gl\u00fcck herein.", "tokens": ["Ver\u00b7wir\u00b7rung", "brin\u00b7gen", "sie", "in's", "Gl\u00fcck", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn ich dich sehe w\u00fcnsch\u2019 ich taub zu seyn,", "tokens": ["Wenn", "ich", "dich", "se\u00b7he", "w\u00fcn\u00b7sch'", "ich", "taub", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "VVFIN", "PPER", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Wenn ich dich h\u00f6re blind.", "tokens": ["Wenn", "ich", "dich", "h\u00f6\u00b7re", "blind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Auch in der Ferne dir so nah!", "tokens": ["Auch", "in", "der", "Fer\u00b7ne", "dir", "so", "nah", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und unerwartet kommt die Qual.", "tokens": ["Und", "un\u00b7er\u00b7war\u00b7tet", "kommt", "die", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da h\u00f6r\u2019 ich wieder dich einmal,", "tokens": ["Da", "h\u00f6r'", "ich", "wie\u00b7der", "dich", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf einmal bist du wieder da!", "tokens": ["Auf", "ein\u00b7mal", "bist", "du", "wie\u00b7der", "da", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}