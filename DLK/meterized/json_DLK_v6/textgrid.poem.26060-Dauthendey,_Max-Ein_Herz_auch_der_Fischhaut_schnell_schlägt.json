{"textgrid.poem.26060": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Ein Herz auch der Fischhaut schnell schl\u00e4gt", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der gl\u00e4nzende Mittag zum Flu\u00df sich legt,", "tokens": ["Der", "gl\u00e4n\u00b7zen\u00b7de", "Mit\u00b7tag", "zum", "Flu\u00df", "sich", "legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Jede Well' tr\u00e4gt ein Kr\u00f6nlein silberhell und erregt,", "tokens": ["Je\u00b7de", "Well'", "tr\u00e4gt", "ein", "Kr\u00f6n\u00b7lein", "sil\u00b7ber\u00b7hell", "und", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "ADJD", "KON", "VVPP", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und das Wasser zieht aus wie ein Festzug bewegt.", "tokens": ["Und", "das", "Was\u00b7ser", "zieht", "aus", "wie", "ein", "Fest\u00b7zug", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "\u00dcbern Flu\u00df tanzt die Sonne auf hitzigem Fu\u00df,", "tokens": ["\u00dc\u00b7bern", "Flu\u00df", "tanzt", "die", "Son\u00b7ne", "auf", "hit\u00b7zi\u00b7gem", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Als ob heut jeder Fisch eine Braut haben mu\u00df,", "tokens": ["Als", "ob", "heut", "je\u00b7der", "Fisch", "ei\u00b7ne", "Braut", "ha\u00b7ben", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ADV", "PIAT", "NN", "ART", "NN", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und ein Herz auch der Fischhaut schnell schl\u00e4gt.", "tokens": ["Und", "ein", "Herz", "auch", "der", "Fischhaut", "schnell", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Hecht und Aal sie verbreiten die Liebeskunde,", "tokens": ["Hecht", "und", "Aal", "sie", "ver\u00b7brei\u00b7ten", "die", "Lie\u00b7bes\u00b7kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Und der Flu\u00df erscheint jetzt zur Mittagsstunde", "tokens": ["Und", "der", "Flu\u00df", "er\u00b7scheint", "jetzt", "zur", "Mit\u00b7tags\u00b7stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "APPRART", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Wie ein Hochzeitssaal und beleuchtet die Runde.", "tokens": ["Wie", "ein", "Hoch\u00b7zeits\u00b7saal", "und", "be\u00b7leuch\u00b7tet", "die", "Run\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}