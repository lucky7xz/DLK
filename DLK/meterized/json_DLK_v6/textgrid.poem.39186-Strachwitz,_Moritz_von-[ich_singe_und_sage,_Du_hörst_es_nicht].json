{"textgrid.poem.39186": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "[ich singe und sage, Du h\u00f6rst es nicht]", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich singe und sage, Du h\u00f6rst es nicht,", "tokens": ["Ich", "sin\u00b7ge", "und", "sa\u00b7ge", ",", "Du", "h\u00f6rst", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich weine und klage, Du h\u00f6rst es nicht,", "tokens": ["Ich", "wei\u00b7ne", "und", "kla\u00b7ge", ",", "Du", "h\u00f6rst", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ich singe im heiligen Grau'n der Nacht,", "tokens": ["Ich", "sin\u00b7ge", "im", "hei\u00b7li\u00b7gen", "Grau'n", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ich singe am Tage, Du h\u00f6rst es nicht,", "tokens": ["Ich", "sin\u00b7ge", "am", "Ta\u00b7ge", ",", "Du", "h\u00f6rst", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Ich singe wohl m\u00e4chtig, wie Donnerhall", "tokens": ["Ich", "sin\u00b7ge", "wohl", "m\u00e4ch\u00b7tig", ",", "wie", "Don\u00b7ner\u00b7hall"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PWAV", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Zum Wetterschlage, Du h\u00f6rst es nicht;", "tokens": ["Zum", "Wet\u00b7ter\u00b7schla\u00b7ge", ",", "Du", "h\u00f6rst", "es", "nicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ich singe wohl leise, wie Westesku\u00df", "tokens": ["Ich", "sin\u00b7ge", "wohl", "lei\u00b7se", ",", "wie", "Wes\u00b7tes\u00b7ku\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PWAV", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Im Rosenhage, Du h\u00f6rst es nicht;", "tokens": ["Im", "Ro\u00b7sen\u00b7ha\u00b7ge", ",", "Du", "h\u00f6rst", "es", "nicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Und wenn ich zum Liede auch Blitz und Sturm", "tokens": ["Und", "wenn", "ich", "zum", "Lie\u00b7de", "auch", "Blitz", "und", "Sturm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPRART", "NN", "ADV", "NN", "KON", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Zusammenschlage, Du h\u00f6rst es nicht;", "tokens": ["Zu\u00b7sam\u00b7men\u00b7schla\u00b7ge", ",", "Du", "h\u00f6rst", "es", "nicht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Und was ich auch immer in Leid und Lust", "tokens": ["Und", "was", "ich", "auch", "im\u00b7mer", "in", "Leid", "und", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "ADV", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und Liebe trage, Du h\u00f6rst es nicht.", "tokens": ["Und", "Lie\u00b7be", "tra\u00b7ge", ",", "Du", "h\u00f6rst", "es", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}