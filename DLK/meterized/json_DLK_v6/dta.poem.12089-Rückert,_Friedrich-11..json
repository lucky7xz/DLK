{"dta.poem.12089": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ragt ein Inselberg, der bis zu seiner Spitze", "tokens": ["Es", "ragt", "ein", "In\u00b7sel\u00b7berg", ",", "der", "bis", "zu", "sei\u00b7ner", "Spit\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Von seinen Wurzeln auf, tr\u00e4gt vielverstreute Sitze,", "tokens": ["Von", "sei\u00b7nen", "Wur\u00b7zeln", "auf", ",", "tr\u00e4gt", "viel\u00b7ver\u00b7streu\u00b7te", "Sit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Landbauerwohnungen, jede von ihren Schatten", "tokens": ["Land\u00b7bau\u00b7er\u00b7woh\u00b7nun\u00b7gen", ",", "je\u00b7de", "von", "ih\u00b7ren", "Schat\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PIAT", "APPR", "PPOSAT", "NN"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Umgr\u00fcnt, umringt von rind- und rehbegrasten Matten.", "tokens": ["Um\u00b7gr\u00fcnt", ",", "um\u00b7ringt", "von", "rin\u00b7d", "und", "reh\u00b7be\u00b7gras\u00b7ten", "Mat\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "APPR", "TRUNC", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Den Gipfel aber kr\u00f6nt ein Thurm und Gotteshaus,", "tokens": ["Den", "Gip\u00b7fel", "a\u00b7ber", "kr\u00f6nt", "ein", "Thurm", "und", "Got\u00b7tes\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Rings sichtbar um den Berg von jeder Wohnung aus.", "tokens": ["Rings", "sicht\u00b7bar", "um", "den", "Berg", "von", "je\u00b7der", "Woh\u00b7nung", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Dort oben wohnt erh\u00f6ht, und Niemand f\u00fchlt sich hehrer,", "tokens": ["Dort", "o\u00b7ben", "wohnt", "er\u00b7h\u00f6ht", ",", "und", "Nie\u00b7mand", "f\u00fchlt", "sich", "heh\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "VVPP", "$,", "KON", "PIS", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Mitten seiner Welt, ein Priester und ein Lehrer.", "tokens": ["In", "Mit\u00b7ten", "sei\u00b7ner", "Welt", ",", "ein", "Pries\u00b7ter", "und", "ein", "Leh\u00b7rer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Von ihm aus ruft die Glock' an jedem Abhang nieder,", "tokens": ["Von", "ihm", "aus", "ruft", "die", "Glock'", "an", "je\u00b7dem", "Ab\u00b7hang", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am Morgen zum Gesch\u00e4ft, zur Ruh am Abend wieder.", "tokens": ["Am", "Mor\u00b7gen", "zum", "Ge\u00b7sch\u00e4ft", ",", "zur", "Ruh", "am", "A\u00b7bend", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$,", "APPRART", "NN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Fr\u00fch h\u00f6ren sie die Glock', und beten beim Erwachen,", "tokens": ["Fr\u00fch", "h\u00f6\u00b7ren", "sie", "die", "Glock'", ",", "und", "be\u00b7ten", "beim", "Er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie sie sie h\u00f6ren sp\u00e4t und Feierabend machen.", "tokens": ["Wie", "sie", "sie", "h\u00f6\u00b7ren", "sp\u00e4t", "und", "Fei\u00b7er\u00b7a\u00b7bend", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "ADJD", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Am Festtag droben schallt der Lebenden Gebet,", "tokens": ["Am", "Fest\u00b7tag", "dro\u00b7ben", "schallt", "der", "Le\u00b7ben\u00b7den", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und ihre Todten ruhn dort himmelsluftumweht.", "tokens": ["Und", "ih\u00b7re", "Tod\u00b7ten", "ruhn", "dort", "him\u00b7mel\u00b7sluf\u00b7tum\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Dorthin zur Schule gehn die lernbegier'gen Kinder;", "tokens": ["Dor\u00b7thin", "zur", "Schu\u00b7le", "gehn", "die", "lern\u00b7be\u00b7gier'\u00b7gen", "Kin\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geschwinde gehts hinauf, herunter noch geschwinder.", "tokens": ["Ge\u00b7schwin\u00b7de", "gehts", "hin\u00b7auf", ",", "her\u00b7un\u00b7ter", "noch", "ge\u00b7schwin\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$,", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Doch vor der Lehr' und nach steht er auf seinem Thurm,", "tokens": ["Doch", "vor", "der", "Lehr'", "und", "nach", "steht", "er", "auf", "sei\u00b7nem", "Thurm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "KON", "APPR", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mi\u00dft Wind- und Wolkenlauf, w\u00e4gt Sonnenschein und Sturm.", "tokens": ["Mi\u00dft", "Win\u00b7d", "und", "Wol\u00b7ken\u00b7lauf", ",", "w\u00e4gt", "Son\u00b7nen\u00b7schein", "und", "Sturm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "TRUNC", "KON", "NN", "$,", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "So den Kalender stellt er seinem V\u00f6lklein immer,", "tokens": ["So", "den", "Ka\u00b7len\u00b7der", "stellt", "er", "sei\u00b7nem", "V\u00f6l\u00b7klein", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "$,"], "meter": "--+--+-+-+-++", "measure": "anapaest.di.plus"}, "line.2": {"text": "Es baut danach sein Feld, und Segen fehlt ihm nimmer.", "tokens": ["Es", "baut", "da\u00b7nach", "sein", "Feld", ",", "und", "Se\u00b7gen", "fehlt", "ihm", "nim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PPOSAT", "NN", "$,", "KON", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Er aber, der am Tag war seines Volkes Hirte,", "tokens": ["Er", "a\u00b7ber", ",", "der", "am", "Tag", "war", "sei\u00b7nes", "Vol\u00b7kes", "Hir\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "APPRART", "NN", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird, wann der Abend naht, den Wanderern zum Wirte.", "tokens": ["Wird", ",", "wann", "der", "A\u00b7bend", "naht", ",", "den", "Wan\u00b7de\u00b7rern", "zum", "Wir\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Vom hohen S\u00f6ller sp\u00e4ht er, ob ein Gast sich nahe,", "tokens": ["Vom", "ho\u00b7hen", "S\u00f6l\u00b7ler", "sp\u00e4ht", "er", ",", "ob", "ein", "Gast", "sich", "na\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der von ihm Speis' und Trank und n\u00e4cht'ge Rast empfahe.", "tokens": ["Der", "von", "ihm", "Speis'", "und", "Trank", "und", "n\u00e4cht'\u00b7ge", "Rast", "em\u00b7pfa\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "NN", "KON", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Und sieht er keinen nahn, so winkt er seinen Segen", "tokens": ["Und", "sieht", "er", "kei\u00b7nen", "nahn", ",", "so", "winkt", "er", "sei\u00b7nen", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "ADJA", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach allen H\u00fctten hin, und geht der Ruh zu pflegen.", "tokens": ["Nach", "al\u00b7len", "H\u00fct\u00b7ten", "hin", ",", "und", "geht", "der", "Ruh", "zu", "pfle\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$,", "KON", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}