{"textgrid.poem.56227": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Die alte Waschfrau", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du siehst gesch\u00e4ftig bei dem Linnen", "tokens": ["Du", "siehst", "ge\u00b7sch\u00e4f\u00b7tig", "bei", "dem", "Lin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Alte dort in wei\u00dfem Haar,", "tokens": ["Die", "Al\u00b7te", "dort", "in", "wei\u00b7\u00dfem", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die r\u00fcstigste der W\u00e4scherinnen", "tokens": ["Die", "r\u00fcs\u00b7tigs\u00b7te", "der", "W\u00e4\u00b7sche\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im sechsundsiebenzigsten Jahr.", "tokens": ["Im", "sech\u00b7sund\u00b7sie\u00b7ben\u00b7zigs\u00b7ten", "Jahr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So hat sie stets mit sauerm Schwei\u00df", "tokens": ["So", "hat", "sie", "stets", "mit", "sau\u00b7erm", "Schwei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Brot in Ehr und Zucht gegessen,", "tokens": ["Ihr", "Brot", "in", "Ehr", "und", "Zucht", "ge\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und ausgef\u00fcllt mit treuem Flei\u00df", "tokens": ["Und", "aus\u00b7ge\u00b7f\u00fcllt", "mit", "treu\u00b7em", "Flei\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den Kreis, den Gott ihr zugemessen.", "tokens": ["Den", "Kreis", ",", "den", "Gott", "ihr", "zu\u00b7ge\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie hat in ihren jungen Tagen", "tokens": ["Sie", "hat", "in", "ih\u00b7ren", "jun\u00b7gen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geliebt, gehofft und sich verm\u00e4hlt;", "tokens": ["Ge\u00b7liebt", ",", "ge\u00b7hofft", "und", "sich", "ver\u00b7m\u00e4hlt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "KON", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie hat des Weibes Los getragen,", "tokens": ["Sie", "hat", "des", "Wei\u00b7bes", "Los", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sorgen haben nicht gefehlt;", "tokens": ["Die", "Sor\u00b7gen", "ha\u00b7ben", "nicht", "ge\u00b7fehlt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie hat den kranken Mann gepflegt;", "tokens": ["Sie", "hat", "den", "kran\u00b7ken", "Mann", "ge\u00b7pflegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie hat drei Kinder ihm geboren;", "tokens": ["Sie", "hat", "drei", "Kin\u00b7der", "ihm", "ge\u00b7bo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie hat ihn in das Grab gelegt,", "tokens": ["Sie", "hat", "ihn", "in", "das", "Grab", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und Glaub und Hoffnung nicht verloren.", "tokens": ["Und", "Glaub", "und", "Hoff\u00b7nung", "nicht", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da galt's die Kinder zu ern\u00e4hren;", "tokens": ["Da", "galt's", "die", "Kin\u00b7der", "zu", "er\u00b7n\u00e4h\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie griff es an mit heiterm Mut,", "tokens": ["Sie", "griff", "es", "an", "mit", "hei\u00b7term", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie zog sie auf in Zucht und Ehren,", "tokens": ["Sie", "zog", "sie", "auf", "in", "Zucht", "und", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Flei\u00df, die Ordnung sind ihr Gut.", "tokens": ["Der", "Flei\u00df", ",", "die", "Ord\u00b7nung", "sind", "ihr", "Gut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu suchen ihren Unterhalt", "tokens": ["Zu", "su\u00b7chen", "ih\u00b7ren", "Un\u00b7ter\u00b7halt"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Entlie\u00df sie segnend ihre Lieben,", "tokens": ["Ent\u00b7lie\u00df", "sie", "seg\u00b7nend", "ih\u00b7re", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So stand sie nun allein und alt,", "tokens": ["So", "stand", "sie", "nun", "al\u00b7lein", "und", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ihr war ihr heitrer Mut geblieben.", "tokens": ["Ihr", "war", "ihr", "hei\u00b7trer", "Mut", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie hat gespart und hat gesonnen", "tokens": ["Sie", "hat", "ge\u00b7spart", "und", "hat", "ge\u00b7son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Flachs gekauft und nachts gewacht,", "tokens": ["Und", "Flachs", "ge\u00b7kauft", "und", "nachts", "ge\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Flachs zu feinem Garn gesponnen,", "tokens": ["Den", "Flachs", "zu", "fei\u00b7nem", "Garn", "ge\u00b7spon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Garn dem Weber hingebracht;", "tokens": ["Das", "Garn", "dem", "We\u00b7ber", "hin\u00b7ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der hat's gewebt zu Leinewand;", "tokens": ["Der", "hat's", "ge\u00b7webt", "zu", "Lei\u00b7ne\u00b7wand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Schere brauchte sie, die Nadel,", "tokens": ["Die", "Sche\u00b7re", "brauch\u00b7te", "sie", ",", "die", "Na\u00b7del", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und n\u00e4hte sich mit eigner Hand", "tokens": ["Und", "n\u00e4h\u00b7te", "sich", "mit", "eig\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ihr Sterbehemde sonder Tadel.", "tokens": ["Ihr", "Ster\u00b7be\u00b7hem\u00b7de", "son\u00b7der", "Ta\u00b7del", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ihr Hemd, ihr Sterbehemd, sie sch\u00e4tzt es,", "tokens": ["Ihr", "Hemd", ",", "ihr", "Ster\u00b7be\u00b7hemd", ",", "sie", "sch\u00e4tzt", "es", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verwahrt's im Schrein am Ehrenplatz;", "tokens": ["Ver\u00b7wahrt's", "im", "Schrein", "am", "Eh\u00b7ren\u00b7platz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist ihr Erstes und ihr Letztes,", "tokens": ["Es", "ist", "ihr", "Ers\u00b7tes", "und", "ihr", "Letz\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Kleinod, ihr ersparter Schatz.", "tokens": ["Ihr", "Klei\u00b7nod", ",", "ihr", "er\u00b7spar\u00b7ter", "Schatz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie legt es an, des Herren Wort", "tokens": ["Sie", "legt", "es", "an", ",", "des", "Her\u00b7ren", "Wort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Am Sonntag fr\u00fch sich einzupr\u00e4gen,", "tokens": ["Am", "Sonn\u00b7tag", "fr\u00fch", "sich", "ein\u00b7zu\u00b7pr\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dann legt sie's wohlgef\u00e4llig fort,", "tokens": ["Dann", "legt", "sie's", "wohl\u00b7ge\u00b7f\u00e4l\u00b7lig", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bis sie darin zur Ruh sie legen.", "tokens": ["Bis", "sie", "da\u00b7rin", "zur", "Ruh", "sie", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und ich, an meinem Abend, wollte,", "tokens": ["Und", "ich", ",", "an", "mei\u00b7nem", "A\u00b7bend", ",", "woll\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich h\u00e4tte, diesem Weibe gleich,", "tokens": ["Ich", "h\u00e4t\u00b7te", ",", "die\u00b7sem", "Wei\u00b7be", "gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PDAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erf\u00fcllt, was ich erf\u00fcllen sollte", "tokens": ["Er\u00b7f\u00fcllt", ",", "was", "ich", "er\u00b7f\u00fcl\u00b7len", "soll\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWS", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In meinen Grenzen und Bereich;", "tokens": ["In", "mei\u00b7nen", "Gren\u00b7zen", "und", "Be\u00b7reich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich wollt, ich h\u00e4tte so gewu\u00dft", "tokens": ["Ich", "wollt", ",", "ich", "h\u00e4t\u00b7te", "so", "ge\u00b7wu\u00dft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Am Kelch des Lebens mich zu laben,", "tokens": ["Am", "Kelch", "des", "Le\u00b7bens", "mich", "zu", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und k\u00f6nnt am Ende gleiche Lust", "tokens": ["Und", "k\u00f6nnt", "am", "En\u00b7de", "glei\u00b7che", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "An meinem Sterbehemde haben.", "tokens": ["An", "mei\u00b7nem", "Ster\u00b7be\u00b7hem\u00b7de", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}