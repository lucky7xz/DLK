{"textgrid.poem.48218": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Dolor Tyrannus", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und Dolor Tyrannus also sprach:", "tokens": ["Und", "Do\u00b7lor", "Ty\u00b7ran\u00b7nus", "al\u00b7so", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "ADV", "VVFIN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "\u00bbihr lieben \u00c4rzte, gemach, gemach,", "tokens": ["\u00bb", "ihr", "lie\u00b7ben", "\u00c4rz\u00b7te", ",", "ge\u00b7mach", ",", "ge\u00b7mach", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$,", "ADV", "$,", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Immer enger wollt ihr mich umziehn", "tokens": ["Im\u00b7mer", "en\u00b7ger", "wollt", "ihr", "mich", "um\u00b7ziehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VMFIN", "PPER", "PRF", "VVIZU"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Mit Opium, Morphium, Koka\u00efn,", "tokens": ["Mit", "O\u00b7pium", ",", "Mor\u00b7phi\u00b7um", ",", "Ko\u00b7ka\u00efn", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NE", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Immer reicher stellt sich euch zur Wahl", "tokens": ["Im\u00b7mer", "rei\u00b7cher", "stellt", "sich", "euch", "zur", "Wahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PRF", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "\u00c4ther, Chloroform, Chloral,", "tokens": ["\u00c4\u00b7ther", ",", "Chlo\u00b7ro\u00b7form", ",", "Chlo\u00b7ral", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und doch, ob Brom, ob Jod, ob Od,", "tokens": ["Und", "doch", ",", "ob", "Brom", ",", "ob", "Jod", ",", "ob", "Od", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "NN", "$,", "KOUS", "NE", "$,", "KOUS", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Schmerz ist ewig wie der Tod.\u00ab", "tokens": ["Der", "Schmerz", "ist", "e\u00b7wig", "wie", "der", "Tod", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}