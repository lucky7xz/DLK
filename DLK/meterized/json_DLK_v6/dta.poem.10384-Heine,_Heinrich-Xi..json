{"dta.poem.10384": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Xi.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Im Rhein, im heiligen Strome,", "tokens": ["Im", "Rhein", ",", "im", "hei\u00b7li\u00b7gen", "Stro\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da spiegelt sich in den Well'n,", "tokens": ["Da", "spie\u00b7gelt", "sich", "in", "den", "Well'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mit seinem gro\u00dfen Dome,", "tokens": ["Mit", "sei\u00b7nem", "gro\u00b7\u00dfen", "Do\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das gro\u00dfe, heilige C\u00f6ln.", "tokens": ["Das", "gro\u00b7\u00dfe", ",", "hei\u00b7li\u00b7ge", "C\u00f6ln", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Im Dom da steht ein Bildni\u00df,", "tokens": ["Im", "Dom", "da", "steht", "ein", "Bild\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf goldenem Leder gemalt;", "tokens": ["Auf", "gol\u00b7de\u00b7nem", "Le\u00b7der", "ge\u00b7malt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "In meines Lebens Wildni\u00df", "tokens": ["In", "mei\u00b7nes", "Le\u00b7bens", "Wild\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat's freundlich hineingestrahlt.", "tokens": ["Hat's", "freund\u00b7lich", "hin\u00b7ein\u00b7ge\u00b7strahlt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Es schweben Blumen und Englein", "tokens": ["Es", "schwe\u00b7ben", "Blu\u00b7men", "und", "En\u00b7glein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Um unsre liebe Frau;", "tokens": ["Um", "uns\u00b7re", "lie\u00b7be", "Frau", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Augen, die Lippen, die W\u00e4nglein,", "tokens": ["Die", "Au\u00b7gen", ",", "die", "Lip\u00b7pen", ",", "die", "W\u00e4ng\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+-+++", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die gleichen der Liebsten genau.", "tokens": ["Die", "glei\u00b7chen", "der", "Liebs\u00b7ten", "ge\u00b7nau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}