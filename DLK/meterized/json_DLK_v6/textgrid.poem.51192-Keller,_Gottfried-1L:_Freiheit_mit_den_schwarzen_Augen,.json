{"textgrid.poem.51192": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freiheit mit den schwarzen Augen,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freiheit mit den schwarzen Augen,", "tokens": ["Frei\u00b7heit", "mit", "den", "schwar\u00b7zen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wachst du auf am Tiberstrande?", "tokens": ["Wachst", "du", "auf", "am", "Ti\u00b7ber\u00b7stran\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Freiheit mit den blauen Augen,", "tokens": ["Frei\u00b7heit", "mit", "den", "blau\u00b7en", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schl\u00e4fst du noch im deutschen Lande?", "tokens": ["Schl\u00e4fst", "du", "noch", "im", "deut\u00b7schen", "Lan\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00fchne, trikolore Dirne,", "tokens": ["K\u00fch\u00b7ne", ",", "tri\u00b7ko\u00b7lo\u00b7re", "Dir\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sch\u00fcrze wieder dich zum Tanze!", "tokens": ["Sch\u00fcr\u00b7ze", "wie\u00b7der", "dich", "zum", "Tan\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wei\u00dfe Schweizer-Gletscherfirne,", "tokens": ["Wei\u00b7\u00dfe", "Schwei\u00b7zer\u00b7Glet\u00b7scher\u00b7fir\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "R\u00f6te dich im Morgenglanze!", "tokens": ["R\u00f6\u00b7te", "dich", "im", "Mor\u00b7gen\u00b7glan\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und du, schlanke Nereide,", "tokens": ["Und", "du", ",", "schlan\u00b7ke", "Ne\u00b7rei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Tauch aus deinen blauen Wogen!", "tokens": ["Tauch", "aus", "dei\u00b7nen", "blau\u00b7en", "Wo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat dich nicht dein falscher Friede,", "tokens": ["Hat", "dich", "nicht", "dein", "fal\u00b7scher", "Frie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Arme Hellas, arg betrogen?", "tokens": ["Ar\u00b7me", "Hel\u00b7las", ",", "arg", "be\u00b7tro\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Du dann mit dem Todesmute", "tokens": ["Du", "dann", "mit", "dem", "To\u00b7des\u00b7mu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und gebrochnem Schwunggefieder:", "tokens": ["Und", "ge\u00b7broch\u00b7nem", "Schwung\u00b7ge\u00b7fie\u00b7der", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wei\u00dfer Aar im roten Blute,", "tokens": ["Wei\u00b7\u00dfer", "Aar", "im", "ro\u00b7ten", "Blu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Rausche wieder, steige wieder!", "tokens": ["Rau\u00b7sche", "wie\u00b7der", ",", "stei\u00b7ge", "wie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hebt den Schild, ihr Schutzpatrone", "tokens": ["Hebt", "den", "Schild", ",", "ihr", "Schutz\u00b7pat\u00b7ro\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aller V\u00f6lker, auf zum Streite!", "tokens": ["Al\u00b7ler", "V\u00f6l\u00b7ker", ",", "auf", "zum", "Strei\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Flechtet eine Siegeskrone,", "tokens": ["Flech\u00b7tet", "ei\u00b7ne", "Sie\u00b7ges\u00b7kro\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die sich \u00fcber alle breite!", "tokens": ["Die", "sich", "\u00fc\u00b7ber", "al\u00b7le", "brei\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PIAT", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Streifet ab die alten S\u00fcnden,", "tokens": ["Strei\u00b7fet", "ab", "die", "al\u00b7ten", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn gel\u00e4utert und gereinigt", "tokens": ["Denn", "ge\u00b7l\u00e4u\u00b7tert", "und", "ge\u00b7rei\u00b7nigt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVPP", "KON", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sollt ihr euch zum Feste finden,", "tokens": ["Sollt", "ihr", "euch", "zum", "Fes\u00b7te", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Das nur W\u00fcrdige vereinigt!", "tokens": ["Das", "nur", "W\u00fcr\u00b7di\u00b7ge", "ver\u00b7ei\u00b7nigt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}