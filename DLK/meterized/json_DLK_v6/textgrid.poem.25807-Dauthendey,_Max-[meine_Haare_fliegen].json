], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Meiner Liebesgedanken.", "tokens": ["Mei\u00b7ner", "Lie\u00b7bes\u00b7ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}