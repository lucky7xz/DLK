{"textgrid.poem.35709": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "33. Freundschaftsbund", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Hut der Freiheit stimmet an", "tokens": ["Im", "Hut", "der", "Frei\u00b7heit", "stim\u00b7met", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Voll Ernst der Freundschaft Lied!", "tokens": ["Voll", "Ernst", "der", "Freund\u00b7schaft", "Lied", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der ist, bei Gott! kein Ehrenmann,", "tokens": ["Der", "ist", ",", "bei", "Gott", "!", "kein", "Eh\u00b7ren\u00b7mann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "APPR", "NN", "$.", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem hier sein Herz nicht gl\u00fcht!", "tokens": ["Dem", "hier", "sein", "Herz", "nicht", "gl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Freundschaft st\u00e4rkt in Freud' und Not,", "tokens": ["Die", "Freund\u00b7schaft", "st\u00e4rkt", "in", "Freud'", "und", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und folgt durch Leben und durch Tod!", "tokens": ["Und", "folgt", "durch", "Le\u00b7ben", "und", "durch", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Erbarmend sah des Lebens M\u00fch", "tokens": ["Er\u00b7bar\u00b7mend", "sah", "des", "Le\u00b7bens", "M\u00fch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Menschen Vater, schwieg,", "tokens": ["Der", "Men\u00b7schen", "Va\u00b7ter", ",", "schwieg", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erschuf die Freundschaft, wog; und sieh,", "tokens": ["Er\u00b7schuf", "die", "Freund\u00b7schaft", ",", "wog", ";", "und", "sieh", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "VVFIN", "$.", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Elends Schale stieg.", "tokens": ["Des", "E\u00b7lends", "Scha\u00b7le", "stieg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da sprach der Vater: Es ist gut!", "tokens": ["Da", "sprach", "der", "Va\u00b7ter", ":", "Es", "ist", "gut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und alles Leben hauchte Mut.", "tokens": ["Und", "al\u00b7les", "Le\u00b7ben", "hauch\u00b7te", "Mut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wohlthun und Wohl empfangen, lehrt", "tokens": ["Wohl\u00b7thun", "und", "Wohl", "emp\u00b7fan\u00b7gen", ",", "lehrt"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "KON", "ADV", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein allgemeiner Bund.", "tokens": ["Ein", "all\u00b7ge\u00b7mei\u00b7ner", "Bund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Kerker ist die Spinn' uns wert,", "tokens": ["Im", "Ker\u00b7ker", "ist", "die", "Spinn'", "uns", "wert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf \u00f6der Flur ein Hund,", "tokens": ["Auf", "\u00f6\u00b7der", "Flur", "ein", "Hund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein H\u00fchnchen, das gerufen kam,", "tokens": ["Ein", "H\u00fchn\u00b7chen", ",", "das", "ge\u00b7ru\u00b7fen", "kam", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Brot aus unsern H\u00e4nden nahm.", "tokens": ["Und", "Brot", "aus", "un\u00b7sern", "H\u00e4n\u00b7den", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch selig, teilt ein Menschenherz,", "tokens": ["Doch", "se\u00b7lig", ",", "teilt", "ein", "Men\u00b7schen\u00b7herz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verst\u00e4ndig, gut und treu,", "tokens": ["Ver\u00b7st\u00e4n\u00b7dig", ",", "gut", "und", "treu", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Voll Mitgef\u00fchls in Freud' und Schmerz,", "tokens": ["Voll", "Mit\u00b7ge\u00b7f\u00fchls", "in", "Freud'", "und", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Lebens Mancherlei:", "tokens": ["Des", "Le\u00b7bens", "Man\u00b7cher\u00b7lei", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Freund, der sanft mit Rate n\u00fctzt,", "tokens": ["Ein", "Freund", ",", "der", "sanft", "mit", "Ra\u00b7te", "n\u00fctzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und abends traulich bei uns sitzt!", "tokens": ["Und", "a\u00b7bends", "trau\u00b7lich", "bei", "uns", "sitzt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach ohne Freund ist \u00f6d' und stumm", "tokens": ["Ach", "oh\u00b7ne", "Freund", "ist", "\u00f6d'", "und", "stumm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VAFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das sch\u00f6nste Vaterland!", "tokens": ["Das", "sch\u00f6ns\u00b7te", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch bl\u00fchen hei\u00dft Elysium", "tokens": ["Doch", "bl\u00fc\u00b7hen", "hei\u00dft", "E\u00b7ly\u00b7si\u00b7um"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Freund aus d\u00fcrrem Sand!", "tokens": ["Ein", "Freund", "aus", "d\u00fcr\u00b7rem", "Sand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er schmaust mit uns auf grobem Zwilch,", "tokens": ["Er", "schmaust", "mit", "uns", "auf", "gro\u00b7bem", "Zwilch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und w\u00fcrzt durch Liebe Frucht und Milch.", "tokens": ["Und", "w\u00fcrzt", "durch", "Lie\u00b7be", "Frucht", "und", "Milch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Einm\u00fctig h\u00e4lt auf Recht und Pflicht,", "tokens": ["Ein\u00b7m\u00fc\u00b7tig", "h\u00e4lt", "auf", "Recht", "und", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und handelt, Freund und Freund;", "tokens": ["Und", "han\u00b7delt", ",", "Freund", "und", "Freund", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch tr\u00e4gt man gern, und qu\u00e4lt sich nicht,", "tokens": ["Doch", "tr\u00e4gt", "man", "gern", ",", "und", "qu\u00e4lt", "sich", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "$,", "KON", "VVFIN", "PRF", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was jeder glaubt und meint.", "tokens": ["Was", "je\u00b7der", "glaubt", "und", "meint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der zieht den Duft der Rose vor,", "tokens": ["Der", "zieht", "den", "Duft", "der", "Ro\u00b7se", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der andre liebt den Nelkenflor.", "tokens": ["Der", "and\u00b7re", "liebt", "den", "Nel\u00b7ken\u00b7flor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Gedank' und That, und Ehr' und Gl\u00fcck", "tokens": ["Ge\u00b7dank'", "und", "That", ",", "und", "Ehr'", "und", "Gl\u00fcck"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vertraut man ohne Hehl;", "tokens": ["Ver\u00b7traut", "man", "oh\u00b7ne", "Hehl", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auch Schwachheit schaut des Freundes Blick:", "tokens": ["Auch", "Schwach\u00b7heit", "schaut", "des", "Freun\u00b7des", "Blick", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn irrt kein leichter Fehl.", "tokens": ["Ihn", "irrt", "kein", "leich\u00b7ter", "Fehl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Selbst herber Gram an Freundesbrust", "tokens": ["Selbst", "her\u00b7ber", "Gram", "an", "Freun\u00b7des\u00b7brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verweint sich bald in s\u00fc\u00dfe Lust.", "tokens": ["Ver\u00b7weint", "sich", "bald", "in", "s\u00fc\u00b7\u00dfe", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ein Herz und eine Seele sei", "tokens": ["Ein", "Herz", "und", "ei\u00b7ne", "See\u00b7le", "sei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit seinem Freund der Freund:", "tokens": ["Mit", "sei\u00b7nem", "Freund", "der", "Freund", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Liebreich und wahrhaft, mild und frei,", "tokens": ["Lieb\u00b7reich", "und", "wahr\u00b7haft", ",", "mild", "und", "frei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADV", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "In Fern' und Tod vereint!", "tokens": ["In", "Fern'", "und", "Tod", "ver\u00b7eint", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Einst bringt, wer fr\u00fcher starb, in Glanz", "tokens": ["Einst", "bringt", ",", "wer", "fr\u00fc\u00b7her", "starb", ",", "in", "Glanz"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PWS", "ADJD", "VVFIN", "$,", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Brudergeist den Palmenkranz!", "tokens": ["Dem", "Bru\u00b7der\u00b7geist", "den", "Pal\u00b7men\u00b7kranz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Entbl\u00f6\u00dft das Haupt, ihr Freund', und weiht", "tokens": ["Ent\u00b7bl\u00f6\u00dft", "das", "Haupt", ",", "ihr", "Freund'", ",", "und", "weiht"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Freundschaft diesen Trank!", "tokens": ["Der", "Freund\u00b7schaft", "die\u00b7sen", "Trank", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr toten Freunde, h\u00f6rt den Eid,", "tokens": ["Ihr", "to\u00b7ten", "Freun\u00b7de", ",", "h\u00f6rt", "den", "Eid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Einstimmend zum Gesang;", "tokens": ["Ein\u00b7stim\u00b7mend", "zum", "Ge\u00b7sang", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und tr\u00f6stet armer F\u00fcrsten Los,", "tokens": ["Und", "tr\u00f6s\u00b7tet", "ar\u00b7mer", "F\u00fcrs\u00b7ten", "Los", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die nie des Freundes Arm umschlo\u00df!", "tokens": ["Die", "nie", "des", "Freun\u00b7des", "Arm", "um\u00b7schlo\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Wir sch\u00fctteln herzlich uns die Hand,", "tokens": ["Wir", "sch\u00fct\u00b7teln", "herz\u00b7lich", "uns", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und teilen Freud' und Not!", "tokens": ["Und", "tei\u00b7len", "Freud'", "und", "Not", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sei dieser Ku\u00df der Freundschaft Pfand", "tokens": ["Sei", "die\u00b7ser", "Ku\u00df", "der", "Freund\u00b7schaft", "Pfand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PDAT", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Leben und durch Tod!", "tokens": ["Durch", "Le\u00b7ben", "und", "durch", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie David seinen Jonathan,", "tokens": ["Wie", "Da\u00b7vid", "sei\u00b7nen", "Jo\u00b7na\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Vo\u00df dich, Stolberg, liebgewann!", "tokens": ["Und", "Vo\u00df", "dich", ",", "Stol\u00b7berg", ",", "lieb\u00b7ge\u00b7wann", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "NE", "PPER", "$,", "NE", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}