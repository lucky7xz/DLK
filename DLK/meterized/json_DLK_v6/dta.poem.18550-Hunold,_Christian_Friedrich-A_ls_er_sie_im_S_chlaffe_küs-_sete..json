{"dta.poem.18550": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "A ls er sie im  S chlaffe k\u00fcs-  \n sete.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Der Sch\u00f6nheit Meisterst\u00fcck/ verliebter Eigensinn", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "Meis\u00b7ter\u00b7st\u00fcck", "/", "ver\u00b7lieb\u00b7ter", "Ei\u00b7gen\u00b7sinn"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "$(", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sucht' ihren Uberdru\u00df im Schlaffe zu vers\u00fcssen/", "tokens": ["Sucht'", "ih\u00b7ren", "U\u00b7berd\u00b7ru\u00df", "im", "Schlaf\u00b7fe", "zu", "ver\u00b7s\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie striche Quaal und Schmertz des treuen Damons hin/", "tokens": ["Sie", "stri\u00b7che", "Qua\u00b7al", "und", "Schmertz", "des", "treu\u00b7en", "Da\u00b7mons", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Um lieber gar den Tod als seinen Mund zu k\u00fcssen.", "tokens": ["Um", "lie\u00b7ber", "gar", "den", "Tod", "als", "sei\u00b7nen", "Mund", "zu", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADV", "ART", "NN", "KOKOM", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Sinnen schlossen sich mit ihren Augen zu/", "tokens": ["Die", "Sin\u00b7nen", "schlos\u00b7sen", "sich", "mit", "ih\u00b7ren", "Au\u00b7gen", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "PTKZU", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und Venus hilfft sie selbst in ihre Ruhe wiegen!", "tokens": ["Und", "Ve\u00b7nus", "hilfft", "sie", "selbst", "in", "ih\u00b7re", "Ru\u00b7he", "wie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn/ sch\u00f6ne grausame/ sprach sie/ entschl\u00e4ffest du/", "tokens": ["Denn", "/", "sch\u00f6\u00b7ne", "grau\u00b7sa\u00b7me", "/", "sprach", "sie", "/", "ent\u00b7schl\u00e4f\u00b7fest", "du", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$(", "ADJA", "NN", "$(", "VVFIN", "PPER", "$(", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Soll Damon seinen Wunsch gar bald vergn\u00fcget kriegen.", "tokens": ["Soll", "Da\u00b7mon", "sei\u00b7nen", "Wunsch", "gar", "bald", "ver\u00b7gn\u00fc\u00b7get", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "PPOSAT", "NN", "ADV", "ADV", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie nun der strenge Geist auff Phantasien lieff/", "tokens": ["Wie", "nun", "der", "stren\u00b7ge", "Geist", "auff", "Phan\u00b7ta\u00b7si\u00b7en", "lieff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "APPR", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und auch in Traume nicht die Liebe wolte leiden/", "tokens": ["Und", "auch", "in", "Trau\u00b7me", "nicht", "die", "Lie\u00b7be", "wol\u00b7te", "lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "PTKNEG", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Erwachte Damons Lust/ da sein Vergn\u00fcgen schlieff/", "tokens": ["Er\u00b7wach\u00b7te", "Da\u00b7mons", "Lust", "/", "da", "sein", "Ver\u00b7gn\u00fc\u00b7gen", "schlieff", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NN", "$(", "KOUS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und konte sich genug auff ihren Lippen weiden.", "tokens": ["Und", "kon\u00b7te", "sich", "ge\u00b7nug", "auff", "ih\u00b7ren", "Lip\u00b7pen", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So wohl nahm Damons Durst die K\u00fchlungs-Zeit in acht/", "tokens": ["So", "wohl", "nahm", "Da\u00b7mons", "Durst", "die", "K\u00fch\u00b7lungs\u00b7Zeit", "in", "acht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "NE", "NN", "ART", "NN", "APPR", "CARD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So artig wuste sich die Venus auch zu r\u00e4chen;", "tokens": ["So", "ar\u00b7tig", "wus\u00b7te", "sich", "die", "Ve\u00b7nus", "auch", "zu", "r\u00e4\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PRF", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Denn wo der G\u00e4rtner nicht bey seinen Blumen wacht/", "tokens": ["Denn", "wo", "der", "G\u00e4rt\u00b7ner", "nicht", "bey", "sei\u00b7nen", "Blu\u00b7men", "wacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So kan ein schlauer Dieb die sch\u00f6nsten Rosen brechen.", "tokens": ["So", "kan", "ein", "schlau\u00b7er", "Dieb", "die", "sch\u00f6ns\u00b7ten", "Ro\u00b7sen", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Es war ihr spr\u00f6des Thun mit rechter M\u00fcntz bezahlt/", "tokens": ["Es", "war", "ihr", "spr\u00f6\u00b7des", "Thun", "mit", "rech\u00b7ter", "M\u00fcntz", "be\u00b7zahlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Verlachte sie zuvor sein sehnliches Verlangen/", "tokens": ["Ver\u00b7lach\u00b7te", "sie", "zu\u00b7vor", "sein", "sehn\u00b7li\u00b7ches", "Ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So hat sein Freuden-Stern auch nie so sch\u00f6n gestrahlt/", "tokens": ["So", "hat", "sein", "Freu\u00b7den\u00b7S\u00b7tern", "auch", "nie", "so", "sch\u00f6n", "ge\u00b7strahlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ADV", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Als da vor einen Ku\u00df er tausend hat empfangen.", "tokens": ["Als", "da", "vor", "ei\u00b7nen", "Ku\u00df", "er", "tau\u00b7send", "hat", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "PPER", "VVPP", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Drum so verweigert uns/ ihr lieben Kinder nicht/", "tokens": ["Drum", "so", "ver\u00b7wei\u00b7gert", "uns", "/", "ihr", "lie\u00b7ben", "Kin\u00b7der", "nicht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PPER", "$(", "PPOSAT", "ADJA", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Was die Natur euch pflegt an Sch\u00e4tzen mit zu theilen;", "tokens": ["Was", "die", "Na\u00b7tur", "euch", "pflegt", "an", "Sch\u00e4t\u00b7zen", "mit", "zu", "thei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PPER", "VVFIN", "APPR", "NN", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ihr seyd der Artzt den man offt \u00fcm die Cur bespricht/", "tokens": ["Ihr", "seyd", "der", "Artzt", "den", "man", "offt", "\u00fcm", "die", "Cur", "be\u00b7spricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "PIS", "ADV", "ADJD", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und lasset euch wohl selbst an dieser Krackheit heilen.", "tokens": ["Und", "las\u00b7set", "euch", "wohl", "selbst", "an", "die\u00b7ser", "Krack\u00b7heit", "hei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ja wo ihr noch so karg mit einen K\u00fc\u00dfgen seyd/", "tokens": ["Ja", "wo", "ihr", "noch", "so", "karg", "mit", "ei\u00b7nen", "K\u00fc\u00df\u00b7gen", "seyd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PWAV", "PPER", "ADV", "ADV", "ADJD", "APPR", "ART", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und macht uns manches mahl die sch\u00f6ne Kost zu nichte/", "tokens": ["Und", "macht", "uns", "man\u00b7ches", "mahl", "die", "sch\u00f6\u00b7ne", "Kost", "zu", "nich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "ADV", "ART", "ADJA", "NN", "APPR", "PIS", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So mehret ihr dadurch nur ihre S\u00fcssigkeit/", "tokens": ["So", "meh\u00b7ret", "ihr", "da\u00b7durch", "nur", "ih\u00b7re", "S\u00fcs\u00b7sig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Weil man nichts liebers isst/ als die gestohlne Fr\u00fcchte.", "tokens": ["Weil", "man", "nichts", "lie\u00b7bers", "isst", "/", "als", "die", "ge\u00b7stohl\u00b7ne", "Fr\u00fcch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PIS", "ADJA", "NN", "$(", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}