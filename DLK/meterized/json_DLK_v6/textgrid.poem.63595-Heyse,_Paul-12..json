{"textgrid.poem.63595": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ins Reich der Schatten f\u00fchrte mich der Traum.", "tokens": ["Ins", "Reich", "der", "Schat\u00b7ten", "f\u00fchr\u00b7te", "mich", "der", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da sah ich unser liebes Kind sich nahn,", "tokens": ["Da", "sah", "ich", "un\u00b7ser", "lie\u00b7bes", "Kind", "sich", "nahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "PRF", "ADJA", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So still und bla\u00df und ernst \u2013 ich kannt' es kaum.", "tokens": ["So", "still", "und", "bla\u00df", "und", "ernst", "\u2013", "ich", "kannt'", "es", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "KON", "ADJD", "$(", "PPER", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Arme streckt' ich aus, es zu umfahn,", "tokens": ["Die", "Ar\u00b7me", "streckt'", "ich", "aus", ",", "es", "zu", "um\u00b7fahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch sch\u00fcttelt' es die Locken schwer wie Blei,", "tokens": ["Doch", "sch\u00fct\u00b7telt'", "es", "die", "Lo\u00b7cken", "schwer", "wie", "Blei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Als h\u00e4tt' ihm noch das H\u00e4lschen wehgetan.", "tokens": ["Als", "h\u00e4tt'", "ihm", "noch", "das", "H\u00e4l\u00b7schen", "weh\u00b7ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dann deutet' es, als ob es durstig sei,", "tokens": ["Dann", "deu\u00b7tet'", "es", ",", "als", "ob", "es", "durs\u00b7tig", "sei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOKOM", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Auf seine Lippen, und mit m\u00fcdem Winken", "tokens": ["Auf", "sei\u00b7ne", "Lip\u00b7pen", ",", "und", "mit", "m\u00fc\u00b7dem", "Win\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der beiden \u00c4rmchen zog es mich herbei.", "tokens": ["Der", "bei\u00b7den", "\u00c4rm\u00b7chen", "zog", "es", "mich", "her\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Kind, rief ich zu ihm eilend, willst du trinken?", "tokens": ["Kind", ",", "rief", "ich", "zu", "ihm", "ei\u00b7lend", ",", "willst", "du", "trin\u00b7ken", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPR", "PPER", "VVPP", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Sieh hier den Quell, der meiner Brust entquillt,", "tokens": ["Sieh", "hier", "den", "Quell", ",", "der", "mei\u00b7ner", "Brust", "ent\u00b7quillt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Aus breiter Wunde str\u00f6mend an der Linken. \u2013", "tokens": ["Aus", "brei\u00b7ter", "Wun\u00b7de", "str\u00f6\u00b7mend", "an", "der", "Lin\u00b7ken", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Da trank's; und als es seinen Durst gestillt,", "tokens": ["Da", "trank's", ";", "und", "als", "es", "sei\u00b7nen", "Durst", "ge\u00b7stillt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "KON", "KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Das blasse M\u00fcndlein mit dem Blute f\u00e4rbend,", "tokens": ["Das", "blas\u00b7se", "M\u00fcnd\u00b7lein", "mit", "dem", "Blu\u00b7te", "f\u00e4r\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Gewann es Sprache, seufzt' und sagte mild:", "tokens": ["Ge\u00b7wann", "es", "Spra\u00b7che", ",", "seufzt'", "und", "sag\u00b7te", "mild", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$,", "VVFIN", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "O lieber Vater, dem zuerst ich sterbend", "tokens": ["O", "lie\u00b7ber", "Va\u00b7ter", ",", "dem", "zu\u00b7erst", "ich", "ster\u00b7bend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "NN", "$,", "PRELS", "ADV", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "So gro\u00dfes Leid gebracht und mehr noch bringe,", "tokens": ["So", "gro\u00b7\u00dfes", "Leid", "ge\u00b7bracht", "und", "mehr", "noch", "brin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVPP", "KON", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Die Lust am Licht der Sonne dir verderbend,", "tokens": ["Die", "Lust", "am", "Licht", "der", "Son\u00b7ne", "dir", "ver\u00b7der\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Sei doch getrost und wieder guter Dinge.", "tokens": ["Sei", "doch", "ge\u00b7trost", "und", "wie\u00b7der", "gu\u00b7ter", "Din\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "KON", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Ich schweb' hier unten freud- und kummerlos,", "tokens": ["Ich", "schweb'", "hier", "un\u00b7ten", "freu\u00b7d", "und", "kum\u00b7mer\u00b7los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "TRUNC", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Der Nacht gewohnt, wie n\u00e4cht'ge Schmetterlinge.", "tokens": ["Der", "Nacht", "ge\u00b7wohnt", ",", "wie", "n\u00e4cht'\u00b7ge", "Schmet\u00b7ter\u00b7lin\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PWAV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Das eine qu\u00e4lt uns leichte Schatten blo\u00df,", "tokens": ["Das", "ei\u00b7ne", "qu\u00e4lt", "uns", "leich\u00b7te", "Schat\u00b7ten", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "PPER", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Da\u00df ihr mit Tr\u00e4nen unser Grab betrauert,", "tokens": ["Da\u00df", "ihr", "mit", "Tr\u00e4\u00b7nen", "un\u00b7ser", "Grab", "be\u00b7trau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Die zu uns dringen durch der Erde Scho\u00df;", "tokens": ["Die", "zu", "uns", "drin\u00b7gen", "durch", "der", "Er\u00b7de", "Scho\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Da\u00df Regengu\u00df uns winterlich umschauert", "tokens": ["Da\u00df", "Re\u00b7gen\u00b7gu\u00df", "uns", "win\u00b7ter\u00b7lich", "um\u00b7schau\u00b7ert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Und unser Schattenleib von Fr\u00f6steln bebt", "tokens": ["Und", "un\u00b7ser", "Schat\u00b7ten\u00b7leib", "von", "Fr\u00f6s\u00b7teln", "bebt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Und fieberhaft am Lethestrome kauert.", "tokens": ["Und", "fie\u00b7ber\u00b7haft", "am", "Le\u00b7thes\u00b7tro\u00b7me", "kau\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Uns wird nur wohl, wenn ihr zu hemmen strebt", "tokens": ["Uns", "wird", "nur", "wohl", ",", "wenn", "ihr", "zu", "hem\u00b7men", "strebt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$,", "KOUS", "PPER", "PTKZU", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Den blut'gen Quell untr\u00f6stlich bittrer Tr\u00e4nen", "tokens": ["Den", "blut'\u00b7gen", "Quell", "un\u00b7tr\u00f6st\u00b7lich", "bit\u00b7trer", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Und euer Auge rein zum Himmel hebt.", "tokens": ["Und", "eu\u00b7er", "Au\u00b7ge", "rein", "zum", "Him\u00b7mel", "hebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Denn wenn ihr m\u00e4\u00df'ger, mit gefa\u00dftem Sehnen", "tokens": ["Denn", "wenn", "ihr", "m\u00e4\u00df'\u00b7ger", ",", "mit", "ge\u00b7fa\u00df\u00b7tem", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Der Toten denkt, so ist's, als f\u00fchlten wir", "tokens": ["Der", "To\u00b7ten", "denkt", ",", "so", "ist's", ",", "als", "f\u00fchl\u00b7ten", "wir"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "VAFIN", "$,", "KOUS", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "In warmem Schimmer unsern Leib sich dehnen.", "tokens": ["In", "war\u00b7mem", "Schim\u00b7mer", "un\u00b7sern", "Leib", "sich", "deh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Sieh nur, so manche Schatten wandeln hier", "tokens": ["Sieh", "nur", ",", "so", "man\u00b7che", "Schat\u00b7ten", "wan\u00b7deln", "hier"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "ADV", "PIAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Von sanftem Zwielicht wundersam umflossen,", "tokens": ["Von", "sanf\u00b7tem", "Zwie\u00b7licht", "wun\u00b7der\u00b7sam", "um\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Verkl\u00e4rten Blicks im traurigen Revier.", "tokens": ["Ver\u00b7kl\u00e4r\u00b7ten", "Blicks", "im", "trau\u00b7ri\u00b7gen", "Re\u00b7vier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Das sind die Seelen, deren Lichtgenossen", "tokens": ["Das", "sind", "die", "See\u00b7len", ",", "de\u00b7ren", "Licht\u00b7ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Mit steter Treue noch ihr Bild bewahren,", "tokens": ["Mit", "ste\u00b7ter", "Treu\u00b7e", "noch", "ihr", "Bild", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Doch ihre Tr\u00e4nen still ins Herz verschlossen.", "tokens": ["Doch", "ih\u00b7re", "Tr\u00e4\u00b7nen", "still", "ins", "Herz", "ver\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "So, Vater, wenn du nun hinaufgefahren,", "tokens": ["So", ",", "Va\u00b7ter", ",", "wenn", "du", "nun", "hin\u00b7auf\u00b7ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "KOUS", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Tu auch, und sage meinem M\u00fctterlein \u2013 \u2013", "tokens": ["Tu", "auch", ",", "und", "sa\u00b7ge", "mei\u00b7nem", "M\u00fct\u00b7ter\u00b7lein", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADV", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Da schwieg's, als d\u00fcrf' es mehr nicht offenbaren.", "tokens": ["Da", "schwieg's", ",", "als", "d\u00fcr\u00b7f'", "es", "mehr", "nicht", "of\u00b7fen\u00b7ba\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.43": {"text": "O, rief ich, Kind, du mahnst uns, froh zu sein?", "tokens": ["O", ",", "rief", "ich", ",", "Kind", ",", "du", "mahnst", "uns", ",", "froh", "zu", "sein", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "NN", "$,", "PPER", "VVFIN", "PPER", "$,", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Bist du denn froh, seitdem du uns verlassen? \u2013", "tokens": ["Bist", "du", "denn", "froh", ",", "seit\u00b7dem", "du", "uns", "ver\u00b7las\u00b7sen", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,", "KOUS", "PPER", "PRF", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Da winkt' es mit der Hand, als spr\u00e4ch' es: nein.", "tokens": ["Da", "winkt'", "es", "mit", "der", "Hand", ",", "als", "spr\u00e4ch'", "es", ":", "nein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "KOUS", "VVFIN", "PPER", "$.", "PTKANT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Und wie ich's wollt' in meine Arme fassen,", "tokens": ["Und", "wie", "ich's", "wollt'", "in", "mei\u00b7ne", "Ar\u00b7me", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "Schwand es gleich einem Rauch an mir vorbei;", "tokens": ["Schwand", "es", "gleich", "ei\u00b7nem", "Rauch", "an", "mir", "vor\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "Ich sah es nur noch l\u00e4cheln und erblassen.", "tokens": ["Ich", "sah", "es", "nur", "noch", "l\u00e4\u00b7cheln", "und", "er\u00b7blas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "VVFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.49": {"text": "Da weckte mich der erste Hahnenschrei.", "tokens": ["Da", "weck\u00b7te", "mich", "der", "ers\u00b7te", "Hah\u00b7nen\u00b7schrei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}