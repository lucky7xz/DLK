{"textgrid.poem.56609": {"metadata": {"author": {"name": "Groth, Klaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: De Dag geit to Rau,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.57", "nl:0.42"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "De Dag geit to Rau,", "tokens": ["De", "Dag", "geit", "to", "Rau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "NN", "$,"], "meter": "----+", "measure": "unknown.measure.single"}, "line.2": {"text": "Opt Gras liggt de Dau,", "tokens": ["Opt", "Gras", "liggt", "de", "Dau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "NE", "NE", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "De Wulken ann H\u0119ben ward roth.", "tokens": ["De", "Wul\u00b7ken", "ann", "H\u0119\u00b7ben", "ward", "roth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "VAFIN", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Dats Allens so still,", "tokens": ["Dats", "Al\u00b7lens", "so", "still", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Ik weet ni wa'k will,", "tokens": ["Ik", "weet", "ni", "wa'k", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Ik l\u00f6v, mi is truri to Moth.", "tokens": ["Ik", "l\u00f6v", ",", "mi", "is", "tru\u00b7ri", "to", "Moth", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "De Pock quarkt int Rohr,", "tokens": ["De", "Pock", "quarkt", "int", "Rohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "De Voss bru't int Moor,", "tokens": ["De", "Voss", "bru't", "int", "Moor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Un wit inne Feern schallt Gesank.", "tokens": ["Un", "wit", "in\u00b7ne", "Feern", "schallt", "Ge\u00b7sank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "NN", "VVFIN", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Min Hart stiggt to H\u00f6ch,", "tokens": ["Min", "Hart", "stiggt", "to", "H\u00f6ch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Ik weet ni, wa'k seeg,", "tokens": ["Ik", "weet", "ni", ",", "wa'k", "seeg", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "FM.la", "FM.la", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "De Thran lopt de Backen hentlank.", "tokens": ["De", "Thran", "lopt", "de", "Ba\u00b7cken", "hent\u00b7lank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Dar achter de Weid", "tokens": ["Dar", "ach\u00b7ter", "de", "Weid"], "token_info": ["word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Wit \u0153wer de Heid", "tokens": ["Wit", "\u0153wer", "de", "Heid"], "token_info": ["word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Dar schimmert ann Himmel en M\u0153l:", "tokens": ["Dar", "schim\u00b7mert", "ann", "Him\u00b7mel", "en", "M\u0153l", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Dat is mi, as weer", "tokens": ["Dat", "is", "mi", ",", "as", "weer"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "FM.la", "FM.la"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ik dar v\u0153r de D\u0153r,", "tokens": ["Ik", "dar", "v\u0153r", "de", "D\u0153r", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Un seet oppen M\u0153lnbarg un sp\u0119l.", "tokens": ["Un", "seet", "op\u00b7pen", "M\u0153ln\u00b7barg", "un", "sp\u0119l", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Denn seeg dar Een rut,", "tokens": ["Denn", "seeg", "dar", "E\u00b7en", "rut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKVZ", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den kenn ik so gut,", "tokens": ["Den", "kenn", "ik", "so", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "ADV", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Den seet ik so oft oppen Schot;", "tokens": ["Den", "seet", "ik", "so", "oft", "op\u00b7pen", "Schot", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "De Steen leep un klung,", "tokens": ["De", "Steen", "leep", "un", "klung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "De Mann seet un sung,", "tokens": ["De", "Mann", "seet", "un", "sung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ann H\u0119ben de Wulken weern roth.", "tokens": ["Ann", "H\u0119\u00b7ben", "de", "Wul\u00b7ken", "weern", "roth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "VVFIN", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Do weer ik noch kleen,", "tokens": ["Do", "weer", "ik", "noch", "kleen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NE", "ADV", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Nu b\u00fcn ik alleen,", "tokens": ["Nu", "b\u00fcn", "ik", "al\u00b7le\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "FM.nl", "FM.nl", "FM.nl", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wull weet, ob de Ol dar noch steit?", "tokens": ["Wull", "weet", ",", "ob", "de", "Ol", "dar", "noch", "steit", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "KOUS", "NE", "NE", "PTKVZ", "KON", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "De Luch is so luri, \u2013", "tokens": ["De", "Luch", "is", "so", "lu\u00b7ri", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "FM", "FM", "FM", "$,", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Dat Leed is so truri:", "tokens": ["Dat", "Leed", "is", "so", "tru\u00b7ri", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Gottlof, dat de M\u0153l doch noch geit!", "tokens": ["Gott\u00b7lof", ",", "dat", "de", "M\u0153l", "doch", "noch", "geit", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "NE", "NE", "ADV", "ADV", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}