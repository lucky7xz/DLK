{"dta.poem.20637": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  Herrn Professor E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bey den Unsterblichen zu seyn                 ", "tokens": ["Bey", "den", "U\u00b7nsterb\u00b7li\u00b7chen", "zu", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKZU", "VAINF"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "W\u00fcnsch ich, o Freund! da wollt\u2019 ich nieder", "tokens": ["W\u00fcnsch", "ich", ",", "o", "Freund", "!", "da", "wollt'", "ich", "nie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "FM", "NN", "$.", "ADV", "VMFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von dem Olympus sehn; Du w\u00fcrdest Blumen streun", "tokens": ["Von", "dem", "O\u00b7lym\u00b7pus", "sehn", ";", "Du", "w\u00fcr\u00b7dest", "Blu\u00b7men", "streun"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "VVINF", "$.", "PPER", "VAFIN", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem Ueberrest der Geistverla\u00dfnen Glieder.", "tokens": ["Dem", "Ue\u00b7ber\u00b7rest", "der", "Geist\u00b7ver\u00b7la\u00df\u00b7nen", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich s\u00e4he zu, wie auf mein Grab", "tokens": ["Ich", "s\u00e4\u00b7he", "zu", ",", "wie", "auf", "mein", "Grab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PWAV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Finger junge Myrthen pflanzte:", "tokens": ["Dein", "Fin\u00b7ger", "jun\u00b7ge", "Myr\u00b7then", "pflanz\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Schatten des Achills sah nicht so stolz herab,", "tokens": ["Der", "Schat\u00b7ten", "des", "A\u00b7chills", "sah", "nicht", "so", "stolz", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PTKNEG", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als Philipps Sohn bey seiner Asche tanzte", "tokens": ["Als", "Phi\u00b7lipps", "Sohn", "bey", "sei\u00b7ner", "A\u00b7sche", "tanz\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und gl\u00fccklich pries der Thetis Sohn:", "tokens": ["Und", "gl\u00fcck\u00b7lich", "pries", "der", "The\u00b7tis", "Sohn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ihm in seinem Heldenleben", "tokens": ["Da\u00df", "ihm", "in", "sei\u00b7nem", "Hel\u00b7den\u00b7le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein wahrer Freund, und da sein Kriegesgeist entflohn,", "tokens": ["Ein", "wah\u00b7rer", "Freund", ",", "und", "da", "sein", "Krie\u00b7ges\u00b7geist", "ent\u00b7flohn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Herold seinen Thaten ward gegeben.", "tokens": ["Ein", "He\u00b7rold", "sei\u00b7nen", "Tha\u00b7ten", "ward", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}