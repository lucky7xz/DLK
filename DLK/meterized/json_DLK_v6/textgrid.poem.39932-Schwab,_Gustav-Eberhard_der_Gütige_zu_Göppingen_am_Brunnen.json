{"textgrid.poem.39932": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Eberhard der G\u00fctige zu G\u00f6ppingen am Brunnen", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbach Graf, Ihr seid so bleich und krank,", "tokens": ["\u00bb", "ach", "Graf", ",", "Ihr", "seid", "so", "bleich", "und", "krank", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "XY", "NE", "$,", "PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Euch kann der k\u00fchle Felsentrank", "tokens": ["Euch", "kann", "der", "k\u00fch\u00b7le", "Fel\u00b7sen\u00b7trank"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus unserm Quell nicht heilen!", "tokens": ["Aus", "un\u00b7serm", "Quell", "nicht", "hei\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nehmt Abschied Euch von Berg und Flur,", "tokens": ["Nehmt", "Ab\u00b7schied", "Euch", "von", "Berg", "und", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und denkt an Eure Seele nur,", "tokens": ["Und", "denkt", "an", "Eu\u00b7re", "See\u00b7le", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie will von hinnen eilen!\u00ab", "tokens": ["Sie", "will", "von", "hin\u00b7nen", "ei\u00b7len", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Arzt mit traurigem Gesicht", "tokens": ["Der", "Arzt", "mit", "trau\u00b7ri\u00b7gem", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum g\u00fct'gen Eberhard es spricht;", "tokens": ["Zum", "g\u00fct'\u00b7gen", "E\u00b7ber\u00b7hard", "es", "spricht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NE", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit L\u00e4cheln der es h\u00f6ret:", "tokens": ["Mit", "L\u00e4\u00b7cheln", "der", "es", "h\u00f6\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u00bbdich lob' ich, da\u00df du ehrlich bist,", "tokens": ["\u00bb", "dich", "lob'", "ich", ",", "da\u00df", "du", "ehr\u00b7lich", "bist", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch hat mich noch zu dieser Frist", "tokens": ["Doch", "hat", "mich", "noch", "zu", "die\u00b7ser", "Frist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Warnung nicht verst\u00f6ret.\u00ab", "tokens": ["Die", "War\u00b7nung", "nicht", "ver\u00b7st\u00f6\u00b7ret", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbo schm\u00e4ht nicht, Herr, die treue Kunst!\u00ab", "tokens": ["\u00bb", "o", "schm\u00e4ht", "nicht", ",", "Herr", ",", "die", "treu\u00b7e", "Kunst", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "VVFIN", "PTKNEG", "$,", "NN", "$,", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbnicht schm\u00e4h' ich, doch des Himmels Gunst", "tokens": ["\u00bb", "nicht", "schm\u00e4h'", "ich", ",", "doch", "des", "Him\u00b7mels", "Gunst"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PTKNEG", "VVFIN", "PPER", "$,", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gab mir ein besser Zeichen.", "tokens": ["Gab", "mir", "ein", "bes\u00b7ser", "Zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wohl fertig bin ich l\u00e4ngst zu gehn,", "tokens": ["Wohl", "fer\u00b7tig", "bin", "ich", "l\u00e4ngst", "zu", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch eh' zwei Dinge sind geschehn,", "tokens": ["Doch", "eh'", "zwei", "Din\u00b7ge", "sind", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "CARD", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Darf ich noch nicht erbleichen!", "tokens": ["Darf", "ich", "noch", "nicht", "er\u00b7blei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du siehst mich an und gl\u00e4ubst mir nicht,", "tokens": ["Du", "siehst", "mich", "an", "und", "gl\u00e4ubst", "mir", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So h\u00f6re, was des Herrn Gesicht", "tokens": ["So", "h\u00f6\u00b7re", ",", "was", "des", "Herrn", "Ge\u00b7sicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Traume mir verhei\u00dfen:", "tokens": ["Im", "Trau\u00b7me", "mir", "ver\u00b7hei\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir soll, eh' l\u00e4\u00dft das bl\u00fch'nde Weib,", "tokens": ["Mir", "soll", ",", "eh'", "l\u00e4\u00dft", "das", "bl\u00fch'n\u00b7de", "Weib", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Nachbarin, den jungen Leib,", "tokens": ["Die", "Nach\u00b7ba\u00b7rin", ",", "den", "jun\u00b7gen", "Leib", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Lebensband nicht rei\u00dfen.\u00ab", "tokens": ["Das", "Le\u00b7bens\u00b7band", "nicht", "rei\u00b7\u00dfen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Arzt blickt aus dem Fenster bang,", "tokens": ["Der", "Arzt", "blickt", "aus", "dem", "Fens\u00b7ter", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es ruht die Stra\u00dfe breit und lang,", "tokens": ["Es", "ruht", "die", "Stra\u00b7\u00dfe", "breit", "und", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In \u00f6der Mittagsstunde;", "tokens": ["In", "\u00f6\u00b7der", "Mit\u00b7tags\u00b7stun\u00b7de", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur aus dem stillen Nachbarhaus", "tokens": ["Nur", "aus", "dem", "stil\u00b7len", "Nach\u00b7bar\u00b7haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Ein grauer Priester wankt heraus", "tokens": ["Ein", "grau\u00b7er", "Pries\u00b7ter", "wankt", "he\u00b7raus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und fleht mit leisem Munde.", "tokens": ["Und", "fleht", "mit", "lei\u00b7sem", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Graf ermannet sich und spricht:", "tokens": ["Der", "Graf", "er\u00b7man\u00b7net", "sich", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbverbergt mir, frommer Vater, nicht,", "tokens": ["\u00bb", "ver\u00b7bergt", "mir", ",", "from\u00b7mer", "Va\u00b7ter", ",", "nicht", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wem habt Ihr zugesprochen?\u00ab", "tokens": ["Wem", "habt", "Ihr", "zu\u00b7ge\u00b7spro\u00b7chen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da ruft ihm zu der ernste Greis:", "tokens": ["Da", "ruft", "ihm", "zu", "der", "erns\u00b7te", "Greis", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbes hat ein jung und bl\u00fchend Reis", "tokens": ["\u00bb", "es", "hat", "ein", "jung", "und", "bl\u00fc\u00b7hend", "Reis"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADJD", "KON", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der G\u00e4rtner abgebrochen!\u00ab", "tokens": ["Der", "G\u00e4rt\u00b7ner", "ab\u00b7ge\u00b7bro\u00b7chen", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und mit dem heil'gen Sakrament,", "tokens": ["Und", "mit", "dem", "heil'\u00b7gen", "Sak\u00b7ra\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und mit dem Docht, der zagend brennt,", "tokens": ["Und", "mit", "dem", "Docht", ",", "der", "za\u00b7gend", "brennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wankt so der Alte weiter:", "tokens": ["Wankt", "so", "der", "Al\u00b7te", "wei\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Doch ob der Arzt auch seufzt und schweigt,", "tokens": ["Doch", "ob", "der", "Arzt", "auch", "seufzt", "und", "schweigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sein betr\u00fcbtes Antlitz neigt,", "tokens": ["Und", "sein", "be\u00b7tr\u00fcb\u00b7tes", "Ant\u00b7litz", "neigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Grafen Blick ist heiter!", "tokens": ["Des", "Gra\u00b7fen", "Blick", "ist", "hei\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbja, zarte Blumen welken bald!", "tokens": ["\u00bb", "ja", ",", "zar\u00b7te", "Blu\u00b7men", "wel\u00b7ken", "bald", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die B\u00e4ume stehn und werden alt,", "tokens": ["Die", "B\u00e4u\u00b7me", "stehn", "und", "wer\u00b7den", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum bleib' ich ungestorben!", "tokens": ["Drum", "bleib'", "ich", "un\u00b7ge\u00b7stor\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein zweiter Traum mir treu verspricht,", "tokens": ["Mein", "zwei\u00b7ter", "Traum", "mir", "treu", "ver\u00b7spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df meiner H\u00fctte Bau nicht bricht,", "tokens": ["Da\u00df", "mei\u00b7ner", "H\u00fct\u00b7te", "Bau", "nicht", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Eh' da\u00df ein Baum verdorben!", "tokens": ["Eh'", "da\u00df", "ein", "Baum", "ver\u00b7dor\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Siehst du dort in des Hofes Raum", "tokens": ["Siehst", "du", "dort", "in", "des", "Ho\u00b7fes", "Raum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Den schlanken, m\u00e4cht'gen Eichenbaum?", "tokens": ["Den", "schlan\u00b7ken", ",", "m\u00e4cht'\u00b7gen", "Ei\u00b7chen\u00b7baum", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er gr\u00fcnt vom Fu\u00df zum Gipfel.", "tokens": ["Er", "gr\u00fcnt", "vom", "Fu\u00df", "zum", "Gip\u00b7fel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das ist der zweite sichre Spruch:", "tokens": ["Das", "ist", "der", "zwei\u00b7te", "sich\u00b7re", "Spruch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr legt mich nicht in's Leichentuch,", "tokens": ["Ihr", "legt", "mich", "nicht", "in's", "Lei\u00b7chen\u00b7tuch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Eh denn verdorrt sein Wipfel.\u00ab", "tokens": ["Eh", "denn", "ver\u00b7dorrt", "sein", "Wip\u00b7fel", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.10": {"line.1": {"te