{"textgrid.poem.34553": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gross ist das Leben und reich!", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gross ist das Leben und reich!", "tokens": ["Gross", "ist", "das", "Le\u00b7ben", "und", "reich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "KON", "ADJD", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Ewige G\u00f6tter schenkten es uns,", "tokens": ["E\u00b7wi\u00b7ge", "G\u00f6t\u00b7ter", "schenk\u00b7ten", "es", "uns", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "l\u00e4chelnder G\u00fcte voll,", "tokens": ["l\u00e4\u00b7cheln\u00b7der", "G\u00fc\u00b7te", "voll", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "uns den Sterblichen, Freudegeschaffenen.", "tokens": ["uns", "den", "Sterb\u00b7li\u00b7chen", ",", "Freu\u00b7de\u00b7ge\u00b7schaf\u00b7fe\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "NN", "$."], "meter": "--+--+--+--", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Aber arm ist des Menschen Herz!", "tokens": ["A\u00b7ber", "arm", "ist", "des", "Men\u00b7schen", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Schnell verzagt, vergisst es der reifenden Fr\u00fcchte.", "tokens": ["Schnell", "ver\u00b7zagt", ",", "ver\u00b7gisst", "es", "der", "rei\u00b7fen\u00b7den", "Fr\u00fcch\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Immer wieder mit leeren H\u00e4nden", "tokens": ["Im\u00b7mer", "wie\u00b7der", "mit", "lee\u00b7ren", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "sitzt der Bettler an staubiger Strasse,", "tokens": ["sitzt", "der", "Bett\u00b7ler", "an", "stau\u00b7bi\u00b7ger", "Stras\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "drauf das Gl\u00fcck mit den t\u00f6nenden R\u00e4dern", "tokens": ["drauf", "das", "Gl\u00fcck", "mit", "den", "t\u00f6\u00b7nen\u00b7den", "R\u00e4\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "leuchtend vorbeifuhr.", "tokens": ["leuch\u00b7tend", "vor\u00b7bei\u00b7fuhr", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}