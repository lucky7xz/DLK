{"dta.poem.1286": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Himmel-Schl\u00fcssel  \n oder  \n Geistliche Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.85", "nl:0.14"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Der erbla\u00dfte Monden ziehet", "tokens": ["Der", "er\u00b7bla\u00df\u00b7te", "Mon\u00b7den", "zie\u00b7het"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sein geschw\u00e4chtes Silber ein/", "tokens": ["Sein", "ge\u00b7schw\u00e4ch\u00b7tes", "Sil\u00b7ber", "ein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Sternen Heer entfliehet", "tokens": ["Und", "der", "Ster\u00b7nen", "Heer", "ent\u00b7flie\u00b7het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor der Sonne nahem Schein/", "tokens": ["Vor", "der", "Son\u00b7ne", "na\u00b7hem", "Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auff/ mein Hertz/ und la\u00df der S\u00fcnden", "tokens": ["Auff", "/", "mein", "Hertz", "/", "und", "la\u00df", "der", "S\u00fcn\u00b7den"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "$(", "PPOSAT", "NN", "$(", "KON", "VVIMP", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tr\u00fcbe Nacht bey dir verschwinden.", "tokens": ["Tr\u00fc\u00b7be", "Nacht", "bey", "dir", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schau wie Lerch und Nachtigallen/", "tokens": ["Schau", "wie", "Lerch", "und", "Nach\u00b7ti\u00b7gal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die man fr\u00fcher h\u00f6rt als sieht/", "tokens": ["Die", "man", "fr\u00fc\u00b7her", "h\u00f6rt", "als", "sieht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "VVFIN", "KOKOM", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gott zu Ehren lassen schallen", "tokens": ["Gott", "zu", "Eh\u00b7ren", "las\u00b7sen", "schal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVINF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein erfreutes Morgen-Lied/", "tokens": ["Ein", "er\u00b7freu\u00b7tes", "Mor\u00b7gen\u00b7Lied", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Folge nach/ mein Hertz/ und finge", "tokens": ["Fol\u00b7ge", "nach", "/", "mein", "Hertz", "/", "und", "fin\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "$(", "PPOSAT", "NN", "$(", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gott dem Sch\u00f6pffer aller Dinge.", "tokens": ["Gott", "dem", "Sch\u00f6pf\u00b7fer", "al\u00b7ler", "Din\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Herrscher \u00fcber Tod und Leben/", "tokens": ["Herr\u00b7scher", "\u00fc\u00b7ber", "Tod", "und", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meister \u00fcber Nacht und Tag/", "tokens": ["Meis\u00b7ter", "\u00fc\u00b7ber", "Nacht", "und", "Tag", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dir mu\u00df billich Ehre geben", "tokens": ["Dir", "mu\u00df", "bil\u00b7lich", "Eh\u00b7re", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was nur Athem ziehen mag/", "tokens": ["Was", "nur", "A\u00b7them", "zie\u00b7hen", "mag", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und dich/ seine Pflicht zu weisen/", "tokens": ["Und", "dich", "/", "sei\u00b7ne", "Pflicht", "zu", "wei\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit den Morgen-Sternen preisen.", "tokens": ["Mit", "den", "Mor\u00b7gen\u00b7S\u00b7ter\u00b7nen", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Himmels-F\u00fcrst und Erden-K\u00f6nig/", "tokens": ["Him\u00b7mels\u00b7F\u00fcrst", "und", "Er\u00b7den\u00b7K\u00f6\u00b7nig", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Grosser HERR der Herrligkeit/", "tokens": ["Gros\u00b7ser", "HeRR", "der", "Herr\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Zung\u2019 ist viel zu wenig/", "tokens": ["Mei\u00b7ne", "Zung'", "ist", "viel", "zu", "we\u00b7nig", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PTKA", "PIS", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie deinen Ruhm ausbreit/", "tokens": ["Da\u00df", "sie", "dei\u00b7nen", "Ruhm", "aus\u00b7breit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber la\u00df dir doch das Lallen", "tokens": ["A\u00b7ber", "la\u00df", "dir", "doch", "das", "Lal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deines Kindes wohl gefallen.", "tokens": ["Dei\u00b7nes", "Kin\u00b7des", "wohl", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Da\u00df ich deiner Sonne Blincken", "tokens": ["Da\u00df", "ich", "dei\u00b7ner", "Son\u00b7ne", "Blin\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und des hellen Tages Zier/", "tokens": ["Und", "des", "hel\u00b7len", "Ta\u00b7ges", "Zier", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich deiner Sternen Wincken", "tokens": ["Da\u00df", "ich", "dei\u00b7ner", "Ster\u00b7nen", "Win\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und di\u00df gantze Welt-Refier", "tokens": ["Und", "di\u00df", "gant\u00b7ze", "Welt\u00b7Re\u00b7fier"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDS", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deiner Wercke voll gesehen/", "tokens": ["Dei\u00b7ner", "Wer\u00b7cke", "voll", "ge\u00b7se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist durch deine Macht geschehen.", "tokens": ["Ist", "durch", "dei\u00b7ne", "Macht", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Da\u00df ich mit gesundem Leibe", "tokens": ["Da\u00df", "ich", "mit", "ge\u00b7sun\u00b7dem", "Lei\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fr\u00f6lich Athem sch\u00f6pffen kan/", "tokens": ["Fr\u00f6\u00b7lich", "A\u00b7them", "sch\u00f6pf\u00b7fen", "kan", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in meinem Stande treibe", "tokens": ["Und", "in", "mei\u00b7nem", "Stan\u00b7de", "trei\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was mir ist befohlen an/", "tokens": ["Was", "mir", "ist", "be\u00b7foh\u00b7len", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "VVPP", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ich Kleid und Nahrung habe/", "tokens": ["Da\u00df", "ich", "Kleid", "und", "Nah\u00b7rung", "ha\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VAFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nenn ich billich deine Gabe.", "tokens": ["Nenn", "ich", "bil\u00b7lich", "dei\u00b7ne", "Ga\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Da\u00df ich freudig im Gewissen/", "tokens": ["Da\u00df", "ich", "freu\u00b7dig", "im", "Ge\u00b7wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPRART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Frey von S\u00fcnd/ und H\u00f6llen Noth/", "tokens": ["Frey", "von", "S\u00fcnd", "/", "und", "H\u00f6l\u00b7len", "Noth", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$(", "KON", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auff dein theures Blutvergiessen/", "tokens": ["Auff", "dein", "theu\u00b7res", "Blut\u00b7ver\u00b7gies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Geh durch Sorge/ Schmertz und Tod.", "tokens": ["Geh", "durch", "Sor\u00b7ge", "/", "Schmertz", "und", "Tod", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ich ewig dich soll sehen/", "tokens": ["Da\u00df", "ich", "e\u00b7wig", "dich", "soll", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist/ und wird/ durch dich geschehen.", "tokens": ["Ist", "/", "und", "wird", "/", "durch", "dich", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "KON", "VAFIN", "$(", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Fahre fort mich so zu pflegen/", "tokens": ["Fah\u00b7re", "fort", "mich", "so", "zu", "pfle\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "PPER", "ADV", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Halt mich unter deiner Hutt/", "tokens": ["Halt", "mich", "un\u00b7ter", "dei\u00b7ner", "Hutt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kr\u00f6ne mich mit deinem Segen/", "tokens": ["Kr\u00f6\u00b7ne", "mich", "mit", "dei\u00b7nem", "Se\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gieb was hier und ewig gutt/", "tokens": ["Gieb", "was", "hier", "und", "e\u00b7wig", "gutt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PWS", "ADV", "KON", "ADJD", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So soll dir mit Engel-Zungen", "tokens": ["So", "soll", "dir", "mit", "En\u00b7gel\u00b7Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Werden Lob und Danck gesungen.", "tokens": ["Wer\u00b7den", "Lob", "und", "Danck", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}