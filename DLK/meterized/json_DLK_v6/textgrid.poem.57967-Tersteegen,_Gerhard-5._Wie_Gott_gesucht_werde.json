{"textgrid.poem.57967": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "5. Wie Gott gesucht werde", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df los die Kreatur, entsink dem eignen Willen,", "tokens": ["La\u00df", "los", "die", "Kre\u00b7a\u00b7tur", ",", "ent\u00b7sink", "dem", "eig\u00b7nen", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "ART", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gedenk nicht mehr an dich und la\u00df dich Gott im Grund", "tokens": ["Ge\u00b7denk", "nicht", "mehr", "an", "dich", "und", "la\u00df", "dich", "Gott", "im", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ADV", "APPR", "PPER", "KON", "VVIMP", "PPER", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dem\u00fctig, liebreich, sanft, merk, wenn er dich will stillen;", "tokens": ["De\u00b7m\u00fc\u00b7tig", ",", "lieb\u00b7reich", ",", "sanft", ",", "merk", ",", "wenn", "er", "dich", "will", "stil\u00b7len", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So findst du dich in Gott und Gott in dir zur Stund'!", "tokens": ["So", "findst", "du", "dich", "in", "Gott", "und", "Gott", "in", "dir", "zur", "Stund'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "KON", "NN", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}