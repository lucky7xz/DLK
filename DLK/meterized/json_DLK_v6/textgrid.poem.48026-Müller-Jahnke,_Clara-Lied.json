{"textgrid.poem.48026": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Lied", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df ab mit deinen Blicken \u2013", "tokens": ["La\u00df", "ab", "mit", "dei\u00b7nen", "Bli\u00b7cken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "nicht k\u00f6nnen sie fortan", "tokens": ["nicht", "k\u00f6n\u00b7nen", "sie", "for\u00b7tan"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "mich fester noch umstricken,", "tokens": ["mich", "fes\u00b7ter", "noch", "um\u00b7stri\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "als sie es schon getan.", "tokens": ["als", "sie", "es", "schon", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "La\u00df ab mit deinen Worten,", "tokens": ["La\u00df", "ab", "mit", "dei\u00b7nen", "Wor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "die schmeichelnd mich bet\u00f6rt, \u2013", "tokens": ["die", "schmei\u00b7chelnd", "mich", "be\u00b7t\u00f6rt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "PPER", "VVFIN", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "mein Ohr doch allerorten", "tokens": ["mein", "Ohr", "doch", "al\u00b7le\u00b7ror\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "nur deine Stimme h\u00f6rt.", "tokens": ["nur", "dei\u00b7ne", "Stim\u00b7me", "h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "La\u00df ab mit deinen K\u00fcssen, \u2013", "tokens": ["La\u00df", "ab", "mit", "dei\u00b7nen", "K\u00fcs\u00b7sen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PTKVZ", "APPR", "PPOSAT", "NN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "mein Herz pocht bang und schwer:", "tokens": ["mein", "Herz", "pocht", "bang", "und", "schwer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "ich hab dich lieben m\u00fcssen", "tokens": ["ich", "hab", "dich", "lie\u00b7ben", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "und seh kein Ende mehr . . .", "tokens": ["und", "seh", "kein", "En\u00b7de", "mehr", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "ADV", "$.", "$.", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}