{"textgrid.poem.28534": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Finstern\u00fc\u00df ist gut, weil sie viel S\u00fcnden stillet;", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Finstern\u00fc\u00df ist gut, weil sie viel S\u00fcnden stillet;", "tokens": ["Die", "Fins\u00b7ter\u00b7n\u00fc\u00df", "ist", "gut", ",", "weil", "sie", "viel", "S\u00fcn\u00b7den", "stil\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Finstern\u00fc\u00df ist arg, weil sie viel S\u00fcnden h\u00fcllet.", "tokens": ["Die", "Fins\u00b7ter\u00b7n\u00fc\u00df", "ist", "arg", ",", "weil", "sie", "viel", "S\u00fcn\u00b7den", "h\u00fcl\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein iedes Ding ist gut, b\u00f6s ist ein iedes Ding", "tokens": ["Ein", "ie\u00b7des", "Ding", "ist", "gut", ",", "b\u00f6s", "ist", "ein", "ie\u00b7des", "Ding"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VAFIN", "ADJD", "$,", "ADJD", "VAFIN", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nich f\u00fcr sich selbst, nach dem ein ieder mite gieng.", "tokens": ["Nich", "f\u00fcr", "sich", "selbst", ",", "nach", "dem", "ein", "ie\u00b7der", "mi\u00b7te", "gieng", "."], "token_info