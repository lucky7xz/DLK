{"textgrid.poem.54575": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "64.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich lebe/ wo man den mit Recht kan lebend nennen/", "tokens": ["Ich", "le\u00b7be", "/", "wo", "man", "den", "mit", "Recht", "kan", "le\u00b7bend", "nen\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWAV", "PIS", "ART", "APPR", "NN", "VMFIN", "ADJD", "VVINF", "$("], "meter": "-+--+-+-+-+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der sonder Geist verf\u00e4llt in bitter-s\u00fcsse Pein/", "tokens": ["Der", "son\u00b7der", "Geist", "ver\u00b7f\u00e4llt", "in", "bit\u00b7ter\u00b7s\u00fcs\u00b7se", "Pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Seel ist ausser mir/ und sucht den Glantz allein", "tokens": ["Die", "Seel", "ist", "aus\u00b7ser", "mir", "/", "und", "sucht", "den", "Glantz", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "$(", "KON", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Augen/ die mir nur zu angenehme brennen/", "tokens": ["Der", "Au\u00b7gen", "/", "die", "mir", "nur", "zu", "an\u00b7ge\u00b7neh\u00b7me", "bren\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "ADV", "APPR", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was kan in meiner Nacht ich als die Stern erkennen.", "tokens": ["Was", "kan", "in", "mei\u00b7ner", "Nacht", "ich", "als", "die", "Stern", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPR", "PPOSAT", "NN", "PPER", "KOUS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Holdseligst ihr Gesicht/ der Wunder-helle Schein/", "tokens": ["Hold\u00b7se\u00b7ligst", "ihr", "Ge\u00b7sicht", "/", "der", "Wun\u00b7der\u00b7hel\u00b7le", "Schein", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$(", "ART", "NN", "NN", "$("], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Erleuchtet di\u00df Gem\u00fcth/ das (geht die Welt schier ein)", "tokens": ["Er\u00b7leuch\u00b7tet", "di\u00df", "Ge\u00b7m\u00fcth", "/", "das", "(", "geht", "die", "Welt", "schier", "ein", ")"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "NN", "$(", "ART", "$(", "VVFIN", "ART", "NN", "ADJD", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Kein Schwefel-lichter Blitz wird von dem Vorsatz trennen.", "tokens": ["Kein", "Schwe\u00b7fel\u00b7lich\u00b7ter", "Blitz", "wird", "von", "dem", "Vor\u00b7satz", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VAFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Lasset Nord und Wetter toben weil mir diese Rosen bl\u00fchen/", "tokens": ["Las\u00b7set", "Nord", "und", "Wet\u00b7ter", "to\u00b7ben", "weil", "mir", "die\u00b7se", "Ro\u00b7sen", "bl\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "NN", "VVFIN", "KOUS", "PPER", "PDAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Schreckt mich keiner Winter Rasen/ last die heisse Sonn entfliehen/", "tokens": ["Schreckt", "mich", "kei\u00b7ner", "Win\u00b7ter", "Ra\u00b7sen", "/", "last", "die", "heis\u00b7se", "Sonn", "ent\u00b7flie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "NN", "$(", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Mir ist die Abend-Lufft weit lieber als der Tag.", "tokens": ["Mir", "ist", "die", "A\u00b7ben\u00b7dLufft", "weit", "lie\u00b7ber", "als", "der", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "ADV", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ob die Zunge nicht mehr schwatzet/ die nie ein End-Urtheil spricht/", "tokens": ["Ob", "die", "Zun\u00b7ge", "nicht", "mehr", "schwat\u00b7zet", "/", "die", "nie", "ein", "En\u00b7dUrt\u00b7heil", "spricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "ADV", "VVFIN", "$(", "ART", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "--+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Treugt doch der entf\u00e4rbten Wangen lieblich Abendr\u00f6the nicht.", "tokens": ["Treugt", "doch", "der", "ent\u00b7f\u00e4rb\u00b7ten", "Wan\u00b7gen", "lieb\u00b7lich", "A\u00b7bend\u00b7r\u00f6\u00b7the", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "ADJD", "NN", "PTKNEG", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Die redet nur zu wohl/ die schweigend reden mag.", "tokens": ["Die", "re\u00b7det", "nur", "zu", "wohl", "/", "die", "schwei\u00b7gend", "re\u00b7den", "mag."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ADV", "$(", "ART", "ADJD", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}