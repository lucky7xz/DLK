{"dta.poem.12632": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "XxI.  \n  Die Alster.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.42", "sv:0.42", "no:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bef\u00f6rdrer vieler Lustbarkeiten,", "tokens": ["Be\u00b7f\u00f6rd\u00b7rer", "vie\u00b7ler", "Lust\u00b7bar\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du angenehmer Alster-Flu\u00df!", "tokens": ["Du", "an\u00b7ge\u00b7neh\u00b7mer", "Als\u00b7ter\u00b7Flu\u00df", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du mehrest Hamburgs Seltenheiten", "tokens": ["Du", "meh\u00b7rest", "Ham\u00b7burgs", "Sel\u00b7ten\u00b7hei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihren fr\u00f6hlichen Genu\u00df.", "tokens": ["Und", "ih\u00b7ren", "fr\u00f6h\u00b7li\u00b7chen", "Ge\u00b7nu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dir schallen zur Ehre,", "tokens": ["Dir", "schal\u00b7len", "zur", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Du spielende Fluth!", "tokens": ["Du", "spie\u00b7len\u00b7de", "Fluth", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Die singenden Ch\u00f6re,", "tokens": ["Die", "sin\u00b7gen\u00b7den", "Ch\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Der jauchzende Muth.", "tokens": ["Der", "jauch\u00b7zen\u00b7de", "Muth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Der Elbe Schiff-Fahrt macht uns reicher;", "tokens": ["Der", "El\u00b7be", "Schiff\u00b7Fahrt", "macht", "uns", "rei\u00b7cher", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Alster lehrt gesellig seyn:", "tokens": ["Die", "Als\u00b7ter", "lehrt", "ge\u00b7sel\u00b7lig", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch jene f\u00fcllen sich die Speicher;", "tokens": ["Durch", "je\u00b7ne", "f\u00fcl\u00b7len", "sich", "die", "Spei\u00b7cher", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf dieser schmeckt der fremde Wein.", "tokens": ["Auf", "die\u00b7ser", "schmeckt", "der", "frem\u00b7de", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In schwimmenden Nachen", "tokens": ["In", "schwim\u00b7men\u00b7den", "Na\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Schifft Eintracht und Lust,", "tokens": ["Schifft", "Ein\u00b7tracht", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und Freyheit und Lachen", "tokens": ["Und", "Frey\u00b7heit", "und", "La\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Erleichtern die Brust.", "tokens": ["Er\u00b7leich\u00b7tern", "die", "Brust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Das Ufer ziert ein Gang von Linden,", "tokens": ["Das", "U\u00b7fer", "ziert", "ein", "Gang", "von", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In dem wir holde Sch\u00f6nen sehn,", "tokens": ["In", "dem", "wir", "hol\u00b7de", "Sch\u00f6\u00b7nen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die dort, wann Tag und Hitze schwinden,", "tokens": ["Die", "dort", ",", "wann", "Tag", "und", "Hit\u00b7ze", "schwin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "PWAV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Entz\u00fcckend auf- und niedergehn.", "tokens": ["Ent\u00b7z\u00fc\u00b7ckend", "auf", "und", "nie\u00b7der\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "TRUNC", "KON", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kaum haben vorzeiten", "tokens": ["Kaum", "ha\u00b7ben", "vor\u00b7zei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die Nymphen der Jagd,", "tokens": ["Die", "Nym\u00b7phen", "der", "Jagd", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Dianen zur Seiten,", "tokens": ["Di\u00b7a\u00b7nen", "zur", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "So reitzend gelacht.", "tokens": ["So", "reit\u00b7zend", "ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "O siehst du jemahls ohn Ergetzen,", "tokens": ["O", "siehst", "du", "je\u00b7mahls", "ohn", "Er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hammonia! des Walles Pracht?", "tokens": ["Ham\u00b7mo\u00b7nia", "!", "des", "Wal\u00b7les", "Pracht", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wann ihn die blauen Wellen netzen", "tokens": ["Wann", "ihn", "die", "blau\u00b7en", "Wel\u00b7len", "net\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und jeder Fr\u00fchling sch\u00f6ner macht:", "tokens": ["Und", "je\u00b7der", "Fr\u00fch\u00b7ling", "sch\u00f6\u00b7ner", "macht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wann jenes Gestade,", "tokens": ["Wann", "je\u00b7nes", "Ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Das Flora geschm\u00fcckt,", "tokens": ["Das", "Flo\u00b7ra", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "So manche Najade", "tokens": ["So", "man\u00b7che", "Na\u00b7ja\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Gef\u00e4llig erblickt.", "tokens": ["Ge\u00b7f\u00e4l\u00b7lig", "er\u00b7blickt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Ert\u00f6nt, ihr scherzenden Ges\u00e4nge,", "tokens": ["Er\u00b7t\u00f6nt", ",", "ihr", "scher\u00b7zen\u00b7den", "Ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus unserm Lust-Schiff um den Strand!", "tokens": ["Aus", "un\u00b7serm", "Lust\u00b7Schiff", "um", "den", "Strand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den steifen Ernst, das Wort-Gepr\u00e4nge", "tokens": ["Den", "stei\u00b7fen", "Ernst", ",", "das", "Wor\u00b7tGe\u00b7pr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verweis\u2019t die Alster auf das Land.", "tokens": ["Ver\u00b7weis't", "die", "Als\u00b7ter", "auf", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du leeres Gew\u00e4sche,", "tokens": ["Du", "lee\u00b7res", "Ge\u00b7w\u00e4\u00b7sche", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Dem Menschen-Witz fehlt!", "tokens": ["Dem", "Men\u00b7schen\u00b7Witz", "fehlt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "O fahr in die Fr\u00f6sche,", "tokens": ["O", "fahr", "in", "die", "Fr\u00f6\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "La\u00df uns ungequ\u00e4lt.", "tokens": ["La\u00df", "uns", "un\u00b7ge\u00b7qu\u00e4lt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Hier l\u00e4rmt, in N\u00e4chten voll Vergn\u00fcgen,", "tokens": ["Hier", "l\u00e4rmt", ",", "in", "N\u00e4ch\u00b7ten", "voll", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Pauken Schlag, des Waldhorns Schall;", "tokens": ["Der", "Pau\u00b7ken", "Schlag", ",", "des", "Wald\u00b7horns", "Schall", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier wirkt, bey Wein und s\u00fcssen Z\u00fcgen,", "tokens": ["Hier", "wirkt", ",", "bey", "Wein", "und", "s\u00fcs\u00b7sen", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die frohe Freyheit \u00fcberall.", "tokens": ["Die", "fro\u00b7he", "Frey\u00b7heit", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nichts lebet gebunden", "tokens": ["Nichts", "le\u00b7bet", "ge\u00b7bun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VVFIN", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Was Freundschaft hier paart.", "tokens": ["Was", "Freund\u00b7schaft", "hier", "paart", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADV", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "O gl\u00fcckliche Stunden!", "tokens": ["O", "gl\u00fcck\u00b7li\u00b7che", "Stun\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "O liebliche Fahrt!", "tokens": ["O", "lieb\u00b7li\u00b7che", "Fahrt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}