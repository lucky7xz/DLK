{"textgrid.poem.57085": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du schlankes Reh, das du die Menschen fliehst,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du schlankes Reh, das du die Menschen fliehst,", "tokens": ["Du", "schlan\u00b7kes", "Reh", ",", "das", "du", "die", "Men\u00b7schen", "fliehst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "bewegte dich dein Herz, wenn du mich siehst,", "tokens": ["be\u00b7weg\u00b7te", "dich", "dein", "Herz", ",", "wenn", "du", "mich", "siehst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "mich nicht zu fliehen, meinem Blick zu traun,", "tokens": ["mich", "nicht", "zu", "flie\u00b7hen", ",", "mei\u00b7nem", "Blick", "zu", "traun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "PTKZU", "VVINF", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "wie deinesgleichen mir ins Aug zu schaun!", "tokens": ["wie", "dei\u00b7nes\u00b7glei\u00b7chen", "mir", "ins", "Aug", "zu", "schaun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df mich frei von jeder Mordbegier,", "tokens": ["Ich", "wei\u00df", "mich", "frei", "von", "je\u00b7der", "Mord\u00b7be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ich jage mich, mein Bruder, nicht in dir.", "tokens": ["ich", "ja\u00b7ge", "mich", ",", "mein", "Bru\u00b7der", ",", "nicht", "in", "dir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PTKNEG", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du glaubst mir nicht? Ich bin dir nur ein Mann,", "tokens": ["Du", "glaubst", "mir", "nicht", "?", "Ich", "bin", "dir", "nur", "ein", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "ein Mensch ... Ach, Reh, was geht der Mensch mich an!", "tokens": ["ein", "Mensch", "...", "Ach", ",", "Reh", ",", "was", "geht", "der", "Mensch", "mich", "an", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ITJ", "$,", "NN", "$,", "PWS", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}