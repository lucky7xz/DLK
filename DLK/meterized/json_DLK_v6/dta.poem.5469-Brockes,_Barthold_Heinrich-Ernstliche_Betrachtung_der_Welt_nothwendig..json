{"dta.poem.5469": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ernstliche Betrachtung der Welt  \n nothwendig.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kann es auch sonder Kunst geschehn,", "tokens": ["Kann", "es", "auch", "son\u00b7der", "Kunst", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn wir an sch\u00f6nen Schildereyen", "tokens": ["Wenn", "wir", "an", "sch\u00f6\u00b7nen", "Schil\u00b7de\u00b7re\u00b7yen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Uns mit Vernunft ergetzen und erfreuen,", "tokens": ["Uns", "mit", "Ver\u00b7nunft", "er\u00b7get\u00b7zen", "und", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mit vergn\u00fcgtem Geist die Kunst des K\u00fcnftlers sehn?", "tokens": ["Und", "mit", "ver\u00b7gn\u00fcg\u00b7tem", "Geist", "die", "Kunst", "des", "K\u00fcnft\u00b7lers", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "O nein, ein Kunst-Erfahrner wei\u00df,", "tokens": ["O", "nein", ",", "ein", "Kunst\u00b7Er\u00b7fahr\u00b7ner", "wei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das blos durch einen langen Flei\u00df", "tokens": ["Das", "blos", "durch", "ei\u00b7nen", "lan\u00b7gen", "Flei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Man diese Wissenschaft erh\u00e4lt,", "tokens": ["Man", "die\u00b7se", "Wis\u00b7sen\u00b7schaft", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und sonder M\u00fche sie nicht findet;", "tokens": ["Und", "son\u00b7der", "M\u00fc\u00b7he", "sie", "nicht", "fin\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wie da\u00df man sich denn unterwindet,", "tokens": ["Wie", "da\u00df", "man", "sich", "denn", "un\u00b7ter\u00b7win\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PIS", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sich selber so geschickt zu sch\u00e4tzen,", "tokens": ["Sich", "sel\u00b7ber", "so", "ge\u00b7schickt", "zu", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "An dem weit sch\u00f6neren Gem\u00e4hlde dieser Welt", "tokens": ["An", "dem", "weit", "sch\u00f6\u00b7ne\u00b7ren", "Ge\u00b7m\u00e4hl\u00b7de", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Vern\u00fcnftig, ohn Vernunft, sich zu ergetzen,", "tokens": ["Ver\u00b7n\u00fcnf\u00b7tig", ",", "ohn", "Ver\u00b7nunft", ",", "sich", "zu", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUI", "NN", "$,", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Ja, ohn es einmahl anzusehn,", "tokens": ["Ja", ",", "ohn", "es", "ein\u00b7mahl", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUI", "PPER", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Des grossen Meisters Geist doch sattsahm zu verstehn,", "tokens": ["Des", "gros\u00b7sen", "Meis\u00b7ters", "Geist", "doch", "satt\u00b7sahm", "zu", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Den er in seinem Wercke weiset;", "tokens": ["Den", "er", "in", "sei\u00b7nem", "Wer\u00b7cke", "wei\u00b7set", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Ein Werck, das \u00fcberall den grossen Meister preiset?", "tokens": ["Ein", "Werck", ",", "das", "\u00fc\u00b7be\u00b7rall", "den", "gros\u00b7sen", "Meis\u00b7ter", "prei\u00b7set", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ist es nun eine Kunst, der GOttheit Werck zu fassen;", "tokens": ["Ist", "es", "nun", "ei\u00b7ne", "Kunst", ",", "der", "Got\u00b7theit", "Werck", "zu", "fas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "$,", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Will man sich denn darin nicht unterrichten lassen?", "tokens": ["Will", "man", "sich", "denn", "da\u00b7rin", "nicht", "un\u00b7ter\u00b7rich\u00b7ten", "las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PRF", "ADV", "PAV", "PTKNEG", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O ja! ich wolte gern, h\u00f6r\u2019 ich verschiedne sagen,", "tokens": ["O", "ja", "!", "ich", "wol\u00b7te", "gern", ",", "h\u00f6r'", "ich", "ver\u00b7schied\u00b7ne", "sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "PPER", "VMFIN", "ADV", "$,", "VVFIN", "PPER", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mich an des Sch\u00f6pfers Werck vergn\u00fcgen,", "tokens": ["Mich", "an", "des", "Sch\u00f6p\u00b7fers", "Werck", "ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und meine Lust zu seiner Ehre f\u00fcgen;", "tokens": ["Und", "mei\u00b7ne", "Lust", "zu", "sei\u00b7ner", "Eh\u00b7re", "f\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Allein wer lehrt es mich? h\u00f6r\u2019 ich dieselben fragen;", "tokens": ["Al\u00b7lein", "wer", "lehrt", "es", "mich", "?", "h\u00f6r'", "ich", "die\u00b7sel\u00b7ben", "fra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "PPER", "PRF", "$.", "VVFIN", "PPER", "PDAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die GOtts-Gelehrten legen sich,", "tokens": ["Die", "Gotts\u00b7Ge\u00b7lehr\u00b7ten", "le\u00b7gen", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So wie ich schon seit langer Zeit bemercke,", "tokens": ["So", "wie", "ich", "schon", "seit", "lan\u00b7ger", "Zeit", "be\u00b7mer\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Allein auf GOttes Wort, nicht leicht auf GOttes Wercke.", "tokens": ["Al\u00b7lein", "auf", "Got\u00b7tes", "Wort", ",", "nicht", "leicht", "auf", "Got\u00b7tes", "Wer\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "NN", "$,", "PTKNEG", "ADJD", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nun ist es recht, da\u00df sie von seinem Wort nicht schweigen,", "tokens": ["Nun", "ist", "es", "recht", ",", "da\u00df", "sie", "von", "sei\u00b7nem", "Wort", "nicht", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch solt ein jeder auch nicht minder gern", "tokens": ["Doch", "solt", "ein", "je\u00b7der", "auch", "nicht", "min\u00b7der", "gern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "PIS", "ADV", "PTKNEG", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die grossen Wunder seines HErrn", "tokens": ["Die", "gros\u00b7sen", "Wun\u00b7der", "sei\u00b7nes", "Herrn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den sonst stock-blinden H\u00f6rern zeigen.", "tokens": ["Den", "sonst", "stock\u00b7blin\u00b7den", "H\u00f6\u00b7rern", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich hoff\u2019 es wird auch mehr und mehr geschehen;", "tokens": ["Ich", "hoff'", "es", "wird", "auch", "mehr", "und", "mehr", "ge\u00b7sche\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN", "ADV", "ADV", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So lang es nicht geschicht,", "tokens": ["So", "lang", "es", "nicht", "ge\u00b7schicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Erfodert es doch deine Pflicht,", "tokens": ["Er\u00b7fo\u00b7dert", "es", "doch", "dei\u00b7ne", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie oft mit schuldiger Betrachtung anzusehen.", "tokens": ["Sie", "oft", "mit", "schul\u00b7di\u00b7ger", "Be\u00b7trach\u00b7tung", "an\u00b7zu\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}