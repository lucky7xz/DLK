{"dta.poem.7780": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Die Geschichte des Davids,  \n K\u00f6nigs in Juda.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198429", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie unvermuthlich die\u00df dem David war zu h\u00f6ren", "tokens": ["Wie", "un\u00b7ver\u00b7muth\u00b7lich", "die\u00df", "dem", "Da\u00b7vid", "war", "zu", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PDS", "ART", "NE", "VAFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Steht nicht zu bilden f\u00fcr, er wust sich nicht zu kehren", "tokens": ["Steht", "nicht", "zu", "bil\u00b7den", "f\u00fcr", ",", "er", "wust", "sich", "nicht", "zu", "keh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "PTKZU", "VVINF", "APPR", "$,", "PPER", "VVFIN", "PRF", "PTKNEG", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "F\u00fcr Schrecken aus der Stell. Der Donner hatte nicht", "tokens": ["F\u00fcr", "Schre\u00b7cken", "aus", "der", "Stell", ".", "Der", "Don\u00b7ner", "hat\u00b7te", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$.", "ART", "NN", "VAFIN", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jhn so best\u00fcrtzt gemacht, als die\u00df so hie geschicht:", "tokens": ["Jhn", "so", "be\u00b7st\u00fcrtzt", "ge\u00b7macht", ",", "als", "die\u00df", "so", "hie", "ge\u00b7schicht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VVPP", "$,", "KOUS", "PDS", "ADV", "ADV", "VVPP", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Er wei\u00df nun wer sie sind, wei\u00df doch nicht wer ihn liebet;", "tokens": ["Er", "wei\u00df", "nun", "wer", "sie", "sind", ",", "wei\u00df", "doch", "nicht", "wer", "ihn", "lie\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PWS", "PPER", "VAFIN", "$,", "VVFIN", "ADV", "PTKNEG", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Merob und Michal sind diese, deren giebet", "tokens": ["Die", "Me\u00b7rob", "und", "Mic\u00b7hal", "sind", "die\u00b7se", ",", "de\u00b7ren", "gie\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "KON", "NE", "VAFIN", "PDS", "$,", "PDS", "VVFIN"], "meter": "-+--+--++--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Die eine ihre Lieb so deutlich an den Tag,", "tokens": ["Die", "ei\u00b7ne", "ih\u00b7re", "Lieb", "so", "deut\u00b7lich", "an", "den", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "PPOSAT", "NN", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch welche es recht sey, er nicht errathen mag.", "tokens": ["Doch", "wel\u00b7che", "es", "recht", "sey", ",", "er", "nicht", "er\u00b7ra\u00b7then", "mag."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["KON", "PRELS", "PPER", "ADJD", "VAFIN", "$,", "PPER", "PTKNEG", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Er f\u00fchlet schon in ihm ein Liebesfeur entstehen,", "tokens": ["Er", "f\u00fch\u00b7let", "schon", "in", "ihm", "ein", "Lie\u00b7bes\u00b7feur", "ent\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Er w\u00fcnscht den hellen Tag, um diese anzusehen,", "tokens": ["Er", "w\u00fcnscht", "den", "hel\u00b7len", "Tag", ",", "um", "die\u00b7se", "an\u00b7zu\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "KOUI", "PDS", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der er so wol gef\u00e4llt, und dencket Gottes Rath", "tokens": ["Der", "er", "so", "wol", "ge\u00b7f\u00e4llt", ",", "und", "den\u00b7cket", "Got\u00b7tes", "Rath"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "$,", "KON", "VVFIN", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Hab es also geschickt, da\u00df an des K\u00f6nigs Statt", "tokens": ["Hab", "es", "al\u00b7so", "ge\u00b7schickt", ",", "da\u00df", "an", "des", "K\u00f6\u00b7nigs", "Statt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "VVPP", "$,", "KOUS", "APPR", "ART", "NN", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Er soll auf diese Wei\u00df durch eine Heurath kommen,", "tokens": ["Er", "soll", "auf", "die\u00b7se", "Wei\u00df", "durch", "ei\u00b7ne", "Heu\u00b7rath", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PDAT", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Des K\u00f6nigs Tochtermann zu seyn, k\u00f6nnt ihn wol frommen:", "tokens": ["Des", "K\u00f6\u00b7nigs", "Toch\u00b7ter\u00b7mann", "zu", "seyn", ",", "k\u00f6nnt", "ihn", "wol", "from\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VAINF", "$,", "VVFIN", "PPER", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und hatte er sein Lob mit Schamr\u00f6th angeh\u00f6rt", "tokens": ["Und", "hat\u00b7te", "er", "sein", "Lob", "mit", "Scham\u00b7r\u00f6th", "an\u00b7ge\u00b7h\u00f6rt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Aus der Thalmais Mund, die er allstets geehrt,", "tokens": ["Aus", "der", "Thal\u00b7mais", "Mund", ",", "die", "er", "alls\u00b7tets", "ge\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.17": {"text": "Als Jonathans Gemahl, des Freundschafft er genosse,", "tokens": ["Als", "Jo\u00b7na\u00b7thans", "Ge\u00b7mahl", ",", "des", "Freund\u00b7schafft", "er", "ge\u00b7nos\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "$,", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Doch hatt sie was gesagt, das ihn dabey verdrosse,", "tokens": ["Doch", "hatt", "sie", "was", "ge\u00b7sagt", ",", "das", "ihn", "da\u00b7bey", "ver\u00b7dros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PIS", "VVPP", "$,", "PRELS", "PPER", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da\u00df nemlich er dem Krieg auch w\u00e4r gefolget nach,", "tokens": ["Da\u00df", "nem\u00b7lich", "er", "dem", "Krieg", "auch", "w\u00e4r", "ge\u00b7fol\u00b7get", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ART", "NN", "ADV", "VAFIN", "VVPP", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da er doch bey der Heerd allhie so m\u00fcssig lag.", "tokens": ["Da", "er", "doch", "bey", "der", "Heerd", "all\u00b7hie", "so", "m\u00fcs\u00b7sig", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Jmmittelst hub die an, die ihn so hertzlich liebte:", "tokens": ["Jm\u00b7mit\u00b7telst", "hub", "die", "an", ",", "die", "ihn", "so", "hertz\u00b7lich", "lieb\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "PTKVZ", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Er ist nicht mit zu Feld, ob es ihm gleich beliebte,", "tokens": ["Er", "ist", "nicht", "mit", "zu", "Feld", ",", "ob", "es", "ihm", "gleich", "be\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "APPR", "APPR", "NN", "$,", "KOUS", "PPER", "PPER", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Er ist zu Bethlehem, der K\u00f6nig sandt ihn hin", "tokens": ["Er", "ist", "zu", "Beth\u00b7le\u00b7hem", ",", "der", "K\u00f6\u00b7nig", "sandt", "ihn", "hin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "$,", "ART", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Auf Jsai Begehr, und war darinn mein Sinn", "tokens": ["Auf", "Jsai", "Be\u00b7gehr", ",", "und", "war", "da\u00b7rinn", "mein", "Sinn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "$,", "KON", "VAFIN", "PAV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Erf\u00fcllt nach meinem Wunsch. Ich fieng schon an zu sorgen,", "tokens": ["Er\u00b7f\u00fcllt", "nach", "mei\u00b7nem", "Wunsch", ".", "Ich", "fi\u00b7eng", "schon", "an", "zu", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ADV", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Und sah auf die Gefahr; Dann mir ist nicht verborgen,", "tokens": ["Und", "sah", "auf", "die", "Ge\u00b7fahr", ";", "Dann", "mir", "ist", "nicht", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$.", "ADV", "PPER", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wie es im Kriege geht. Ich kenne seinen Muth,", "tokens": ["Wie", "es", "im", "Krie\u00b7ge", "geht", ".", "Ich", "ken\u00b7ne", "sei\u00b7nen", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und da\u00df ob er gleich jung, er mehr wie andre thut;", "tokens": ["Und", "da\u00df", "ob", "er", "gleich", "jung", ",", "er", "mehr", "wie", "and\u00b7re", "thut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "KOUS", "PPER", "ADV", "ADJD", "$,", "PPER", "PIAT", "KOKOM", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Auch immer will f\u00fcran, dabey er unerfahren,", "tokens": ["Auch", "im\u00b7mer", "will", "f\u00fc\u00b7ran", ",", "da\u00b7bey", "er", "un\u00b7er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PTKVZ", "$,", "KOUS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wie bald k\u00f6nnt ihm ein Leyd f\u00fcr andern wiederfahren.", "tokens": ["Wie", "bald", "k\u00f6nnt", "ihm", "ein", "Leyd", "f\u00fcr", "an\u00b7dern", "wie\u00b7der\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die\u00df gab dem David Trost, da\u00df sie nicht \u00fcbel nahm,", "tokens": ["Die\u00df", "gab", "dem", "Da\u00b7vid", "Trost", ",", "da\u00df", "sie", "nicht", "\u00fc\u00b7bel", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NE", "NN", "$,", "KOUS", "PPER", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da\u00df er zu Bethlehem, und nicht zum Treffen kam.", "tokens": ["Da\u00df", "er", "zu", "Beth\u00b7le\u00b7hem", ",", "und", "nicht", "zum", "Tref\u00b7fen", "kam", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "$,", "KON", "PTKNEG", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Er horchte drauf gantz still, was man w\u00fcrd weiters fragen,", "tokens": ["Er", "horch\u00b7te", "drauf", "gantz", "still", ",", "was", "man", "w\u00fcrd", "wei\u00b7ters", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ADV", "ADJD", "$,", "PRELS", "PIS", "VAFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und w\u00fcnscht in solcher Lust noch nicht, da\u00df es m\u00f6cht tagen,", "tokens": ["Und", "w\u00fcnscht", "in", "sol\u00b7cher", "Lust", "noch", "nicht", ",", "da\u00df", "es", "m\u00f6cht", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "ADV", "PTKNEG", "$,", "KOUS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wann ihm der Tag fiel ein, bekam er neue Quaal,", "tokens": ["Wann", "ihm", "der", "Tag", "fiel", "ein", ",", "be\u00b7kam", "er", "neu\u00b7e", "Qua\u00b7al", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "PTKVZ", "$,", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Weil dann w\u00fcrd seine Ruh verschwinden allzumahl.", "tokens": ["Weil", "dann", "w\u00fcrd", "sei\u00b7ne", "Ruh", "ver\u00b7schwin\u00b7den", "all\u00b7zu\u00b7mahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VAFIN", "PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Doch ware auch darnach sein allerh\u00f6chst Verlangen,", "tokens": ["Doch", "wa\u00b7re", "auch", "dar\u00b7nach", "sein", "al\u00b7ler\u00b7h\u00f6chst", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PAV", "PPOSAT", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Weil er dann hofft zu sehn die \u00fcbersch\u00f6nen Wangen,", "tokens": ["Weil", "er", "dann", "hofft", "zu", "sehn", "die", "\u00fc\u00b7bersc\u00b7h\u00f6\u00b7nen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "PTKZU", "VVINF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die schon sein Hertz verwundt, ohn da\u00df er diese kannt,", "tokens": ["Die", "schon", "sein", "Hertz", "ver\u00b7wundt", ",", "ohn", "da\u00df", "er", "die\u00b7se", "kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVPP", "$,", "KOUI", "KOUS", "PPER", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Die sich in ihn verliebt, in der er wieder brannt.", "tokens": ["Die", "sich", "in", "ihn", "ver\u00b7liebt", ",", "in", "der", "er", "wie\u00b7der", "brannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPER", "VVPP", "$,", "APPR", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Sollt es wol Merob seyn? dacht der verliebte Hirte,", "tokens": ["Sollt", "es", "wol", "Me\u00b7rob", "seyn", "?", "dacht", "der", "ver\u00b7lieb\u00b7te", "Hir\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "NE", "VAINF", "$.", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+---+-+-", "measure": "trochaic.penta.relaxed"}, "line.42": {"text": "Mir deucht die Merob wars, die diese Reden f\u00fchrte;", "tokens": ["Mir", "deucht", "die", "Me\u00b7rob", "wars", ",", "die", "die\u00b7se", "Re\u00b7den", "f\u00fchr\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VAFIN", "$,", "PRELS", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Ja wann es Merob ist, so liebet sie mein Sinn,", "tokens": ["Ja", "wann", "es", "Me\u00b7rob", "ist", ",", "so", "lie\u00b7bet", "sie", "mein", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PWAV", "PPER", "NE", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Wanns aber Michal ist, ich ihr ergeben bin.", "tokens": ["Wanns", "a\u00b7ber", "Mic\u00b7hal", "ist", ",", "ich", "ihr", "er\u00b7ge\u00b7ben", "bin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NE", "VAFIN", "$,", "PPER", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Die Merob ist zu stoltz, sie redt von stoltzen Sinnen,", "tokens": ["Die", "Me\u00b7rob", "ist", "zu", "stoltz", ",", "sie", "redt", "von", "stolt\u00b7zen", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Drum wird es Michal seyn, die ich kan so gewinnen;", "tokens": ["Drum", "wird", "es", "Mic\u00b7hal", "seyn", ",", "die", "ich", "kan", "so", "ge\u00b7win\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "NE", "VAINF", "$,", "PRELS", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.47": {"text": "Nicht aus mir selber nein, aus Gottes weisem Rath,", "tokens": ["Nicht", "aus", "mir", "sel\u00b7ber", "nein", ",", "aus", "Got\u00b7tes", "wei\u00b7sem", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPER", "ADV", "PTKANT", "$,", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Der sie mir ausersehn, und so gelencket hat.", "tokens": ["Der", "sie", "mir", "au\u00b7ser\u00b7sehn", ",", "und", "so", "ge\u00b7len\u00b7cket", "hat", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVINF", "$,", "KON", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Es sey dann wer es sey, ich will sie wieder lieben,", "tokens": ["Es", "sey", "dann", "wer", "es", "sey", ",", "ich", "will", "sie", "wie\u00b7der", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PWS", "PPER", "VAFIN", "$,", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Ich lieb sie alle beyd, um keine zu betr\u00fcben.", "tokens": ["Ich", "lieb", "sie", "al\u00b7le", "beyd", ",", "um", "kei\u00b7ne", "zu", "be\u00b7tr\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "PIS", "$,", "KOUI", "PIAT", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Die Merob die ist sch\u00f6n, der Michal Lieblichkeit", "tokens": ["Die", "Me\u00b7rob", "die", "ist", "sch\u00f6n", ",", "der", "Mic\u00b7hal", "Lieb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "VAFIN", "ADJD", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Steigt \u00fcber alles auf, und machet mich bereit", "tokens": ["Steigt", "\u00fc\u00b7ber", "al\u00b7les", "auf", ",", "und", "ma\u00b7chet", "mich", "be\u00b7reit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PIS", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Zu fassen diesen Muth, Sauls Eydam dann zu werden.", "tokens": ["Zu", "fas\u00b7sen", "die\u00b7sen", "Muth", ",", "Sauls", "Ey\u00b7dam", "dann", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PDAT", "NN", "$,", "NE", "NE", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Ich bin ja schon gesalbt, was schaden mir die Heerden,", "tokens": ["Ich", "bin", "ja", "schon", "ge\u00b7salbt", ",", "was", "scha\u00b7den", "mir", "die", "Heer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "$,", "PWS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Thalmais sagt ja selbst, ob ich zwar sey ein Hirt,", "tokens": ["Thal\u00b7mais", "sagt", "ja", "selbst", ",", "ob", "ich", "zwar", "sey", "ein", "Hirt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADV", "$,", "KOUS", "PPER", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.56": {"text": "So zeig ich doch schon an, was aus mir werden wird.", "tokens": ["So", "zeig", "ich", "doch", "schon", "an", ",", "was", "aus", "mir", "wer\u00b7den", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$,", "PRELS", "APPR", "PPER", "VAINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Also dacht David hin, inzwischen diese dreye,", "tokens": ["Al\u00b7so", "dacht", "Da\u00b7vid", "hin", ",", "in\u00b7zwi\u00b7schen", "die\u00b7se", "drey\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "PTKVZ", "$,", "ADV", "PDAT", "ADJA", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.58": {"text": "Dieweil sie sich allein vermeynten, ohne Scheue", "tokens": ["Die\u00b7weil", "sie", "sich", "al\u00b7lein", "ver\u00b7meyn\u00b7ten", ",", "oh\u00b7ne", "Scheu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,", "KOUI", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Einander machten kund, was sonsten niemand wust,", "tokens": ["Ein\u00b7an\u00b7der", "mach\u00b7ten", "kund", ",", "was", "sons\u00b7ten", "nie\u00b7mand", "wust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "PWS", "VMFIN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Und weil Thalmais sucht hierinnen ihre Lust,", "tokens": ["Und", "weil", "Thal\u00b7mais", "sucht", "hie\u00b7rin\u00b7nen", "ih\u00b7re", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NE", "VVFIN", "PAV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.61": {"text": "Zu schertzen mit sie beyd, mu\u00df Adriel sich leiden,", "tokens": ["Zu", "schert\u00b7zen", "mit", "sie", "beyd", ",", "mu\u00df", "Ad\u00b7riel", "sich", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "PPER", "PTKVZ", "$,", "VMFIN", "NE", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.62": {"text": "Und kan die so ihn liebt, f\u00fcr ihr nicht mehr vermeiden", "tokens": ["Und", "kan", "die", "so", "ihn", "liebt", ",", "f\u00fcr", "ihr", "nicht", "mehr", "ver\u00b7mei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "ADV", "PPER", "VVFIN", "$,", "APPR", "PPER", "PTKNEG", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Die sonst gebergte Gluth. Die Dunckelheit der Nacht,", "tokens": ["Die", "sonst", "ge\u00b7berg\u00b7te", "Gluth", ".", "Die", "Dun\u00b7ckel\u00b7heit", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$.", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Indem sie niemand sieht, sie also munter macht,", "tokens": ["In\u00b7dem", "sie", "nie\u00b7mand", "sieht", ",", "sie", "al\u00b7so", "mun\u00b7ter", "macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVFIN", "$,", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Da\u00df sie so zu ihr sagt: Ja der Meholathiter", "tokens": ["Da\u00df", "sie", "so", "zu", "ihr", "sagt", ":", "Ja", "der", "Me\u00b7ho\u00b7la\u00b7thi\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$.", "PTKANT", "ART", "NN"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.66": {"text": "Ist meiner auch wohl werth, er ist ein tapfrer Ritter.", "tokens": ["Ist", "mei\u00b7ner", "auch", "wohl", "werth", ",", "er", "ist", "ein", "tapf\u00b7rer", "Rit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADV", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Ich berg mich nicht f\u00fcr dir, Thalmais, du weist wohl", "tokens": ["Ich", "berg", "mich", "nicht", "f\u00fcr", "dir", ",", "Thal\u00b7mais", ",", "du", "weist", "wohl"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "PPER", "$,", "NE", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+--+-+-+++", "measure": "iambic.hexa.relaxed"}, "line.68": {"text": "Wie seine W\u00fcrdigkeit man billig preisen soll.", "tokens": ["Wie", "sei\u00b7ne", "W\u00fcr\u00b7dig\u00b7keit", "man", "bil\u00b7lig", "prei\u00b7sen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PIS", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Ich wei\u00df, Thalmais sprach, was Adriel verdienet,", "tokens": ["Ich", "wei\u00df", ",", "Thal\u00b7mais", "sprach", ",", "was", "Ad\u00b7riel", "ver\u00b7die\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "VVFIN", "$,", "PRELS", "NE", "VVFIN", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.70": {"text": "Mein Jonathan ist l\u00e4ngst mit ihme ausges\u00fchnet;", "tokens": ["Mein", "Jo\u00b7na\u00b7than", "ist", "l\u00e4ngst", "mit", "ih\u00b7me", "aus\u00b7ge\u00b7s\u00fch\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "Dein Bruder ist nicht mehr sein Feind wie wol vorhin,", "tokens": ["Dein", "Bru\u00b7der", "ist", "nicht", "mehr", "sein", "Feind", "wie", "wol", "vor\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADV", "PPOSAT", "NN", "KOKOM", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Die Eifersucht ist aus, nun ich getrauet bin.", "tokens": ["Die", "Ei\u00b7fer\u00b7sucht", "ist", "aus", ",", "nun", "ich", "ge\u00b7trau\u00b7et", "bin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "$,", "ADV", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.73": {"text": "Erzehl uns eh die Nacht sich endt, was sich begeben,", "tokens": ["Er\u00b7zehl", "uns", "eh", "die", "Nacht", "sich", "endt", ",", "was", "sich", "be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KOUS", "ART", "NN", "PRF", "VVFIN", "$,", "PRELS", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.74": {"text": "Mit dir und Adriel. Nach deinem Willn zu leben,", "tokens": ["Mit", "dir", "und", "Ad\u00b7riel", ".", "Nach", "dei\u00b7nem", "Willn", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "NE", "$.", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.75": {"text": "Sagt sie, so will ich gleich dir ein Gen\u00fcgen thun,", "tokens": ["Sagt", "sie", ",", "so", "will", "ich", "gleich", "dir", "ein", "Ge\u00b7n\u00fc\u00b7gen", "thun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VMFIN", "PPER", "ADV", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.76": {"text": "H\u00f6rt beyde flei\u00dfig zu, was ich f\u00fcrbringe nun.", "tokens": ["H\u00f6rt", "bey\u00b7de", "flei\u00b7\u00dfig", "zu", ",", "was", "ich", "f\u00fcr\u00b7brin\u00b7ge", "nun", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "PTKVZ", "$,", "PWS", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}