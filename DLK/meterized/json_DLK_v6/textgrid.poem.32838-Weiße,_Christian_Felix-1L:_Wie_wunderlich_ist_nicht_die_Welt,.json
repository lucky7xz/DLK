{"textgrid.poem.32838": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie wunderlich ist nicht die Welt,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie wunderlich ist nicht die Welt,", "tokens": ["Wie", "wun\u00b7der\u00b7lich", "ist", "nicht", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df sie die Aussicht hier f\u00fcr unvergleichlich h\u00e4lt:", "tokens": ["Da\u00df", "sie", "die", "Aus\u00b7sicht", "hier", "f\u00fcr", "un\u00b7ver\u00b7gleich\u00b7lich", "h\u00e4lt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPR", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hier bl\u00fcht, sagt sie, ein junger Wald,", "tokens": ["Hier", "bl\u00fcht", ",", "sagt", "sie", ",", "ein", "jun\u00b7ger", "Wald", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der von Ges\u00e4ngen wiederschallt.", "tokens": ["Der", "von", "Ge\u00b7s\u00e4n\u00b7gen", "wie\u00b7der\u00b7schallt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dort schwatzt ein buhlerischer Bach", "tokens": ["Dort", "schwatzt", "ein", "buh\u00b7le\u00b7ri\u00b7scher", "Bach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem lispelnden Ger\u00e4usch der nahen Str\u00e4uche nach:", "tokens": ["Dem", "lis\u00b7peln\u00b7den", "Ge\u00b7r\u00e4usch", "der", "na\u00b7hen", "Str\u00e4u\u00b7che", "nach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Hier strahlt die bunte Wiese vor:", "tokens": ["Hier", "strahlt", "die", "bun\u00b7te", "Wie\u00b7se", "vor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dort schwillt ein stolzer Berg empor.", "tokens": ["Dort", "schwillt", "ein", "stol\u00b7zer", "Berg", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da duften Blumen ohne Zahl", "tokens": ["Da", "duf\u00b7ten", "Blu\u00b7men", "oh\u00b7ne", "Zahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Ros im Garten hier, die Veilche dort im Thal,", "tokens": ["Die", "Ros", "im", "Gar\u00b7ten", "hier", ",", "die", "Veil\u00b7che", "dort", "im", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADV", "$,", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hier \u2013 \u2013 doch la\u00dft mich erst Doris sehn,", "tokens": ["Hier", "\u2013", "\u2013", "doch", "la\u00dft", "mich", "erst", "Do\u00b7ris", "sehn", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "$(", "KON", "VVIMP", "PPER", "ADV", "NE", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist ohne Doris etwas sch\u00f6n?", "tokens": ["Ist", "oh\u00b7ne", "Do\u00b7ris", "et\u00b7was", "sch\u00f6n", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NE", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}