{"dta.poem.14997": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "L\u00fctzow's wilde Jagd.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.71", "sv:0.28"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6r's n\u00e4her und n\u00e4her brausen.", "tokens": ["H\u00f6r's", "n\u00e4\u00b7her", "und", "n\u00e4\u00b7her", "brau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Es zieht sich herunter in d\u00fcsteren Reihn,", "tokens": ["Es", "zieht", "sich", "her\u00b7un\u00b7ter", "in", "d\u00fcs\u00b7te\u00b7ren", "Reihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APZR", "APPR", "VVFIN", "NE", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und gellende H\u00f6rner schallen darein,", "tokens": ["Und", "gel\u00b7len\u00b7de", "H\u00f6r\u00b7ner", "schal\u00b7len", "da\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PAV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und erf\u00fcllen die Seele mit Grausen.", "tokens": ["Und", "er\u00b7f\u00fcl\u00b7len", "die", "See\u00b7le", "mit", "Grau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Und wenn ihr die schwarzen Gesellen fragt,", "tokens": ["Und", "wenn", "ihr", "die", "schwar\u00b7zen", "Ge\u00b7sel\u00b7len", "fragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Das ist L\u00fctzow's wilde verwegene Jagd.", "tokens": ["Das", "ist", "L\u00fct\u00b7zow's", "wil\u00b7de", "ver\u00b7we\u00b7ge\u00b7ne", "Jagd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NE", "ADJA", "ADJA", "NN", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Was zieht dort rasch durch den finstern Wald,", "tokens": ["Was", "zieht", "dort", "rasch", "durch", "den", "fins\u00b7tern", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und streift von Bergen zu Bergen?", "tokens": ["Und", "streift", "von", "Ber\u00b7gen", "zu", "Ber\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es legt sich in n\u00e4chtlichen Hinterhalt,", "tokens": ["Es", "legt", "sich", "in", "n\u00e4cht\u00b7li\u00b7chen", "Hin\u00b7ter\u00b7halt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Das Hurrah jauchzt, und die B\u00fcchse knallt,", "tokens": ["Das", "Hur\u00b7rah", "jauchzt", ",", "und", "die", "B\u00fcch\u00b7se", "knallt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Es fallen die fr\u00e4nkischen Schergen.", "tokens": ["Es", "fal\u00b7len", "die", "fr\u00e4n\u00b7ki\u00b7schen", "Scher\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Und wenn ihr die schwarzen J\u00e4ger fragt,", "tokens": ["Und", "wenn", "ihr", "die", "schwar\u00b7zen", "J\u00e4\u00b7ger", "fragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.7": {"text": "Das ist L\u00fctzow's wilde verwegene Jagd.", "tokens": ["Das", "ist", "L\u00fct\u00b7zow's", "wil\u00b7de", "ver\u00b7we\u00b7ge\u00b7ne", "Jagd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NE", "ADJA", "ADJA", "NN", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Wo die Reben dort gl\u00fchen, dort brau\u00dft der Rhein,", "tokens": ["Wo", "die", "Re\u00b7ben", "dort", "gl\u00fc\u00b7hen", ",", "dort", "brau\u00dft", "der", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVINF", "$,", "ADV", "VVFIN", "ART", "NE", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Der W\u00fcthrich geborgen sich meinte,", "tokens": ["Der", "W\u00fcth\u00b7rich", "ge\u00b7bor\u00b7gen", "sich", "mein\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da naht es schnell mit Gewitterschein,", "tokens": ["Da", "naht", "es", "schnell", "mit", "Ge\u00b7wit\u00b7ter\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und wirft sich mit r\u00fcst'gen Armen hinein\u201a", "tokens": ["Und", "wirft", "sich", "mit", "r\u00fcst'\u00b7gen", "Ar\u00b7men", "hin\u00b7ein", "\u201a"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und springt ans Ufer der Feinde,", "tokens": ["Und", "springt", "ans", "U\u00b7fer", "der", "Fein\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und wenn ihr die schwarzen Schwimmer fragt,", "tokens": ["Und", "wenn", "ihr", "die", "schwar\u00b7zen", "Schwim\u00b7mer", "fragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.7": {"text": "Das ist L\u00fctzow's wilde verwegene Jagd.", "tokens": ["Das", "ist", "L\u00fct\u00b7zow's", "wil\u00b7de", "ver\u00b7we\u00b7ge\u00b7ne", "Jagd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NE", "ADJA", "ADJA", "NN", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Was brau\u00dft dort im Thale die laute Schlacht,", "tokens": ["Was", "brau\u00dft", "dort", "im", "Tha\u00b7le", "die", "lau\u00b7te", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Was schlagen die Schwerdter zusammen?", "tokens": ["Was", "schla\u00b7gen", "die", "Schwerd\u00b7ter", "zu\u00b7sam\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wildherzige Reiter schlagen die Schlacht,", "tokens": ["Wild\u00b7her\u00b7zi\u00b7ge", "Rei\u00b7ter", "schla\u00b7gen", "die", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "++--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und der Funke der Freiheit ist gl\u00fchend erwacht,", "tokens": ["Und", "der", "Fun\u00b7ke", "der", "Frei\u00b7heit", "ist", "gl\u00fc\u00b7hend", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Und lodert in blutigen Flammen.", "tokens": ["Und", "lo\u00b7dert", "in", "blu\u00b7ti\u00b7gen", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Und wenn ihr die schwarzen Reiter fragt,", "tokens": ["Und", "wenn", "ihr", "die", "schwar\u00b7zen", "Rei\u00b7ter", "fragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Das ist L\u00fctzow's wilde verwegene Jagd.", "tokens": ["Das", "ist", "L\u00fct\u00b7zow's", "wil\u00b7de", "ver\u00b7we\u00b7ge\u00b7ne", "Jagd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NE", "ADJA", "ADJA", "NN", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Wer scheidet dort r\u00f6chelnd vom Sonnenlicht,", "tokens": ["Wer", "schei\u00b7det", "dort", "r\u00f6\u00b7chelnd", "vom", "Son\u00b7nen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Unter winselnde Feinde gebettet? \u2014", "tokens": ["Un\u00b7ter", "win\u00b7seln\u00b7de", "Fein\u00b7de", "ge\u00b7bet\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Es zuckt der Tod auf dem Angesicht,", "tokens": ["Es", "zuckt", "der", "Tod", "auf", "dem", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch die wackern Herzen erzittern nicht\u201a", "tokens": ["Doch", "die", "wa\u00b7ckern", "Her\u00b7zen", "er\u00b7zit\u00b7tern", "nicht", "\u201a"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Das Vaterland ist ja gerettet!", "tokens": ["Das", "Va\u00b7ter\u00b7land", "ist", "ja", "ge\u00b7ret\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und wenn ihr die schwarzen Gefallnen fragt,", "tokens": ["Und", "wenn", "ihr", "die", "schwar\u00b7zen", "Ge\u00b7fall\u00b7nen", "fragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Das war L\u00fctzo's wilde verwegene Jagd.", "tokens": ["Das", "war", "L\u00fct\u00b7zo's", "wil\u00b7de", "ver\u00b7we\u00b7ge\u00b7ne", "Jagd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NE", "ADJA", "ADJA", "NN", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Die wilde Jagd und die deutsche Jagd", "tokens": ["Die", "wil\u00b7de", "Jagd", "und", "die", "deut\u00b7sche", "Jagd"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf Henkers Blut und Tyrannen.", "tokens": ["Auf", "Hen\u00b7kers", "Blut", "und", "Ty\u00b7ran\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Drum, die ihr uns liebt, nicht geweint und geklagt,", "tokens": ["Drum", ",", "die", "ihr", "uns", "liebt", ",", "nicht", "ge\u00b7weint", "und", "ge\u00b7klagt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,", "PTKNEG", "VVPP", "KON", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Das Land ist ja frei und der Morgen tagt,", "tokens": ["Das", "Land", "ist", "ja", "frei", "und", "der", "Mor\u00b7gen", "tagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Wenn wir's auch nur sterbend gewannen,", "tokens": ["Wenn", "wir's", "auch", "nur", "ster\u00b7bend", "ge\u00b7wan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "ADJD", "ADJA", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Und von Enkeln, zu Enkeln sey's nachgesagt:", "tokens": ["Und", "von", "En\u00b7keln", ",", "zu", "En\u00b7keln", "sey's", "nach\u00b7ge\u00b7sagt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Das war L\u00fctzow's wilde verwegene Jagd.", "tokens": ["Das", "war", "L\u00fct\u00b7zow's", "wil\u00b7de", "ver\u00b7we\u00b7ge\u00b7ne", "Jagd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NE", "ADJA", "ADJA", "NN", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}