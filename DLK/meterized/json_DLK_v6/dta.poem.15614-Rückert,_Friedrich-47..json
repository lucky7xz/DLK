{"dta.poem.15614": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "47.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hast du einmal bedacht, da\u00df du in einer Stunde", "tokens": ["Hast", "du", "ein\u00b7mal", "be\u00b7dacht", ",", "da\u00df", "du", "in", "ei\u00b7ner", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vollkommner Ruhe machst durchs Weltall eine Runde?", "tokens": ["Voll\u00b7komm\u00b7ner", "Ru\u00b7he", "machst", "durchs", "Wel\u00b7tall", "ei\u00b7ne", "Run\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Erde, die dich tr\u00e4gt, tr\u00e4gt um die Sonne dich,", "tokens": ["Die", "Er\u00b7de", ",", "die", "dich", "tr\u00e4gt", ",", "tr\u00e4gt", "um", "die", "Son\u00b7ne", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die selbst auf ihrer Fahrt euch beide nimmt mit sich.", "tokens": ["Die", "selbst", "auf", "ih\u00b7rer", "Fahrt", "euch", "bei\u00b7de", "nimmt", "mit", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "PPER", "PIS", "VVFIN", "APPR", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das sch\u00f6nst' an dieser Fahrt ist, da\u00df du sie nicht sp\u00fcrest,", "tokens": ["Das", "sch\u00f6nst'", "an", "die\u00b7ser", "Fahrt", "ist", ",", "da\u00df", "du", "sie", "nicht", "sp\u00fc\u00b7rest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PDAT", "NN", "VAFIN", "$,", "KOUS", "PPER", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil du die s\u00e4mmtliche Umgebung mit dir f\u00fchrest.", "tokens": ["Weil", "du", "die", "s\u00e4mmt\u00b7li\u00b7che", "Um\u00b7ge\u00b7bung", "mit", "dir", "f\u00fch\u00b7rest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Bequemer ist die Reis' und bringt dich doch viel weiter,", "tokens": ["Be\u00b7que\u00b7mer", "ist", "die", "Reis'", "und", "bringt", "dich", "doch", "viel", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "KON", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als die, zu der du dir erst suchen mu\u00dft Begleiter,", "tokens": ["Als", "die", ",", "zu", "der", "du", "dir", "erst", "su\u00b7chen", "mu\u00dft", "Be\u00b7glei\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "APPR", "PRELS", "PPER", "PPER", "ADV", "VVINF", "VMFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wo du auf jedem Schritt bist au\u00dfer dich gesetzt,", "tokens": ["Wo", "du", "auf", "je\u00b7dem", "Schritt", "bist", "au\u00b7\u00dfer", "dich", "ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PIAT", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und herzlich m\u00fcde nur kommst wieder heim zuletzt.", "tokens": ["Und", "herz\u00b7lich", "m\u00fc\u00b7de", "nur", "kommst", "wie\u00b7der", "heim", "zu\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "ADV", "VVFIN", "ADV", "PTKVZ", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ich will die Reiselust dir nicht auf Erden schm\u00e4lern,", "tokens": ["Ich", "will", "die", "Rei\u00b7se\u00b7lust", "dir", "nicht", "auf", "Er\u00b7den", "schm\u00e4\u00b7lern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPER", "PTKNEG", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du dich noch nicht satt an Bergen sahst und Th\u00e4lern.", "tokens": ["Wenn", "du", "dich", "noch", "nicht", "satt", "an", "Ber\u00b7gen", "sahst", "und", "Th\u00e4\u00b7lern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PTKNEG", "ADJD", "APPR", "NN", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Doch mir vergieng die Lust an Erdenreisen gr\u00fcndlich,", "tokens": ["Doch", "mir", "ver\u00b7gieng", "die", "Lust", "an", "Er\u00b7den\u00b7rei\u00b7sen", "gr\u00fcnd\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Seitdem ich f\u00fchle da\u00df ich reis' im Himmel st\u00fcndlich.", "tokens": ["Seit\u00b7dem", "ich", "f\u00fch\u00b7le", "da\u00df", "ich", "reis'", "im", "Him\u00b7mel", "st\u00fcnd\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVFIN", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}