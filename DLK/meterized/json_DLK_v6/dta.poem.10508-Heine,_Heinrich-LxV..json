{"dta.poem.10508": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LxV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Diesen liebensw\u00fcrd'gen J\u00fcngling", "tokens": ["Die\u00b7sen", "lie\u00b7bens\u00b7w\u00fcrd'\u00b7gen", "J\u00fcng\u00b7ling"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kann man nicht genug verehren;", "tokens": ["Kann", "man", "nicht", "ge\u00b7nug", "ver\u00b7eh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Oft traktirt er mich mit Austern,", "tokens": ["Oft", "trak\u00b7tirt", "er", "mich", "mit", "Aus\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Und mit Rheinwein und Liqu\u00f6ren.", "tokens": ["Und", "mit", "Rhein\u00b7wein", "und", "Li\u00b7qu\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "KON", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Zierlich sitzt ihm Rock und H\u00f6schen,", "tokens": ["Zier\u00b7lich", "sitzt", "ihm", "Rock", "und", "H\u00f6\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch noch zierlicher die Binde,", "tokens": ["Doch", "noch", "zier\u00b7li\u00b7cher", "die", "Bin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und so kommt er jeden Morgen;", "tokens": ["Und", "so", "kommt", "er", "je\u00b7den", "Mor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fragt, ob ich mich wohlbefinde;", "tokens": ["Fragt", ",", "ob", "ich", "mich", "wohl\u00b7be\u00b7fin\u00b7de", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PRF", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Spricht von meinem weiten Ruhme,", "tokens": ["Spricht", "von", "mei\u00b7nem", "wei\u00b7ten", "Ruh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner Anmuth, meinen Witzen;", "tokens": ["Mei\u00b7ner", "An\u00b7muth", ",", "mei\u00b7nen", "Wit\u00b7zen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eifrig und gesch\u00e4ftig ist er", "tokens": ["Eif\u00b7rig", "und", "ge\u00b7sch\u00e4f\u00b7tig", "ist", "er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir zu dienen, mir zu n\u00fctzen.", "tokens": ["Mir", "zu", "die\u00b7nen", ",", "mir", "zu", "n\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und des Abends, in Gesellschaft,", "tokens": ["Und", "des", "A\u00b7bends", ",", "in", "Ge\u00b7sell\u00b7schaft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit begeistertem Gesichte,", "tokens": ["Mit", "be\u00b7geis\u00b7ter\u00b7tem", "Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deklamirt er vor den Damen", "tokens": ["De\u00b7kla\u00b7mirt", "er", "vor", "den", "Da\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine g\u00f6ttlichen Gedichte.", "tokens": ["Mei\u00b7ne", "g\u00f6tt\u00b7li\u00b7chen", "Ge\u00b7dich\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "O, wie ist es hoch erfreulich,", "tokens": ["O", ",", "wie", "ist", "es", "hoch", "er\u00b7freu\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Solchen J\u00fcngling noch zu finden,", "tokens": ["Sol\u00b7chen", "J\u00fcng\u00b7ling", "noch", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jetzt in unsrer Zeit, wo t\u00e4glich", "tokens": ["Jetzt", "in", "uns\u00b7rer", "Zeit", ",", "wo", "t\u00e4g\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,", "PWAV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mehr und mehr die Bessern schwinden.", "tokens": ["Mehr", "und", "mehr", "die", "Bes\u00b7sern", "schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KON", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}