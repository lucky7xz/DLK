{"textgrid.poem.26730": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die zwei Tugendwege", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei sind der Wege, auf welchen der Mensch zur Tugend emporstrebt,", "tokens": ["Zwei", "sind", "der", "We\u00b7ge", ",", "auf", "wel\u00b7chen", "der", "Mensch", "zur", "Tu\u00b7gend", "em\u00b7por\u00b7strebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VAFIN", "ART", "NN", "$,", "APPR", "PWAT", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+-+---+-+--+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Schlie\u00dft sich der eine dir zu, tut sich der andre dir auf.", "tokens": ["Schlie\u00dft", "sich", "der", "ei\u00b7ne", "dir", "zu", ",", "tut", "sich", "der", "and\u00b7re", "dir", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ART", "PPER", "PTKVZ", "$,", "VVFIN", "PRF", "ART", "ADJA", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Handelnd erringt der Gl\u00fcckliche sie, der Leidende duldend.", "tokens": ["Han\u00b7delnd", "er\u00b7ringt", "der", "Gl\u00fcck\u00b7li\u00b7che", "sie", ",", "der", "Lei\u00b7den\u00b7de", "dul\u00b7dend", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PPER", "$,", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-++-+--+-", "measure": "iambic.septa.invert"}, "line.4": {"text": "Wohl ihm, den sein Geschick liebend auf beiden gef\u00fchrt.", "tokens": ["Wohl", "ihm", ",", "den", "sein", "Ge\u00b7schick", "lie\u00b7bend", "auf", "bei\u00b7den", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PRELS", "PPOSAT", "NN", "ADJD", "APPR", "PIAT", "VVPP", "$."], "meter": "+-+---+-++--+", "measure": "iambic.hexa.chol"}}}}}