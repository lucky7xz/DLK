{"textgrid.poem.63716": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: In Gedanken an die Ferne", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Gedanken an die Ferne", "tokens": ["In", "Ge\u00b7dan\u00b7ken", "an", "die", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der N\u00e4he wenig froh,", "tokens": ["Und", "der", "N\u00e4\u00b7he", "we\u00b7nig", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Senkt man wohl die Augen gerne,", "tokens": ["Senkt", "man", "wohl", "die", "Au\u00b7gen", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und auch heut geschah mir so.", "tokens": ["Und", "auch", "heut", "ge\u00b7schah", "mir", "so", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da in weichen L\u00fcften schwanken", "tokens": ["Da", "in", "wei\u00b7chen", "L\u00fcf\u00b7ten", "schwan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sah ich einen Schmetterling,", "tokens": ["Sah", "ich", "ei\u00b7nen", "Schmet\u00b7ter\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sein Schatten auf dem blanken", "tokens": ["Da\u00df", "sein", "Schat\u00b7ten", "auf", "dem", "blan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gartenweg spazieren ging.", "tokens": ["Gar\u00b7ten\u00b7weg", "spa\u00b7zie\u00b7ren", "ging", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hell in Sonne lag das G\u00e4rtchen,", "tokens": ["Hell", "in", "Son\u00b7ne", "lag", "das", "G\u00e4rt\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die durch zarte Zweige brach,", "tokens": ["Die", "durch", "zar\u00b7te", "Zwei\u00b7ge", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein t\u00f6richtes Lazertchen", "tokens": ["Und", "ein", "t\u00f6\u00b7rich\u00b7tes", "La\u00b7zert\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Lief dem Falterschatten nach.", "tokens": ["Lief", "dem", "Fal\u00b7ter\u00b7schat\u00b7ten", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dacht' ihn jetzt der Wicht zu haschen,", "tokens": ["Dacht'", "ihn", "jetzt", "der", "Wicht", "zu", "ha\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War er wieder weit voraus,", "tokens": ["War", "er", "wie\u00b7der", "weit", "vo\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und fast ging ihm bei der raschen", "tokens": ["Und", "fast", "ging", "ihm", "bei", "der", "ra\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Jagd Geduld und Atem aus.", "tokens": ["Jagd", "Ge\u00b7duld", "und", "A\u00b7tem", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Zwischen Lachen und Erbauung", "tokens": ["Zwi\u00b7schen", "La\u00b7chen", "und", "Er\u00b7bau\u00b7ung"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sah ich zu dem holden Trug", "tokens": ["Sah", "ich", "zu", "dem", "hol\u00b7den", "Trug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Idealer Weltanschauung,", "tokens": ["I\u00b7dea\u00b7ler", "Welt\u00b7an\u00b7schau\u00b7ung", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch \u2013 wer wird durch Schaden klug!", "tokens": ["Doch", "\u2013", "wer", "wird", "durch", "Scha\u00b7den", "klug", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PWS", "VAFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}