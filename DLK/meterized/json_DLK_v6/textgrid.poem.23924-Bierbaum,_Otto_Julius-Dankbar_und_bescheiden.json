{"textgrid.poem.23924": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Dankbar und bescheiden", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u2013: \u00bbIch hab dich lieb\u00ab ...", "tokens": ["\u2013", ":", "\u00bb", "Ich", "hab", "dich", "lieb", "\u00ab", "..."], "token_info": ["punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "$.", "$(", "PPER", "VAFIN", "PPER", "ADJD", "$(", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ich h\u00f6r das so.", "tokens": ["Ich", "h\u00f6r", "das", "so", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "K\u00f6nnt ich es glauben,", "tokens": ["K\u00f6nnt", "ich", "es", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "W\u00e4r ich wohl froh.", "tokens": ["W\u00e4r", "ich", "wohl", "froh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "\u2013: \u00bbIch hab dich lieb\u00ab ...", "tokens": ["\u2013", ":", "\u00bb", "Ich", "hab", "dich", "lieb", "\u00ab", "..."], "token_info": ["punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "$.", "$(", "PPER", "VAFIN", "PPER", "ADJD", "$(", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Welch holder Ton!", "tokens": ["Welch", "hol\u00b7der", "Ton", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wie Geig und Fl\u00f6te ...", "tokens": ["Wie", "Geig", "und", "Fl\u00f6\u00b7te", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ich h\u00f6rte ihn schon.", "tokens": ["Ich", "h\u00f6r\u00b7te", "ihn", "schon", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "\u2013: \u00bbIch hab dich lieb\u00ab ...", "tokens": ["\u2013", ":", "\u00bb", "Ich", "hab", "dich", "lieb", "\u00ab", "..."], "token_info": ["punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "$.", "$(", "PPER", "VAFIN", "PPER", "ADJD", "$(", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Sags immer, Kind!", "tokens": ["Sags", "im\u00b7mer", ",", "Kind", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "$,", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ich wei\u00df, da\u00df L\u00fcgen", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "L\u00fc\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Geschenke sind.", "tokens": ["Ge\u00b7schen\u00b7ke", "sind", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}