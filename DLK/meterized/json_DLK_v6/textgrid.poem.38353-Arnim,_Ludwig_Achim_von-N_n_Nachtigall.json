{"textgrid.poem.38353": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "N n Nachtigall", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Nachtigall dein edler Schall, bringt uns sehr gro\u00dfe Freud,", "tokens": ["O", "Nach\u00b7ti\u00b7gall", "dein", "ed\u00b7ler", "Schall", ",", "bringt", "uns", "sehr", "gro\u00b7\u00dfe", "Freud", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Dein Stimm durchstreift all Berg und Thal, zur sch\u00f6nen Sommerzeit,", "tokens": ["Dein", "Stimm", "durch\u00b7streift", "all", "Berg", "und", "Thal", ",", "zur", "sch\u00f6\u00b7nen", "Som\u00b7mer\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "NN", "KON", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Wenn du f\u00e4ngst an zu z\u00fccken, die V\u00f6glein schweigen still,", "tokens": ["Wenn", "du", "f\u00e4ngst", "an", "zu", "z\u00fc\u00b7cken", ",", "die", "V\u00f6\u00b7glein", "schwei\u00b7gen", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Es l\u00e4\u00dft sich keiner blicken, keiner mehr singen will.", "tokens": ["Es", "l\u00e4\u00dft", "sich", "kei\u00b7ner", "bli\u00b7cken", ",", "kei\u00b7ner", "mehr", "sin\u00b7gen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIS", "VVINF", "$,", "PIS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}