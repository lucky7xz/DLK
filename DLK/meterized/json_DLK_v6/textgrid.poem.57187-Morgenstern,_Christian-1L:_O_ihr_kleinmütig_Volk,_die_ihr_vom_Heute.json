{"textgrid.poem.57187": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: O ihr kleinm\u00fctig Volk, die ihr vom Heute", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O ihr kleinm\u00fctig Volk, die ihr vom Heute", "tokens": ["O", "ihr", "klein\u00b7m\u00fc\u00b7tig", "Volk", ",", "die", "ihr", "vom", "Heu\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "ADJD", "NN", "$,", "PRELS", "PPER", "APPRART", "NN"], "meter": "+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "nicht loskommt, die ihr meint: so ist es, war es", "tokens": ["nicht", "los\u00b7kommt", ",", "die", "ihr", "meint", ":", "so", "ist", "es", ",", "war", "es"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$.", "ADV", "VAFIN", "PPER", "$,", "VAFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und wird es sein, so lange Menschen leben \u2013.", "tokens": ["und", "wird", "es", "sein", ",", "so", "lan\u00b7ge", "Men\u00b7schen", "le\u00b7ben", "\u2013", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "PPER", "VAINF", "$,", "ADV", "ADJA", "NN", "VVINF", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O w\u00fcrdet ihr doch andrer Hoffnung Beute", "tokens": ["O", "w\u00fcr\u00b7det", "ihr", "doch", "an\u00b7drer", "Hoff\u00b7nung", "Beu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "ADV", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und lerntet wieder schauen Offenbares", "tokens": ["und", "lern\u00b7tet", "wie\u00b7der", "schau\u00b7en", "Of\u00b7fen\u00b7ba\u00b7res"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und Hirn und Herz zu h\u00f6chstem Ziel erheben!", "tokens": ["und", "Hirn", "und", "Herz", "zu", "h\u00f6chs\u00b7tem", "Ziel", "er\u00b7he\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}