{"textgrid.poem.45874": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[wenn die Sonne weggegangen]", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn die Sonne weggegangen,", "tokens": ["Wenn", "die", "Son\u00b7ne", "weg\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00f6mmt die Dunkelheit heran,", "tokens": ["K\u00f6mmt", "die", "Dun\u00b7kel\u00b7heit", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Abendrot hat goldne Wangen,", "tokens": ["A\u00b7ben\u00b7drot", "hat", "gold\u00b7ne", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Nacht hat Trauer an.", "tokens": ["Und", "die", "Nacht", "hat", "Trau\u00b7er", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "NN", "PTKVZ", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Seit die Liebe weggegangen,", "tokens": ["Seit", "die", "Lie\u00b7be", "weg\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bin ich nun ein Mohrenkind,", "tokens": ["Bin", "ich", "nun", "ein", "Moh\u00b7ren\u00b7kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die roten, frohen Wangen,", "tokens": ["Und", "die", "ro\u00b7ten", ",", "fro\u00b7hen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dunkel und verloren sind.", "tokens": ["Dun\u00b7kel", "und", "ver\u00b7lo\u00b7ren", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dunkelheit mu\u00df tief verschweigen,", "tokens": ["Dun\u00b7kel\u00b7heit", "mu\u00df", "tief", "ver\u00b7schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles Wehe, alle Lust,", "tokens": ["Al\u00b7les", "We\u00b7he", ",", "al\u00b7le", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber Mond und Sterne zeigen,", "tokens": ["A\u00b7ber", "Mond", "und", "Ster\u00b7ne", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was ihr wohnet in der Brust.", "tokens": ["Was", "ihr", "woh\u00b7net", "in", "der", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn die Lippen dir verschweigen", "tokens": ["Wenn", "die", "Lip\u00b7pen", "dir", "ver\u00b7schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meines Herzens stille Glut,", "tokens": ["Mei\u00b7nes", "Her\u00b7zens", "stil\u00b7le", "Glut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00fcssen Blick und Tr\u00e4nen zeigen,", "tokens": ["M\u00fcs\u00b7sen", "Blick", "und", "Tr\u00e4\u00b7nen", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie die Liebe nimmer ruht.", "tokens": ["Wie", "die", "Lie\u00b7be", "nim\u00b7mer", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}