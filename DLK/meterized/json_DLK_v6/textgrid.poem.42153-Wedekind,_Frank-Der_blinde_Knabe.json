{"textgrid.poem.42153": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Der blinde Knabe", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O ihr Tage meiner Kindheit,", "tokens": ["O", "ihr", "Ta\u00b7ge", "mei\u00b7ner", "Kind\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun dahin auf immerdar,", "tokens": ["Nun", "da\u00b7hin", "auf", "im\u00b7mer\u00b7dar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "APPR", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da die ", "tokens": ["Da", "die"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Noch voll Licht das ", "tokens": ["Noch", "voll", "Licht", "das"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "NN", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Meine Blicke lie\u00df ich schweifen", "tokens": ["Mei\u00b7ne", "Bli\u00b7cke", "lie\u00df", "ich", "schwei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jedem frei ins Angesicht;", "tokens": ["Je\u00b7dem", "frei", "ins", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Glauben galt mir f\u00fcr Begreifen", "tokens": ["Glau\u00b7ben", "galt", "mir", "f\u00fcr", "Be\u00b7grei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und Gedanken kannt ich nicht.", "tokens": ["Und", "Ge\u00b7dan\u00b7ken", "kannt", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich begann jedoch zu sinnen", "tokens": ["Ich", "be\u00b7gann", "je\u00b7doch", "zu", "sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und zu gr\u00fcbeln hin und her,", "tokens": ["Und", "zu", "gr\u00fc\u00b7beln", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in meiner Seele drinnen", "tokens": ["Und", "in", "mei\u00b7ner", "See\u00b7le", "drin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwoll ein wildemp\u00f6rtes Meer.", "tokens": ["Schwoll", "ein", "wil\u00b7dem\u00b7p\u00f6r\u00b7tes", "Meer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Meine Blicke senkt ich nieder,", "tokens": ["Mei\u00b7ne", "Bli\u00b7cke", "senkt", "ich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schaute tief in mich hinein", "tokens": ["Schau\u00b7te", "tief", "in", "mich", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPR", "PRF", "APZR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und erhob sie nimmer wieder", "tokens": ["Und", "er\u00b7hob", "sie", "nim\u00b7mer", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zu dem goldnen Sonnenschein.", "tokens": ["Zu", "dem", "gold\u00b7nen", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mu\u00dft ich doch die Welt verachten,", "tokens": ["Mu\u00dft", "ich", "doch", "die", "Welt", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mir Gottes Garten schien,", "tokens": ["Die", "mir", "Got\u00b7tes", "Gar\u00b7ten", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn die Guten l\u00e4\u00dft er schmachten,", "tokens": ["Denn", "die", "Gu\u00b7ten", "l\u00e4\u00dft", "er", "schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die B\u00f6sen preisen ihn.", "tokens": ["Und", "die", "B\u00f6\u00b7sen", "prei\u00b7sen", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Freude, Lust und Ruh vergehen \u2013", "tokens": ["Freu\u00b7de", ",", "Lust", "und", "Ruh", "ver\u00b7ge\u00b7hen", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Oh, wie wohl war einst dem Kind!", "tokens": ["Oh", ",", "wie", "wohl", "war", "einst", "dem", "Kind", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADV", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Meine ", "tokens": ["Mei\u00b7ne"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Meine ", "tokens": ["Mei\u00b7ne"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+-", "measure": "trochaic.single"}}}}}