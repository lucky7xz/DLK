{"textgrid.poem.44881": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Krakau", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Korsen Lehren nahmt ihr wohl in acht,", "tokens": ["Des", "Kor\u00b7sen", "Leh\u00b7ren", "nahmt", "ihr", "wohl", "in", "acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "ADV", "APPR", "CARD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die echten K\u00f6nige bis nun vermiedens,", "tokens": ["Die", "ech\u00b7ten", "K\u00f6\u00b7ni\u00b7ge", "bis", "nun", "ver\u00b7mie\u00b7dens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Recht zu messen nach des Gegners Macht,", "tokens": ["Das", "Recht", "zu", "mes\u00b7sen", "nach", "des", "Geg\u00b7ners", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ihr seid die Napoleon des Friedens.", "tokens": ["Und", "ihr", "seid", "die", "Na\u00b7po\u00b7le\u00b7on", "des", "Frie\u00b7dens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NE", "ART", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}}}}