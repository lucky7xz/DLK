{"dta.poem.15602": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "35.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So sprach der Filosof: Gebt Stoff mir und Bewegung;", "tokens": ["So", "sprach", "der", "Fi\u00b7lo\u00b7sof", ":", "Gebt", "Stoff", "mir", "und", "Be\u00b7we\u00b7gung", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "VVIMP", "NN", "PPER", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Genug ist beides mir zu einer Welt Anlegung.", "tokens": ["Ge\u00b7nug", "ist", "bei\u00b7des", "mir", "zu", "ei\u00b7ner", "Welt", "An\u00b7le\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Stoff und Bewegung ist gegeben, nimm sie nur!", "tokens": ["Stoff", "und", "Be\u00b7we\u00b7gung", "ist", "ge\u00b7ge\u00b7ben", ",", "nimm", "sie", "nur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "VVPP", "$,", "VVIMP", "PPER", "ADV", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Was hast du angelegt? ach eine gro\u00dfe Uhr.", "tokens": ["Was", "hast", "du", "an\u00b7ge\u00b7legt", "?", "ach", "ei\u00b7ne", "gro\u00b7\u00dfe", "Uhr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VVPP", "$.", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und sei es eine Uhr mit stets gespannter Feder,", "tokens": ["Und", "sei", "es", "ei\u00b7ne", "Uhr", "mit", "stets", "ge\u00b7spann\u00b7ter", "Fe\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An der auch nie im Lauf sich laufen ab die R\u00e4der,", "tokens": ["An", "der", "auch", "nie", "im", "Lauf", "sich", "lau\u00b7fen", "ab", "die", "R\u00e4\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADV", "APPRART", "NN", "PRF", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und sei es eine Uhr, die selbst, indem sie geht,", "tokens": ["Und", "sei", "es", "ei\u00b7ne", "Uhr", ",", "die", "selbst", ",", "in\u00b7dem", "sie", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich aufzieht, richtet ein, und auf sich selber steht;", "tokens": ["Sich", "auf\u00b7zieht", ",", "rich\u00b7tet", "ein", ",", "und", "auf", "sich", "sel\u00b7ber", "steht", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "VVFIN", "PTKVZ", "$,", "KON", "APPR", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "An der mit Fl\u00f6tenton beim Stundenschlag hervor", "tokens": ["An", "der", "mit", "Fl\u00f6\u00b7ten\u00b7ton", "beim", "Stun\u00b7den\u00b7schlag", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "NN", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Tritt bunter Bildertanz, und wieder ab im Chor:", "tokens": ["Tritt", "bun\u00b7ter", "Bil\u00b7der\u00b7tanz", ",", "und", "wie\u00b7der", "ab", "im", "Chor", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "$,", "KON", "ADV", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "So f\u00fchl' ich selber doch kein Bild mich, keine Glocke;", "tokens": ["So", "f\u00fchl'", "ich", "sel\u00b7ber", "doch", "kein", "Bild", "mich", ",", "kei\u00b7ne", "Glo\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PIAT", "NN", "PPER", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was verschl\u00fcg' es mir, ob dieses Schlagwerk stocke?", "tokens": ["Und", "was", "ver\u00b7schl\u00fcg'", "es", "mir", ",", "ob", "die\u00b7ses", "Schlag\u00b7werk", "sto\u00b7cke", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "PPER", "$,", "KOUS", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ich f\u00fchle mich kein Rad im blinden Radgetriebe,", "tokens": ["Ich", "f\u00fch\u00b7le", "mich", "kein", "Rad", "im", "blin\u00b7den", "Rad\u00b7ge\u00b7trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und unterbringen kann ich nirgends meine Liebe.", "tokens": ["Und", "un\u00b7ter\u00b7brin\u00b7gen", "kann", "ich", "nir\u00b7gends", "mei\u00b7ne", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "So hat der Filosof mich und sich selbst vergessen,", "tokens": ["So", "hat", "der", "Fi\u00b7lo\u00b7sof", "mich", "und", "sich", "selbst", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "KON", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als nach Bewegung er und Stoff die Welt gemessen.", "tokens": ["Als", "nach", "Be\u00b7we\u00b7gung", "er", "und", "Stoff", "die", "Welt", "ge\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PPER", "KON", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Die Unruh fehlt der Uhr, die in mir selbst nie stille", "tokens": ["Die", "Un\u00b7ruh", "fehlt", "der", "Uhr", ",", "die", "in", "mir", "selbst", "nie", "stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch in der Sch\u00f6pfung steht, der ew'ge Sch\u00f6pferwille.", "tokens": ["Noch", "in", "der", "Sch\u00f6p\u00b7fung", "steht", ",", "der", "ew'\u00b7ge", "Sch\u00f6p\u00b7fer\u00b7wil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}