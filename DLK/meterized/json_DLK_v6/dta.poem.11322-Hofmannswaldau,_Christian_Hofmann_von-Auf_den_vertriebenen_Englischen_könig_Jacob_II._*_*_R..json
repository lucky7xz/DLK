{"dta.poem.11322": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf den vertriebenen Englischen  \n k\u00f6nig Jacob  II.  \n * *  R.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du siehst des vatern bild hier in dem todten sohne,", "tokens": ["Du", "siehst", "des", "va\u00b7tern", "bild", "hier", "in", "dem", "tod\u00b7ten", "soh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur ist ein eintzger strich, der uns un\u00e4hnlich macht.", "tokens": ["Nur", "ist", "ein", "eintz\u00b7ger", "strich", ",", "der", "uns", "un\u00b7\u00e4hn\u00b7lich", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "ADJD", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Urtheile, wo man ihn am schlimmsten angebracht:", "tokens": ["Ur\u00b7thei\u00b7le", ",", "wo", "man", "ihn", "am", "schlimms\u00b7ten", "an\u00b7ge\u00b7bracht", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PIS", "PPER", "APPRART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jhm sieht man\u2019s an dem hals, und mir nur an der erone.", "tokens": ["Jhm", "sieht", "man's", "an", "dem", "hals", ",", "und", "mir", "nur", "an", "der", "e\u00b7ro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word