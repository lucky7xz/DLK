{"textgrid.poem.63283": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nummer 1 tr\u00e4gt eine Radfahrerm\u00fctze.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nummer 1 tr\u00e4gt eine Radfahrerm\u00fctze.", "tokens": ["Num\u00b7mer", "1", "tr\u00e4gt", "ei\u00b7ne", "Rad\u00b7fah\u00b7rer\u00b7m\u00fct\u00b7ze", "."], "token_info": ["word", "number", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Nummer 2 hat die Kr\u00e4tze.", "tokens": ["Num\u00b7mer", "2", "hat", "die", "Kr\u00e4t\u00b7ze", "."], "token_info": ["word", "number", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nummer 3 erinnert sich an seine dritte Braut.", "tokens": ["Num\u00b7mer", "3", "e\u00b7rin\u00b7nert", "sich", "an", "sei\u00b7ne", "drit\u00b7te", "Braut", "."], "token_info": ["word", "number", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+----+-+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Nummer 4 weint.", "tokens": ["Num\u00b7mer", "4", "weint", "."], "token_info": ["word", "number", "word", "punct"], "pos": ["ADV", "CARD", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Der W\u00e4rter rasselt mit den Schl\u00fcsseln.", "tokens": ["Der", "W\u00e4r\u00b7ter", "ras\u00b7selt", "mit", "den", "Schl\u00fcs\u00b7seln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u00d6ffnet keine Pforte.", "tokens": ["\u00d6ff\u00b7net", "kei\u00b7ne", "Pfor\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Oben im ovalen Fenster", "tokens": ["O\u00b7ben", "im", "o\u00b7va\u00b7len", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4ngt der Himmel wie eine Scheibe Brot.", "tokens": ["H\u00e4ngt", "der", "Him\u00b7mel", "wie", "ei\u00b7ne", "Schei\u00b7be", "Brot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KOKOM", "ART", "NN", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Hunger Hunger nach dem Himmel", "tokens": ["Hun\u00b7ger", "Hun\u00b7ger", "nach", "dem", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hunger Hunger nach der Scheibe", "tokens": ["Hun\u00b7ger", "Hun\u00b7ger", "nach", "der", "Schei\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Brot und nach der Sonnenscheibe.", "tokens": ["Brot", "und", "nach", "der", "Son\u00b7nen\u00b7schei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nach dem Schreiten", "tokens": ["Nach", "dem", "Schrei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "weiten Schreiten", "tokens": ["wei\u00b7ten", "Schrei\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "in die Weiten.", "tokens": ["in", "die", "Wei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Hunger Hunger nach dem kleinsten L\u00e4cheln", "tokens": ["Hun\u00b7ger", "Hun\u00b7ger", "nach", "dem", "kleins\u00b7ten", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Einer verfallenen Frau.", "tokens": ["Ei\u00b7ner", "ver\u00b7fal\u00b7le\u00b7nen", "Frau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Hier ist kein Ausgang", "tokens": ["Hier", "ist", "kein", "Aus\u00b7gang"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "NN"], "meter": "++-+-", "measure": "iambic.di"}, "line.2": {"text": "und kein Ende.", "tokens": ["und", "kein", "En\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.3": {"text": "Paragraphen", "tokens": ["Pa\u00b7ra\u00b7gra\u00b7phen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Trafen", "tokens": ["Tra\u00b7fen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "T\u00f6dlich.", "tokens": ["T\u00f6d\u00b7lich", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Vor den Augen saust es r\u00f6tlich.", "tokens": ["Vor", "den", "Au\u00b7gen", "saust", "es", "r\u00f6t\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "An den Mauern trommeln St\u00fcmpfe ohne H\u00e4nde.", "tokens": ["An", "den", "Mau\u00b7ern", "trom\u00b7meln", "St\u00fcmp\u00b7fe", "oh\u00b7ne", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}