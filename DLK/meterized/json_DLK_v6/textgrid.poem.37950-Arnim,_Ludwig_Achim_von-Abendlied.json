{"textgrid.poem.37950": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Abendlied", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun la\u00dft uns singen das Abendlied,", "tokens": ["Nun", "la\u00dft", "uns", "sin\u00b7gen", "das", "A\u00b7ben\u00b7dlied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Denn wir m\u00fcssen gehn,", "tokens": ["Denn", "wir", "m\u00fcs\u00b7sen", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Das K\u00e4nnchen mit dem Weine,", "tokens": ["Das", "K\u00e4nn\u00b7chen", "mit", "dem", "Wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Lassen wir nun stehn.", "tokens": ["Las\u00b7sen", "wir", "nun", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Das K\u00e4nnchen mit dem Weine,", "tokens": ["Das", "K\u00e4nn\u00b7chen", "mit", "dem", "Wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das mu\u00df geleeret seyn,", "tokens": ["Das", "mu\u00df", "ge\u00b7lee\u00b7ret", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Also mu\u00df auch das Abendlied", "tokens": ["Al\u00b7so", "mu\u00df", "auch", "das", "A\u00b7ben\u00b7dlied"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "ART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wohl fein gesungen seyn.", "tokens": ["Wohl", "fein", "ge\u00b7sun\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wohl unterm gr\u00fcnen Tannenbaum,", "tokens": ["Wohl", "un\u00b7term", "gr\u00fc\u00b7nen", "Tan\u00b7nen\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Allda ich fr\u00f6hlich lag,", "tokens": ["All\u00b7da", "ich", "fr\u00f6h\u00b7lich", "lag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In mein feins Liebchens Armen", "tokens": ["In", "mein", "feins", "Lieb\u00b7chens", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die lange liebe Nacht.", "tokens": ["Die", "lan\u00b7ge", "lie\u00b7be", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Bl\u00e4tter von den B\u00e4umen", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "von", "den", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die fallen nun auf mich,", "tokens": ["Die", "fal\u00b7len", "nun", "auf", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df mich mein Schatz verlassen hat,", "tokens": ["Da\u00df", "mich", "mein", "Schatz", "ver\u00b7las\u00b7sen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das freuet wohl mich.", "tokens": ["Das", "freu\u00b7et", "wohl", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Da\u00df mich mein Schatz verlassen hat,", "tokens": ["Da\u00df", "mich", "mein", "Schatz", "ver\u00b7las\u00b7sen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das k\u00f6mmt wohl daher,", "tokens": ["Das", "k\u00f6mmt", "wohl", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PAV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Sie dacht sich zu verbessern,", "tokens": ["Sie", "dacht", "sich", "zu", "ver\u00b7bes\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Betrog sich gar sehr.", "tokens": ["Be\u00b7trog", "sich", "gar", "sehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Des Abends, wenn es dunkel wird,", "tokens": ["Des", "A\u00b7bends", ",", "wenn", "es", "dun\u00b7kel", "wird", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Steht er wohl vor der Th\u00fcr,", "tokens": ["Steht", "er", "wohl", "vor", "der", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Mit seinem blanken Schwerdte,", "tokens": ["Mit", "sei\u00b7nem", "blan\u00b7ken", "Schwerd\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als wie ein Offizier.", "tokens": ["Als", "wie", "ein", "Of\u00b7fi\u00b7zier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Mit seinem blanken Schwerdte,", "tokens": ["Mit", "sei\u00b7nem", "blan\u00b7ken", "Schwerd\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gleich einem rechten Held,", "tokens": ["Gleich", "ei\u00b7nem", "rech\u00b7ten", "Held", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit ihm will ich es wagen,", "tokens": ["Mit", "ihm", "will", "ich", "es", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ins weite, weite Feld.", "tokens": ["Ins", "wei\u00b7te", ",", "wei\u00b7te", "Feld", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Mit ihm will ich es wagen,", "tokens": ["Mit", "ihm", "will", "ich", "es", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Zu Wasser und zu Land,", "tokens": ["Zu", "Was\u00b7ser", "und", "zu", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df mich mein Schatz verlassen hat,", "tokens": ["Da\u00df", "mich", "mein", "Schatz", "ver\u00b7las\u00b7sen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das bringt mir keine Schand.", "tokens": ["Das", "bringt", "mir", "kei\u00b7ne", "Schand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Das Abendlied gesungen ist,", "tokens": ["Das", "A\u00b7ben\u00b7dlied", "ge\u00b7sun\u00b7gen", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das K\u00e4nnchen ist geleert,", "tokens": ["Das", "K\u00e4nn\u00b7chen", "ist", "ge\u00b7leert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df sehn nun wie du Kerl aussiehst,", "tokens": ["La\u00df", "sehn", "nun", "wie", "du", "Kerl", "aus\u00b7siehst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "ADV", "KOKOM", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit deinem blanken Schwerdt.", "tokens": ["Mit", "dei\u00b7nem", "blan\u00b7ken", "Schwerdt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}