{"textgrid.poem.55849": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Der Einsame", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie einer, der auf fremden Meeren fuhr,", "tokens": ["Wie", "ei\u00b7ner", ",", "der", "auf", "frem\u00b7den", "Mee\u00b7ren", "fuhr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "PRELS", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "so bin ich bei den ewig Einheimischen;", "tokens": ["so", "bin", "ich", "bei", "den", "e\u00b7wig", "Ein\u00b7hei\u00b7mi\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "die vollen Tage stehn auf ihren Tischen,", "tokens": ["die", "vol\u00b7len", "Ta\u00b7ge", "stehn", "auf", "ih\u00b7ren", "Ti\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "mir aber ist die Ferne voll Figur.", "tokens": ["mir", "a\u00b7ber", "ist", "die", "Fer\u00b7ne", "voll", "Fi\u00b7gur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In mein Gesicht reicht eine Welt herein,", "tokens": ["In", "mein", "Ge\u00b7sicht", "reicht", "ei\u00b7ne", "Welt", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "die vielleicht unbewohnt ist wie ein Mond,", "tokens": ["die", "viel\u00b7leicht", "un\u00b7be\u00b7wohnt", "ist", "wie", "ein", "Mond", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VAFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "sie aber lassen kein Gef\u00fchl allein,", "tokens": ["sie", "a\u00b7ber", "las\u00b7sen", "kein", "Ge\u00b7f\u00fchl", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und alle ihre Worte sind bewohnt.", "tokens": ["und", "al\u00b7le", "ih\u00b7re", "Wor\u00b7te", "sind", "be\u00b7wohnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Dinge, die ich weither mit mir nahm,", "tokens": ["Die", "Din\u00b7ge", ",", "die", "ich", "weit\u00b7her", "mit", "mir", "nahm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "sehn selten aus, gehalten an das Ihre \u2013:", "tokens": ["sehn", "sel\u00b7ten", "aus", ",", "ge\u00b7hal\u00b7ten", "an", "das", "Ih\u00b7re", "\u2013", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$,", "VVPP", "APPR", "ART", "PPOSAT", "$(", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "in ihrer gro\u00dfen Heimat sind sie Tiere,", "tokens": ["in", "ih\u00b7rer", "gro\u00b7\u00dfen", "Hei\u00b7mat", "sind", "sie", "Tie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "hier halten sie den Atem an vor Scham.", "tokens": ["hier", "hal\u00b7ten", "sie", "den", "A\u00b7tem", "an", "vor", "Scham", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}