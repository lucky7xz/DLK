{"dta.poem.1402": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Tauff- und Pathen-W\u00fcnsche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Du gr\u00fcssest diese Welt zu naher Oster-Zeit/", "tokens": ["Du", "gr\u00fcs\u00b7sest", "die\u00b7se", "Welt", "zu", "na\u00b7her", "Os\u00b7ter\u00b7Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des HErren Purpur-Schmuck wird deiner Bl\u00f6sse", "tokens": ["Des", "Her\u00b7ren", "Pur\u00b7pur\u00b7Schmuck", "wird", "dei\u00b7ner", "Bl\u00f6s\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Flecken", "tokens": ["Fle\u00b7cken"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Und angeerbte Schuld nach diesem Heyl-Bad decken/", "tokens": ["Und", "an\u00b7ge\u00b7erb\u00b7te", "Schuld", "nach", "die\u00b7sem", "Heyl\u00b7Bad", "de\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Der Unschuld Christi Schnee wird dir ein Feyer-Kleid:", "tokens": ["Der", "Un\u00b7schuld", "Chris\u00b7ti", "Schnee", "wird", "dir", "ein", "Feyer\u00b7Kleid", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NN", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Dich m\u00f6ge durch die Welt ein steter Lentz begleiten/", "tokens": ["Dich", "m\u00f6\u00b7ge", "durch", "die", "Welt", "ein", "ste\u00b7ter", "Lentz", "be\u00b7glei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bi\u00df du den Sommer siehst der selgen Ewigkeiten.", "tokens": ["Bi\u00df", "du", "den", "Som\u00b7mer", "siehst", "der", "sel\u00b7gen", "E\u00b7wig\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}