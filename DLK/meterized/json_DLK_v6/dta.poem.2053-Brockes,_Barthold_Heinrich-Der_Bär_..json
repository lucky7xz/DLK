{"dta.poem.2053": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der B\u00e4r .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auch der zottichte, gefr\u00e4\u00dfig', uners\u00e4ttlich', starke B\u00e4r", "tokens": ["Auch", "der", "zot\u00b7tich\u00b7te", ",", "ge\u00b7fr\u00e4\u00b7\u00dfig'", ",", "un\u00b7er\u00b7s\u00e4tt\u00b7lich'", ",", "star\u00b7ke", "B\u00e4r"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "VVFIN", "$,", "VVPP", "$,", "ADJD", "$,", "ADJA", "NN"], "meter": "--+---+-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Dienet dem, der ihn erschuf, auch nicht weniger", "tokens": ["Die\u00b7net", "dem", ",", "der", "ihn", "er\u00b7schuf", ",", "auch", "nicht", "we\u00b7ni\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADV", "PTKNEG", "ADV"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "zur Ehr.", "tokens": ["zur", "Ehr", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Seiner Glieder Ebenmaa\u00dfe, St\u00e4rke, Fertigkeit, und", "tokens": ["Sei\u00b7ner", "Glie\u00b7der", "E\u00b7ben\u00b7maa\u00b7\u00dfe", ",", "St\u00e4r\u00b7ke", ",", "Fer\u00b7tig\u00b7keit", ",", "und"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "NN", "$,", "NN", "$,", "NN", "$,", "KON"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "Muth,", "tokens": ["Muth", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Auch da\u00df er nicht leicht den Menschen ungereizet Scha-", "tokens": ["Auch", "da\u00df", "er", "nicht", "leicht", "den", "Men\u00b7schen", "un\u00b7ge\u00b7rei\u00b7zet", "Scha"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "PTKNEG", "ADJD", "ART", "NN", "ADJD", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "den thut;", "tokens": ["den", "thut", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Da\u00df sie auch, wie andre Raubthier\u2019, um uns weniger", "tokens": ["Da\u00df", "sie", "auch", ",", "wie", "and\u00b7re", "Raubt\u00b7hier'", ",", "um", "uns", "we\u00b7ni\u00b7ger"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "$,", "PWAV", "ADJA", "NN", "$,", "KOUI", "PPER", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.9": {"text": "zu schrecken,", "tokens": ["zu", "schre\u00b7cken", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Noch uns Schaden zuzuf\u00fcgen, sich in W\u00fcsteneyen,", "tokens": ["Noch", "uns", "Scha\u00b7den", "zu\u00b7zu\u00b7f\u00fc\u00b7gen", ",", "sich", "in", "W\u00fcs\u00b7te\u00b7ne\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "NN", "VVIZU", "$,", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.11": {"text": "Hecken,", "tokens": ["He\u00b7cken", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Und Gebirgen gern befinden; da\u00df sie, uns f\u00fcr Frost", "tokens": ["Und", "Ge\u00b7bir\u00b7gen", "gern", "be\u00b7fin\u00b7den", ";", "da\u00df", "sie", ",", "uns", "f\u00fcr", "Frost"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "VVINF", "$.", "KOUS", "PPER", "$,", "PPER", "APPR", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.13": {"text": "zu sch\u00fctzen", "tokens": ["zu", "sch\u00fct\u00b7zen"], "token_info": ["word", "word"], "pos": ["PTKZU", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Mit der rauchen weichen Haut, uns so diensam sind und", "tokens": ["Mit", "der", "rau\u00b7chen", "wei\u00b7chen", "Haut", ",", "uns", "so", "dien\u00b7sam", "sind", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,", "PPER", "ADV", "ADJD", "VAFIN", "KON"], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "n\u00fctzen,", "tokens": ["n\u00fct\u00b7zen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Da man zur Bequemlichkeit, zu der W\u00e4rme, und zur", "tokens": ["Da", "man", "zur", "Be\u00b7quem\u00b7lich\u00b7keit", ",", "zu", "der", "W\u00e4r\u00b7me", ",", "und", "zur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PIS", "APPRART", "NN", "$,", "APPR", "ART", "NN", "$,", "KON", "APPRART"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Pracht,", "tokens": ["Pracht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.18": {"text": "Lagerst\u00e4tte, M\u00fctzen, Stiefeln, Muff\u2019 und Pferdedecken", "tokens": ["La\u00b7ger\u00b7st\u00e4t\u00b7te", ",", "M\u00fct\u00b7zen", ",", "Stie\u00b7feln", ",", "Muff'", "und", "Pfer\u00b7de\u00b7de\u00b7cken"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.19": {"text": "macht.", "tokens": ["macht", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.20": {"text": "Alles die\u00df ist dankensw\u00fcrdig; doch, wo wird es recht", "tokens": ["Al\u00b7les", "die\u00df", "ist", "dan\u00b7kens\u00b7w\u00fcr\u00b7dig", ";", "doch", ",", "wo", "wird", "es", "recht"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "PDS", "VAFIN", "ADJD", "$.", "ADV", "$,", "PWAV", "VAFIN", "PPER", "ADJD"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.21": {"text": "bedacht?", "tokens": ["be\u00b7dacht", "?"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.22": {"text": "Es giebt unterschiedne Sorten, braune, schwarz\u2019 und", "tokens": ["Es", "giebt", "un\u00b7ter\u00b7schied\u00b7ne", "Sor\u00b7ten", ",", "brau\u00b7ne", ",", "schwa\u00b7rz'", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,", "VVFIN", "$,", "VVFIN", "KON"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "wei\u00dfe B\u00e4ren,", "tokens": ["wei\u00b7\u00dfe", "B\u00e4\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.24": {"text": "Die sich auf verschiedne Weise, sonderlich im Winter,", "tokens": ["Die", "sich", "auf", "ver\u00b7schied\u00b7ne", "Wei\u00b7se", ",", "son\u00b7der\u00b7lich", "im", "Win\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ADJA", "NN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.25": {"text": "n\u00e4hren,", "tokens": ["n\u00e4h\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.26": {"text": "Da sie, welches kaum begreiflich, Fett aus ihren Tatzen", "tokens": ["Da", "sie", ",", "wel\u00b7ches", "kaum", "be\u00b7greif\u00b7lich", ",", "Fett", "aus", "ih\u00b7ren", "Tat\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "PRELS", "ADV", "ADJD", "$,", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.27": {"text": "saugen,", "tokens": ["sau\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.28": {"text": "Und damit, ohn\u2019 andre Nahrung, doch sich zu erhalten", "tokens": ["Und", "da\u00b7mit", ",", "ohn'", "and\u00b7re", "Nah\u00b7rung", ",", "doch", "sich", "zu", "er\u00b7hal\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PAV", "$,", "KOUI", "ADJA", "NN", "$,", "ADV", "PRF", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.29": {"text": "taugen.", "tokens": ["tau\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}