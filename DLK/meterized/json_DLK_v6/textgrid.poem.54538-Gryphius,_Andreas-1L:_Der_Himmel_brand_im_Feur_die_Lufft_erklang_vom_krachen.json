{"textgrid.poem.54538": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Himmel brand im Feur/ die Lufft erklang vom krachen", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel brand im Feur/ die Lufft erklang vom krachen", "tokens": ["Der", "Him\u00b7mel", "brand", "im", "Feur", "/", "die", "Lufft", "er\u00b7klang", "vom", "kra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$(", "ART", "NN", "VVFIN", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der schr\u00f6cklich harten Donner Schl\u00e4ge/", "tokens": ["Der", "schr\u00f6ck\u00b7lich", "har\u00b7ten", "Don\u00b7ner", "Schl\u00e4\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Nacht verdekcte Land und Stege/", "tokens": ["Die", "Nacht", "ver\u00b7de\u00b7kc\u00b7te", "Land", "und", "Ste\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Nacht die in den Lauff des Lichts sich drang zu machen;", "tokens": ["Die", "Nacht", "die", "in", "den", "Lauff", "des", "Lichts", "sich", "drang", "zu", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPR", "ART", "NN", "ART", "NN", "PRF", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Wind heult/ br\u00fcllt und blie\u00df und rie\u00df aus ", "tokens": ["Der", "Wind", "heult", "/", "br\u00fcllt", "und", "blie\u00df", "und", "rie\u00df", "aus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$(", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die tollen Geister wurden rege/", "tokens": ["Die", "tol\u00b7len", "Geis\u00b7ter", "wur\u00b7den", "re\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Mauern st\u00fcrtzen und die Wege/", "tokens": ["Die", "Mau\u00b7ern", "st\u00fcrt\u00b7zen", "und", "die", "We\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Verdeckte Grau\u00df und Kalck der Strom zustie\u00df die Nachen;", "tokens": ["Ver\u00b7deck\u00b7te", "Grau\u00df", "und", "Kalck", "der", "Strom", "zu\u00b7stie\u00df", "die", "Na\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Als mitten in dem Feld mich/ Herr/ der Todt ergriff/", "tokens": ["Als", "mit\u00b7ten", "in", "dem", "Feld", "mich", "/", "Herr", "/", "der", "Todt", "er\u00b7griff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "PPER", "$(", "NN", "$(", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der hinter mir in Sturm/ vor mir in Flammen lieff/", "tokens": ["Der", "hin\u00b7ter", "mir", "in", "Sturm", "/", "vor", "mir", "in", "Flam\u00b7men", "lieff", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "APPR", "NN", "$(", "APPR", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Vor mir die Bahn verf\u00e4llt/ und \u00fcber mir die H\u00fctten", "tokens": ["Vor", "mir", "die", "Bahn", "ver\u00b7f\u00e4llt", "/", "und", "\u00fc\u00b7ber", "mir", "die", "H\u00fct\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "VVPP", "$(", "KON", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "In leichte Splitter stie\u00df. Doch lebt ich/ Herr/ durch dich/", "tokens": ["In", "leich\u00b7te", "Split\u00b7ter", "stie\u00df", ".", "Doch", "lebt", "ich", "/", "Herr", "/", "durch", "dich", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$.", "KON", "VVFIN", "PPER", "$(", "NN", "$(", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mir selber war ich todt/ dein Engel wacht um mich/", "tokens": ["Mir", "sel\u00b7ber", "war", "ich", "todt", "/", "dein", "En\u00b7gel", "wacht", "um", "mich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "ADJD", "$(", "PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Stets neu gebohren wird/ den Gott wil stets beg\u00fctten.", "tokens": ["Stets", "neu", "ge\u00b7boh\u00b7ren", "wird", "/", "den", "Gott", "wil", "stets", "be\u00b7g\u00fct\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "VAFIN", "$(", "ART", "NN", "VMFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}