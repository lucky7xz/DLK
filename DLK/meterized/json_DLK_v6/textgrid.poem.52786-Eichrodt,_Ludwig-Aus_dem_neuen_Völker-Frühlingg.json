{"textgrid.poem.52786": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Aus dem neuen V\u00f6lker-Fr\u00fchlingg", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heilige Gew\u00e4sser w\u00e4lzen", "tokens": ["Hei\u00b7li\u00b7ge", "Ge\u00b7w\u00e4s\u00b7ser", "w\u00e4l\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich vom Grat des Himalaya,", "tokens": ["Sich", "vom", "Grat", "des", "Hi\u00b7ma\u00b7la\u00b7ya", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und die Muse geht auf Stelzen,", "tokens": ["Und", "die", "Mu\u00b7se", "geht", "auf", "Stel\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kaufbesorgt vom Sohn der Maja:", "tokens": ["Kauf\u00b7be\u00b7sorgt", "vom", "Sohn", "der", "Ma\u00b7ja", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Aus erloschner Felsengrotte", "tokens": ["Aus", "er\u00b7lo\u00b7schner", "Fel\u00b7sen\u00b7grot\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "T\u00f6net delphisches Orakel,", "tokens": ["T\u00f6\u00b7net", "del\u00b7phi\u00b7sches", "O\u00b7ra\u00b7kel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch die Welt steht vor dem Gotte", "tokens": ["Doch", "die", "Welt", "steht", "vor", "dem", "Got\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit der umgekehrten Fackel.", "tokens": ["Mit", "der", "um\u00b7ge\u00b7kehr\u00b7ten", "Fa\u00b7ckel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wenn sich Selene entschleiert dem Pol,", "tokens": ["Wenn", "sich", "Se\u00b7le\u00b7ne", "ent\u00b7schlei\u00b7ert", "dem", "Pol", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-----+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Dunkler Geheimnisse Schmerz-Alkohol", "tokens": ["Dunk\u00b7ler", "Ge\u00b7heim\u00b7nis\u00b7se", "Schmer\u00b7zAl\u00b7ko\u00b7hol"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.11": {"text": "Jauchzt in Phiolen und sch\u00e4umet Mirakel.", "tokens": ["Jauchzt", "in", "Phi\u00b7o\u00b7len", "und", "sch\u00e4u\u00b7met", "Mi\u00b7ra\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "VVFIN", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Aus dem Geisterland der Skythen", "tokens": ["Aus", "dem", "Geis\u00b7ter\u00b7land", "der", "Sky\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weht es ahnungsvoll her\u00fcber,", "tokens": ["Weht", "es", "ah\u00b7nungs\u00b7voll", "her\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch die D\u00e4mmerung der Mythen", "tokens": ["Durch", "die", "D\u00e4m\u00b7me\u00b7rung", "der", "My\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zittern Schwerter, gros Kaliber.", "tokens": ["Zit\u00b7tern", "Schwer\u00b7ter", ",", "gros", "Ka\u00b7li\u00b7ber", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Fluch, o Fluch dem kranken Wahne,", "tokens": ["Fluch", ",", "o", "Fluch", "dem", "kran\u00b7ken", "Wah\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Segen jeder stummen Klage,", "tokens": ["Se\u00b7gen", "je\u00b7der", "stum\u00b7men", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Tr\u00f6stung jeder Karawane,", "tokens": ["Tr\u00f6s\u00b7tung", "je\u00b7der", "Ka\u00b7ra\u00b7wa\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Friede jedem Sarkophage!", "tokens": ["Frie\u00b7de", "je\u00b7dem", "Sar\u00b7ko\u00b7pha\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Sonnig im Ozean schlummert das Ei", "tokens": ["Son\u00b7nig", "im", "O\u00b7ze\u00b7an", "schlum\u00b7mert", "das", "Ei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.10": {"text": "Seliger Insel und Akakelei", "tokens": ["Se\u00b7li\u00b7ger", "In\u00b7sel", "und", "A\u00b7ka\u00b7ke\u00b7lei"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+---+-++-+", "measure": "dactylic.init"}, "line.11": {"text": "Strahlt vom Skorpion und entschimmert der Waage.", "tokens": ["Strahlt", "vom", "Skor\u00b7pi\u00b7on", "und", "ent\u00b7schim\u00b7mert", "der", "Waa\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Lasset mich vom Aetna schreiten", "tokens": ["Las\u00b7set", "mich", "vom", "A\u00b7et\u00b7na", "schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NE", "VVFIN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Ueber hadesgraue Schlacken,", "tokens": ["Ue\u00b7ber", "ha\u00b7des\u00b7grau\u00b7e", "Schla\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00dft mich in die Fluth der Zeiten", "tokens": ["La\u00dft", "mich", "in", "die", "Fluth", "der", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tauchen mit dem Priesternacken;", "tokens": ["Tau\u00b7chen", "mit", "dem", "Pries\u00b7ter\u00b7na\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unabsehbar schleichen Z\u00fcge", "tokens": ["Un\u00b7ab\u00b7seh\u00b7bar", "schlei\u00b7chen", "Z\u00fc\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Schneegezeugter wilder Ahnen", "tokens": ["Schne\u00b7e\u00b7ge\u00b7zeug\u00b7ter", "wil\u00b7der", "Ah\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Um die eingesargte L\u00fcge", "tokens": ["Um", "die", "ein\u00b7ge\u00b7sarg\u00b7te", "L\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Des Jahrhunderts der Alanen.", "tokens": ["Des", "Jahr\u00b7hun\u00b7derts", "der", "A\u00b7la\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Zweifelhaft ist die Unsterblichkeit,", "tokens": ["Zwei\u00b7fel\u00b7haft", "ist", "die", "U\u00b7nsterb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Hol mich der Teufel eh' morgen als heut,", "tokens": ["Hol", "mich", "der", "Teu\u00b7fel", "eh'", "mor\u00b7gen", "als", "heut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "ADV", "KOKOM", "ADV", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}