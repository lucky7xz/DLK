{"textgrid.poem.27811": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier und dort sind Br\u00fcder zwar,", "tokens": ["Hier", "und", "dort", "sind", "Br\u00fc\u00b7der", "zwar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "VAFIN", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch ein gantz verkehrtes Paar.", "tokens": ["Doch", "ein", "gantz", "ver\u00b7kehr\u00b7tes", "Paar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Hier f\u00fchrt wider dort viel Krieg;", "tokens": ["Hier", "f\u00fchrt", "wi\u00b7der", "dort", "viel", "Krieg", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADV", "PIAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Doch behauptet dort den Sieg.", "tokens": ["Doch", "be\u00b7haup\u00b7tet", "dort", "den", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ieder mu\u00df in diesen Zug.", "tokens": ["Ie\u00b7der", "mu\u00df", "in", "die\u00b7sen", "Zug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wer dem dort dient, der ist klug;", "tokens": ["Wer", "dem", "dort", "dient", ",", "der", "ist", "klug", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADV", "VVFIN", "$,", "PRELS", "VAFIN", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Dort belohnt mit lauter Gott;", "tokens": ["Dort", "be\u00b7lohnt", "mit", "lau\u00b7ter", "Gott", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Hier bezahlt mit lauter Tod.", "tokens": ["Hier", "be\u00b7zahlt", "mit", "lau\u00b7ter", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}