{"dta.poem.23780": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Die Lieblings Leidenschaft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Du kleiner Trommelschl\u00e4ger Du,", "tokens": ["Du", "klei\u00b7ner", "Trom\u00b7mel\u00b7schl\u00e4\u00b7ger", "Du", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn h\u00f6rst Du einmal auf zu", "tokens": ["Wenn", "h\u00f6rst", "Du", "ein\u00b7mal", "auf", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "ADV", "APPR", "PTKZU"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "schw\u00e4rmen?", "tokens": ["schw\u00e4r\u00b7men", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "So sitze doch einmal in Ruh!", "tokens": ["So", "sit\u00b7ze", "doch", "ein\u00b7mal", "in", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kein Ende hat das Stunden lange L\u00e4rmen.", "tokens": ["Kein", "En\u00b7de", "hat", "das", "Stun\u00b7den", "lan\u00b7ge", "L\u00e4r\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du kleine Puppent\u00e4ndlerinn,", "tokens": ["Du", "klei\u00b7ne", "Pup\u00b7pen\u00b7t\u00e4nd\u00b7le\u00b7rinn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du hast auch wohl zu reden Ehre:", "tokens": ["Du", "hast", "auch", "wohl", "zu", "re\u00b7den", "Eh\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PTKZU", "VVINF", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du bringst die Zeit mit Puppen hin,", "tokens": ["Du", "bringst", "die", "Zeit", "mit", "Pup\u00b7pen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als ob die\u00df nicht so gut als Trommeln", "tokens": ["Als", "ob", "die\u00df", "nicht", "so", "gut", "als", "Trom\u00b7meln"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PDS", "PTKNEG", "ADV", "ADJD", "KOKOM", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "w\u00e4re.", "tokens": ["w\u00e4\u00b7re", "."], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Sich zu vergn\u00fcgen ist auch Pflicht;", "tokens": ["Sich", "zu", "ver\u00b7gn\u00fc\u00b7gen", "ist", "auch", "Pflicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKZU", "VVINF", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch werd ich damit niemand plagen,", "tokens": ["Doch", "werd", "ich", "da\u00b7mit", "nie\u00b7mand", "pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr mich schickt sich das Trommeln nicht:", "tokens": ["F\u00fcr", "mich", "schickt", "sich", "das", "Trom\u00b7meln", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PRF", "ART", "NN", "PTKNEG", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Doch Puppenspiel; das mu\u00dft du selber", "tokens": ["Doch", "Pup\u00b7pen\u00b7spiel", ";", "das", "mu\u00dft", "du", "sel\u00b7ber"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$.", "PDS", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "sagen.", "tokens": ["sa\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Ich sag, eins ist das andre werth,", "tokens": ["Ich", "sag", ",", "eins", "ist", "das", "and\u00b7re", "werth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PIS", "VAFIN", "ART", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist so klug, als ich mir scheine;", "tokens": ["Du", "bist", "so", "klug", ",", "als", "ich", "mir", "schei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein jedes liebt sein Steckenpferd:", "tokens": ["Ein", "je\u00b7des", "liebt", "sein", "Ste\u00b7cken\u00b7pferd", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Pupp ist deins, die Trommel ist das", "tokens": ["Die", "Pupp", "ist", "deins", ",", "die", "Trom\u00b7mel", "ist", "das"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "NE", "$,", "ART", "NN", "VAFIN", "ART"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "meine.", "tokens": ["mei\u00b7ne", "."], "token_info": ["word", "punct"], "pos": ["PPOSAT", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}