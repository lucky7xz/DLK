{"dta.poem.8003": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Des Knaben Tod .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ezeuch nicht den dunkeln Wald hinab!", "tokens": ["\u201e", "zeuch", "nicht", "den", "dun\u00b7keln", "Wald", "hin\u00b7ab", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PTKNEG", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es gilt dein Leben, du junger Knab!\u201c", "tokens": ["Es", "gilt", "dein", "Le\u00b7ben", ",", "du", "jun\u00b7ger", "Knab", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u201emein Gott im Himmel, der ist mein Licht,", "tokens": ["\u201e", "mein", "Gott", "im", "Him\u00b7mel", ",", "der", "ist", "mein", "Licht", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "APPRART", "NN", "$,", "PRELS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der l\u00e4\u00dft mich im dunkeln Walde nicht.\u201c", "tokens": ["Der", "l\u00e4\u00dft", "mich", "im", "dun\u00b7keln", "Wal\u00b7de", "nicht", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "PTKNEG", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Da zeucht er hinunter, der junge Knab,", "tokens": ["Da", "zeucht", "er", "hin\u00b7un\u00b7ter", ",", "der", "jun\u00b7ge", "Knab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Es braust ihm zu F\u00fc\u00dfen der Strom hinab,", "tokens": ["Es", "braust", "ihm", "zu", "F\u00fc\u00b7\u00dfen", "der", "Strom", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Es saust ihm zu Haupte der schwarze Wald,", "tokens": ["Es", "saust", "ihm", "zu", "Haup\u00b7te", "der", "schwar\u00b7ze", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und die Sonne versinket in Wolken bald.", "tokens": ["Und", "die", "Son\u00b7ne", "ver\u00b7sin\u00b7ket", "in", "Wol\u00b7ken", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN", "ADV", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Und er kommt an\u2019s finstere R\u00e4uberhaus,", "tokens": ["Und", "er", "kommt", "an's", "fins\u00b7te\u00b7re", "R\u00e4u\u00b7ber\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Eine holde Jungfrau schauet heraus:", "tokens": ["Ei\u00b7ne", "hol\u00b7de", "Jung\u00b7frau", "schau\u00b7et", "he\u00b7raus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "\u201eo wehe! du bist so ein junger Knab,", "tokens": ["\u201e", "o", "we\u00b7he", "!", "du", "bist", "so", "ein", "jun\u00b7ger", "Knab", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "ADJD", "$.", "PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Was kommst du in\u2019s Thal des Todes herab?\u201c", "tokens": ["Was", "kommst", "du", "in's", "Thal", "des", "To\u00b7des", "her\u00b7ab", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "ADV", "$.", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Aus dem Thor die m\u00f6rdrische Rotte bricht,", "tokens": ["Aus", "dem", "Thor", "die", "m\u00f6rd\u00b7ri\u00b7sche", "Rot\u00b7te", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Jungfrau decket ihr Angesicht,", "tokens": ["Die", "Jung\u00b7frau", "de\u00b7cket", "ihr", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie sto\u00dfen ihn nieder, sie rauben sein Gut,", "tokens": ["Sie", "sto\u00b7\u00dfen", "ihn", "nie\u00b7der", ",", "sie", "rau\u00b7ben", "sein", "Gut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sie lassen ihn liegen in seinem Blut.", "tokens": ["Sie", "las\u00b7sen", "ihn", "lie\u00b7gen", "in", "sei\u00b7nem", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "\u201eo weh! wie dunkel! keine Sonne, kein Stern!", "tokens": ["\u201e", "o", "weh", "!", "wie", "dun\u00b7kel", "!", "kei\u00b7ne", "Son\u00b7ne", ",", "kein", "Stern", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "PTKVZ", "$.", "PWAV", "ADJD", "$.", "PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Wen ruf\u2019 ich an? ist mein Gott so fern?", "tokens": ["Wen", "ruf'", "ich", "an", "?", "ist", "mein", "Gott", "so", "fern", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKVZ", "$.", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ha! Jungfrau dort, im himmlischen Schein,", "tokens": ["Ha", "!", "Jung\u00b7frau", "dort", ",", "im", "himm\u00b7li\u00b7schen", "Schein", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "ADV", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Nimm auf meine Seel\u2019 in die H\u00e4nde dein!\u201c", "tokens": ["Nimm", "auf", "mei\u00b7ne", "Seel'", "in", "die", "H\u00e4n\u00b7de", "dein", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "PPOSAT", "$.", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}}}}