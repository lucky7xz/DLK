{"textgrid.poem.55275": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Kommt Zeit, kommt Rat", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer will denn alles gleich ergr\u00fcnden!", "tokens": ["Wer", "will", "denn", "al\u00b7les", "gleich", "er\u00b7gr\u00fcn\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sobald der Schnee schmilzt, wird sich's finden.", "tokens": ["So\u00b7bald", "der", "Schnee", "schmilzt", ",", "wird", "sich's", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,", "VAFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hier hilft nun weiter kein Bem\u00fchn!", "tokens": ["Hier", "hilft", "nun", "wei\u00b7ter", "kein", "Be\u00b7m\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind's Rosen, nun, sie werden bl\u00fchn.", "tokens": ["Sin\u00b7d's", "Ro\u00b7sen", ",", "nun", ",", "sie", "wer\u00b7den", "bl\u00fchn", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADV", "$,", "PPER", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}