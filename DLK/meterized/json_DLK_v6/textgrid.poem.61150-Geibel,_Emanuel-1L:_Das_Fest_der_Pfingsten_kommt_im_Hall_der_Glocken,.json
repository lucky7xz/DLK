{"textgrid.poem.61150": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Fest der Pfingsten kommt im Hall der Glocken,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Fest der Pfingsten kommt im Hall der Glocken,", "tokens": ["Das", "Fest", "der", "Pfings\u00b7ten", "kommt", "im", "Hall", "der", "Glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da jauchzt in Fr\u00fchlingsschauern die Natur;", "tokens": ["Da", "jauchzt", "in", "Fr\u00fch\u00b7lings\u00b7schau\u00b7ern", "die", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf jedem Strauch des Waldes und der Flur", "tokens": ["Auf", "je\u00b7dem", "Strauch", "des", "Wal\u00b7des", "und", "der", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Schwebt eine Ros' als Flamme mit Frohlocken.", "tokens": ["Schwebt", "ei\u00b7ne", "Ros'", "als", "Flam\u00b7me", "mit", "Froh\u00b7lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KOUS", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O Geist, der einst in goldnen Feuerflocken", "tokens": ["O", "Geist", ",", "der", "einst", "in", "gold\u00b7nen", "Feu\u00b7er\u00b7flo\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aufs Haupt der J\u00fcnger brausend niederfuhr,", "tokens": ["Aufs", "Haupt", "der", "J\u00fcn\u00b7ger", "brau\u00b7send", "nie\u00b7der\u00b7fuhr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von deinem Reichtum einen Funken nur,", "tokens": ["Von", "dei\u00b7nem", "Reich\u00b7tum", "ei\u00b7nen", "Fun\u00b7ken", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hernieder send' ihn auf des S\u00e4ngers Locken!", "tokens": ["Her\u00b7nie\u00b7der", "send'", "ihn", "auf", "des", "S\u00e4n\u00b7gers", "Lo\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich wei\u00df es wohl, nicht w\u00fcrdig bin ich dein;", "tokens": ["Ich", "wei\u00df", "es", "wohl", ",", "nicht", "w\u00fcr\u00b7dig", "bin", "ich", "dein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PTKNEG", "ADJD", "VAFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch hast du nie die Tugend ja gemessen,", "tokens": ["Doch", "hast", "du", "nie", "die", "Tu\u00b7gend", "ja", "ge\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Glaube zieht, die Sehnsucht dich allein.", "tokens": ["Der", "Glau\u00b7be", "zieht", ",", "die", "Sehn\u00b7sucht", "dich", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Armen hast du nimmermehr vergessen,", "tokens": ["Der", "Ar\u00b7men", "hast", "du", "nim\u00b7mer\u00b7mehr", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du kehrtest in der Fischer H\u00fctten ein,", "tokens": ["Du", "kehr\u00b7test", "in", "der", "Fi\u00b7scher", "H\u00fct\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und an der S\u00fcnder Tisch bist du gesessen.", "tokens": ["Und", "an", "der", "S\u00fcn\u00b7der", "Tisch", "bist", "du", "ge\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}