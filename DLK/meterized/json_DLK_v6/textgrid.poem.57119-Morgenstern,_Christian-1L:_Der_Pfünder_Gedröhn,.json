{"textgrid.poem.57119": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Pf\u00fcnder Gedr\u00f6hn,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Pf\u00fcnder Gedr\u00f6hn,", "tokens": ["Der", "Pf\u00fcn\u00b7der", "Ge\u00b7dr\u00f6hn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "der Flinten Alarm,", "tokens": ["der", "Flin\u00b7ten", "A\u00b7larm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "das Schrein und Gest\u00f6hn,", "tokens": ["das", "Schrein", "und", "Ge\u00b7st\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "die Wut und der Harm \u2013", "tokens": ["die", "Wut", "und", "der", "Harm", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "der Sturm und die Flucht,", "tokens": ["der", "Sturm", "und", "die", "Flucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "die H\u00fcgel voll Qual", "tokens": ["die", "H\u00fc\u00b7gel", "voll", "Qual"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "der k\u00f6stlichen Frucht,", "tokens": ["der", "k\u00f6st\u00b7li\u00b7chen", "Frucht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "der D\u00f6rfer Fanal \u2013", "tokens": ["der", "D\u00f6r\u00b7fer", "Fa\u00b7nal", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "der Mensch als Held", "tokens": ["der", "Mensch", "als", "Held"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KOUS", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "und der Mensch als Tier \u2013", "tokens": ["und", "der", "Mensch", "als", "Tier", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KOUS", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "in Lettern gestellt", "tokens": ["in", "Let\u00b7tern", "ge\u00b7stellt"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVPP"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "auf ein Blatt Papier.", "tokens": ["auf", "ein", "Blatt", "Pa\u00b7pier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}