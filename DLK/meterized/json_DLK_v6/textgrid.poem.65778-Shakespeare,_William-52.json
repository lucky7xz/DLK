{"textgrid.poem.65778": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "52", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So bin ich wie der Reiche, der sich still", "tokens": ["So", "bin", "ich", "wie", "der", "Rei\u00b7che", ",", "der", "sich", "still"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "KOKOM", "ART", "NE", "$,", "PRELS", "PRF", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Am Schl\u00fcssel labt zu heimlichem Besitze,", "tokens": ["Am", "Schl\u00fcs\u00b7sel", "labt", "zu", "heim\u00b7li\u00b7chem", "Be\u00b7sit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den er nicht alle Stunden z\u00e4hlen will,", "tokens": ["Den", "er", "nicht", "al\u00b7le", "Stun\u00b7den", "z\u00e4h\u00b7len", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "PIAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Um nicht zu stumpfen seltnen Reizes Spitze.", "tokens": ["Um", "nicht", "zu", "stump\u00b7fen", "selt\u00b7nen", "Rei\u00b7zes", "Spit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKNEG", "PTKZU", "VVINF", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Daher der Feste W\u00fcrd' und Herrlichkeit,", "tokens": ["Da\u00b7her", "der", "Fes\u00b7te", "W\u00fcrd'", "und", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Weil sie nur sparsam in des Jahres Reigen,", "tokens": ["Weil", "sie", "nur", "spar\u00b7sam", "in", "des", "Jah\u00b7res", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wie gr\u00f6\u00df're Edelstein' im Halsgeschmeid,", "tokens": ["Wie", "gr\u00f6\u00df'\u00b7re", "E\u00b7del\u00b7stein'", "im", "Hals\u00b7ge\u00b7schmeid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wie reinste Perlen d\u00fcnnges\u00e4t sich zeigen.", "tokens": ["Wie", "reins\u00b7te", "Per\u00b7len", "d\u00fcnn\u00b7ge\u00b7s\u00e4t", "sich", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "So ist die Zeit, die dich mir birgt, der Schrein", "tokens": ["So", "ist", "die", "Zeit", ",", "die", "dich", "mir", "birgt", ",", "der", "Schrein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und Kasten, der ein gut Gewand verwahrt,", "tokens": ["Und", "Kas\u00b7ten", ",", "der", "ein", "gut", "Ge\u00b7wand", "ver\u00b7wahrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "ART", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "F\u00fcr einen Ehrentag erlesner Schmuck zu sein,", "tokens": ["F\u00fcr", "ei\u00b7nen", "Eh\u00b7ren\u00b7tag", "er\u00b7les\u00b7ner", "Schmuck", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn es verborgnen Glanz von neuem offenbart.", "tokens": ["Wenn", "es", "ver\u00b7borg\u00b7nen", "Glanz", "von", "neu\u00b7em", "of\u00b7fen\u00b7bart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Heil dir und deinem Wert! denn du beseelst", "tokens": ["Heil", "dir", "und", "dei\u00b7nem", "Wert", "!", "denn", "du", "be\u00b7seelst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "KON", "PPOSAT", "NN", "$.", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Zum Jubel, wo du bist; zum Hoffen, wo du fehlst.", "tokens": ["Zum", "Ju\u00b7bel", ",", "wo", "du", "bist", ";", "zum", "Hof\u00b7fen", ",", "wo", "du", "fehlst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPER", "VAFIN", "$.", "APPRART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}