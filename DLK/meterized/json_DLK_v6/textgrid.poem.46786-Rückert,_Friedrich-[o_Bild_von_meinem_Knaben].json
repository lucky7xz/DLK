{"textgrid.poem.46786": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[o Bild von meinem Knaben]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Bild von meinem Knaben,", "tokens": ["O", "Bild", "von", "mei\u00b7nem", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du bist nicht ganz getroffen,", "tokens": ["Du", "bist", "nicht", "ganz", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und doch so sprechend \u00e4hnlich,", "tokens": ["Und", "doch", "so", "spre\u00b7chend", "\u00e4hn\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Will das sich widersprechen?", "tokens": ["Will", "das", "sich", "wi\u00b7der\u00b7spre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich wei\u00df es auszugleichen,", "tokens": ["Ich", "wei\u00df", "es", "aus\u00b7zu\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und l\u00f6se so das R\u00e4thsel.", "tokens": ["Und", "l\u00f6\u00b7se", "so", "das", "R\u00e4th\u00b7sel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Als ich in meiner Burg hielt,", "tokens": ["Als", "ich", "in", "mei\u00b7ner", "Burg", "hielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das hei\u00dft in meinem Hause,", "tokens": ["Das", "hei\u00dft", "in", "mei\u00b7nem", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Freund von Hildburghausen,", "tokens": ["Den", "Freund", "von", "Hild\u00b7burg\u00b7hau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ihm der Geist es eingab,", "tokens": ["Und", "ihm", "der", "Geist", "es", "ein\u00b7gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Zum Ungl\u00fcck um zum Gl\u00fccke,", "tokens": ["Zum", "Un\u00b7gl\u00fcck", "um", "zum", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zum leid'gen Trost im Leide,", "tokens": ["Zum", "lei\u00b7d'\u00b7gen", "Trost", "im", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Mit Farbenglanz ein Scheinbild", "tokens": ["Mit", "Far\u00b7ben\u00b7glanz", "ein", "Schein\u00b7bild"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Lebens zu erschaffen,", "tokens": ["Des", "Le\u00b7bens", "zu", "er\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und mir ins Haus zu stiften", "tokens": ["Und", "mir", "ins", "Haus", "zu", "stif\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Zu ewigem Ged\u00e4chtni\u00df,", "tokens": ["Zu", "e\u00b7wi\u00b7gem", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Von meinen beiden Kindern,", "tokens": ["Von", "mei\u00b7nen", "bei\u00b7den", "Kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Die fr\u00fch erblassen sollten;", "tokens": ["Die", "fr\u00fch", "er\u00b7blas\u00b7sen", "soll\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Da malt er erst das M\u00e4dchen,", "tokens": ["Da", "malt", "er", "erst", "das", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Das nun zuerst gestorben,", "tokens": ["Das", "nun", "zu\u00b7erst", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Darauf nach vierzehn Tagen", "tokens": ["Da\u00b7rauf", "nach", "vier\u00b7zehn", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Den Knaben, der dem M\u00e4dchen", "tokens": ["Den", "Kna\u00b7ben", ",", "der", "dem", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "In vierzehn Tagen nachstarb.", "tokens": ["In", "vier\u00b7zehn", "Ta\u00b7gen", "nach\u00b7starb", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.18": {"text": "Wie wenig sah er damals", "tokens": ["Wie", "we\u00b7nig", "sah", "er", "da\u00b7mals"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Zum Sterben aus, zum Welken,", "tokens": ["Zum", "Ster\u00b7ben", "aus", ",", "zum", "Wel\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Der Lebensbl\u00fcthenreiche;", "tokens": ["Der", "Le\u00b7bens\u00b7bl\u00fc\u00b7then\u00b7rei\u00b7che", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Der Bl\u00fcthenlebensvolle!", "tokens": ["Der", "Bl\u00fc\u00b7then\u00b7le\u00b7bens\u00b7vol\u00b7le", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Da sa\u00df er gegen\u00fcber", "tokens": ["Da", "sa\u00df", "er", "ge\u00b7gen\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Dem Maler in der Kammer,", "tokens": ["Dem", "Ma\u00b7ler", "in", "der", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Der vor der Staffelei sa\u00df,", "tokens": ["Der", "vor", "der", "Staf\u00b7fe\u00b7lei", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.25": {"text": "Von der Natur gemalet,", "tokens": ["Von", "der", "Na\u00b7tur", "ge\u00b7ma\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Ein Bild der Liebesanmuth.", "tokens": ["Ein", "Bild", "der", "Lie\u00b7be\u00b7san\u00b7muth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Mit L\u00e4cheln, das zum Voraus", "tokens": ["Mit", "L\u00e4\u00b7cheln", ",", "das", "zum", "Vo\u00b7raus"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Besch\u00e4mend, das der Zauber", "tokens": ["Be\u00b7sch\u00e4\u00b7mend", ",", "das", "der", "Zau\u00b7ber"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Der Kunst ihm wollt' entwenden;", "tokens": ["Der", "Kunst", "ihm", "wollt'", "ent\u00b7wen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Dem K\u00fcnstleraug' entgegen", "tokens": ["Dem", "K\u00fcnst\u00b7leraug'", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "PTKVZ"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.31": {"text": "Mit k\u00fchner Unschuld haltend", "tokens": ["Mit", "k\u00fch\u00b7ner", "Un\u00b7schuld", "hal\u00b7tend"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Den Abgrund seiner Augen,", "tokens": ["Den", "Ab\u00b7grund", "sei\u00b7ner", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Die unbewu\u00dften Trotzes", "tokens": ["Die", "un\u00b7be\u00b7wu\u00df\u00b7ten", "Trot\u00b7zes"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Zum Kampf den Pinsel fordern,", "tokens": ["Zum", "Kampf", "den", "Pin\u00b7sel", "for\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Ein dunkles Licht zu malen.", "tokens": ["Ein", "dunk\u00b7les", "Licht", "zu", "ma\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich lie\u00df in diesem Kampfe", "tokens": ["Ich", "lie\u00df", "in", "die\u00b7sem", "Kamp\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Bildner und das Urbild", "tokens": ["Den", "Bild\u00b7ner", "und", "das", "Ur\u00b7bild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und schrieb an meinem Stehpult", "tokens": ["Und", "schrieb", "an", "mei\u00b7nem", "Steh\u00b7pult"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Zimmer an der Kammer.", "tokens": ["Im", "Zim\u00b7mer", "an", "der", "Kam\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch konnt' ich viel nicht schreiben,", "tokens": ["Doch", "konnt'", "ich", "viel", "nicht", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Denn immer mu\u00dft' ich lauschen", "tokens": ["Denn", "im\u00b7mer", "mu\u00dft'", "ich", "lau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dem nebenan inzwischen", "tokens": ["Dem", "ne\u00b7be\u00b7nan", "in\u00b7zwi\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "In Gang gekommen, str\u00f6mend", "tokens": ["In", "Gang", "ge\u00b7kom\u00b7men", ",", "str\u00f6\u00b7mend"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "VVPP", "$,", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Im Zug erhaltnen Sitzungs-", "tokens": ["Im", "Zug", "er\u00b7halt\u00b7nen", "Sit\u00b7zungs"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "TRUNC"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Gespr\u00e4che meiner Nachbarn.", "tokens": ["Ge\u00b7spr\u00e4\u00b7che", "mei\u00b7ner", "Nach\u00b7barn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der K\u00fcnstler hat den Grundsatz,", "tokens": ["Der", "K\u00fcnst\u00b7ler", "hat", "den", "Grund\u00b7satz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und auch dazu die Gabe,", "tokens": ["Und", "auch", "da\u00b7zu", "die", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Sprechen so viel m\u00f6glich", "tokens": ["Im", "Spre\u00b7chen", "so", "viel", "m\u00f6g\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Sitzenden zu setzen,", "tokens": ["Die", "Sit\u00b7zen\u00b7den", "zu", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df ihre Mienen sprechen,", "tokens": ["Da\u00df", "ih\u00b7re", "Mie\u00b7nen", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und so dann auch die Bilder.", "tokens": ["Und", "so", "dann", "auch", "die", "Bil\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und diesem Umstand dank' ich's", "tokens": ["Und", "die\u00b7sem", "Um\u00b7stand", "dank'", "ich's"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "VVFIN", "PIS"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "(somit l\u00f6s' ich das R\u00e4thsel),", "tokens": ["(", "so\u00b7mit", "l\u00f6s'", "ich", "das", "R\u00e4th\u00b7sel", ")", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ART", "NN", "$(", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Da\u00df ich das Bild des Knaben", "tokens": ["Da\u00df", "ich", "das", "Bild", "des", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Nun sprechend \u00e4hnlich finde,", "tokens": ["Nun", "spre\u00b7chend", "\u00e4hn\u00b7lich", "fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Wenn auch nicht ganz getroffen.", "tokens": ["Wenn", "auch", "nicht", "ganz", "ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Der Maler traf die Z\u00fcge,", "tokens": ["Der", "Ma\u00b7ler", "traf", "die", "Z\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Die Mienen, die belebten,", "tokens": ["Die", "Mie\u00b7nen", ",", "die", "be\u00b7leb\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Die hielt er fest, die Worte,", "tokens": ["Die", "hielt", "er", "fest", ",", "die", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Die sie belebten aber,", "tokens": ["Die", "sie", "be\u00b7leb\u00b7ten", "a\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Lie\u00df er dabei verfliegen;", "tokens": ["Lie\u00df", "er", "da\u00b7bei", "ver\u00b7flie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Womit konnt' er sie halten?", "tokens": ["Wo\u00b7mit", "konnt'", "er", "sie", "hal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Ich aber hab' im Ohre,", "tokens": ["Ich", "a\u00b7ber", "hab'", "im", "Oh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Dem lauschenden, behalten", "tokens": ["Dem", "lau\u00b7schen\u00b7den", ",", "be\u00b7hal\u00b7ten"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "ADJA", "$,", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Den s\u00fc\u00dfen Schwarm von Bienen,", "tokens": ["Den", "s\u00fc\u00b7\u00dfen", "Schwarm", "von", "Bie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Der dieser Rosenknospe", "tokens": ["Der", "die\u00b7ser", "Ro\u00b7sen\u00b7knos\u00b7pe"], "token_info": ["word", "word", "word"], "pos": ["ART", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Des Mundes honigduftend", "tokens": ["Des", "Mun\u00b7des", "ho\u00b7nig\u00b7duf\u00b7tend"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Entquoll, vom Athem trunken.", "tokens": ["Ent\u00b7quoll", ",", "vom", "A\u00b7them", "trun\u00b7ken", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Und seh' ich nun das Bild an,", "tokens": ["Und", "seh'", "ich", "nun", "das", "Bild", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "So h\u00e4ngen sich die Trauben", "tokens": ["So", "h\u00e4n\u00b7gen", "sich", "die", "Trau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Von Bienen an die Rose,", "tokens": ["Von", "Bie\u00b7nen", "an", "die", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Und summen ins Gem\u00fcth mir", "tokens": ["Und", "sum\u00b7men", "ins", "Ge\u00b7m\u00fcth", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Schwerm\u00fcthge Schw\u00e4rmereien,", "tokens": ["Schwer\u00b7m\u00fcth\u00b7ge", "Schw\u00e4r\u00b7me\u00b7rei\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.29": {"text": "Ansprechend mit dem Nachklang", "tokens": ["An\u00b7spre\u00b7chend", "mit", "dem", "Nach\u00b7klang"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Der nun versiegten Quelle", "tokens": ["Der", "nun", "ver\u00b7sieg\u00b7ten", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Des kindlichen Geschw\u00e4tzes,", "tokens": ["Des", "kind\u00b7li\u00b7chen", "Ge\u00b7schw\u00e4t\u00b7zes", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Ansprechend die Erinnrung", "tokens": ["An\u00b7spre\u00b7chend", "die", "E\u00b7rinn\u00b7rung"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Im tiefsten Grund der Seele,", "tokens": ["Im", "tiefs\u00b7ten", "Grund", "der", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Um ew'ge Lieb' und Trauer", "tokens": ["Um", "ew'\u00b7ge", "Lieb'", "und", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Und Freud' am sch\u00f6nen Bildni\u00df.", "tokens": ["Und", "Freud'", "am", "sch\u00f6\u00b7nen", "Bild\u00b7ni\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "So wird das Bild nun freilich,", "tokens": ["So", "wird", "das", "Bild", "nun", "frei\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Als wie zu mir, zu keinem", "tokens": ["Als", "wie", "zu", "mir", ",", "zu", "kei\u00b7nem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "KOKOM", "APPR", "PPER", "$,", "APPR", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Beschauer sprechen k\u00f6nnen,", "tokens": ["Be\u00b7schau\u00b7er", "spre\u00b7chen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Der es nur sieht, nicht h\u00f6rt auch.", "tokens": ["Der", "es", "nur", "sieht", ",", "nicht", "h\u00f6rt", "auch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$,", "PTKNEG", "VVFIN", "ADV", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}