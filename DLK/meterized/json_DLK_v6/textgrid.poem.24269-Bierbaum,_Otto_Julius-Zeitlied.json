{"textgrid.poem.24269": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Zeitlied", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Tr\u00e4umer und Propheten,", "tokens": ["Die", "Tr\u00e4u\u00b7mer", "und", "Pro\u00b7phe\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die raten und die reden", "tokens": ["Die", "ra\u00b7ten", "und", "die", "re\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "KON", "ART", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Viel von der Ewigkeit.", "tokens": ["Viel", "von", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "++-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wohlan, wers kann, der fliege!", "tokens": ["Wo\u00b7hlan", ",", "wers", "kann", ",", "der", "flie\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "VMFIN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wir steigen auf der Stiege", "tokens": ["Wir", "stei\u00b7gen", "auf", "der", "Stie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bescheiden, stufenweise; so dienen wir der Zeit.", "tokens": ["Be\u00b7schei\u00b7den", ",", "stu\u00b7fen\u00b7wei\u00b7se", ";", "so", "die\u00b7nen", "wir", "der", "Zeit", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$.", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Wir bleiben auf der Erden,", "tokens": ["Wir", "blei\u00b7ben", "auf", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hier gilt es reif zu werden", "tokens": ["Hier", "gilt", "es", "reif", "zu", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKZU", "VAINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In Kraft und Fr\u00f6hlichkeit.", "tokens": ["In", "Kraft", "und", "Fr\u00f6h\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das ist des Lebens Segen:", "tokens": ["Das", "ist", "des", "Le\u00b7bens", "Se\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Im Lichte sich zu regen;", "tokens": ["Im", "Lich\u00b7te", "sich", "zu", "re\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "APPR", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wir messen unsre Kr\u00e4fte am Kraftma\u00df unsrer Zeit.", "tokens": ["Wir", "mes\u00b7sen", "uns\u00b7re", "Kr\u00e4f\u00b7te", "am", "Kraft\u00b7ma\u00df", "uns\u00b7rer", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie gibt uns viel, wir geben", "tokens": ["Sie", "gibt", "uns", "viel", ",", "wir", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr unser ganzes Leben", "tokens": ["Ihr", "un\u00b7ser", "gan\u00b7zes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In Kindesdankbarkeit;", "tokens": ["In", "Kin\u00b7des\u00b7dank\u00b7bar\u00b7keit", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das Erbe gilts zu mehren,", "tokens": ["Das", "Er\u00b7be", "gilts", "zu", "meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df wir mit ihr in Ehren", "tokens": ["Da\u00df", "wir", "mit", "ihr", "in", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vor uns bestehen k\u00f6nnen, froh einer reichen Zeit.", "tokens": ["Vor", "uns", "be\u00b7ste\u00b7hen", "k\u00f6n\u00b7nen", ",", "froh", "ei\u00b7ner", "rei\u00b7chen", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "VMINF", "$,", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Sch\u00f6n soll sie sein, und St\u00e4rke", "tokens": ["Sch\u00f6n", "soll", "sie", "sein", ",", "und", "St\u00e4r\u00b7ke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "VAINF", "$,", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Merkmal ihrer Werke;", "tokens": ["Das", "Merk\u00b7mal", "ih\u00b7rer", "Wer\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Kraft sei sie geweiht,", "tokens": ["Der", "Kraft", "sei", "sie", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Seele, Geist und Triebe", "tokens": ["Die", "See\u00b7le", ",", "Geist", "und", "Trie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Umfa\u00dft mit gleicher Liebe,", "tokens": ["Um\u00b7fa\u00dft", "mit", "glei\u00b7cher", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df wir mit Stolz bekennen: wir dienen dieser Zeit.", "tokens": ["Da\u00df", "wir", "mit", "Stolz", "be\u00b7ken\u00b7nen", ":", "wir", "die\u00b7nen", "die\u00b7ser", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVINF", "$.", "PPER", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}