{"textgrid.poem.38828": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus den uralten Tiefen,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus den uralten Tiefen,", "tokens": ["Aus", "den", "ur\u00b7al\u00b7ten", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In denen Sehnsucht, Schmerz und Wollust brannte,", "tokens": ["In", "de\u00b7nen", "Sehn\u00b7sucht", ",", "Schmerz", "und", "Wol\u00b7lust", "brann\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Welt sich selbst erkannte", "tokens": ["Die", "Welt", "sich", "selbst", "er\u00b7kann\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und nicht mehr ihre ewgen Keime schliefen,", "tokens": ["Und", "nicht", "mehr", "ih\u00b7re", "ew\u00b7gen", "Kei\u00b7me", "schlie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Entz\u00fcnden sich von neuen", "tokens": ["Ent\u00b7z\u00fcn\u00b7den", "sich", "von", "neu\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Strahlen, wollen mich von mir befreien. \u2013", "tokens": ["Die", "Strah\u00b7len", ",", "wol\u00b7len", "mich", "von", "mir", "be\u00b7frei\u00b7en", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "VMFIN", "PRF", "APPR", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O Mensch, was k\u00f6nnen Sinnen,", "tokens": ["O", "Mensch", ",", "was", "k\u00f6n\u00b7nen", "Sin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWS", "VMFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gefangen in den alten Frevel-Banden,", "tokens": ["Ge\u00b7fan\u00b7gen", "in", "den", "al\u00b7ten", "Fre\u00b7vel\u00b7Ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In den erstorbnen Landen,", "tokens": ["In", "den", "ers\u00b7torb\u00b7nen", "Lan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor Zittern, Qual und herber Angst beginnen?", "tokens": ["Vor", "Zit\u00b7tern", ",", "Qual", "und", "her\u00b7ber", "Angst", "be\u00b7gin\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So hellres Sehnsuchtscheinen", "tokens": ["So", "hell\u00b7res", "Sehn\u00b7sucht\u00b7schei\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mu\u00df dich nur fester in dir selbst versteinen!", "tokens": ["Mu\u00df", "dich", "nur", "fes\u00b7ter", "in", "dir", "selbst", "ver\u00b7stei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da bricht der Zorn in Wogen", "tokens": ["Da", "bricht", "der", "Zorn", "in", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Her\u00fcber, rei\u00dft das Herz mit Sturmgewalten;", "tokens": ["Her\u00b7\u00fc\u00b7ber", ",", "rei\u00dft", "das", "Herz", "mit", "Sturm\u00b7ge\u00b7wal\u00b7ten", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie kann da immer halten", "tokens": ["Wie", "kann", "da", "im\u00b7mer", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Panzer, der mit Dumpfheit es umzogen?", "tokens": ["Der", "Pan\u00b7zer", ",", "der", "mit", "Dum\u00b7pf\u00b7heit", "es", "um\u00b7zo\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "PPER", "VVIZU", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Gieb, Seele, dich gefangen,", "tokens": ["Gieb", ",", "See\u00b7le", ",", "dich", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Errette dich zerschmelzend von dem Bangen.", "tokens": ["Er\u00b7ret\u00b7te", "dich", "zer\u00b7schmel\u00b7zend", "von", "dem", "Ban\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Vom Abgrund seh ich spiegeln", "tokens": ["Vom", "Ab\u00b7grund", "seh", "ich", "spie\u00b7geln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die gr\u00fcnen Blitze durch das n\u00e4chtge Dunkel,", "tokens": ["Die", "gr\u00fc\u00b7nen", "Blit\u00b7ze", "durch", "das", "n\u00e4cht\u00b7ge", "Dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein freudenreich Gefunkel", "tokens": ["Ein", "freu\u00b7den\u00b7reich", "Ge\u00b7fun\u00b7kel"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Err\u00f6thet sich, da klingt mit Engelfl\u00fcgeln", "tokens": ["Er\u00b7r\u00f6t\u00b7het", "sich", ",", "da", "klingt", "mit", "En\u00b7gel\u00b7fl\u00fc\u00b7geln"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "$,", "ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Entbunden und gefunden", "tokens": ["Ent\u00b7bun\u00b7den", "und", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Wohllaut, zitternd, aus des Herzens Wunden.", "tokens": ["Der", "Wohl\u00b7laut", ",", "zit\u00b7ternd", ",", "aus", "des", "Her\u00b7zens", "Wun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich sehe sie entfliehen", "tokens": ["Ich", "se\u00b7he", "sie", "ent\u00b7flie\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die schwarze Angst, den Zorn, die wilden Qualen,", "tokens": ["Die", "schwar\u00b7ze", "Angst", ",", "den", "Zorn", ",", "die", "wil\u00b7den", "Qua\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die goldnen Sonnenstrahlen", "tokens": ["Die", "gold\u00b7nen", "Son\u00b7nen\u00b7strah\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie im Triumphe nach dem Feinde ziehen:", "tokens": ["Wie", "im", "Tri\u00b7um\u00b7phe", "nach", "dem", "Fein\u00b7de", "zie\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So wohl thut mir das Reuen,", "tokens": ["So", "wohl", "thut", "mir", "das", "Reu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df Schmerzen, Wunden, Thr\u00e4nen mich jetzt freuen.", "tokens": ["Da\u00df", "Schmer\u00b7zen", ",", "Wun\u00b7den", ",", "Thr\u00e4\u00b7nen", "mich", "jetzt", "freu\u00b7en", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "$,", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Zum Paradiesesgarten", "tokens": ["Zum", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7gar\u00b7ten"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hinauf, hinauf, erklimmt ihn ihr Ges\u00e4nge!", "tokens": ["Hin\u00b7auf", ",", "hin\u00b7auf", ",", "er\u00b7klimmt", "ihn", "ihr", "Ge\u00b7s\u00e4n\u00b7ge", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ermuthigt im Gedr\u00e4nge", "tokens": ["Er\u00b7mut\u00b7higt", "im", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.4": {"text": "Seht dort die Engelschaaren eurer warten.", "tokens": ["Seht", "dort", "die", "En\u00b7gel\u00b7schaa\u00b7ren", "eu\u00b7rer", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wes Auge schaut hernieder", "tokens": ["Wes", "Au\u00b7ge", "schaut", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und blizt mir Lieb und Furcht in meine Lieder?", "tokens": ["Und", "blizt", "mir", "Lieb", "und", "Furcht", "in", "mei\u00b7ne", "Lie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Des Auges ernstes Blicken", "tokens": ["Des", "Au\u00b7ges", "erns\u00b7tes", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Macht mich in stummer Freudenangst vergehen;", "tokens": ["Macht", "mich", "in", "stum\u00b7mer", "Freu\u00b7de\u00b7nangst", "ver\u00b7ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O wunders\u00fc\u00dfe Wehen,", "tokens": ["O", "wun\u00b7der\u00b7s\u00fc\u00b7\u00dfe", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Euch bricht mein Herz in Leid und im Entz\u00fccken!", "tokens": ["Euch", "bricht", "mein", "Herz", "in", "Leid", "und", "im", "Ent\u00b7z\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Hosannah dir zu singen", "tokens": ["Ho\u00b7san\u00b7nah", "dir", "zu", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wird dort vielleicht als Engel mir gelingen.", "tokens": ["Wird", "dort", "viel\u00b7leicht", "als", "En\u00b7gel", "mir", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "KOUS", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}