{"dta.poem.16228": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "282.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Unzufriedenheit mit deinem Thun, die Reue,", "tokens": ["Die", "Un\u00b7zu\u00b7frie\u00b7den\u00b7heit", "mit", "dei\u00b7nem", "Thun", ",", "die", "Reu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hilft dazu, da\u00df sich nicht das falsche Thun erneue.", "tokens": ["Hilft", "da\u00b7zu", ",", "da\u00df", "sich", "nicht", "das", "fal\u00b7sche", "Thun", "er\u00b7neu\u00b7e", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "$,", "KOUS", "PRF", "PTKNEG", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Allein zum rechten Thun hilft sie dir wenig nur:", "tokens": ["Al\u00b7lein", "zum", "rech\u00b7ten", "Thun", "hilft", "sie", "dir", "we\u00b7nig", "nur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVFIN", "PPER", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Reue reutet aus, doch wer bestellt die Flur?", "tokens": ["Die", "Reu\u00b7e", "reu\u00b7tet", "aus", ",", "doch", "wer", "be\u00b7stellt", "die", "Flur", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "KON", "PWS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Um deines Herzen Flur gedeihlich zu bestellen,", "tokens": ["Um", "dei\u00b7nes", "Her\u00b7zen", "Flur", "ge\u00b7deih\u00b7lich", "zu", "be\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mu\u00df Selbstvertraun, gen\u00e4hrt von Gottvertraun, dich schwellen.", "tokens": ["Mu\u00df", "Selbst\u00b7ver\u00b7traun", ",", "ge\u00b7n\u00e4hrt", "von", "Gott\u00b7ver\u00b7traun", ",", "dich", "schwel\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "NE", "$,", "VVPP", "APPR", "NE", "$,", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}