{"textgrid.poem.26601": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aber dennoch selig", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aber dennoch selig", "tokens": ["A\u00b7ber", "den\u00b7noch", "se\u00b7lig"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sind die fr\u00fche Verstorbenen!", "tokens": ["Sind", "die", "fr\u00fc\u00b7he", "Ver\u00b7stor\u00b7be\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Nur Thorheit beklagt es,", "tokens": ["Nur", "Thor\u00b7heit", "be\u00b7klagt", "es", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPER", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Da\u00df sie dahingegangen,", "tokens": ["Da\u00df", "sie", "da\u00b7hin\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ohne des Daseins Lust und Gl\u00fcck", "tokens": ["Oh\u00b7ne", "des", "Da\u00b7seins", "Lust", "und", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Ausgekostet \u2013", "tokens": ["Aus\u00b7ge\u00b7kos\u00b7tet", "\u2013"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Ohne vollbracht zu haben,", "tokens": ["Oh\u00b7ne", "voll\u00b7bracht", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Was sie hoffnungsvoll angestrebt.", "tokens": ["Was", "sie", "hoff\u00b7nungs\u00b7voll", "an\u00b7ge\u00b7strebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Denn unergr\u00fcndlich", "tokens": ["Denn", "un\u00b7er\u00b7gr\u00fcnd\u00b7lich"], "token_info": ["word", "word"], "pos": ["KON", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Ist Menschenschicksal \u2013", "tokens": ["Ist", "Men\u00b7schen\u00b7schick\u00b7sal", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Und keiner wei\u00df,", "tokens": ["Und", "kei\u00b7ner", "wei\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Was der morgende Tag ihm bringt.", "tokens": ["Was", "der", "mor\u00b7gen\u00b7de", "Tag", "ihm", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Unabl\u00e4ssig lauert Unheil,", "tokens": ["Un\u00b7ab\u00b7l\u00e4s\u00b7sig", "lau\u00b7ert", "Un\u00b7heil", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Lauert der Entt\u00e4uschung Schmerz", "tokens": ["Lau\u00b7ert", "der", "Ent\u00b7t\u00e4u\u00b7schung", "Schmerz"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Und die Gefahr, schuldig zu werden ...", "tokens": ["Und", "die", "Ge\u00b7fahr", ",", "schul\u00b7dig", "zu", "wer\u00b7den", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Selig daher,", "tokens": ["Se\u00b7lig", "da\u00b7her", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PAV", "$,"], "meter": "+-++", "measure": "unknown.measure.tri"}, "line.2": {"text": "Dreimal selig", "tokens": ["Drei\u00b7mal", "se\u00b7lig"], "token_info": ["word", "word"], "pos": ["ADV", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Sind die fr\u00fche Verstorbenen.", "tokens": ["Sind", "die", "fr\u00fc\u00b7he", "Ver\u00b7stor\u00b7be\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}}}}