{"dta.poem.5283": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der gl\u00e4ntzenden G\u00e4rten bezauberndes Lust-Revier,", "tokens": ["Der", "gl\u00e4nt\u00b7zen\u00b7den", "G\u00e4r\u00b7ten", "be\u00b7zau\u00b7bern\u00b7des", "Lust\u00b7Re\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.2": {"text": "In welchem jetzt alles verherrlichet bl\u00fchet,", "tokens": ["In", "wel\u00b7chem", "jetzt", "al\u00b7les", "ver\u00b7herr\u00b7li\u00b7chet", "bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "PIS", "VVFIN", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Beflammet die Blicke mit feuriger Farben Zier,", "tokens": ["Be\u00b7flam\u00b7met", "die", "Bli\u00b7cke", "mit", "feu\u00b7ri\u00b7ger", "Far\u00b7ben", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.4": {"text": "Da alles fast weniger gl\u00e4ntzet, als gl\u00fchet.", "tokens": ["Da", "al\u00b7les", "fast", "we\u00b7ni\u00b7ger", "gl\u00e4nt\u00b7zet", ",", "als", "gl\u00fc\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "VVFIN", "$,", "KOUS", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Indem nun, im Fr\u00fchling, in L\u00fcften und in der Fluth,", "tokens": ["In\u00b7dem", "nun", ",", "im", "Fr\u00fch\u00b7ling", ",", "in", "L\u00fcf\u00b7ten", "und", "in", "der", "Fluth", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "APPRART", "NN", "$,", "APPR", "NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "--+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "In Th\u00e4lern, auf Bergen und Fl\u00e4chen der Erden,", "tokens": ["In", "Th\u00e4\u00b7lern", ",", "auf", "Ber\u00b7gen", "und", "Fl\u00e4\u00b7chen", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Der herrliche Sch\u00f6pfer unz\u00e4hliche Wunder thut;", "tokens": ["Der", "herr\u00b7li\u00b7che", "Sch\u00f6p\u00b7fer", "un\u00b7z\u00e4h\u00b7li\u00b7che", "Wun\u00b7der", "thut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "So lasst uns uns freuen, um danckbar zu werden!", "tokens": ["So", "lasst", "uns", "uns", "freu\u00b7en", ",", "um", "dan\u00b7ck\u00b7bar", "zu", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVINF", "$,", "KOUI", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Es strahlet, durch G\u00f6ttliches Wollen, das Sonnen-Licht;", "tokens": ["Es", "strah\u00b7let", ",", "durch", "G\u00f6tt\u00b7li\u00b7ches", "Wol\u00b7len", ",", "das", "Son\u00b7nen\u00b7Licht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.10": {"text": "Die C\u00f6rper sind sichtbar; GOtt schenckt uns die Augen:", "tokens": ["Die", "C\u00f6r\u00b7per", "sind", "sicht\u00b7bar", ";", "Gott", "schenckt", "uns", "die", "Au\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Wofern nun die Menschheit so tr\u00e4g ist, und sieht sie nicht;", "tokens": ["Wo\u00b7fern", "nun", "die", "Menschheit", "so", "tr\u00e4g", "ist", ",", "und", "sieht", "sie", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "ADJD", "VAFIN", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Was kan doch den Fehl zu entschuldigen taugen?", "tokens": ["Was", "kan", "doch", "den", "Fehl", "zu", "ent\u00b7schul\u00b7di\u00b7gen", "tau\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.13": {"text": "Drum, weil ich den Sch\u00f6pfer nicht anders erheben kann,", "tokens": ["Drum", ",", "weil", "ich", "den", "Sch\u00f6p\u00b7fer", "nicht", "an\u00b7ders", "er\u00b7he\u00b7ben", "kann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PPER", "ART", "NN", "PTKNEG", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.14": {"text": "Als wenn ich sein Wircken empfind und erzehle;", "tokens": ["Als", "wenn", "ich", "sein", "Wir\u00b7cken", "emp\u00b7find", "und", "er\u00b7zeh\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PPOSAT", "NN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.15": {"text": "So seh ich betrachtend, mit Freuden die Wunder an,", "tokens": ["So", "seh", "ich", "be\u00b7trach\u00b7tend", ",", "mit", "Freu\u00b7den", "die", "Wun\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$,", "APPR", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.16": {"text": "Und opfer\u2019 ihm meine bewundernde Seele.", "tokens": ["Und", "op\u00b7fer'", "ihm", "mei\u00b7ne", "be\u00b7wun\u00b7dern\u00b7de", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}