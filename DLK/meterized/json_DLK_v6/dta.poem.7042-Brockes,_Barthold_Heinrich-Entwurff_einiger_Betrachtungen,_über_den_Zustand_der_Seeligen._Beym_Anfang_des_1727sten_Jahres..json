{"dta.poem.7042": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die ungeheure Last, das schreckliche Gewichte", "tokens": ["Die", "un\u00b7ge\u00b7heu\u00b7re", "Last", ",", "das", "schreck\u00b7li\u00b7che", "Ge\u00b7wich\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Erd- und Wasser-Welt, hemmt itzt den strengen", "tokens": ["Der", "Erd", "und", "Was\u00b7ser\u00b7Welt", ",", "hemmt", "itzt", "den", "stren\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "TRUNC", "KON", "NN", "$,", "VVFIN", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Lauff.", "tokens": ["Lauff", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Jhr rascher Schwang, der so gewaltig, h\u00f6ret auf,", "tokens": ["Ihr", "ra\u00b7scher", "Schwang", ",", "der", "so", "ge\u00b7wal\u00b7tig", ",", "h\u00f6\u00b7ret", "auf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJD", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es kehrt sich unsre Fl\u00e4ch\u2019 aufs neu zum Sonnen-Lichte.", "tokens": ["Es", "kehrt", "sich", "uns\u00b7re", "Fl\u00e4ch'", "aufs", "neu", "zum", "Son\u00b7nen\u00b7Lich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PPOSAT", "NN", "APPRART", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wer hemmt nun diese Flucht? Wer h\u00e4lt die grause Schwere", "tokens": ["Wer", "hemmt", "nun", "die\u00b7se", "Flucht", "?", "Wer", "h\u00e4lt", "die", "grau\u00b7se", "Schwe\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "PDAT", "NN", "$.", "PWS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Erde, samt der Last der Meere,", "tokens": ["Der", "Er\u00b7de", ",", "samt", "der", "Last", "der", "Mee\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So Nordwerts sich bi\u00dfher gedreht, im Rennen an?", "tokens": ["So", "Nord\u00b7werts", "sich", "bi\u00df\u00b7her", "ge\u00b7dreht", ",", "im", "Ren\u00b7nen", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PRF", "ADV", "VVFIN", "$,", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wer bringt den schnellen Druck zur Aenderung, zur Stille?", "tokens": ["Wer", "bringt", "den", "schnel\u00b7len", "Druck", "zur", "A\u00b7en\u00b7de\u00b7rung", ",", "zur", "Stil\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Wer ists, der dieses w\u00fcrcken kann", "tokens": ["Wer", "ists", ",", "der", "die\u00b7ses", "w\u00fcr\u00b7cken", "kann"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "PDAT", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Als Du, unendlichs ALL, allein?", "tokens": ["Als", "Du", ",", "un\u00b7end\u00b7lichs", "AlL", ",", "al\u00b7lein", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJA", "NN", "$,", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ach GOTT! ach la\u00df uns doch dadurch erweckt, ger\u00fchret,", "tokens": ["Ach", "GoTT", "!", "ach", "la\u00df", "uns", "doch", "da\u00b7durch", "er\u00b7weckt", ",", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "NE", "$.", "XY", "VVIMP", "PPER", "ADV", "PAV", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und aus der alten Bahn der Schl\u00e4ffrigkeit gef\u00fchret,", "tokens": ["Und", "aus", "der", "al\u00b7ten", "Bahn", "der", "Schl\u00e4f\u00b7frig\u00b7keit", "ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Zu Deinem Ruhm aufs neu, o HERR, geleitet seyn!", "tokens": ["Zu", "Dei\u00b7nem", "Ruhm", "aufs", "neu", ",", "o", "HeRR", ",", "ge\u00b7lei\u00b7tet", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "ADJD", "$,", "FM", "NN", "$,", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wir n\u00e4hern uns der Sonnen Lebens-Schein,", "tokens": ["Wir", "n\u00e4\u00b7hern", "uns", "der", "Son\u00b7nen", "Le\u00b7bens\u00b7Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Es tritt dadurch zugleich ein Neues Jahr herein.", "tokens": ["Es", "tritt", "da\u00b7durch", "zu\u00b7gleich", "ein", "Neu\u00b7es", "Jahr", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ach! lasst uns denn, da wir zur Sonn\u2019 uns wieder lencken,", "tokens": ["Ach", "!", "lasst", "uns", "denn", ",", "da", "wir", "zur", "Sonn'", "uns", "wie\u00b7der", "len\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "APPRART", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Doch an der Sonnen SONN\u2019 in Ehrfurcht itzt gedencken!", "tokens": ["Doch", "an", "der", "Son\u00b7nen", "SoNN'", "in", "Ehr\u00b7furcht", "itzt", "ge\u00b7den\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NE", "APPR", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}