{"textgrid.poem.33086": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es trat Allt\u00e4glichkeit", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es trat Allt\u00e4glichkeit", "tokens": ["Es", "trat", "All\u00b7t\u00e4g\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zur Poesie:", "tokens": ["Zur", "Poe\u00b7sie", ":"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "\u00bbgib mir dein buntes Kleid!\u00ab", "tokens": ["\u00bb", "gib", "mir", "dein", "bun\u00b7tes", "Kleid", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "PPER", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sprach herrisch sie,", "tokens": ["Sprach", "her\u00b7risch", "sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "\u00bbgib aus den Locken mir", "tokens": ["\u00bb", "gib", "aus", "den", "Lo\u00b7cken", "mir"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVIMP", "APPR", "ART", "NN", "PPER"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Den gold'nen Kranz,", "tokens": ["Den", "gold'\u00b7nen", "Kranz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Nur die poet'sche Zier", "tokens": ["Nur", "die", "po\u00b7et'\u00b7sche", "Zier"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Verleiht dir Glanz.\u00ab", "tokens": ["Ver\u00b7leiht", "dir", "Glanz", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Die Gute, mild und zart,", "tokens": ["Die", "Gu\u00b7te", ",", "mild", "und", "zart", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In ihre Hand", "tokens": ["In", "ih\u00b7re", "Hand"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Gab mit der holden Art", "tokens": ["Gab", "mit", "der", "hol\u00b7den", "Art"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Kranz und Gewand;", "tokens": ["Kranz", "und", "Ge\u00b7wand", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Die Andre h\u00fcllt sich d'rein", "tokens": ["Die", "And\u00b7re", "h\u00fcllt", "sich", "d'\u00b7rein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "PRF", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Mit eck'ger Hast \u2013", "tokens": ["Mit", "eck'\u00b7ger", "Hast", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wie Blei zum Edelstein", "tokens": ["Wie", "Blei", "zum", "E\u00b7del\u00b7stein"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Es f\u00fcr sie pa\u00dft.", "tokens": ["Es", "f\u00fcr", "sie", "pa\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Dann sprach noch weiter sie:", "tokens": ["Dann", "sprach", "noch", "wei\u00b7ter", "sie", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbnimm du den Pflug!", "tokens": ["\u00bb", "nimm", "du", "den", "Pflug", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ich hatte Plag' und M\u00fch'", "tokens": ["Ich", "hat\u00b7te", "Plag'", "und", "M\u00fch'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NE", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Jetzt lang' genug;", "tokens": ["Jetzt", "lang'", "ge\u00b7nug", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Reich mir die Leier her,", "tokens": ["Reich", "mir", "die", "Lei\u00b7er", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Arbeite du \u2013", "tokens": ["Ar\u00b7bei\u00b7te", "du", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Zu singen ist nicht schwer", "tokens": ["Zu", "sin\u00b7gen", "ist", "nicht", "schwer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "VAFIN", "PTKNEG", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "In guter Ruh!\u00ab", "tokens": ["In", "gu\u00b7ter", "Ruh", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Sie r\u00fchrt die Saiten an", "tokens": ["Sie", "r\u00fchrt", "die", "Sai\u00b7ten", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mit rauher Hand,", "tokens": ["Mit", "rau\u00b7her", "Hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Gefild und Waldesplan", "tokens": ["Ge\u00b7fild", "und", "Wal\u00b7des\u00b7plan"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Erstarrend stand,", "tokens": ["Er\u00b7star\u00b7rend", "stand", ","], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Der Vogel fliegt erschreckt", "tokens": ["Der", "Vo\u00b7gel", "fliegt", "er\u00b7schreckt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Vom Ast empor,", "tokens": ["Vom", "Ast", "em\u00b7por", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Der Hirtenbube deckt", "tokens": ["Der", "Hir\u00b7ten\u00b7bu\u00b7be", "deckt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Sein lauschend Ohr.", "tokens": ["Sein", "lau\u00b7schend", "Ohr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Doch sieh', die Himmelsmaid", "tokens": ["Doch", "sieh'", ",", "die", "Him\u00b7mels\u00b7maid"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Voll Majest\u00e4t,", "tokens": ["Voll", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mit Blicken strahlend weit,", "tokens": ["Mit", "Bli\u00b7cken", "strah\u00b7lend", "weit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Jetzt vor ihr steht,", "tokens": ["Jetzt", "vor", "ihr", "steht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Sie spricht: \u00bbAllt\u00e4glichkeit,", "tokens": ["Sie", "spricht", ":", "\u00bb", "All\u00b7t\u00e4g\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Erkenne dich!", "tokens": ["Er\u00b7ken\u00b7ne", "dich", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Dich macht nicht Kranz noch Kleid", "tokens": ["Dich", "macht", "nicht", "Kranz", "noch", "Kleid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "ADV", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Zu dem, was ich.", "tokens": ["Zu", "dem", ",", "was", "ich", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PWS", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Dein Thun veracht' ich nie,", "tokens": ["Dein", "Thun", "ver\u00b7acht'", "ich", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Es braucht die Welt", "tokens": ["Es", "braucht", "die", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Uns beide; bleib' wo sie", "tokens": ["Uns", "bei\u00b7de", ";", "bleib'", "wo", "sie"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PIS", "$.", "VVFIN", "PWAV", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dich hingestellt!", "tokens": ["Dich", "hin\u00b7ge\u00b7stellt", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Du denkst im M\u00fc\u00dfiggang", "tokens": ["Du", "denkst", "im", "M\u00fc\u00b7\u00dfig\u00b7gang"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ging' tr\u00e4g' ich her \u2013", "tokens": ["Ging'", "tr\u00e4g'", "ich", "her", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Wie keiner mehr.", "tokens": ["Wie", "kei\u00b7ner", "mehr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Ich baue fr\u00fch und spat", "tokens": ["Ich", "bau\u00b7e", "fr\u00fch", "und", "spat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Des Geistes Feld,", "tokens": ["Des", "Geis\u00b7tes", "Feld", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der Zukunft gold'ne Saat", "tokens": ["Der", "Zu\u00b7kunft", "gold'\u00b7ne", "Saat"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mein Flei\u00df bestellt.", "tokens": ["Mein", "Flei\u00df", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und, wenn ich tr\u00e4umend geh',", "tokens": ["Und", ",", "wenn", "ich", "tr\u00e4u\u00b7mend", "geh'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ein Schattenbild,", "tokens": ["Ein", "Schat\u00b7ten\u00b7bild", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "F\u00fcr tausendfaches Weh", "tokens": ["F\u00fcr", "tau\u00b7send\u00b7fa\u00b7ches", "Weh"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Mir Trost entquillt.", "tokens": ["Mir", "Trost", "ent\u00b7quillt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Und meiner Seele Leid", "tokens": ["Und", "mei\u00b7ner", "See\u00b7le", "Leid"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das ahnst du nie,", "tokens": ["Das", "ahnst", "du", "nie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Weil du Allt\u00e4glichkeit,", "tokens": ["Weil", "du", "All\u00b7t\u00e4g\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich Poesie!\u00ab", "tokens": ["Ich", "Poe\u00b7sie", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["PPER", "NN", "$.", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Dann hob ihr Fl\u00fcgelpaar", "tokens": ["Dann", "hob", "ihr", "Fl\u00fc\u00b7gel\u00b7paar"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sie leise auf,", "tokens": ["Sie", "lei\u00b7se", "auf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wo ihre Heimath war,", "tokens": ["Wo", "ih\u00b7re", "Hei\u00b7math", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Schwebt sie hinauf.", "tokens": ["Schwebt", "sie", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}