{"textgrid.poem.60147": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gott ist noch Gott, in Schauervoller Stille", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott ist noch Gott, in Schauervoller Stille", "tokens": ["Gott", "ist", "noch", "Gott", ",", "in", "Schau\u00b7er\u00b7vol\u00b7ler", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "NN", "$,", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nenn ich o Freund, ihn wunderbahr,", "tokens": ["Nenn", "ich", "o", "Freund", ",", "ihn", "wun\u00b7der\u00b7bahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "FM", "NN", "$,", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Krieg dr\u00fcckt das Land, er giebt uns Brod die F\u00fclle", "tokens": ["Krieg", "dr\u00fcckt", "das", "Land", ",", "er", "giebt", "uns", "Brod", "die", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und seine G\u00fcte cr\u00f6nt das Jahr.", "tokens": ["Und", "sei\u00b7ne", "G\u00fc\u00b7te", "cr\u00f6nt", "das", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Feind verschlang mit nie erf\u00fclltem Schlunde", "tokens": ["Der", "Feind", "ver\u00b7schlang", "mit", "nie", "er\u00b7f\u00fcll\u00b7tem", "Schlun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Drey Erndten, floh vor Friedrichs Zorn,", "tokens": ["Drey", "Ernd\u00b7ten", ",", "floh", "vor", "Fried\u00b7richs", "Zorn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "VVFIN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Trug unsern lezten Bissen noch im Munde,", "tokens": ["Trug", "un\u00b7sern", "lez\u00b7ten", "Bis\u00b7sen", "noch", "im", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch haben wir noch Oel und Korn!", "tokens": ["Doch", "ha\u00b7ben", "wir", "noch", "O\u00b7el", "und", "Korn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Er ri\u00df des Landmanns lezte Leinwandsh\u00fclle", "tokens": ["Er", "ri\u00df", "des", "Land\u00b7manns", "lez\u00b7te", "Lein\u00b7wands\u00b7h\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihm grimmig von der Schulter ab;", "tokens": ["Ihm", "grim\u00b7mig", "von", "der", "Schul\u00b7ter", "ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch war ein Gott, der aus des Seegens-F\u00fclle,", "tokens": ["Doch", "war", "ein", "Gott", ",", "der", "aus", "des", "See\u00b7gens\u00b7F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Armen Brod und Kleider gab.", "tokens": ["Den", "Ar\u00b7men", "Brod", "und", "Klei\u00b7der", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Viel tausende sind durch des Schwerdtes Sch\u00e4rfe", "tokens": ["Viel", "tau\u00b7sen\u00b7de", "sind", "durch", "des", "Schwerd\u00b7tes", "Sch\u00e4r\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gem\u00e4het, so wie Gra\u00df im Thal!", "tokens": ["Ge\u00b7m\u00e4\u00b7het", ",", "so", "wie", "Gra\u00df", "im", "Thal", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "KOKOM", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Oft that der Herr, als ob er uns verwerfe,", "tokens": ["Oft", "that", "der", "Herr", ",", "als", "ob", "er", "uns", "ver\u00b7wer\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KOKOM", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dem Verderber Herz befahl!", "tokens": ["Und", "dem", "Ver\u00b7der\u00b7ber", "Herz", "be\u00b7fahl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Doch leben wir, doch ziehen unsre Heere", "tokens": ["Doch", "le\u00b7ben", "wir", ",", "doch", "zie\u00b7hen", "uns\u00b7re", "Hee\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Kraft bewafnet aus zum Streit!", "tokens": ["Mit", "Kraft", "be\u00b7waf\u00b7net", "aus", "zum", "Streit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Knabe lacht des Riesen Schild und Speere", "tokens": ["Ein", "Kna\u00b7be", "lacht", "des", "Rie\u00b7sen", "Schild", "und", "Spee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und schl\u00e4gt ihn, wenn es Gott gebeut!", "tokens": ["Und", "schl\u00e4gt", "ihn", ",", "wenn", "es", "Gott", "ge\u00b7beut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Gott hilft uns die wir seiner H\u00fclfe warten,", "tokens": ["Gott", "hilft", "uns", "die", "wir", "sei\u00b7ner", "H\u00fcl\u00b7fe", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und seine H\u00fclfe ist uns nah;", "tokens": ["Und", "sei\u00b7ne", "H\u00fcl\u00b7fe", "ist", "uns", "nah", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir sehn nach ihr hinauf, Freund! wie dein Garten", "tokens": ["Wir", "sehn", "nach", "ihr", "hin\u00b7auf", ",", "Freund", "!", "wie", "dein", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$,", "NN", "$.", "PWAV", "PPOSAT", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Empor nach Regenwolken sah.", "tokens": ["Em\u00b7por", "nach", "Re\u00b7gen\u00b7wol\u00b7ken", "sah", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Als ihm in dreymahl sieben langen Tagen", "tokens": ["Als", "ihm", "in", "drey\u00b7mahl", "sie\u00b7ben", "lan\u00b7gen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADV", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Sonne jeden Saft benahm,", "tokens": ["Die", "Son\u00b7ne", "je\u00b7den", "Saft", "be\u00b7nahm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da bracht ein Sturm den vollen Schlauch getragen,", "tokens": ["Da", "bracht", "ein", "Sturm", "den", "vol\u00b7len", "Schlauch", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Feld und Garten tr\u00e4nken kam.", "tokens": ["Der", "Feld", "und", "Gar\u00b7ten", "tr\u00e4n\u00b7ken", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Die Blitze creuzten ohne Donnerschl\u00e4ge,", "tokens": ["Die", "Blit\u00b7ze", "creuz\u00b7ten", "oh\u00b7ne", "Don\u00b7ner\u00b7schl\u00e4\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gott sprach, und das Gewitter wich.", "tokens": ["Gott", "sprach", ",", "und", "das", "Ge\u00b7wit\u00b7ter", "wich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er spricht so zu des Meeres Toben: lege", "tokens": ["Er", "spricht", "so", "zu", "des", "Mee\u00b7res", "To\u00b7ben", ":", "le\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "NN", "$.", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hier, stolze Welle, lege dich!", "tokens": ["Hier", ",", "stol\u00b7ze", "Wel\u00b7le", ",", "le\u00b7ge", "dich", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "So spricht er zu des Krieges Wetterwolke", "tokens": ["So", "spricht", "er", "zu", "des", "Krie\u00b7ges", "Wet\u00b7ter\u00b7wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sie gehorcht ihm, wie das Meer!", "tokens": ["Und", "sie", "ge\u00b7horcht", "ihm", ",", "wie", "das", "Meer", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann sing ich ihm. So sang vor ihrem Volke", "tokens": ["Dann", "sing", "ich", "ihm", ".", "So", "sang", "vor", "ih\u00b7rem", "Vol\u00b7ke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$.", "ADV", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Einst Mirjam Gottes Thaten her!", "tokens": ["Einst", "Mir\u00b7jam", "Got\u00b7tes", "Tha\u00b7ten", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}