{"dta.poem.23841": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Der Tod des ungerechten Geitzhalses.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Den Harpax/ welcher sich zum reichen Mann gelogen/", "tokens": ["Den", "Har\u00b7pax", "/", "wel\u00b7cher", "sich", "zum", "rei\u00b7chen", "Mann", "ge\u00b7lo\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$(", "PRELS", "PRF", "APPRART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und selten einen Spruch im Richter-Amt gethan/", "tokens": ["Und", "sel\u00b7ten", "ei\u00b7nen", "Spruch", "im", "Rich\u00b7ter\u00b7Amt", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den er nicht nach dem Werth der Gaben abgewogen/", "tokens": ["Den", "er", "nicht", "nach", "dem", "Werth", "der", "Ga\u00b7ben", "ab\u00b7ge\u00b7wo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Den griff vor kurtzer Zeit ein brennend Fieber an.", "tokens": ["Den", "griff", "vor", "kurt\u00b7zer", "Zeit", "ein", "bren\u00b7nend", "Fie\u00b7ber", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "ART", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die weil es aber fand gar wenig anzuz\u00fcnden/", "tokens": ["Die", "weil", "es", "a\u00b7ber", "fand", "gar", "we\u00b7nig", "an\u00b7zu\u00b7z\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "PPER", "ADV", "VVFIN", "ADV", "PIS", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Imdem der schn\u00f6de Geitz das meiste weggezehrt/", "tokens": ["Im\u00b7dem", "der", "schn\u00f6\u00b7de", "Geitz", "das", "meis\u00b7te", "weg\u00b7ge\u00b7zehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "ADJA", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Kroch es der Flam\u0303e gleich/ die auch bey starcken Winden", "tokens": ["Kroch", "es", "der", "Flam\u0303e", "gleich", "/", "die", "auch", "bey", "star\u00b7cken", "Win\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "ART", "NN", "ADV", "$(", "PRELS", "ADV", "APPR", "ADJA", "NN"], "meter": "+---+-+-+-+-", "measure": "dactylic.init"}, "line.8": {"text": "Nur langsam durch den Sand verwachsner Aecker", "tokens": ["Nur", "lang\u00b7sam", "durch", "den", "Sand", "ver\u00b7wachs\u00b7ner", "A\u00b7e\u00b7cker"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.9": {"text": "Vermeynest du/ mein Freund/ da\u00df dieses ihm verdrossen?", "tokens": ["Ver\u00b7mey\u00b7nest", "du", "/", "mein", "Freund", "/", "da\u00df", "die\u00b7ses", "ihm", "ver\u00b7dros\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "PPOSAT", "NN", "$(", "KOUS", "PDAT", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "O nein! der weise Mann braucht die Gelegenheit;", "tokens": ["O", "nein", "!", "der", "wei\u00b7se", "Mann", "braucht", "die", "Ge\u00b7le\u00b7gen\u00b7heit", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$.", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Weil ihm kein Essen schmeckt/ ist seinen Hau\u00df-Genossen", "tokens": ["Weil", "ihm", "kein", "Es\u00b7sen", "schmeckt", "/", "ist", "sei\u00b7nen", "Hau\u00df\u00b7Ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$(", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Auch nur die halbe Kost/ ein Krancken-Mahl bereit.", "tokens": ["Auch", "nur", "die", "hal\u00b7be", "Kost", "/", "ein", "Kran\u00b7cken\u00b7Mahl", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$(", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Er l\u00e4\u00dft sie ingesamt vor seinen Stuhl bescheiden/", "tokens": ["Er", "l\u00e4\u00dft", "sie", "in\u00b7ge\u00b7samt", "vor", "sei\u00b7nen", "Stuhl", "be\u00b7schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und lehrt was M", "tokens": ["Und", "lehrt", "was", "M"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PWS", "NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Auch wie von Uberflu\u00df sein Magen m\u00fcsse leiden/", "tokens": ["Auch", "wie", "von", "U\u00b7ber\u00b7flu\u00df", "sein", "Ma\u00b7gen", "m\u00fcs\u00b7se", "lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "NN", "PPOSAT", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der gleichwol in geheim den falschen Kl\u00e4ger strafft.", "tokens": ["Der", "gleich\u00b7wol", "in", "ge\u00b7heim", "den", "fal\u00b7schen", "Kl\u00e4\u00b7ger", "strafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die Knechte deren Hertz sich noch nicht lo\u00df gerissen", "tokens": ["Die", "Knech\u00b7te", "de\u00b7ren", "Hertz", "sich", "noch", "nicht", "lo\u00df", "ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRELAT", "NN", "PRF", "ADV", "PTKNEG", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Von dem was Regung hei\u00dft/ die sehnen sich nach", "tokens": ["Von", "dem", "was", "Re\u00b7gung", "hei\u00dft", "/", "die", "seh\u00b7nen", "sich", "nach"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$(", "ART", "VVFIN", "PRF", "APPR"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.19": {"text": "Ihr Hunger der nichts wil von leeren Regeln wissen/", "tokens": ["Ihr", "Hun\u00b7ger", "der", "nichts", "wil", "von", "lee\u00b7ren", "Re\u00b7geln", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "PIS", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "W\u00fcnscht bald dem krancken Wirth/ Gesundheit/ bald", "tokens": ["W\u00fcnscht", "bald", "dem", "kran\u00b7cken", "Wirth", "/", "Ge\u00b7sund\u00b7heit", "/", "bald"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$(", "NN", "$(", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Die Schwachheit mehret sich; Doch Harpax wil nicht", "tokens": ["Die", "Schwach\u00b7heit", "meh\u00b7ret", "sich", ";", "Doch", "Har\u00b7pax", "wil", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "$.", "KON", "NE", "VMFIN", "PTKNEG"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.22": {"text": "Er denckt der Sache nach/ wie kl\u00e4glich da\u00df es sey:", "tokens": ["Er", "denckt", "der", "Sa\u00b7che", "nach", "/", "wie", "kl\u00e4g\u00b7lich", "da\u00df", "es", "sey", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "$(", "PWAV", "ADJD", "KOUS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Eh\u2019 als die Welt vergehn/ und andre lassen erben;", "tokens": ["Eh'", "als", "die", "Welt", "ver\u00b7gehn", "/", "und", "and\u00b7re", "las\u00b7sen", "er\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "VVINF", "$(", "KON", "PIS", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Drum suchet er den Raht der Seinigen herbey.", "tokens": ["Drum", "su\u00b7chet", "er", "den", "Raht", "der", "Sei\u00b7ni\u00b7gen", "her\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die wollen seine Gluth mit Kraut und E\u00dfig brechen/", "tokens": ["Die", "wol\u00b7len", "sei\u00b7ne", "Gluth", "mit", "Kraut", "und", "E\u00b7\u00dfig", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Er schl\u00e4gt es aber ab/ weil er die Kosten scheut/", "tokens": ["Er", "schl\u00e4gt", "es", "a\u00b7ber", "ab", "/", "weil", "er", "die", "Kos\u00b7ten", "scheut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und fragt nach jemand sonst/ der blo\u00df durch Seegen-", "tokens": ["Und", "fragt", "nach", "je\u00b7mand", "sonst", "/", "der", "blo\u00df", "durch", "See\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PIS", "ADV", "$(", "ART", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Aus Freundschafft/ ohne Geld/ und anders nichts/", "tokens": ["Aus", "Freund\u00b7schafft", "/", "oh\u00b7ne", "Geld", "/", "und", "an\u00b7ders", "nichts", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "APPR", "NN", "$(", "KON", "ADV", "PIS", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Der Anschlag geht nicht an: man mu\u00df zum Artzte schicken/", "tokens": ["Der", "An\u00b7schlag", "geht", "nicht", "an", ":", "man", "mu\u00df", "zum", "Artz\u00b7te", "schi\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "PTKVZ", "$.", "PIS", "VMFIN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der kommt/ der Krancke spricht: Es fehlt mir an der", "tokens": ["Der", "kommt", "/", "der", "Kran\u00b7cke", "spricht", ":", "Es", "fehlt", "mir", "an", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$(", "ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Und wird mir ener Flei\u00df in dieser Cur gel\u00fccken/", "tokens": ["Und", "wird", "mir", "e\u00b7ner", "Flei\u00df", "in", "die\u00b7ser", "Cur", "ge\u00b7l\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJA", "NN", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Sag\u2019 ich zur Danckbarkeit euch meine Dienste zu;", "tokens": ["Sag'", "ich", "zur", "Dan\u00b7ck\u00b7bar\u00b7keit", "euch", "mei\u00b7ne", "Diens\u00b7te", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.33": {"text": "Ich kenne euren Streit/ und wei\u00df vielleicht von allen", "tokens": ["Ich", "ken\u00b7ne", "eu\u00b7ren", "Streit", "/", "und", "wei\u00df", "viel\u00b7leicht", "von", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "KON", "VVFIN", "ADV", "APPR", "PIAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Mehr Nachricht als ihr selbst und bildet euch nur ein/", "tokens": ["Mehr", "Nach\u00b7richt", "als", "ihr", "selbst", "und", "bil\u00b7det", "euch", "nur", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KOUS", "PPER", "ADV", "KON", "VVFIN", "PPER", "ADV", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Da\u00df wider euch gewi\u00df das Urtheil werde fallen/", "tokens": ["Da\u00df", "wi\u00b7der", "euch", "ge\u00b7wi\u00df", "das", "Ur\u00b7theil", "wer\u00b7de", "fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ADV", "ART", "NN", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So bald ein anderer als ich wird Richter seyn.", "tokens": ["So", "bald", "ein", "an\u00b7de\u00b7rer", "als", "ich", "wird", "Rich\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "KOUS", "PPER", "VAFIN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Der Artzt dem dieses Wort durch Marck und Beine", "tokens": ["Der", "Artzt", "dem", "die\u00b7ses", "Wort", "durch", "Marck", "und", "Bei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "PDAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "F\u00e4llt auf den Krancken zu/ beklammert Pul\u00df und Hand/", "tokens": ["F\u00e4llt", "auf", "den", "Kran\u00b7cken", "zu", "/", "be\u00b7klam\u00b7mert", "Pul\u00df", "und", "Hand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKZU", "$(", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und weil sein eignes Blut/ aus Furcht und Hofnung", "tokens": ["Und", "weil", "sein", "eig\u00b7nes", "Blut", "/", "aus", "Furcht", "und", "Hof\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "NN", "$(", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "So setzt er aufs Papier mehr als ihm selbst bekandt.", "tokens": ["So", "setzt", "er", "aufs", "Pa\u00b7pier", "mehr", "als", "ihm", "selbst", "be\u00b7kandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ADV", "KOUS", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Eins kr\u00e4nckt den Harpax noch/ da\u00df er nichts von Pro-", "tokens": ["Eins", "kr\u00e4nckt", "den", "Har\u00b7pax", "noch", "/", "da\u00df", "er", "nichts", "von", "Pro"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NE", "ADV", "$(", "KOUS", "PPER", "PIS", "APPR