{"dta.poem.11540": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Jung Volkers Lied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Und die mich schwang im Kissen,", "tokens": ["Und", "die", "mich", "schwang", "im", "Kis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die war ein sch\u00f6n frech braunes Weib,", "tokens": ["Die", "war", "ein", "sch\u00f6n", "frech", "brau\u00b7nes", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wollte nichts vom Mannsvolk wissen.", "tokens": ["Woll\u00b7te", "nichts", "vom", "Manns\u00b7volk", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie scherzte nur und lachte laut,", "tokens": ["Sie", "scherz\u00b7te", "nur", "und", "lach\u00b7te", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und lie\u00df die Freier stehen:", "tokens": ["Und", "lie\u00df", "die", "Frei\u00b7er", "ste\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "M\u00f6cht' lieber seyn des Windes Braut,", "tokens": ["M\u00f6cht'", "lie\u00b7ber", "seyn", "des", "Win\u00b7des", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VAINF", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn in die Ehe gehen!", "tokens": ["Denn", "in", "die", "E\u00b7he", "ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da kam der Wind, da nahm der Wind", "tokens": ["Da", "kam", "der", "Wind", ",", "da", "nahm", "der", "Wind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als Buhle sie gefangen:", "tokens": ["Als", "Buh\u00b7le", "sie", "ge\u00b7fan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von dem hat sie ein lustig Kind", "tokens": ["Von", "dem", "hat", "sie", "ein", "lus\u00b7tig", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "VAFIN", "PPER", "ART", "ADJD", "NN"], "meter": "--+----+", "measure": "anapaest.init"}, "line.4": {"text": "In ihren Schoos empfangen.", "tokens": ["In", "ih\u00b7ren", "Schoos", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}