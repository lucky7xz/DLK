{"textgrid.poem.56756": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zw\u00f6lf stumme M\u00e4nner trugen mich", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zw\u00f6lf stumme M\u00e4nner trugen mich", "tokens": ["Zw\u00f6lf", "stum\u00b7me", "M\u00e4n\u00b7ner", "tru\u00b7gen", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "in einem Sarge von Kristall", "tokens": ["in", "ei\u00b7nem", "Sar\u00b7ge", "von", "Kris\u00b7tall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "hinunter an des Meeres Strand,", "tokens": ["hin\u00b7un\u00b7ter", "an", "des", "Mee\u00b7res", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "bis an der Brandung Rand hinaus.", "tokens": ["bis", "an", "der", "Bran\u00b7dung", "Rand", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So hatte ich's im Testament", "tokens": ["So", "hat\u00b7te", "ich's", "im", "Tes\u00b7ta\u00b7ment"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "bestimmt: Man bette meinen Leib", "tokens": ["be\u00b7stimmt", ":", "Man", "bet\u00b7te", "mei\u00b7nen", "Leib"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$.", "PIS", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "in einem Sarge von Kristall", "tokens": ["in", "ei\u00b7nem", "Sar\u00b7ge", "von", "Kris\u00b7tall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "und trage ihn der Ebbe nach,", "tokens": ["und", "tra\u00b7ge", "ihn", "der", "Eb\u00b7be", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "bis sie den tiefsten Stand erreicht.", "tokens": ["bis", "sie", "den", "tiefs\u00b7ten", "Stand", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der Sonne ungeheurer Gott", "tokens": ["Der", "Son\u00b7ne", "un\u00b7ge\u00b7heu\u00b7rer", "Gott"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "stand bis zum G\u00fcrtel schon im Meer:", "tokens": ["stand", "bis", "zum", "G\u00fcr\u00b7tel", "schon", "im", "Meer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPRART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "An seinem Glanze tr\u00e4nkte sich", "tokens": ["An", "sei\u00b7nem", "Glan\u00b7ze", "tr\u00e4nk\u00b7te", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "woll\u00fcstig noch einmal die Welt.", "tokens": ["wol\u00b7l\u00fcs\u00b7tig", "noch", "ein\u00b7mal", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Ich selber lag in rotem Schein", "tokens": ["Ich", "sel\u00b7ber", "lag", "in", "ro\u00b7tem", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "wie ein Gebilde aus Porphyr.", "tokens": ["wie", "ein", "Ge\u00b7bil\u00b7de", "aus", "Por\u00b7phyr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Da streckte katzengleich die Flut", "tokens": ["Da", "streck\u00b7te", "kat\u00b7zen\u00b7gleich", "die", "Flut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "die erste Welle nach mir aus.", "tokens": ["die", "ers\u00b7te", "Wel\u00b7le", "nach", "mir", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und ging zur\u00fcck und schob sich vor", "tokens": ["Und", "ging", "zu\u00b7r\u00fcck", "und", "schob", "sich", "vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "VVFIN", "PRF", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "und tastete am Sarg hinauf", "tokens": ["und", "tas\u00b7te\u00b7te", "am", "Sarg", "hin\u00b7auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "und wandte fl\u00fcsternd sich zur Flucht.", "tokens": ["und", "wand\u00b7te", "fl\u00fcs\u00b7ternd", "sich", "zur", "Flucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Und kam zur\u00fcck und griff und stie\u00df", "tokens": ["Und", "kam", "zu\u00b7r\u00fcck", "und", "griff", "und", "stie\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "und raunte lauter, warf sich k\u00fchn", "tokens": ["und", "raun\u00b7te", "lau\u00b7ter", ",", "warf", "sich", "k\u00fchn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "$,", "VVFIN", "PRF", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "dar\u00fcber, einmal, viele mal.", "tokens": ["da\u00b7r\u00fc\u00b7ber", ",", "ein\u00b7mal", ",", "vie\u00b7le", "mal", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "$,", "ADV", "$,", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Und blieb, und ihrer Macht gewi\u00df,", "tokens": ["Und", "blieb", ",", "und", "ih\u00b7rer", "Macht", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "umlief frohlockend sie mein Haus", "tokens": ["um\u00b7lief", "froh\u00b7lo\u00b7ckend", "sie", "mein", "Haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "und pochte dran und sch\u00e4umte auf,", "tokens": ["und", "poch\u00b7te", "dran", "und", "sch\u00e4um\u00b7te", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "als ihrer Faust es widerstand.", "tokens": ["als", "ih\u00b7rer", "Faust", "es", "wi\u00b7der\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Und hoch und h\u00f6her wuchs und wuchs", "tokens": ["Und", "hoch", "und", "h\u00f6\u00b7her", "wuchs", "und", "wuchs"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "das Wasser um mein gl\u00e4sern Schlo\u00df.", "tokens": ["das", "Was\u00b7ser", "um", "mein", "gl\u00e4\u00b7sern", "Schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Nun wankte es, als h\u00e4tt' ein Arm", "tokens": ["Nun", "wank\u00b7te", "es", ",", "als", "h\u00e4tt'", "ein", "Arm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOKOM", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "und noch ein Arm es rauh gepackt,", "tokens": ["und", "noch", "ein", "Arm", "es", "rauh", "ge\u00b7packt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "und scholl in allen Fugen, als", "tokens": ["und", "scholl", "in", "al\u00b7len", "Fu\u00b7gen", ",", "als"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADJD", "APPR", "PIAT", "NN", "$,", "KOUS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "ein Wellenberg auf ihm sich brach", "tokens": ["ein", "Wel\u00b7len\u00b7berg", "auf", "ihm", "sich", "brach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "PRF", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.34": {"text": "und es wie ein Lawinensturz", "tokens": ["und", "es", "wie", "ein", "La\u00b7wi\u00b7nen\u00b7sturz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "KOKOM", "ART", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.35": {"text": "umdr\u00f6hnte und versch\u00fcttete.", "tokens": ["um\u00b7dr\u00f6hn\u00b7te", "und", "ver\u00b7sch\u00fct\u00b7te\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Und langsam wich der nasse Sand.", "tokens": ["Und", "lang\u00b7sam", "wich", "der", "nas\u00b7se", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Und seitlings neigte sich der Sarg.", "tokens": ["Und", "seit\u00b7lings", "neig\u00b7te", "sich", "der", "Sarg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Und, unterw\u00fchlt und \u00fcbertobt,", "tokens": ["Und", ",", "un\u00b7ter\u00b7w\u00fchlt", "und", "\u00fc\u00b7ber\u00b7tobt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "begann er um sich selber sich", "tokens": ["be\u00b7gann", "er", "um", "sich", "sel\u00b7ber", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PRF", "ADV", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "schwerf\u00e4llig in die See zu drehn.", "tokens": ["schwer\u00b7f\u00e4l\u00b7lig", "in", "die", "See", "zu", "drehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Zu m\u00e4chtig, da\u00df die Brandung ihn", "tokens": ["Zu", "m\u00e4ch\u00b7tig", ",", "da\u00df", "die", "Bran\u00b7dung", "ihn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "$,", "KOUS", "ART", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "zum Strand zu schleppen h\u00e4tt' vermocht,", "tokens": ["zum", "Strand", "zu", "schlep\u00b7pen", "h\u00e4tt'", "ver\u00b7mocht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "vergrub er rollend sich und mich", "tokens": ["ver\u00b7grub", "er", "rol\u00b7lend", "sich", "und", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "PRF", "KON", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "in totenstillen Meeresgrund.", "tokens": ["in", "to\u00b7tens\u00b7til\u00b7len", "Mee\u00b7res\u00b7grund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "So lag ich denn, wie ich gewollt.", "tokens": ["So", "lag", "ich", "denn", ",", "wie", "ich", "ge\u00b7wollt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "VMPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Und dunkle Fische zogen still", "tokens": ["Und", "dunk\u00b7le", "Fi\u00b7sche", "zo\u00b7gen", "still"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "zu meinen H\u00e4upten hin und her.", "tokens": ["zu", "mei\u00b7nen", "H\u00e4up\u00b7ten", "hin", "und", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Und schwarzer Seetang \u00fcberschwamm", "tokens": ["Und", "schwar\u00b7zer", "See\u00b7tang", "\u00fc\u00b7bersc\u00b7hwamm"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPRART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "mein Grab. Und mein Bewu\u00dftsein schwand.", "tokens": ["mein", "Grab", ".", "Und", "mein", "Be\u00b7wu\u00df\u00b7tsein", "schwand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}