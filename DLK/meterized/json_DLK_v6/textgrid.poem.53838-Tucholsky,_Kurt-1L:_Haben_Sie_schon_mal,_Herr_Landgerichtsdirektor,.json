{"textgrid.poem.53838": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "1L: Haben Sie schon mal, Herr Landgerichtsdirektor,", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Haben Sie schon mal, Herr Landgerichtsdirektor,", "tokens": ["Ha\u00b7ben", "Sie", "schon", "mal", ",", "Herr", "Land\u00b7ge\u00b7richts\u00b7di\u00b7rek\u00b7tor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "$,", "NN", "NN", "$,"], "meter": "+---+-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "als Gefangener eine Nacht durchwacht?", "tokens": ["als", "Ge\u00b7fan\u00b7ge\u00b7ner", "ei\u00b7ne", "Nacht", "durch\u00b7wacht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Haben Sie schon mal vom Herrn Inspektor", "tokens": ["Ha\u00b7ben", "Sie", "schon", "mal", "vom", "Herrn", "Ins\u00b7pek\u00b7tor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "einen Tritt bekommen, da\u00df es kracht?", "tokens": ["ei\u00b7nen", "Tritt", "be\u00b7kom\u00b7men", ",", "da\u00df", "es", "kracht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Standen Sie schon mal, total versch\u00fcchtert,", "tokens": ["Stan\u00b7den", "Sie", "schon", "mal", ",", "to\u00b7tal", "ver\u00b7sch\u00fcch\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "vor dem Tisch, wo einer untersuchungsrichtert?", "tokens": ["vor", "dem", "Tisch", ",", "wo", "ei\u00b7ner", "un\u00b7ter\u00b7su\u00b7chungs\u00b7rich\u00b7tert", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "PIS", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Ihnen ist das bis zum Ruhestand", "tokens": ["Ih\u00b7nen", "ist", "das", "bis", "zum", "Ru\u00b7he\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PDS", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "dienstlich nicht bekannt.", "tokens": ["dienst\u00b7lich", "nicht", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Haben Sie schon mal acht hei\u00dfe Stunden", "tokens": ["Ha\u00b7ben", "Sie", "schon", "mal", "acht", "hei\u00b7\u00dfe", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "CARD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "ein Verh\u00f6r bestanden, das Sie nicht verstehn?", "tokens": ["ein", "Ver\u00b7h\u00f6r", "be\u00b7stan\u00b7den", ",", "das", "Sie", "nicht", "ver\u00b7stehn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Haben Sie schon mal die Nachtsekunden", "tokens": ["Ha\u00b7ben", "Sie", "schon", "mal", "die", "Nacht\u00b7se\u00b7kun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "an der Zellenwand vor\u00fcberlaufen sehn?", "tokens": ["an", "der", "Zel\u00b7len\u00b7wand", "vor\u00b7\u00fc\u00b7berl\u00b7au\u00b7fen", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Oben d\u00e4mmert ein Quadrat mit Gittern;", "tokens": ["O\u00b7ben", "d\u00e4m\u00b7mert", "ein", "Quad\u00b7rat", "mit", "Git\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "unten liegt ein Tier und darf nur zittern . . .", "tokens": ["un\u00b7ten", "liegt", "ein", "Tier", "und", "darf", "nur", "zit\u00b7tern", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VMFIN", "ADV", "VVINF", "$.", "$.", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Diese kleinen Z\u00fcge sind in Ihrem Stand", "tokens": ["Die\u00b7se", "klei\u00b7nen", "Z\u00fc\u00b7ge", "sind", "in", "Ih\u00b7rem", "Stand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "dienstlich nicht bekannt.", "tokens": ["dienst\u00b7lich", "nicht", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Aber Kommunistenjungen jagen,", "tokens": ["A\u00b7ber", "Kom\u00b7mu\u00b7nis\u00b7ten\u00b7jun\u00b7gen", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "wegen Hochverrat ins Loch gesperrt;", "tokens": ["we\u00b7gen", "Hoch\u00b7ver\u00b7rat", "ins", "Loch", "ge\u00b7sperrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "vor Gericht die Spitzel mild befragen,", "tokens": ["vor", "Ge\u00b7richt", "die", "Spit\u00b7zel", "mild", "be\u00b7fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Saal ger\u00e4umt, wenn eine Mutter pl\u00e4rrt;", "tokens": ["Saal", "ge\u00b7r\u00e4umt", ",", "wenn", "ei\u00b7ne", "Mut\u00b7ter", "pl\u00e4rrt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Femem\u00f6rder sanft verschoben,", "tokens": ["Fe\u00b7me\u00b7m\u00f6r\u00b7der", "sanft", "ver\u00b7scho\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "mit dem leisen Schleierblick nach oben,", "tokens": ["mit", "dem", "lei\u00b7sen", "Schlei\u00b7er\u00b7blick", "nach", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Existenzen glatt vernichtet,", "tokens": ["E\u00b7xis\u00b7ten\u00b7zen", "glatt", "ver\u00b7nich\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "die von Waffenpl\u00e4tzen was berichtet . . .", "tokens": ["die", "von", "Waf\u00b7fen\u00b7pl\u00e4t\u00b7zen", "was", "be\u00b7rich\u00b7tet", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "APPR", "NN", "PWS", "VVFIN", "$.", "$.", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Ungl\u00fcck rings verbreitet, Not und Qual \u2013:", "tokens": ["Un\u00b7gl\u00fcck", "rings", "ver\u00b7brei\u00b7tet", ",", "Not", "und", "Qual", "\u2013", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "VVPP", "$,", "NN", "KON", "NN", "$(", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Ja, das haben Sie schon mal \u2013!", "tokens": ["Ja", ",", "das", "ha\u00b7ben", "Sie", "schon", "mal", "\u2013", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "$,", "PDS", "VAFIN", "PPER", "ADV", "ADV", "$(", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}