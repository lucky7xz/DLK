{"dta.poem.1096": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "S.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ob es wol nicht ohn mag sein/", "tokens": ["Ob", "es", "wol", "nicht", "ohn", "mag", "sein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das sich Ladon nicht so fein/", "tokens": ["Das", "sich", "La\u00b7don", "nicht", "so", "fein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "NE", "PTKNEG", "ADV", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd wacker als du kan st\u00f6llen;", "tokens": ["Vnd", "wa\u00b7cker", "als", "du", "kan", "st\u00f6l\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Jsidoch seine sitsamkeit/", "tokens": ["Jsi\u00b7doch", "sei\u00b7ne", "sit\u00b7sam\u00b7keit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie deine geschwindigkeit/", "tokens": ["Wie", "dei\u00b7ne", "ge\u00b7schwin\u00b7dig\u00b7keit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Villeicht nicht so bald zuf\u00e4llen.", "tokens": ["Vil\u00b7leicht", "nicht", "so", "bald", "zu\u00b7f\u00e4l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "p