{"textgrid.poem.64847": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und glaubst du, weil im Weltgew\u00fchl", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und glaubst du, weil im Weltgew\u00fchl", "tokens": ["Und", "glaubst", "du", ",", "weil", "im", "Welt\u00b7ge\u00b7w\u00fchl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Mund dich nie genannt,", "tokens": ["Mein", "Mund", "dich", "nie", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df drum der Liebe Glutgef\u00fchl", "tokens": ["Da\u00df", "drum", "der", "Lie\u00b7be", "Glut\u00b7ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PAV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus meinem Busen schwand?", "tokens": ["Aus", "mei\u00b7nem", "Bu\u00b7sen", "schwand", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich sah dein Herz in fr\u00fch'rer Zeit", "tokens": ["Ich", "sah", "dein", "Herz", "in", "fr\u00fch'\u00b7rer", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vom Dolch des Grams verletzt,", "tokens": ["Vom", "Dolch", "des", "Grams", "ver\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und klaglos, wie du trugst dein Leid,", "tokens": ["Und", "klag\u00b7los", ",", "wie", "du", "trugst", "dein", "Leid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Will ich es tragen jetzt.", "tokens": ["Will", "ich", "es", "tra\u00b7gen", "jetzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zu heilig rein war unser Bund", "tokens": ["Zu", "hei\u00b7lig", "rein", "war", "un\u00b7ser", "Bund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "ADJD", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit seinem stillen Gl\u00fcck,", "tokens": ["Mit", "sei\u00b7nem", "stil\u00b7len", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Um frevelnd ihn zu geben kund", "tokens": ["Um", "fre\u00b7velnd", "ihn", "zu", "ge\u00b7ben", "kund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADJD", "PPER", "PTKZU", "VVINF", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Menge Sp\u00e4herblick.", "tokens": ["Der", "Men\u00b7ge", "Sp\u00e4\u00b7her\u00b7blick", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Verborgen trug ich in der Brust", "tokens": ["Ver\u00b7bor\u00b7gen", "trug", "ich", "in", "der", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Wonnen sonder Zahl;", "tokens": ["Die", "Won\u00b7nen", "son\u00b7der", "Zahl", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da Niemand schaute meine Lust,", "tokens": ["Da", "Nie\u00b7mand", "schau\u00b7te", "mei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Seh' Niemand meine Qual!", "tokens": ["Seh'", "Nie\u00b7mand", "mei\u00b7ne", "Qual", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Vergessen?! Nacht! sei Zeugin mir!", "tokens": ["Ver\u00b7ges\u00b7sen", "?!", "Nacht", "!", "sei", "Zeu\u00b7gin", "mir", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "NN", "$.", "VAFIN", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie lang ich meine Pein", "tokens": ["Wie", "lang", "ich", "mei\u00b7ne", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Getreulich wahre f\u00fcr und f\u00fcr,", "tokens": ["Ge\u00b7treu\u00b7lich", "wah\u00b7re", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das wei\u00dft nur du allein!", "tokens": ["Das", "wei\u00dft", "nur", "du", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So flie\u00dft der Strom bei Tag wohl sacht,", "tokens": ["So", "flie\u00dft", "der", "Strom", "bei", "Tag", "wohl", "sacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Klar spiegelnd Sonn' und Wald,", "tokens": ["Klar", "spie\u00b7gelnd", "Sonn'", "und", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dem in der schweigend stillen Nacht", "tokens": ["Dem", "in", "der", "schwei\u00b7gend", "stil\u00b7len", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Klagelaut enthallt.", "tokens": ["Ein", "Kla\u00b7ge\u00b7laut", "ent\u00b7hallt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Bei Nacht, wo lauer, s\u00fc\u00dfer Duft", "tokens": ["Bei", "Nacht", ",", "wo", "lau\u00b7er", ",", "s\u00fc\u00b7\u00dfer", "Duft"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "PWAV", "ADJD", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Blumenkelchen schwebt,", "tokens": ["Aus", "Blu\u00b7men\u00b7kel\u00b7chen", "schwebt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo die Erinnerung die Luft", "tokens": ["Wo", "die", "E\u00b7rin\u00b7ne\u00b7rung", "die", "Luft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bev\u00f6lkert und belebt,", "tokens": ["Be\u00b7v\u00f6l\u00b7kert", "und", "be\u00b7lebt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bei Nacht, bei Nacht \u2013 \u2013 wie ferne du", "tokens": ["Bei", "Nacht", ",", "bei", "Nacht", "\u2013", "\u2013", "wie", "fer\u00b7ne", "du"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$(", "$(", "PWAV", "ADV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mir auch entr\u00fcckt magst sein:", "tokens": ["Mir", "auch", "ent\u00b7r\u00fcckt", "magst", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Noch l\u00e4chelt hold dein Bild mir zu", "tokens": ["Noch", "l\u00e4\u00b7chelt", "hold", "dein", "Bild", "mir", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "PPOSAT", "NN", "PPER", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit sternenklarem Schein.", "tokens": ["Mit", "ster\u00b7nen\u00b7kla\u00b7rem", "Schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Vergessen ist was Hoffnung je", "tokens": ["Ver\u00b7ges\u00b7sen", "ist", "was", "Hoff\u00b7nung", "je"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PIS", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir schmeichelnd vorgemalt,", "tokens": ["Mir", "schmei\u00b7chelnd", "vor\u00b7ge\u00b7malt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vergessen ist so Lust als Weh;", "tokens": ["Ver\u00b7ges\u00b7sen", "ist", "so", "Lust", "als", "Weh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "NN", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von feindlicher Gewalt", "tokens": ["Von", "feind\u00b7li\u00b7cher", "Ge\u00b7walt"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ist ihre Farbenglut erbla\u00dft,", "tokens": ["Ist", "ih\u00b7re", "Far\u00b7beng\u00b7lut", "er\u00b7bla\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Wie eine Bl\u00fcth' an morschem Ast", "tokens": ["Wie", "ei\u00b7ne", "Bl\u00fcth'", "an", "mor\u00b7schem", "Ast"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Noch schwanket, sanft und tr\u00fcb!", "tokens": ["Noch", "schwan\u00b7ket", ",", "sanft", "und", "tr\u00fcb", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Thr\u00e4nen Trost versag' ich mir", "tokens": ["Der", "Thr\u00e4\u00b7nen", "Trost", "ver\u00b7sag'", "ich", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An meines Grams Altar,", "tokens": ["An", "mei\u00b7nes", "Grams", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und spr\u00e4che, Heil'ge! dann von dir", "tokens": ["Und", "spr\u00e4\u00b7che", ",", "Heil'\u00b7ge", "!", "dann", "von", "dir"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "NN", "$.", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zur lauten Thorenschaar? \u2013", "tokens": ["Zur", "lau\u00b7ten", "Tho\u00b7ren\u00b7schaar", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mir sagt ein jeder Sonnentag:", "tokens": ["Mir", "sagt", "ein", "je\u00b7der", "Son\u00b7nen\u00b7tag", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbsie lebt nicht mehr im Licht!\u00ab", "tokens": ["\u00bb", "sie", "lebt", "nicht", "mehr", "im", "Licht", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ob es auch Keiner ahnen mag,", "tokens": ["Ob", "es", "auch", "Kei\u00b7ner", "ah\u00b7nen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vergessen bist du nicht!", "tokens": ["Ver\u00b7ges\u00b7sen", "bist", "du", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}