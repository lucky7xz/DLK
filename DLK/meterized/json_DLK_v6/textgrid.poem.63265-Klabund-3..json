{"textgrid.poem.63265": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie lang ists her, dass ich mit dir im Grase lag.", "tokens": ["Wie", "lang", "ists", "her", ",", "dass", "ich", "mit", "dir", "im", "Gra\u00b7se", "lag", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PTKVZ", "$,", "KOUS", "PPER", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das gefl\u00fcgelte Ur-insekt schwirrte \u00fcber uns.", "tokens": ["Das", "ge\u00b7fl\u00fc\u00b7gel\u00b7te", "Ur\u00b7in\u00b7sekt", "schwirr\u00b7te", "\u00fc\u00b7ber", "uns", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Ich fing mit der Hand schlanke Ringelnattern", "tokens": ["Ich", "fing", "mit", "der", "Hand", "schlan\u00b7ke", "Rin\u00b7gel\u00b7nat\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "VVFIN", "NN"], "meter": "-+-+++-+-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Und hing dir ein Dutzend um den nackten Leib.", "tokens": ["Und", "hing", "dir", "ein", "Dut\u00b7zend", "um", "den", "nack\u00b7ten", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "In den Felsen spielte der Wind auf dem belaubten Cello.", "tokens": ["In", "den", "Fel\u00b7sen", "spiel\u00b7te", "der", "Wind", "auf", "dem", "be\u00b7laub\u00b7ten", "Cel\u00b7lo", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Vom Monte Ceneri schossen die Soldaten", "tokens": ["Vom", "Mon\u00b7te", "Ce\u00b7ne\u00b7ri", "schos\u00b7sen", "die", "Sol\u00b7da\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "NE", "VVFIN", "ART", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In die leere Luft.", "tokens": ["In", "die", "lee\u00b7re", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Schuss auf Schuss klang zwischen unsren K\u00fcssen.", "tokens": ["Schuss", "auf", "Schuss", "klang", "zwi\u00b7schen", "un\u00b7sren", "K\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Warum nicht traf uns eine verirrte Kugel,", "tokens": ["Wa\u00b7rum", "nicht", "traf", "uns", "ei\u00b7ne", "ver\u00b7irr\u00b7te", "Ku\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "---+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ehe sich Lippe von Lippe l\u00f6ste?", "tokens": ["E\u00b7he", "sich", "Lip\u00b7pe", "von", "Lip\u00b7pe", "l\u00f6s\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NE", "APPR", "NE", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Unser seliges Aas h\u00e4tten dankbar gesegnet", "tokens": ["Un\u00b7ser", "se\u00b7li\u00b7ges", "Aas", "h\u00e4t\u00b7ten", "dank\u00b7bar", "ge\u00b7seg\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "VVPP"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Aask\u00e4fer, Ameise, Geier und wilder Fuchs.", "tokens": ["Aas\u00b7k\u00e4\u00b7fer", ",", "A\u00b7mei\u00b7se", ",", "Gei\u00b7er", "und", "wil\u00b7der", "Fuchs", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "KON", "ADJA", "NE", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}}}}