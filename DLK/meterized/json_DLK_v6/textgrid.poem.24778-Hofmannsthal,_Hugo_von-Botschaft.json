{"textgrid.poem.24778": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "Botschaft", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe mich bedacht, da\u00df sch\u00f6nste Tage", "tokens": ["Ich", "ha\u00b7be", "mich", "be\u00b7dacht", ",", "da\u00df", "sch\u00f6ns\u00b7te", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur jene hei\u00dfen d\u00fcrfen, da wir redend", "tokens": ["Nur", "je\u00b7ne", "hei\u00b7\u00dfen", "d\u00fcr\u00b7fen", ",", "da", "wir", "re\u00b7dend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PDS", "VVINF", "VMINF", "$,", "KOUS", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Landschaft uns vor Augen in ein Reich", "tokens": ["Die", "Land\u00b7schaft", "uns", "vor", "Au\u00b7gen", "in", "ein", "Reich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "APPR", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Seele wandelten: da h\u00fcgelan", "tokens": ["Der", "See\u00b7le", "wan\u00b7del\u00b7ten", ":", "da", "h\u00fc\u00b7ge\u00b7lan"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dem Schatten zu wir stiegen in den Hain,", "tokens": ["Dem", "Schat\u00b7ten", "zu", "wir", "stie\u00b7gen", "in", "den", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der uns umfing, wie schon einmal Erlebtes,", "tokens": ["Der", "uns", "um\u00b7fing", ",", "wie", "schon", "ein\u00b7mal", "Er\u00b7leb\u00b7tes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "PWAV", "ADV", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da wir auf abgetrennten Wiesen still", "tokens": ["Da", "wir", "auf", "ab\u00b7ge\u00b7trenn\u00b7ten", "Wie\u00b7sen", "still"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Den Traum vom Leben niegeahnter Wesen,", "tokens": ["Den", "Traum", "vom", "Le\u00b7ben", "nie\u00b7geahn\u00b7ter", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Ja ihres Gehns und Trinkens Spuren fanden", "tokens": ["Ja", "ih\u00b7res", "Gehns", "und", "Trin\u00b7kens", "Spu\u00b7ren", "fan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PPOSAT", "NN", "KON", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und \u00fcbern Teich ein gleitendes Gespr\u00e4ch,", "tokens": ["Und", "\u00fc\u00b7bern", "Teich", "ein", "glei\u00b7ten\u00b7des", "Ge\u00b7spr\u00e4ch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Noch tiefere W\u00f6lbung spiegelnd als der Himmel:", "tokens": ["Noch", "tie\u00b7fe\u00b7re", "W\u00f6l\u00b7bung", "spie\u00b7gelnd", "als", "der", "Him\u00b7mel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Ich habe mich bedacht auf solche Tage,", "tokens": ["Ich", "ha\u00b7be", "mich", "be\u00b7dacht", "auf", "sol\u00b7che", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und da\u00df n\u00e4chst diesen drei: gesund zu sein,", "tokens": ["Und", "da\u00df", "n\u00e4chst", "die\u00b7sen", "drei", ":", "ge\u00b7sund", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PDAT", "CARD", "$.", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Am eignen Leib und Leben sich zu freuen,", "tokens": ["Am", "eig\u00b7nen", "Leib", "und", "Le\u00b7ben", "sich", "zu", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KON", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Und an Gedanken, Fl\u00fcgeln junger Adler,", "tokens": ["Und", "an", "Ge\u00b7dan\u00b7ken", ",", "Fl\u00fc\u00b7geln", "jun\u00b7ger", "Ad\u00b7ler", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Nur eines frommt: gesellig sein mit Freunden.", "tokens": ["Nur", "ei\u00b7nes", "frommt", ":", "ge\u00b7sel\u00b7lig", "sein", "mit", "Freun\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "$.", "ADJD", "VAINF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "So will ich, da\u00df du kommst und mit mir trinkst", "tokens": ["So", "will", "ich", ",", "da\u00df", "du", "kommst", "und", "mit", "mir", "trinkst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "KON", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Aus jenen Kr\u00fcgen, die mein Erbe sind,", "tokens": ["Aus", "je\u00b7nen", "Kr\u00fc\u00b7gen", ",", "die", "mein", "Er\u00b7be", "sind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Geschm\u00fcckt mit Laubwerk und beschwingten Kindern,", "tokens": ["Ge\u00b7schm\u00fcckt", "mit", "Laub\u00b7werk", "und", "be\u00b7schwing\u00b7ten", "Kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Und mit mir sitzest in dem Garten-Turm:", "tokens": ["Und", "mit", "mir", "sit\u00b7zest", "in", "dem", "Gar\u00b7ten\u00b7Turm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Zwei J\u00fcnglinge bewachen seine T\u00fcr,", "tokens": ["Zwei", "J\u00fcng\u00b7lin\u00b7ge", "be\u00b7wa\u00b7chen", "sei\u00b7ne", "T\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-++--+-+-+", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "In deren K\u00f6pfen mit ged\u00e4mpftem Blick", "tokens": ["In", "de\u00b7ren", "K\u00f6p\u00b7fen", "mit", "ge\u00b7d\u00e4mpf\u00b7tem", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Halbabgewandt ein ungeheures", "tokens": ["Hal\u00b7bab\u00b7ge\u00b7wandt", "ein", "un\u00b7ge\u00b7heu\u00b7res"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.24": {"text": "Geschick dich steinern anschaut, da\u00df du schweigst", "tokens": ["Ge\u00b7schick", "dich", "stei\u00b7nern", "an\u00b7schaut", ",", "da\u00df", "du", "schweigst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "VVINF", "VVFIN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Und meine Landschaft hingebreitet siehst:", "tokens": ["Und", "mei\u00b7ne", "Land\u00b7schaft", "hin\u00b7ge\u00b7brei\u00b7tet", "siehst", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Da\u00df dann vielleicht ein Vers von dir sie mir", "tokens": ["Da\u00df", "dann", "viel\u00b7leicht", "ein", "Vers", "von", "dir", "sie", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ART", "NN", "APPR", "PPER", "PPER", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Veredelt k\u00fcnftig in der Einsamkeit", "tokens": ["Ver\u00b7e\u00b7delt", "k\u00fcnf\u00b7tig", "in", "der", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Und da und dort Erinnerung an dich", "tokens": ["Und", "da", "und", "dort", "E\u00b7rin\u00b7ne\u00b7rung", "an", "dich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KON", "ADV", "NN", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Im Schatten nistet und zur D\u00e4mmerung", "tokens": ["Im", "Schat\u00b7ten", "nis\u00b7tet", "und", "zur", "D\u00e4m\u00b7me\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "KON", "APPRART", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.30": {"text": "Die Stra\u00dfe zwischen dunklen Wipfeln rollt", "tokens": ["Die", "Stra\u00b7\u00dfe", "zwi\u00b7schen", "dunk\u00b7len", "Wip\u00b7feln", "rollt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Und schattenlose Wege in der Luft", "tokens": ["Und", "schat\u00b7ten\u00b7lo\u00b7se", "We\u00b7ge", "in", "der", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Dahinrollt wie ein ferner goldner Donner.", "tokens": ["Da\u00b7hin\u00b7rollt", "wie", "ein", "fer\u00b7ner", "gold\u00b7ner", "Don\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}