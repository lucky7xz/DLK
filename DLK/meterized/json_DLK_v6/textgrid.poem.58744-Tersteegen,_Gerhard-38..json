{"textgrid.poem.58744": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "38.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ei, mein Auskehren mir nichts nutzet,", "tokens": ["Ei", ",", "mein", "Aus\u00b7keh\u00b7ren", "mir", "nichts", "nut\u00b7zet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da Satan stets den Platz mit neuem Kot beschmutzet,", "tokens": ["Da", "Sa\u00b7tan", "stets", "den", "Platz", "mit", "neu\u00b7em", "Kot", "be\u00b7schmut\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich geb' es auf, wei\u00df keinen Rat,", "tokens": ["Ich", "geb'", "es", "auf", ",", "wei\u00df", "kei\u00b7nen", "Rat", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dir, Liebe, la\u00df ich mich und deiner puren Gnad'.", "tokens": ["Dir", ",", "Lie\u00b7be", ",", "la\u00df", "ich", "mich", "und", "dei\u00b7ner", "pu\u00b7ren", "Gnad'", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVIMP", "PPER", "PRF", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich merk', je mehr ich feg', je mehr ich's h\u00e4\u00dflich mache,", "tokens": ["Ich", "merk'", ",", "je", "mehr", "ich", "feg'", ",", "je", "mehr", "ich's", "h\u00e4\u00df\u00b7lich", "ma\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ADV", "PPER", "PTKVZ", "$,", "ADV", "ADV", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich werd' best\u00fcrzt ob dieser Sache;", "tokens": ["Ich", "werd'", "be\u00b7st\u00fcrzt", "ob", "die\u00b7ser", "Sa\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KOUS", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ach, komm mir doch zu Hilfe, Herr,", "tokens": ["Ach", ",", "komm", "mir", "doch", "zu", "Hil\u00b7fe", ",", "Herr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sonst w\u00fcrde mir mein Schmerz zu schwer!", "tokens": ["Sonst", "w\u00fcr\u00b7de", "mir", "mein", "Schmerz", "zu", "schwer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich seh, du kommst und willst mit starker Hand zerschlagen", "tokens": ["Ich", "seh", ",", "du", "kommst", "und", "willst", "mit", "star\u00b7ker", "Hand", "zer\u00b7schla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "KON", "VMFIN", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Feind, den sch\u00e4ndlichen verjagen;", "tokens": ["Den", "Feind", ",", "den", "sch\u00e4nd\u00b7li\u00b7chen", "ver\u00b7ja\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie so erkenntlich ist mein Herz f\u00fcr solche Gnad',", "tokens": ["Wie", "so", "er\u00b7kennt\u00b7lich", "ist", "mein", "Herz", "f\u00fcr", "sol\u00b7che", "Gnad'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erzeig sie v\u00f6llig mit der Tat!", "tokens": ["Er\u00b7zeig", "sie", "v\u00f6l\u00b7lig", "mit", "der", "Tat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Hinfort ich m\u00fcsse nie den Feind mehr vor mir leiden,", "tokens": ["Hin\u00b7fort", "ich", "m\u00fcs\u00b7se", "nie", "den", "Feind", "mehr", "vor", "mir", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "ADV", "ART", "NN", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn ich bei dir nur bin, dann mu\u00df er mich wohl meiden;", "tokens": ["Wenn", "ich", "bei", "dir", "nur", "bin", ",", "dann", "mu\u00df", "er", "mich", "wohl", "mei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "VAFIN", "$,", "ADV", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Werd meine St\u00fctze lediglich,", "tokens": ["Werd", "mei\u00b7ne", "St\u00fct\u00b7ze", "le\u00b7di\u00b7glich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Meister, den ich ehr', dir \u00fcberlass' ich mich!", "tokens": ["Mein", "Meis\u00b7ter", ",", "den", "ich", "ehr'", ",", "dir", "\u00fc\u00b7ber\u00b7lass'", "ich", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}