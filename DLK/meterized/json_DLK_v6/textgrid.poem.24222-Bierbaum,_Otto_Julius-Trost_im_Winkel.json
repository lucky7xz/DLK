{"textgrid.poem.24222": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Trost im Winkel", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df es gehn, Herz, la\u00df dich treiben,", "tokens": ["La\u00df", "es", "gehn", ",", "Herz", ",", "la\u00df", "dich", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "NN", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles hat hier seine Bahn,", "tokens": ["Al\u00b7les", "hat", "hier", "sei\u00b7ne", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenig gilt hier: Mitgetan,", "tokens": ["We\u00b7nig", "gilt", "hier", ":", "Mit\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$.", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles gilt: Im Strome bleiben.", "tokens": ["Al\u00b7les", "gilt", ":", "Im", "Stro\u00b7me", "blei\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$.", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ist es dir bestimmt zu wohnen,", "tokens": ["Ist", "es", "dir", "be\u00b7stimmt", "zu", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die Sch\u00f6nheit Ruhe gibt,", "tokens": ["Wo", "die", "Sch\u00f6n\u00b7heit", "Ru\u00b7he", "gibt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wirst du, wie du bist, geliebt, \u2013", "tokens": ["Wirst", "du", ",", "wie", "du", "bist", ",", "ge\u00b7liebt", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "$,", "PWAV", "PPER", "VAFIN", "$,", "VVPP", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebe schenkt sich, ist kein Lohnen.", "tokens": ["Lie\u00b7be", "schenkt", "sich", ",", "ist", "kein", "Loh\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "$,", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "La\u00df es gehn, Herz, la\u00df dich treiben,", "tokens": ["La\u00df", "es", "gehn", ",", "Herz", ",", "la\u00df", "dich", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "NN", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spare dir des Zweifels Qual,", "tokens": ["Spa\u00b7re", "dir", "des", "Zwei\u00b7fels", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und du findest doch einmal", "tokens": ["Und", "du", "fin\u00b7dest", "doch", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen Herd, begl\u00fcckt zu bleiben.", "tokens": ["Ei\u00b7nen", "Herd", ",", "be\u00b7gl\u00fcckt", "zu", "blei\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}