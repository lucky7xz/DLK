{"textgrid.poem.56664": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Atem stockt; denn schwer und trunken schmiegt", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Atem stockt; denn schwer und trunken schmiegt", "tokens": ["Der", "A\u00b7tem", "stockt", ";", "denn", "schwer", "und", "trun\u00b7ken", "schmiegt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "KON", "ADJD", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "sich heut der Tag der Erde an", "tokens": ["sich", "heut", "der", "Tag", "der", "Er\u00b7de", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "ART", "NN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und eines dummen Vogels Lied fliegt,", "tokens": ["und", "ei\u00b7nes", "dum\u00b7men", "Vo\u00b7gels", "Lied", "fliegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "fliegt, ein Ding das noch nicht fliegen kann", "tokens": ["fliegt", ",", "ein", "Ding", "das", "noch", "nicht", "flie\u00b7gen", "kann"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ART", "NN", "ART", "ADV", "PTKNEG", "VVINF", "VMFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "und immer wieder gleich zur Erde f\u00e4llt,", "tokens": ["und", "im\u00b7mer", "wie\u00b7der", "gleich", "zur", "Er\u00b7de", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "\u00e4ngstlich durch die w\u00fcstenwarme Welt", "tokens": ["\u00e4ngst\u00b7lich", "durch", "die", "w\u00fcs\u00b7ten\u00b7war\u00b7me", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "und regt mich auf! Wie sich der Tag", "tokens": ["und", "regt", "mich", "auf", "!", "Wie", "sich", "der", "Tag"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$.", "PWAV", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mit unertr\u00e4glich weicher Schwere", "tokens": ["mit", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "wei\u00b7cher", "Schwe\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "dr\u00e4ngt in dieses jungen Vogels Lied!", "tokens": ["dr\u00e4ngt", "in", "die\u00b7ses", "jun\u00b7gen", "Vo\u00b7gels", "Lied", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und himmelan mit Hast und Fl\u00fcgelschlag", "tokens": ["Und", "him\u00b7me\u00b7lan", "mit", "Hast", "und", "Fl\u00fc\u00b7gel\u00b7schlag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "flattert in die k\u00fchle braune Leere!", "tokens": ["flat\u00b7tert", "in", "die", "k\u00fch\u00b7le", "brau\u00b7ne", "Lee\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und ihn ewige Ermattung mit", "tokens": ["Und", "ihn", "e\u00b7wi\u00b7ge", "Er\u00b7mat\u00b7tung", "mit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJA", "NN", "APPR"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "tausend Armen immer wieder niederzieht!", "tokens": ["tau\u00b7send", "Ar\u00b7men", "im\u00b7mer", "wie\u00b7der", "nie\u00b7der\u00b7zieht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch auf den Bergen lauert schon der F\u00f6hn", "tokens": ["Doch", "auf", "den", "Ber\u00b7gen", "lau\u00b7ert", "schon", "der", "F\u00f6hn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und wird noch \u00fcber Nacht aus seinen H\u00f6hn", "tokens": ["und", "wird", "noch", "\u00fc\u00b7ber", "Nacht", "aus", "sei\u00b7nen", "H\u00f6hn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "APPR", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und Wolkenhallen", "tokens": ["und", "Wol\u00b7ken\u00b7hal\u00b7len"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "brausend in die Ebne fallen!", "tokens": ["brau\u00b7send", "in", "die", "Eb\u00b7ne", "fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}