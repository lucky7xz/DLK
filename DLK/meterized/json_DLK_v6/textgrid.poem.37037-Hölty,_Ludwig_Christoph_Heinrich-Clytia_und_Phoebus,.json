{"textgrid.poem.37037": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Clytia und Phoebus,", "genre": "verse", "period": "N.A.", "pub_year": 1769, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mi\u00df Clytia, das sch\u00f6nste Kind,", "tokens": ["Mi\u00df", "Cly\u00b7tia", ",", "das", "sch\u00f6ns\u00b7te", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Cytherens Ebenbild,", "tokens": ["Cy\u00b7the\u00b7rens", "E\u00b7ben\u00b7bild", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "War, wie die M\u00e4dchen alle sind,", "tokens": ["War", ",", "wie", "die", "M\u00e4d\u00b7chen", "al\u00b7le", "sind", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWAV", "ART", "NN", "PIS", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Liebe stets erf\u00fcllt.", "tokens": ["Mit", "Lie\u00b7be", "stets", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie liebte ihres Nachbars Sohn,", "tokens": ["Sie", "lieb\u00b7te", "ih\u00b7res", "Nach\u00b7bars", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil man doch lieben mu\u00df,", "tokens": ["Weil", "man", "doch", "lie\u00b7ben", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im bunten Fl\u00fcgelkleide schon,", "tokens": ["Im", "bun\u00b7ten", "Fl\u00fc\u00b7gel\u00b7klei\u00b7de", "schon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und gab ihm manchen Ku\u00df.", "tokens": ["Und", "gab", "ihm", "man\u00b7chen", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie war die Freude doch so gro\u00df,", "tokens": ["Wie", "war", "die", "Freu\u00b7de", "doch", "so", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihre Brust durchglitt,", "tokens": ["Die", "ih\u00b7re", "Brust", "durch\u00b7glitt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn er auf seinem Steckenro\u00df,", "tokens": ["Wenn", "er", "auf", "sei\u00b7nem", "Ste\u00b7cken\u00b7ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor ihrem Fenster ritt!", "tokens": ["Vor", "ih\u00b7rem", "Fens\u00b7ter", "ritt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die gute Jungfer sah einmahl", "tokens": ["Die", "gu\u00b7te", "Jung\u00b7fer", "sah", "ein\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Phoebus, welcher sich", "tokens": ["Den", "Phoe\u00b7bus", ",", "wel\u00b7cher", "sich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NE", "$,", "PRELS", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht selten aus dem Himmel stahl,", "tokens": ["Nicht", "sel\u00b7ten", "aus", "dem", "Him\u00b7mel", "stahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und zu den Nymphen schlich.", "tokens": ["Und", "zu", "den", "Nym\u00b7phen", "schlich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie kramte ihren Busen aus,", "tokens": ["Sie", "kram\u00b7te", "ih\u00b7ren", "Bu\u00b7sen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch Phoebus, wie es hie\u00df,", "tokens": ["Doch", "Phoe\u00b7bus", ",", "wie", "es", "hie\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zog seine Stirn best\u00e4ndig kraus,", "tokens": ["Zog", "sei\u00b7ne", "Stirn", "be\u00b7st\u00e4n\u00b7dig", "kraus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So oft sie Reize wies.", "tokens": ["So", "oft", "sie", "Rei\u00b7ze", "wies", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Satyrisch sah er auf sie hin,", "tokens": ["Sa\u00b7ty\u00b7risch", "sah", "er", "auf", "sie", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Hohn im Blick, und sprach,", "tokens": ["Mit", "Hohn", "im", "Blick", ",", "und", "sprach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Entflieh, du kleine Buhlerinn,", "tokens": ["Ent\u00b7flieh", ",", "du", "klei\u00b7ne", "Buh\u00b7le\u00b7rinn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schleich Erdens\u00f6hnen nach.", "tokens": ["Schleich", "Er\u00b7den\u00b7s\u00f6h\u00b7nen", "nach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Dies niederschlagende Gebot,", "tokens": ["Dies", "nie\u00b7der\u00b7schla\u00b7gen\u00b7de", "Ge\u00b7bot", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ersch\u00fctterte sie tief,", "tokens": ["Er\u00b7sch\u00fct\u00b7ter\u00b7te", "sie", "tief", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und machte, da\u00df ein lichtes Roth", "tokens": ["Und", "mach\u00b7te", ",", "da\u00df", "ein", "lich\u00b7tes", "Roth"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Durch ihre Wangen lief.", "tokens": ["Durch", "ih\u00b7re", "Wan\u00b7gen", "lief", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Von Liebesschmerzen aufgezehrt,", "tokens": ["Von", "Lie\u00b7bes\u00b7schmer\u00b7zen", "auf\u00b7ge\u00b7zehrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ward endlich Clytia", "tokens": ["Ward", "end\u00b7lich", "Cly\u00b7tia"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADV", "NE"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Zur Sonnenblume. Hingekehrt", "tokens": ["Zur", "Son\u00b7nen\u00b7blu\u00b7me", ".", "Hin\u00b7ge\u00b7kehrt"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPRART", "NN", "$.", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gen Himmel stand sie da.", "tokens": ["Gen", "Him\u00b7mel", "stand", "sie", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Mit liebetrunkner Miene lacht", "tokens": ["Mit", "lie\u00b7be\u00b7trunk\u00b7ner", "Mie\u00b7ne", "lacht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie ihren Phoebus an,", "tokens": ["Sie", "ih\u00b7ren", "Phoe\u00b7bus", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So bald der junge Tag erwacht,", "tokens": ["So", "bald", "der", "jun\u00b7ge", "Tag", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schauet himmelan.", "tokens": ["Und", "schau\u00b7et", "him\u00b7me\u00b7lan", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Sie blickt ihm nach, wenn er am Saum", "tokens": ["Sie", "blickt", "ihm", "nach", ",", "wenn", "er", "am", "Saum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Abendhimmels blinkt,", "tokens": ["Des", "A\u00b7bend\u00b7him\u00b7mels", "blinkt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis er tr\u00fcbr\u00f6thlicht in den Schaum", "tokens": ["Bis", "er", "tr\u00fcb\u00b7r\u00f6th\u00b7licht", "in", "den", "Schaum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Des Oceans versinkt.", "tokens": ["Des", "O\u00b7ceans", "ver\u00b7sinkt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "--+-+", "measure": "anapaest.init"}}}}}