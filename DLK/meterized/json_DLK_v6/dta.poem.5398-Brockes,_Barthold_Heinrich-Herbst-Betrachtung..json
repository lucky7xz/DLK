{"dta.poem.5398": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Herbst-Betrachtung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf! la\u00dft uns, unserm GOtt zu Ehren,", "tokens": ["Auf", "!", "la\u00dft", "uns", ",", "un\u00b7serm", "Gott", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVIMP", "PPER", "$,", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Erden ", "tokens": ["Der", "Er\u00b7den"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "In G\u00e4rten, Feld-und W\u00e4lder gehn;", "tokens": ["In", "G\u00e4r\u00b7ten", ",", "Feld\u00b7\u00b7und", "W\u00e4l\u00b7der", "gehn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es wird gewi\u00df sein Lob vermehren!", "tokens": ["Es", "wird", "ge\u00b7wi\u00df", "sein", "Lob", "ver\u00b7meh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kann man wol sonder Lust erblicken,", "tokens": ["Kann", "man", "wol", "son\u00b7der", "Lust", "er\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie sich anjetzt, mit neuer Zier,", "tokens": ["Wie", "sich", "an\u00b7jetzt", ",", "mit", "neu\u00b7er", "Zier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Und neuen Farben, dort und hier,", "tokens": ["Und", "neu\u00b7en", "Far\u00b7ben", ",", "dort", "und", "hier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der feuchten Erde Fl\u00e4chen schm\u00fccken?", "tokens": ["Der", "feuch\u00b7ten", "Er\u00b7de", "Fl\u00e4\u00b7chen", "schm\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wenn Laub und Schatten d\u00fcnne werden", "tokens": ["Wenn", "Laub", "und", "Schat\u00b7ten", "d\u00fcn\u00b7ne", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "VAINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Erh\u00f6ht und mehrt sich \u00fcberall,", "tokens": ["Er\u00b7h\u00f6ht", "und", "mehrt", "sich", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Auch selber bey der Bl\u00e4tter Fall,", "tokens": ["Auch", "sel\u00b7ber", "bey", "der", "Bl\u00e4t\u00b7ter", "Fall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der sonst nur gr\u00fcne Schmuck der Erden", "tokens": ["Der", "sonst", "nur", "gr\u00fc\u00b7ne", "Schmuck", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Es \u00e4ndrn sich anjetzt die W\u00e4lder;", "tokens": ["Es", "\u00e4ndrn", "sich", "an\u00b7jetzt", "die", "W\u00e4l\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Das Gr\u00fcn ist nicht mehr allgemein;", "tokens": ["Das", "Gr\u00fcn", "ist", "nicht", "mehr", "all\u00b7ge\u00b7mein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Es funckeln jetzt in buntem Schein", "tokens": ["Es", "fun\u00b7ckeln", "jetzt", "in", "bun\u00b7tem", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Der B\u00e4ume Gipfel, Garten, Felder.", "tokens": ["Der", "B\u00e4u\u00b7me", "Gip\u00b7fel", ",", "Gar\u00b7ten", ",", "Fel\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Ein Baum, wenn ihn, im frohen Lentzen,", "tokens": ["Ein", "Baum", ",", "wenn", "ihn", ",", "im", "fro\u00b7hen", "Lent\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Der Sonnen g\u00fcldnes Licht bestrahlt,", "tokens": ["Der", "Son\u00b7nen", "g\u00fcld\u00b7nes", "Licht", "be\u00b7strahlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Ist durch ein r\u00f6htlich Gelb bemahlt;", "tokens": ["Ist", "durch", "ein", "r\u00f6ht\u00b7lich", "Gelb", "be\u00b7mahlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "So sieht man jetzt die B\u00e4ume gl\u00e4ntzen.", "tokens": ["So", "sieht", "man", "jetzt", "die", "B\u00e4u\u00b7me", "gl\u00e4nt\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Jetzt scheinen die gef\u00e4rbten Bl\u00e4tter", "tokens": ["Jetzt", "schei\u00b7nen", "die", "ge\u00b7f\u00e4rb\u00b7ten", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Und irher Wipfel r\u00f6htlich Gr\u00fcn,", "tokens": ["Und", "ir\u00b7her", "Wip\u00b7fel", "r\u00f6ht\u00b7lich", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Als wenn die Sonne sie beschien,", "tokens": ["Als", "wenn", "die", "Son\u00b7ne", "sie", "be\u00b7schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Auch selbst bey einem duncklen Wetter.", "tokens": ["Auch", "selbst", "bey", "ei\u00b7nem", "dunck\u00b7len", "Wet\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Wenn auch die Schatten alles dr\u00fccken", "tokens": ["Wenn", "auch", "die", "Schat\u00b7ten", "al\u00b7les", "dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "PIS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Und \u00fcberziehen; sieht man sie,", "tokens": ["Und", "\u00fc\u00b7ber\u00b7zie\u00b7hen", ";", "sieht", "man", "sie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$.", "VVFIN", "PIS", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Doch mit ged\u00e4mpfter Harmonie,", "tokens": ["Doch", "mit", "ge\u00b7d\u00e4mpf\u00b7ter", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Mit bunter Glut die Felder schm\u00fccken.", "tokens": ["Mit", "bun\u00b7ter", "Glut", "die", "Fel\u00b7der", "schm\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}