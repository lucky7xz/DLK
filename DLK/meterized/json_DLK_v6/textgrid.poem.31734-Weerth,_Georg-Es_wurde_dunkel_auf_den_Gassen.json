{"textgrid.poem.31734": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Es wurde dunkel auf den Gassen", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wurde dunkel auf den Gassen,", "tokens": ["Es", "wur\u00b7de", "dun\u00b7kel", "auf", "den", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da schlichen sie ins letzte Haus,", "tokens": ["Da", "schli\u00b7chen", "sie", "ins", "letz\u00b7te", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie t\u00e4ten stumm die Gl\u00e4ser fassen", "tokens": ["Sie", "t\u00e4\u00b7ten", "stumm", "die", "Gl\u00e4\u00b7ser", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und tranken tr\u00fcbes Bier daraus.", "tokens": ["Und", "tran\u00b7ken", "tr\u00fc\u00b7bes", "Bier", "da\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erst als die Mitternacht gekommen,", "tokens": ["Erst", "als", "die", "Mit\u00b7ter\u00b7nacht", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da hat ein Alter das Wort genommen:", "tokens": ["Da", "hat", "ein", "Al\u00b7ter", "das", "Wort", "ge\u00b7nom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbwohl hab ich lang auf Gott vertrauet,", "tokens": ["\u00bb", "wohl", "hab", "ich", "lang", "auf", "Gott", "ver\u00b7trau\u00b7et", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denn dieser, sagt man, lenkt die Welt,", "tokens": ["Denn", "die\u00b7ser", ",", "sagt", "man", ",", "lenkt", "die", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "VVFIN", "PIS", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mit dem Pflug hab ich bebauet", "tokens": ["Und", "mit", "dem", "Pflug", "hab", "ich", "be\u00b7bau\u00b7et"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein sch\u00f6nes gr\u00fcnumgebnes Feld.", "tokens": ["Mein", "sch\u00f6\u00b7nes", "gr\u00fc\u00b7num\u00b7geb\u00b7nes", "Feld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch ach, was half der Felder Prangen?", "tokens": ["Doch", "ach", ",", "was", "half", "der", "Fel\u00b7der", "Pran\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PWS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bin hungrig oft zu Bett gegangen.\u00ab", "tokens": ["Bin", "hung\u00b7rig", "oft", "zu", "Bett", "ge\u00b7gan\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "APPR", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbund wir, wir f\u00fchrten manche Jahre", "tokens": ["\u00bb", "und", "wir", ",", "wir", "f\u00fchr\u00b7ten", "man\u00b7che", "Jah\u00b7re"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "KON", "PPER", "$,", "PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Spindel schon mit rascher Hand,", "tokens": ["Die", "Spin\u00b7del", "schon", "mit", "ra\u00b7scher", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir spannen F\u00e4den, fein und klare,", "tokens": ["Wir", "span\u00b7nen", "F\u00e4\u00b7den", ",", "fein", "und", "kla\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu warmem wollenem Gewand.", "tokens": ["Zu", "war\u00b7mem", "wol\u00b7le\u00b7nem", "Ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch ach, was auch die H\u00e4nde taten \u2013", "tokens": ["Doch", "ach", ",", "was", "auch", "die", "H\u00e4n\u00b7de", "ta\u00b7ten", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sind selber nie in die Wolle geraten.\u00ab", "tokens": ["Sind", "sel\u00b7ber", "nie", "in", "die", "Wol\u00b7le", "ge\u00b7ra\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "ADV", "APPR", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Und andre sehr gemeine Leute \u2013", "tokens": ["Und", "and\u00b7re", "sehr", "ge\u00b7mei\u00b7ne", "Leu\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Man sah's am schlechten sch\u00e4b'gen Rock \u2013", "tokens": ["Man", "sah's", "am", "schlech\u00b7ten", "sch\u00e4b'\u00b7gen", "Rock", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sprachen: \u00bbFast es uns gereute,", "tokens": ["Sie", "spra\u00b7chen", ":", "\u00bb", "Fast", "es", "uns", "ge\u00b7reu\u00b7te", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "VVFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df wir gepflanzt den Rebenstock.", "tokens": ["Da\u00df", "wir", "ge\u00b7pflanzt", "den", "Re\u00b7ben\u00b7stock", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ob lustig spr\u00fchn des Weines Funken,", "tokens": ["Ob", "lus\u00b7tig", "spr\u00fchn", "des", "Wei\u00b7nes", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wir haben selbst nur Wasser getrunken!\u00ab", "tokens": ["Wir", "ha\u00b7ben", "selbst", "nur", "Was\u00b7ser", "ge\u00b7trun\u00b7ken", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}