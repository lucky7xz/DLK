{"textgrid.poem.41775": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "Der Sohn des Volkes", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbim stillen Dorfe war's, wo ich geboren,", "tokens": ["\u00bb", "im", "stil\u00b7len", "Dor\u00b7fe", "wa\u00b7r's", ",", "wo", "ich", "ge\u00b7bo\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "ADJA", "NN", "VAFIN", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wo unter'm Strohdach meine Wiege stand;", "tokens": ["Wo", "un\u00b7ter'm", "Stroh\u00b7dach", "mei\u00b7ne", "Wie\u00b7ge", "stand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Drum hab' ich Treu dem biedern Volk geschworen,", "tokens": ["Drum", "hab'", "ich", "Treu", "dem", "bie\u00b7dern", "Volk", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bei dem mir meine Jugendzeit entschwand.", "tokens": ["Bei", "dem", "mir", "mei\u00b7ne", "Ju\u00b7gend\u00b7zeit", "ent\u00b7schwand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Pflugschaar, hinter der mein Vater ging,", "tokens": ["Die", "Pflug\u00b7schaar", ",", "hin\u00b7ter", "der", "mein", "Va\u00b7ter", "ging", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Des armen Herdes k\u00fcmmerliche Flamme,", "tokens": ["Des", "ar\u00b7men", "Her\u00b7des", "k\u00fcm\u00b7mer\u00b7li\u00b7che", "Flam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sie sind das Sch\u00f6nste, was ich fr\u00fch empfing:", "tokens": ["Sie", "sind", "das", "Sch\u00f6ns\u00b7te", ",", "was", "ich", "fr\u00fch", "emp\u00b7fing", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PWS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Es ist mein Stolz, da\u00df ich vom Volke stamme!", "tokens": ["Es", "ist", "mein", "Stolz", ",", "da\u00df", "ich", "vom", "Vol\u00b7ke", "stam\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Pflugschaar lernt' ich als ein Heil'ges ehren", "tokens": ["Die", "Pflug\u00b7schaar", "lernt'", "ich", "als", "ein", "Heil'\u00b7ges", "eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "KOUS", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ehren jede Hand, die sie gef\u00fchrt;", "tokens": ["Und", "eh\u00b7ren", "je\u00b7de", "Hand", ",", "die", "sie", "ge\u00b7f\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie ist das Werkzeug, Tausende zu n\u00e4hren,", "tokens": ["Sie", "ist", "das", "Werk\u00b7zeug", ",", "Tau\u00b7sen\u00b7de", "zu", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn sie die Felder segensvoll ber\u00fchrt.", "tokens": ["Wenn", "sie", "die", "Fel\u00b7der", "se\u00b7gens\u00b7voll", "be\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Arbeit ist es, der mein Preis erklingt,", "tokens": ["Die", "Ar\u00b7beit", "ist", "es", ",", "der", "mein", "Preis", "er\u00b7klingt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und drum den M\u00fc\u00dfigang ich laut verdamme,", "tokens": ["Und", "drum", "den", "M\u00fc\u00b7\u00dfi\u00b7gang", "ich", "laut", "ver\u00b7dam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein Jauchzen meinem Herzen sich entringt:", "tokens": ["Ein", "Jauch\u00b7zen", "mei\u00b7nem", "Her\u00b7zen", "sich", "ent\u00b7ringt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Es ist mein Stolz, da\u00df ich vom Volke stamme!", "tokens": ["Es", "ist", "mein", "Stolz", ",", "da\u00df", "ich", "vom", "Vol\u00b7ke", "stam\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es ist mein Stolz, als Bruder Dich zu nennen,", "tokens": ["Es", "ist", "mein", "Stolz", ",", "als", "Bru\u00b7der", "Dich", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "KOUS", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Du das Feld beh\u00fctet und bebaut;", "tokens": ["Der", "Du", "das", "Feld", "be\u00b7h\u00fc\u00b7tet", "und", "be\u00b7baut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im finstern Sturm und bei der Sonne Brennen", "tokens": ["Im", "fins\u00b7tern", "Sturm", "und", "bei", "der", "Son\u00b7ne", "Bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hab' ich mit Ehrfurcht zu Dir aufgeschaut,", "tokens": ["Hab'", "ich", "mit", "Ehr\u00b7furcht", "zu", "Dir", "auf\u00b7ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und w\u00e4rst Du blieben nur ein armer Knecht,", "tokens": ["Und", "w\u00e4rst", "Du", "blie\u00b7ben", "nur", "ein", "ar\u00b7mer", "Knecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich weihe doch Dir meiner Liebe Flamme;", "tokens": ["Ich", "wei\u00b7he", "doch", "Dir", "mei\u00b7ner", "Lie\u00b7be", "Flam\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nur wer nichts thut, ist f\u00fcr mein Herz zu schlecht", "tokens": ["Nur", "wer", "nichts", "thut", ",", "ist", "f\u00fcr", "mein", "Herz", "zu", "schlecht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWS", "PIS", "VVFIN", "$,", "VAFIN", "APPR", "PPOSAT", "NN", "PTKZU", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Es ist mein Stolz, da\u00df ich vom Volke stamme!", "tokens": ["Es", "ist", "mein", "Stolz", ",", "da\u00df", "ich", "vom", "Vol\u00b7ke", "stam\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Im Volke, das da schafft mit kr\u00e4ft'gen H\u00e4nden,", "tokens": ["Im", "Vol\u00b7ke", ",", "das", "da", "schafft", "mit", "kr\u00e4ft'\u00b7gen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohnt auch die Kraft, der Jetztzeit ganzes Leid", "tokens": ["Wohnt", "auch", "die", "Kraft", ",", "der", "Jetzt\u00b7zeit", "gan\u00b7zes", "Leid"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu Freud' und Freiheit siegend einst zu wenden;", "tokens": ["Zu", "Freud'", "und", "Frei\u00b7heit", "sie\u00b7gend", "einst", "zu", "wen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Drum ruf' ich's meinen Br\u00fcdern: seid bereit!", "tokens": ["Drum", "ruf'", "ich's", "mei\u00b7nen", "Br\u00fc\u00b7dern", ":", "seid", "be\u00b7reit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "PPOSAT", "NN", "$.", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den Bruder, der das Bruderwort verstand,", "tokens": ["Den", "Bru\u00b7der", ",", "der", "das", "Bru\u00b7der\u00b7wort", "ver\u00b7stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Den fa\u00dft allm\u00e4chtig der Begeistrung Flamme;", "tokens": ["Den", "fa\u00dft", "all\u00b7m\u00e4ch\u00b7tig", "der", "Be\u00b7geis\u00b7trung", "Flam\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Mich kn\u00fcpft an Euch ein unzertrennlich Band:", "tokens": ["Mich", "kn\u00fcpft", "an", "Euch", "ein", "un\u00b7zer\u00b7trenn\u00b7lich", "Band", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Es ist mein Stolz, da\u00df ich vom Volke stamme!\u00ab", "tokens": ["Es", "ist", "mein", "Stolz", ",", "da\u00df", "ich", "vom", "Vol\u00b7ke", "stam\u00b7me", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}