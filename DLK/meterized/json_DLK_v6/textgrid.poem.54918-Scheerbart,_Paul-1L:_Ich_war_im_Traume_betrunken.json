{"textgrid.poem.54918": {"metadata": {"author": {"name": "Scheerbart, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich war im Traume betrunken", "genre": "verse", "period": "N.A.", "pub_year": 1889, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich war im Traume betrunken", "tokens": ["Ich", "war", "im", "Trau\u00b7me", "be\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und sah ein altes Kamel,", "tokens": ["Und", "sah", "ein", "al\u00b7tes", "Ka\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das war zu Boden gesunken \u2013", "tokens": ["Das", "war", "zu", "Bo\u00b7den", "ge\u00b7sun\u00b7ken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Es lachte \u2013 bei meiner Seel!", "tokens": ["Es", "lach\u00b7te", "\u2013", "bei", "mei\u00b7ner", "Seel", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und bald lag mein ganzes Genie", "tokens": ["Und", "bald", "lag", "mein", "gan\u00b7zes", "Ge\u00b7nie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Neben dem lachenden Vieh.", "tokens": ["Ne\u00b7ben", "dem", "la\u00b7chen\u00b7den", "Vieh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Der Himmel lachte \u00fcber mir,", "tokens": ["Der", "Him\u00b7mel", "lach\u00b7te", "\u00fc\u00b7ber", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ich trank immer noch f\u00fcr Vier.", "tokens": ["Und", "ich", "trank", "im\u00b7mer", "noch", "f\u00fcr", "Vier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV", "APPR", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mein Kamel kam nicht zu kurz dabei;", "tokens": ["Mein", "Ka\u00b7mel", "kam", "nicht", "zu", "kurz", "da\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "PTKA", "ADJD", "PAV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich lie\u00df es trinken fast f\u00fcr Drei.", "tokens": ["Ich", "lie\u00df", "es", "trin\u00b7ken", "fast", "f\u00fcr", "Drei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ADV", "APPR", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dies war meine sch\u00f6nste Zecherei;", "tokens": ["Dies", "war", "mei\u00b7ne", "sch\u00f6ns\u00b7te", "Ze\u00b7che\u00b7rei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich f\u00fchlte mich so gro\u00df und frei.", "tokens": ["Ich", "f\u00fchl\u00b7te", "mich", "so", "gro\u00df", "und", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich trinke \u2013 bei meiner ewigen Seele! \u2013", "tokens": ["Ich", "trin\u00b7ke", "\u2013", "bei", "mei\u00b7ner", "e\u00b7wi\u00b7gen", "See\u00b7le", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$(", "APPR", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nur noch mit einem alten Kamele.", "tokens": ["Nur", "noch", "mit", "ei\u00b7nem", "al\u00b7ten", "Ka\u00b7me\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mit Menschen trinken ist der gr\u00f6\u00dfte Kohl \u2013", "tokens": ["Mit", "Men\u00b7schen", "trin\u00b7ken", "ist", "der", "gr\u00f6\u00df\u00b7te", "Kohl", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kamele nur verstehn den Alkohol.", "tokens": ["Ka\u00b7me\u00b7le", "nur", "ver\u00b7stehn", "den", "Al\u00b7ko\u00b7hol", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}