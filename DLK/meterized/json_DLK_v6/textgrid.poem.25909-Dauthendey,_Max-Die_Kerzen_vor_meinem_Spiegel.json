{"textgrid.poem.25909": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Die Kerzen vor meinem Spiegel", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Kerzen vor meinem Spiegel,", "tokens": ["Die", "Ker\u00b7zen", "vor", "mei\u00b7nem", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die doppelt im Glase sich zeigen,", "tokens": ["Die", "dop\u00b7pelt", "im", "Gla\u00b7se", "sich", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "PRF", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sind wie die Augen, die schweigen,", "tokens": ["Sind", "wie", "die", "Au\u00b7gen", ",", "die", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und Augen sind gl\u00fchende Siegel.", "tokens": ["Und", "Au\u00b7gen", "sind", "gl\u00fc\u00b7hen\u00b7de", "Sie\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Von einem unendlichen Liede", "tokens": ["Von", "ei\u00b7nem", "un\u00b7end\u00b7li\u00b7chen", "Lie\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Zwei Noten, dunkel und stumm,", "tokens": ["Zwei", "No\u00b7ten", ",", "dun\u00b7kel", "und", "stumm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Gehen mir im klopfenden Herzen", "tokens": ["Ge\u00b7hen", "mir", "im", "klop\u00b7fen\u00b7den", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Deine brennenden Augen um.", "tokens": ["Dei\u00b7ne", "bren\u00b7nen\u00b7den", "Au\u00b7gen", "um", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}