{"textgrid.poem.57831": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich h\u00f6r' ein V\u00f6glein singen,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich h\u00f6r' ein V\u00f6glein singen,", "tokens": ["Ich", "h\u00f6r'", "ein", "V\u00f6\u00b7glein", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das V\u00f6gelein singt zipp und zapp;", "tokens": ["Das", "V\u00f6\u00b7ge\u00b7lein", "singt", "zipp", "und", "zapp", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich la\u00df den Rappen laufen,", "tokens": ["Ich", "la\u00df", "den", "Rap\u00b7pen", "lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bald Schritt und auch bald Trab.", "tokens": ["Bald", "Schritt", "und", "auch", "bald", "Trab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Trompeter und die blasen,", "tokens": ["Trom\u00b7pe\u00b7ter", "und", "die", "bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Schatz, nun la\u00df das Weinen sein;", "tokens": ["Mein", "Schatz", ",", "nun", "la\u00df", "das", "Wei\u00b7nen", "sein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VVIMP", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vier Jahre gehn vor\u00fcber,", "tokens": ["Vier", "Jah\u00b7re", "gehn", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dann bin ich wieder dein.", "tokens": ["Dann", "bin", "ich", "wie\u00b7der", "dein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das F\u00e4hnlein tut winken,", "tokens": ["Das", "F\u00e4hn\u00b7lein", "tut", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wir sind des Kaisers K\u00fcrassier;", "tokens": ["Wir", "sind", "des", "Kai\u00b7sers", "K\u00fc\u00b7ras\u00b7sier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist meine Zeit vor\u00fcber,", "tokens": ["Ist", "mei\u00b7ne", "Zeit", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kehr' ich zur\u00fcck zu dir.", "tokens": ["Kehr'", "ich", "zu\u00b7r\u00fcck", "zu", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das sind die schweren Reiter,", "tokens": ["Das", "sind", "die", "schwe\u00b7ren", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die f\u00fcrchten sich vor keinem Blei;", "tokens": ["Die", "f\u00fcrch\u00b7ten", "sich", "vor", "kei\u00b7nem", "Blei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Kleid, das ist von Eisen,", "tokens": ["Ihr", "Kleid", ",", "das", "ist", "von", "Ei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Herz und das ist treu.", "tokens": ["Ihr", "Herz", "und", "das", "ist", "treu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "La\u00df traben, la\u00df traben,", "tokens": ["La\u00df", "tra\u00b7ben", ",", "la\u00df", "tra\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "VVINF", "$,", "VVIMP", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Welt ist weit, die Welt ist breit;", "tokens": ["Die", "Welt", "ist", "weit", ",", "die", "Welt", "ist", "breit", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Rosen bl\u00fchen wieder,", "tokens": ["Die", "Ro\u00b7sen", "bl\u00fc\u00b7hen", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kommt erst die rechte Zeit.", "tokens": ["Kommt", "erst", "die", "rech\u00b7te", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}