{"textgrid.poem.61110": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "10. Aperanthos auf Naxos", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, das hei\u00df' ich f\u00fcrwahr Dionysos' heilige St\u00e4tte!", "tokens": ["Ja", ",", "das", "hei\u00df'", "ich", "f\u00fcr\u00b7wahr", "Di\u00b7o\u00b7ny\u00b7sos'", "hei\u00b7li\u00b7ge", "St\u00e4t\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VVFIN", "PPER", "ADV", "NE", "ADJA", "NN", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "\u00dcppiges Traubengel\u00e4nd' kr\u00e4nzt das gesegnete Tal.", "tokens": ["\u00dcp\u00b7pi\u00b7ges", "Trau\u00b7ben\u00b7ge\u00b7l\u00e4nd'", "kr\u00e4nzt", "das", "ge\u00b7se\u00b7gne\u00b7te", "Tal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+---+-+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Jeglicher Abhang triefet von Wein; um die Giebel der H\u00e4user,", "tokens": ["Jeg\u00b7li\u00b7cher", "Ab\u00b7hang", "trie\u00b7fet", "von", "Wein", ";", "um", "die", "Gie\u00b7bel", "der", "H\u00e4u\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "NN", "$.", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Um der Kastanien Schaft schlingt sich das gr\u00fcne Gerank.", "tokens": ["Um", "der", "Kas\u00b7ta\u00b7ni\u00b7en", "Schaft", "schlingt", "sich", "das", "gr\u00fc\u00b7ne", "Ge\u00b7rank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Horch, schon wandelt der bakchische Zug; schwarz\u00e4ugige Jungfraun", "tokens": ["Horch", ",", "schon", "wan\u00b7delt", "der", "bak\u00b7chi\u00b7sche", "Zug", ";", "schwarz\u00b7\u00e4u\u00b7gi\u00b7ge", "Jung\u00b7fraun"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "ADJA", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "F\u00fchren den Reihn, du vernimmst Zithern und Paukenget\u00f6n.", "tokens": ["F\u00fch\u00b7ren", "den", "Reihn", ",", "du", "ver\u00b7nimmst", "Zi\u00b7thern", "und", "Pau\u00b7ken\u00b7ge\u00b7t\u00f6n", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PPER", "VVFIN", "NN", "KON", "NE", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Jener ergl\u00fchende Greis auf dem Esel, er scheint mir Silenos;", "tokens": ["Je\u00b7ner", "er\u00b7gl\u00fc\u00b7hen\u00b7de", "Greis", "auf", "dem", "E\u00b7sel", ",", "er", "scheint", "mir", "Si\u00b7le\u00b7nos", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "NN", "$."], "meter": "+--+--+-+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Folgt nicht, die Schl\u00e4fe bekr\u00e4nzt, bald mit den Panthern der Gott?", "tokens": ["Folgt", "nicht", ",", "die", "Schl\u00e4\u00b7fe", "be\u00b7kr\u00e4nzt", ",", "bald", "mit", "den", "Pan\u00b7thern", "der", "Gott", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "ART", "NN", "VVPP", "$,", "ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Aber indes nicht l\u00e4ssig, o Schenk! Frisch, walte des Amtes,", "tokens": ["A\u00b7ber", "in\u00b7des", "nicht", "l\u00e4s\u00b7sig", ",", "o", "Schenk", "!", "Frisch", ",", "wal\u00b7te", "des", "Am\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ADJD", "$,", "FM", "NN", "$.", "NE", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Mit dem ambrosischen Trank f\u00fclle den weiten Pokal.", "tokens": ["Mit", "dem", "am\u00b7bro\u00b7si\u00b7schen", "Trank", "f\u00fcl\u00b7le", "den", "wei\u00b7ten", "Po\u00b7kal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "---+--++--+--+", "measure": "iambic.penta.relaxed"}}}}}