{"textgrid.poem.42657": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gekr\u00f6nt mit Veilchen, liess in rosenfarbnem Kleid", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gekr\u00f6nt mit Veilchen, liess in rosenfarbnem Kleid", "tokens": ["Ge\u00b7kr\u00f6nt", "mit", "Veil\u00b7chen", ",", "liess", "in", "ro\u00b7sen\u00b7farb\u00b7nem", "Kleid"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der junge May vom Himmel sich hernieder,", "tokens": ["Der", "jun\u00b7ge", "May", "vom", "Him\u00b7mel", "sich", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ihm zur Seite schwebt' auf wallendem Gefieder", "tokens": ["Und", "ihm", "zur", "Sei\u00b7te", "schwebt'", "auf", "wal\u00b7len\u00b7dem", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Gott der Munterkeit.", "tokens": ["Der", "Gott", "der", "Mun\u00b7ter\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und sieh! es schm\u00fcckte nun die Blumenk\u00f6niginn", "tokens": ["Und", "sieh", "!", "es", "schm\u00fcck\u00b7te", "nun", "die", "Blu\u00b7men\u00b7k\u00f6\u00b7ni\u00b7ginn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "$.", "PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Bl\u00fchten rings die neubelebten \u00c4ste:", "tokens": ["Mit", "Bl\u00fch\u00b7ten", "rings", "die", "neu\u00b7be\u00b7leb\u00b7ten", "\u00c4s\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Muthwillig gaukelten in Schw\u00e4rmen laue Weste", "tokens": ["Mut\u00b7hwil\u00b7lig", "gau\u00b7kel\u00b7ten", "in", "Schw\u00e4r\u00b7men", "lau\u00b7e", "Wes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "NN", "ADJA", "NN"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.4": {"text": "Im Grase her und hin.", "tokens": ["Im", "Gra\u00b7se", "her", "und", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es trillerten entz\u00fcckt im s\u00e4uselnden Gew\u00fchl", "tokens": ["Es", "tril\u00b7ler\u00b7ten", "ent\u00b7z\u00fcckt", "im", "s\u00e4u\u00b7seln\u00b7den", "Ge\u00b7w\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "VVPP", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des zarten Laubs die s\u00fcssen Nachtigallen.", "tokens": ["Des", "zar\u00b7ten", "Laubs", "die", "s\u00fcs\u00b7sen", "Nach\u00b7ti\u00b7gal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die halbe Welt ward froh: doch mir gebrach's bey allen", "tokens": ["Die", "hal\u00b7be", "Welt", "ward", "froh", ":", "doch", "mir", "ge\u00b7brach's", "bey", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$.", "ADV", "PPER", "PIS", "APPR", "PIAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Reitzen an Gef\u00fchl.", "tokens": ["Den", "Reit\u00b7zen", "an", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Denn ach! im schwarzen Reich der grauenvollen Nacht,", "tokens": ["Denn", "ach", "!", "im", "schwar\u00b7zen", "Reich", "der", "grau\u00b7en\u00b7vol\u00b7len", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo, aufgebl\u00e4ht vom Gifte fauler Drachen,", "tokens": ["Wo", ",", "auf\u00b7ge\u00b7bl\u00e4ht", "vom", "Gif\u00b7te", "fau\u00b7ler", "Dra\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "VVPP", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Seuchen Heerschaar st\u00e4ts mit aufgesperrtem Rachen,", "tokens": ["Der", "Seu\u00b7chen", "Heer\u00b7schaar", "st\u00e4ts", "mit", "auf\u00b7ge\u00b7sperr\u00b7tem", "Ra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Voll Gier nach Beute, wacht,", "tokens": ["Voll", "Gier", "nach", "Beu\u00b7te", ",", "wacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Riss z\u00e4hnefletschend sich ein t\u00fcckisch Fieber los,", "tokens": ["Riss", "z\u00e4h\u00b7ne\u00b7flet\u00b7schend", "sich", "ein", "t\u00fc\u00b7ckisch", "Fie\u00b7ber", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PRF", "ART", "ADJD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schlich heran mit m\u00f6rderischen Blicken", "tokens": ["Und", "schlich", "he\u00b7ran", "mit", "m\u00f6r\u00b7de\u00b7ri\u00b7schen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PTKVZ", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An meine Lagerstatt, mir in die Brust zu dr\u00fccken", "tokens": ["An", "mei\u00b7ne", "La\u00b7ger\u00b7statt", ",", "mir", "in", "die", "Brust", "zu", "dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPER", "APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sein giftiges Geschoss.", "tokens": ["Sein", "gif\u00b7ti\u00b7ges", "Ge\u00b7schoss", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und weh mir! kaum begann die schlummernde Natur", "tokens": ["Und", "weh", "mir", "!", "kaum", "be\u00b7gann", "die", "schlum\u00b7mern\u00b7de", "Na\u00b7tur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den frischen Hauch der Morgenluft zu wittern,", "tokens": ["Den", "fri\u00b7schen", "Hauch", "der", "Mor\u00b7gen\u00b7luft", "zu", "wit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So weckte mich der Schmerz, und ich empfand mit Zittern", "tokens": ["So", "weck\u00b7te", "mich", "der", "Schmerz", ",", "und", "ich", "emp\u00b7fand", "mit", "Zit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Giftes schnelle Spur.", "tokens": ["Des", "Gif\u00b7tes", "schnel\u00b7le", "Spur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Umnebelt war mein Blick, ich f\u00fchlte heisse Glut", "tokens": ["Um\u00b7ne\u00b7belt", "war", "mein", "Blick", ",", "ich", "f\u00fchl\u00b7te", "heis\u00b7se", "Glut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Ungest\u00fcm mein schwindelnd Haupt zernagen,", "tokens": ["Mit", "Un\u00b7ge\u00b7st\u00fcm", "mein", "schwin\u00b7delnd", "Haupt", "zer\u00b7na\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein wankendes Gebein war bis in's Mark zerschlagen,", "tokens": ["Mein", "wan\u00b7ken\u00b7des", "Ge\u00b7bein", "war", "bis", "in's", "Mark", "zer\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "APPR", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und tobend rann mein Blut.", "tokens": ["Und", "to\u00b7bend", "rann", "mein", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "In Flammen eingepresst, als w\u00e4r' ich angethan", "tokens": ["In", "Flam\u00b7men", "ein\u00b7ge\u00b7presst", ",", "als", "w\u00e4r'", "ich", "an\u00b7ge\u00b7than"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$,", "KOKOM", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Nessus Kleid, w\u00e4lzt' ich, nach Heilung lechzend,", "tokens": ["Mit", "Nes\u00b7sus", "Kleid", ",", "w\u00e4lzt'", "ich", ",", "nach", "Hei\u00b7lung", "lech\u00b7zend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,", "VVFIN", "PPER", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mich \u00e4ngstlich her und hin, und rief den Himmel \u00e4chzend", "tokens": ["Mich", "\u00e4ngst\u00b7lich", "her", "und", "hin", ",", "und", "rief", "den", "Him\u00b7mel", "\u00e4ch\u00b7zend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,", "KON", "VVFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Um seinen Beystand an.", "tokens": ["Um", "sei\u00b7nen", "Beys\u00b7tand", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Da sandte dich der Herr, wohlth\u00e4tiger Quarin!", "tokens": ["Da", "sand\u00b7te", "dich", "der", "Herr", ",", "wohlt\u00b7h\u00e4\u00b7ti\u00b7ger", "Qua\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Tausende zum Leben neu geboren,", "tokens": ["Der", "Tau\u00b7sen\u00b7de", "zum", "Le\u00b7ben", "neu", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf die das Schattenreich mit angelweiten Thoren", "tokens": ["Auf", "die", "das", "Schat\u00b7ten\u00b7reich", "mit", "an\u00b7gel\u00b7wei\u00b7ten", "Tho\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bereits zu harren schien.", "tokens": ["Be\u00b7reits", "zu", "har\u00b7ren", "schien", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Befl\u00fcgelt eiltest du, und Hygieja kam", "tokens": ["Be\u00b7fl\u00fc\u00b7gelt", "eil\u00b7test", "du", ",", "und", "Hy\u00b7gie\u00b7ja", "kam"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "$,", "KON", "NE", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "An deinem Arm vertraut einhergegangen:", "tokens": ["An", "dei\u00b7nem", "Arm", "ver\u00b7traut", "ein\u00b7her\u00b7ge\u00b7gan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der G\u00f6ttinn holder Blick, die Anmuth ihrer Wangen", "tokens": ["Der", "G\u00f6t\u00b7tinn", "hol\u00b7der", "Blick", ",", "die", "An\u00b7muth", "ih\u00b7rer", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verscheuchten meinen Gram.", "tokens": ["Ver\u00b7scheuch\u00b7ten", "mei\u00b7nen", "Gram", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Mit br\u00fcnstigem Vertraun fasst' ich, emporgerafft", "tokens": ["Mit", "br\u00fcns\u00b7ti\u00b7gem", "Ver\u00b7traun", "fasst'", "ich", ",", "em\u00b7por\u00b7ge\u00b7rafft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch neuen Muth, ihr Kleid mit schwachen H\u00e4nden:", "tokens": ["Durch", "neu\u00b7en", "Muth", ",", "ihr", "Kleid", "mit", "schwa\u00b7chen", "H\u00e4n\u00b7den", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da str\u00f6mt' in mein Gebein aus ihres Mantels Enden", "tokens": ["Da", "str\u00f6mt'", "in", "mein", "Ge\u00b7bein", "aus", "ih\u00b7res", "Man\u00b7tels", "En\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Urpl\u00f6tzlich Heilungskraft.", "tokens": ["Ur\u00b7pl\u00f6tz\u00b7lich", "Hei\u00b7lungs\u00b7kraft", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Dank sey dir, edler Mann! dir, dem kein heilend Kraut", "tokens": ["Dank", "sey", "dir", ",", "ed\u00b7ler", "Mann", "!", "dir", ",", "dem", "kein", "hei\u00b7lend", "Kraut"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "VAFIN", "PPER", "$,", "ADJA", "NN", "$.", "PPER", "$,", "PRELS", "PIAT", "ADJD", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Verborgen ist vom Grashalm bis zur Eiche,", "tokens": ["Ver\u00b7bor\u00b7gen", "ist", "vom", "Gras\u00b7halm", "bis", "zur", "Ei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPRART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dir, welchem die Natur zu ihrem weiten Reiche", "tokens": ["Dir", ",", "wel\u00b7chem", "die", "Na\u00b7tur", "zu", "ih\u00b7rem", "wei\u00b7ten", "Rei\u00b7che"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NE"], "meter": "-+-+-+-+-+-+-", "measure": "alexan