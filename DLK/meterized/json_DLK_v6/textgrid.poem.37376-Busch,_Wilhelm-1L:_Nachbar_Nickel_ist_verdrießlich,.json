{"textgrid.poem.37376": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nachbar Nickel ist verdrie\u00dflich,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nachbar Nickel ist verdrie\u00dflich,", "tokens": ["Nach\u00b7bar", "Ni\u00b7ckel", "ist", "ver\u00b7drie\u00df\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und er darf sich wohl beklagen,", "tokens": ["Und", "er", "darf", "sich", "wohl", "be\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil ihm seine Pl\u00e4ne schlie\u00dflich", "tokens": ["Weil", "ihm", "sei\u00b7ne", "Pl\u00e4\u00b7ne", "schlie\u00df\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle g\u00e4nzlich fehlgeschlagen.", "tokens": ["Al\u00b7le", "g\u00e4nz\u00b7lich", "fehl\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Unsre Ziege starb heut morgen.", "tokens": ["Uns\u00b7re", "Zie\u00b7ge", "starb", "heut", "mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geh und sag's ihm, lieber Knabe!", "tokens": ["Geh", "und", "sag's", "ihm", ",", "lie\u00b7ber", "Kna\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "PPER", "$,", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df er nach so vielen Sorgen", "tokens": ["Da\u00df", "er", "nach", "so", "vie\u00b7len", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch mal eine Freude habe.", "tokens": ["Auch", "mal", "ei\u00b7ne", "Freu\u00b7de", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}