{"textgrid.poem.53301": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Leich-Text", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach wie hat der Mensch sein Leben", "tokens": ["Ach", "wie", "hat", "der", "Mensch", "sein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "VAFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch so pl\u00f6tzlich auff zu geben!", "tokens": ["Doch", "so", "pl\u00f6tz\u00b7lich", "auff", "zu", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "APPR", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist nicht seine gantze Zeit", "tokens": ["Ist", "nicht", "sei\u00b7ne", "gant\u00b7ze", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gra\u00df, das bald wird abgemeyt?", "tokens": ["Gra\u00df", ",", "das", "bald", "wird", "ab\u00b7ge\u00b7meyt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zwar er bl\u00fcht vnd ist zu schawen", "tokens": ["Zwar", "er", "bl\u00fcht", "vnd", "ist", "zu", "scha\u00b7wen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "KON", "VAFIN", "PTKZU", "VVINF"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Wie die Blum auff gr\u00fcner Awen,", "tokens": ["Wie", "die", "Blum", "auff", "gr\u00fc\u00b7ner", "A\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NE", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die sich in den Sommer-Tagen", "tokens": ["Die", "sich", "in", "den", "Som\u00b7mer\u00b7Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sch\u00f6n vnd pr\u00e4chtig pflegt zu tragen.", "tokens": ["Sch\u00f6n", "vnd", "pr\u00e4ch\u00b7tig", "pflegt", "zu", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn ein Wind sie nur ber\u00fchret,", "tokens": ["Wenn", "ein", "Wind", "sie", "nur", "be\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So wird sie nicht mehr gesp\u00fcret,", "tokens": ["So", "wird", "sie", "nicht", "mehr", "ge\u00b7sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ja sie wei\u00df nicht eins dabey,", "tokens": ["Ja", "sie", "wei\u00df", "nicht", "eins", "da\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "PTKNEG", "PIS", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo sie vor gestanden sey:", "tokens": ["Wo", "sie", "vor", "ge\u00b7stan\u00b7den", "sey", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber Gottes reiche G\u00fcte", "tokens": ["A\u00b7ber", "Got\u00b7tes", "rei\u00b7che", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vnd sein V\u00e4terlich Gem\u00fcte", "tokens": ["Vnd", "sein", "V\u00e4\u00b7ter\u00b7lich", "Ge\u00b7m\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mu\u00df ob denen ewig walten,", "tokens": ["Mu\u00df", "ob", "de\u00b7nen", "e\u00b7wig", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "KOUS", "PDS", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Die in seiner Furcht sich halten.", "tokens": ["Die", "in", "sei\u00b7ner", "Furcht", "sich", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sein gerechtes Recht wird bleiben,", "tokens": ["Sein", "ge\u00b7rech\u00b7tes", "Recht", "wird", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil sich Tag' vnd N\u00e4chte treiben,", "tokens": ["Weil", "sich", "Tag'", "vnd", "N\u00e4ch\u00b7te", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Reicht auff derer Kindes Kind,", "tokens": ["Reicht", "auff", "de\u00b7rer", "Kin\u00b7des", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDS", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die in seinem Bunde sind,", "tokens": ["Die", "in", "sei\u00b7nem", "Bun\u00b7de", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vnd nicht flei\u00df noch Arbeit sparen", "tokens": ["Vnd", "nicht", "flei\u00df", "noch", "Ar\u00b7beit", "spa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "VVFIN", "ADV", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihn im Hertzen zu bewahren,", "tokens": ["Ihn", "im", "Hert\u00b7zen", "zu", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die auff sein Wort achtung geben,", "tokens": ["Die", "auff", "sein", "Wort", "ach\u00b7tung", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Da\u00df sie nach demselben leben.", "tokens": ["Da\u00df", "sie", "nach", "dem\u00b7sel\u00b7ben", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}