{"textgrid.poem.50830": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nein! \u2013 zwischen uns soll Friede sein,", "tokens": ["Nein", "!", "\u2013", "zwi\u00b7schen", "uns", "soll", "Frie\u00b7de", "sein", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "$(", "APPR", "PPER", "VMFIN", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich stell die wei\u00dfe Fahne auf,", "tokens": ["Ich", "stell", "die", "wei\u00b7\u00dfe", "Fah\u00b7ne", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df in geharnischtem Verein", "tokens": ["Da\u00df", "in", "ge\u00b7har\u00b7nischtem", "Ver\u00b7ein"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Wir fahren ", "tokens": ["Wir", "fah\u00b7ren"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Voran! voran! ihr Bittern,", "tokens": ["Vo\u00b7ran", "!", "vo\u00b7ran", "!", "ihr", "Bit\u00b7tern", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$.", "PTKVZ", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In fegenden Gewittern!", "tokens": ["In", "fe\u00b7gen\u00b7den", "Ge\u00b7wit\u00b7tern", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wir aber ziehen hintendrein", "tokens": ["Wir", "a\u00b7ber", "zie\u00b7hen", "hin\u00b7ten\u00b7drein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit klar gestimmten Zithern!", "tokens": ["Mit", "klar", "ge\u00b7stimm\u00b7ten", "Zi\u00b7thern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Ihr seid die feuerschwangre Kraft,", "tokens": ["Ihr", "seid", "die", "feu\u00b7er\u00b7schwang\u00b7re", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Luft und Erde reinigt,", "tokens": ["Die", "Luft", "und", "Er\u00b7de", "rei\u00b7nigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sprengt den entlaubten Eichenschaft,", "tokens": ["Sprengt", "den", "ent\u00b7laub\u00b7ten", "Ei\u00b7chen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Der dorrend \u00fcberm Abgrund steht.", "tokens": ["Der", "dor\u00b7rend", "\u00fc\u00b7berm", "Ab\u00b7grund", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch funkelnd aufgezogen", "tokens": ["Doch", "fun\u00b7kelnd", "auf\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sind ", "tokens": ["Sind"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Der nach dem Sturm am Himmel lacht,", "tokens": ["Der", "nach", "dem", "Sturm", "am", "Him\u00b7mel", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn aller Dunst verflogen.", "tokens": ["Wenn", "al\u00b7ler", "Dunst", "ver\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr seid des Winters kalter Graus,", "tokens": ["Ihr", "seid", "des", "Win\u00b7ters", "kal\u00b7ter", "Graus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verjagt das schw\u00fcle Heidentum,", "tokens": ["Ver\u00b7jagt", "das", "schw\u00fc\u00b7le", "Hei\u00b7den\u00b7tum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr j\u00e4tet Dorn und Distel aus", "tokens": ["Ihr", "j\u00e4\u00b7tet", "Dorn", "und", "Dis\u00b7tel", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und pfl\u00fcgt den starren Acker um.", "tokens": ["Und", "pfl\u00fcgt", "den", "star\u00b7ren", "A\u00b7cker", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch wir auf Lenzesschwingen,", "tokens": ["Doch", "wir", "auf", "Len\u00b7zes\u00b7schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit Spielen und mit Singen,", "tokens": ["Mit", "Spie\u00b7len", "und", "mit", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wir m\u00fcssen in die Furche dann", "tokens": ["Wir", "m\u00fcs\u00b7sen", "in", "die", "Fur\u00b7che", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den neuen Samen bringen!", "tokens": ["Den", "neu\u00b7en", "Sa\u00b7men", "brin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ihr seid die Vorhut und die Wacht,", "tokens": ["Ihr", "seid", "die", "Vor\u00b7hut", "und", "die", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die sengt und brennt in Feindes Land,", "tokens": ["Die", "sengt", "und", "brennt", "in", "Fein\u00b7des", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ihr durchkreuzt die schwarze Nacht", "tokens": ["Und", "ihr", "durch\u00b7kreuzt", "die", "schwar\u00b7ze", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit glei\u00dfend rotem Fackelbrand.", "tokens": ["Mit", "glei\u00b7\u00dfend", "ro\u00b7tem", "Fa\u00b7ckel\u00b7brand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von der Posaunen Schallen", "tokens": ["Von", "der", "Po\u00b7sau\u00b7nen", "Schal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist Jericho gefallen:", "tokens": ["Ist", "Je\u00b7ri\u00b7cho", "ge\u00b7fal\u00b7len", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vor eurem Hauche st\u00fcrzen selbst", "tokens": ["Vor", "eu\u00b7rem", "Hau\u00b7che", "st\u00fcr\u00b7zen", "selbst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des Himmels hohe Hallen!", "tokens": ["Des", "Him\u00b7mels", "ho\u00b7he", "Hal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dann aber folgt die Dichterschar,", "tokens": ["Dann", "a\u00b7ber", "folgt", "die", "Dich\u00b7ter\u00b7schar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die einen neuen Himmel baut,", "tokens": ["Die", "ei\u00b7nen", "neu\u00b7en", "Him\u00b7mel", "baut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Darinnen man im Lichttalar", "tokens": ["Da\u00b7rin\u00b7nen", "man", "im", "Licht\u00b7ta\u00b7lar"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "PIS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den alten Gott der Liebe schaut.", "tokens": ["Den", "al\u00b7ten", "Gott", "der", "Lie\u00b7be", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Voran, voran, ihr Bittern,", "tokens": ["Vo\u00b7ran", ",", "vo\u00b7ran", ",", "ihr", "Bit\u00b7tern", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In fegenden Gewittern!", "tokens": ["In", "fe\u00b7gen\u00b7den", "Ge\u00b7wit\u00b7tern", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mit klar gestimmten Zithern.", "tokens": ["Mit", "klar", "ge\u00b7stimm\u00b7ten", "Zi\u00b7thern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}