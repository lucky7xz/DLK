{"dta.poem.2796": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Dem\u00fcthiges Bu\u00df-Lied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Fliest Thr\u00e4nen/ fliest/", "tokens": ["Fliest", "Thr\u00e4\u00b7nen", "/", "fliest", "/"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "NN", "$(", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Jhr Augen giest", "tokens": ["Ihr", "Au\u00b7gen", "giest"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Erpreste Jammer-Fluthen/", "tokens": ["Er\u00b7pres\u00b7te", "Jam\u00b7mer\u00b7Flu\u00b7then", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schmeltzt in ein Quell", "tokens": ["Sch\u00b7meltzt", "in", "ein", "Quell"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und rinnet hell", "tokens": ["Und", "rin\u00b7net", "hell"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Zu d\u00e4mpffen GOttes Gluthen.", "tokens": ["Zu", "d\u00e4mpf\u00b7fen", "Got\u00b7tes", "Glu\u00b7then", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der S\u00fcnden Brand", "tokens": ["Der", "S\u00fcn\u00b7den", "Brand"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Hat \u00fcberhand", "tokens": ["Hat", "\u00fc\u00b7ber\u00b7hand"], "token_info": ["word", "word"], "pos": ["VAFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "In unserm Fleisch genommen/", "tokens": ["In", "un\u00b7serm", "Fleisch", "ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er w\u00fchlt mit Schmertz", "tokens": ["Er", "w\u00fchlt", "mit", "Schmertz"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Durch Seel und Hertz", "tokens": ["Durch", "Seel", "und", "Hertz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und wird noch weiter kommen.", "tokens": ["Und", "wird", "noch", "wei\u00b7ter", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ver\u00fcbte Schuld", "tokens": ["Ver\u00b7\u00fcb\u00b7te", "Schuld"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Macht Ungedult", "tokens": ["Macht", "Un\u00b7ge\u00b7dult"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und wecket das Gewissen.", "tokens": ["Und", "we\u00b7cket", "das", "Ge\u00b7wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Laster Schmach", "tokens": ["Der", "Las\u00b7ter", "Schmach"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Zeigt nach und nach", "tokens": ["Zeigt", "nach", "und", "nach"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "KON", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Auf was es ist geflissen.", "tokens": ["Auf", "was", "es", "ist", "ge\u00b7flis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie unsre Zeit", "tokens": ["Wie", "uns\u00b7re", "Zeit"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "In Uppigkeit", "tokens": ["In", "Up\u00b7pig\u00b7keit"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "So l\u00fcderlich verschwendet/", "tokens": ["So", "l\u00fc\u00b7der\u00b7lich", "ver\u00b7schwen\u00b7det", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie unser Sinn", "tokens": ["Wie", "un\u00b7ser", "Sinn"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Sich niemals hin", "tokens": ["Sich", "nie\u00b7mals", "hin"], "token_info": ["word", "word", "word"], "pos": ["PRF", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Zum Gottesdienst gewendet.", "tokens": ["Zum", "Got\u00b7tes\u00b7dienst", "ge\u00b7wen\u00b7det", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da sind unrein", "tokens": ["Da", "sind", "un\u00b7rein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Marck/ Blut und Bein", "tokens": ["Marck", "/", "Blut", "und", "Bein"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Samt ihrer faulen H\u00fctte.", "tokens": ["Samt", "ih\u00b7rer", "fau\u00b7len", "H\u00fct\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein Ader schl\u00e4gt/", "tokens": ["Kein", "A\u00b7der", "schl\u00e4gt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Kein Glied sich regt/", "tokens": ["Kein", "Glied", "sich", "regt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PRF", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Da\u00df Frevel nicht zerr\u00fctte.", "tokens": ["Da\u00df", "Fre\u00b7vel", "nicht", "zer\u00b7r\u00fct\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Augen Glantz", "tokens": ["Der", "Au\u00b7gen", "Glantz"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Hat Wollust gantz", "tokens": ["Hat", "Wol\u00b7lust", "gantz"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "NN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mit Hoffart \u00fcberzogen/", "tokens": ["Mit", "Hof\u00b7fart", "\u00fc\u00b7berz\u00b7o\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Angesicht", "tokens": ["Das", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "jst ohne Licht", "tokens": ["jst", "oh\u00b7ne", "Licht"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Dem Dunckeln nachgeflogen.", "tokens": ["Dem", "Dun\u00b7ckeln", "nach\u00b7ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Was nur zu Spott", "tokens": ["Was", "nur", "zu", "Spott"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Dem h\u00f6chsten GOtt", "tokens": ["Dem", "h\u00f6chs\u00b7ten", "Gott"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der Mund hat treiben m\u00f6gen/", "tokens": ["Der", "Mund", "hat", "trei\u00b7ben", "m\u00f6\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das ist geschehn", "tokens": ["Das", "ist", "ge\u00b7schehn"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VAFIN", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "S\u00fcnd\u2019 sch\u00e4ndlich Schm\u00e4hn", "tokens": ["S\u00fcnd'", "sch\u00e4nd\u00b7lich", "Schm\u00e4hn"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJD", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Steht \u00fcberall zugegen.", "tokens": ["Steht", "\u00fc\u00b7be\u00b7rall", "zu\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Was vor Verdru\u00df", "tokens": ["Was", "vor", "Ver\u00b7dru\u00df"], "token_info": ["word", "word", "word"], "pos": ["PWS", "APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Von H\u00e4nd und Fu\u00df", "tokens": ["Von", "H\u00e4nd", "und", "Fu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Dem Sch\u00f6pffer wiederfahren/", "tokens": ["Dem", "Sch\u00f6pf\u00b7fer", "wie\u00b7der\u00b7fah\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bringt der ans Licht/", "tokens": ["Bringt", "der", "ans", "Licht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPRART", "NN", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wenn f\u00fcr Gericht", "tokens": ["Wenn", "f\u00fcr", "Ge\u00b7richt"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "APPR", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Geh\u2019n alle Menschen-Schaaren.", "tokens": ["Geh'n", "al\u00b7le", "Men\u00b7schen\u00b7Schaa\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wo bleibt das Hertz", "tokens": ["Wo", "bleibt", "das", "Hertz"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Das Spiel und Schertz", "tokens": ["Das", "Spiel", "und", "Schertz"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mit GOttes-Wort getrieben?", "tokens": ["Mit", "Got\u00b7tes\u00b7Wort", "ge\u00b7trie\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das feist erstarrt", "tokens": ["Das", "feist", "er\u00b7starrt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und vest verharrt/", "tokens": ["Und", "vest", "ver\u00b7harrt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Auf schn\u00f6dem Jrrthumb blieben.", "tokens": ["Auf", "schn\u00f6\u00b7dem", "Jrr\u00b7thumb", "blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und schl\u00e4fft du noch", "tokens": ["Und", "schl\u00e4fft", "du", "noch"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "An deinem Joch", "tokens": ["An", "dei\u00b7nem", "Joch"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der ungeheuren S\u00fcnden?", "tokens": ["Der", "un\u00b7ge\u00b7heu\u00b7ren", "S\u00fcn\u00b7den", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wach auf! die Zeit", "tokens": ["Wach", "auf", "!", "die", "Zeit"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$.", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ist allbereit", "tokens": ["Ist", "all\u00b7be\u00b7reit"], "token_info": ["word", "word"], "pos": ["VAFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Da GOtt dich heim wird finden.", "tokens": ["Da", "Gott", "