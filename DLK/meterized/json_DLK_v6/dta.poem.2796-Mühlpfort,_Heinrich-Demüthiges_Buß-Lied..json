info": ["word", "word"], "pos": ["PPER", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Dem HErren fl\u00fcchtig werden?", "tokens": ["Dem", "Her\u00b7ren", "fl\u00fcch\u00b7tig", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ach eitler Traum", "tokens": ["Ach", "eit\u00b7ler", "Traum"], "token_info": ["word", "word", "word"], "pos": ["ITJ", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Da ist kein Raum", "tokens": ["Da", "ist", "kein", "Raum"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Auf dieser weiten Erden.", "tokens": ["Auf", "die\u00b7ser", "wei\u00b7ten", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Sein Zorn wird mich", "tokens": ["Sein", "Zorn", "wird", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Behendiglich", "tokens": ["Be\u00b7hen\u00b7dig\u00b7lich"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+---", "measure": "dactylic.init"}, "line.3": {"text": "Auf meine Flucht ergreiffen.", "tokens": ["Auf", "mei\u00b7ne", "Flucht", "er\u00b7greif\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein Donnerstral", "tokens": ["Sein", "Don\u00b7ner\u00b7stral"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Mit tausend Qual", "tokens": ["Mit", "tau\u00b7send", "Qual"], "token_info"