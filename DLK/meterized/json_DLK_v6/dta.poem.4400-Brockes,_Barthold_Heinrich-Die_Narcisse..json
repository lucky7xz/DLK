{"dta.poem.4400": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Narcisse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1727", "urn": "urn:nbn:de:kobv:b4-200905198599", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mu\u00df ich gleich euren Schimmer missen,", "tokens": ["Mu\u00df", "ich", "gleich", "eu\u00b7ren", "Schim\u00b7mer", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr Sternen-f\u00f6rmige Narcissen;", "tokens": ["Ihr", "Ster\u00b7nen\u00b7f\u00f6r\u00b7mi\u00b7ge", "Nar\u00b7cis\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Darf ich darum nicht traurig seyn.", "tokens": ["Darf", "ich", "da\u00b7rum", "nicht", "trau\u00b7rig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PAV", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich seh\u2019 an den Sapphirnen Zimmern", "tokens": ["Ich", "seh'", "an", "den", "Sap\u00b7phir\u00b7nen", "Zim\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Der himmlischen Narcissen Schein", "tokens": ["Der", "himm\u00b7li\u00b7schen", "Nar\u00b7cis\u00b7sen", "Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In noch weit hellerm Lichte schimmern.", "tokens": ["In", "noch", "weit", "hel\u00b7lerm", "Lich\u00b7te", "schim\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "--++-+-+-", "measure": "anapaest.init"}}}}}