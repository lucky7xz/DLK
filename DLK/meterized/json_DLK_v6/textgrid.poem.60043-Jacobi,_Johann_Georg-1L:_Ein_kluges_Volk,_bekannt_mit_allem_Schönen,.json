{"textgrid.poem.60043": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein kluges Volk, bekannt mit allem Sch\u00f6nen,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein kluges Volk, bekannt mit allem Sch\u00f6nen,", "tokens": ["Ein", "klu\u00b7ges", "Volk", ",", "be\u00b7kannt", "mit", "al\u00b7lem", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lie\u00df, in Athen, den weisen Sokrates", "tokens": ["Lie\u00df", ",", "in", "A\u00b7then", ",", "den", "wei\u00b7sen", "Sok\u00b7ra\u00b7tes"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "NE", "$,", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Auf \u00f6ffentlicher B\u00fchne h\u00f6hnen \u2013", "tokens": ["Auf", "\u00f6f\u00b7fent\u00b7li\u00b7cher", "B\u00fch\u00b7ne", "h\u00f6h\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Doch nur von Aristophanes,", "tokens": ["Doch", "nur", "von", "A\u00b7ris\u00b7to\u00b7pha\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Dem Liebling scherzender Cam\u00f6nen;", "tokens": ["Dem", "Lieb\u00b7ling", "scher\u00b7zen\u00b7der", "Ca\u00b7m\u00f6\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und als der weise Mann die B\u00fchne selbst bestieg,", "tokens": ["Und", "als", "der", "wei\u00b7se", "Mann", "die", "B\u00fch\u00b7ne", "selbst", "be\u00b7stieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da \u2013 neigte sich das Volk, und schwieg.", "tokens": ["Da", "\u2013", "neig\u00b7te", "sich", "das", "Volk", ",", "und", "schwieg", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "$(", "VVFIN", "PRF", "ART", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}