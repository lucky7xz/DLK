{"textgrid.poem.35139": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Deine Welt", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denk stets an dich! Nie darfst du dich vergessen.", "tokens": ["Denk", "stets", "an", "dich", "!", "Nie", "darfst", "du", "dich", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "PPER", "$.", "ADV", "VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer sich vergi\u00dft, denkt immer nur an sich.", "tokens": ["Wer", "sich", "ver\u00b7gi\u00dft", ",", "denkt", "im\u00b7mer", "nur", "an", "sich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "VVFIN", "$,", "VVFIN", "ADV", "ADV", "APPR", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es wurde deine Welt dir zugemessen", "tokens": ["Es", "wur\u00b7de", "dei\u00b7ne", "Welt", "dir", "zu\u00b7ge\u00b7mes\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Niemals f\u00fcr dich und dennoch nur f\u00fcr dich.", "tokens": ["Nie\u00b7mals", "f\u00fcr", "dich", "und", "den\u00b7noch", "nur", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "KON", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nimm sie nur hin! Sie ist ja ganz dein Eigen,", "tokens": ["Nimm", "sie", "nur", "hin", "!", "Sie", "ist", "ja", "ganz", "dein", "Ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PTKVZ", "$.", "PPER", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und dennoch soll sie nicht dein Eigen sein.", "tokens": ["Und", "den\u00b7noch", "soll", "sie", "nicht", "dein", "Ei\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nie darf sie dir sich unterth\u00e4nig zeigen,", "tokens": ["Nie", "darf", "sie", "dir", "sich", "un\u00b7ter\u00b7th\u00e4\u00b7nig", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und trotzdem ist sie dein, nur immer dein.", "tokens": ["Und", "trotz\u00b7dem", "ist", "sie", "dein", ",", "nur", "im\u00b7mer", "dein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "PPER", "PPOSAT", "$,", "ADV", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wer an sich denkt und seine Welt bezwingt,", "tokens": ["Wer", "an", "sich", "denkt", "und", "sei\u00b7ne", "Welt", "be\u00b7zwingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PRF", "VVFIN", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Macht sich zur Gabe, die der Welt er bringt.", "tokens": ["Macht", "sich", "zur", "Ga\u00b7be", ",", "die", "der", "Welt", "er", "bringt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPRART", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vergi\u00df dich ganz! Nie darfst du an dich denken.", "tokens": ["Ver\u00b7gi\u00df", "dich", "ganz", "!", "Nie", "darfst", "du", "an", "dich", "den\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$.", "ADV", "VVFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer an sich denkt, vergi\u00dft sich ganz und gar.", "tokens": ["Wer", "an", "sich", "denkt", ",", "ver\u00b7gi\u00dft", "sich", "ganz", "und", "gar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PRF", "VVFIN", "$,", "VVFIN", "PRF", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Strebst du, in deine Welt dich zu versenken,", "tokens": ["Strebst", "du", ",", "in", "dei\u00b7ne", "Welt", "dich", "zu", "ver\u00b7sen\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wird sie nur dir, doch nur f\u00fcr Andre klar.", "tokens": ["Wird", "sie", "nur", "dir", ",", "doch", "nur", "f\u00fcr", "And\u00b7re", "klar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPER", "$,", "ADV", "ADV", "APPR", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gieb sie nur hin! Du darfst sie nicht behalten,", "tokens": ["Gieb", "sie", "nur", "hin", "!", "Du", "darfst", "sie", "nicht", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PTKVZ", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Denn dann, erst dann nimmst du Besitz von ihr.", "tokens": ["Denn", "dann", ",", "erst", "dann", "nimmst", "du", "Be\u00b7sitz", "von", "ihr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "ADV", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "H\u00f6r niemals auf, als Herr sie zu verwalten,", "tokens": ["H\u00f6r", "nie\u00b7mals", "auf", ",", "als", "Herr", "sie", "zu", "ver\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKVZ", "$,", "KOUS", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Denn keinen Augenblick geh\u00f6rt sie dir.", "tokens": ["Denn", "kei\u00b7nen", "Au\u00b7gen\u00b7blick", "ge\u00b7h\u00f6rt", "sie", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wer sich vergi\u00dft und in die Welt versenkt,", "tokens": ["Wer", "sich", "ver\u00b7gi\u00dft", "und", "in", "die", "Welt", "ver\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "VVFIN", "KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Hat sich und sie dem Herrn zur\u00fcckgeschenkt.", "tokens": ["Hat", "sich", "und", "sie", "dem", "Herrn", "zu\u00b7r\u00fcck\u00b7ge\u00b7schenkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "KON", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}