{"dta.poem.18164": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die abwechselnde Zeit  \n Eine weise Einrichtung GOttes  \n f\u00fcr die Menschen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Sonnen schneller Lauf der seinen Zir-", "tokens": ["Der", "Son\u00b7nen", "schnel\u00b7ler", "Lauf", "der", "sei\u00b7nen", "Zir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "ART", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "kel zieht,", "tokens": ["kel", "zieht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Bringt stets ein andres Jahr: indem", "tokens": ["Bringt", "stets", "ein", "and\u00b7res", "Jahr", ":", "in\u00b7dem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$.", "KOUS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "das alte flieht", "tokens": ["das", "al\u00b7te", "flieht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Als wie ein Wassertropf der in ein Meer", "tokens": ["Als", "wie", "ein", "Was\u00b7ser\u00b7tropf", "der", "in", "ein", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "NN", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "verschossen:", "tokens": ["ver\u00b7schos\u00b7sen", ":"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Dies ist ein Bild der Zeit, die ihren Lauf beschlossen.", "tokens": ["Dies", "ist", "ein", "Bild", "der", "Zeit", ",", "die", "ih\u00b7ren", "Lauf", "be\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Zeit ists eigentlich die uns den Kreis der Welt,", "tokens": ["Die", "Zeit", "ists", "ei\u00b7gent\u00b7lich", "die", "uns", "den", "Kreis", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mit der Ver\u00e4nderung zum Schauplatz f\u00fcrgestellt:", "tokens": ["Mit", "der", "Ver\u00b7\u00e4n\u00b7de\u00b7rung", "zum", "Schau\u00b7platz", "f\u00fcr\u00b7ge\u00b7stellt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Worauf die Lebenden nach unsers H\u00f6chsten Schl\u00fcssen,", "tokens": ["Wo\u00b7rauf", "die", "Le\u00b7ben\u00b7den", "nach", "un\u00b7sers", "H\u00f6chs\u00b7ten", "Schl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie es die Ordnung f\u00fcgt die Rolle spielen m\u00fcssen.", "tokens": ["Wie", "es", "die", "Ord\u00b7nung", "f\u00fcgt", "die", "Rol\u00b7le", "spie\u00b7len", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "ART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wer dies mit Achtsamkeit recht siehet und bedenkt", "tokens": ["Wer", "dies", "mit", "Acht\u00b7sam\u00b7keit", "recht", "sie\u00b7het", "und", "be\u00b7denkt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PDS", "APPR", "NN", "ADJD", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der sieht, wohin er nur, das Licht der Augen lenkt,", "tokens": ["Der", "sieht", ",", "wo\u00b7hin", "er", "nur", ",", "das", "Licht", "der", "Au\u00b7gen", "lenkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWAV", "PPER", "ADV", "$,", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die stete Wechselung: da eins das andre bringet,", "tokens": ["Die", "ste\u00b7te", "Wech\u00b7se\u00b7lung", ":", "da", "eins", "das", "and\u00b7re", "brin\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "KOUS", "PIS", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und ehe man es meint auch wiederum verschlinget.", "tokens": ["Und", "e\u00b7he", "man", "es", "meint", "auch", "wie\u00b7de\u00b7rum", "ver\u00b7schlin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PPER", "VVFIN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Dies lehret uns den Satz: die Zeit verfliegt geschwind", "tokens": ["Dies", "leh\u00b7ret", "uns", "den", "Satz", ":", "die", "Zeit", "ver\u00b7fliegt", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "$.", "ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und mit derselbigen, die Dinge die da sind,", "tokens": ["Und", "mit", "der\u00b7sel\u00b7bi\u00b7gen", ",", "die", "Din\u00b7ge", "die", "da", "sind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDS", "$,", "ART", "NN", "ART", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Das hat das ewge Licht, das Sonn und Welt regieret,", "tokens": ["Das", "hat", "das", "ew\u00b7ge", "Licht", ",", "das", "Sonn", "und", "Welt", "re\u00b7gie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Zum Nutz der Sterblichen, so weislich eingef\u00fchret.", "tokens": ["Zum", "Nutz", "der", "Sterb\u00b7li\u00b7chen", ",", "so", "weis\u00b7lich", "ein\u00b7ge\u00b7f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wie br\u00e4chte nicht die Zeit auch seine Wechsel mit,", "tokens": ["Wie", "br\u00e4ch\u00b7te", "nicht", "die", "Zeit", "auch", "sei\u00b7ne", "Wech\u00b7sel", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PTKNEG", "ART", "NN", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und ginge das Geschik mit ihr stets gleichen Schritt:", "tokens": ["Und", "gin\u00b7ge", "das", "Ge\u00b7schik", "mit", "ihr", "stets", "glei\u00b7chen", "Schritt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So w\u00e4re stets die Welt, so lang die Sonne schiene,", "tokens": ["So", "w\u00e4\u00b7re", "stets", "die", "Welt", ",", "so", "lang", "die", "Son\u00b7ne", "schie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "$,", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Des einen Freuden Bahn, des andern Trauer B\u00fchne.", "tokens": ["Des", "ei\u00b7nen", "Freu\u00b7den", "Bahn", ",", "des", "an\u00b7dern", "Trau\u00b7er", "B\u00fch\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ein Cr\u00f6sus lachte stets bei seinen reichen Gl\u00fck,", "tokens": ["Ein", "Cr\u00f6\u00b7sus", "lach\u00b7te", "stets", "bei", "sei\u00b7nen", "rei\u00b7chen", "Gl\u00fck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ein Jrus s\u00e4he nichts als lauter Klag Geschik;", "tokens": ["Ein", "Jrus", "s\u00e4\u00b7he", "nichts", "als", "lau\u00b7ter", "Klag", "Ge\u00b7schik", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "KOKOM", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.26": {"text": "Der eine w\u00fcrde sich noch immer mehr erheben,", "tokens": ["Der", "ei\u00b7ne", "w\u00fcr\u00b7de", "sich", "noch", "im\u00b7mer", "mehr", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "PRF", "ADV", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der andre st\u00fcrbe gar, indem er solte leben.", "tokens": ["Der", "and\u00b7re", "st\u00fcr\u00b7be", "gar", ",", "in\u00b7dem", "er", "sol\u00b7te", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Beseht ihr ", "tokens": ["Be\u00b7seht", "ihr"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.29": {"text": "Ist euch die Welt anjetzt, ein recht gelobtes Land:", "tokens": ["Ist", "euch", "die", "Welt", "an\u00b7jetzt", ",", "ein", "recht", "ge\u00b7lob\u00b7tes", "Land", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVFIN", "$,", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die Zeit ist wechselreich, es kann euch auf der Erden,", "tokens": ["Die", "Zeit", "ist", "wech\u00b7sel\u00b7reich", ",", "es", "kann", "euch", "auf", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PPER", "VMFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Gar bald ein Canaan zur Jammerw\u00fcste werden:", "tokens": ["Gar", "bald", "ein", "Ca\u00b7naan", "zur", "Jam\u00b7mer\u00b7w\u00fcs\u00b7te", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NE", "APPRART", "NN", "VAINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.32": {"text": "Sie \u00e4ndert Freud und Leid; Wer in der Asche sitzt,", "tokens": ["Sie", "\u00e4n\u00b7dert", "Freud", "und", "Leid", ";", "Wer", "in", "der", "A\u00b7sche", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$.", "PWS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Bei der und jener Noth, wie Hiob, Angstschweis schwitzt", "tokens": ["Bei", "der", "und", "je\u00b7ner", "Noth", ",", "wie", "Hiob", ",", "A\u00b7ngstschweis", "schwitzt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "KON", "PDAT", "NN", "$,", "PWAV", "NE", "$,", "NN", "VVFIN"], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.34": {"text": "Dem l\u00e4st die Hofnung doch in Jammer, das Ver-", "tokens": ["Dem", "l\u00e4st", "die", "Hof\u00b7nung", "doch", "in", "Jam\u00b7mer", ",", "das", "Ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "APPR", "NN", "$,", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "gn\u00fcgen,", "tokens": ["gn\u00fc\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.36": {"text": "Er werde mit der Zeit auch seinen Schmerz besiegen.", "tokens": ["Er", "wer\u00b7de", "mit", "der", "Zeit", "auch", "sei\u00b7nen", "Schmerz", "be\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die Zeit vergeht geschwind, es h\u00e4uft sich Jahr auf Jahr,", "tokens": ["Die", "Zeit", "ver\u00b7geht", "ge\u00b7schwind", ",", "es", "h\u00e4uft", "sich", "Jahr", "auf", "Jahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "PPER", "VVFIN", "PRF", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und wie sie uns verl\u00e4uft, das wird man kaum gewahr,", "tokens": ["Und", "wie", "sie", "uns", "ver\u00b7l\u00e4uft", ",", "das", "wird", "man", "kaum", "ge\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPER", "VVFIN", "$,", "PDS", "VAFIN", "PIS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Der Mensch kann selbst davon mit seinen ganzen Leben,", "tokens": ["Der", "Mensch", "kann", "selbst", "da\u00b7von", "mit", "sei\u00b7nen", "gan\u00b7zen", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "PAV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Das ihn verflogen ist, ein klares Zeugnis geben.", "tokens": ["Das", "ihn", "ver\u00b7flo\u00b7gen", "ist", ",", "ein", "kla\u00b7res", "Zeug\u00b7nis", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVPP", "VAFIN", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Der Kindheit erster Stand, da man im Wiegen", "tokens": ["Der", "Kind\u00b7heit", "ers\u00b7ter", "Stand", ",", "da", "man", "im", "Wie\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "KOUS", "PIS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "liegt,", "tokens": ["liegt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.43": {"text": "Da man uns, wie im Traum fast unempfindlich", "tokens": ["Da", "man", "uns", ",", "wie", "im", "Traum", "fast", "un\u00b7emp\u00b7find\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "$,", "PWAV", "APPRART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "wiegt,", "tokens": ["wiegt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.45": {"text": "Nimt einge Jahre weg, die uns von hinnen rennen,", "tokens": ["Nimt", "ein\u00b7ge", "Jah\u00b7re", "weg", ",", "die", "uns", "von", "hin\u00b7nen", "ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "PPER", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Eh wir