{"textgrid.poem.49747": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun welkt, was einstens gr\u00fcn war, Philippine.", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun welkt, was einstens gr\u00fcn war, Philippine.", "tokens": ["Nun", "welkt", ",", "was", "eins\u00b7tens", "gr\u00fcn", "war", ",", "Phil\u00b7ip\u00b7pi\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$,", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach dem Gesetze der Verg\u00e4nglichkeit", "tokens": ["Nach", "dem", "Ge\u00b7set\u00b7ze", "der", "Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Weist die Natur uns ihre Sterbemiene.", "tokens": ["Weist", "die", "Na\u00b7tur", "uns", "ih\u00b7re", "Ster\u00b7be\u00b7mie\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auch uns, Geliebte, droht es seinerzeit!", "tokens": ["Auch", "uns", ",", "Ge\u00b7lieb\u00b7te", ",", "droht", "es", "sei\u00b7ner\u00b7zeit", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O schaue rings um dich! Mit ernsten Lettern", "tokens": ["O", "schau\u00b7e", "rings", "um", "dich", "!", "Mit", "erns\u00b7ten", "Let\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "APPR", "PPER", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schreibt es der Herbst in unser Lebensbuch:", "tokens": ["Schreibt", "es", "der", "Herbst", "in", "un\u00b7ser", "Le\u00b7bens\u00b7buch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir werden nach und nach uns ganz entbl\u00e4ttern,", "tokens": ["Wir", "wer\u00b7den", "nach", "und", "nach", "uns", "ganz", "ent\u00b7bl\u00e4t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "KON", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann, Philippine, kommt das Leichentuch.", "tokens": ["Dann", ",", "Phil\u00b7ip\u00b7pi\u00b7ne", ",", "kommt", "das", "Lei\u00b7chen\u00b7tuch", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NE", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sieh dort am Rand des Waldes: immer gelber", "tokens": ["Sieh", "dort", "am", "Rand", "des", "Wal\u00b7des", ":", "im\u00b7mer", "gel\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADV", "APPRART", "NN", "ART", "NN", "$.", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00e4rbt sich die Linde; gestern war sie gr\u00fcn.", "tokens": ["F\u00e4rbt", "sich", "die", "Lin\u00b7de", ";", "ge\u00b7stern", "war", "sie", "gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NE", "$.", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Und sprich, Geliebte, merkst du es nicht selber,", "tokens": ["Und", "sprich", ",", "Ge\u00b7lieb\u00b7te", ",", "merkst", "du", "es", "nicht", "sel\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "NN", "$,", "VVFIN", "PPER", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df unsre Triebe minder heftig gl\u00fchn?", "tokens": ["Da\u00df", "uns\u00b7re", "Trie\u00b7be", "min\u00b7der", "hef\u00b7tig", "gl\u00fchn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Glocken l\u00e4uten dumpf. 's ist Allerseelen.", "tokens": ["Die", "Glo\u00b7cken", "l\u00e4u\u00b7ten", "dumpf", ".", "'s", "ist", "Al\u00b7ler\u00b7see\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$.", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Man wendet seinen Sinn den Toten zu.", "tokens": ["Man", "wen\u00b7det", "sei\u00b7nen", "Sinn", "den", "To\u00b7ten", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie bald wird eines von uns beiden fehlen!", "tokens": ["Wie", "bald", "wird", "ei\u00b7nes", "von", "uns", "bei\u00b7den", "feh\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "ART", "APPR", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Entweder ich \u2013 entweder oder du!", "tokens": ["Ent\u00b7we\u00b7der", "ich", "\u2013", "ent\u00b7we\u00b7der", "o\u00b7der", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "KON", "KON", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}