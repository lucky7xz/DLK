{"textgrid.poem.26594": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie lieb' ich euch,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie lieb' ich euch,", "tokens": ["Wie", "lieb'", "ich", "euch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Leise schwankende Pappeln,", "tokens": ["Lei\u00b7se", "schwan\u00b7ken\u00b7de", "Pap\u00b7peln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Die ihr, gesammelten Wuchses,", "tokens": ["Die", "ihr", ",", "ge\u00b7sam\u00b7mel\u00b7ten", "Wuch\u00b7ses", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zum Himmel aufstrebt!", "tokens": ["Zum", "Him\u00b7mel", "auf\u00b7strebt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Freilich wohl", "tokens": ["Frei\u00b7lich", "wohl"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Erreicht ihr ihn nicht \u2013", "tokens": ["Er\u00b7reicht", "ihr", "ihn", "nicht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Aber hoch empor ragt ihr", "tokens": ["A\u00b7ber", "hoch", "em\u00b7por", "ragt", "ihr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PTKVZ", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "\u00dcber nied'res Gestr\u00fcpp nicht blo\u00df", "tokens": ["\u00dc\u00b7ber", "nie\u00b7d'\u00b7res", "Ge\u00b7str\u00fcpp", "nicht", "blo\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PTKNEG", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Und den verkr\u00fcppelten Fruchtbaum:", "tokens": ["Und", "den", "ver\u00b7kr\u00fcp\u00b7pel\u00b7ten", "Frucht\u00b7baum", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.10": {"text": "Auch die m\u00e4chtige Eiche,", "tokens": ["Auch", "die", "m\u00e4ch\u00b7ti\u00b7ge", "Ei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.11": {"text": "Die schattenspendende Linde", "tokens": ["Die", "schat\u00b7ten\u00b7spen\u00b7den\u00b7de", "Lin\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "La\u00dft ihr unter euch.", "tokens": ["La\u00dft", "ihr", "un\u00b7ter", "euch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "Und mit ihnen", "tokens": ["Und", "mit", "ih\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "Die dumpfen Wohnungen der Menschen,", "tokens": ["Die", "dum\u00b7pfen", "Woh\u00b7nun\u00b7gen", "der", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Deren kurzer Blick, dem N\u00fctzlichen zugewandt,", "tokens": ["De\u00b7ren", "kur\u00b7zer", "Blick", ",", "dem", "N\u00fctz\u00b7li\u00b7chen", "zu\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "Nur selten an euch, den Nutzlosen,", "tokens": ["Nur", "sel\u00b7ten", "an", "euch", ",", "den", "Nutz\u00b7lo\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Empor sich hebt,", "tokens": ["Em\u00b7por", "sich", "hebt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "Inde\u00df ihr,", "tokens": ["In\u00b7de\u00df", "ihr", ","], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.19": {"text": "Weithin \u00fcberschauend die Landschaft,", "tokens": ["Weit\u00b7hin", "\u00fc\u00b7bersc\u00b7hau\u00b7end", "die", "Land\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.20": {"text": "Selig einsam die H\u00e4upter wieget", "tokens": ["Se\u00b7lig", "ein\u00b7sam", "die", "H\u00e4up\u00b7ter", "wie\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "ART", "NN", "VVFIN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.21": {"text": "Im ewigen \u00c4ther.", "tokens": ["Im", "e\u00b7wi\u00b7gen", "\u00c4\u00b7ther", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}