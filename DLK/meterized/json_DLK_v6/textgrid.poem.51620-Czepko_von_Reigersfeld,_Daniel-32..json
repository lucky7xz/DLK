{"textgrid.poem.51620": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "32.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ertzt ist so von Natur, da\u00df es Gold werden kan,", "tokens": ["Ertzt", "ist", "so", "von", "Na\u00b7tur", ",", "da\u00df", "es", "Gold", "wer\u00b7den", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "APPR", "NN", "$,", "KOUS", "PPER", "NN", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Es ruht nicht, bi\u00df als Gold es die Natur nimmt an.", "tokens": ["Es", "ruht", "nicht", ",", "bi\u00df", "als", "Gold", "es", "die", "Na\u00b7tur", "nimmt", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KON", "KOUS", "NN", "PPER", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}