{"textgrid.poem.56750": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Am Schreibtisch finde ich mich wieder,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Schreibtisch finde ich mich wieder,", "tokens": ["Am", "Schreib\u00b7tisch", "fin\u00b7de", "ich", "mich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "als wie aus krausem Traum erwacht..:", "tokens": ["als", "wie", "aus", "krau\u00b7sem", "Traum", "er\u00b7wacht", "..", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "KOKOM", "APPR", "ADJA", "NN", "VVPP", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor mir ein Buch seltsamer Lieder,", "tokens": ["Vor", "mir", "ein", "Buch", "selt\u00b7sa\u00b7mer", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und um mich stille Mondesnacht.", "tokens": ["und", "um", "mich", "stil\u00b7le", "Mon\u00b7des\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich schaue auf den kleinen Ort,", "tokens": ["Ich", "schau\u00b7e", "auf", "den", "klei\u00b7nen", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "aus dem mein Geist im Zorn geflohn: \u2013", "tokens": ["aus", "dem", "mein", "Geist", "im", "Zorn", "ge\u00b7flohn", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nachtw\u00e4chter ruft sein Hirtenwort", "tokens": ["Nacht\u00b7w\u00e4ch\u00b7ter", "ruft", "sein", "Hir\u00b7ten\u00b7wort"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "zu greiser Turmuhr biedrem Ton ...", "tokens": ["zu", "grei\u00b7ser", "Tur\u00b7muhr", "bied\u00b7rem", "Ton", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wie knochige Philisterglatzen", "tokens": ["Wie", "kno\u00b7chi\u00b7ge", "Phi\u00b7lis\u00b7ter\u00b7glat\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "ergl\u00e4nzt des Pflasters holprig Beet ...", "tokens": ["er\u00b7gl\u00e4nzt", "des", "Pflas\u00b7ters", "holp\u00b7rig", "Beet", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und auf den Giebeln weinen Katzen", "tokens": ["Und", "auf", "den", "Gie\u00b7beln", "wei\u00b7nen", "Kat\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "um ein versagtes t\u00eate-\u00e0-t\u00eate.", "tokens": ["um", "ein", "ver\u00b7sag\u00b7tes", "t\u00eate\u00b7\u00e0\u00b7t\u00eate", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Euch also, winklige Gem\u00e4uer,", "tokens": ["Euch", "al\u00b7so", ",", "wink\u00b7li\u00b7ge", "Ge\u00b7m\u00e4u\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "durchschnarcht von edlen Atta Trolls,", "tokens": ["durch\u00b7schnarcht", "von", "ed\u00b7len", "At\u00b7ta", "Trolls", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "bewarf ich einst mit wildem Feuer", "tokens": ["be\u00b7warf", "ich", "einst", "mit", "wil\u00b7dem", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "aus den Vulkanen meines Grolls!", "tokens": ["aus", "den", "Vul\u00b7ka\u00b7nen", "mei\u00b7nes", "Grolls", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Ich sah in eurer Kleinlichkeit", "tokens": ["Ich", "sah", "in", "eu\u00b7rer", "Klein\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "die Welt, die in mir selbst ich trug:", "tokens": ["die", "Welt", ",", "die", "in", "mir", "selbst", "ich", "trug", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "es war ein St\u00fcck Vergangenheit,", "tokens": ["es", "war", "ein", "St\u00fcck", "Ver\u00b7gan\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "das ich in eurem Bild zerschlug.", "tokens": ["das", "ich", "in", "eu\u00b7rem", "Bild", "zer\u00b7schlug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Von oben hab ich lachen lernen", "tokens": ["Von", "o\u00b7ben", "hab", "ich", "la\u00b7chen", "ler\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VAFIN", "PPER", "VVINF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "auf euer enges Kreuz und Quer!", "tokens": ["auf", "eu\u00b7er", "en\u00b7ges", "Kreuz", "und", "Quer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wer Kurzweil trieb mit Sonn und Sternen,", "tokens": ["Wer", "Kurz\u00b7weil", "trieb", "mit", "Sonn", "und", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "dem seid ihr kein Memento mehr!", "tokens": ["dem", "seid", "ihr", "kein", "Me\u00b7men\u00b7to", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "In tiefentz\u00fcckten Weihestunden", "tokens": ["In", "tie\u00b7fent\u00b7z\u00fcck\u00b7ten", "Wei\u00b7hes\u00b7tun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "fernab dem Staub der breiten Spur,", "tokens": ["fern\u00b7ab", "dem", "Staub", "der", "brei\u00b7ten", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "hab ich mich wieder heimgefunden", "tokens": ["hab", "ich", "mich", "wie\u00b7der", "heim\u00b7ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.16": {"text": "zum Mutterherzen der Natur!", "tokens": ["zum", "Mut\u00b7ter\u00b7her\u00b7zen", "der", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "In ihm ist alles gro\u00df und echt,", "tokens": ["In", "ihm", "ist", "al\u00b7les", "gro\u00df", "und", "echt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "von gut und b\u00f6se unentweiht:", "tokens": ["von", "gut", "und", "b\u00f6\u00b7se", "un\u00b7ent\u00b7weiht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sch\u00f6nheit ist Kraft ihm, Kraft ihm Recht,", "tokens": ["Sch\u00f6n\u00b7heit", "ist", "Kraft", "ihm", ",", "Kraft", "ihm", "Recht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "PPER", "$,", "NN", "PPER", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "sein Pulsschlag ist die Ewigkeit.", "tokens": ["sein", "Puls\u00b7schlag", "ist", "die", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wen dieser Mutter H\u00e4nde leiten", "tokens": ["Wen", "die\u00b7ser", "Mut\u00b7ter", "H\u00e4n\u00b7de", "lei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PDAT", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "vom Heut ins Ewige hinein,", "tokens": ["vom", "Heut", "ins", "E\u00b7wi\u00b7ge", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "der lernt den Schritt des Siegers schreiten,", "tokens": ["der", "lernt", "den", "Schritt", "des", "Sie\u00b7gers", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "und Mensch sein hei\u00dft ihm K\u00f6nig sein!", "tokens": ["und", "Mensch", "sein", "hei\u00dft", "ihm", "K\u00f6\u00b7nig", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAINF", "VVFIN", "PPER", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}