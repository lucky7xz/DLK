{"dta.poem.9574": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die verlohrne und wieder gefundene Venus.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach! mutter trockne doch die thr\u00e4nen von den wangen/", "tokens": ["Ach", "!", "mut\u00b7ter", "trock\u00b7ne", "doch", "die", "thr\u00e4\u00b7nen", "von", "den", "wan\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die dein betr\u00fcbtes kind mit seuffzen nach dir schickt!", "tokens": ["Die", "dein", "be\u00b7tr\u00fcb\u00b7tes", "kind", "mit", "seuff\u00b7zen", "nach", "dir", "schickt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "APPR", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich habe dich verlohrn! o kummer/ der mich dr\u00fcckt!", "tokens": ["Ich", "ha\u00b7be", "dich", "ver\u00b7lohrn", "!", "o", "kum\u00b7mer", "/", "der", "mich", "dr\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "$.", "FM", "NN", "$(", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo bist du? la\u00df mich dich nur einmahl noch umfangen!", "tokens": ["Wo", "bist", "du", "?", "la\u00df", "mich", "dich", "nur", "ein\u00b7mahl", "noch", "um\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "VVIMP", "PPER", "PRF", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Komm! hertzens-mutter komm! ach komm doch bald gegangen/", "tokens": ["Komm", "!", "hert\u00b7zens\u00b7mut\u00b7ter", "komm", "!", "ach", "komm", "doch", "bald", "ge\u00b7gan\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "NN", "VVFIN", "$.", "ADV", "VVFIN", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Eh\u2019 geist und leben mir aus blut und adern r\u00fcckt!", "tokens": ["Eh'", "geist", "und", "le\u00b7ben", "mir", "aus", "blut", "und", "a\u00b7dern", "r\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "VVFIN", "PPER", "APPR", "NN", "KON", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So klagte Cupido in angst und pein verstrickt/", "tokens": ["So", "klag\u00b7te", "Cu\u00b7pi\u00b7do", "in", "angst", "und", "pein", "ver\u00b7strickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und suchte/ doch umsonst/ die Venus mit verlangen/", "tokens": ["Und", "such\u00b7te", "/", "doch", "um\u00b7sonst", "/", "die", "Ve\u00b7nus", "mit", "ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "ADV", "ADV", "$(", "ART", "NN", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er weinte fort f\u00fcr fort. Hier aber wei\u00df ich nicht/", "tokens": ["Er", "wein\u00b7te", "fort", "f\u00fcr", "fort", ".", "Hier", "a\u00b7ber", "wei\u00df", "ich", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "PTKVZ", "$.", "ADV", "ADV", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Woher ihm Solime kam pl\u00f6tzlich zu gesicht;", "tokens": ["Wo\u00b7her", "ihm", "So\u00b7li\u00b7me", "kam", "pl\u00f6tz\u00b7lich", "zu", "ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVFIN", "ADJD", "PTKZU", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie ward er nicht erfreut bey so begl\u00fcckten stunden?", "tokens": ["Wie", "ward", "er", "nicht", "er\u00b7freut", "bey", "so", "be\u00b7gl\u00fcck\u00b7ten", "stun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PTKNEG", "ADJD", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Er zog die thr\u00e4nen ein/ man sahe/ wie er lieff", "tokens": ["Er", "zog", "die", "thr\u00e4\u00b7nen", "ein", "/", "man", "sa\u00b7he", "/", "wie", "er", "lieff"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "$(", "PIS", "VVFIN", "$(", "PWAV", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und mehr denn tausendmal ihr di\u00df entgegen rieff:", "tokens": ["Und", "mehr", "denn", "tau\u00b7send\u00b7mal", "ihr", "di\u00df", "ent\u00b7ge\u00b7gen", "rieff", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "PPER", "PDS", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Bleib/ sch\u00f6nste mutter/ bleib/ itzt hab ich dich gefunden!", "tokens": ["Bleib", "/", "sch\u00f6ns\u00b7te", "mut\u00b7ter", "/", "bleib", "/", "itzt", "hab", "ich", "dich", "ge\u00b7fun\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADJA", "NN", "$(", "VVFIN", "$(", "ADV", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}