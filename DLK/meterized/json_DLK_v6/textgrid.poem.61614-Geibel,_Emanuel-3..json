{"textgrid.poem.61614": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo des \u00d6lwalds Schatten d\u00e4mmern,", "tokens": ["Wo", "des", "\u00d6l\u00b7walds", "Schat\u00b7ten", "d\u00e4m\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rast' ich matt vom Sonnenschein;", "tokens": ["Rast'", "ich", "matt", "vom", "Son\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fern am Berg bei ihren L\u00e4mmern", "tokens": ["Fern", "am", "Berg", "bei", "ih\u00b7ren", "L\u00e4m\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lagern Hirten und schalmein.", "tokens": ["La\u00b7gern", "Hir\u00b7ten", "und", "schal\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "ADJD", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "M\u00fcd eint\u00f6nig schwimmt die Weise", "tokens": ["M\u00fcd", "ein\u00b7t\u00f6\u00b7nig", "schwimmt", "die", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch den Mittagsduft heran,", "tokens": ["Durch", "den", "Mit\u00b7tags\u00b7duft", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mir tr\u00e4umt, es sei das leise", "tokens": ["Und", "mir", "tr\u00e4umt", ",", "es", "sei", "das", "lei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fl\u00f6tenspiel des gro\u00dfen Pan.", "tokens": ["Fl\u00f6\u00b7ten\u00b7spiel", "des", "gro\u00b7\u00dfen", "Pan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}