{"textgrid.poem.55658": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Vertrauter", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast so manche Bitte gew\u00e4hrt,", "tokens": ["Du", "hast", "so", "man\u00b7che", "Bit\u00b7te", "ge\u00b7w\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und wenn sie dir auch sch\u00e4dlich war;", "tokens": ["Und", "wenn", "sie", "dir", "auch", "sch\u00e4d\u00b7lich", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der gute Mann da hat wenig begehrt.", "tokens": ["Der", "gu\u00b7te", "Mann", "da", "hat", "we\u00b7nig", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dabei hat es doch keine Gefahr.", "tokens": ["Da\u00b7bei", "hat", "es", "doch", "kei\u00b7ne", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}}}}