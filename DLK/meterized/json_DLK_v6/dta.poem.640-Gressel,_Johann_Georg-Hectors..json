{"dta.poem.640": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Hectors.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich war ein starcker Held/ und w\u00fcrde noch wol leben", "tokens": ["Ich", "war", "ein", "star\u00b7cker", "Held", "/", "und", "w\u00fcr\u00b7de", "noch", "wol", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "KON", "VAFIN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn ich die Tapfferkeit nur besser angewandt/", "tokens": ["Wenn", "ich", "die", "Tapf\u00b7fer\u00b7keit", "nur", "bes\u00b7ser", "an\u00b7ge\u00b7wandt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So bald ich aber wolt der Bo\u00dfheit-Schild abgeben/", "tokens": ["So", "bald", "ich", "a\u00b7ber", "wolt", "der", "Bo\u00df\u00b7heit\u00b7Schild", "ab\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-++--", "measure": "unknown.measure.hexa"}, "line.4": {"text": "So bald er legte mich ", "tokens": ["So", "bald", "er", "leg\u00b7te", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}