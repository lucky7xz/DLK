{"dta.poem.6418": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geschmack.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Saure wird im Feur stumpf und verzehret sich.", "tokens": ["Das", "Sau\u00b7re", "wird", "im", "Feur", "stumpf", "und", "ver\u00b7zeh\u00b7ret", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "ADJD", "KON", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was bittres trifft man an in C\u00f6rpern, die verbrannt.", "tokens": ["Was", "bitt\u00b7res", "trifft", "man", "an", "in", "C\u00f6r\u00b7pern", ",", "die", "ver\u00b7brannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "PIS", "APPR", "APPR", "NE", "$,", "PRELS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In reiffen Fr\u00fcchten wird so herb als saur erkannt,", "tokens": ["In", "reif\u00b7fen", "Fr\u00fcch\u00b7ten", "wird", "so", "herb", "als", "saur", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ADV", "ADJD", "KOKOM", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "S\u00fc\u00df sind sie reiff: gantz bitter, widerlich", "tokens": ["S\u00fc\u00df", "sind", "sie", "reiff", ":", "gantz", "bit\u00b7ter", ",", "wi\u00b7der\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "VAFIN", "PPER", "VVFIN", "$.", "ADV", "ADJD", "$,", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sind sie, wenn sie die F\u00e4ulni\u00df trennt.", "tokens": ["Sind", "sie", ",", "wenn", "sie", "die", "F\u00e4ul\u00b7ni\u00df", "trennt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}