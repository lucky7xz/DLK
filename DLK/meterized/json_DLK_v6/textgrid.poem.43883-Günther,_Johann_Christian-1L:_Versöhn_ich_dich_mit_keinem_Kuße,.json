{"textgrid.poem.43883": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vers\u00f6hn ich dich mit keinem Ku\u00dfe,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vers\u00f6hn ich dich mit keinem Ku\u00dfe,", "tokens": ["Ver\u00b7s\u00f6hn", "ich", "dich", "mit", "kei\u00b7nem", "Ku\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So brich mir nur das Herz entzwey.", "tokens": ["So", "brich", "mir", "nur", "das", "Herz", "ent\u00b7zwey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wasche deinen Fu\u00df mit Thr\u00e4nen,", "tokens": ["Ich", "wa\u00b7sche", "dei\u00b7nen", "Fu\u00df", "mit", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vergieb und h\u00f6re dies mein Sehnen;", "tokens": ["Ver\u00b7gieb", "und", "h\u00f6\u00b7re", "dies", "mein", "Seh\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KON", "VVFIN", "PDS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Erkennen ist die beste Reu", "tokens": ["Er\u00b7ken\u00b7nen", "ist", "die", "bes\u00b7te", "Reu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und nicht mehr thun die beste Bu\u00dfe.", "tokens": ["Und", "nicht", "mehr", "thun", "die", "bes\u00b7te", "Bu\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "VVINF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du bist die F\u00fcrstin unsrer Sch\u00f6nen,", "tokens": ["Du", "bist", "die", "F\u00fcrs\u00b7tin", "uns\u00b7rer", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Zorn verl\u00e4st die Majest\u00e4t,", "tokens": ["Der", "Zorn", "ver\u00b7l\u00e4st", "die", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Himmel k\u00fcst uns nach dem Schmerze;", "tokens": ["Der", "Him\u00b7mel", "k\u00fcst", "uns", "nach", "dem", "Schmer\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du bist sein Bild, trag auch sein Herze,", "tokens": ["Du", "bist", "sein", "Bild", ",", "trag", "auch", "sein", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "VAINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das Sanftmuth giebt, wenn Demuth fleht;", "tokens": ["Das", "Sanft\u00b7muth", "giebt", ",", "wenn", "De\u00b7muth", "fleht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sonst d\u00f6rfte dich der P\u00f6bel h\u00f6hnen.", "tokens": ["Sonst", "d\u00f6rf\u00b7te", "dich", "der", "P\u00f6\u00b7bel", "h\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Befiehl mir, was du wilst, zur Strafe;", "tokens": ["Be\u00b7fiehl", "mir", ",", "was", "du", "wilst", ",", "zur", "Stra\u00b7fe", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PWS", "PPER", "VMFIN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich leide gern, sey du nur gut", "tokens": ["Ich", "lei\u00b7de", "gern", ",", "sey", "du", "nur", "gut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "VAFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schone mich mit deinem Grolle.", "tokens": ["Und", "scho\u00b7ne", "mich", "mit", "dei\u00b7nem", "Grol\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gesezt auch, da\u00df ich sterben solle,", "tokens": ["Ge\u00b7sezt", "auch", ",", "da\u00df", "ich", "ster\u00b7ben", "sol\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich leide lieber Beil und Glut", "tokens": ["Ich", "lei\u00b7de", "lie\u00b7ber", "Beil", "und", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als diese Marter in dem Schlafe.", "tokens": ["Als", "die\u00b7se", "Mar\u00b7ter", "in", "dem", "Schla\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Im Schlafe werd ich deinem Grimme", "tokens": ["Im", "Schla\u00b7fe", "werd", "ich", "dei\u00b7nem", "Grim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zur sch\u00e4rfsten Marter dargestellt;", "tokens": ["Zur", "sch\u00e4rfs\u00b7ten", "Mar\u00b7ter", "dar\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da foltern mich die sauren Blicke,", "tokens": ["Da", "fol\u00b7tern", "mich", "die", "sau\u00b7ren", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da macht dein Eifer, da\u00df ich z\u00fccke,", "tokens": ["Da", "macht", "dein", "Ei\u00b7fer", ",", "da\u00df", "ich", "z\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da flieh ich, wenn dein Donner f\u00e4llt,", "tokens": ["Da", "flieh", "ich", ",", "wenn", "dein", "Don\u00b7ner", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als wie ein Wild vors J\u00e4gers Stimme.", "tokens": ["Als", "wie", "ein", "Wild", "vors", "J\u00e4\u00b7gers", "Stim\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NE", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du solst nur sehn, du solst nur h\u00f6ren,", "tokens": ["Du", "solst", "nur", "sehn", ",", "du", "solst", "nur", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$,", "PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich will davor erkenntlich seyn;", "tokens": ["Ich", "will", "da\u00b7vor", "er\u00b7kennt\u00b7lich", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PAV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Amor sezt sich dir zum B\u00fcrgen:", "tokens": ["Mein", "A\u00b7mor", "sezt", "sich", "dir", "zum", "B\u00fcr\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "VVFIN", "PRF", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich will dir keine Tauben w\u00fcrgen,", "tokens": ["Ich", "will", "dir", "kei\u00b7ne", "Tau\u00b7ben", "w\u00fcr\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich will ein be\u00dfer Opfer weihn,", "tokens": ["Ich", "will", "ein", "be\u00b7\u00dfer", "Op\u00b7fer", "weihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.6": {"text": "Ich will dich mit Gehorsam ehren.", "tokens": ["Ich", "will", "dich", "mit", "Ge\u00b7hor\u00b7sam", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Jedoch erscheint die lezte Stunde", "tokens": ["Je\u00b7doch", "er\u00b7scheint", "die", "lez\u00b7te", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ist kein Rettungsmittel hier,", "tokens": ["Und", "ist", "kein", "Ret\u00b7tungs\u00b7mit\u00b7tel", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So las den Tod dem Fehler weichen,", "tokens": ["So", "las", "den", "Tod", "dem", "Feh\u00b7ler", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Truncke will ich dich beschleichen,", "tokens": ["Im", "Trun\u00b7cke", "will", "ich", "dich", "be\u00b7schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ers\u00e4ufe mich in Malvasier \u2013", "tokens": ["Er\u00b7s\u00e4u\u00b7fe", "mich", "in", "Mal\u00b7va\u00b7sier", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich mein auf deinem sch\u00f6nen Munde.", "tokens": ["Ich", "mein", "auf", "dei\u00b7nem", "sch\u00f6\u00b7nen", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}