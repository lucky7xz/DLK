{"textgrid.poem.45277": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[sie sehn die Flut das Hoch und Niedre mischene]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie sehn die Flut das Hoch und Niedre mischen,", "tokens": ["Sie", "sehn", "die", "Flut", "das", "Hoch", "und", "Nied\u00b7re", "mi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "KON", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und alle zittern bang vor der Gefahr.", "tokens": ["Und", "al\u00b7le", "zit\u00b7tern", "bang", "vor", "der", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein jeder m\u00f6chte gern das Wasser klar,", "tokens": ["Ein", "je\u00b7der", "m\u00f6ch\u00b7te", "gern", "das", "Was\u00b7ser", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch vorher noch im Tr\u00fcben fischen.", "tokens": ["Doch", "vor\u00b7her", "noch", "im", "Tr\u00fc\u00b7ben", "fi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}