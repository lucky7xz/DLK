{"dta.poem.20901": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Besteht nicht die Unsterbligkeit/", "tokens": ["Be\u00b7steht", "nicht", "die", "U\u00b7nster\u00b7blig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "So wird der Geist/ wann er befreyt/", "tokens": ["So", "wird", "der", "Geist", "/", "wann", "er", "be\u00b7freyt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$(", "PWAV", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Leib/ und was er vor beweget/", "tokens": ["Den", "Leib", "/", "und", "was", "er", "vor", "be\u00b7we\u00b7get", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KON", "PWS", "PPER", "APPR", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "zu Staub und Asche/ ja die Welt/", "tokens": ["zu", "Staub", "und", "A\u00b7sche", "/", "ja", "die", "Welt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$(", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und was sie im Beschlosse h\u00e4lt/", "tokens": ["Und", "was", "sie", "im", "Be\u00b7schlos\u00b7se", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wird in den Todes Staub geleget;", "tokens": ["Wird", "in", "den", "To\u00b7des", "Staub", "ge\u00b7le\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es wiche selbst des Himmels Zier/", "tokens": ["Es", "wi\u00b7che", "selbst", "des", "Him\u00b7mels", "Zier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die G\u00f6tter st\u00fcrben gleich wie wir.", "tokens": ["Die", "G\u00f6t\u00b7ter", "st\u00fcr\u00b7ben", "gleich", "wie", "wir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}