{"textgrid.poem.56076": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Rosa Hortensie", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer nahm das Rosa an? Wer wu\u00dfte auch,", "tokens": ["Wer", "nahm", "das", "Ro\u00b7sa", "an", "?", "Wer", "wu\u00df\u00b7te", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PTKVZ", "$.", "PWS", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "da\u00df es sich sammelte in diesen Dolden?", "tokens": ["da\u00df", "es", "sich", "sam\u00b7mel\u00b7te", "in", "die\u00b7sen", "Dol\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Wie Dinge unter Gold, die sich entgolden,", "tokens": ["Wie", "Din\u00b7ge", "un\u00b7ter", "Gold", ",", "die", "sich", "ent\u00b7gol\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "$,", "PRELS", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "entr\u00f6ten sie sich sanft, wie im Gebrauch.", "tokens": ["ent\u00b7r\u00f6\u00b7ten", "sie", "sich", "sanft", ",", "wie", "im", "Ge\u00b7brauch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "$,", "PWAV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da\u00df sie f\u00fcr solches Rosa nichts verlangen.", "tokens": ["Da\u00df", "sie", "f\u00fcr", "sol\u00b7ches", "Ro\u00b7sa", "nichts", "ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bleibt es f\u00fcr sie und l\u00e4chelt aus der Luft?", "tokens": ["Bleibt", "es", "f\u00fcr", "sie", "und", "l\u00e4\u00b7chelt", "aus", "der", "Luft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sind Engel da, es z\u00e4rtlich zu empfangen,", "tokens": ["Sind", "En\u00b7gel", "da", ",", "es", "z\u00e4rt\u00b7lich", "zu", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADV", "$,", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "wenn es vergeht, gro\u00dfm\u00fctig wie ein Duft?", "tokens": ["wenn", "es", "ver\u00b7geht", ",", "gro\u00df\u00b7m\u00fc\u00b7tig", "wie", "ein", "Duft", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Oder vielleicht auch geben sie es preis,", "tokens": ["O\u00b7der", "viel\u00b7leicht", "auch", "ge\u00b7ben", "sie", "es", "preis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "damit es nie erf\u00fchre vom Verbl\u00fchn.", "tokens": ["da\u00b7mit", "es", "nie", "er\u00b7f\u00fch\u00b7re", "vom", "Ver\u00b7bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch unter diesem Rosa hat ein Gr\u00fcn", "tokens": ["Doch", "un\u00b7ter", "die\u00b7sem", "Ro\u00b7sa", "hat", "ein", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "gehorcht, das jetzt verwelkt und alles wei\u00df.", "tokens": ["ge\u00b7horcht", ",", "das", "jetzt", "ver\u00b7welkt", "und", "al\u00b7les", "wei\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRELS", "ADV", "VVPP", "KON", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}