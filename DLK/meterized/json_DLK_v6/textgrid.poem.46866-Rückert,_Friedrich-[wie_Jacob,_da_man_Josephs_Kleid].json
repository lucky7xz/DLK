{"textgrid.poem.46866": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wie Jacob, da man Josephs Kleid]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Jacob, da man Josephs Kleid", "tokens": ["Wie", "Ja\u00b7cob", ",", "da", "man", "Jo\u00b7se\u00b7phs", "Kleid"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "$,", "KOUS", "PIS", "NE", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Ihm brachte;", "tokens": ["Ihm", "brach\u00b7te", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und sein begrabnes Herzeleid", "tokens": ["Und", "sein", "be\u00b7grab\u00b7nes", "Her\u00b7ze\u00b7leid"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erwachte;", "tokens": ["Er\u00b7wach\u00b7te", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Der Vater roch des Kleides Duft,", "tokens": ["Der", "Va\u00b7ter", "roch", "des", "Klei\u00b7des", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da schwebte", "tokens": ["Da", "schweb\u00b7te"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Der Sohn im Duft aus seiner Gruft,", "tokens": ["Der", "Sohn", "im", "Duft", "aus", "sei\u00b7ner", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lebte.", "tokens": ["Und", "leb\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Er lebte wirklich, den er todt", "tokens": ["Er", "leb\u00b7te", "wirk\u00b7lich", ",", "den", "er", "todt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PRELS", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geglaubet,", "tokens": ["Ge\u00b7glau\u00b7bet", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "O sei der Trost nicht meiner Noth", "tokens": ["O", "sei", "der", "Trost", "nicht", "mei\u00b7ner", "Noth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geraubet!", "tokens": ["Ge\u00b7rau\u00b7bet", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Nie soll der Duft, es lebt mein Sohn,", "tokens": ["Nie", "soll", "der", "Duft", ",", "es", "lebt", "mein", "Sohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Mir schwinden,", "tokens": ["Mir", "schwin\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Ich werd' an eines K\u00f6nigs Thron", "tokens": ["Ich", "werd'", "an", "ei\u00b7nes", "K\u00f6\u00b7nigs", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn finden.", "tokens": ["Ihn", "fin\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}