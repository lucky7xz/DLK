{"textgrid.poem.66259": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Deutscher Spruch", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ", "tokens": ["Wenn"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Nicht mehr mit Auslands Puppen spielt;", "tokens": ["Nicht", "mehr", "mit", "Aus\u00b7lands", "Pup\u00b7pen", "spielt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die alte deutsche Sitt' und Art", "tokens": ["Die", "al\u00b7te", "deut\u00b7sche", "Sitt'", "und", "Art"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Wort und Wandel treu bewahrt;", "tokens": ["In", "Wort", "und", "Wan\u00b7del", "treu", "be\u00b7wahrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Christenglauben nie verletzt,", "tokens": ["Den", "Chris\u00b7ten\u00b7glau\u00b7ben", "nie", "ver\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Wahrheit \u00fcber alles sch\u00e4tzt;", "tokens": ["Und", "Wahr\u00b7heit", "\u00fc\u00b7ber", "al\u00b7les", "sch\u00e4tzt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht Irrwischlicht Aufkl\u00e4rung nennt,", "tokens": ["Nicht", "Irr\u00b7wi\u00b7schlicht", "Auf\u00b7kl\u00e4\u00b7rung", "nennt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Weil es die Leuchte Gottes kennt;", "tokens": ["Weil", "es", "die", "Leuch\u00b7te", "Got\u00b7tes", "kennt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wenn Mannkraft, wie zu Hermanns Zeit,", "tokens": ["Wenn", "Mann\u00b7kraft", ",", "wie", "zu", "Her\u00b7manns", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PWAV", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Den Enkel st\u00e4hlt mit Tapferkeit;", "tokens": ["Den", "En\u00b7kel", "st\u00e4hlt", "mit", "Tap\u00b7fer\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wenn Deutschland all dies thut und h\u00e4lt,", "tokens": ["Wenn", "Deutschland", "all", "dies", "thut", "und", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PIAT", "PDS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "So wird's das erste Land der Welt.", "tokens": ["So", "wird's", "das", "ers\u00b7te", "Land", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}