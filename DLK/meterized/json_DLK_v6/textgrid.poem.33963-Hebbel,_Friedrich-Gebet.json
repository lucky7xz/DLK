{"textgrid.poem.33963": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Gebet", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die du \u00fcber die Sterne weg", "tokens": ["Die", "du", "\u00fc\u00b7ber", "die", "Ster\u00b7ne", "weg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "PTKVZ"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Mit der geleerten Schaale", "tokens": ["Mit", "der", "ge\u00b7leer\u00b7ten", "Schaa\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aufschwebst, um sie am ew'gen Born", "tokens": ["Auf\u00b7schwebst", ",", "um", "sie", "am", "ew'\u00b7gen", "Born"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUI", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eilig wieder zu f\u00fcllen:", "tokens": ["Ei\u00b7lig", "wie\u00b7der", "zu", "f\u00fcl\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Einmal schwenke sie noch, o Gl\u00fcck,", "tokens": ["Ein\u00b7mal", "schwen\u00b7ke", "sie", "noch", ",", "o", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "FM", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Einmal, l\u00e4chelnde G\u00f6ttin!", "tokens": ["Ein\u00b7mal", ",", "l\u00e4\u00b7cheln\u00b7de", "G\u00f6t\u00b7tin", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Sieh, ein einziger Tropfen h\u00e4ngt", "tokens": ["Sieh", ",", "ein", "ein\u00b7zi\u00b7ger", "Trop\u00b7fen", "h\u00e4ngt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Noch verloren am Rande,", "tokens": ["Noch", "ver\u00b7lo\u00b7ren", "am", "Ran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPRART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.9": {"text": "Und der einzige Tropfen gen\u00fcgt,", "tokens": ["Und", "der", "ein\u00b7zi\u00b7ge", "Trop\u00b7fen", "ge\u00b7n\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.10": {"text": "Eine himmlische Seele,", "tokens": ["Ei\u00b7ne", "himm\u00b7li\u00b7sche", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.11": {"text": "Die hier unten in Schmerz erstarrt,", "tokens": ["Die", "hier", "un\u00b7ten", "in", "Schmerz", "er\u00b7starrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.12": {"text": "Wieder in Wonne zu l\u00f6sen.", "tokens": ["Wie\u00b7der", "in", "Won\u00b7ne", "zu", "l\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.13": {"text": "Ach! sie weint dir s\u00fc\u00dferen Dank,", "tokens": ["Ach", "!", "sie", "weint", "dir", "s\u00fc\u00b7\u00dfe\u00b7ren", "Dank", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.14": {"text": "Als die Anderen alle,", "tokens": ["Als", "die", "An\u00b7de\u00b7ren", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "PIS", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.15": {"text": "Die du gl\u00fccklich und reich gemacht;", "tokens": ["Die", "du", "gl\u00fcck\u00b7lich", "und", "reich", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.16": {"text": "La\u00df ihn fallen, den Tropfen!", "tokens": ["La\u00df", "ihn", "fal\u00b7len", ",", "den", "Trop\u00b7fen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}