{"dta.poem.21225": {"metadata": {"author": {"name": "Czepko, Daniel von", "birth": "N.A.", "death": "N.A."}, "title": "V.  \n  Gl\u00fccks Verwandlung  \n bey desz Hertzogs von Friedland Vn-  \n tergange.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20871-8", "language": ["de:0.99"], "booktitle": "Czepko, Daniel von: Triumph Bogen. Breslau, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Vmb dessen Zelt vnd Hau\u00df viel Tausend musten wachen:", "tokens": ["Vmb", "des\u00b7sen", "Zelt", "vnd", "Hau\u00df", "viel", "Tau\u00b7send", "mus\u00b7ten", "wa\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "KON", "NN", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat keine Faust/ auff die sein Todt was fussen kan.", "tokens": ["Hat", "kei\u00b7ne", "Faust", "/", "auff", "die", "sein", "Todt", "was", "fus\u00b7sen", "kan", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$(", "APPR", "ART", "PPOSAT", "NN", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der au\u00df Jhm selber kont auch was er wolte machen/", "tokens": ["Der", "au\u00df", "Jhm", "sel\u00b7ber", "kont", "auch", "was", "er", "wol\u00b7te", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "ADV", "VMFIN", "ADV", "PWS", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kriegt seinen Sto\u00df vnd Lohn vnd ligt auff offner Bahn.", "tokens": ["Kriegt", "sei\u00b7nen", "Sto\u00df", "vnd", "Lohn", "vnd", "ligt", "auff", "off\u00b7ner", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "NN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Jhm Pal\u00e4ste lie\u00df au\u00df gantzen St\u00e4dten bawen:", "tokens": ["Der", "Jhm", "Pa\u00b7l\u00e4s\u00b7te", "lie\u00df", "au\u00df", "gant\u00b7zen", "St\u00e4d\u00b7ten", "ba\u00b7wen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "VVFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "Dem fehlts/ was sonsten hat/ der betteln pflegt zugehn.", "tokens": ["Dem", "fehlts", "/", "was", "sons\u00b7ten", "hat", "/", "der", "bet\u00b7teln", "pflegt", "zu\u00b7gehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "PWS", "ADV", "VAFIN", "$(", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der seine Thaten wolt in ewge Schrifften hawen;", "tokens": ["Der", "sei\u00b7ne", "Tha\u00b7ten", "wolt", "in", "ew\u00b7ge", "Schriff\u00b7ten", "ha\u00b7wen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.8": {"text": "Hat seinen Nahmen nicht auff seinem Sarche stehn.", "tokens": ["Hat", "sei\u00b7nen", "Nah\u00b7men", "nicht", "auff", "sei\u00b7nem", "Sar\u00b7che", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dem auff sein Anbefehl die Sterne musten lauffen;", "tokens": ["Dem", "auff", "sein", "An\u00b7be\u00b7fehl", "die", "Ster\u00b7ne", "mus\u00b7ten", "lauf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Den f\u00e4llt aus jhrer Zunfft ", "tokens": ["Den", "f\u00e4llt", "aus", "jhrer", "Zunfft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Der vmb viel Tonnen Gold das Gl\u00fcck Jhm konte kauffen;", "tokens": ["Der", "vmb", "viel", "Ton\u00b7nen", "Gold", "das", "Gl\u00fcck", "Jhm", "kon\u00b7te", "kauf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "NN", "ART", "NN", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Verl\u00e4st nicht/ das man Jhn schlepp an de\u00df Grabes Port.", "tokens": ["Ver\u00b7l\u00e4st", "nicht", "/", "das", "man", "Jhn", "schlepp", "an", "de\u00df", "Gra\u00b7bes", "Port", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$(", "PRELS", "PIS", "PPER", "ADJD", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Du Andrer ", "tokens": ["Du", "A\u00b7ndrer"], "token_info": ["word", "word"], "pos": ["PPER", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Dieweil bey dir so viel dein ", "tokens": ["Die\u00b7weil", "bey", "dir", "so", "viel", "dein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "ADV", "ADV", "PPOSAT"], "meter": "+-+-++-", "measure": "unknown.measure.tetra"}, "line.15": {"text": "Jtzt sihst du/ ob dich ", "tokens": ["Jtzt", "sihst", "du", "/", "ob", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.16": {"text": "Da\u00df Vnser auff der Welt vielmehr als deiner kan.", "tokens": ["Da\u00df", "Vn\u00b7ser", "auff", "der", "Welt", "viel\u00b7mehr", "als", "dei\u00b7ner", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "ADV", "KOUS", "PPOSAT", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}