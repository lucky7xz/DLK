{"textgrid.poem.66573": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit Fackeln und Serenaden,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Fackeln und Serenaden,", "tokens": ["Mit", "Fa\u00b7ckeln", "und", "Se\u00b7re\u00b7na\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit Reden und Lorbeerkranz,", "tokens": ["Mit", "Re\u00b7den", "und", "Lor\u00b7beer\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "O gro\u00dfer Mann, wie beladen", "tokens": ["O", "gro\u00b7\u00dfer", "Mann", ",", "wie", "be\u00b7la\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PWAV", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wirst heut du mit Ehrenglanz!", "tokens": ["Wirst", "heut", "du", "mit", "Eh\u00b7ren\u00b7glanz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und weil dir so herrlich der Sprecher", "tokens": ["Und", "weil", "dir", "so", "herr\u00b7lich", "der", "Spre\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADJD", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Geredet mit lautem Ton,", "tokens": ["Ge\u00b7re\u00b7det", "mit", "lau\u00b7tem", "Ton", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Kommt morgen mit silbernem Becher", "tokens": ["Kommt", "mor\u00b7gen", "mit", "sil\u00b7ber\u00b7nem", "Be\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Zu ihm schon die Deputation.", "tokens": ["Zu", "ihm", "schon", "die", "De\u00b7pu\u00b7ta\u00b7ti\u00b7on", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Auch diese redet vorz\u00fcglich,", "tokens": ["Auch", "die\u00b7se", "re\u00b7det", "vor\u00b7z\u00fcg\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie wei\u00df, bald kommt's auch an sie,", "tokens": ["Sie", "wei\u00df", ",", "bald", "kommt's", "auch", "an", "sie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und so geht's weiter vergn\u00fcglich", "tokens": ["Und", "so", "geht's", "wei\u00b7ter", "ver\u00b7gn\u00fcg\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "ADJD"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nach bekannter Melodie.", "tokens": ["Nach", "be\u00b7kann\u00b7ter", "Me\u00b7lo\u00b7die", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie g\u00f6nnen sich nicht den Bissen,", "tokens": ["Sie", "g\u00f6n\u00b7nen", "sich", "nicht", "den", "Bis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und hassen sich, da\u00df es raucht!", "tokens": ["Und", "has\u00b7sen", "sich", ",", "da\u00df", "es", "raucht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und r\u00e4uchern und dudeln beflissen,", "tokens": ["Und", "r\u00e4u\u00b7chern", "und", "du\u00b7deln", "be\u00b7flis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "KON", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil Einer den Andern braucht.", "tokens": ["Weil", "Ei\u00b7ner", "den", "An\u00b7dern", "braucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}