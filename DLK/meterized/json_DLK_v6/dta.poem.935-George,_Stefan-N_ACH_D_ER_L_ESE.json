{"dta.poem.935": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "N ACH   D ER   L ESE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Du willst am mauerbrunnen wasser sch\u00f6pfen               ", "tokens": ["Du", "willst", "am", "mau\u00b7er\u00b7brun\u00b7nen", "was\u00b7ser", "sch\u00f6p\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und spielend in die k&#252;hlen strahlen langen               ", "tokens": ["Und", "spie\u00b7lend", "in", "die", "k", "&#252;", "hlen", "strah\u00b7len", "lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "$(", "VVINF", "VVFIN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch scheint es mir du wendest mit befangen               ", "tokens": ["Doch", "scheint", "es", "mir", "du", "wen\u00b7dest", "mit", "be\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PPER", "VVFIN", "APPR", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die h&#228;nde von den beiden l\u00f6wenk\u00f6pfen             ", "tokens": ["Die", "h", "&#228;", "nde", "von", "den", "bei\u00b7den", "l\u00f6\u00b7wen\u00b7k\u00f6p\u00b7fen"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$(", "VVFIN", "APPR", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Den ring mit dem erblindeten juwele               ", "tokens": ["Den", "ring", "mit", "dem", "er\u00b7blin\u00b7de\u00b7ten", "ju\u00b7we\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich suchte dir vom finger ihn zu drehen               ", "tokens": ["Ich", "such\u00b7te", "dir", "vom", "fin\u00b7ger", "ihn", "zu", "dre\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "ADJA", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein feuchtes auge k&#252;sste meine seele               ", "tokens": ["Dein", "feuch\u00b7tes", "au\u00b7ge", "k", "&#252;", "ss\u00b7te", "mei\u00b7ne", "see\u00b7le"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "NE", "$(", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Als antwort auf mein unverh&#252;lltes flehen.             ", "tokens": ["Als", "ant\u00b7wort", "auf", "mein", "un\u00b7verh", "&#252;", "ll\u00b7tes", "fle\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "ADJD", "$(", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}