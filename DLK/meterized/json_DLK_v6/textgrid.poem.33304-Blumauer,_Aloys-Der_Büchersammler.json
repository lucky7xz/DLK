{"textgrid.poem.33304": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Der B\u00fcchersammler", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Thrax tapeziret alle seine W\u00e4nde", "tokens": ["Thrax", "ta\u00b7pe\u00b7zi\u00b7ret", "al\u00b7le", "sei\u00b7ne", "W\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit B\u00fcchern aus, in die er niemals schaut:", "tokens": ["Mit", "B\u00fc\u00b7chern", "aus", ",", "in", "die", "er", "nie\u00b7mals", "schaut", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "APPR", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So schrieben einst der alten Weisen H\u00e4nde", "tokens": ["So", "schrie\u00b7ben", "einst", "der", "al\u00b7ten", "Wei\u00b7sen", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der gr\u00f6\u00dften Weisheit Schatz auf eines Esels Haut.", "tokens": ["Der", "gr\u00f6\u00df\u00b7ten", "Weis\u00b7heit", "Schatz", "auf", "ei\u00b7nes", "E\u00b7sels", "Haut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}