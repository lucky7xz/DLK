{"textgrid.poem.67031": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der n\u00e4chtliche Wanderer", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hu! der Kauz! wie er heult,", "tokens": ["Hu", "!", "der", "Kauz", "!", "wie", "er", "heult", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "$.", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wie sein Furchtgeschrei kr\u00e4cht.", "tokens": ["Wie", "sein", "Furcht\u00b7ge\u00b7schrei", "kr\u00e4cht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Erw\u00fcrgen \u2013 ha! du hungerst nach erw\u00fcrgtem Aas,", "tokens": ["Er\u00b7w\u00fcr\u00b7gen", "\u2013", "ha", "!", "du", "hun\u00b7gerst", "nach", "er\u00b7w\u00fcrg\u00b7tem", "Aas", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ITJ", "$.", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du naher W\u00fcrger, komme, komme.", "tokens": ["Du", "na\u00b7her", "W\u00fcr\u00b7ger", ",", "kom\u00b7me", ",", "kom\u00b7me", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sieh! er lauscht, schnaubend Tod \u2013", "tokens": ["Sieh", "!", "er", "lauscht", ",", "schnau\u00b7bend", "Tod", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "$,", "ADJD", "NN", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ringsum schnarchet der Hauf,", "tokens": ["Ring\u00b7sum", "schnar\u00b7chet", "der", "Hauf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Des Mordes Hauf, er h\u00f6rts, er h\u00f6rts, im Traume h\u00f6rt' ers,", "tokens": ["Des", "Mor\u00b7des", "Hauf", ",", "er", "h\u00f6rts", ",", "er", "h\u00f6rts", ",", "im", "Trau\u00b7me", "h\u00f6rt'", "ers", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PPER", "ADV", "$,", "PPER", "ADV", "$,", "APPRART", "NN", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "Ich irre, W\u00fcrger, schlafe, schlafe.", "tokens": ["Ich", "ir\u00b7re", ",", "W\u00fcr\u00b7ger", ",", "schla\u00b7fe", ",", "schla\u00b7fe", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}