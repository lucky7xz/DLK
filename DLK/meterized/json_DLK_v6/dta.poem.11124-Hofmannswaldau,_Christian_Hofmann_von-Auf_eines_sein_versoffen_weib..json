{"dta.poem.11124": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf eines sein versoffen weib.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.57", "da:0.42"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Beym brunnen redte dort Rebecca kluge sachen;", "tokens": ["Beym", "brun\u00b7nen", "red\u00b7te", "dort", "Re\u00b7bec\u00b7ca", "klu\u00b7ge", "sa\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "NE", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beym wasser l\u00e4st sichs noch mit deinem weibe machen.", "tokens": ["Beym", "was\u00b7ser", "l\u00e4st", "sichs", "noch", "mit", "dei\u00b7nem", "wei\u00b7be", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil sie nur dieses trinckt, so bleibt sie noch gescheut;", "tokens": ["Weil", "sie", "nur", "die\u00b7ses", "trinckt", ",", "so", "bleibt", "sie", "noch", "ge\u00b7scheut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PDS", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch k\u00f6mmt es an den wein, so ist der narr nicht weit.", "tokens": ["Doch", "k\u00f6mmt", "es", "an", "den", "wein", ",", "so", "ist", "der", "narr", "nicht", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}