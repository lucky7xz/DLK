{"textgrid.poem.59324": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Bei einer Linde", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seh ich dich wieder, du geliebter Baum,", "tokens": ["Seh", "ich", "dich", "wie\u00b7der", ",", "du", "ge\u00b7lieb\u00b7ter", "Baum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "In dessen junge Triebe", "tokens": ["In", "des\u00b7sen", "jun\u00b7ge", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich einst in jenes Fr\u00fchlings sch\u00f6nstem Traum", "tokens": ["Ich", "einst", "in", "je\u00b7nes", "Fr\u00fch\u00b7lings", "sch\u00f6ns\u00b7tem", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "PDAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den Namen schnitt von meiner ersten Liebe?", "tokens": ["Den", "Na\u00b7men", "schnitt", "von", "mei\u00b7ner", "ers\u00b7ten", "Lie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie anders ist seitdem der \u00c4ste Bug,", "tokens": ["Wie", "an\u00b7ders", "ist", "seit\u00b7dem", "der", "\u00c4s\u00b7te", "Bug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Verwachsen und verschwunden", "tokens": ["Ver\u00b7wach\u00b7sen", "und", "ver\u00b7schwun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "KON", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im h\u00e4rtren Stamm der vielgeliebte Zug,", "tokens": ["Im", "h\u00e4r\u00b7tren", "Stamm", "der", "viel\u00b7ge\u00b7lieb\u00b7te", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie ihre Liebe und die sch\u00f6nen Stunden!", "tokens": ["Wie", "ih\u00b7re", "Lie\u00b7be", "und", "die", "sch\u00f6\u00b7nen", "Stun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Auch ich seitdem wuchs stille fort, wie du,", "tokens": ["Auch", "ich", "seit\u00b7dem", "wuchs", "stil\u00b7le", "fort", ",", "wie", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "PAV", "VVFIN", "VVFIN", "PTKVZ", "$,", "PWAV", "PPER", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und nichts an mir wollt weilen,", "tokens": ["Und", "nichts", "an", "mir", "wollt", "wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch meine Wunde wuchs \u2013 und wuchs nicht zu,", "tokens": ["Doch", "mei\u00b7ne", "Wun\u00b7de", "wuchs", "\u2013", "und", "wuchs", "nicht", "zu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$(", "KON", "VVFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wird wohl niemals mehr hienieden heilen.", "tokens": ["Und", "wird", "wohl", "nie\u00b7mals", "mehr", "hien\u00b7ie\u00b7den", "hei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}