{"textgrid.poem.37083": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An J.M. Miller", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denk' ich, Bester, des Trennungstags,", "tokens": ["Denk'", "ich", ",", "Bes\u00b7ter", ",", "des", "Tren\u00b7nungs\u00b7tags", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Ach, dann bricht mir das Herz, Tage der Zukunft, ach,", "tokens": ["Ach", ",", "dann", "bricht", "mir", "das", "Herz", ",", "Ta\u00b7ge", "der", "Zu\u00b7kunft", ",", "ach", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,", "NN", "ART", "NN", "$,", "ITJ", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Einer traurigen Zukunft, nahn,", "tokens": ["Ei\u00b7ner", "trau\u00b7ri\u00b7gen", "Zu\u00b7kunft", ",", "nahn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Mit umd\u00fcsterter Stirn, jeder den Wermuthkelch", "tokens": ["Mit", "um\u00b7d\u00fcs\u00b7ter\u00b7ter", "Stirn", ",", "je\u00b7der", "den", "Wer\u00b7muth\u00b7kelch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PIS", "ART", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "In den H\u00e4nden, und dr\u00e4uen mir.", "tokens": ["In", "den", "H\u00e4n\u00b7den", ",", "und", "dr\u00e4u\u00b7en", "mir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Ach, sie kommen zu bald, sch\u00fctten den ganzen Kelch", "tokens": ["Ach", ",", "sie", "kom\u00b7men", "zu", "bald", ",", "sch\u00fct\u00b7ten", "den", "gan\u00b7zen", "Kelch"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "APPR", "ADV", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "\u00dcber deinen Getreuen aus,", "tokens": ["\u00dc\u00b7ber", "dei\u00b7nen", "Ge\u00b7treu\u00b7en", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Wann er ferne von dir weinet, von eurem Ku\u00df,", "tokens": ["Wann", "er", "fer\u00b7ne", "von", "dir", "wei\u00b7net", ",", "von", "eu\u00b7rem", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Bundesbr\u00fcder, von eurem Sang,", "tokens": ["Bun\u00b7des\u00b7br\u00fc\u00b7der", ",", "von", "eu\u00b7rem", "Sang", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.10": {"text": "An der Leine Gestad weinet, und keinen Trost", "tokens": ["An", "der", "Lei\u00b7ne", "Ge\u00b7stad", "wei\u00b7net", ",", "und", "kei\u00b7nen", "Trost"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,", "KON", "PIAT", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Auf der Erde mehr finden kan.", "tokens": ["Auf", "der", "Er\u00b7de", "mehr", "fin\u00b7den", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.12": {"text": "Rosen schlie\u00dfen sich zu, nahet dein Traurer sich,", "tokens": ["Ro\u00b7sen", "schlie\u00b7\u00dfen", "sich", "zu", ",", "na\u00b7het", "dein", "Trau\u00b7rer", "sich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "PTKVZ", "$,", "VVFIN", "PPOSAT", "NN", "PRF", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Wo sie brannten in ihrem Thau,", "tokens": ["Wo", "sie", "brann\u00b7ten", "in", "ih\u00b7rem", "Thau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.14": {"text": "Als noch Lauren der Hain k\u00fchlte, das Abendroth", "tokens": ["Als", "noch", "Lau\u00b7ren", "der", "Hain", "k\u00fchl\u00b7te", ",", "das", "A\u00b7ben\u00b7droth"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADV", "NN", "ART", "NN", "VVFIN", "$,", "ART", "NN"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.15": {"text": "Um die Locken des M\u00e4dchens flo\u00df,", "tokens": ["Um", "die", "Lo\u00b7cken", "des", "M\u00e4d\u00b7chens", "flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.16": {"text": "Und mein Erstlingsgesang s\u00e4uselte durch die Flur.", "tokens": ["Und", "mein", "Erst\u00b7lings\u00b7ge\u00b7sang", "s\u00e4u\u00b7sel\u00b7te", "durch", "die", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.17": {"text": "Ach, die Seelen der Abende,", "tokens": ["Ach", ",", "die", "See\u00b7len", "der", "A\u00b7ben\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.18": {"text": "Die wir Br\u00fcder verkos't, werden oft vor mir stehn,", "tokens": ["Die", "wir", "Br\u00fc\u00b7der", "ver\u00b7kos't", ",", "wer\u00b7den", "oft", "vor", "mir", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "VVPP", "$,", "VAFIN", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.19": {"text": "Sch\u00f6n und l\u00e4chelnd wie Seraphim,", "tokens": ["Sch\u00f6n", "und", "l\u00e4\u00b7chelnd", "wie", "Se\u00b7ra\u00b7phim", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "KOKOM", "NE", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.20": {"text": "Und die Seelen des Sangs, welcher dem Harfengriff", "tokens": ["Und", "die", "See\u00b7len", "des", "Sangs", ",", "wel\u00b7cher", "dem", "Har\u00b7fen\u00b7griff"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "--+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Meiner Lieben entst\u00fcrmete.", "tokens": ["Mei\u00b7ner", "Lie\u00b7ben", "ent\u00b7st\u00fcr\u00b7me\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.22": {"text": "Deines Herzensgespr\u00e4chs, Trauter, und Freundesblicks", "tokens": ["Dei\u00b7nes", "Her\u00b7zens\u00b7ge\u00b7spr\u00e4chs", ",", "Trau\u00b7ter", ",", "und", "Freun\u00b7des\u00b7blicks"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "KON", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "Werd ich gehren, und, ach, umsonst!", "tokens": ["Werd", "ich", "geh\u00b7ren", ",", "und", ",", "ach", ",", "um\u00b7sonst", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "VVINF", "$,", "KON", "$,", "ITJ", "$,", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.24": {"text": "Deines Minnegesangs, welcher so lieblich scholl", "tokens": ["Dei\u00b7nes", "Min\u00b7ne\u00b7ge\u00b7sangs", ",", "wel\u00b7cher", "so", "lieb\u00b7lich", "scholl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "ADJD", "ADJD"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.25": {"text": "Guten Frauen, und, ach, umsonst!", "tokens": ["Gu\u00b7ten", "Frau\u00b7en", ",", "und", ",", "ach", ",", "um\u00b7sonst", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "$,", "ITJ", "$,", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.26": {"text": "Angeduftet vom May, flimmert der Abendstern", "tokens": ["An\u00b7ge\u00b7duf\u00b7tet", "vom", "May", ",", "flim\u00b7mert", "der", "A\u00b7bends\u00b7tern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "APPRART", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "+-+--++-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.27": {"text": "Durch die Bl\u00fcthen, der Schauer war", "tokens": ["Durch", "die", "Bl\u00fc\u00b7then", ",", "der", "Schau\u00b7er", "war"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "VAFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.28": {"text": "Unsrer lenzlichen Lust, werd ich dich sp\u00e4hn, den Arm", "tokens": ["Uns\u00b7rer", "lenz\u00b7li\u00b7chen", "Lust", ",", "werd", "ich", "dich", "sp\u00e4hn", ",", "den", "Arm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VAFIN", "PPER", "PRF", "VVINF", "$,", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.29": {"text": "Nach dir strecken, und, ach, umsonst!", "tokens": ["Nach", "dir", "stre\u00b7cken", ",", "und", ",", "ach", ",", "um\u00b7sonst", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "KON", "$,", "ITJ", "$,", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.30": {"text": "Keine Thr\u00e4ne, kein Flehn sehnet an diese Brust", "tokens": ["Kei\u00b7ne", "Thr\u00e4\u00b7ne", ",", "kein", "Flehn", "seh\u00b7net", "an", "die\u00b7se", "Brust"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VVFIN", "APPR", "PDAT", "NN"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.31": {"text": "Dich her\u00fcber, an diese Brust,", "tokens": ["Dich", "her\u00b7\u00fc\u00b7ber", ",", "an", "die\u00b7se", "Brust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.32": {"text": "Wo mit Lauren dein Bild lebet, und leben wird,", "tokens": ["Wo", "mit", "Lau\u00b7ren", "dein", "Bild", "le\u00b7bet", ",", "und", "le\u00b7ben", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "PPOSAT", "NN", "VVFIN", "$,", "KON", "VVINF", "VAFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.33": {"text": "Bis mich h\u00fcllet die Rasengruft.", "tokens": ["Bis", "mich", "h\u00fcl\u00b7let", "die", "Ra\u00b7sen\u00b7gruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.34": {"text": "Und die h\u00fcllet mich bald. Fl\u00fcstert das Rebengr\u00fcn,", "tokens": ["Und", "die", "h\u00fcl\u00b7let", "mich", "bald", ".", "Fl\u00fcs\u00b7tert", "das", "Re\u00b7ben\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PPER", "ADV", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Wo du sitzest, im Mondenglanz,", "tokens": ["Wo", "du", "sit\u00b7zest", ",", "im", "Mon\u00b7den\u00b7glanz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "APPRART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.36": {"text": "Flimmert eine Gestalt, Traurigkeit im Gesicht,", "tokens": ["Flim\u00b7mert", "ei\u00b7ne", "Ge\u00b7stalt", ",", "Trau\u00b7rig\u00b7keit", "im", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.37": {"text": "Melancholisch vor dir vorbey,", "tokens": ["Me\u00b7lan\u00b7cho\u00b7lisch", "vor", "dir", "vor\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Winkt und l\u00e4chelt dir zu, Bester, es ist dein Freund.", "tokens": ["Winkt", "und", "l\u00e4\u00b7chelt", "dir", "zu", ",", "Bes\u00b7ter", ",", "es", "ist", "dein", "Freund", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "PTKVZ", "$,", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.39": {"text": "Er begegnet, der fromme Geist,", "tokens": ["Er", "be\u00b7geg\u00b7net", ",", "der", "from\u00b7me", "Geist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.40": {"text": "Einen Engel am Arm, sinket dein Todesloos,", "tokens": ["Ei\u00b7nen", "En\u00b7gel", "am", "Arm", ",", "sin\u00b7ket", "dein", "To\u00b7des\u00b7loos", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.41": {"text": "Dir am Ufer der Ewigkeit,", "tokens": ["Dir", "am", "U\u00b7fer", "der", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.42": {"text": "Bleibet ewig dein Freund. Freundschaft und Liebe wohnt", "tokens": ["Blei\u00b7bet", "e\u00b7wig", "dein", "Freund", ".", "Freund\u00b7schaft", "und", "Lie\u00b7be", "wohnt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$.", "NN", "KON", "NN", "VVFIN"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.43": {"text": "Unter Palmen Jehovahs, setzt", "tokens": ["Un\u00b7ter", "Pal\u00b7men", "Je\u00b7ho\u00b7vahs", ",", "setzt"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "NE", "$,", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.44": {"text": "Himmelskronen uns auf, welche kein Schicksal welkt.", "tokens": ["Him\u00b7mels\u00b7kro\u00b7nen", "uns", "auf", ",", "wel\u00b7che", "kein", "Schick\u00b7sal", "welkt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}}}}}