{"textgrid.poem.26271": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Bali-Tempel", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im alten Tempelhof, der grau ist und verlassen,", "tokens": ["Im", "al\u00b7ten", "Tem\u00b7pel\u00b7hof", ",", "der", "grau", "ist", "und", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "ADJD", "VAFIN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da bl\u00fchn allein vielarmig Frangipanib\u00e4ume", "tokens": ["Da", "bl\u00fchn", "al\u00b7lein", "viel\u00b7ar\u00b7mig", "Fran\u00b7gi\u00b7pa\u00b7ni\u00b7b\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und halten ihre Bl\u00fcten hoch, die weltentr\u00fcckten, blassen,", "tokens": ["Und", "hal\u00b7ten", "ih\u00b7re", "Bl\u00fc\u00b7ten", "hoch", ",", "die", "wel\u00b7ten\u00b7tr\u00fcck\u00b7ten", ",", "blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADJD", "$,", "PRELS", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Und opfern ihre scharfen D\u00fcfte, ihre ganz verz\u00fcckten,", "tokens": ["Und", "op\u00b7fern", "ih\u00b7re", "schar\u00b7fen", "D\u00fcf\u00b7te", ",", "ih\u00b7re", "ganz", "ver\u00b7z\u00fcck\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "Den grimmen G\u00f6tterbildern, die da, Stein bei Stein, sich selbst besehen,", "tokens": ["Den", "grim\u00b7men", "G\u00f6t\u00b7ter\u00b7bil\u00b7dern", ",", "die", "da", ",", "Stein", "bei", "Stein", ",", "sich", "selbst", "be\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "$,", "NN", "APPR", "NN", "$,", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.6": {"text": "Im alten leeren Hofe dort im Schatten stehen und verwildern.", "tokens": ["Im", "al\u00b7ten", "lee\u00b7ren", "Ho\u00b7fe", "dort", "im", "Schat\u00b7ten", "ste\u00b7hen", "und", "ver\u00b7wil\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "ADV", "APPRART", "NN", "VVFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}}}}