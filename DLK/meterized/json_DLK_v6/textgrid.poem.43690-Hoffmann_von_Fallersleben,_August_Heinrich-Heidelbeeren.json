{"textgrid.poem.43690": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Heidelbeeren", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frau Sonne hat es brav gemacht,", "tokens": ["Frau", "Son\u00b7ne", "hat", "es", "brav", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie hat die Beeren zur Reife gebracht,", "tokens": ["Sie", "hat", "die", "Bee\u00b7ren", "zur", "Rei\u00b7fe", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Heidelbeeren.", "tokens": ["Die", "Hei\u00b7del\u00b7bee\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wir wollen uns b\u00fccken", "tokens": ["Wir", "wol\u00b7len", "uns", "b\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und flei\u00dfig pfl\u00fccken", "tokens": ["Und", "flei\u00b7\u00dfig", "pfl\u00fc\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Die Heidelbeeren.", "tokens": ["Die", "Hei\u00b7del\u00b7bee\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Wir wollen verzehren,", "tokens": ["Wir", "wol\u00b7len", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Frau Sonne zu Ehren,", "tokens": ["Frau", "Son\u00b7ne", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die Heidelbeeren,", "tokens": ["Die", "Hei\u00b7del\u00b7bee\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die lieben, blauen Heidelbeeren.", "tokens": ["Die", "lie\u00b7ben", ",", "blau\u00b7en", "Hei\u00b7del\u00b7bee\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}