{"textgrid.poem.25537": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Unter meinem Lindenbaume", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unter meinem Lindenbaume", "tokens": ["Un\u00b7ter", "mei\u00b7nem", "Lin\u00b7den\u00b7bau\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lag ich schlafend hingestreckt,", "tokens": ["Lag", "ich", "schla\u00b7fend", "hin\u00b7ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als mich aus dem sch\u00f6nsten Traume", "tokens": ["Als", "mich", "aus", "dem", "sch\u00f6ns\u00b7ten", "Trau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nahes Gehn und Reden weckt.", "tokens": ["Na\u00b7hes", "Gehn", "und", "Re\u00b7den", "weckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn ein Mann am Blumenstabe,", "tokens": ["Denn", "ein", "Mann", "am", "Blu\u00b7men\u00b7sta\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ging da hoher W\u00fcrde voll,", "tokens": ["Ging", "da", "ho\u00b7her", "W\u00fcr\u00b7de", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auch ein wundersch\u00f6ner Knabe;", "tokens": ["Auch", "ein", "wun\u00b7der\u00b7sch\u00f6\u00b7ner", "Kna\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Amor war es und Apoll.", "tokens": ["A\u00b7mor", "war", "es", "und", "A\u00b7poll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "KON", "NE", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.9": {"text": "\u00bbsieh! wer ist das? Meinen Bogen,", "tokens": ["\u00bb", "sieh", "!", "wer", "ist", "das", "?", "Mei\u00b7nen", "Bo\u00b7gen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$.", "PWS", "VAFIN", "PDS", "$.", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wei\u00df ich sicher, hab' ich noch", "tokens": ["Wei\u00df", "ich", "si\u00b7cher", ",", "hab'", "ich", "noch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Nie f\u00fcr diesen aufgezogen;", "tokens": ["Nie", "f\u00fcr", "die\u00b7sen", "auf\u00b7ge\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Endlich treff' ich ihn nun doch.\u00ab", "tokens": ["End\u00b7lich", "treff'", "ich", "ihn", "nun", "doch", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Ich erschrack, doch blieb ich liegen,", "tokens": ["Ich", "er\u00b7schrack", ",", "doch", "blieb", "ich", "lie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "That, als schlief ich; denn Apoll", "tokens": ["That", ",", "als", "schlief", "ich", ";", "denn", "A\u00b7poll"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "KOUS", "VVFIN", "PPER", "$.", "KON", "NE"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.15": {"text": "Rief: Halt ein! Wenn ihn besiegen", "tokens": ["Rief", ":", "Halt", "ein", "!", "Wenn", "ihn", "be\u00b7sie\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "VVIMP", "PTKVZ", "$.", "KOUS", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Keine zweite Sapho soll.", "tokens": ["Kei\u00b7ne", "zwei\u00b7te", "Sa\u00b7pho", "soll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "\u00bblehrt' ich nicht genug ergr\u00fcnden?", "tokens": ["\u00bb", "lehrt'", "ich", "nicht", "ge\u00b7nug", "er\u00b7gr\u00fcn\u00b7den", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Alles Sch\u00f6ne ward durch mich!", "tokens": ["Al\u00b7les", "Sch\u00f6\u00b7ne", "ward", "durch", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Aber solch ein M\u00e4dchen finden:", "tokens": ["A\u00b7ber", "solch", "ein", "M\u00e4d\u00b7chen", "fin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Lieber! das geh\u00f6rt f\u00fcr dich!\u00ab", "tokens": ["Lie\u00b7ber", "!", "das", "ge\u00b7h\u00f6rt", "f\u00fcr", "dich", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "$.", "PDS", "VVFIN", "APPR", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Amor sprach's, und eine Z\u00e4hre", "tokens": ["A\u00b7mor", "sprach's", ",", "und", "ei\u00b7ne", "Z\u00e4h\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Schlich Apollens Wang' herab.", "tokens": ["Schlich", "A\u00b7pol\u00b7lens", "Wang'", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "\u00bbja! wenn Plutus hier nicht w\u00e4re!", "tokens": ["\u00bb", "ja", "!", "wenn", "Plu\u00b7tus", "hier", "nicht", "w\u00e4\u00b7re", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$.", "KOUS", "NE", "ADV", "PTKNEG", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Alles rei\u00dft er von mir ab!", "tokens": ["Al\u00b7les", "rei\u00dft", "er", "von", "mir", "ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Doch sey ohne Saphos Schmerzen,", "tokens": ["Doch", "sey", "oh\u00b7ne", "Sa\u00b7phos", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "So wie Sapho, die gepflegt,", "tokens": ["So", "wie", "Sa\u00b7pho", ",", "die", "ge\u00b7pflegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "NE", "$,", "PRELS", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Die das sch\u00f6nste aller Herzen", "tokens": ["Die", "das", "sch\u00f6ns\u00b7te", "al\u00b7ler", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "In dem sch\u00f6nsten Leibe tr\u00e4gt.", "tokens": ["In", "dem", "sch\u00f6ns\u00b7ten", "Lei\u00b7be", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Plutus soll mit allen Narren", "tokens": ["Plu\u00b7tus", "soll", "mit", "al\u00b7len", "Nar\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Stutzen, da\u00df durch unsre Macht,", "tokens": ["Stut\u00b7zen", ",", "da\u00df", "durch", "uns\u00b7re", "Macht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Nicht durch seine Silberbarren,", "tokens": ["Nicht", "durch", "sei\u00b7ne", "Sil\u00b7ber\u00b7bar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Edle Wollust beiden lacht.\u00ab", "tokens": ["Ed\u00b7le", "Wol\u00b7lust", "bei\u00b7den", "lacht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "PIAT", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Gut! rief Amor fr\u00f6hlich, spannte", "tokens": ["Gut", "!", "rief", "A\u00b7mor", "fr\u00f6h\u00b7lich", ",", "spann\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "$.", "VVFIN", "NE", "ADJD", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Seinen Bogen, und der Pfeil \u2013", "tokens": ["Sei\u00b7nen", "Bo\u00b7gen", ",", "und", "der", "Pfeil", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Ha! da sa\u00df er! Nante! Nante!", "tokens": ["Ha", "!", "da", "sa\u00df", "er", "!", "Nan\u00b7te", "!", "Nan\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "$.", "NE", "$.", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Rief er, macht die Wunde heil!", "tokens": ["Rief", "er", ",", "macht", "die", "Wun\u00b7de", "heil", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Grade da kamst du gegangen.", "tokens": ["Gra\u00b7de", "da", "kamst", "du", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.38": {"text": "G\u00f6tter! o wie ward mir da,", "tokens": ["G\u00f6t\u00b7ter", "!", "o", "wie", "ward", "mir", "da", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "FM", "KOKOM", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Als ich schon auf deinen Wangen", "tokens": ["Als", "ich", "schon", "auf", "dei\u00b7nen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Die verhei\u00dfne Wollust sah!", "tokens": ["Die", "ver\u00b7hei\u00df\u00b7ne", "Wol\u00b7lust", "sah", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}