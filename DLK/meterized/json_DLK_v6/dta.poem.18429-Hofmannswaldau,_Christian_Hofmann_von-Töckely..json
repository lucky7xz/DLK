{"dta.poem.18429": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "T\u00f6ckely.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das gl\u00fccke hat mit mir ein schauspiel angericht/", "tokens": ["Das", "gl\u00fc\u00b7cke", "hat", "mit", "mir", "ein", "schau\u00b7spiel", "an\u00b7ge\u00b7richt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VAFIN", "APPR", "PPER", "ART", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darinnen ich mich offt ver\u00e4ndert auffgef\u00fchret:", "tokens": ["Da\u00b7rin\u00b7nen", "ich", "mich", "offt", "ver\u00b7\u00e4n\u00b7dert", "auff\u00b7ge\u00b7f\u00fch\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vier actus sind vorbey; gelingt der f\u00fcnffte nicht/", "tokens": ["Vier", "ac\u00b7tus", "sind", "vor\u00b7bey", ";", "ge\u00b7lingt", "der", "f\u00fcnff\u00b7te", "nicht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NE", "VAFIN", "ADV", "$.", "VVFIN", "ART", "ADJA", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So hab ich auf der welt ein trauerspiel agiret.", "tokens": ["So", "hab", "ich", "auf", "der", "welt", "ein", "trau\u00b7er\u00b7spiel", "a\u00b7gi\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}