{"textgrid.poem.35650": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "13. Bundesgesang", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Prahlt nur, S\u00e4nger Lutetiens!", "tokens": ["Prahlt", "nur", ",", "S\u00e4n\u00b7ger", "Lu\u00b7te\u00b7ti\u00b7ens", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "NN", "NE", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Gleichet euren Gesang selber (so ziemt es sich!)", "tokens": ["Glei\u00b7chet", "eu\u00b7ren", "Ge\u00b7sang", "sel\u00b7ber", "(", "so", "ziemt", "es", "sich", "!", ")"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$(", "ADV", "VVFIN", "PPER", "PRF", "$.", "$("], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Der unsterblichen Grazie", "tokens": ["Der", "uns\u00b7terb\u00b7li\u00b7chen", "Gra\u00b7zie"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Des Dirc\u00e4ers, und trotzt jenem, der Latiums", "tokens": ["Des", "Dir\u00b7c\u00e4\u00b7ers", ",", "und", "trotzt", "je\u00b7nem", ",", "der", "La\u00b7ti\u00b7ums"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "PDAT", "$,", "ART", "NN"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Freiheitsm\u00f6rder verg\u00f6tterte!", "tokens": ["Frei\u00b7heits\u00b7m\u00f6r\u00b7der", "ver\u00b7g\u00f6t\u00b7ter\u00b7te", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Warum solltet ihr's nicht? Habt ihr die Hoffnungen", "tokens": ["Wa\u00b7rum", "soll\u00b7tet", "ih\u00b7r's", "nicht", "?", "Habt", "ihr", "die", "Hoff\u00b7nun\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "PTKNEG", "$.", "VAFIN", "PPER", "ART", "NN"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Eurer K\u00f6nige, welche zur", "tokens": ["Eu\u00b7rer", "K\u00f6\u00b7ni\u00b7ge", ",", "wel\u00b7che", "zur"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "APPRART"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Kurzweil scharenweis euch f\u00fctterten, nicht erf\u00fcllt?", "tokens": ["Kurz\u00b7weil", "scha\u00b7ren\u00b7weis", "euch", "f\u00fct\u00b7ter\u00b7ten", ",", "nicht", "er\u00b7f\u00fcllt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVFIN", "$,", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Scholl nicht tausendmal euer Lied", "tokens": ["Scholl", "nicht", "tau\u00b7send\u00b7mal", "eu\u00b7er", "Lied"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKNEG", "ADV", "PPOSAT", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.10": {"text": "Aus den Schl\u00fcnden des Ruhms? Haucht' es nicht tausendmal", "tokens": ["Aus", "den", "Schl\u00fcn\u00b7den", "des", "Ruhms", "?", "Haucht'", "es", "nicht", "tau\u00b7send\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$.", "NN", "PPER", "PTKNEG", "ADV"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Wilde Gluten dem J\u00fcnglinge,", "tokens": ["Wil\u00b7de", "Glu\u00b7ten", "dem", "J\u00fcng\u00b7lin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "Und der heiligen Brust bl\u00fchender M\u00e4dchen ein?", "tokens": ["Und", "der", "hei\u00b7li\u00b7gen", "Brust", "bl\u00fc\u00b7hen\u00b7der", "M\u00e4d\u00b7chen", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Billig werft ihr den Seitenblick", "tokens": ["Bil\u00b7lig", "werft", "ihr", "den", "Sei\u00b7ten\u00b7blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.14": {"text": "Sp\u00f6ttisch \u00fcber den Rhein, in das barbarische", "tokens": ["Sp\u00f6t\u00b7tisch", "\u00fc\u00b7ber", "den", "Rhein", ",", "in", "das", "bar\u00b7ba\u00b7ri\u00b7sche"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NE", "$,", "APPR", "ART", "ADJA"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "Land, wo Ro\u00dfbach und H\u00f6chst\u00e4dt noch", "tokens": ["Land", ",", "wo", "Ro\u00df\u00b7bach", "und", "H\u00f6chs\u00b7t\u00e4dt", "noch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "NN", "KON", "NN", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.16": {"text": "Vom unmenschlichen Mord feinerer Franzen raucht!", "tokens": ["Vom", "un\u00b7menschli\u00b7chen", "Mord", "fei\u00b7ne\u00b7rer", "Fran\u00b7zen", "raucht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--++--+-+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Billig schimpft ihr das rauhe Lied,", "tokens": ["Bil\u00b7lig", "schimpft", "ihr", "das", "rau\u00b7he", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.18": {"text": "(ach! kein M\u00e4dchen und kein witziger H\u00f6fling liebt's!)", "tokens": ["(", "ach", "!", "kein", "M\u00e4d\u00b7chen", "und", "kein", "wit\u00b7zi\u00b7ger", "H\u00f6f\u00b7ling", "liebt's", "!", ")"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$.", "PIAT", "NN", "KON", "PIAT", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Das, in holpernden T\u00f6nen, Gott,", "tokens": ["Das", ",", "in", "hol\u00b7pern\u00b7den", "T\u00f6\u00b7nen", ",", "Gott", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "$,", "APPR", "ADJA", "NN", "$,", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.20": {"text": "Dieses M\u00e4rchen! und ha! Freiheit und Vaterland", "tokens": ["Die\u00b7ses", "M\u00e4r\u00b7chen", "!", "und", "ha", "!", "Frei\u00b7heit", "und", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "$.", "KON", "NE", "$.", "NN", "KON", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.21": {"text": "Und altv\u00e4trische Tugend singt! \u2013", "tokens": ["Und", "alt\u00b7v\u00e4t\u00b7ri\u00b7sche", "Tu\u00b7gend", "singt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Doch la\u00df ab, o Gesang! Spotte der T\u00e4ndler nicht!", "tokens": ["Doch", "la\u00df", "ab", ",", "o", "Ge\u00b7sang", "!", "Spot\u00b7te", "der", "T\u00e4nd\u00b7ler", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PTKVZ", "$,", "FM", "NN", "$.", "NN", "ART", "NN", "PTKNEG", "$."], "meter": "-++--++--+-+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Unbesorgt um den tr\u00e4gen Strau\u00df,", "tokens": ["Un\u00b7be\u00b7sorgt", "um", "den", "tr\u00e4\u00b7gen", "Strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.24": {"text": "Der, dem \u00c4ther zu schwer, segelnde Schwingen dem", "tokens": ["Der", ",", "dem", "\u00c4\u00b7ther", "zu", "schwer", ",", "se\u00b7geln\u00b7de", "Schwin\u00b7gen", "dem"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "ART", "NN", "PTKA", "ADJD", "$,", "ADJA", "NN", "ART"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.25": {"text": "Wind' ausspreitend, den Sand durchscharrt,", "tokens": ["Wind'", "aus\u00b7sprei\u00b7tend", ",", "den", "Sand", "durch\u00b7scharrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.26": {"text": "St\u00fcrmt der Adler voll Stolz leuchtenden Sonnen zu!", "tokens": ["St\u00fcrmt", "der", "Ad\u00b7ler", "voll", "Stolz", "leuch\u00b7ten\u00b7den", "Son\u00b7nen", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.27": {"text": "Tritt, ger\u00fcstet mit K\u00fchnheit, auf,", "tokens": ["Tritt", ",", "ge\u00b7r\u00fcs\u00b7tet", "mit", "K\u00fchn\u00b7heit", ",", "auf", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "APPR", "NN", "$,", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.28": {"text": "Und frag' jegliches Volk unter dem Himmel, frag',", "tokens": ["Und", "frag'", "jeg\u00b7li\u00b7ches", "Volk", "un\u00b7ter", "dem", "Him\u00b7mel", ",", "frag'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "APPR", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+---+---+-+", "measure": "dactylic.init"}, "line.29": {"text": "Welcher einzig noch Antwort hat,", "tokens": ["Wel\u00b7cher", "ein\u00b7zig", "noch", "Ant\u00b7wort", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADJD", "ADV", "NN", "VAFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.30": {"text": "Selbst den Britten, ob er habe der J\u00fcnglinge,", "tokens": ["Selbst", "den", "Brit\u00b7ten", ",", "ob", "er", "ha\u00b7be", "der", "J\u00fcng\u00b7lin\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "KOUS", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.31": {"text": "Die, von F\u00fcrsten unangefeu'rt,", "tokens": ["Die", ",", "von", "F\u00fcrs\u00b7ten", "un\u00b7an\u00b7ge\u00b7feu'rt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.32": {"text": "Hasser goldenen Lohns, Hasser weitstrahlender", "tokens": ["Has\u00b7ser", "gol\u00b7de\u00b7nen", "Lohns", ",", "Has\u00b7ser", "weit\u00b7strah\u00b7len\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "NN", "ADJA"], "meter": "+-+-+-+-++-+", "measure": "unknown.measure.septa"}, "line.33": {"text": "P\u00f6belehren, mit hohem Schwur", "tokens": ["P\u00f6\u00b7be\u00b7leh\u00b7ren", ",", "mit", "ho\u00b7hem", "Schwur"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.34": {"text": "Alles Leben nur dir, Tugendgesang, geweiht!", "tokens": ["Al\u00b7les", "Le\u00b7ben", "nur", "dir", ",", "Tu\u00b7gend\u00b7ge\u00b7sang", ",", "ge\u00b7weiht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "PPER", "$,", "NN", "$,", "VVPP", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.35": {"text": "Der allwissend in unser Herz", "tokens": ["Der", "all\u00b7wis\u00b7send", "in", "un\u00b7ser", "Herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.36": {"text": "Schaute, warum, o Gott, schwieg in der Rechten dir", "tokens": ["Schau\u00b7te", ",", "wa\u00b7rum", ",", "o", "Gott", ",", "schwieg", "in", "der", "Rech\u00b7ten", "dir"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "$,", "FM", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "PPER"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.37": {"text": "Der heimsuchende Donnerstrahl?", "tokens": ["Der", "heim\u00b7su\u00b7chen\u00b7de", "Don\u00b7ner\u00b7strahl", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Warum leuchtete sanftl\u00e4chelnd dein Antlitz uns,", "tokens": ["Wa\u00b7rum", "leuch\u00b7te\u00b7te", "sanft\u00b7l\u00e4\u00b7chelnd", "dein", "Ant\u00b7litz", "uns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADJD", "PPOSAT", "NN", "PPER", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.39": {"text": "Da\u00df der Mond in dem Wiederschein", "tokens": ["Da\u00df", "der", "Mond", "in", "dem", "Wie\u00b7der\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.40": {"text": "Und der sternende Pol' l\u00e4chelt', und ehrfurchtsvoll", "tokens": ["Und", "der", "ster\u00b7nen\u00b7de", "Pol'", "l\u00e4\u00b7chelt'", ",", "und", "ehr\u00b7furchts\u00b7voll"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,", "KON", "ADJD"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.41": {"text": "Jedes feiernde L\u00fcftchen sank?", "tokens": ["Je\u00b7des", "fei\u00b7ern\u00b7de", "L\u00fcft\u00b7chen", "sank", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.42": {"text": "Meine Br\u00fcder, Triumph! Uns hat gesegnet Gott!", "tokens": ["Mei\u00b7ne", "Br\u00fc\u00b7der", ",", "Tri\u00b7umph", "!", "Uns", "hat", "ge\u00b7seg\u00b7net", "Gott", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$.", "PPER", "VAFIN", "VVPP", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.43": {"text": "Kommt, umarmt mich, und reicht den Kranz", "tokens": ["Kommt", ",", "um\u00b7armt", "mich", ",", "und", "reicht", "den", "Kranz"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Mir des heiligen Laubs, welches uns schattete!", "tokens": ["Mir", "des", "hei\u00b7li\u00b7gen", "Laubs", ",", "wel\u00b7ches", "uns", "schat\u00b7te\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.45": {"text": "Uns gesegnet hat Gott! O kommt,", "tokens": ["Uns", "ge\u00b7seg\u00b7net", "hat", "Gott", "!", "O", "kommt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVPP", "VAFIN", "NN", "$.", "NE", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.46": {"text": "Meine Miller, am Arm eures geliebten Hahn!", "tokens": ["Mei\u00b7ne", "Mil\u00b7ler", ",", "am", "Arm", "eu\u00b7res", "ge\u00b7lieb\u00b7ten", "Hahn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPRART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.47": {"text": "Und du, welchem die z\u00e4rtliche", "tokens": ["Und", "du", ",", "wel\u00b7chem", "die", "z\u00e4rt\u00b7li\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PRELS", "ART", "ADJA"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.48": {"text": "Wollustthr\u00e4ne den Blick tr\u00fcbet, o H\u00f6lty, komm!", "tokens": ["Wol\u00b7lust\u00b7thr\u00e4\u00b7ne", "den", "Blick", "tr\u00fc\u00b7bet", ",", "o", "H\u00f6l\u00b7ty", ",", "komm", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "$,", "FM", "NE", "$,", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.49": {"text": "Seht den klopfenden Busen hier,", "to