{"textgrid.poem.59784": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Horch' nicht auf das Gel\u00e4ute und Gebimmel,", "tokens": ["Horch'", "nicht", "auf", "das", "Ge\u00b7l\u00e4u\u00b7te", "und", "Ge\u00b7bim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wonach die liebe Menge horcht und schreit;", "tokens": ["Wo\u00b7nach", "die", "lie\u00b7be", "Men\u00b7ge", "horcht", "und", "schreit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es klingt dich nur heraus aus deinem Himmel,", "tokens": ["Es", "klingt", "dich", "nur", "he\u00b7raus", "aus", "dei\u00b7nem", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Lockt nur wie Schlachtgesang hinein in Streit.", "tokens": ["Lockt", "nur", "wie", "Schlacht\u00b7ge\u00b7sang", "hin\u00b7ein", "in", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOKOM", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O bleibe lieber, wo die Stillen wohnen,", "tokens": ["O", "blei\u00b7be", "lie\u00b7ber", ",", "wo", "die", "Stil\u00b7len", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo stille Blumen im Verborgnen bl\u00fchn;", "tokens": ["Wo", "stil\u00b7le", "Blu\u00b7men", "im", "Ver\u00b7borg\u00b7nen", "bl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da winde dir des Gl\u00fcckes zarte Kronen", "tokens": ["Da", "win\u00b7de", "dir", "des", "Gl\u00fc\u00b7ckes", "zar\u00b7te", "Kro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und la\u00df den Weltschall froh vor\u00fcberziehn.", "tokens": ["Und", "la\u00df", "den", "Welt\u00b7schall", "froh", "vor\u00b7\u00fc\u00b7ber\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}