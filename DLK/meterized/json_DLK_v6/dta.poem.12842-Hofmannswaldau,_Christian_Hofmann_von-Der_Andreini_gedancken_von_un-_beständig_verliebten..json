{"dta.poem.12842": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der Andreini gedancken von un-  \n best\u00e4ndig verliebten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hertzen, welche bald entbrennen, aber auch alsbald er-", "tokens": ["Hert\u00b7zen", ",", "wel\u00b7che", "bald", "ent\u00b7bren\u00b7nen", ",", "a\u00b7ber", "auch", "als\u00b7bald", "er"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADV", "VVINF", "$,", "ADV", "ADV", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "kalten,", "tokens": ["kal\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["ADJA", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Kommen mir", "tokens": ["Kom\u00b7men", "mir"], "token_info": ["word", "word"], "pos": ["NN", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Wie spiegel f\u00fcr,", "tokens": ["Wie", "spie\u00b7gel", "f\u00fcr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die ein iedes bild annehmen, und kein eintziges behalten.", "tokens": ["Die", "ein", "ie\u00b7des", "bild", "an\u00b7neh\u00b7men", ",", "und", "kein", "eint\u00b7zi\u00b7ges", "be\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "PIAT", "NN", "VVINF", "$,", "KON", "PIAT", "PIS", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}