{"textgrid.poem.56543": {"metadata": {"author": {"name": "Groth, Klaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ik sprung noch inne Kinnerb\u00fcx,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ik sprung noch inne Kinnerb\u00fcx,", "tokens": ["Ik", "sprung", "noch", "in\u00b7ne", "Kin\u00b7ner\u00b7b\u00fcx", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Do weer ik al en Daugenix,", "tokens": ["Do", "weer", "ik", "al", "en", "Dau\u00b7ge\u00b7nix", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dat s\u00e4n ok alle Nawers gliks:", "tokens": ["Dat", "s\u00e4n", "ok", "al\u00b7le", "Na\u00b7wers", "gliks", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "De Jung dat ward en Sleef.", "tokens": ["De", "Jung", "dat", "ward", "en", "Sleef", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "VAFIN", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wat sch\u0119rt mi all dat Sn\u0153tersnack!", "tokens": ["Wat", "sch\u0119rt", "mi", "all", "dat", "Sn\u0153ters\u00b7nack", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "PIAT", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ik sing un dreih min Dudelsack,", "tokens": ["Ik", "sing", "un", "dreih", "min", "Du\u00b7del\u00b7sack", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Belach den ganzen Rummelpack,", "tokens": ["Be\u00b7lach", "den", "gan\u00b7zen", "Rum\u00b7mel\u00b7pack", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "De mi keen S\u00fcsselnk gev!", "tokens": ["De", "mi", "ke\u00b7en", "S\u00fcs\u00b7selnk", "gev", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "NN", "ADJD", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}}, "stanza.2": {"line.1": {"text": "Min Vader schick mi hen na Schol,", "tokens": ["Min", "Va\u00b7der", "schick", "mi", "hen", "na", "Schol", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ik hal mi oft en Puckel vull", "tokens": ["Ik", "hal", "mi", "oft", "en", "Pu\u00b7ckel", "vull"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Un mak den Rekter spitterndull:", "tokens": ["Un", "mak", "den", "Rek\u00b7ter", "spit\u00b7tern\u00b7dull", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Min Lex den wuss ik slech.", "tokens": ["Min", "Lex", "den", "wuss", "ik", "slech", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ik fl\u00f6k den Kram tum D\u00f6wel hin,", "tokens": ["Ik", "fl\u00f6k", "den", "Kram", "tum", "D\u00f6\u00b7wel", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "NE", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "En Prester steek der doch nich in!", "tokens": ["En", "Pres\u00b7ter", "steek", "der", "doch", "nich", "in", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ART", "ADV", "PTKNEG", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mi stunn dat Swart inn Weg.", "tokens": ["Mi", "stunn", "dat", "Swart", "inn", "Weg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Min Moder leet mi'n netten Kn\u00fcll", "tokens": ["Min", "Mo\u00b7der", "leet", "mi'n", "net\u00b7ten", "Kn\u00fcll"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "NE", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vull Wutteln un Kant\u00fcffelp\u00fcll;", "tokens": ["Vull", "Wut\u00b7teln", "un", "Kan\u00b7t\u00fcf\u00b7fel\u00b7p\u00fcll", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dat weer \u0119r letzte gude Will:", "tokens": ["Dat", "weer", "\u0119r", "letz\u00b7te", "gu\u00b7de", "Will", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ik schull'n Plantasche gr\u00fcnn'.", "tokens": ["Ik", "schull'n", "Plan\u00b7ta\u00b7sche", "gr\u00fcnn'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Harr ik man Lust hatt, Gras to mei'n,", "tokens": ["Harr", "ik", "man", "Lust", "hatt", ",", "Gras", "to", "mei'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PIS", "NN", "VAFIN", "$,", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ann Ellbagn ran inne Schit to klei'n,", "tokens": ["Ann", "Ell\u00b7bagn", "ran", "in\u00b7ne", "Schit", "to", "klei'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Mitn Sack umme Nack den Rogg to sei'n,", "tokens": ["Mitn", "Sack", "um\u00b7me", "Nack", "den", "Rogg", "to", "sei'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.8": {"text": "So kunn ik Goldkorns finn'.", "tokens": ["So", "kunn", "ik", "Gold\u00b7korns", "finn'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Kant\u00fcffeln weern der as min Hot,", "tokens": ["Kan\u00b7t\u00fcf\u00b7feln", "weern", "der", "as", "min", "Hot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NE", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Un Wutteln as min Been so grot,", "tokens": ["Un", "Wut\u00b7teln", "as", "min", "Be\u00b7en", "so", "grot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NE", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Un Dreck to klei'n in \u00c6werflot \u2013", "tokens": ["Un", "Dreck", "to", "klei'n", "in", "\u00c6werflot", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "VVFIN", "APPR", "NE", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Dat weer di en Vergn\u00f6gn!", "tokens": ["Dat", "weer", "di", "en", "Ver\u00b7gn\u00f6gn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Min Ol sin S\u0153n de weer ni dumm:", "tokens": ["Min", "Ol", "sin", "S\u0153n", "de", "weer", "ni", "dumm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vunt Arbeidn ward man stif un krumm;", "tokens": ["Vunt", "Ar\u00b7beidn", "ward", "man", "stif", "un", "krumm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "PIS", "ADJD", "FM", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ik sett den Kn\u00fcll in S\u00fclwer um", "tokens": ["Ik", "sett", "den", "Kn\u00fcll", "in", "S\u00fcl\u00b7wer", "um"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Un t\u0119hr vun min Verm\u00f6gn.", "tokens": ["Un", "t\u0119hr", "vun", "min", "Ver\u00b7m\u00f6gn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Juchheisa! in en Reiterb\u00fcx!", "tokens": ["Juch\u00b7hei\u00b7sa", "!", "in", "en", "Rei\u00b7ter\u00b7b\u00fcx", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bequaste St\u0119weln blank in Wichs!", "tokens": ["Be\u00b7quas\u00b7te", "St\u0119weln", "blank", "in", "Wichs", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "APPR", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Klar is de Kees, de Junker fix!", "tokens": ["Klar", "is", "de", "Kees", ",", "de", "Jun\u00b7ker", "fix", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "FM", "FM", "FM", "$,", "NE", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So gung ik denn to Mark.", "tokens": ["So", "gung", "ik", "denn", "to", "Mark", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "NE", "KON", "FM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Klei du in Dreck bet \u0153wern Kopp!", "tokens": ["Klei", "du", "in", "Dreck", "bet", "\u0153wern", "Kopp", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NE", "VVFIN", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Din Fru sett di en Spint derop,", "tokens": ["Din", "Fru", "sett", "di", "en", "Spint", "de\u00b7rop", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Un hett se di de Jack utkloppt,", "tokens": ["Un", "hett", "se", "di", "de", "Jack", "ut\u00b7kloppt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So humpel du to Kark!", "tokens": ["So", "hum\u00b7pel", "du", "to", "Kark", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Min Geld is all, min Kn\u00fcll vert\u0119hrt,", "tokens": ["Min", "Geld", "is", "all", ",", "min", "Kn\u00fcll", "ver\u00b7t\u0119hrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "FM", "PIAT", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "De Junker is keen Dreelnk weerth,", "tokens": ["De", "Jun\u00b7ker", "is", "ke\u00b7en", "Dre\u00b7elnk", "weerth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "FM", "VVFIN", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Min Kn\u0119p heff ik vun buten lehrt:", "tokens": ["Min", "Kn\u0119p", "heff", "ik", "vun", "bu\u00b7ten", "lehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wat sch\u0119rt mi all dat Rummelpack!", "tokens": ["Wat", "sch\u0119rt", "mi", "all", "dat", "Rum\u00b7mel\u00b7pack", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ik heff min heel Musik um Nack,", "tokens": ["Ik", "heff", "min", "heel", "Mu\u00b7sik", "um", "Nack", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "NN", "APPR", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Ik sing min Leed un mak min Snack", "tokens": ["Ik", "sing", "min", "Leed", "un", "mak", "min", "Snack"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "PPOSAT", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.7": {"text": "Un dreih min Orgel rum.", "tokens": ["Un", "dreih", "min", "Or\u00b7gel", "rum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}