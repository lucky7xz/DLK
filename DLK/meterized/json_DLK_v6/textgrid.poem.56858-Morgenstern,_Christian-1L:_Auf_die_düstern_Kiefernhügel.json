{"textgrid.poem.56858": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf die d\u00fcstern Kiefernh\u00fcgel", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf die d\u00fcstern Kiefernh\u00fcgel", "tokens": ["Auf", "die", "d\u00fcs\u00b7tern", "Kie\u00b7fern\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "legt sich kupfern letzte Sonne ...", "tokens": ["legt", "sich", "kup\u00b7fern", "letz\u00b7te", "Son\u00b7ne", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJA", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sanft wie \u00fcber weichen Sammet", "tokens": ["Sanft", "wie", "\u00fc\u00b7ber", "wei\u00b7chen", "Sam\u00b7met"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "schmeicheln Winde dr\u00fcber hin ...", "tokens": ["schmei\u00b7cheln", "Win\u00b7de", "dr\u00fc\u00b7ber", "hin", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PAV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eine kurze Spanne weilt sie", "tokens": ["Ei\u00b7ne", "kur\u00b7ze", "Span\u00b7ne", "weilt", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "goldbraun auf den stillen W\u00e4ldern,", "tokens": ["gold\u00b7braun", "auf", "den", "stil\u00b7len", "W\u00e4l\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "bis ihr milder s\u00fc\u00dfer Schimmer", "tokens": ["bis", "ihr", "mil\u00b7der", "s\u00fc\u00b7\u00dfer", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "pl\u00f6tzlich, wie ein L\u00e4cheln, stirbt.", "tokens": ["pl\u00f6tz\u00b7lich", ",", "wie", "ein", "L\u00e4\u00b7cheln", ",", "stirbt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}