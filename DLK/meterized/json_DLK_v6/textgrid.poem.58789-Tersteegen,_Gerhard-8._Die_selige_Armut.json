{"textgrid.poem.58789": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "8. Die selige Armut", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie selig mu\u00df der Arme sein,", "tokens": ["Wie", "se\u00b7lig", "mu\u00df", "der", "Ar\u00b7me", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der gar nichts mehr mit Lust besitzet,", "tokens": ["Der", "gar", "nichts", "mehr", "mit", "Lust", "be\u00b7sit\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der innerlich entbl\u00f6\u00dft und klein", "tokens": ["Der", "in\u00b7ner\u00b7lich", "ent\u00b7bl\u00f6\u00dft", "und", "klein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nichts hat als Gott, drauf er sich st\u00fctzet!", "tokens": ["Nichts", "hat", "als", "Gott", ",", "drauf", "er", "sich", "st\u00fct\u00b7zet", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "KOKOM", "NN", "$,", "PAV", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}