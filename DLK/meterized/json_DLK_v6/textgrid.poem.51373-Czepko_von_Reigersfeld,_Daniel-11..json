{"textgrid.poem.51373": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum schleust du den Hals und deine weisse Hand", "tokens": ["Wa\u00b7rum", "schleust", "du", "den", "Hals", "und", "dei\u00b7ne", "weis\u00b7se", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An diese goldne Kett', an dieses goldne Band?", "tokens": ["An", "die\u00b7se", "gold\u00b7ne", "Kett'", ",", "an", "die\u00b7ses", "gold\u00b7ne", "Band", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Indem du dich geziert, und gehest wie gefangen,", "tokens": ["In\u00b7dem", "du", "dich", "ge\u00b7ziert", ",", "und", "ge\u00b7hest", "wie", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVPP", "$,", "KON", "VVFIN", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So f\u00fchrst du mich herumb, und bindest mein Verlangen.", "tokens": ["So", "f\u00fchrst", "du", "mich", "he\u00b7rumb", ",", "und", "bin\u00b7dest", "mein", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APZR", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Sch\u00f6nheit ist mein Joch. Drumb ist es recht gethan,", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "ist", "mein", "Joch", ".", "Drumb", "ist", "es", "recht", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$.", "PAV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df ich, und nicht da\u00df du die Ketten tr\u00e4gest an.", "tokens": ["Da\u00df", "ich", ",", "und", "nicht", "da\u00df", "du", "die", "Ket\u00b7ten", "tr\u00e4\u00b7gest", "an", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KON", "PTKNEG", "KOUS", "PPER", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}