{"dta.poem.23466": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gef\u00e4hrdetes Geleite.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit Reichthum und mit Ehre", "tokens": ["Mit", "Reicht\u00b7hum", "und", "mit", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Je wieder in dasselbe Herz;", "tokens": ["Je", "wie\u00b7der", "in", "das\u00b7sel\u00b7be", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie finden Hemmung allerw\u00e4rts:", "tokens": ["Sie", "fin\u00b7den", "Hem\u00b7mung", "al\u00b7ler\u00b7w\u00e4rts", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Untreu h\u00e4lt Hof und Leute,", "tokens": ["Un\u00b7treu", "h\u00e4lt", "Hof", "und", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Gewalt f\u00e4hrt aus auf Beute;", "tokens": ["Ge\u00b7walt", "f\u00e4hrt", "aus", "auf", "Beu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So Fried' als Recht sind todeswund:", "tokens": ["So", "Fried'", "als", "Recht", "sind", "to\u00b7des\u00b7wund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "KOUS", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die dreie haben kein Geleit, die zwei denn werden", "tokens": ["Die", "drei\u00b7e", "ha\u00b7ben", "kein", "Ge\u00b7leit", ",", "die", "zwei", "denn", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "CARD", "VAFIN", "PIAT", "NN", "$,", "PRELS", "CARD", "KON", "VAINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}