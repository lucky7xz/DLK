{"textgrid.poem.36473": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "23. Der Esel, die Nachtigall und der Star", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Esel stand vor seinem Stall,", "tokens": ["Ein", "E\u00b7sel", "stand", "vor", "sei\u00b7nem", "Stall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und h\u00f6rte fr\u00fch die Morgenlieder", "tokens": ["Und", "h\u00f6r\u00b7te", "fr\u00fch", "die", "Mor\u00b7gen\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Nachtigall.", "tokens": ["Der", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Sie singt, man h\u00f6r' einmal! schon wieder,", "tokens": ["Sie", "singt", ",", "man", "h\u00f6r'", "ein\u00b7mal", "!", "schon", "wie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PIS", "VVFIN", "ADV", "$.", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die liebe S\u00e4ngerin!", "tokens": ["Die", "lie\u00b7be", "S\u00e4n\u00b7ge\u00b7rin", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Spricht er zu seiner Eselin:", "tokens": ["Spricht", "er", "zu", "sei\u00b7ner", "E\u00b7se\u00b7lin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gut w\u00e4r's; allein ihr Stimmchen ist zu schwach,", "tokens": ["Gut", "w\u00e4r's", ";", "al\u00b7lein", "ihr", "Stimm\u00b7chen", "ist", "zu", "schwach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$.", "ADV", "PPOSAT", "NN", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich wett', ich s\u00e4nge sie danieder.", "tokens": ["Ich", "wett'", ",", "ich", "s\u00e4n\u00b7ge", "sie", "da\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und pl\u00f6tzlich singt er \u00fcbers Dach,", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "singt", "er", "\u00fc\u00b7bers", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Garten hin,", "tokens": ["Zum", "Gar\u00b7ten", "hin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sein: Ya \u2013 ach!", "tokens": ["Sein", ":", "Y\u00b7a", "\u2013", "ach", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "$.", "FM", "$(", "ITJ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Der V\u00f6gel ganzes Chor", "tokens": ["Der", "V\u00f6\u00b7gel", "gan\u00b7zes", "Chor"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Erschrickt, und fliegt ans Licht, aus H\u00f6hl' und Busch hervor,", "tokens": ["Er\u00b7schrickt", ",", "und", "fliegt", "ans", "Licht", ",", "aus", "H\u00f6hl'", "und", "Busch", "her\u00b7vor", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "VVFIN", "APPRART", "NN", "$,", "APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und lauscht, und singt nicht fort.", "tokens": ["Und", "lauscht", ",", "und", "singt", "nicht", "fort", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der ungeheure Schall", "tokens": ["Der", "un\u00b7ge\u00b7heu\u00b7re", "Schall"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Erschreckt zwar auch die Nachtigall,", "tokens": ["Er\u00b7schreckt", "zwar", "auch", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Allein sie fliegt, und sucht neugierig einen Ort,", "tokens": ["Al\u00b7lein", "sie", "fliegt", ",", "und", "sucht", "neu\u00b7gie\u00b7rig", "ei\u00b7nen", "Ort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,", "KON", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu sehn, was f\u00fcr ein Ungeheuer", "tokens": ["Zu", "sehn", ",", "was", "f\u00fcr", "ein", "Un\u00b7ge\u00b7heu\u00b7er"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Stimm' erhoben hat, und fliegt, und fliegt empor,", "tokens": ["Die", "Stimm'", "er\u00b7ho\u00b7ben", "hat", ",", "und", "fliegt", ",", "und", "fliegt", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und h\u00f6rt das Ya \u2013 ach!", "tokens": ["Und", "h\u00f6rt", "das", "Y\u00b7a", "\u2013", "ach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "FM", "$(", "ITJ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Fliegt auf des hohen Hauses Dach,", "tokens": ["Fliegt", "auf", "des", "ho\u00b7hen", "Hau\u00b7ses", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sieht in den Hof, und sieht", "tokens": ["Sieht", "in", "den", "Hof", ",", "und", "sieht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Zuerst ein langes Ohr,", "tokens": ["Zu\u00b7erst", "ein", "lan\u00b7ges", "Ohr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Und dann den ganzen Schreier!", "tokens": ["Und", "dann", "den", "gan\u00b7zen", "Schrei\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O du, bei dessen T\u00e4ndelein", "tokens": ["O", "du", ",", "bei", "des\u00b7sen", "T\u00e4n\u00b7del\u00b7ein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "APPR", "PRELAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Musen, und die Grazien sich freun,", "tokens": ["Die", "Mu\u00b7sen", ",", "und", "die", "Gra\u00b7zi\u00b7en", "sich", "freun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du, dessen kleinen Liederband", "tokens": ["Du", ",", "des\u00b7sen", "klei\u00b7nen", "Lie\u00b7der\u00b7band"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie gern, mit eigner Hand,", "tokens": ["Sie", "gern", ",", "mit", "eig\u00b7ner", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dianens Nymphen zum Geschenke bringen,", "tokens": ["Di\u00b7a\u00b7nens", "Nym\u00b7phen", "zum", "Ge\u00b7schen\u00b7ke", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Mein Gerstenberg, o denk einmal,", "tokens": ["Mein", "Gers\u00b7ten\u00b7berg", ",", "o", "denk", "ein\u00b7mal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der gro\u00dfe Peter R\u00fcbezahl", "tokens": ["Der", "gro\u00b7\u00dfe", "Pe\u00b7ter", "R\u00fc\u00b7be\u00b7zahl"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Will unsern Uz, und dich, und mich darnieder singen!", "tokens": ["Will", "un\u00b7sern", "Uz", ",", "und", "dich", ",", "und", "mich", "dar\u00b7nie\u00b7der", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "$,", "KON", "PPER", "$,", "KON", "PRF", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}