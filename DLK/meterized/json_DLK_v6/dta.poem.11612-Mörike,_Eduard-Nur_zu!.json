{"dta.poem.11612": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Nur zu!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n prangt im Silberthau die junge Rose,", "tokens": ["Sch\u00f6n", "prangt", "im", "Sil\u00b7bert\u00b7hau", "die", "jun\u00b7ge", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den ihr der Morgen in den Busen rollte,", "tokens": ["Den", "ihr", "der", "Mor\u00b7gen", "in", "den", "Bu\u00b7sen", "roll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie bl\u00fcht, als ob sie nie verbl\u00fchen wollte,", "tokens": ["Sie", "bl\u00fcht", ",", "als", "ob", "sie", "nie", "ver\u00b7bl\u00fc\u00b7hen", "woll\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie ahnet nichts vom lezten Blumen-Loose.", "tokens": ["Sie", "ah\u00b7net", "nichts", "vom", "lez\u00b7ten", "Blu\u00b7men\u00b7Loo\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Adler strebt hinan in's Grenzenlose,", "tokens": ["Der", "Ad\u00b7ler", "strebt", "hi\u00b7nan", "in's", "Gren\u00b7zen\u00b7lo\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein Auge trinkt sich voll von spr\u00fchndem Golde,", "tokens": ["Sein", "Au\u00b7ge", "trinkt", "sich", "voll", "von", "spr\u00fchn\u00b7dem", "Gol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er ist der Thor nicht, da\u00df er fragen sollte,", "tokens": ["Er", "ist", "der", "Thor", "nicht", ",", "da\u00df", "er", "fra\u00b7gen", "soll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKNEG", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ob er das Haupt nicht an die W\u00f6lbung sto\u00dfe.", "tokens": ["Ob", "er", "das", "Haupt", "nicht", "an", "die", "W\u00f6l\u00b7bung", "sto\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mag denn der Jugend Blume uns verbleichen,", "tokens": ["Mag", "denn", "der", "Ju\u00b7gend", "Blu\u00b7me", "uns", "ver\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch gl\u00e4nzet sie und reizt unwiderstehlich,", "tokens": ["Noch", "gl\u00e4n\u00b7zet", "sie", "und", "reizt", "un\u00b7wi\u00b7der\u00b7steh\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer will so holdem Trug zu bald entsagen?", "tokens": ["Wer", "will", "so", "hol\u00b7dem", "Trug", "zu", "bald", "ent\u00b7sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADJA", "NN", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und Liebe, darf sie nicht dem Adler gleichen?", "tokens": ["Und", "Lie\u00b7be", ",", "darf", "sie", "nicht", "dem", "Ad\u00b7ler", "glei\u00b7chen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch f\u00fcrchtet sie; auch f\u00fcrchten ist ihr selig,", "tokens": ["Doch", "f\u00fcrch\u00b7tet", "sie", ";", "auch", "f\u00fcrch\u00b7ten", "ist", "ihr", "se\u00b7lig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "ADV", "VVINF", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn all' ihr Gl\u00fcck, was ist's? ein endlos Wagen!", "tokens": ["Denn", "all'", "ihr", "Gl\u00fcck", ",", "was", "ist's", "?", "ein", "end\u00b7los", "Wa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPOSAT", "NN", "$,", "PWS", "VAFIN", "$.", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}