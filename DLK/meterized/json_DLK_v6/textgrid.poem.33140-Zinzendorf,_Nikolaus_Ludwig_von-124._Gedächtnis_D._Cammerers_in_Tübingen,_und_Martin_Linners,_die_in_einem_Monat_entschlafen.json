{"textgrid.poem.33140": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "124. Ged\u00e4chtnis D. Cammerers in T\u00fcbingen, und Martin Linners, die in einem Monat entschlafen", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.71", "en:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Chor. ", "tokens": ["Chor", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.2": {"text": "V\u00e4ter ey wohin, Mit so sanftem Sinn: etc.", "tokens": ["V\u00e4\u00b7ter", "ey", "wo\u00b7hin", ",", "Mit", "so", "sanf\u00b7tem", "Sinn", ":", "et\u00b7c."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "abbreviation"], "pos": ["NN", "NE", "PWAV", "$,", "APPR", "ADV", "ADJA", "NN", "$.", "ADV"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Ihr selgen Friedens-Geister!s", "tokens": ["Ihr", "sel\u00b7gen", "Frie\u00b7dens\u00b7Geis\u00b7ter", "!", "s"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "XY"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir sehn euch sehnlich nach,", "tokens": ["Wir", "sehn", "euch", "sehn\u00b7lich", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir wissen, da\u00df der Meister", "tokens": ["Wir", "wis\u00b7sen", ",", "da\u00df", "der", "Meis\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die H\u00fctten nicht zerbrach,", "tokens": ["Die", "H\u00fct\u00b7ten", "nicht", "zer\u00b7brach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bis ihre holden G\u00e4ste", "tokens": ["Bis", "ih\u00b7re", "hol\u00b7den", "G\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie gnugsam ausgebraucht", "tokens": ["Sie", "gnug\u00b7sam", "aus\u00b7ge\u00b7braucht"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJD", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und auf dem Herzens-Teste", "tokens": ["Und", "auf", "dem", "Her\u00b7zens\u00b7Tes\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Schlakken ausgeraucht.", "tokens": ["Die", "Schlak\u00b7ken", "aus\u00b7ge\u00b7raucht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Freund und Schmelzer, Du thust treulich,", "tokens": ["Freund", "und", "Schmel\u00b7zer", ",", "Du", "thust", "treu\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und probirst das Gold zur Kron! etc.", "tokens": ["Und", "pro\u00b7birst", "das", "Gold", "zur", "Kron", "!", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "abbreviation"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$.", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Wir sahn dich, w\u00fcrdigs Paar!", "tokens": ["Wir", "sahn", "dich", ",", "w\u00fcr\u00b7digs", "Paar", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die H\u00fctte war zerbrechlich,", "tokens": ["Die", "H\u00fct\u00b7te", "war", "zer\u00b7brech\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das \u00e4u\u00dfre Leben schw\u00e4chlich;", "tokens": ["Das", "\u00e4u\u00df\u00b7re", "Le\u00b7ben", "schw\u00e4ch\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Innre licht und klar.", "tokens": ["Das", "Inn\u00b7re", "licht", "und", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der ewge Lebens-Zunder", "tokens": ["Der", "ew\u00b7ge", "Le\u00b7bens\u00b7Zun\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Legt seine grossen Wunder", "tokens": ["Legt", "sei\u00b7ne", "gros\u00b7sen", "Wun\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Drey Jahr an Linnern dar;", "tokens": ["Drey", "Jahr", "an", "Lin\u00b7nern", "dar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "An Cammerern neun Jahr.", "tokens": ["An", "Cam\u00b7me\u00b7rern", "neun", "Jahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "CARD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O Arzt! ist man verwundt, sind ausgezehrt die Kr\u00e4fte,", "tokens": ["O", "Arzt", "!", "ist", "man", "ver\u00b7wundt", ",", "sind", "aus\u00b7ge\u00b7zehrt", "die", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VAFIN", "PIS", "VVPP", "$,", "VAFIN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So kan die Liebs-Tinctur, Dein theur vergo\u00dfnes Blut,", "tokens": ["So", "kan", "die", "Liebs\u00b7Tinc\u00b7tur", ",", "Dein", "theur", "ver\u00b7go\u00df\u00b7nes", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "$,", "PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Uns heilen, und des Geists Erneurungs-Lebens-S\u00e4fte,", "tokens": ["Uns", "hei\u00b7len", ",", "und", "des", "Geists", "Er\u00b7neu\u00b7rungs\u00b7Le\u00b7bens\u00b7S\u00e4f\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die laben und erfreu'n, die st\u00e4rken Herz und Muth.", "tokens": ["Die", "la\u00b7ben", "und", "er\u00b7freu'n", ",", "die", "st\u00e4r\u00b7ken", "Herz", "und", "Muth", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "KON", "VVINF", "$,", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Es ward Euch auf der Reise", "tokens": ["Es", "ward", "Euch", "auf", "der", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Streiter-Speise", "tokens": ["Die", "Strei\u00b7ter\u00b7Spei\u00b7se"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Nach Patriarchen-Weise,", "tokens": ["Nach", "Pa\u00b7tri\u00b7a\u00b7rchen\u00b7Wei\u00b7se", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Oft aufgetischt;", "tokens": ["Oft", "auf\u00b7ge\u00b7tischt", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Das laimerne Geh\u00e4use", "tokens": ["Das", "lai\u00b7mer\u00b7ne", "Ge\u00b7h\u00e4u\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit aufgefrischt.", "tokens": ["Mit", "auf\u00b7ge\u00b7frischt", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Doch ginget ihr so leise,", "tokens": ["Doch", "gin\u00b7get", "ihr", "so", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Als auf dem Eise:", "tokens": ["Als", "auf", "dem", "Ei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Itzt \u00f6ffnet sich die Schleuse,", "tokens": ["Itzt", "\u00f6ff\u00b7net", "sich", "die", "Schleu\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Der Geist entwischt.", "tokens": ["Der", "Geist", "ent\u00b7wischt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Nun k\u00fcssen euch der Weisheit s\u00fcsse Blikke,", "tokens": ["Nun", "k\u00fcs\u00b7sen", "euch", "der", "Weis\u00b7heit", "s\u00fcs\u00b7se", "Blik\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nun ruht die Seel in Christi Liebes-Schoo\u00df,", "tokens": ["Nun", "ruht", "die", "Seel", "in", "Chris\u00b7ti", "Lie\u00b7bes\u00b7Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nun ist das Herz vom Tod, vom S\u00fcnden-Strikke,", "tokens": ["Nun", "ist", "das", "Herz", "vom", "Tod", ",", "vom", "S\u00fcn\u00b7den\u00b7Strik\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und von dem Geist der Eitelkeiten los.", "tokens": ["Und", "von", "dem", "Geist", "der", "Ei\u00b7tel\u00b7kei\u00b7ten", "los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Wie kl\u00e4rlich wird die Hand des Herrn", "tokens": ["Wie", "kl\u00e4r\u00b7lich", "wird", "die", "Hand", "des", "Herrn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bey diesem Lauf gesp\u00fcret!", "tokens": ["Bey", "die\u00b7sem", "Lauf", "ge\u00b7sp\u00fc\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer siehet nicht, da\u00df sie Sein Stern", "tokens": ["Wer", "sie\u00b7het", "nicht", ",", "da\u00df", "sie", "Sein", "Stern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So ein- als ausgef\u00fchret?", "tokens": ["So", "ein", "als", "aus\u00b7ge\u00b7f\u00fch\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "TRUNC", "KOKOM", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ihr Br\u00fcder, man mu\u00df sich dem Licht", "tokens": ["Ihr", "Br\u00fc\u00b7der", ",", "man", "mu\u00df", "sich", "dem", "Licht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PIS", "VMFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur blindlings \u00fcberlassen,", "tokens": ["Nur", "blind\u00b7lings", "\u00fc\u00b7ber\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und, was uns Christi Geist verspricht,", "tokens": ["Und", ",", "was", "uns", "Chris\u00b7ti", "Geist", "ver\u00b7spricht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "PPER", "NE", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit k\u00fchnem Glauben fassen.", "tokens": ["Mit", "k\u00fch\u00b7nem", "Glau\u00b7ben", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Warum wird doch das Volk des Herrn nicht weiser:", "tokens": ["Wa\u00b7rum", "wird", "doch", "das", "Volk", "des", "Herrn", "nicht", "wei\u00b7ser", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und trauet Ihm von nun an alles zu, etc.", "tokens": ["Und", "trau\u00b7et", "Ihm", "von", "nun", "an", "al\u00b7les", "zu", ",", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "abbreviation"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADV", "APPR", "PIS", "PTKVZ", "$,", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}