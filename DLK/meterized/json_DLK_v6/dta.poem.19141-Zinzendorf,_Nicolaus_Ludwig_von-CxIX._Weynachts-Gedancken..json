{"dta.poem.19141": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "CxIX.  Weynachts-Gedancken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcr uns gebohrnes Kind!", "tokens": ["F\u00fcr", "uns", "ge\u00b7bohr\u00b7nes", "Kind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "O Sohn! vor unser Leben", "tokens": ["O", "Sohn", "!", "vor", "un\u00b7ser", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In einen Tod gegeben,", "tokens": ["In", "ei\u00b7nen", "Tod", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Tod und H\u00f6lle bindt.", "tokens": ["Der", "Tod", "und", "H\u00f6l\u00b7le", "bindt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O m\u00f6chten wir uns schmiegen", "tokens": ["O", "m\u00f6ch\u00b7ten", "wir", "uns", "schmie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Kind! bi\u00df zu deinen Wiegen!", "tokens": ["Kind", "!", "bi\u00df", "zu", "dei\u00b7nen", "Wie\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "O w\u00e4ren wir so klein,", "tokens": ["O", "w\u00e4\u00b7ren", "wir", "so", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Als du im Krippelein.", "tokens": ["Als", "du", "im", "Krip\u00b7pe\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hierher Vernunfft und Witz!", "tokens": ["Hier\u00b7her", "Ver\u00b7nunfft", "und", "Witz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da liegt ein Mann in Bindlein", "tokens": ["Da", "liegt", "ein", "Mann", "in", "Bind\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der abgeri\u00dfnen Windlein,", "tokens": ["Der", "ab\u00b7ge\u00b7ri\u00df\u00b7nen", "Wind\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der auf dem stoltzen Sitz", "tokens": ["Der", "auf", "dem", "stolt\u00b7zen", "Sitz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der rechten Hand der Kr\u00e4ffte", "tokens": ["Der", "rech\u00b7ten", "Hand", "der", "Kr\u00e4ff\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und siegenden Gesch\u00e4ffte,", "tokens": ["Und", "sie\u00b7gen\u00b7den", "Ge\u00b7sch\u00e4ff\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den Nahmen und die That", "tokens": ["Den", "Nah\u00b7men", "und", "die", "That"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Gott aller G\u00f6tter hat.", "tokens": ["Gott", "al\u00b7ler", "G\u00f6t\u00b7ter", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er heisset Wunderbahr:", "tokens": ["Er", "heis\u00b7set", "Wun\u00b7der\u00b7bahr", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und alle seine Nahmen", "tokens": ["Und", "al\u00b7le", "sei\u00b7ne", "Nah\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Versiegelt er mit Amen,", "tokens": ["Ver\u00b7sie\u00b7gelt", "er", "mit", "A\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Eingang war zur Krippe;", "tokens": ["Der", "Ein\u00b7gang", "war", "zur", "Krip\u00b7pe", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Ausgang durch die Klippe.", "tokens": ["Der", "Aus\u00b7gang", "durch", "die", "Klip\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein ungebahnter Weg,", "tokens": ["Ein", "un\u00b7ge\u00b7bahn\u00b7ter", "Weg", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein wunderbahrer Steg.", "tokens": ["Ein", "wun\u00b7der\u00b7bah\u00b7rer", "Steg", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Jhr ", "tokens": ["Ihr"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Hier sind die Wei\u00dfheits Throne:", "tokens": ["Hier", "sind", "die", "Wei\u00df\u00b7heits", "Thro\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jhr findet bey dem Sohne", "tokens": ["Ihr", "fin\u00b7det", "bey", "dem", "Soh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den allertreusten Rath;", "tokens": ["Den", "al\u00b7ler\u00b7treus\u00b7ten", "Rath", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und euer Pilger-Wandel", "tokens": ["Und", "eu\u00b7er", "Pil\u00b7ger\u00b7Wan\u00b7del"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und euer Streiter-Handel", "tokens": ["Und", "eu\u00b7er", "Strei\u00b7ter\u00b7Han\u00b7del"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wird, durch dis klare Licht,", "tokens": ["Wird", ",", "durch", "dis", "kla\u00b7re", "Licht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Vollkommen eingericht.", "tokens": ["Voll\u00b7kom\u00b7men", "ein\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Jhr ", "tokens": ["Ihr"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "In diesen Arbeits-Tagen,", "tokens": ["In", "die\u00b7sen", "Ar\u00b7beits\u00b7Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit Tapfferkeit zu tragen,", "tokens": ["Mit", "Tapf\u00b7fer\u00b7keit", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ohne tr\u00e4ge Rast", "tokens": ["Und", "oh\u00b7ne", "tr\u00e4\u00b7ge", "Rast"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Werck in euren H\u00e4nden", "tokens": ["Das", "Werck", "in", "eu\u00b7ren", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu kehren und zu wenden,", "tokens": ["Zu", "keh\u00b7ren", "und", "zu", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Damit es Seegen schafft;", "tokens": ["Da\u00b7mit", "es", "See\u00b7gen", "schafft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Greift zu! hier liegt die Krafft.", "tokens": ["Greift", "zu", "!", "hier", "liegt", "die", "Krafft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du ", "tokens": ["Du"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Und leider allzu zahme", "tokens": ["Und", "lei\u00b7der", "all\u00b7zu", "zah\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "PTKA", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ver\u00e4chter der Gefahr,)", "tokens": ["Ver\u00b7\u00e4ch\u00b7ter", "der", "Ge\u00b7fahr", ",", ")"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Last euch den grossen Helden,", "tokens": ["Last", "euch", "den", "gros\u00b7sen", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Den GOtt mit uns, vermelden.", "tokens": ["Den", "Gott", "mit", "uns", ",", "ver\u00b7mel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Seht, da\u00df ihr fertig steht,", "tokens": ["Seht", ",", "da\u00df", "ihr", "fer\u00b7tig", "steht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wenn er zu Felde geht.", "tokens": ["Wenn", "er", "zu", "Fel\u00b7de", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Jhr ", "tokens": ["Ihr"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Der Vater kan erwehlen,", "tokens": ["Der", "Va\u00b7ter", "kan", "er\u00b7weh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ob sich das Kind verm\u00e4hlen,", "tokens": ["Ob", "sich", "das", "Kind", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Obs einsam bleiben soll?", "tokens": ["Obs", "ein\u00b7sam", "blei\u00b7ben", "soll", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach! w\u00fcrden eure Sinnen", "tokens": ["Ach", "!", "w\u00fcr\u00b7den", "eu\u00b7re", "Sin\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des ewgen Vaters innen,", "tokens": ["Des", "ew\u00b7gen", "Va\u00b7ters", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und g\u00e4ben Hertz und Sinn", "tokens": ["Und", "g\u00e4\u00b7ben", "Hertz", "und", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "In seine Sorge hin.", "tokens": ["In", "sei\u00b7ne", "Sor\u00b7ge", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der mit der argen Welt,", "tokens": ["Der", "mit", "der", "ar\u00b7gen", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und mit der S\u00fcnde krieget,", "tokens": ["Und", "mit", "der", "S\u00fcn\u00b7de", "krie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Belial besieget,", "tokens": ["Und", "Be\u00b7li\u00b7al", "be\u00b7sie\u00b7get", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ewig Treue h\u00e4lt;", "tokens": ["Und", "e\u00b7wig", "Treu\u00b7e", "h\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der sey auch unsrer ", "tokens": ["Der", "sey", "auch", "uns\u00b7rer"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Die nach der Gnade d\u00fcrst,", "tokens": ["Die", "nach", "der", "Gna\u00b7de", "d\u00fcrst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Jhr wohl gewogner F\u00fcrst.", "tokens": ["Ihr", "wohl", "ge\u00b7wog\u00b7ner", "F\u00fcrst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ja Amen! das sey wahr:", "tokens": ["Ja", "A\u00b7men", "!", "das", "sey", "wahr", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "$.", "PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Du F\u00fcrst der stillen Ch\u00f6re!", "tokens": ["Du", "F\u00fcrst", "der", "stil\u00b7len", "Ch\u00f6\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du Held der GOttes-Heere!", "tokens": ["Du", "Held", "der", "Got\u00b7tes\u00b7Hee\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Krafft, Rath und Wunderbahr.", "tokens": ["Krafft", ",", "Rath", "und", "Wun\u00b7der\u00b7bahr", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir schwern zu deiner Krippen,", "tokens": ["Wir", "schwern", "zu", "dei\u00b7ner", "Krip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit Hertzen und mit Lippen,", "tokens": ["Mit", "Hert\u00b7zen", "und", "mit", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wir folgen deiner Spur", "tokens": ["Wir", "fol\u00b7gen", "dei\u00b7ner", "Spur"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Zur g\u00f6ttlichen Natur.", "tokens": ["Zur", "g\u00f6tt\u00b7li\u00b7chen", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}