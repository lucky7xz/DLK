{"textgrid.poem.34577": {"metadata": {"author": {"name": "Hoffmannswaldau, Christian Hoffmann von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach! was benebelt doch die Kr\u00e4fte deiner Sinnen?", "genre": "verse", "period": "N.A.", "pub_year": 1647, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach! was benebelt doch die Kr\u00e4fte deiner Sinnen?", "tokens": ["Ach", "!", "was", "be\u00b7ne\u00b7belt", "doch", "die", "Kr\u00e4f\u00b7te", "dei\u00b7ner", "Sin\u00b7nen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wirst du bei Sonnenschein Nichts mehr erkiesen k\u00f6nnen?", "tokens": ["Wirst", "du", "bei", "Son\u00b7nen\u00b7schein", "Nichts", "mehr", "er\u00b7kie\u00b7sen", "k\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "PIS", "ADV", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kennst du dich selber nicht?", "tokens": ["Kennst", "du", "dich", "sel\u00b7ber", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "PTKNEG", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Dich hungert bei der Kost, dich d\u00fcrstet bei den Fl\u00fcssen,", "tokens": ["Dich", "hun\u00b7gert", "bei", "der", "Kost", ",", "dich", "d\u00fcrs\u00b7tet", "bei", "den", "Fl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PPER", "VMFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du wirst zu Eis und Schnee beim Feuer werden m\u00fcssen,", "tokens": ["Du", "wirst", "zu", "Eis", "und", "Schnee", "beim", "Feu\u00b7er", "wer\u00b7den", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "APPRART", "NN", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du klagst bei Ueberflu\u00df, da\u00df Alles dir gebricht.", "tokens": ["Du", "klagst", "bei", "Ue\u00b7berf\u00b7lu\u00df", ",", "da\u00df", "Al\u00b7les", "dir", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was marterst du dich selbst mit d\u00fcrftigen Gedanken,", "tokens": ["Was", "mar\u00b7terst", "du", "dich", "selbst", "mit", "d\u00fcrf\u00b7ti\u00b7gen", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dr\u00e4ngst bei gesunder Haut dich in die Reih' der Kranken", "tokens": ["Dr\u00e4ngst", "bei", "ge\u00b7sun\u00b7der", "Haut", "dich", "in", "die", "Reih'", "der", "Kran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und seufzest bei der Lust?", "tokens": ["Und", "seuf\u00b7zest", "bei", "der", "Lust", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wer sich am Herzen nagt, der speiset allzutheuer.", "tokens": ["Wer", "sich", "am", "Her\u00b7zen", "nagt", ",", "der", "spei\u00b7set", "all\u00b7zu\u00b7theu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPRART", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach, mache dich nicht selbst zu einem Ungeheuer,", "tokens": ["Ach", ",", "ma\u00b7che", "dich", "nicht", "selbst", "zu", "ei\u00b7nem", "Un\u00b7ge\u00b7heu\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das sich die N\u00e4gel sch\u00e4rft, zu schaden seiner Brust!", "tokens": ["Das", "sich", "die", "N\u00e4\u00b7gel", "sch\u00e4rft", ",", "zu", "scha\u00b7den", "sei\u00b7ner", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ART", "NN", "VVFIN", "$,", "PTKA", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Will denn der Liebesbaum stets Argwohnsfr\u00fcchte tragen?", "tokens": ["Will", "denn", "der", "Lie\u00b7bes\u00b7baum", "stets", "Arg\u00b7wohns\u00b7fr\u00fcch\u00b7te", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Soll denn sein Schatten uns die beste Lust verjagen", "tokens": ["Soll", "denn", "sein", "Schat\u00b7ten", "uns", "die", "bes\u00b7te", "Lust", "ver\u00b7ja\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "PPER", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und bringen Ach und Weh?", "tokens": ["Und", "brin\u00b7gen", "Ach", "und", "Weh", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Man weint oft ohne Noth und zweifelt ohne Gr\u00fcnde,", "tokens": ["Man", "weint", "oft", "oh\u00b7ne", "Noth", "und", "zwei\u00b7felt", "oh\u00b7ne", "Gr\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "NN", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Plagt seiner Sinne Schiff mit ungest\u00fcmem Winde", "tokens": ["Plagt", "sei\u00b7ner", "Sin\u00b7ne", "Schiff", "mit", "un\u00b7ge\u00b7st\u00fc\u00b7mem", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und st\u00fcrzt sich ohne Sturm tief in die Trauersee.", "tokens": ["Und", "st\u00fcrzt", "sich", "oh\u00b7ne", "Sturm", "tief", "in", "die", "Trau\u00b7er\u00b7see", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Rosen bl\u00fchen dir; was willst du Nesseln hegen,", "tokens": ["Die", "Ro\u00b7sen", "bl\u00fc\u00b7hen", "dir", ";", "was", "willst", "du", "Nes\u00b7seln", "he\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "PWS", "VMFIN", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Disteln, reich an Angst, zu Lustnarcissen legen?", "tokens": ["Und", "Dis\u00b7teln", ",", "reich", "an", "Angst", ",", "zu", "Lust\u00b7nar\u00b7cis\u00b7sen", "le\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJD", "APPR", "NN", "$,", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was Uebels st\u00f6\u00dft dich an?", "tokens": ["Was", "Ue\u00b7bels", "st\u00f6\u00dft", "dich", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bem\u00fch' dich, deinen Geist in s\u00fc\u00dfe Ruh' zu setzen,", "tokens": ["Be\u00b7m\u00fch'", "dich", ",", "dei\u00b7nen", "Geist", "in", "s\u00fc\u00b7\u00dfe", "Ruh'", "zu", "set\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und rei\u00df' dich mit Gewalt aus Schmerz und Trauernetzen!", "tokens": ["Und", "rei\u00df'", "dich", "mit", "Ge\u00b7walt", "aus", "Schmerz", "und", "Trau\u00b7er\u00b7net\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dem schadet nicht Verzug, wer Zeit erwarten kann.", "tokens": ["Dem", "scha\u00b7det", "nicht", "Ver\u00b7zug", ",", "wer", "Zeit", "er\u00b7war\u00b7ten", "kann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "NN", "$,", "PWS", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wen blinder Eifer wiegt, der tr\u00e4umt von Ungel\u00fccke,", "tokens": ["Wen", "blin\u00b7der", "Ei\u00b7fer", "wiegt", ",", "der", "tr\u00e4umt", "von", "Un\u00b7ge\u00b7l\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ruft, frei und ungel\u00e4hmt, nach Rettung und nach Kr\u00fccke,", "tokens": ["Ruft", ",", "frei", "und", "un\u00b7ge\u00b7l\u00e4hmt", ",", "nach", "Ret\u00b7tung", "und", "nach", "Kr\u00fc\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "KON", "ADJD", "$,", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Meint stets auf Eis zu stehn.", "tokens": ["Meint", "stets", "auf", "Eis", "zu", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Erwach' und streich dir doch die Schuppen vom Gesichte!", "tokens": ["Er\u00b7wach'", "und", "streich", "dir", "doch", "die", "Schup\u00b7pen", "vom", "Ge\u00b7sich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kein Kluger macht sich selbst durch Wahn und Dunst zu nichte.", "tokens": ["Kein", "Klu\u00b7ger", "macht", "sich", "selbst", "durch", "Wahn", "und", "Dunst", "zu", "nich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "ADV", "APPR", "NN", "KON", "NN", "APPR", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was s\u00e4umest du doch, selbst in's Paradies zu gehn?", "tokens": ["Was", "s\u00e4u\u00b7mest", "du", "doch", ",", "selbst", "in's", "Pa\u00b7ra\u00b7dies", "zu", "gehn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}