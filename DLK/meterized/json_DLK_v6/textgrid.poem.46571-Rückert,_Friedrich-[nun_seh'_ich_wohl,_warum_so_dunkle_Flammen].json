{"textgrid.poem.46571": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[nun seh' ich wohl, warum so dunkle Flammen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun seh' ich wohl, warum so dunkle Flammen", "tokens": ["Nun", "seh'", "ich", "wohl", ",", "wa\u00b7rum", "so", "dunk\u00b7le", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PWAV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr spr\u00fchtet mir in manchem Augenblicke,", "tokens": ["Ihr", "spr\u00fch\u00b7tet", "mir", "in", "man\u00b7chem", "Au\u00b7gen\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O Augen, gleichsam um in einem Blicke", "tokens": ["O", "Au\u00b7gen", ",", "gleich\u00b7sam", "um", "in", "ei\u00b7nem", "Bli\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "ADJD", "APPR", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu dr\u00e4ngen eure ganze Macht zusammen.", "tokens": ["Zu", "dr\u00e4n\u00b7gen", "eu\u00b7re", "gan\u00b7ze", "Macht", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dort ahnt' ich nicht, weil Nebel mich umschwammen,", "tokens": ["Dort", "ahnt'", "ich", "nicht", ",", "weil", "Ne\u00b7bel", "mich", "um\u00b7schwam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gewoben vom verblendenden Geschicke,", "tokens": ["Ge\u00b7wo\u00b7ben", "vom", "ver\u00b7blen\u00b7den\u00b7den", "Ge\u00b7schi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df sich der Strahl bereits zur Heimkehr schicke", "tokens": ["Da\u00df", "sich", "der", "Strahl", "be\u00b7reits", "zur", "Heim\u00b7kehr", "schi\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dorthin, von wannen alle Strahlen stammen.", "tokens": ["Dor\u00b7thin", ",", "von", "wan\u00b7nen", "al\u00b7le", "Strah\u00b7len", "stam\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ihr wolltet mir mit eurem Leuchten sagen:", "tokens": ["Ihr", "woll\u00b7tet", "mir", "mit", "eu\u00b7rem", "Leuch\u00b7ten", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir m\u00f6chten nah dir immer bleiben gerne,", "tokens": ["Wir", "m\u00f6ch\u00b7ten", "nah", "dir", "im\u00b7mer", "blei\u00b7ben", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "PPER", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch ist und das vom Schicksal abgeschlagen.", "tokens": ["Doch", "ist", "und", "das", "vom", "Schick\u00b7sal", "ab\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "KON", "ART", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sieh' recht uns an! denn bald sind wir dir ferne.", "tokens": ["Sieh'", "recht", "uns", "an", "!", "denn", "bald", "sind", "wir", "dir", "fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPER", "PTKVZ", "$.", "KON", "ADV", "VAFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was dir noch Augen sind in diesen Tagen,", "tokens": ["Was", "dir", "noch", "Au\u00b7gen", "sind", "in", "die\u00b7sen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "NN", "VAFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In k\u00fcnft'gen N\u00e4chten sind es dir nur Sterne.", "tokens": ["In", "k\u00fcnft'\u00b7gen", "N\u00e4ch\u00b7ten", "sind", "es", "dir", "nur", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}