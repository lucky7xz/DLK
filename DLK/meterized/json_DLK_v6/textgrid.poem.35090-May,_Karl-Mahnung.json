{"textgrid.poem.35090": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Mahnung", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siehst du an des Berges Hange", "tokens": ["Siehst", "du", "an", "des", "Ber\u00b7ges", "Han\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Irgendwo ein Kirchlein stehn,", "tokens": ["Ir\u00b7gend\u00b7wo", "ein", "Kirch\u00b7lein", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Warte, warte ja nicht lange;", "tokens": ["War\u00b7te", ",", "war\u00b7te", "ja", "nicht", "lan\u00b7ge", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei so gut, hinein zu gehn!", "tokens": ["Sei", "so", "gut", ",", "hin\u00b7ein", "zu", "gehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Liebst du es, mit Gott zu reden,", "tokens": ["Liebst", "du", "es", ",", "mit", "Gott", "zu", "re\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kniee hin, und bete still;", "tokens": ["Kni\u00b7ee", "hin", ",", "und", "be\u00b7te", "still", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "KON", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Kirche h\u00f6rt er Jeden,", "tokens": ["In", "der", "Kir\u00b7che", "h\u00f6rt", "er", "Je\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der ihm Etwas sagen will.", "tokens": ["Der", "ihm", "Et\u00b7was", "sa\u00b7gen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Bist du Zweifler, nun, so falte", "tokens": ["Bist", "du", "Zweif\u00b7ler", ",", "nun", ",", "so", "fal\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "NN", "$,", "ADV", "$,", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenigstens die H\u00e4nde stumm,", "tokens": ["We\u00b7nigs\u00b7tens", "die", "H\u00e4n\u00b7de", "stumm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df der Herr dich noch erhalte,", "tokens": ["Da\u00df", "der", "Herr", "dich", "noch", "er\u00b7hal\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn du auch nicht wei\u00dft, warum.", "tokens": ["Wenn", "du", "auch", "nicht", "wei\u00dft", ",", "wa\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,", "PWAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und bist du vielleicht gescheidter", "tokens": ["Und", "bist", "du", "viel\u00b7leicht", "ge\u00b7scheid\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADJD"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Als die \u00bbEinfalt, welche glaubt\u00ab,", "tokens": ["Als", "die", "\u00bb", "Ein\u00b7falt", ",", "wel\u00b7che", "glaubt", "\u00ab", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "$(", "NN", "$,", "PRELS", "VVFIN", "$(", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun, so thue gar nichts weiter", "tokens": ["Nun", ",", "so", "thue", "gar", "nichts", "wei\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VVFIN", "ADV", "PIS", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als: entbl\u00f6\u00df dein stolzes Haupt.", "tokens": ["Als", ":", "ent\u00b7bl\u00f6\u00df", "dein", "stol\u00b7zes", "Haupt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$.", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ja, siehst du am Bergeshange", "tokens": ["Ja", ",", "siehst", "du", "am", "Ber\u00b7ges\u00b7han\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Irgendwo ein Kirchlein stehn,", "tokens": ["Ir\u00b7gend\u00b7wo", "ein", "Kirch\u00b7lein", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieses Opfer w\u00e4hrt nicht lange:", "tokens": ["Die\u00b7ses", "Op\u00b7fer", "w\u00e4hrt", "nicht", "lan\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei so gut, hinein zu gehn!", "tokens": ["Sei", "so", "gut", ",", "hin\u00b7ein", "zu", "gehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}