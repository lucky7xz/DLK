{"textgrid.poem.37154": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "A.", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus elysischen Gefilden, Myrthenhainen,", "tokens": ["Aus", "e\u00b7ly\u00b7si\u00b7schen", "Ge\u00b7fil\u00b7den", ",", "Myr\u00b7then\u00b7hai\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "$,"], "meter": "-++-+-+-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Wo die abgeschiednen Geister Liebe weinen,", "tokens": ["Wo", "die", "ab\u00b7ge\u00b7schied\u00b7nen", "Geis\u00b7ter", "Lie\u00b7be", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Von kristallnen B\u00e4chen, die der Mond begl\u00e4nzt", "tokens": ["Von", "kris\u00b7tall\u00b7nen", "B\u00e4\u00b7chen", ",", "die", "der", "Mond", "be\u00b7gl\u00e4nzt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVPP"], "meter": "-++-+-+-+-+", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Und ein Fr\u00fchling ewig jung und l\u00e4chelnd kr\u00e4nzt,", "tokens": ["Und", "ein", "Fr\u00fch\u00b7ling", "e\u00b7wig", "jung", "und", "l\u00e4\u00b7chelnd", "kr\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Will ich einsam in zerri\u00dfnen Lumpen eilen,", "tokens": ["Will", "ich", "ein\u00b7sam", "in", "zer\u00b7ri\u00df\u00b7nen", "Lum\u00b7pen", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Meinen finstern liebeskranken Gram zu heilen.", "tokens": ["Mei\u00b7nen", "fins\u00b7tern", "lie\u00b7bes\u00b7kran\u00b7ken", "Gram", "zu", "hei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVFIN", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Luna blieb am Himmel sp\u00e4t,", "tokens": ["Lu\u00b7na", "blieb", "am", "Him\u00b7mel", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Fr\u00f6hlich schwebte Mab im Tanze,", "tokens": ["Fr\u00f6h\u00b7lich", "schweb\u00b7te", "Mab", "im", "Tan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Oberon voll Majest\u00e4t", "tokens": ["O\u00b7be\u00b7ron", "voll", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Sah, wie Mars mit seiner Lanze", "tokens": ["Sah", ",", "wie", "Mars", "mit", "sei\u00b7ner", "Lan\u00b7ze"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Die Liebesg\u00f6ttin verwunden th\u00e4t.", "tokens": ["Die", "Lie\u00b7bes\u00b7g\u00f6t\u00b7tin", "ver\u00b7wun\u00b7den", "th\u00e4t", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "In jener Primel tief begraben", "tokens": ["In", "je\u00b7ner", "Pri\u00b7mel", "tief", "be\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Liegt er in hellen Tropfen Thau.", "tokens": ["Liegt", "er", "in", "hel\u00b7len", "Trop\u00b7fen", "Thau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "T\u00e4glich soll dich meine Thr\u00e4ne laben,", "tokens": ["T\u00e4g\u00b7lich", "soll", "dich", "mei\u00b7ne", "Thr\u00e4\u00b7ne", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PRF", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Da\u00df du nicht welkst, o Bl\u00fcmchen, auf der Au.", "tokens": ["Da\u00df", "du", "nicht", "welkst", ",", "o", "Bl\u00fcm\u00b7chen", ",", "auf", "der", "Au", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "$,", "FM", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Denn seit Er tot ist, hab ich keine Freude mehr!", "tokens": ["Denn", "seit", "Er", "tot", "ist", ",", "hab", "ich", "kei\u00b7ne", "Freu\u00b7de", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADJD", "VAFIN", "$,", "VAFIN", "PPER", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Vergi\u00dfmeinnicht und Rosen will ich finden,", "tokens": ["Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", "und", "Ro\u00b7sen", "will", "ich", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Ein Kr\u00e4nzchen meinem Freund zu binden.", "tokens": ["Ein", "Kr\u00e4nz\u00b7chen", "mei\u00b7nem", "Freund", "zu", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Statt der Musik erschall ein Seufzen um mich her!", "tokens": ["Statt", "der", "Mu\u00b7sik", "er\u00b7schall", "ein", "Seuf\u00b7zen", "um", "mich", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.20": {"text": "In einen holen Baum will ich mich niederlegen,", "tokens": ["In", "ei\u00b7nen", "ho\u00b7len", "Baum", "will", "ich", "mich", "nie\u00b7der\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Dem Tode l\u00e4cheln, segnen das Verderben", "tokens": ["Dem", "To\u00b7de", "l\u00e4\u00b7cheln", ",", "seg\u00b7nen", "das", "Ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Und sterben.", "tokens": ["Und", "ster\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.23": {"text": "Raben, Katzen, Flederm\u00e4use", "tokens": ["Ra\u00b7ben", ",", "Kat\u00b7zen", ",", "Fle\u00b7der\u00b7m\u00e4u\u00b7se"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "In der b\u00e4ngsten, f\u00fcrchterlichsten Weise", "tokens": ["In", "der", "b\u00e4ngs\u00b7ten", ",", "f\u00fcrch\u00b7ter\u00b7lichs\u00b7ten", "Wei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "Sollen Wald und Felsen zum Gef\u00fchl bewegen!", "tokens": ["Sol\u00b7len", "Wald", "und", "Fel\u00b7sen", "zum", "Ge\u00b7f\u00fchl", "be\u00b7we\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.26": {"text": "Uhus, Eulen", "tokens": ["U\u00b7hus", ",", "Eu\u00b7len"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$,", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.27": {"text": "Sollen mir mein Grablied heulen!", "tokens": ["Sol\u00b7len", "mir", "mein", "Grab\u00b7lied", "heu\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Saht ihr ihn nicht? Wie ihm die schwarzen Augen brennen!", "tokens": ["Saht", "ihr", "ihn", "nicht", "?", "Wie", "ihm", "die", "schwar\u00b7zen", "Au\u00b7gen", "bren\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "$.", "PWAV", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "M\u00e4dchen, f\u00fcrchtet ihre Macht!", "tokens": ["M\u00e4d\u00b7chen", ",", "f\u00fcrch\u00b7tet", "ih\u00b7re", "Macht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Nehmt euer Herz in Acht!", "tokens": ["Nehmt", "eu\u00b7er", "Herz", "in", "Acht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Wie w\u00fcrdet, w\u00fcrdet ihr dem Mann entfliehen k\u00f6nnen!", "tokens": ["Wie", "w\u00fcr\u00b7det", ",", "w\u00fcr\u00b7det", "ihr", "dem", "Mann", "ent\u00b7flie\u00b7hen", "k\u00f6n\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "$,", "VAFIN", "PPER", "ART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Horch, horch der alte Charon!", "tokens": ["Horch", ",", "horch", "der", "al\u00b7te", "Cha\u00b7ron", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.33": {"text": "Er will nicht l\u00e4nger warten!", "tokens": ["Er", "will", "nicht", "l\u00e4n\u00b7ger", "war\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Die Furien erheben ihre Peitschen,", "tokens": ["Die", "Fu\u00b7ri\u00b7en", "er\u00b7he\u00b7ben", "ih\u00b7re", "Peit\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Und rufen: von hinnen! von hinnen!", "tokens": ["Und", "ru\u00b7fen", ":", "von", "hin\u00b7nen", "!", "von", "hin\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "APPR", "ADV", "$.", "APPR", "ADV", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.36": {"text": "So kehr ich denn zur\u00fcck woher ich kam.", "tokens": ["So", "kehr", "ich", "denn", "zu\u00b7r\u00fcck", "wo\u00b7her", "ich", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Die Welt ist viel zu toll, nichts lindert meinen Gram.", "tokens": ["Die", "Welt", "ist", "viel", "zu", "toll", ",", "nichts", "lin\u00b7dert", "mei\u00b7nen", "Gram", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$,", "PIS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Was soll ich l\u00e4nger schmachten?", "tokens": ["Was", "soll", "ich", "l\u00e4n\u00b7ger", "schmach\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Die Lieb' ist alles Elends Same,", "tokens": ["Die", "Lieb'", "ist", "al\u00b7les", "E\u00b7lends", "Sa\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Ist eine Seifenblas', ein Schatten und ein Name,", "tokens": ["Ist", "ei\u00b7ne", "Sei\u00b7fen\u00b7blas'", ",", "ein", "Schat\u00b7ten", "und", "ein", "Na\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Den Narren bewundern und Weise verachten.", "tokens": ["Den", "Nar\u00b7ren", "be\u00b7wun\u00b7dern", "und", "Wei\u00b7se", "ver\u00b7ach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "NN", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.42": {"text": "Kalt und hungrig bin ich nun \u2013", "tokens": ["Kalt", "und", "hung\u00b7rig", "bin", "ich", "nun", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "PPER", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Unter Blumen will ich ruhn,", "tokens": ["Un\u00b7ter", "Blu\u00b7men", "will", "ich", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.44": {"text": "Tr\u00e4umend hin auf Himmelsmatten sinken,", "tokens": ["Tr\u00e4u\u00b7mend", "hin", "auf", "Him\u00b7mels\u00b7mat\u00b7ten", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.45": {"text": "G\u00f6tterspeise kosten, Nectar trinken", "tokens": ["G\u00f6t\u00b7ter\u00b7spei\u00b7se", "kos\u00b7ten", ",", "Nec\u00b7tar", "trin\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.46": {"text": "Und singen:", "tokens": ["Und", "sin\u00b7gen", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.47": {"text": "Wer heiter ist und froh,", "tokens": ["Wer", "hei\u00b7ter", "ist", "und", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.48": {"text": "Kann jeden Gram bezwingen.", "tokens": ["Kann", "je\u00b7den", "Gram", "be\u00b7zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.49": {"text": "Bei Wa\u00dfer und auf Stroh", "tokens": ["Bei", "Wa\u00b7\u00dfer", "und", "auf", "Stroh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.50": {"text": "Bin ich in meinem Sinn", "tokens": ["Bin", "ich", "in", "mei\u00b7nem", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.51": {"text": "Zufriedner als die K\u00f6nigin,", "tokens": ["Zu\u00b7fried\u00b7ner", "als", "die", "K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "So lang ich ohne Fe\u00dfel bin!", "tokens": ["So", "lang", "ich", "oh\u00b7ne", "Fe\u00b7\u00dfel", "bin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}