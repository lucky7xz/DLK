{"textgrid.poem.52818": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Morgenstund", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fch aufzusein, o du Vergn\u00fcgen,", "tokens": ["Fr\u00fch", "auf\u00b7zu\u00b7sein", ",", "o", "du", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "FM", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du reinster Wonne Hochbefund!", "tokens": ["Du", "reins\u00b7ter", "Won\u00b7ne", "Hoch\u00b7be\u00b7fund", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer soll da lang im Bette liegen,", "tokens": ["Wer", "soll", "da", "lang", "im", "Bet\u00b7te", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo Morgenstund hat Gold im Mund?", "tokens": ["Wo", "Mor\u00b7gen\u00b7stund", "hat", "Gold", "im", "Mund", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VAFIN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Fr\u00fch aufzusein erquicket Jeden,", "tokens": ["Fr\u00fch", "auf\u00b7zu\u00b7sein", "er\u00b7quic\u00b7ket", "Je\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVIZU", "VVFIN", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Fr\u00fch aufzus", "tokens": ["Fr\u00fch", "auf\u00b7zus"], "token_info": ["word", "word"], "pos": ["ADJD", "NE"], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "Ha! wer gelanget in das Eden", "tokens": ["Ha", "!", "wer", "ge\u00b7lan\u00b7get", "in", "das", "E\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PWS", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Durch ein bequemes Ohngef\u00e4hr?", "tokens": ["Durch", "ein", "be\u00b7que\u00b7mes", "Ohn\u00b7ge\u00b7f\u00e4hr", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die gr\u00f6\u00dften M\u00e4nner, wie zu lesen,", "tokens": ["Die", "gr\u00f6\u00df\u00b7ten", "M\u00e4n\u00b7ner", ",", "wie", "zu", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die n\u00fctzlich ihrem Vaterland,", "tokens": ["Die", "n\u00fctz\u00b7lich", "ih\u00b7rem", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gelehrte, Feldherrn sind's gewesen,", "tokens": ["Ge\u00b7lehr\u00b7te", ",", "Feld\u00b7herrn", "sin\u00b7d's", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VAFIN", "VAPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie waren all' fr\u00fch bei der Hand.", "tokens": ["Sie", "wa\u00b7ren", "all'", "fr\u00fch", "bei", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Erinnern will ich blos an Solon,", "tokens": ["E\u00b7rin\u00b7nern", "will", "ich", "blos", "an", "So\u00b7lon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "APPR", "NE", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Glaubt Ihr, da\u00df er lang ", "tokens": ["Glaubt", "Ihr", ",", "da\u00df", "er", "lang"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Die Makkab\u00e4er, Christoph Kolon,", "tokens": ["Die", "Mak\u00b7ka\u00b7b\u00e4\u00b7er", ",", "Chris\u00b7toph", "Ko\u00b7lon", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NE", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.5": {"line.1": {"text": "Diocletian, der gro\u00dfe Kaiser,", "tokens": ["Dio\u00b7cle\u00b7ti\u00b7an", ",", "der", "gro\u00b7\u00dfe", "Kai\u00b7ser", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wu\u00dft' auch, warum er ", "tokens": ["Wu\u00dft'", "auch", ",", "wa\u00b7rum", "er"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "PWAV", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und Wilhelm Tell, der k\u00fchne Schweizer,", "tokens": ["Und", "Wil\u00b7helm", "Tell", ",", "der", "k\u00fch\u00b7ne", "Schwei\u00b7zer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dacht': Morgenstund hat Gold im Mund!", "tokens": ["Dacht'", ":", "Mor\u00b7gen\u00b7stund", "hat", "Gold", "im", "Mund", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "NN", "VAFIN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So hab' auch ", "tokens": ["So", "hab'", "auch"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Zu Jenen Nichts \u2013 erfahren sp\u00e4t,", "tokens": ["Zu", "Je\u00b7nen", "Nichts", "\u2013", "er\u00b7fah\u00b7ren", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$(", "VVINF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df in dem menschlichen Bereiche", "tokens": ["Da\u00df", "in", "dem", "menschli\u00b7chen", "Be\u00b7rei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Da\u00df mit den H\u00fchnern aufgestanden,", "tokens": ["Da\u00df", "mit", "den", "H\u00fch\u00b7nern", "auf\u00b7ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich noch vor Abend selbst belohnt,", "tokens": ["Sich", "noch", "vor", "A\u00b7bend", "selbst", "be\u00b7lohnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und da\u00df sich Alle wohlbefanden,", "tokens": ["Und", "da\u00df", "sich", "Al\u00b7le", "wohl\u00b7be\u00b7fan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sobald sie's ", "tokens": ["So\u00b7bald", "sie's"], "token_info": ["word", "word"], "pos": ["KOUS", "NE"], "meter": "-+-", "measure": "amphibrach.single"}}}}}