{"textgrid.poem.54793": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "7. Uber seinen Traum von Gl\u00fcckseligkeit der Seeligen", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Di\u00df Leben zeigte dir/ vil/ doch verg\u00e4nglich Gutt/", "tokens": ["Di\u00df", "Le\u00b7ben", "zeig\u00b7te", "dir", "/", "vil", "/", "doch", "ver\u00b7g\u00e4ng\u00b7lich", "Gutt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "PPER", "$(", "ADV", "$(", "ADV", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Traum hergegen wi\u00df das Lusthau\u00df steter Ehren.", "tokens": ["Ein", "Traum", "her\u00b7ge\u00b7gen", "wi\u00df", "das", "Lust\u00b7hau\u00df", "ste\u00b7ter", "Eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du nimst was ewig an/ und setzest aus dem Muth/", "tokens": ["Du", "nimst", "was", "e\u00b7wig", "an", "/", "und", "set\u00b7zest", "aus", "dem", "Muth", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "ADJD", "PTKVZ", "$(", "KON", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Welt und was mit ihr mu\u00df unversehns auffh\u00f6ren.", "tokens": ["Die", "Welt", "und", "was", "mit", "ihr", "mu\u00df", "un\u00b7ver\u00b7sehns", "auff\u00b7h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PWS", "APPR", "PPER", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sch\u00e4tzt man di\u00df Leben hoch/ das schlechter als ein Traum/", "tokens": ["Sch\u00e4tzt", "man", "di\u00df", "Le\u00b7ben", "hoch", "/", "das", "schlech\u00b7ter", "als", "ein", "Traum", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PDS", "NN", "ADJD", "$(", "ART", "ADJA", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dein Traum war wahre Freud dein Leben Dunst und Schaum.", "tokens": ["Dein", "Traum", "war", "wah\u00b7re", "Freud", "dein", "Le\u00b7ben", "Dunst", "und", "Schaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "NN", "PPOSAT", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}