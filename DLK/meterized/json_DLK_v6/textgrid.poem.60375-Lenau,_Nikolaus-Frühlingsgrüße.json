{"textgrid.poem.60375": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsgr\u00fc\u00dfe", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach langem Frost, wie weht die Luft so lind!", "tokens": ["Nach", "lan\u00b7gem", "Frost", ",", "wie", "weht", "die", "Luft", "so", "lind", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da bringt Fr\u00fchveilchen mir ein bettelnd Kind.", "tokens": ["Da", "bringt", "Fr\u00fch\u00b7veil\u00b7chen", "mir", "ein", "bet\u00b7telnd", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PPER", "ART", "VVPP", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Es ist betr\u00fcbt, da\u00df so den ersten Gru\u00df", "tokens": ["Es", "ist", "be\u00b7tr\u00fcbt", ",", "da\u00df", "so", "den", "ers\u00b7ten", "Gru\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Fr\u00fchlings mir das Elend bringen mu\u00df.", "tokens": ["Des", "Fr\u00fch\u00b7lings", "mir", "das", "E\u00b7lend", "brin\u00b7gen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und doch der sch\u00f6nen Tage liebes Pfand", "tokens": ["Und", "doch", "der", "sch\u00f6\u00b7nen", "Ta\u00b7ge", "lie\u00b7bes", "Pfand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist mir noch werter aus des Ungl\u00fccks Hand.", "tokens": ["Ist", "mir", "noch", "wer\u00b7ter", "aus", "des", "Un\u00b7gl\u00fccks", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So bringt dem Nachgeschlechte unser Leid", "tokens": ["So", "bringt", "dem", "Nach\u00b7ge\u00b7schlech\u00b7te", "un\u00b7ser", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Fr\u00fchlingsgr\u00fc\u00dfe einer bessern Zeit.", "tokens": ["Die", "Fr\u00fch\u00b7lings\u00b7gr\u00fc\u00b7\u00dfe", "ei\u00b7ner", "bes\u00b7sern", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}