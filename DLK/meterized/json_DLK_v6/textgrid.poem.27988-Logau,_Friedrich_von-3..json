{"textgrid.poem.27988": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man h\u00e4lt mir nicht f\u00fcr gut die Poesie zu \u00fcben;", "tokens": ["Man", "h\u00e4lt", "mir", "nicht", "f\u00fcr", "gut", "die", "Poe\u00b7sie", "zu", "\u00fc\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKNEG", "APPR", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das Buch, das grosse Buch, darinnen auffgeschrieben", "tokens": ["Das", "Buch", ",", "das", "gros\u00b7se", "Buch", ",", "da\u00b7rin\u00b7nen", "auff\u00b7ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der R\u00f6mer langes Recht, solt eher meine Hand", "tokens": ["Der", "R\u00f6\u00b7mer", "lan\u00b7ges", "Recht", ",", "solt", "e\u00b7her", "mei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "VMFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durchsuchen, da\u00df darauff sich gr\u00fcnde mein Verstand.", "tokens": ["Durch\u00b7su\u00b7chen", ",", "da\u00df", "dar\u00b7auff", "sich", "gr\u00fcn\u00b7de", "mein", "Ver\u00b7stand", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PAV", "PRF", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ists etwan ungesund, auff Speisen, die da n\u00e4hren,", "tokens": ["Ists", "et\u00b7wan", "un\u00b7ge\u00b7sund", ",", "auff", "Spei\u00b7sen", ",", "die", "da", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "$,", "APPR", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zu Zeiten frisches Obst erquicklich zu verzehren?", "tokens": ["Zu", "Zei\u00b7ten", "fri\u00b7sches", "Obst", "er\u00b7quick\u00b7lich", "zu", "ver\u00b7zeh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die edle Poesie ermuntert Sinn und Geist,", "tokens": ["Die", "ed\u00b7le", "Poe\u00b7sie", "er\u00b7mun\u00b7tert", "Sinn", "und", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Da\u00df er greifft an mit Lust, was schwer und wichtig heist.", "tokens": ["Da\u00df", "er", "greifft", "an", "mit", "Lust", ",", "was", "schwer", "und", "wich\u00b7tig", "heist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "APPR", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das n\u00f6thigst ist das Brot; doch l\u00e4st man gleichwohl gelten", "tokens": ["Das", "n\u00f6\u00b7thigst", "ist", "das", "Brot", ";", "doch", "l\u00e4st", "man", "gleich\u00b7wohl", "gel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "VAFIN", "ART", "NN", "$.", "ADV", "VVFIN", "PIS", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die weit gereiste W\u00fcrtz und sonsten, was da selten", "tokens": ["Die", "weit", "ge\u00b7reis\u00b7te", "W\u00fcrtz", "und", "sons\u00b7ten", ",", "was", "da", "sel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "KON", "VVFIN", "$,", "PRELS", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "In unsre Kuchel kummt; man g\u00fcnnet auch der Lust,", "tokens": ["In", "uns\u00b7re", "Ku\u00b7chel", "kummt", ";", "man", "g\u00fcn\u00b7net", "auch", "der", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$.", "PIS", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bedarff es nicht Natur, zu Zeiten eine Kost.", "tokens": ["Be\u00b7darff", "es", "nicht", "Na\u00b7tur", ",", "zu", "Zei\u00b7ten", "ei\u00b7ne", "Kost", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "NN", "$,", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der heilsame Verstand, da\u00df einer z\u00fcchtig lebe,", "tokens": ["Der", "heil\u00b7sa\u00b7me", "Ver\u00b7stand", ",", "da\u00df", "ei\u00b7ner", "z\u00fcch\u00b7tig", "le\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Niemanden Schaden thu und iedem gleiches gebe,", "tokens": ["Nie\u00b7man\u00b7den", "Scha\u00b7den", "thu", "und", "ie\u00b7dem", "glei\u00b7ches", "ge\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "KON", "PIAT", "PIS", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.15": {"text": "Ist n\u00f6thig, als wol was; doch steht es gleichwol frey,", "tokens": ["Ist", "n\u00f6\u00b7thig", ",", "als", "wol", "was", ";", "doch", "steht", "es", "gleich\u00b7wol", "frey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KOUS", "ADV", "PIS", "$.", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Zu salzen Kunst und Witz durch die Poeterey.", "tokens": ["Zu", "sal\u00b7zen", "Kunst", "und", "Witz", "durch", "die", "Poe\u00b7te\u00b7rey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "NN", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Weil Recht ein Knecht ietzt ist, dem Frevel hat zu schaffen,", "tokens": ["Weil", "Recht", "ein", "Knecht", "ietzt", "ist", ",", "dem", "Fre\u00b7vel", "hat", "zu", "schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "ADV", "VAFIN", "$,", "ART", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Weil eignen Willens Zaum pflegt frey verhenckt zu schlaffen,", "tokens": ["Weil", "eig\u00b7nen", "Wil\u00b7lens", "Zaum", "pflegt", "frey", "ver\u00b7henckt", "zu", "schlaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "NN", "VVFIN", "ADJD", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Weil Mars das Rothe stellt und auch das Schwartze setzt,", "tokens": ["Weil", "Mars", "das", "Ro\u00b7the", "stellt", "und", "auch", "das", "Schwart\u00b7ze", "setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVFIN", "KON", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Weil er Gesetz erkl\u00e4rt, wann er den Degen wetzt,", "tokens": ["Weil", "er", "Ge\u00b7setz", "er\u00b7kl\u00e4rt", ",", "wann", "er", "den", "De\u00b7gen", "wetzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVPP", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Dieweil er Urtheil f\u00e4llt, nach dem der Sieg gefallen,", "tokens": ["Die\u00b7weil", "er", "Ur\u00b7theil", "f\u00e4llt", ",", "nach", "dem", "der", "Sieg", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$,", "APPR", "ART", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Weil grober St\u00fccke Knall und holen Ertztes schallen", "tokens": ["Weil", "gro\u00b7ber", "St\u00fc\u00b7cke", "Knall", "und", "ho\u00b7len", "Ertz\u00b7tes", "schal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "NN", "KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Viel klagens nicht gestehn: So sey es mir vergunt,", "tokens": ["Viel", "kla\u00b7gens", "nicht", "ge\u00b7stehn", ":", "So", "sey", "es", "mir", "ver\u00b7gunt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "VVPP", "$.", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Auff da\u00df der Zeiten Weh, darinnen wenig Grund", "tokens": ["Auff", "da\u00df", "der", "Zei\u00b7ten", "Weh", ",", "da\u00b7rin\u00b7nen", "we\u00b7nig", "Grund"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "KOUS", "ART", "NN", "NN", "$,", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Zum from seyn \u00fcbrig ist, ich etwas mag bes\u00fcssen", "tokens": ["Zum", "from", "seyn", "\u00fcb\u00b7rig", "ist", ",", "ich", "et\u00b7was", "mag", "be\u00b7s\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJD", "VAINF", "ADJD", "VAFIN", "$,", "PPER", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Durch das, was ieder Zeit f\u00fcr ein ger\u00fchmtes wissen", "tokens": ["Durch", "das", ",", "was", "ie\u00b7der", "Zeit", "f\u00fcr", "ein", "ge\u00b7r\u00fchm\u00b7tes", "wis\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDS", "$,", "PRELS", "PIAT", "NN", "APPR", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Gesch\u00e4tzet war und wird. Man lasse mir die Lust,", "tokens": ["Ge\u00b7sch\u00e4t\u00b7zet", "war", "und", "wird", ".", "Man", "las\u00b7se", "mir", "die", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "KON", "VAFIN", "$.", "PIS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die, wo sie wenig bringt, noch weniger doch kost.", "tokens": ["Die", ",", "wo", "sie", "we\u00b7nig", "bringt", ",", "noch", "we\u00b7ni\u00b7ger", "doch", "kost", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Sie wird mir n\u00fctzer seyn, als M\u00e4gden zu gefallen,", "tokens": ["Sie", "wird", "mir", "n\u00fct\u00b7zer", "seyn", ",", "als", "M\u00e4g\u00b7den", "zu", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VAINF", "$,", "KOUS", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Als in der geilen Brunst der Uppigkeiten wallen,", "tokens": ["Als", "in", "der", "gei\u00b7len", "Brunst", "der", "Up\u00b7pig\u00b7kei\u00b7ten", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Als eingeschrieben seyn in frevlen Raube-Bund,", "tokens": ["Als", "ein\u00b7ge\u00b7schrie\u00b7ben", "seyn", "in", "frev\u00b7len", "Rau\u00b7be\u00b7Bund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "VAINF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Der durch gebrauchten Trotz der Welt hilfft auff den Grund,", "tokens": ["Der", "durch", "ge\u00b7brauch\u00b7ten", "Trotz", "der", "Welt", "hilfft", "auff", "den", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Als da\u00df mein Sinn im Wein, und Wein schw\u00fcmm in dem Sinne,", "tokens": ["Als", "da\u00df", "mein", "Sinn", "im", "Wein", ",", "und", "Wein", "schw\u00fcmm", "in", "dem", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPOSAT", "NN", "APPRART", "NN", "$,", "KON", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Als da\u00df der Spieler Dank, der schlecht ist, ich gew\u00fcnne,", "tokens": ["Als", "da\u00df", "der", "Spie\u00b7ler", "Dank", ",", "der", "schlecht", "ist", ",", "ich", "ge\u00b7w\u00fcn\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Als da\u00df ich mich befliess' auff Hunds-Philosophey", "tokens": ["Als", "da\u00df", "ich", "mich", "be\u00b7fliess'", "auff", "Hunds\u00b7Phi\u00b7lo\u00b7so\u00b7phey"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "PRF", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und trieb, als eine Kunst, ein b\u00e4urisch Feldgeschrey.", "tokens": ["Und", "trieb", ",", "als", "ei\u00b7ne", "Kunst", ",", "ein", "b\u00e4u\u00b7risch", "Feld\u00b7ge\u00b7schrey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN", "$,", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "So f\u00fchl ich auch nicht Hitz auff Hofegunst zu schnappen;", "tokens": ["So", "f\u00fchl", "ich", "auch", "nicht", "Hitz", "auff", "Ho\u00b7fe\u00b7gunst", "zu", "schnap\u00b7pen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ich biege keine Knie und r\u00fccke keine Kappen", "tokens": ["Ich", "bie\u00b7ge", "kei\u00b7ne", "Knie", "und", "r\u00fc\u00b7cke", "kei\u00b7ne", "Kap\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "KON", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "F\u00fcr auffgeputzter Ehr und angestrichner Gunst,", "tokens": ["F\u00fcr", "auff\u00b7ge\u00b7putz\u00b7ter", "Ehr", "und", "an\u00b7ge\u00b7strich\u00b7ner", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Die mancher sucht mit M\u00fch durch schn\u00f6de Schmeichel-Kunst.", "tokens": ["Die", "man\u00b7cher", "sucht", "mit", "M\u00fch", "durch", "schn\u00f6\u00b7de", "Schmei\u00b7chel\u00b7Kunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Genug, wann ich mir selbst im Friede kan befehlen", "tokens": ["Ge\u00b7nug", ",", "wann", "ich", "mir", "selbst", "im", "Frie\u00b7de", "kan", "be\u00b7feh\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PPER", "PPER", "ADV", "APPRART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und darff zu fremder Pflicht nicht Tag und Stunden zehlen.", "tokens": ["Und", "darff", "zu", "frem\u00b7der", "Pflicht", "nicht", "Tag", "und", "Stun\u00b7den", "zeh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "ADJA", "NN", "PTKNEG", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Ein K\u00f6nig bin ich so, mein Haus ein K\u00f6nigreich,", "tokens": ["Ein", "K\u00f6\u00b7nig", "bin", "ich", "so", ",", "mein", "Haus", "ein", "K\u00f6\u00b7nig\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Da weder Hold noch Gram mich roth macht oder bleich.", "tokens": ["Da", "we\u00b7der", "Hold", "noch", "Gram", "mich", "roth", "macht", "o\u00b7der", "bleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "NE", "ADV", "NN", "PPER", "ADJD", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Der Himmel, hat mir der vertraut und was gegeben,", "tokens": ["Der", "Him\u00b7mel", ",", "hat", "mir", "der", "ver\u00b7traut", "und", "was", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "PPER", "ART", "ADJD", "KON", "PWS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "So geb ich dieses dem, der bey mir wohnt daneben;", "tokens": ["So", "geb", "ich", "die\u00b7ses", "dem", ",", "der", "bey", "mir", "wohnt", "da\u00b7ne\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "ART", "$,", "PRELS", "APPR", "PPER", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Ich diene, wem ich kan, bin eines ieden Knecht,", "tokens": ["Ich", "die\u00b7ne", ",", "wem", "ich", "kan", ",", "bin", "ei\u00b7nes", "ie\u00b7den", "Knecht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$,", "VAFIN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Doch da\u00df mir \u00fcber mich bleibt unverr\u00fcckt mein Recht.", "tokens": ["Doch", "da\u00df", "mir", "\u00fc\u00b7ber", "mich", "bleibt", "un\u00b7ver\u00b7r\u00fcckt", "mein", "Recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Hierzwischen la\u00df ich nun zur Zeit mit unterlauffen", "tokens": ["Hier\u00b7zwi\u00b7schen", "la\u00df", "ich", "nun", "zur", "Zeit", "mit", "un\u00b7ter\u00b7lauf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "APPR", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Die viel-gef\u00fcsten Reim und f\u00fchre sie zu Hauffen", "tokens": ["Die", "viel\u00b7ge\u00b7f\u00fcs\u00b7ten", "Reim", "und", "f\u00fch\u00b7re", "sie", "zu", "Hauf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "F\u00fcr gute Freunde hin; gefallen sie, Gar wol!", "tokens": ["F\u00fcr", "gu\u00b7te", "Freun\u00b7de", "hin", ";", "ge\u00b7fal\u00b7len", "sie", ",", "Gar", "wol", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$.", "VVPP", "PPER", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Wo nicht, was liegt mir dran? Es ist kein n\u00f6thig sol", "tokens": ["Wo", "nicht", ",", "was", "liegt", "mir", "dran", "?", "Es", "ist", "kein", "n\u00f6\u00b7thig", "sol"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PTKNEG", "$,", "PWS", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VAFIN", "PIAT", "ADJD", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Gef\u00e4llig allen seyn. Ein ieder mag es machen,", "tokens": ["Ge\u00b7f\u00e4l\u00b7lig", "al\u00b7len", "seyn", ".", "Ein", "ie\u00b7der", "mag", "es", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "VAINF", "$.", "ART", "PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Da\u00df \u00fcber seinem Thun die Engel selbsten lachen,", "tokens": ["Da\u00df", "\u00fc\u00b7ber", "sei\u00b7nem", "Thun", "die", "En\u00b7gel", "selbs\u00b7ten", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Und da\u00df die Wei\u00dfheit sich selbst drob verwundern kan;", "tokens": ["Und", "da\u00df", "die", "Wei\u00df\u00b7heit", "sich", "selbst", "drob", "ver\u00b7wun\u00b7dern", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PRF", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Der, dem ich wo nicht taug, der seh mich nur nicht an.", "tokens": ["Der", ",", "dem", "ich", "wo", "nicht", "taug", ",", "der", "seh", "mich", "nur", "nicht", "an", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "PPER", "PWAV", "PTKNEG", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}