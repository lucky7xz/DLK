{"dta.poem.2783": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das zehende Capitel Hiobs.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich habe nun Verdru\u00df auf dieser Welt zu leben/", "tokens": ["Ich", "ha\u00b7be", "nun", "Ver\u00b7dru\u00df", "auf", "die\u00b7ser", "Welt", "zu", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "APPR", "PDAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und meine Seele \u00e4chz\u2019t/ da\u00df ich so leben mu\u00df.", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "\u00e4ch\u00b7z't", "/", "da\u00df", "ich", "so", "le\u00b7ben", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$(", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-++-+-+-+", "measure": "unknown.measure.septa"}, "line.3": {"text": "Es rinnt aus meinem Aug ein steter Thr\u00e4nen-Flu\u00df.", "tokens": ["Es", "rinnt", "aus", "mei\u00b7nem", "Aug", "ein", "ste\u00b7ter", "Thr\u00e4\u00b7nen\u00b7Flu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich mu\u00df in Angst-Geschrey jetzt meine Klag\u2019 erheben/", "tokens": ["Ich", "mu\u00df", "in", "Angst\u00b7Ge\u00b7schrey", "jetzt", "mei\u00b7ne", "Klag'", "er\u00b7he\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Noth ist gar zu gro\u00df die meine Seele plagt/", "tokens": ["Die", "Noth", "ist", "gar", "zu", "gro\u00df", "die", "mei\u00b7ne", "See\u00b7le", "plagt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wohlan/ es sey doch GOtt mein Hertzenleyd geklagt.", "tokens": ["Wo\u00b7hlan", "/", "es", "sey", "doch", "Gott", "mein", "Hert\u00b7zen\u00b7leyd", "ge\u00b7klagt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PPER", "VAFIN", "ADV", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Verdamme mich nicht bald und geh nicht ins Gerichte", "tokens": ["Ver\u00b7dam\u00b7me", "mich", "nicht", "bald", "und", "geh", "nicht", "ins", "Ge\u00b7rich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "KON", "VVFIN", "PTKNEG", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit mir Unwissenden. Erzehle was es sey?", "tokens": ["Mit", "mir", "Un\u00b7wis\u00b7sen\u00b7den", ".", "Er\u00b7zeh\u00b7le", "was", "es", "sey", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "$.", "NN", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Eh ich erscheinen soll/ und mach mich Zweiffels frey/", "tokens": ["Eh", "ich", "er\u00b7schei\u00b7nen", "soll", "/", "und", "mach", "mich", "Zweif\u00b7fels", "frey", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$(", "KON", "VVFIN", "PPER", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Warumb du mit mir zanckst/ da\u00df ich mich unterrichte.", "tokens": ["Wa\u00b7rumb", "du", "mit", "mir", "zanckst", "/", "da\u00df", "ich", "mich", "un\u00b7ter\u00b7rich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "VVFIN", "$(", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Gef\u00e4llts dir denn O GOtt! an mir Gewalt zu thun;", "tokens": ["Ge\u00b7f\u00e4llts", "dir", "denn", "O", "Gott", "!", "an", "mir", "Ge\u00b7walt", "zu", "thun", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "NE", "NN", "$.", "APPR", "PPER", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und kanst du keine Zeit und Viertelstunde ruhn?", "tokens": ["Und", "kanst", "du", "kei\u00b7ne", "Zeit", "und", "Vier\u00b7tel\u00b7stun\u00b7de", "ruhn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PIAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Verwirffst du diesen auch den deine Hand erbauet/", "tokens": ["Ver\u00b7wirffst", "du", "die\u00b7sen", "auch", "den", "dei\u00b7ne", "Hand", "er\u00b7bau\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "ADV", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und st\u00f6st der Meister selbst sein Meisterst\u00fcck jetzt ein?", "tokens": ["Und", "st\u00f6st", "der", "Meis\u00b7ter", "selbst", "sein", "Meis\u00b7ter\u00b7st\u00fcck", "jetzt", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "PPOSAT", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Mu\u00df der Gottlosen Rath durch dich geehret seyn;", "tokens": ["Mu\u00df", "der", "Gott\u00b7lo\u00b7sen", "Rath", "durch", "dich", "ge\u00b7eh\u00b7ret", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "NN", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da man mich ausgethan von deinem Antlitz schauet?", "tokens": ["Da", "man", "mich", "aus\u00b7ge\u00b7than", "von", "dei\u00b7nem", "Ant\u00b7litz", "schau\u00b7et", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVPP", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Bist du denn fleischlich auch/ wie Menschen sind/ gesinnet?", "tokens": ["Bist", "du", "denn", "fleischlich", "auch", "/", "wie", "Men\u00b7schen", "sind", "/", "ge\u00b7sin\u00b7net", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "ADV", "$(", "KOKOM", "NN", "VAFIN", "$(", "VVPP", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.18": {"text": "Und siehst du so wie sie/ das an/ was man beginnet?", "tokens": ["Und", "siehst", "du", "so", "wie", "sie", "/", "das", "an", "/", "was", "man", "be\u00b7gin\u00b7net", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "KOKOM", "PPER", "$(", "PDS", "PTKVZ", "$(", "PWS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Vergleicht denn deine Zeit sich mit der Menschen Zeiten?", "tokens": ["Ver\u00b7gleicht", "denn", "dei\u00b7ne", "Zeit", "sich", "mit", "der", "Men\u00b7schen", "Zei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PPOSAT", "NN", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ist deiner Jahre Raum als Manns-Zeit eingefast/", "tokens": ["Ist", "dei\u00b7ner", "Jah\u00b7re", "Raum", "als", "Manns\u00b7Zeit", "ein\u00b7ge\u00b7fast", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "KOUS", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Da\u00df du nach Missethat so eine Frage hast/", "tokens": ["Da\u00df", "du", "nach", "Mis\u00b7se\u00b7that", "so", "ei\u00b7ne", "Fra\u00b7ge", "hast", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADV", "ART", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und r\u00fcgest meine S\u00fcnd und Schuld und Eitelkeiten?", "tokens": ["Und", "r\u00fc\u00b7gest", "mei\u00b7ne", "S\u00fcnd", "und", "Schuld", "und", "Ei\u00b7tel\u00b7kei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Es ist dir zwar bewust da\u00df ich nicht gottlo\u00df bin.", "tokens": ["Es", "ist", "dir", "zwar", "be\u00b7wust", "da\u00df", "ich", "nicht", "gott\u00b7lo\u00df", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVFIN", "KOUS", "PPER", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Doch kan sich deiner Hand durchaus niemand entziehn.", "tokens": ["Doch", "kan", "sich", "dei\u00b7ner", "Hand", "durc\u00b7haus", "nie\u00b7mand", "ent\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "PPOSAT", "NN", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Du hast mich selbst zu erst gemacht mit deinen Fingern;", "tokens": ["Du", "hast", "mich", "selbst", "zu", "erst", "ge\u00b7macht", "mit", "dei\u00b7nen", "Fin\u00b7gern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "ADV", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und was sich umb und umb an diesem Leibe regt/", "tokens": ["Und", "was", "sich", "umb", "und", "umb", "an", "die\u00b7sem", "Lei\u00b7be", "regt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "APPR", "KON", "APPR", "APPR", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Das hat mir deine Hand/ mein Sch\u00f6pffer/ angelegt;", "tokens": ["Das", "hat", "mir", "dei\u00b7ne", "Hand", "/", "mein", "Sch\u00f6pf\u00b7fer", "/", "an\u00b7ge\u00b7legt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und nun versenckst du mich/ und willst mich gar vorringern.", "tokens": ["Und", "nun", "ver\u00b7senckst", "du", "mich", "/", "und", "willst", "mich", "gar", "vor\u00b7rin\u00b7gern", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "$(", "KON", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "Gedencke/ da\u00df ich nur aus L\u00e4imen bin gemacht/", "tokens": ["Ge\u00b7den\u00b7cke", "/", "da\u00df", "ich", "nur", "aus", "L\u00e4i\u00b7men", "bin", "ge\u00b7macht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "ADV", "APPR", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und da\u00df ich wieder werd in L\u00e4im und Erde bracht.", "tokens": ["Und", "da\u00df", "ich", "wie\u00b7der", "werd", "in", "L\u00e4im", "und", "Er\u00b7de", "bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VAFIN", "APPR", "NE", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Hast du mich nicht wie Milch und fetten Rahm gemolcken?", "tokens": ["Hast", "du", "mich", "nicht", "wie", "Milch", "und", "fet\u00b7ten", "Rahm", "ge\u00b7mol\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "PTKNEG", "KOKOM", "NN", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Gerann ich nicht darnach den weichen K\u00e4sen gleich?", "tokens": ["Ge\u00b7rann", "ich", "nicht", "dar\u00b7nach", "den", "wei\u00b7chen", "K\u00e4\u00b7sen", "gleich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKNEG", "PAV", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und eh ich Armer noch kam auf der Erden Reich/", "tokens": ["Und", "eh", "ich", "Ar\u00b7mer", "noch", "kam", "auf", "der", "Er\u00b7den", "Reich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "ADV", "VVFIN", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So gabst du mir ein Kleid das sch\u00f6ner als die Wolcken.", "tokens": ["So", "gabst", "du", "mir", "ein", "Kleid", "das", "sch\u00f6\u00b7ner", "als", "die", "Wol\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "ART", "ADJA", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Von frisch gesundem Fleisch und einer zarten Haut", "tokens": ["Von", "frisch", "ge\u00b7sun\u00b7dem", "Fleisch", "und", "ei\u00b7ner", "zar\u00b7ten", "Haut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Hat meine Mutter mich nach ihrer Angst geschaut.", "tokens": ["Hat", "mei\u00b7ne", "Mut\u00b7ter", "mich", "nach", "ih\u00b7rer", "Angst", "ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Du f\u00fcgtest Bein an Bein mit Adern wohl zusammen/", "tokens": ["Du", "f\u00fcg\u00b7test", "Bein", "an", "Bein", "mit", "A\u00b7dern", "wohl", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "APPR", "NN", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Das Leben gabst du mir und Wohlthat auch darzu.", "tokens": ["Das", "Le\u00b7ben", "gabst", "du", "mir", "und", "Wohlt\u00b7hat", "auch", "dar\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "KON", "NN", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Von deinem Auffsehn hat mein Athem seine Ruh.", "tokens": ["Von", "dei\u00b7nem", "Auff\u00b7sehn", "hat", "mein", "A\u00b7them", "sei\u00b7ne", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wie stellst denn du dich jetzt als wollstu mich verdammen?", "tokens": ["Wie", "stellst", "denn", "du", "dich", "jetzt", "als", "woll\u00b7stu", "mich", "ver\u00b7dam\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "KON", "PPER", "PRF", "ADV", "KOUS", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Und obschon alles di\u00df dein Vater-Hertz verdeckt/", "tokens": ["Und", "ob\u00b7schon", "al\u00b7les", "di\u00df", "dein", "Va\u00b7ter\u00b7Hertz", "ver\u00b7deckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PDS", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Doch wei\u00df ich/ da\u00df es dich/ mein zu gedencken/ weckt.", "tokens": ["Doch", "wei\u00df", "ich", "/", "da\u00df", "es", "dich", "/", "mein", "zu", "ge\u00b7den\u00b7cken", "/", "weckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$(", "KOUS", "PPER", "PRF", "$(", "PPOSAT", "PTKZU", "VVINF", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Denn wenn ich s\u00fcndige/ so kanst du es bald mercken/", "tokens": ["Denn", "wenn", "ich", "s\u00fcn\u00b7di\u00b7ge", "/", "so", "kanst", "du", "es", "bald", "mer\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$(", "ADV", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Und meine Missethat bleibt niemahls ohne Pein.", "tokens": ["Und", "mei\u00b7ne", "Mis\u00b7se\u00b7that", "bleibt", "nie\u00b7mahls", "oh\u00b7ne", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Es mu\u00df mir Angst und Weh bey meinen Lastern seyn.", "tokens": ["Es", "mu\u00df", "mir", "Angst", "und", "Weh", "bey", "mei\u00b7nen", "Las\u00b7tern", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Und bin ich gleich gerecht in Worten und in Wercken/", "tokens": ["Und", "bin", "ich", "gleich", "ge\u00b7recht", "in", "Wor\u00b7ten", "und", "in", "Wer\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADJD", "APPR", "NN", "KON", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "So darff ich doch mein Haupt nicht richten auff empor/", "tokens": ["So", "darff", "ich", "doch", "mein", "Haupt", "nicht", "rich\u00b7ten", "auff", "em\u00b7por", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKNEG", "VVFIN", "APPR", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Weil ich voll Elend bin/ und mein Schmertz gehet vor.", "tokens": ["Weil", "ich", "voll", "E\u00b7lend", "bin", "/", "und", "mein", "Schmertz", "ge\u00b7het", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "NN", "VAFIN", "$(", "KON", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Du aber richt\u2019st dich auff/ wie auffgeweckte L\u00f6wen/", "tokens": ["Du", "a\u00b7ber", "richt'st", "dich", "auff", "/", "wie", "auff\u00b7ge\u00b7weck\u00b7te", "L\u00f6\u00b7wen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "APPR", "$(", "KOKOM", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Der ein ohnm\u00e4chtig Lamm in seinen Rachen schlingt.", "tokens": ["Der", "ein", "ohn\u00b7m\u00e4ch\u00b7tig", "Lamm", "in", "sei\u00b7nen", "Ra\u00b7chen", "schlingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJD", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Wie grausam handelst du? Wie hast du mich umbringt;", "tokens": ["Wie", "grau\u00b7sam", "han\u00b7delst", "du", "?", "Wie", "hast", "du", "mich", "um\u00b7bringt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "$.", "PWAV", "VAFIN", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Und h\u00f6rest noch nicht auff mich ernstlich zu bedr\u00e4uen!", "tokens": ["Und", "h\u00f6\u00b7rest", "noch", "nicht", "auff", "mich", "ernst\u00b7lich", "zu", "be\u00b7dr\u00e4u\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "APPR", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Du stellest Zeugen vor/ die wissen gar zu viel;", "tokens": ["Du", "stel\u00b7lest", "Zeu\u00b7gen", "vor", "/", "die", "wis\u00b7sen", "gar", "zu", "viel", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$(", "ART", "VVFIN", "ADV", "PTKA", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Dein Zorn der h\u00e4uffet sich. Ich bin der Menschen Spiel.", "tokens": ["Dein", "Zorn", "der", "h\u00e4uf\u00b7fet", "sich", ".", "Ich", "bin", "der", "Men\u00b7schen", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VVFIN", "PRF", "$.", "PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Es plaget eines nach dem andern mich mit Hauffen.", "tokens": ["Es", "pla\u00b7get", "ei\u00b7nes", "nach", "dem", "an\u00b7dern", "mich", "mit", "Hauf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "APPR", "ART", "ADJA", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Ach warumb must ich doch aus Mutterleibe gehn!", "tokens": ["Ach", "wa\u00b7rumb", "must", "ich", "doch", "aus", "Mut\u00b7ter\u00b7lei\u00b7be", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWAV", "VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Ach warumb blieb ich nicht in der Geburts-Statt stehn;", "tokens": ["Ach", "wa\u00b7rumb", "blieb", "ich", "nicht", "in", "der", "Ge\u00b7burts\u00b7Statt", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWAV", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Ob ich im Schlamme gleich und Unflat m\u00fcst ersauffen?", "tokens": ["Ob", "ich", "im", "Schlam\u00b7me", "gleich", "und", "Un\u00b7flat", "m\u00fcst", "er\u00b7sauf\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADV", "KON", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "O da\u00df kein Auge mich auf dieser Welt gesehn!", "tokens": ["O", "da\u00df", "kein", "Au\u00b7ge", "mich", "auf", "die\u00b7ser", "Welt", "ge\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PIAT", "NN", "PPER", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "So w\u00e4re mir ja wohl und mehr als wohl geschehn.", "tokens": ["So", "w\u00e4\u00b7re", "mir", "ja", "wohl", "und", "mehr", "als", "wohl", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "KON", "PIAT", "KOKOM", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Ich w\u00e4r\u2019 al\u00dfdenn gewest als die so nie gewesen;", "tokens": ["Ich", "w\u00e4r'", "al\u00df\u00b7denn", "ge\u00b7west", "als", "die", "so", "nie", "ge\u00b7we\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KOKOM", "ART", "ADV", "ADV", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Von Mutterleibe bald zur Grabes St\u00e4tt gebracht/", "tokens": ["Von", "Mut\u00b7ter\u00b7lei\u00b7be", "bald", "zur", "Gra\u00b7bes", "St\u00e4tt", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "APPRART", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Wird denn kein Ende nicht der schweren Zeit gemacht?", "tokens": ["Wird", "denn", "kein", "En\u00b7de", "nicht", "der", "schwe\u00b7ren", "Zeit", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "PTKNEG", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Mu\u00df ich noch vor dem Tod in Traurigkeit verwesen?", "tokens": ["Mu\u00df", "ich", "noch", "vor", "dem", "Tod", "in", "Trau\u00b7rig\u00b7keit", "ver\u00b7we\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "L\u00e4st denn der grimme Schmertz ein wenig nicht von mir?", "tokens": ["L\u00e4st", "denn", "der", "grim\u00b7me", "Schmertz", "ein", "we\u00b7nig", "nicht", "von", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "ART", "PIS", "PTKNEG", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Da\u00df ich ein wenig nur erquicket w\u00fcrde hier;", "tokens": ["Da\u00df", "ich", "ein", "we\u00b7nig", "nur", "er\u00b7quic\u00b7ket", "w\u00fcr\u00b7de", "hier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "PIS", "ADV", "VVFIN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Eh denn ich gar vergeh und komme nicht mehr wieder.", "tokens": ["Eh", "denn", "ich", "gar", "ver\u00b7geh", "und", "kom\u00b7me", "nicht", "mehr", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "p