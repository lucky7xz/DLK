{"textgrid.poem.31296": {"metadata": {"author": {"name": "Ramler, Karl Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Konkordia! \u2013 durch dich rollt jede Sph\u00e4re;", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Konkordia! \u2013 durch dich rollt jede Sph\u00e4re;", "tokens": ["Kon\u00b7kor\u00b7dia", "!", "\u2013", "durch", "dich", "rollt", "je\u00b7de", "Sph\u00e4\u00b7re", ";"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "$(", "APPR", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und wo dein Fuss ein Land betrat,", "tokens": ["Und", "wo", "dein", "Fuss", "ein", "Land", "be\u00b7trat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da zeichneten volkreiche St\u00e4dte, T\u00e4nze, Ch\u00f6re", "tokens": ["Da", "zeich\u00b7ne\u00b7ten", "vol\u00b7krei\u00b7che", "St\u00e4d\u00b7te", ",", "T\u00e4n\u00b7ze", ",", "Ch\u00f6\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$,", "NN", "$,", "NN"], "meter": "-+--++-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der Jungfraun deinen Pfad:", "tokens": ["Der", "Jung\u00b7fraun", "dei\u00b7nen", "Pfad", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "(doch Drat und Beil tr\u00e4gt dir mit schnellem Schritte,", "tokens": ["(", "doch", "Drat", "und", "Beil", "tr\u00e4gt", "dir", "mit", "schnel\u00b7lem", "Schrit\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NN", "KON", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Blicke drohend, taub das Ohr,", "tokens": ["Die", "Bli\u00b7cke", "dro\u00b7hend", ",", "taub", "das", "Ohr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Br\u00fcder Blut, der Ehen Schmach, den Raub der H\u00fctte", "tokens": ["Der", "Br\u00fc\u00b7der", "Blut", ",", "der", "E\u00b7hen", "Schmach", ",", "den", "Raub", "der", "H\u00fct\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu r\u00e4chen, Ate vor:)", "tokens": ["Zu", "r\u00e4\u00b7chen", ",", "A\u00b7te", "vor", ":)"], "token_info": ["word", "word", "punct", "word", "word", "emoticon"], "pos": ["PTKZU", "VVINF", "$,", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zu dir erheben aus zerst\u00f6rten St\u00e4dten,", "tokens": ["Zu", "dir", "er\u00b7he\u00b7ben", "aus", "zer\u00b7st\u00f6r\u00b7ten", "St\u00e4d\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu dir auf Tr\u00fcmmern um den Strand,", "tokens": ["Zu", "dir", "auf", "Tr\u00fcm\u00b7mern", "um", "den", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu dir auf Saaten, die des Rosses Huf zertreten,", "tokens": ["Zu", "dir", "auf", "Saa\u00b7ten", ",", "die", "des", "Ros\u00b7ses", "Huf", "zer\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die V\u00f6lker, Mund und Hand;", "tokens": ["Die", "V\u00f6l\u00b7ker", ",", "Mund", "und", "Hand", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Zu dir die Pflanzstadt ungeborner S\u00f6hne,", "tokens": ["Zu", "dir", "die", "Pflanz\u00b7stadt", "un\u00b7ge\u00b7bor\u00b7ner", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die deiner milden K\u00fcnst' entbehrt: \u2013", "tokens": ["Die", "dei\u00b7ner", "mil\u00b7den", "K\u00fcnst'", "ent\u00b7behrt", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dass doch dein Geist den Zorn der K\u00f6nige vers\u00f6hne,", "tokens": ["Dass", "doch", "dein", "Geist", "den", "Zorn", "der", "K\u00f6\u00b7ni\u00b7ge", "ver\u00b7s\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der itzt die Welt verheert.", "tokens": ["Der", "itzt", "die", "Welt", "ver\u00b7heert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dir hat dein Freund, Teutoniens Erretter,", "tokens": ["Dir", "hat", "dein", "Freund", ",", "Teu\u00b7to\u00b7ni\u00b7ens", "Er\u00b7ret\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Held, der dreymal Frieden heischt,", "tokens": ["Der", "Held", ",", "der", "drey\u00b7mal", "Frie\u00b7den", "heischt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bevor sein schwerer Arm durch sieben Donnerwetter", "tokens": ["Be\u00b7vor", "sein", "schwe\u00b7rer", "Arm", "durch", "sie\u00b7ben", "Don\u00b7ner\u00b7wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der F\u00fcrsten Raubsucht t\u00e4uscht,", "tokens": ["Der", "F\u00fcrs\u00b7ten", "Raub\u00b7sucht", "t\u00e4uscht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Vereint mit Suecien durch deine Bande,", "tokens": ["Ver\u00b7eint", "mit", "Sue\u00b7ci\u00b7en", "durch", "dei\u00b7ne", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und mit Ruthenien vertraut,", "tokens": ["Und", "mit", "Ru\u00b7the\u00b7ni\u00b7en", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "ADJD", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nach langer Arbeit einen Tempel an dem Rande", "tokens": ["Nach", "lan\u00b7ger", "Ar\u00b7beit", "ei\u00b7nen", "Tem\u00b7pel", "an", "dem", "Ran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Des alten Belts erbaut.", "tokens": ["Des", "al\u00b7ten", "Belts", "er\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Schr\u00e4nkt sich Semiramis in ihre weiten", "tokens": ["Schr\u00e4nkt", "sich", "Se\u00b7mi\u00b7ra\u00b7mis", "in", "ih\u00b7re", "wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "NE", "APPR", "PPOSAT", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Fruchtreichen Dynastien ein:", "tokens": ["Fruch\u00b7trei\u00b7chen", "Dy\u00b7nas\u00b7ti\u00b7en", "ein", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So wird er mit entz\u00fcckter Seele dir den zweyten", "tokens": ["So", "wird", "er", "mit", "ent\u00b7z\u00fcck\u00b7ter", "See\u00b7le", "dir", "den", "zwey\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ADJA", "NN", "PPER", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf den Sudeten weihn.", "tokens": ["Auf", "den", "Su\u00b7de\u00b7ten", "weihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}