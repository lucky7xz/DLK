{"textgrid.poem.65568": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "41. Empfindlichkeit", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ziehe dem Himmel ein schiefes Gesicht,", "tokens": ["Zie\u00b7he", "dem", "Him\u00b7mel", "ein", "schie\u00b7fes", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Gott und die Engel bemerken's nicht.", "tokens": ["Gott", "und", "die", "En\u00b7gel", "be\u00b7mer\u00b7ken's", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "VVFIN", "PTKNEG", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Lach' einmal in die H\u00f6lle hinein,", "tokens": ["Lach'", "ein\u00b7mal", "in", "die", "H\u00f6l\u00b7le", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Was da wird f\u00fcr ein Zischen sein!", "tokens": ["Was", "da", "wird", "f\u00fcr", "ein", "Zi\u00b7schen", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}