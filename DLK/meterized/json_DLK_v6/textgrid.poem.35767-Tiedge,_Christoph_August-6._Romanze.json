{"textgrid.poem.35767": {"metadata": {"author": {"name": "Tiedge, Christoph August", "birth": "N.A.", "death": "N.A."}, "title": "6. Romanze", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf dem Berge dort oben, da wehet der Wind,", "tokens": ["Auf", "dem", "Ber\u00b7ge", "dort", "o\u00b7ben", ",", "da", "we\u00b7het", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADV", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Da sitzet Mariechen, und wieget ihr Kind;", "tokens": ["Da", "sit\u00b7zet", "Ma\u00b7rie\u00b7chen", ",", "und", "wie\u00b7get", "ihr", "Kind", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Sie wiegt es mit ihrer schneewei\u00dfen Hand,", "tokens": ["Sie", "wiegt", "es", "mit", "ih\u00b7rer", "schnee\u00b7wei\u00b7\u00dfen", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Den Blick in die Ferne hinaus gewandt.", "tokens": ["Den", "Blick", "in", "die", "Fer\u00b7ne", "hin\u00b7aus", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APZR", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "In die Ferne hin\u00fcber schweift all ihr Sinn;", "tokens": ["In", "die", "Fer\u00b7ne", "hin\u00b7\u00fc\u00b7ber", "schweift", "all", "ihr", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "PIAT", "PPOSAT", "NN", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ihr Lieber, ihr Treuer, der ging dahin!", "tokens": ["Ihr", "Lie\u00b7ber", ",", "ihr", "Treu\u00b7er", ",", "der", "ging", "da\u00b7hin", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "PAV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Sonst ging er, sonst kam er, nun kommt er nicht mehr!", "tokens": ["Sonst", "ging", "er", ",", "sonst", "kam", "er", ",", "nun", "kommt", "er", "nicht", "mehr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Nun ist's um Mariechen so tot und so leer!", "tokens": ["Nun", "ist's", "um", "Ma\u00b7rie\u00b7chen", "so", "tot", "und", "so", "leer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "In den Busen, da fallen die Thr\u00e4nen hinein,", "tokens": ["In", "den", "Bu\u00b7sen", ",", "da", "fal\u00b7len", "die", "Thr\u00e4\u00b7nen", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Da trinkt ihr Kindlein sie saugend mit ein;", "tokens": ["Da", "trinkt", "ihr", "Kin\u00b7dlein", "sie", "sau\u00b7gend", "mit", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "ADJD", "APPR", "ART", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es schmeichelt der Mutter die kindliche Hand,", "tokens": ["Es", "schmei\u00b7chelt", "der", "Mut\u00b7ter", "die", "kind\u00b7li\u00b7che", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ihr Blick ist hinaus in die Ferne gewandt.", "tokens": ["Ihr", "Blick", "ist", "hin\u00b7aus", "in", "die", "Fer\u00b7ne", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Ach, wie sausend wehet der Wind und kalt!", "tokens": ["Ach", ",", "wie", "sau\u00b7send", "we\u00b7het", "der", "Wind", "und", "kalt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADJD", "VVFIN", "ART", "NN", "KON", "ADJD", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Mariechen, dein Liebster ging aus in den Wald,", "tokens": ["Ma\u00b7rie\u00b7chen", ",", "dein", "Liebs\u00b7ter", "ging", "aus", "in", "den", "Wald", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "VVFIN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ihm reichten die tanzenden Elfen die Hand;", "tokens": ["Ihm", "reich\u00b7ten", "die", "tan\u00b7zen\u00b7den", "El\u00b7fen", "die", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Er folgte der lockenden Schar, und verschwand.", "tokens": ["Er", "folg\u00b7te", "der", "lo\u00b7cken\u00b7den", "Schar", ",", "und", "ver\u00b7schwand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Auf dem Berge dort oben, da wehet der Wind,", "tokens": ["Auf", "dem", "Ber\u00b7ge", "dort", "o\u00b7ben", ",", "da", "we\u00b7het", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADV", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Da sitzet Mariechen, und wieget ihr Kind,", "tokens": ["Da", "sit\u00b7zet", "Ma\u00b7rie\u00b7chen", ",", "und", "wie\u00b7get", "ihr", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und schaut in die Nacht hin, mit weinendem Blick.", "tokens": ["Und", "schaut", "in", "die", "Nacht", "hin", ",", "mit", "wei\u00b7nen\u00b7dem", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Dahin ging ihr Liebster, und kehrt nicht zur\u00fcck!", "tokens": ["Da\u00b7hin", "ging", "ihr", "Liebs\u00b7ter", ",", "und", "kehrt", "nicht", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}