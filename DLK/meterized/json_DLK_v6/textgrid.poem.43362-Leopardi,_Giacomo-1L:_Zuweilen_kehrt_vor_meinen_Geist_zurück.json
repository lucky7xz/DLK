{"textgrid.poem.43362": {"metadata": {"author": {"name": "Leopardi, Giacomo", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zuweilen kehrt vor meinen Geist zur\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zuweilen kehrt vor meinen Geist zur\u00fcck", "tokens": ["Zu\u00b7wei\u00b7len", "kehrt", "vor", "mei\u00b7nen", "Geist", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dein Bild, Aspasia. Mag es fl\u00fcchtig mir", "tokens": ["Dein", "Bild", ",", "As\u00b7pa\u00b7sia", ".", "Mag", "es", "fl\u00fcch\u00b7tig", "mir"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "NE", "$.", "VMFIN", "PPER", "ADJD", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vor\u00fcberblitzen im Gew\u00fchl der Stadt", "tokens": ["Vor\u00b7\u00fc\u00b7berb\u00b7lit\u00b7zen", "im", "Ge\u00b7w\u00fchl", "der", "Stadt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aus andern Z\u00fcgen; mag im \u00f6den Feld", "tokens": ["Aus", "an\u00b7dern", "Z\u00fc\u00b7gen", ";", "mag", "im", "\u00f6\u00b7den", "Feld"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "VMFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Am heitern Tag, im Glanz der stummen Sterne,", "tokens": ["Am", "hei\u00b7tern", "Tag", ",", "im", "Glanz", "der", "stum\u00b7men", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gleichsam erweckt von sanfter Harmonie,", "tokens": ["Gleich\u00b7sam", "er\u00b7weckt", "von", "sanf\u00b7ter", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Mir in der Seele, die noch leicht erschrickt,", "tokens": ["Mir", "in", "der", "See\u00b7le", ",", "die", "noch", "leicht", "er\u00b7schrickt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dies stolze Traumbild pl\u00f6tzlich auferstehn.", "tokens": ["Dies", "stol\u00b7ze", "Traum\u00b7bild", "pl\u00f6tz\u00b7lich", "auf\u00b7er\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wie angebetet einst, ihr G\u00f6tter, wie", "tokens": ["Wie", "an\u00b7ge\u00b7be\u00b7tet", "einst", ",", "ihr", "G\u00f6t\u00b7ter", ",", "wie"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWAV", "VVPP", "ADV", "$,", "PPOSAT", "NN", "$,", "PWAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Mir Wonn' und Fluch zugleich! Und nie umwehen", "tokens": ["Mir", "Wonn'", "und", "Fluch", "zu\u00b7gleich", "!", "Und", "nie", "um\u00b7we\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NN", "ADV", "$.", "KON", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Die D\u00fcfte mich von blumenreicher Flur,", "tokens": ["Die", "D\u00fcf\u00b7te", "mich", "von", "blu\u00b7men\u00b7rei\u00b7cher", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Noch aus den G\u00e4rten in der St\u00e4dte Mitten,", "tokens": ["Noch", "aus", "den", "G\u00e4r\u00b7ten", "in", "der", "St\u00e4d\u00b7te", "Mit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.13": {"text": "Da\u00df ich des Tags nicht denke, wo ich dich", "tokens": ["Da\u00df", "ich", "des", "Tags", "nicht", "den\u00b7ke", ",", "wo", "ich", "dich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$,", "PWAV", "PPER", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "In deinen lieblichen Gem\u00e4chern fand,", "tokens": ["In", "dei\u00b7nen", "lieb\u00b7li\u00b7chen", "Ge\u00b7m\u00e4\u00b7chern", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Durchduftet alle von den frischen Bl\u00fcten", "tokens": ["Durch\u00b7duf\u00b7tet", "al\u00b7le", "von", "den", "fri\u00b7schen", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Des Fr\u00fchlings, wo gekleidet in die Farbe", "tokens": ["Des", "Fr\u00fch\u00b7lings", ",", "wo", "ge\u00b7klei\u00b7det", "in", "die", "Far\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Des dunklen Veilchens deine himmlische", "tokens": ["Des", "dunk\u00b7len", "Veil\u00b7chens", "dei\u00b7ne", "himm\u00b7li\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Gestalt erschien, nachl\u00e4ssig hingeschmiegt", "tokens": ["Ge\u00b7stalt", "er\u00b7schien", ",", "nach\u00b7l\u00e4s\u00b7sig", "hin\u00b7ge\u00b7schmiegt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "ADJD", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Auf gl\u00e4nzende Polster, von geheimer Wollust", "tokens": ["Auf", "gl\u00e4n\u00b7zen\u00b7de", "Pols\u00b7ter", ",", "von", "ge\u00b7hei\u00b7mer", "Wol\u00b7lust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Rings \u00fcberhaucht; inde\u00df du, ausgelernte", "tokens": ["Rings", "\u00fc\u00b7ber\u00b7haucht", ";", "in\u00b7de\u00df", "du", ",", "aus\u00b7ge\u00b7lern\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "VVPP", "$.", "ADV", "PPER", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Verf\u00fchrerin, inbr\u00fcnstig gl\u00fch'nde K\u00fcsse", "tokens": ["Ver\u00b7f\u00fch\u00b7re\u00b7rin", ",", "in\u00b7br\u00fcns\u00b7tig", "gl\u00fch'n\u00b7de", "K\u00fcs\u00b7se"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Auf deiner Kinder sanftgeschwellte M\u00fcndchen", "tokens": ["Auf", "dei\u00b7ner", "Kin\u00b7der", "sanft\u00b7ge\u00b7schwell\u00b7te", "M\u00fcnd\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Laut schallend dr\u00fccktest, deinen schneeigen Nacken", "tokens": ["Laut", "schal\u00b7lend", "dr\u00fcck\u00b7test", ",", "dei\u00b7nen", "schne\u00b7e\u00b7i\u00b7gen", "Na\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJD", "VVFIN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Vorbiegend und die arglos junge Brut", "tokens": ["Vor\u00b7bie\u00b7gend", "und", "die", "arg\u00b7los", "jun\u00b7ge", "Brut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "An den verh\u00fcllten, ach, ersehnten Busen", "tokens": ["An", "den", "ver\u00b7h\u00fcll\u00b7ten", ",", "ach", ",", "er\u00b7sehn\u00b7ten", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ITJ", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Zogst mit der wundersch\u00f6nen Hand. Da schienen", "tokens": ["Zogst", "mit", "der", "wun\u00b7der\u00b7sch\u00f6\u00b7nen", "Hand", ".", "Da", "schie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Mir Erd' und Himmel neu, und fast ein Strahl", "tokens": ["Mir", "Erd'", "und", "Him\u00b7mel", "neu", ",", "und", "fast", "ein", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NN", "ADJD", "$,", "KON", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Der Gottheit gl\u00e4nzt' in mir. Da traf, beschwingt", "tokens": ["Der", "Got\u00b7theit", "gl\u00e4nzt'", "in", "mir", ".", "Da", "traf", ",", "be\u00b7schwingt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$.", "ADV", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Von deiner Hand, die Brust, die wohlbewehrt schien,", "tokens": ["Von", "dei\u00b7ner", "Hand", ",", "die", "Brust", ",", "die", "wohl\u00b7be\u00b7wehrt", "schien", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.30": {"text": "Mit Macht der Pfeil, den unentrei\u00dfbar fest", "tokens": ["Mit", "Macht", "der", "Pfeil", ",", "den", "un\u00b7en\u00b7trei\u00df\u00b7bar", "fest"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "$,", "PRELS", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Ich st\u00f6hnend trug, bis sich zum zweiten Mal", "tokens": ["Ich", "st\u00f6h\u00b7nend", "trug", ",", "bis", "sich", "zum", "zwei\u00b7ten", "Mal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "VVFIN", "$,", "KOUS", "PRF", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Im Lauf der Sonne j\u00e4hrte jener Tag.", "tokens": ["Im", "Lauf", "der", "Son\u00b7ne", "j\u00e4hr\u00b7te", "je\u00b7ner", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Strahl der Gottheit selbst erschien mir damals,", "tokens": ["Ein", "Strahl", "der", "Got\u00b7theit", "selbst", "er\u00b7schien", "mir", "da\u00b7mals", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weib, deine Sch\u00f6ne. Gleiche Zaubermacht", "tokens": ["Weib", ",", "dei\u00b7ne", "Sch\u00f6\u00b7ne", ".", "Glei\u00b7che", "Zau\u00b7ber\u00b7macht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "$.", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00dcbt Sch\u00f6nheit, wie Musik, die uns so oft", "tokens": ["\u00dcbt", "Sch\u00f6n\u00b7heit", ",", "wie", "Mu\u00b7sik", ",", "die", "uns", "so", "oft"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "$,", "PWAV", "NN", "$,", "PRELS", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von unbekannten Paradiesen hehres", "tokens": ["Von", "un\u00b7be\u00b7kann\u00b7ten", "Pa\u00b7ra\u00b7die\u00b7sen", "heh\u00b7res"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Geheimni\u00df zu enth\u00fcllen scheint. Dann h\u00e4tschelt", "tokens": ["Ge\u00b7heim\u00b7ni\u00df", "zu", "ent\u00b7h\u00fcl\u00b7len", "scheint", ".", "Dann", "h\u00e4t\u00b7schelt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PTKZU", "VVINF", "VVFIN", "$.", "ADV", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der tiefgetroffne Sterbliche das Kind", "tokens": ["Der", "tief\u00b7ge\u00b7troff\u00b7ne", "Sterb\u00b7li\u00b7che", "das", "Kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der eignen Seele, das geliebte Urbild,", "tokens": ["Der", "eig\u00b7nen", "See\u00b7le", ",", "das", "ge\u00b7lieb\u00b7te", "Ur\u00b7bild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den Inbegriff der ew'gen Himmelswonne,", "tokens": ["Den", "In\u00b7be\u00b7griff", "der", "ew'\u00b7gen", "Him\u00b7mels\u00b7won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ganz an Gesicht, Geberde, Stimm' und Rede", "tokens": ["Ganz", "an", "Ge\u00b7sicht", ",", "Ge\u00b7ber\u00b7de", ",", "Stimm'", "und", "Re\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Dem irdischen Weibe gleich, das zu ersehnen", "tokens": ["Dem", "ir\u00b7di\u00b7schen", "Wei\u00b7be", "gleich", ",", "das", "zu", "er\u00b7seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "PRELS", "PTKZU", "VVINF"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "In seinem Taumel w\u00e4hnt der Liebende.", "tokens": ["In", "sei\u00b7nem", "Tau\u00b7mel", "w\u00e4hnt", "der", "Lie\u00b7ben\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.12": {"text": "Und doch nicht dieses, jenes nur, das Urbild", "tokens": ["Und", "doch", "nicht", "die\u00b7ses", ",", "je\u00b7nes", "nur", ",", "das", "Ur\u00b7bild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "PTKNEG", "PDAT", "$,", "PDS", "ADV", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Liebt und ersehnt er selbst im Rausch der Sinne.", "tokens": ["Liebt", "und", "er\u00b7sehnt", "er", "selbst", "im", "Rausch", "der", "Sin\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Doch endlich wird er inne seines Wahns", "tokens": ["Doch", "end\u00b7lich", "wird", "er", "in\u00b7ne", "sei\u00b7nes", "Wahns"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und der Verwechslung, z\u00fcrnt dann und beschuldigt", "tokens": ["Und", "der", "Ver\u00b7wechs\u00b7lung", ",", "z\u00fcrnt", "dann", "und", "be\u00b7schul\u00b7digt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "VVFIN", "ADV", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Gar ungerecht das Weib. Es schwingt zur H\u00f6he", "tokens": ["Gar", "un\u00b7ge\u00b7recht", "das", "Weib", ".", "Es", "schwingt", "zur", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "punct", "word", "wo