{"textgrid.poem.46314": {"metadata": {"author": {"name": "Spee, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[einmahl hast mich gezogen]", "genre": "verse", "period": "N.A.", "pub_year": 1613, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einmahl hast mich gezogen,", "tokens": ["Ein\u00b7mahl", "hast", "mich", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O welt, in deine strick,", "tokens": ["O", "welt", ",", "in", "dei\u00b7ne", "strick", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Einmahl hast mich betrogen", "tokens": ["Ein\u00b7mahl", "hast", "mich", "be\u00b7tro\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In schnellem augenblick.", "tokens": ["In", "schnel\u00b7lem", "au\u00b7gen\u00b7blick", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bist warlich gar verlogen,", "tokens": ["Bist", "war\u00b7lich", "gar", "ver\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gibst gar zu schlechten lust:", "tokens": ["Gibst", "gar", "zu", "schlech\u00b7ten", "lust", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O wee, da\u00df ich gesogen", "tokens": ["O", "wee", ",", "da\u00df", "ich", "ge\u00b7so\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "KOUS", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Jemahl an deiner brust!", "tokens": ["Je\u00b7mahl", "an", "dei\u00b7ner", "brust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die frewd ist bald entflogen,", "tokens": ["Die", "frewd", "ist", "bald", "ent\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bald, bald f\u00e4hrt alles wegk;", "tokens": ["Bald", ",", "bald", "f\u00e4hrt", "al\u00b7les", "wegk", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer sich zur welt gebogen,", "tokens": ["Wer", "sich", "zur", "welt", "ge\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wird schnell zum alber geck.", "tokens": ["Wird", "schnell", "zum", "al\u00b7ber", "geck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPRART", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ey was hat mich betrogen,", "tokens": ["Ey", "was", "hat", "mich", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "O Fraw von Babylon?", "tokens": ["O", "Fraw", "von", "Ba\u00b7by\u00b7lon", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ie nach dir thet frogen,", "tokens": ["Da\u00df", "ie", "nach", "dir", "thet", "fro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPER", "VVFIN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weil nur trag leid darvon?", "tokens": ["Weil", "nur", "trag", "leid", "dar\u00b7von", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "ADJD", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dein kelch ist zwar gezogen", "tokens": ["Dein", "kelch", "ist", "zwar", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von lauter golt so rein,", "tokens": ["Von", "lau\u00b7ter", "golt", "so", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Stehn drin sch\u00f6n au\u00dfgebogen", "tokens": ["Stehn", "drin", "sch\u00f6n", "au\u00df\u00b7ge\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "ADJD", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Vill perl, vnd edel stein:", "tokens": ["Vill", "perl", ",", "vnd", "e\u00b7del", "stein", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Doch wers mit dir darff wogen,", "tokens": ["Doch", "wers", "mit", "dir", "darff", "wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vnd drinckt ein s\u00fcssen sau\u00df,", "tokens": ["Vnd", "drinckt", "ein", "s\u00fcs\u00b7sen", "sau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Find lauter h\u00f6llisch plogen,", "tokens": ["Find", "lau\u00b7ter", "h\u00f6l\u00b7lisch", "plo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die man thut sauffen drau\u00df.", "tokens": ["Die", "man", "thut", "sauf\u00b7fen", "drau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Drumb bin von dir gezogen,", "tokens": ["Drumb", "bin", "von", "dir", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ade zu guter nacht:", "tokens": ["A\u00b7de", "zu", "gu\u00b7ter", "nacht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Bin schon zu vil betrogen,", "tokens": ["Bin", "schon", "zu", "vil", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PIS", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ach hett ichs ba\u00df bedacht!", "tokens": ["Ach", "hett", "ichs", "ba\u00df", "be\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wan mir nicht so gewogen", "tokens": ["Wan", "mir", "nicht", "so", "ge\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PTKNEG", "ADV", "VVPP"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Mein Herr Gott w\u00e4r gewest,", "tokens": ["Mein", "Herr", "Gott", "w\u00e4r", "ge\u00b7west", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "W\u00e4r schon in kertz geflogen,", "tokens": ["W\u00e4r", "schon", "in", "kertz", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An schwerer s\u00fcnden pest.", "tokens": ["An", "schwe\u00b7rer", "s\u00fcn\u00b7den", "pest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "In Gottes hand schon logen", "tokens": ["In", "Got\u00b7tes", "hand", "schon", "lo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des todes pfeil bereit;", "tokens": ["Des", "to\u00b7des", "pfeil", "be\u00b7reit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ietz, ietz sprang ab der bogen.", "tokens": ["Ietz", ",", "ietz", "sprang", "ab", "der", "bo\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "APPR", "ART", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O, O, O Ewigkeit!", "tokens": ["O", ",", "O", ",", "O", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Da ward ich schnell entzogen,", "tokens": ["Da", "ward", "ich", "schnell", "ent\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schnell, schnell, zur ander seit;", "tokens": ["Schnell", ",", "schnell", ",", "zur", "an\u00b7der", "seit", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df mich nit traff der bogen,", "tokens": ["Da\u00df", "mich", "nit", "traff", "der", "bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch pfeil mir th\u00e4ten leid.", "tokens": ["Noch", "pfeil", "mir", "th\u00e4\u00b7ten", "leid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "O Gott, will dich nun loben,", "tokens": ["O", "Gott", ",", "will", "dich", "nun", "lo\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VMFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Loben dein g\u00fctigkeit;", "tokens": ["Lo\u00b7ben", "dein", "g\u00fc\u00b7tig\u00b7keit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ja loben, vnd noch loben,", "tokens": ["Ja", "lo\u00b7ben", ",", "vnd", "noch", "lo\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVINF", "$,", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Loben in ewigkeit.", "tokens": ["Lo\u00b7ben", "in", "e\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}