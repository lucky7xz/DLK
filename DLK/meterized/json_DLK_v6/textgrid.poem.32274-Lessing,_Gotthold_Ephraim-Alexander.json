{"textgrid.poem.32274": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Alexander", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Weise sprach zu Alexandern:", "tokens": ["Der", "Wei\u00b7se", "sprach", "zu", "A\u00b7lex\u00b7an\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbdort, wo die lichten Welten wandern,", "tokens": ["\u00bb", "dort", ",", "wo", "die", "lich\u00b7ten", "Wel\u00b7ten", "wan\u00b7dern", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ist manches Volk, ist manche Stadt.\u00ab", "tokens": ["Ist", "man\u00b7ches", "Volk", ",", "ist", "man\u00b7che", "Stadt", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$,", "VAFIN", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was tut der Mann von tausend Siegen?", "tokens": ["Was", "tut", "der", "Mann", "von", "tau\u00b7send", "Sie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Memme weint, da\u00df, dort zu kriegen,", "tokens": ["Die", "Mem\u00b7me", "weint", ",", "da\u00df", ",", "dort", "zu", "krie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Himmel keine Br\u00fccken hat.", "tokens": ["Der", "Him\u00b7mel", "kei\u00b7ne", "Br\u00fc\u00b7cken", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ists wahr, was ihn der Weise lehret,", "tokens": ["Ists", "wahr", ",", "was", "ihn", "der", "Wei\u00b7se", "leh\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und finden, was zur Welt geh\u00f6ret,", "tokens": ["Und", "fin\u00b7den", ",", "was", "zur", "Welt", "ge\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Daselbst auch Wein und M\u00e4dchen statt:", "tokens": ["Da\u00b7selbst", "auch", "Wein", "und", "M\u00e4d\u00b7chen", "statt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So lasset, Br\u00fcder, Tr\u00e4nen flie\u00dfen,", "tokens": ["So", "las\u00b7set", ",", "Br\u00fc\u00b7der", ",", "Tr\u00e4\u00b7nen", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "NN", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df, dort zu trinken und zu k\u00fcssen,", "tokens": ["Da\u00df", ",", "dort", "zu", "trin\u00b7ken", "und", "zu", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADV", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Himmel keine Br\u00fccken hat.", "tokens": ["Der", "Him\u00b7mel", "kei\u00b7ne", "Br\u00fc\u00b7cken", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}