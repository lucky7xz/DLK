{"dta.poem.23469": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein Kinderlied,  \n  zu singen wider die zween Erzfeinde Christi und  \n seiner heiligen Kirchen, den Papst und T\u00fcrken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erhalt uns, Herr, bei deinem Wort", "tokens": ["Er\u00b7halt", "uns", ",", "Herr", ",", "bei", "dei\u00b7nem", "Wort"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und steur des Papsts und T\u00fcrken Mord,", "tokens": ["Und", "steur", "des", "Papsts", "und", "T\u00fcr\u00b7ken", "Mord", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Jesum Christum deinen Sohn", "tokens": ["Die", "Je\u00b7sum", "Chris\u00b7tum", "dei\u00b7nen", "Sohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "NE", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wollen st\u00fcrzen von deinem Thron.", "tokens": ["Wol\u00b7len", "st\u00fcr\u00b7zen", "von", "dei\u00b7nem", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Beweis dein Macht, Herr Jesu Christ,", "tokens": ["Be\u00b7weis", "dein", "Macht", ",", "Herr", "Je\u00b7su", "Christ", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "NN", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df du Herr aller Herren bist!", "tokens": ["Da\u00df", "du", "Herr", "al\u00b7ler", "Her\u00b7ren", "bist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Beschirm dein arme Christenheit,", "tokens": ["Be\u00b7schirm", "dein", "ar\u00b7me", "Chris\u00b7ten\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df sie dich lob in Ewigkeit!", "tokens": ["Da\u00df", "sie", "dich", "lob", "in", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Gott heilger Geist, du Tr\u00f6ster werth,", "tokens": ["Gott", "heil\u00b7ger", "Geist", ",", "du", "Tr\u00f6s\u00b7ter", "werth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "PPER", "NN", "ADJD", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Gieb deim Volk einrlei Sinn auf Erd!", "tokens": ["Gieb", "deim", "Volk", "einr\u00b7lei", "Sinn", "auf", "Erd", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PIAT", "NN", "APPR", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Steh bei uns in der letzten Noth,", "tokens": ["Steh", "bei", "uns", "in", "der", "letz\u00b7ten", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "G'leit uns ins Leben aus dem Tod!", "tokens": ["G'\u00b7leit", "uns", "ins", "Le\u00b7ben", "aus", "dem", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}