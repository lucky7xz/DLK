{"dta.poem.19468": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Ii .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Nieder tr\u00e4gt der warme F\u00f6hn", "tokens": ["Nie\u00b7der", "tr\u00e4gt", "der", "war\u00b7me", "F\u00f6hn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Lawine fern Get\u00f6n,", "tokens": ["Der", "La\u00b7wi\u00b7ne", "fern", "Ge\u00b7t\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hinter jenen hohen F\u00f6hren", "tokens": ["Hin\u00b7ter", "je\u00b7nen", "ho\u00b7hen", "F\u00f6h\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann den dumpfen Schlag ich h\u00f6ren.", "tokens": ["Kann", "den", "dum\u00b7pfen", "Schlag", "ich", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In des Lenzes blauen Schein", "tokens": ["In", "des", "Len\u00b7zes", "blau\u00b7en", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der Scholle dunkelm Schrein", "tokens": ["Aus", "der", "Schol\u00b7le", "dun\u00b7kelm", "Schrein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dr\u00e4ngt und dr\u00fcckt das neue Leben,", "tokens": ["Dr\u00e4ngt", "und", "dr\u00fcckt", "das", "neu\u00b7e", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00fcftet Kleid und Decken eben \u2014", "tokens": ["L\u00fcf\u00b7tet", "Kleid", "und", "De\u00b7cken", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Von derselben Kraft und Lust", "tokens": ["Von", "der\u00b7sel\u00b7ben", "Kraft", "und", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4chst das Herz mir in der Brust,", "tokens": ["W\u00e4chst", "das", "Herz", "mir", "in", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heute kann es noch sich dehnen", "tokens": ["Heu\u00b7te", "kann", "es", "noch", "sich", "deh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit den Liedern, mit den Thr\u00e4nen!", "tokens": ["Mit", "den", "Lie\u00b7dern", ",", "mit", "den", "Thr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber blauen wird ein Tag,", "tokens": ["A\u00b7ber", "blau\u00b7en", "wird", "ein", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da sich's nicht mehr dehnen mag \u2014", "tokens": ["Da", "sich's", "nicht", "mehr", "deh\u00b7nen", "mag"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "ADV", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann kommt mich der Lenz zu t\u00f6dten", "tokens": ["Dann", "kommt", "mich", "der", "Lenz", "zu", "t\u00f6d\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit den Veilchen, mit den Fl\u00f6ten.", "tokens": ["Mit", "den", "Veil\u00b7chen", ",", "mit", "den", "Fl\u00f6\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}