{"textgrid.poem.44015": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Abermahl ein Theil vom Jahre,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Abermahl ein Theil vom Jahre,", "tokens": ["A\u00b7ber\u00b7mahl", "ein", "Theil", "vom", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Abermahl ein Tag vollbracht;", "tokens": ["A\u00b7ber\u00b7mahl", "ein", "Tag", "voll\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Abermahl ein Bret zu Baare", "tokens": ["A\u00b7ber\u00b7mahl", "ein", "Bret", "zu", "Baa\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ein Schritt zur Gruft gemacht,", "tokens": ["Und", "ein", "Schritt", "zur", "Gruft", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Also n\u00e4hert sich die Zeit", "tokens": ["Al\u00b7so", "n\u00e4\u00b7hert", "sich", "die", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Nach und nach der Ewigkeit,", "tokens": ["Nach", "und", "nach", "der", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Also m\u00fc\u00dfen wir auf Erden", "tokens": ["Al\u00b7so", "m\u00fc\u00b7\u00dfen", "wir", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zu dem Tode reifer werden.", "tokens": ["Zu", "dem", "To\u00b7de", "rei\u00b7fer", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Herr und Sch\u00f6pfer aller Dinge,", "tokens": ["Herr", "und", "Sch\u00f6p\u00b7fer", "al\u00b7ler", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der du mir den Tag verliehn,", "tokens": ["Der", "du", "mir", "den", "Tag", "ver\u00b7liehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6re, was ich thr\u00e4nend singe,", "tokens": ["H\u00f6\u00b7re", ",", "was", "ich", "thr\u00e4\u00b7nend", "sin\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Las mich w\u00fcrdig niederknien:", "tokens": ["Las", "mich", "w\u00fcr\u00b7dig", "nie\u00b7der\u00b7kni\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nimm das Abendopfer hin,", "tokens": ["Nimm", "das", "A\u00b7ben\u00b7dop\u00b7fer", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Das ich heute schuldig bin;", "tokens": ["Das", "ich", "heu\u00b7te", "schul\u00b7dig", "bin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn es sind nicht schlechte S\u00fcnden,", "tokens": ["Denn", "es", "sind", "nicht", "schlech\u00b7te", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Welche mich darzu verbinden.", "tokens": ["Wel\u00b7che", "mich", "dar\u00b7zu", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PAV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Treuer Vater, deine G\u00fcte", "tokens": ["Treu\u00b7er", "Va\u00b7ter", ",", "dei\u00b7ne", "G\u00fc\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hei\u00dfet \u00fcberschwenglich gro\u00df,", "tokens": ["Hei\u00b7\u00dfet", "\u00fc\u00b7bersc\u00b7hweng\u00b7lich", "gro\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drum erquicke mein Gem\u00fcthe,", "tokens": ["Drum", "er\u00b7qui\u00b7cke", "mein", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sprich mich ledig, frey und los.", "tokens": ["Sprich", "mich", "le\u00b7dig", ",", "frey", "und", "los", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "$,", "ADJD", "KON", "PTKVZ", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Gieb der Bu\u00dfe stets Geh\u00f6r,", "tokens": ["Gieb", "der", "Bu\u00b7\u00dfe", "stets", "Ge\u00b7h\u00f6r", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn dein Knecht verspricht nunmehr,", "tokens": ["Denn", "dein", "Knecht", "ver\u00b7spricht", "nun\u00b7mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dein Geseze, deinen Willen", "tokens": ["Dein", "Ge\u00b7se\u00b7ze", ",", "dei\u00b7nen", "Wil\u00b7len"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nach Verm\u00f6gen zu erf\u00fcllen.", "tokens": ["Nach", "Ver\u00b7m\u00f6\u00b7gen", "zu", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Das Verdienst der vielen Wunden,", "tokens": ["Das", "Ver\u00b7dienst", "der", "vie\u00b7len", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mein Heiland scharf gef\u00fchlt,", "tokens": ["Die", "mein", "Hei\u00b7land", "scharf", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat in seinen Todesstunden", "tokens": ["Hat", "in", "sei\u00b7nen", "To\u00b7dess\u00b7tun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Zornglut abgek\u00fchlt.", "tokens": ["Dei\u00b7ne", "Zorn\u00b7glut", "ab\u00b7ge\u00b7k\u00fchlt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schweig, wenn dieses L\u00f6segeld", "tokens": ["Schweig", ",", "wenn", "die\u00b7ses", "L\u00f6\u00b7se\u00b7geld"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meiner Schuld die Waage h\u00e4lt,", "tokens": ["Mei\u00b7ner", "Schuld", "die", "Waa\u00b7ge", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und beschicke mich im Schlafe", "tokens": ["Und", "be\u00b7schi\u00b7cke", "mich", "im", "Schla\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Durch kein Aufboth deiner Strafe.", "tokens": ["Durch", "kein", "Auf\u00b7both", "dei\u00b7ner", "Stra\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Las mich an der Brust erwarmen,", "tokens": ["Las", "mich", "an", "der", "Brust", "er\u00b7war\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die am Creuze nackend hing;", "tokens": ["Die", "am", "Creu\u00b7ze", "na\u00b7ckend", "hing", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wiege mich in de\u00dfen Armen,", "tokens": ["Wie\u00b7ge", "mich", "in", "de\u00b7\u00dfen", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Der den Sch\u00e4cher noch umfing;", "tokens": ["Der", "den", "Sch\u00e4\u00b7cher", "noch", "um\u00b7fing", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Stelle mir der Engel Chor", "tokens": ["Stel\u00b7le", "mir", "der", "En\u00b7gel", "Chor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Als die beste Schildwacht vor!", "tokens": ["Als", "die", "bes\u00b7te", "Schild\u00b7wacht", "vor", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Satan m\u00f6chte sonst ein Schr\u00f6cken", "tokens": ["Sa\u00b7tan", "m\u00f6ch\u00b7te", "sonst", "ein", "Schr\u00f6\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In der Finstern\u00fc\u00df erwecken.", "tokens": ["In", "der", "Fins\u00b7ter\u00b7n\u00fc\u00df", "er\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sch\u00fcze den, der meiner Liebe", "tokens": ["Sch\u00fc\u00b7ze", "den", ",", "der", "mei\u00b7ner", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An das Herz gebunden ist,", "tokens": ["An", "das", "Herz", "ge\u00b7bun\u00b7den", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df kein Fall sein Ohr betr\u00fcbe,", "tokens": ["Da\u00df", "kein", "Fall", "sein", "Ohr", "be\u00b7tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das vielleicht den Seiger mi\u00dft.", "tokens": ["Das", "viel\u00b7leicht", "den", "Sei\u00b7ger", "mi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "St\u00e4rck ihm den betr\u00fcbten Geist,", "tokens": ["St\u00e4rck", "ihm", "den", "be\u00b7tr\u00fcb\u00b7ten", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn er bittre Salsen speist,", "tokens": ["Wenn", "er", "bitt\u00b7re", "Sal\u00b7sen", "speist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und las noch in diesem Leben", "tokens": ["Und", "las", "noch", "in", "die\u00b7sem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PDAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Uns einander wiedergeben.", "tokens": ["Uns", "ein\u00b7an\u00b7der", "wie\u00b7der\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Trag das Alter meiner Eltern", "tokens": ["Trag", "das", "Al\u00b7ter", "mei\u00b7ner", "El\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf den Fl\u00fcgeln deiner Hut,", "tokens": ["Auf", "den", "Fl\u00fc\u00b7geln", "dei\u00b7ner", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tritt vor sie die Schwachheitskeltern;", "tokens": ["Tritt", "vor", "sie", "die", "Schwach\u00b7heits\u00b7kel\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mehre derer Hab und Gut,", "tokens": ["Meh\u00b7re", "de\u00b7rer", "Hab", "und", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PDS", "NN", "KON", "ADJD", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Die mir jemahls Guts gethan;", "tokens": ["Die", "mir", "je\u00b7mahls", "Guts", "ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nimm dich meiner Freundschaft an", "tokens": ["Nimm", "dich", "mei\u00b7ner", "Freund\u00b7schaft", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und verzeih den L\u00e4sterzungen,", "tokens": ["Und", "ver\u00b7zeih", "den", "L\u00e4s\u00b7ter\u00b7zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "\u00dcber die ich oft gesprungen.", "tokens": ["\u00dc\u00b7ber", "die", "ich", "oft", "ge\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Seegne die gerechten Wafen", "tokens": ["Seeg\u00b7ne", "die", "ge\u00b7rech\u00b7ten", "Wa\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner werthen Christenheit,", "tokens": ["Dei\u00b7ner", "wert\u00b7hen", "Chris\u00b7ten\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Uns den Frieden herzuschafen,", "tokens": ["Uns", "den", "Frie\u00b7den", "her\u00b7zu\u00b7scha\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den der Feind zu stehlen dr\u00e4ut;", "tokens": ["Den", "der", "Feind", "zu", "steh\u00b7len", "dr\u00e4ut", ";"], "token_info": ["word", 