{"dta.poem.5609": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom Ursprunge der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sollt solch ein Jrthum nun wol wahr seyn, und zu leiden?", "tokens": ["Sollt", "solch", "ein", "Jr\u00b7thum", "nun", "wol", "wahr", "seyn", ",", "und", "zu", "lei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "ART", "NN", "ADV", "ADV", "ADJD", "VAINF", "$,", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man mu\u00df ja das, was man beweget, unterscheiden", "tokens": ["Man", "mu\u00df", "ja", "das", ",", "was", "man", "be\u00b7we\u00b7get", ",", "un\u00b7ter\u00b7schei\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PIS", "VMFIN", "ADV", "PDS", "$,", "PRELS", "PIS", "VVFIN", "$,", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von dem, der es bewegt. Die Ordnung, Zierlichkeit,", "tokens": ["Von", "dem", ",", "der", "es", "be\u00b7wegt", ".", "Die", "Ord\u00b7nung", ",", "Zier\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PPER", "VVFIN", "$.", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erfordern einen allezeit.", "tokens": ["Er\u00b7for\u00b7dern", "ei\u00b7nen", "al\u00b7le\u00b7zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der sie gemacht. Kein Werck wird jemals angefangen,", "tokens": ["Der", "sie", "ge\u00b7macht", ".", "Kein", "Werck", "wird", "je\u00b7mals", "an\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$.", "PIAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df der, so es gew\u00fcrckt, ihm nicht sey vorgegangen.", "tokens": ["Da\u00df", "der", ",", "so", "es", "ge\u00b7w\u00fcrckt", ",", "ihm", "nicht", "sey", "vor\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "ADV", "PPER", "VVPP", "$,", "PPER", "PTKNEG", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und wenn solch Werck erscheint, ist es nicht offenbahr,", "tokens": ["Und", "wenn", "solch", "Werck", "er\u00b7scheint", ",", "ist", "es", "nicht", "of\u00b7fen\u00b7bahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIAT", "NN", "VVFIN", "$,", "VAFIN", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df es vom ersteren Befehl die Folge war?", "tokens": ["Da\u00df", "es", "vom", "ers\u00b7te\u00b7ren", "Be\u00b7fehl", "die", "Fol\u00b7ge", "war", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dies grosse Werck besteht im Allerh\u00f6chsten Geist.", "tokens": ["Dies", "gros\u00b7se", "Werck", "be\u00b7steht", "im", "Al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Denselben mu\u00df man ja vom Werckzeug unterscheiden.", "tokens": ["Den\u00b7sel\u00b7ben", "mu\u00df", "man", "ja", "vom", "Wer\u00b7ck\u00b7zeug", "un\u00b7ter\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIS", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Wir sehen, wie sich uns ein herrlich Schau-Werck weist.", "tokens": ["Wir", "se\u00b7hen", ",", "wie", "sich", "uns", "ein", "herr\u00b7lich", "Schau\u00b7\u00b7Werck", "weist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PRF", "PPER", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Auf! lasst uns was von Dem, der es verfertigt, wissen,", "tokens": ["Auf", "!", "lasst", "uns", "was", "von", "Dem", ",", "der", "es", "ver\u00b7fer\u00b7tigt", ",", "wis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "PPER", "PIS", "APPR", "PDS", "$,", "PRELS", "PPER", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Den wir, in selbigem mit vielen Freuden,", "tokens": ["Den", "wir", ",", "in", "sel\u00b7bi\u00b7gem", "mit", "vie\u00b7len", "Freu\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "APPR", "ADJA", "APPR", "PIAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.14": {"text": "Erkennen und bewundern m\u00fcssen.", "tokens": ["Er\u00b7ken\u00b7nen", "und", "be\u00b7wun\u00b7dern", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}