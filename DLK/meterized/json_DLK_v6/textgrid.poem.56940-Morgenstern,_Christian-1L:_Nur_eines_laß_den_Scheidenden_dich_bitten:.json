{"textgrid.poem.56940": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nur eines la\u00df den Scheidenden dich bitten:", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur eines la\u00df den Scheidenden dich bitten:", "tokens": ["Nur", "ei\u00b7nes", "la\u00df", "den", "Schei\u00b7den\u00b7den", "dich", "bit\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Tu ohne Reue, was du immer tust!", "tokens": ["Tu", "oh\u00b7ne", "Reu\u00b7e", ",", "was", "du", "im\u00b7mer", "tust", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$,", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich will, da\u00df du des nachts in Frieden ruhst, \u2013", "tokens": ["Ich", "will", ",", "da\u00df", "du", "des", "nachts", "in", "Frie\u00b7den", "ruhst", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PPER", "ART", "ADV", "APPR", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "sonst haben beide wir umsonst gelitten.", "tokens": ["sonst", "ha\u00b7ben", "bei\u00b7de", "wir", "um\u00b7sonst", "ge\u00b7lit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wars not, da\u00df du das Tafeltuch zerschnitten,", "tokens": ["Wars", "not", ",", "da\u00df", "du", "das", "Ta\u00b7fel\u00b7tuch", "zer\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "ist Bruch mit mir, darauf dein Leben fu\u00dft, \u2013", "tokens": ["ist", "Bruch", "mit", "mir", ",", "da\u00b7rauf", "dein", "Le\u00b7ben", "fu\u00dft", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "NN", "APPR", "PPER", "$,", "PAV", "PPOSAT", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "verwirr dich nicht in Gramgedankenwust!", "tokens": ["ver\u00b7wirr", "dich", "nicht", "in", "Gram\u00b7ge\u00b7dan\u00b7ken\u00b7wust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was du erstrittst, hab reuelos erstritten!", "tokens": ["Was", "du", "er\u00b7strittst", ",", "hab", "reu\u00b7e\u00b7los", "er\u00b7strit\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Genie\u00dfe deines Wollens Frucht in Kraft,", "tokens": ["Ge\u00b7nie\u00b7\u00dfe", "dei\u00b7nes", "Wol\u00b7lens", "Frucht", "in", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "verh\u00fcll gleich mir des Einst verschlungne Tage:", "tokens": ["ver\u00b7h\u00fcll", "gleich", "mir", "des", "Einst", "ver\u00b7schlung\u00b7ne", "Ta\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df jeder so, gesund in Schaft und Saft,", "tokens": ["Da\u00df", "je\u00b7der", "so", ",", "ge\u00b7sund", "in", "Schaft", "und", "Saft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "ein starker, grader Stamm gen Himmel rage.", "tokens": ["ein", "star\u00b7ker", ",", "gra\u00b7der", "Stamm", "gen", "Him\u00b7mel", "ra\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Vernichten hie\u00df dich deine Leidenschaft \u2013:", "tokens": ["Ver\u00b7nich\u00b7ten", "hie\u00df", "dich", "dei\u00b7ne", "Lei\u00b7den\u00b7schaft", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So schreit' in Sch\u00f6nheit, ohne Reu und Klage!", "tokens": ["So", "schreit'", "in", "Sch\u00f6n\u00b7heit", ",", "oh\u00b7ne", "Reu", "und", "Kla\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$,", "KOUI", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}