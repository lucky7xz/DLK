{"dta.poem.23497": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Von rc. beeden  \n Fr\u00e4wlin rc. Agnes vnd  \n Anna Hertzoginen zu Wir-  \n temberg rc.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1619", "urn": "urn:nbn:de:kobv:b4-20627-4", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ewer G\u00f6ttin-gleiche person/", "tokens": ["E\u00b7wer", "G\u00f6t\u00b7tin\u00b7glei\u00b7che", "per\u00b7son", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Segnend die zuseh er mit wohn/", "tokens": ["Seg\u00b7nend", "die", "zu\u00b7seh", "er", "mit", "wohn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "VVFIN", "PPER", "APPR", "NN", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ewer gang/ stand/ dantz vnd geberden", "tokens": ["E\u00b7wer", "gang", "/", "stand", "/", "dantz", "vnd", "ge\u00b7ber\u00b7den"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "VVFIN", "$(", "ADV", "KON", "NN"], "meter": "+-+-+----", "measure": "unknown.measure.tri"}, "line.4": {"text": "Machend ein Paradi\u00df auf erden/", "tokens": ["Ma\u00b7chend", "ein", "Pa\u00b7ra\u00b7di\u00df", "auf", "er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPR", "NN", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Ewrer sch\u00f6nheit h\u00f6flicher pracht/", "tokens": ["Ew\u00b7rer", "sch\u00f6n\u00b7heit", "h\u00f6f\u00b7li\u00b7cher", "pracht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Darab sich die G\u00f6tter erg\u00f6tzen/", "tokens": ["Da\u00b7rab", "sich", "die", "G\u00f6t\u00b7ter", "er\u00b7g\u00f6t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "ART", "NN", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Machen/ Anna/ mit s\u00fcsser macht", "tokens": ["Ma\u00b7chen", "/", "An\u00b7na", "/", "mit", "s\u00fcs\u00b7ser", "macht"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$(", "NE", "$(", "APPR", "ADJA", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Sich die himel ab Euch entsetzen.", "tokens": ["Sich", "die", "hi\u00b7mel", "ab", "Euch", "ent\u00b7set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}