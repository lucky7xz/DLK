{"textgrid.poem.35669": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Frage nicht, was mir fehle, du Schmeichlerin; atm' ich doch ringsum", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frage nicht, was mir fehle, du Schmeichlerin; atm' ich doch ringsum", "tokens": ["Fra\u00b7ge", "nicht", ",", "was", "mir", "feh\u00b7le", ",", "du", "Schmeich\u00b7le\u00b7rin", ";", "atm'", "ich", "doch", "ring\u00b7sum"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$,", "PPER", "NN", "$.", "VVFIN", "PPER", "ADV", "ADV"], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "D\u00fcfte des sprossenden Laubs, h\u00f6re die Nachtigall rings,", "tokens": ["D\u00fcf\u00b7te", "des", "spros\u00b7sen\u00b7den", "Laubs", ",", "h\u00f6\u00b7re", "die", "Nach\u00b7ti\u00b7gall", "rings", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Und betracht' im Abend die wolkigen Riesengestalten,", "tokens": ["Und", "be\u00b7tracht'", "im", "A\u00b7bend", "die", "wol\u00b7ki\u00b7gen", "Rie\u00b7sen\u00b7ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Welche mit Pupur den Leib g\u00fcrten, mit Golde das Haupt.", "tokens": ["Wel\u00b7che", "mit", "Pu\u00b7pur", "den", "Leib", "g\u00fcr\u00b7ten", ",", "mit", "Gol\u00b7de", "das", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPR", "NN", "ART", "NN", "VVFIN", "$,", "APPR", "NN", "ART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Siehe, mein Geist entschwebt zu den Heldenseelen der Vorwelt,", "tokens": ["Sie\u00b7he", ",", "mein", "Geist", "ent\u00b7schwebt", "zu", "den", "Hel\u00b7den\u00b7see\u00b7len", "der", "Vor\u00b7welt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+--+-+-+-+", "measure": "iambic.septa.invert"}, "line.6": {"text": "Da man das Gro\u00dfe noch gro\u00df achtete, Kleines noch klein;", "tokens": ["Da", "man", "das", "Gro\u00b7\u00dfe", "noch", "gro\u00df", "ach\u00b7te\u00b7te", ",", "Klei\u00b7nes", "noch", "klein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "ADV", "ADJD", "VVFIN", "$,", "ADJA", "ADV", "ADJD", "$."], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Da sich der Mensch noch seiner erinnerte, da\u00df er verst\u00e4ndig", "tokens": ["Da", "sich", "der", "Mensch", "noch", "sei\u00b7ner", "e\u00b7rin\u00b7ner\u00b7te", ",", "da\u00df", "er", "ver\u00b7st\u00e4n\u00b7dig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "PPOSAT", "VVFIN", "$,", "KOUS", "PPER", "ADJD"], "meter": "-+-+-+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "Sei, ein empfindender Geist, nicht ein gefr\u00e4\u00dfiger Bauch;", "tokens": ["Sei", ",", "ein", "emp\u00b7fin\u00b7den\u00b7der", "Geist", ",", "nicht", "ein", "ge\u00b7fr\u00e4\u00b7\u00dfi\u00b7ger", "Bauch", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ART", "ADJA", "NN", "$,", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Und auch liebliche Blumen der Menschlichkeit emsig gewartet", "tokens": ["Und", "auch", "lieb\u00b7li\u00b7che", "Blu\u00b7men", "der", "Menschlich\u00b7keit", "em\u00b7sig", "ge\u00b7war\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "NN", "ART", "NN", "ADJD", "VVPP"], "meter": "--+--+--++-+-+-", "measure": "anapaest.tri.plus"}, "line.10": {"text": "Bl\u00fcheten, nicht allein Futter f\u00fcr Menschen und Vieh.", "tokens": ["Bl\u00fc\u00b7he\u00b7ten", ",", "nicht", "al\u00b7lein", "Fut\u00b7ter", "f\u00fcr", "Men\u00b7schen", "und", "Vieh", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKNEG", "ADV", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.11": {"text": "Lebt' ich in jener Zeit, da Homer den starken Achilleus,", "tokens": ["Lebt'", "ich", "in", "je\u00b7ner", "Zeit", ",", "da", "Ho\u00b7mer", "den", "star\u00b7ken", "A\u00b7chil\u00b7leus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN", "$,", "KOUS", "NE", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "Und des duldenden Manns Tugend und Weisheit besang:", "tokens": ["Und", "des", "dul\u00b7den\u00b7den", "Manns", "Tu\u00b7gend", "und", "Weis\u00b7heit", "be\u00b7sang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "M\u00fchsam wandert' ich fern aus den hyperborischen W\u00e4ldern,", "tokens": ["M\u00fch\u00b7sam", "wan\u00b7dert'", "ich", "fern", "aus", "den", "hy\u00b7per\u00b7bo\u00b7ri\u00b7schen", "W\u00e4l\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Wie zum krotonischen Greis' Abaris, hin zu Homer.", "tokens": ["Wie", "zum", "kro\u00b7to\u00b7ni\u00b7schen", "Greis'", "A\u00b7ba\u00b7ris", ",", "hin", "zu", "Ho\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "ADJA", "NE", "NE", "$,", "ADV", "APPR", "NE", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.15": {"text": "Nicht unkundig des Liedes, denn hell in den Hainen Apollons", "tokens": ["Nicht", "un\u00b7kun\u00b7dig", "des", "Lie\u00b7des", ",", "denn", "hell", "in", "den", "Hai\u00b7nen", "A\u00b7pol\u00b7lons"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "ART", "NN", "$,", "KON", "ADJD", "APPR", "ART", "NN", "NE"], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.16": {"text": "T\u00f6nt' auch unser Gesang, w\u00fcrd' ich sein Reisegeno\u00df.", "tokens": ["T\u00f6nt'", "auch", "un\u00b7ser", "Ge\u00b7sang", ",", "w\u00fcrd'", "ich", "sein", "Rei\u00b7se\u00b7ge\u00b7no\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$,", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Singend z\u00f6gen wir bald in Jonien, bald in den Inseln,", "tokens": ["Sin\u00b7gend", "z\u00f6\u00b7gen", "wir", "bald", "in", "Jo\u00b7ni\u00b7en", ",", "bald", "in", "den", "In\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "APPR", "NE", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Bald durch Hellas umher, und das arkadische Thal;", "tokens": ["Bald", "durch", "Hel\u00b7las", "um\u00b7her", ",", "und", "das", "ar\u00b7ka\u00b7di\u00b7sche", "Thal", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PTKVZ", "$,", "KON", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.19": {"text": "S\u00e4hn noch ungef\u00e4lscht die Natur, und des goldenen Alters", "tokens": ["S\u00e4hn", "noch", "un\u00b7ge\u00b7f\u00e4lscht", "die", "Na\u00b7tur", ",", "und", "des", "gol\u00b7de\u00b7nen", "Al\u00b7ters"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJD", "ART", "NN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Sitte, da gern ein Gott oder ein Engel erschien;", "tokens": ["Sit\u00b7te", ",", "da", "gern", "ein", "Gott", "o\u00b7der", "ein", "En\u00b7gel", "er\u00b7schien", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ADV", "ART", "NN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.21": {"text": "Unschuld, Treu' und Thaten der ungefesselten Menschheit", "tokens": ["Un\u00b7schuld", ",", "Treu'", "und", "Tha\u00b7ten", "der", "un\u00b7ge\u00b7fes\u00b7sel\u00b7ten", "Menschheit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "NN", "KON", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+--+----+", "measure": "trochaic.penta.relaxed"}, "line.22": {"text": "S\u00e4hn wir, und streuten zur That edleren Samen umher.", "tokens": ["S\u00e4hn", "wir", ",", "und", "streu\u00b7ten", "zur", "That", "ed\u00b7le\u00b7ren", "Sa\u00b7men", "um\u00b7her", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN", "APPRART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.23": {"text": "Gleich willkommen im Hirtengeheg' und Palaste des K\u00f6nigs,", "tokens": ["Gleich", "will\u00b7kom\u00b7men", "im", "Hir\u00b7ten\u00b7ge\u00b7heg'", "und", "Pa\u00b7las\u00b7te", "des", "K\u00f6\u00b7nigs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.24": {"text": "Beim nachbarlichen Schmaus' oder bei Festen des Volks,", "tokens": ["Beim", "nach\u00b7bar\u00b7li\u00b7chen", "Schmaus'", "o\u00b7der", "bei", "Fes\u00b7ten", "des", "Volks", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KON", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.25": {"text": "W\u00e4ren wir \u00fcberall wie daheim, und n\u00e4hmen mit Hauskost,", "tokens": ["W\u00e4\u00b7ren", "wir", "\u00fc\u00b7be\u00b7rall", "wie", "da\u00b7heim", ",", "und", "n\u00e4h\u00b7men", "mit", "Haus\u00b7kost", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "KOKOM", "ADV", "$,", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.26": {"text": "Milch und Fr\u00fcchten vorlieb, lieber mit r\u00f6tlichem Wein.", "tokens": ["Milch", "und", "Fr\u00fcch\u00b7ten", "vor\u00b7lieb", ",", "lie\u00b7ber", "mit", "r\u00f6t\u00b7li\u00b7chem", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$,", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.27": {"text": "Stattlich s\u00e4\u00dfen wir beide mit Lorbeer gekr\u00e4nzt; und der Jungfraun", "tokens": ["Statt\u00b7lich", "s\u00e4\u00b7\u00dfen", "wir", "bei\u00b7de", "mit", "Lor\u00b7beer", "ge\u00b7kr\u00e4nzt", ";", "und", "der", "Jung\u00b7fraun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PIS", "APPR", "NN", "VVPP", "$.", "KON", "ART", "NN"], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.28": {"text": "Sch\u00f6nste, noch sch\u00f6ner vom Tanz, setzte sich traulich zu uns,", "tokens": ["Sch\u00f6ns\u00b7te", ",", "noch", "sch\u00f6\u00b7ner", "vom", "Tanz", ",", "setz\u00b7te", "sich", "trau\u00b7lich", "zu", "uns", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "APPRART", "NN", "$,", "VVFIN", "PRF", "ADJD", "APPR", "PPER", "$,"], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.29": {"text": "R\u00fchmte hold den Gesang, und betastete klimpernd die Saiten,", "tokens": ["R\u00fchm\u00b7te", "hold", "den", "Ge\u00b7sang", ",", "und", "be\u00b7tas\u00b7te\u00b7te", "klim\u00b7pernd", "die", "Sai\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,", "KON", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.30": {"text": "F\u00fcllte dann unser Gef\u00e4\u00df n\u00f6tigend wieder mit Wein.", "tokens": ["F\u00fcll\u00b7te", "dann", "un\u00b7ser", "Ge\u00b7f\u00e4\u00df", "n\u00f6\u00b7ti\u00b7gend", "wie\u00b7der", "mit", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "ADJD", "ADV", "APPR", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.31": {"text": "So wie wir Th\u00e4ler und H\u00f6hn durchwanderten, h\u00f6rten wir ringsum", "tokens": ["So", "wie", "wir", "Th\u00e4\u00b7ler", "und", "H\u00f6hn", "durch\u00b7wan\u00b7der\u00b7ten", ",", "h\u00f6r\u00b7ten", "wir", "ring\u00b7sum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PPER", "NN", "KON", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ADV"], "meter": "-+-+--+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.32": {"text": "Unserer Lieder Klang: hier von dem J\u00e4ger im Forst,", "tokens": ["Un\u00b7se\u00b7rer", "Lie\u00b7der", "Klang", ":", "hier", "von", "dem", "J\u00e4\u00b7ger", "im", "Forst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$.", "ADV", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.33": {"text": "Dort beim Pflug' und der Sense, vom Fischer am See, und der Hirtin;", "tokens": ["Dort", "beim", "Pflug'", "und", "der", "Sen\u00b7se", ",", "vom", "Fi\u00b7scher", "am", "See", ",", "und", "der", "Hir\u00b7tin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "KON", "ART", "NN", "$,", "APPRART", "NN", "APPRART", "NN", "$,", "KON", "ART", "NN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.34": {"text": "Unter den Linden des Dorfs lallten die Kinder sie nach;", "tokens": ["Un\u00b7ter", "den", "Lin\u00b7den", "des", "Dorfs", "lall\u00b7ten", "die", "Kin\u00b7der", "sie", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.35": {"text": "Und Arbeiter am Weg' und Wanderer zeigten mit Fingern,", "tokens": ["Und", "Ar\u00b7bei\u00b7ter", "am", "Weg'", "und", "Wan\u00b7de\u00b7rer", "zeig\u00b7ten", "mit", "Fin\u00b7gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "KON", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.36": {"text": "Nickten und gr\u00fc\u00dften uns zu, namentlich, und wie bekannt.", "tokens": ["Nick\u00b7ten", "und", "gr\u00fc\u00df\u00b7ten", "uns", "zu", ",", "na\u00b7ment\u00b7lich", ",", "und", "wie", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "$,", "KON", "PWAV", "ADJD", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.37": {"text": "Aber kehrten wir m\u00fcd' am Mittag' oder am Abend", "tokens": ["A\u00b7ber", "kehr\u00b7ten", "wir", "m\u00fcd'", "am", "Mit\u00b7tag'", "o\u00b7der", "am", "A\u00b7bend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "VMFIN", "APPRART", "NN", "KON", "APPRART", "NN"], "meter": "+-+--+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.38": {"text": "In ein friedsames Dorf; schnell, wie der Ruf sich ergeu\u0