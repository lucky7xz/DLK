{"textgrid.poem.46592": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ungl\u00fcckselige Wohnung]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ungl\u00fcckselige Wohnung", "tokens": ["Un\u00b7gl\u00fcck\u00b7se\u00b7li\u00b7ge", "Woh\u00b7nung"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hier weit hinter der Mauer,", "tokens": ["Hier", "weit", "hin\u00b7ter", "der", "Mau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Aus der Stadt, der bewohnten,", "tokens": ["Aus", "der", "Stadt", ",", "der", "be\u00b7wohn\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Aus der Welt wie verwiesen!", "tokens": ["Aus", "der", "Welt", "wie", "ver\u00b7wie\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KOKOM", "VVPP", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wo ein heulender Kater", "tokens": ["Wo", "ein", "heu\u00b7len\u00b7der", "Ka\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und ein \u00e4chzendes K\u00e4uzchen,", "tokens": ["Und", "ein", "\u00e4ch\u00b7zen\u00b7des", "K\u00e4uz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Zwei unheimliche W\u00e4chter,", "tokens": ["Zwei", "un\u00b7heim\u00b7li\u00b7che", "W\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Unheildeutenden Lautes", "tokens": ["Un\u00b7heil\u00b7deu\u00b7ten\u00b7den", "Lau\u00b7tes"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.9": {"text": "Dumpfe Stunden der Nacht mir,", "tokens": ["Dump\u00b7fe", "Stun\u00b7den", "der", "Nacht", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "PPER", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.10": {"text": "Dem Schlaflosen, betonen;", "tokens": ["Dem", "Schlaf\u00b7lo\u00b7sen", ",", "be\u00b7to\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}, "line.11": {"text": "W\u00e4hrend fern an der Ecke", "tokens": ["W\u00e4h\u00b7rend", "fern", "an", "der", "E\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.12": {"text": "Des verlorenen G\u00e4\u00dfchens,", "tokens": ["Des", "ver\u00b7lo\u00b7re\u00b7nen", "G\u00e4\u00df\u00b7chens", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "Des verrufenen Winkels,", "tokens": ["Des", "ver\u00b7ru\u00b7fe\u00b7nen", "Win\u00b7kels", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.14": {"text": "Der Nachtw\u00e4chter nur fl\u00fcchtig", "tokens": ["Der", "Nacht\u00b7w\u00e4ch\u00b7ter", "nur", "fl\u00fcch\u00b7tig"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADJD"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Abgewendet ins Horn st\u00f6\u00dft,", "tokens": ["Ab\u00b7ge\u00b7wen\u00b7det", "ins", "Horn", "st\u00f6\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.16": {"text": "Da\u00df der Ton nicht heraufschallt;", "tokens": ["Da\u00df", "der", "Ton", "nicht", "her\u00b7auf\u00b7schallt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Auch vom Thurme, dem n\u00e4chsten,", "tokens": ["Auch", "vom", "Thur\u00b7me", ",", "dem", "n\u00e4chs\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Von zu zwergigem Wuchse,", "tokens": ["Von", "zu", "zwer\u00b7gi\u00b7gem", "Wuch\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.19": {"text": "Von zu schm\u00e4chtigem Schallloch", "tokens": ["Von", "zu", "schm\u00e4ch\u00b7ti\u00b7gem", "Schall\u00b7loch"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.20": {"text": "Und zu sp\u00e4rlichem Schlagwerk,", "tokens": ["Und", "zu", "sp\u00e4r\u00b7li\u00b7chem", "Schlag\u00b7werk", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.21": {"text": "Unverk\u00fcmmert und deutlich", "tokens": ["Un\u00b7ver\u00b7k\u00fcm\u00b7mert", "und", "deut\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "ADJD"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.22": {"text": "Keine Stundenbezeichnung", "tokens": ["Kei\u00b7ne", "Stun\u00b7den\u00b7be\u00b7zeich\u00b7nung"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.23": {"text": "Mir, dem Lauschenden, zukommt.", "tokens": ["Mir", ",", "dem", "Lau\u00b7schen\u00b7den", ",", "zu\u00b7kommt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Nur bei grauendem Morgen", "tokens": ["Nur", "bei", "grau\u00b7en\u00b7dem", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.25": {"text": "Ist ein Glockengel\u00e4ute", "tokens": ["Ist", "ein", "Glo\u00b7cken\u00b7ge\u00b7l\u00e4u\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.26": {"text": "Stark genug mich zu wecken,", "tokens": ["Stark", "ge\u00b7nug", "mich", "zu", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.27": {"text": "Das erwachenden Andern", "tokens": ["Das", "er\u00b7wa\u00b7chen\u00b7den", "An\u00b7dern"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "ADJA"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.28": {"text": "Frohen Tages Zur\u00fcckkehr", "tokens": ["Fro\u00b7hen", "Ta\u00b7ges", "Zu\u00b7r\u00fcck\u00b7kehr"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Ansagt, aber mich immer", "tokens": ["An\u00b7sagt", ",", "a\u00b7ber", "mich", "im\u00b7mer"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "KON", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Traurig mahnt an die Fr\u00fche,", "tokens": ["Trau\u00b7rig", "mahnt", "an", "die", "Fr\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.31": {"text": "Wo es so von dem Thurm klang", "tokens": ["Wo", "es", "so", "von", "dem", "Thurm", "klang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Und mein Liebstes zu Grab rief.", "tokens": ["Und", "mein", "Liebs\u00b7tes", "zu", "Grab", "rief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}}}}