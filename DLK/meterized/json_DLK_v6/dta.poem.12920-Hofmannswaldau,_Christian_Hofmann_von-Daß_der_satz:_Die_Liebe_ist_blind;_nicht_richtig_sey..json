{"dta.poem.12920": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Da\u00df der satz: Die Liebe ist blind;  \n nicht richtig sey.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich stieg auf einen fels, die Liebe stieg mir nach:", "tokens": ["Ich", "stieg", "auf", "ei\u00b7nen", "fels", ",", "die", "Lie\u00b7be", "stieg", "mir", "nach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich sprung in eine bach, so schwam sie mir entgegen.", "tokens": ["Ich", "sprung", "in", "ei\u00b7ne", "bach", ",", "so", "schwam", "sie", "mir", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich lief und floh vor ihr aus \u00e4userstem verm\u00f6gen,", "tokens": ["Ich", "lief", "und", "floh", "vor", "ihr", "aus", "\u00e4u\u00b7sers\u00b7tem", "ver\u00b7m\u00f6\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie aber folgte mir durch alles ungemach.", "tokens": ["Sie", "a\u00b7ber", "folg\u00b7te", "mir", "durch", "al\u00b7les", "un\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "APPR", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich suchte zwar n\u00e4chsthin in abgelegnen hecken,", "tokens": ["Ich", "such\u00b7te", "zwar", "n\u00e4chst\u00b7hin", "in", "ab\u00b7ge\u00b7leg\u00b7nen", "he\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wo dicke finsterni\u00df\u2019 und grause tieffen sind,", "tokens": ["Wo", "di\u00b7cke", "fins\u00b7ter\u00b7ni\u00df'", "und", "grau\u00b7se", "tief\u00b7fen", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Vor ihren angen mich auf ewig zu verstecken;", "tokens": ["Vor", "ih\u00b7ren", "an\u00b7gen", "mich", "auf", "e\u00b7wig", "zu", "ver\u00b7ste\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "APPR", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Lernt\u2019 aber \u00fcberall: Die Liebe sey nicht blind.", "tokens": ["Lernt'", "a\u00b7ber", "\u00fc\u00b7be\u00b7rall", ":", "Die", "Lie\u00b7be", "sey", "nicht", "blind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$.", "ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}