{"dta.poem.12494": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "V.  \n  Dem Censor.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unseliger Eunuche, du,", "tokens": ["Un\u00b7se\u00b7li\u00b7ger", "Eu\u00b7nu\u00b7che", ",", "du", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der unsres Geistes Hauch bewacht", "tokens": ["Der", "uns\u00b7res", "Geis\u00b7tes", "Hauch", "be\u00b7wacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sich f\u00fcr seines Sultans Ruh'", "tokens": ["Und", "sich", "f\u00fcr", "sei\u00b7nes", "Sul\u00b7tans", "Ruh'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum gottverfluchten Knechte macht!", "tokens": ["Zum", "gott\u00b7ver\u00b7fluch\u00b7ten", "Knech\u00b7te", "macht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du hast mein blo\u00dfes Wort verdammt,", "tokens": ["Du", "hast", "mein", "blo\u00b7\u00dfes", "Wort", "ver\u00b7dammt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil's nicht in Eure K\u00fcche pa\u00dft: \u2014", "tokens": ["Weil's", "nicht", "in", "Eu\u00b7re", "K\u00fc\u00b7che", "pa\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat minder drum die\u00df Herz geflammt", "tokens": ["Hat", "min\u00b7der", "drum", "die\u00df", "Herz", "ge\u00b7flammt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PAV", "PDS", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und minder Dich und Ihn geha\u00dft?", "tokens": ["Und", "min\u00b7der", "Dich", "und", "Ihn", "ge\u00b7ha\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "KON", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O glaub' den Geist nicht unterjocht,", "tokens": ["O", "glaub'", "den", "Geist", "nicht", "un\u00b7ter\u00b7jocht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn du vom Leib ein Glied getrennt!", "tokens": ["Wenn", "du", "vom", "Leib", "ein", "Glied", "ge\u00b7trennt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du, Sklave, putzest nur den Docht,", "tokens": ["Du", ",", "Skla\u00b7ve", ",", "put\u00b7zest", "nur", "den", "Docht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Damit das Licht noch heller brennt.", "tokens": ["Da\u00b7mit", "das", "Licht", "noch", "hel\u00b7ler", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}