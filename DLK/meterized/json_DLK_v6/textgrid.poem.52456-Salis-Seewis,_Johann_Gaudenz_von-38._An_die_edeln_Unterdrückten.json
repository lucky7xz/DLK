{"textgrid.poem.52456": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "38. An die edeln Unterdr\u00fcckten", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Getrost, ihr edeln Unterdr\u00fcckten,", "tokens": ["Ge\u00b7trost", ",", "ihr", "e\u00b7deln", "Un\u00b7ter\u00b7dr\u00fcck\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn euch kein Strahl der Hoffnung blinkt!", "tokens": ["Wenn", "euch", "kein", "Strahl", "der", "Hoff\u00b7nung", "blinkt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Tugend Opferkr\u00e4nze schm\u00fcckten", "tokens": ["Der", "Tu\u00b7gend", "Op\u00b7fer\u00b7kr\u00e4n\u00b7ze", "schm\u00fcck\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Euch, eh' ihr am Altare sinkt.", "tokens": ["Euch", ",", "eh'", "ihr", "am", "Al\u00b7ta\u00b7re", "sinkt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.5": {"text": "Des Ruhmes Flitterkrone werde", "tokens": ["Des", "Ruh\u00b7mes", "Flit\u00b7ter\u00b7kro\u00b7ne", "wer\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hier des begl\u00fcckten Frevlers Preis,", "tokens": ["Hier", "des", "be\u00b7gl\u00fcck\u00b7ten", "Frev\u00b7lers", "Preis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Entkeimt aus eurer Gr\u00e4ber Erde,", "tokens": ["Ent\u00b7keimt", "aus", "eu\u00b7rer", "Gr\u00e4\u00b7ber", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gr\u00fcnt sp\u00e4t erst euer Eichenreis.", "tokens": ["Gr\u00fcnt", "sp\u00e4t", "erst", "eu\u00b7er", "Ei\u00b7chen\u00b7reis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr, die, verpflanzt in arge Zeiten,", "tokens": ["Ihr", ",", "die", ",", "ver\u00b7pflanzt", "in", "ar\u00b7ge", "Zei\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit der Gewalt zu k\u00e4mpfen wagt,", "tokens": ["Mit", "der", "Ge\u00b7walt", "zu", "k\u00e4mp\u00b7fen", "wagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr sollt dem Lichte Bahn bereiten", "tokens": ["Ihr", "sollt", "dem", "Lich\u00b7te", "Bahn", "be\u00b7rei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fchlt die Schauer, eh' es tagt;", "tokens": ["Und", "f\u00fchlt", "die", "Schau\u00b7er", ",", "eh'", "es", "tagt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn ihr mit kr\u00e4ftigem Erk\u00fchnen", "tokens": ["Wenn", "ihr", "mit", "kr\u00e4f\u00b7ti\u00b7gem", "Er\u00b7k\u00fch\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Euch dem Verfall entgegenstemmt,", "tokens": ["Euch", "dem", "Ver\u00b7fall", "ent\u00b7ge\u00b7gens\u00b7temmt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Verkl\u00e4rt ihr glorreich die Ruinen,", "tokens": ["Ver\u00b7kl\u00e4rt", "ihr", "glor\u00b7reich", "die", "Ru\u00b7i\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die keine Macht im Sturze hemmt.", "tokens": ["Die", "kei\u00b7ne", "Macht", "im", "Stur\u00b7ze", "hemmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dann f\u00fchlt ihr zwar des Schicksals Schwere,", "tokens": ["Dann", "f\u00fchlt", "ihr", "zwar", "des", "Schick\u00b7sals", "Schwe\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn es der L\u00e4st'rung Plan gelingt,", "tokens": ["Wenn", "es", "der", "L\u00e4st'\u00b7rung", "Plan", "ge\u00b7lingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df euer letztes Gut, die Ehre,", "tokens": ["Da\u00df", "eu\u00b7er", "letz\u00b7tes", "Gut", ",", "die", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Klapperschlangenhauch verschlingt;", "tokens": ["Ihr", "Klap\u00b7per\u00b7schlan\u00b7gen\u00b7hauch", "ver\u00b7schlingt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schaut ernst der \u00dcbermacht Triumphe,", "tokens": ["Schaut", "ernst", "der", "\u00dc\u00b7ber\u00b7macht", "Tri\u00b7um\u00b7phe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Wenn h\u00f6hnend euch ihr Tro\u00df umzischt!", "tokens": ["Wenn", "h\u00f6h\u00b7nend", "euch", "ihr", "Tro\u00df", "um\u00b7zischt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wi\u00dft, da\u00df ihr Irrlicht aus dem Sumpfe", "tokens": ["Wi\u00dft", ",", "da\u00df", "ihr", "Irr\u00b7licht", "aus", "dem", "Sump\u00b7fe"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "+--+++-+-", "measure": "dactylic.init"}, "line.8": {"text": "Nur tr\u00fcglich aufgl\u00e4nzt und verlischt!", "tokens": ["Nur", "tr\u00fcg\u00b7lich", "auf\u00b7gl\u00e4nzt", "und", "ver\u00b7lischt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "KON", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Die Wahrheit harrt mit sichrer Wage", "tokens": ["Die", "Wahr\u00b7heit", "harrt", "mit", "sich\u00b7rer", "Wa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Wolkenzelt der Folgezeit,", "tokens": ["Im", "Wol\u00b7ken\u00b7zelt", "der", "Fol\u00b7ge\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verweht die Spreu gedung'ner Sage", "tokens": ["Ver\u00b7weht", "die", "Spreu", "ge\u00b7dung'\u00b7ner", "Sa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und huldigt der Gerechtigkeit.", "tokens": ["Und", "hul\u00b7digt", "der", "Ge\u00b7rech\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vernunft folgt ewigen Gesetzen,", "tokens": ["Ver\u00b7nunft", "folgt", "e\u00b7wi\u00b7gen", "Ge\u00b7set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die P\u00f6belswut, die ein Tyrann", "tokens": ["Die", "P\u00f6\u00b7bels\u00b7wut", ",", "die", "ein", "Ty\u00b7rann"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Menschenalter durch verletzen,", "tokens": ["Ein", "Men\u00b7schen\u00b7al\u00b7ter", "durch", "ver\u00b7let\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Doch ewig nicht vertilgen kann.", "tokens": ["Doch", "e\u00b7wig", "nicht", "ver\u00b7til\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Denkt, wenn im Kampf f\u00fcr Menschenrechte", "tokens": ["Denkt", ",", "wenn", "im", "Kampf", "f\u00fcr", "Men\u00b7schen\u00b7rech\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr des Erfolges Glanz entbehrt,", "tokens": ["Ihr", "des", "Er\u00b7fol\u00b7ges", "Glanz", "ent\u00b7behrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df durch des Mi\u00dfgeschickes N\u00e4chte", "tokens": ["Da\u00df", "durch", "des", "Mi\u00df\u00b7ge\u00b7schi\u00b7ckes", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Unschuld Haupt sich still verkl\u00e4rt.", "tokens": ["Der", "Un\u00b7schuld", "Haupt", "sich", "still", "ver\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schaut fest nach euerm hohen Ziele,", "tokens": ["Schaut", "fest", "nach", "eu\u00b7erm", "ho\u00b7hen", "Zie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verschm\u00e4ht die nahe Hindernis,", "tokens": ["Ver\u00b7schm\u00e4ht", "die", "na\u00b7he", "Hin\u00b7der\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und st\u00fcrzt, gedr\u00e4ngt vom Pflichtgef\u00fchle,", "tokens": ["Und", "st\u00fcrzt", ",", "ge\u00b7dr\u00e4ngt", "vom", "Pflicht\u00b7ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In des entflammten Abgrunds Ri\u00df.", "tokens": ["In", "des", "ent\u00b7flamm\u00b7ten", "Ab\u00b7grunds", "Ri\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wenn, vom Verh\u00e4ngnis losgerissen,", "tokens": ["Wenn", ",", "vom", "Ver\u00b7h\u00e4ng\u00b7nis", "los\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Hoffnung letzte Tr\u00fcmmer st\u00fcrzt,", "tokens": ["Der", "Hoff\u00b7nung", "letz\u00b7te", "Tr\u00fcm\u00b7mer", "st\u00fcrzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sollt ihr den Kelch zu kosten wissen,", "tokens": ["Sollt", "ihr", "den", "Kelch", "zu", "kos\u00b7ten", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der jedes Erdenweh verk\u00fcrzt.", "tokens": ["Der", "je\u00b7des", "Er\u00b7den\u00b7weh", "ver\u00b7k\u00fcrzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Recht, verbannt, verschm\u00e4ht, erw\u00fcrget,", "tokens": ["Das", "Recht", ",", "ver\u00b7bannt", ",", "ver\u00b7schm\u00e4ht", ",", "er\u00b7w\u00fcr\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "$,", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erlegen im gerechten Streit,", "tokens": ["Er\u00b7le\u00b7gen", "im", "ge\u00b7rech\u00b7ten", "Streit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Fleht um Vergeltung und verb\u00fcrget", "tokens": ["Fleht", "um", "Ver\u00b7gel\u00b7tung", "und", "ver\u00b7b\u00fcr\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den Geistern die Unsterblichkeit!", "tokens": ["Den", "Geis\u00b7tern", "die", "U\u00b7nsterb\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Dem Staub entflohn, wirkt eure Seele", "tokens": ["Dem", "Staub", "ent\u00b7flohn", ",", "wirkt", "eu\u00b7re", "See\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Begeisternd auf der Edeln Bund;", "tokens": ["Be\u00b7geis\u00b7ternd", "auf", "der", "E\u00b7deln", "Bund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verwandelt erst, thut Philomele", "tokens": ["Ver\u00b7wan\u00b7delt", "erst", ",", "thut", "Phi\u00b7lo\u00b7me\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "VVFIN", "NE"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Die Unthat ihres Dr\u00e4ngers kund!", "tokens": ["Die", "Un\u00b7that", "ih\u00b7res", "Dr\u00e4n\u00b7gers", "kund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr M\u00e4rtyrer f\u00fcr Menschenw\u00fcrde,", "tokens": ["Ihr", "M\u00e4r\u00b7ty\u00b7rer", "f\u00fcr", "Men\u00b7schen\u00b7w\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vertraut der Wahrheit und der Zeit:", "tokens": ["Ver\u00b7traut", "der", "Wahr\u00b7heit", "und", "der", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Verg\u00e4nglich ist des Druckes B\u00fcrde,", "tokens": ["Ver\u00b7g\u00e4ng\u00b7lich", "ist", "des", "Dru\u00b7ckes", "B\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Doch ewig die Gerechtigkeit!", "tokens": ["Doch", "e\u00b7wig", "die", "Ge\u00b7rech\u00b7tig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}