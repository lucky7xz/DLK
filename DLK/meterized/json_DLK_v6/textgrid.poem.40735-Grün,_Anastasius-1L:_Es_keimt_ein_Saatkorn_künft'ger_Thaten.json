{"textgrid.poem.40735": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es keimt ein Saatkorn k\u00fcnft'ger Thaten", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es keimt ein Saatkorn k\u00fcnft'ger Thaten", "tokens": ["Es", "keimt", "ein", "Saat\u00b7korn", "k\u00fcnft'\u00b7ger", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In jedem Wunsch; \u2013 drum w\u00fcnsche nur!", "tokens": ["In", "je\u00b7dem", "Wunsch", ";", "\u2013", "drum", "w\u00fcn\u00b7sche", "nur", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$.", "$(", "PAV", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch streu' auf deine Lebensflur", "tokens": ["Doch", "streu'", "auf", "dei\u00b7ne", "Le\u00b7bens\u00b7flur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur gutes Korn und reine Saaten.", "tokens": ["Nur", "gu\u00b7tes", "Korn", "und", "rei\u00b7ne", "Saa\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So will auch ich die Glocke wiegen,", "tokens": ["So", "will", "auch", "ich", "die", "Glo\u00b7cke", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df weit ihr Aufschrei widerhallt,", "tokens": ["Da\u00df", "weit", "ihr", "Auf\u00b7schrei", "wi\u00b7der\u00b7hallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und da\u00df, so lang ihr Ton mir schallt,", "tokens": ["Und", "da\u00df", ",", "so", "lang", "ihr", "Ton", "mir", "schallt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "ADV", "ADJD", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Himmel meine W\u00fcnsche fliegen:", "tokens": ["Zum", "Him\u00b7mel", "mei\u00b7ne", "W\u00fcn\u00b7sche", "flie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbaus der Bet\u00e4ubung dumpfer Tr\u00e4ume,", "tokens": ["\u00bb", "aus", "der", "Be\u00b7t\u00e4u\u00b7bung", "dum\u00b7pfer", "Tr\u00e4u\u00b7me", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Heimatland, mein Volk, wach' auf!", "tokens": ["Mein", "Hei\u00b7mat\u00b7land", ",", "mein", "Volk", ",", "wach'", "auf", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sieh deiner Nachbarn Siegeslauf!", "tokens": ["Sieh", "dei\u00b7ner", "Nach\u00b7barn", "Sie\u00b7ges\u00b7lauf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O Schmach, wer da im Wettkampf s\u00e4ume!", "tokens": ["O", "Schmach", ",", "wer", "da", "im", "Wett\u00b7kampf", "s\u00e4u\u00b7me", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWS", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Den w\u00fcsten Schlaf reib' aus den Augen,", "tokens": ["Den", "w\u00fcs\u00b7ten", "Schlaf", "reib'", "aus", "den", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die noch umflort, obschon es Tag;", "tokens": ["Die", "noch", "um\u00b7flort", ",", "ob\u00b7schon", "es", "Tag", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Blick' in den Glanz! \u2013 Lichtscheue mag", "tokens": ["Blick'", "in", "den", "Glanz", "!", "\u2013", "Licht\u00b7scheu\u00b7e", "mag"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$.", "$(", "NN", "VMFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Dem Olm in deinen Grotten taugen.", "tokens": ["Dem", "Olm", "in", "dei\u00b7nen", "Grot\u00b7ten", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Bist scharfen Blicks, geweckten Geistes,", "tokens": ["Bist", "schar\u00b7fen", "Blicks", ",", "ge\u00b7weck\u00b7ten", "Geis\u00b7tes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bist klug, wie schon dein Dichter sang;", "tokens": ["Bist", "klug", ",", "wie", "schon", "dein", "Dich\u00b7ter", "sang", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "PWAV", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Schlaftrunk doch wirkt stark und lang,", "tokens": ["Der", "Schlaf\u00b7trunk", "doch", "wirkt", "stark", "und", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Den man im Kelch kredenzt, du wei\u00dft es!", "tokens": ["Den", "man", "im", "Kelch", "kre\u00b7denzt", ",", "du", "wei\u00dft", "es", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPRART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Von Berg zu Berg das Feuerzeichen", "tokens": ["Von", "Berg", "zu", "Berg", "das", "Feu\u00b7er\u00b7zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Rief einst zur Wacht in T\u00fcrkennoth,", "tokens": ["Rief", "einst", "zur", "Wacht", "in", "T\u00fcr\u00b7ken\u00b7noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sklaverei, die dir gedroht,", "tokens": ["Der", "Skla\u00b7ve\u00b7rei", ",", "die", "dir", "ge\u00b7droht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu wehren mit des Schwertes Streichen.", "tokens": ["Zu", "weh\u00b7ren", "mit", "des", "Schwer\u00b7tes", "Strei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch Greise jetzt und Neugebor'ne", "tokens": ["Doch", "Grei\u00b7se", "jetzt", "und", "Neu\u00b7ge\u00b7bor'\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Umschn\u00fcrt ein andres Sklavenband:", "tokens": ["Um\u00b7schn\u00fcrt", "ein", "and\u00b7res", "Skla\u00b7ven\u00b7band", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kaftan und Kutte sind verwandt,", "tokens": ["Kaf\u00b7tan", "und", "Kut\u00b7te", "sind", "ver\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sowie Beschnitt'ne und Geschor'ne.", "tokens": ["So\u00b7wie", "Be\u00b7schnitt'\u00b7ne", "und", "Ge\u00b7schor'\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Von Haupt zu Haupt des Lichtes Zeichen,", "tokens": ["Von", "Haupt", "zu", "Haupt", "des", "Lich\u00b7tes", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das auch die neuen T\u00fcrken bannt,", "tokens": ["Das", "auch", "die", "neu\u00b7en", "T\u00fcr\u00b7ken", "bannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df flammen jetzt durchs weite Land", "tokens": ["La\u00df", "flam\u00b7men", "jetzt", "durchs", "wei\u00b7te", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "VVFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und diese Flammen nie erbleichen!", "tokens": ["Und", "die\u00b7se", "Flam\u00b7men", "nie", "er\u00b7blei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Das Licht, entquollen einst in Strahlen", "tokens": ["Das", "Licht", ",", "ent\u00b7quol\u00b7len", "einst", "in", "Strah\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem L\u00e4mpchen jenes Bergmannssohns,", "tokens": ["Dem", "L\u00e4mp\u00b7chen", "je\u00b7nes", "Berg\u00b7manns\u00b7sohns", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es flog vom Schacht zu H\u00f6hn des Throns", "tokens": ["Es", "flog", "vom", "Schacht", "zu", "H\u00f6hn", "des", "Throns"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und leuchtet' einst auch diesen Thalen.", "tokens": ["Und", "leuch\u00b7tet'", "einst", "auch", "die\u00b7sen", "Tha\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Gesalbte Schergen doch zertraten", "tokens": ["Ge\u00b7salb\u00b7te", "Scher\u00b7gen", "doch", "zer\u00b7tra\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit plumpem Fu\u00df den Funkenrest;", "tokens": ["Mit", "plum\u00b7pem", "Fu\u00df", "den", "Fun\u00b7ken\u00b7rest", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Finsterni\u00df begann ihr Fest", "tokens": ["Die", "Fins\u00b7ter\u00b7ni\u00df", "be\u00b7gann", "ihr", "Fest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Geistesnacht reift ihre Saaten.", "tokens": ["Und", "Geis\u00b7tes\u00b7nacht", "reift", "ih\u00b7re", "Saa\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Sie heimsen ein; welch lustig Treiben!", "tokens": ["Sie", "heim\u00b7sen", "ein", ";", "welch", "lus\u00b7tig", "Trei\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "PWAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hei, wie der Peterspfennig springt!", "tokens": ["Hei", ",", "wie", "der", "Pe\u00b7ter\u00b7spfen\u00b7nig", "springt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Doch wo des Tetzels B\u00fcchse klingt", "tokens": ["Doch", "wo", "des", "Tet\u00b7zels", "B\u00fcch\u00b7se", "klingt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird auch nicht fern der Luther bleiben. \u2013", "tokens": ["Wird", "auch", "nicht", "fern", "der", "Lu\u00b7ther", "blei\u00b7ben", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADJD", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Vom \u00f6den Karst, von eis'gen Tauern", "tokens": ["Vom", "\u00f6\u00b7den", "Karst", ",", "von", "eis'\u00b7gen", "Tau\u00b7ern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Umschlossen ist dein Wunderland;", "tokens": ["Um\u00b7schlos\u00b7sen", "ist", "dein", "Wun\u00b7der\u00b7land", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Berge sind nicht Kerkerwand,", "tokens": ["Die", "Ber\u00b7ge", "sind", "nicht", "Ker\u00b7ker\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Einsamkeit dich einzumauern.", "tokens": ["In", "Ein\u00b7sam\u00b7keit", "dich", "ein\u00b7zu\u00b7mau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Doch Zinnen sind's und die erklimme!", "tokens": ["Doch", "Zin\u00b7nen", "sin\u00b7d's", "und", "die", "er\u00b7klim\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "KON", "ART", "ADJA", "$."], "meter": "-+----+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Halt Umschau! Sieh, wie dir die Welt", "tokens": ["Halt", "Um\u00b7schau", "!", "Sieh", ",", "wie", "dir", "die", "Welt"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "NN", "$.", "NE", "$,", "PWAV", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Eisenarm entgegenh\u00e4lt,", "tokens": ["Den", "Ei\u00b7sen\u00b7arm", "ent\u00b7ge\u00b7gen\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dir zuruft mit des Blitzes Stimme.", "tokens": ["Dir", "zu\u00b7ruft", "mit", "des", "Blit\u00b7zes", "Stim\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.14": {"line.1": {"text": "Tritt in des Weltmarkts offne Hallen,", "tokens": ["Tritt", "in", "des", "Welt\u00b7markts", "off\u00b7ne", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du sie