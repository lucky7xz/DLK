{"text": "Ein treuer doch des V\u00f6lkerbundes.", "tokens": ["Ein", "treu\u00b7er", "doch", "des", "V\u00f6l\u00b7ker\u00b7bun\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Wach' auf, wach' auf! Vom Leibe raffe", "tokens": ["Wach'", "auf", ",", "wach'", "auf", "!", "Vom", "Lei\u00b7be", "raf\u00b7fe"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$.", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Lappen finstrer Dienstbarkeit!", "tokens": ["Die", "Lap\u00b7pen", "finst\u00b7rer", "Dienst\u00b7bar\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr hohe Ziele k\u00e4mpft die Zeit,", "tokens": ["F\u00fcr", "ho\u00b7he", "Zie\u00b7le", "k\u00e4mpft", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Umg\u00fcrt' auch dich mit ihrer Waffe!", "tokens": ["Um\u00b7g\u00fcrt'", "auch", "dich", "mit", "ih\u00b7rer", "Waf\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Sei wie dein Strom, der in die Kl\u00fcfte", "tokens": ["Sei", "wie", "dein", "Strom", ",", "der", "in", "die", "Kl\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "KOKOM", "PPOSAT", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des H\u00f6hlendunkels j\u00e4h verschwand,", "tokens": ["Des", "H\u00f6h\u00b7len\u00b7dun\u00b7kels", "j\u00e4h", "ver\u00b7schwand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Weg zum Licht doch wieder fand,", "tokens": ["Den", "Weg", "zum", "Licht", "doch", "wie\u00b7der", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und funkelnd gr\u00fc\u00dft die sonn'gen L\u00fcfte.\u00ab \u2013 \u2013", "tokens": ["Und", "fun\u00b7kelnd", "gr\u00fc\u00dft", "die", "sonn'\u00b7gen", "L\u00fcf\u00b7te", ".", "\u00ab", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$.", "$(", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "Das war mein W\u00fcnschen, w\u00e4hrend dessen", "tokens": ["Das", "war", "mein", "W\u00fcn\u00b7schen", ",", "w\u00e4h\u00b7rend", "des\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "KOUS", "PDS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Glocke Klang die Luft durchschnitt,", "tokens": ["Der", "Glo\u00b7cke", "Klang", "die", "Luft", "durch\u00b7schnitt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis m\u00fcd' mein Arm vom Seile glitt; \u2013", "tokens": ["Bis", "m\u00fcd'", "mein", "Arm", "vom", "Sei\u00b7le", "glitt", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "VMFIN", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein eigen Selbst hatt' ich vergessen.", "tokens": ["Mein", "ei\u00b7gen", "Selbst", "hatt'", "ich", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.19": {"line.1": {"text": "Doch ohne Klage will ich tragen", "tokens": ["Doch", "oh\u00b7ne", "Kla\u00b7ge", "will", "ich", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Leid, das meine Brust verschlie\u00dft,", "tokens": ["Das", "Leid", ",", "das", "mei\u00b7ne", "Brust", "ver\u00b7schlie\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn Gl\u00fcck und Ruhm die\u00df Land umflie\u00dft", "tokens": ["Wenn", "Gl\u00fcck", "und", "Ruhm", "die\u00df", "Land", "um\u00b7flie\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "PDS", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dr\u00fcber hell're Sterne tagen.", "tokens": ["Und", "dr\u00fc\u00b7ber", "hell'\u00b7re", "Ster\u00b7ne", "ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}