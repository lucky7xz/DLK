{"textgrid.poem.59405": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Pilot", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Glaube stehet still erhoben", "tokens": ["Glau\u00b7be", "ste\u00b7het", "still", "er\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcberm n\u00e4cht'gen Wellenklang,", "tokens": ["\u00dc\u00b7berm", "n\u00e4cht'\u00b7gen", "Wel\u00b7len\u00b7klang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lieset in den Sternen droben", "tokens": ["Lie\u00b7set", "in", "den", "Ster\u00b7nen", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fromm des Schiffleins sichern Gang.", "tokens": ["Fromm", "des", "Schif\u00b7fleins", "si\u00b7chern", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Liebe schwellet sanft die Segel,", "tokens": ["Lie\u00b7be", "schwel\u00b7let", "sanft", "die", "Se\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "D\u00e4mmernd zwischen Tag und Nacht", "tokens": ["D\u00e4m\u00b7mernd", "zwi\u00b7schen", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schweifen Paradiesesv\u00f6gel,", "tokens": ["Schwei\u00b7fen", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7v\u00f6\u00b7gel", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob der Morgen bald erwacht?", "tokens": ["Ob", "der", "Mor\u00b7gen", "bald", "er\u00b7wacht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Morgen will sich k\u00fchn entz\u00fcnden,", "tokens": ["Mor\u00b7gen", "will", "sich", "k\u00fchn", "ent\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PRF", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun wird's mir auf einmal kund:", "tokens": ["Nun", "wird's", "mir", "auf", "ein\u00b7mal", "kund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ADV", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hoffnung wird die Heimat finden", "tokens": ["Hoff\u00b7nung", "wird", "die", "Hei\u00b7mat", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und den stillen Ankergrund.", "tokens": ["Und", "den", "stil\u00b7len", "An\u00b7ker\u00b7grund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}