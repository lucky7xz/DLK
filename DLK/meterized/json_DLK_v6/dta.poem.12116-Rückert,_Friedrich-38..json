{"dta.poem.12116": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "38.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich gieng, die Gegenden zu sehn, die auch mich freuten;", "tokens": ["Ich", "gieng", ",", "die", "Ge\u00b7gen\u00b7den", "zu", "sehn", ",", "die", "auch", "mich", "freu\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch mehr als ich gedacht, labt' ich mich an den Leuten.", "tokens": ["Doch", "mehr", "als", "ich", "ge\u00b7dacht", ",", "labt'", "ich", "mich", "an", "den", "Leu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "PPER", "VVPP", "$,", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die m\u00e4chtige Natur tritt in den Hintergrund", "tokens": ["Die", "m\u00e4ch\u00b7ti\u00b7ge", "Na\u00b7tur", "tritt", "in", "den", "Hin\u00b7ter\u00b7grund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor den Bewohnern sch\u00f6n, treu, t\u00fcchtig, kerngesund.", "tokens": ["Vor", "den", "Be\u00b7woh\u00b7nern", "sch\u00f6n", ",", "treu", ",", "t\u00fcch\u00b7tig", ",", "kern\u00b7ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Das Landschaftbild ist nicht die h\u00f6chste Malerei;", "tokens": ["Das", "Land\u00b7schaft\u00b7bild", "ist", "nicht", "die", "h\u00f6chs\u00b7te", "Ma\u00b7le\u00b7rei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich wei\u00df nun, da\u00df der Mensch das Kunstwerk Gottes sei.", "tokens": ["Ich", "wei\u00df", "nun", ",", "da\u00df", "der", "Mensch", "das", "Kunst\u00b7werk", "Got\u00b7tes", "sei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "ART", "NN", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}