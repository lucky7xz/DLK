{"textgrid.poem.49518": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Aglaja an Athamas", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie lange willstu dich betr\u00fcben?", "tokens": ["Wie", "lan\u00b7ge", "will\u00b7stu", "dich", "be\u00b7tr\u00fc\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich f\u00fchle ja f\u00fcr dich der Liebe st\u00e4rcksten Zug.", "tokens": ["Ich", "f\u00fch\u00b7le", "ja", "f\u00fcr", "dich", "der", "Lie\u00b7be", "st\u00e4rcks\u00b7ten", "Zug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du bleibst auch meine Lust bey Kohl und R\u00fcben,", "tokens": ["Du", "bleibst", "auch", "mei\u00b7ne", "Lust", "bey", "Kohl", "und", "R\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Stolz im Bauernrock, mein F\u00fcrst beym Pflug.", "tokens": ["Mein", "Stolz", "im", "Bau\u00b7ern\u00b7rock", ",", "mein", "F\u00fcrst", "beym", "Pflug", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Lieb' ich dich, Freund, denn nicht genug?", "tokens": ["Lieb'", "ich", "dich", ",", "Freund", ",", "denn", "nicht", "ge\u00b7nug", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "NN", "$,", "ADV", "PTKNEG", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Mein Hertz, o Freundin, sagt, du k\u00f6nnest st\u00e4rcker lieben;", "tokens": ["Mein", "Hertz", ",", "o", "Freun\u00b7din", ",", "sagt", ",", "du", "k\u00f6n\u00b7nest", "st\u00e4r\u00b7cker", "lie\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "NN", "$,", "VVFIN", "$,", "PPER", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du liebst mich nicht genug \u2013 \u2013,", "tokens": ["Du", "liebst", "mich", "nicht", "ge\u00b7nug", "\u2013", "\u2013", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$(", "$(", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu Tode werd ich mich betr\u00fcben!", "tokens": ["Zu", "To\u00b7de", "werd", "ich", "mich", "be\u00b7tr\u00fc\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}