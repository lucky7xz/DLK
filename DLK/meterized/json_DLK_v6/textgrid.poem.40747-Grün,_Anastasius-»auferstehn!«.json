{"textgrid.poem.40747": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "\u00bbauferstehn!\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Grau umflort schleicht die Novemberwolke,", "tokens": ["Grau", "um\u00b7flort", "schleicht", "die", "No\u00b7vem\u00b7ber\u00b7wol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nebeldunst umqualmt die welke Flur;", "tokens": ["Ne\u00b7bel\u00b7dunst", "um\u00b7qualmt", "die", "wel\u00b7ke", "Flur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "W\u00e4re nicht schon Trauer g'nug im Volke,", "tokens": ["W\u00e4\u00b7re", "nicht", "schon", "Trau\u00b7er", "g'\u00b7nug", "im", "Vol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Trauern lehrt' es heute die Natur.", "tokens": ["Trau\u00b7ern", "lehrt'", "es", "heu\u00b7te", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wimmernd l\u00e4uten jetzt die Glockenkl\u00e4nge", "tokens": ["Wim\u00b7mernd", "l\u00e4u\u00b7ten", "jetzt", "die", "Glo\u00b7cken\u00b7kl\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aus dem Dorf den Tag der Todten ein,", "tokens": ["Aus", "dem", "Dorf", "den", "Tag", "der", "Tod\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nach dem Friedhofgarten zieht die Menge,", "tokens": ["Nach", "dem", "Fried\u00b7hof\u00b7gar\u00b7ten", "zieht", "die", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Blumen tr\u00e4gt jetzt nur der Leichenstein.", "tokens": ["Blu\u00b7men", "tr\u00e4gt", "jetzt", "nur", "der", "Lei\u00b7chen\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "In der Gr\u00e4berzeil' am letzten Raine", "tokens": ["In", "der", "Gr\u00e4\u00b7ber\u00b7zeil'", "am", "letz\u00b7ten", "Rai\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ist ein frisch Soldatengrab zu schaun,", "tokens": ["Ist", "ein", "frisch", "Sol\u00b7da\u00b7ten\u00b7grab", "zu", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ein zerbrochnes Schwert ist roh dem Steine", "tokens": ["Ein", "zer\u00b7broch\u00b7nes", "Schwert", "ist", "roh", "dem", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Neben einer Trommel eingehaun.", "tokens": ["Ne\u00b7ben", "ei\u00b7ner", "Trom\u00b7mel", "ein\u00b7ge\u00b7haun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Ja, der Tambour ist, die Inschrift sagt es,", "tokens": ["Ja", ",", "der", "Tam\u00b7bour", "ist", ",", "die", "In\u00b7schrift", "sagt", "es", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "$,", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wieder hier in Reih' und Glied ger\u00fcckt,", "tokens": ["Wie\u00b7der", "hier", "in", "Reih'", "und", "Glied", "ge\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Seine Wirbel schweigen, \u2013 wer beklagt es,", "tokens": ["Sei\u00b7ne", "Wir\u00b7bel", "schwei\u00b7gen", ",", "\u2013", "wer", "be\u00b7klagt", "es", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,", "$(", "PWS", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Den ein Leid, wie unsres, niederdr\u00fcckt!", "tokens": ["Den", "ein", "Leid", ",", "wie", "uns\u00b7res", ",", "nie\u00b7der\u00b7dr\u00fcckt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ART", "NN", "$,", "PWAV", "PIS", "$,", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Denn ein Trauerfest der Allerseelen", "tokens": ["Denn", "ein", "Trau\u00b7er\u00b7fest", "der", "Al\u00b7ler\u00b7see\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Soll die\u00df Jahr im ganzen Lande sein,", "tokens": ["Soll", "die\u00df", "Jahr", "im", "gan\u00b7zen", "Lan\u00b7de", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "NN", "APPRART", "ADJA", "NN", "VAINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo an jedem Herd viel Liebe fehlen", "tokens": ["Wo", "an", "je\u00b7dem", "Herd", "viel", "Lie\u00b7be", "feh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PIAT", "NN", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und nur Kr\u00e4nze tr\u00e4gt das Grab allein.", "tokens": ["Und", "nur", "Kr\u00e4n\u00b7ze", "tr\u00e4gt", "das", "Grab", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Sohn und Sohneskinder stehn am H\u00fcgel,", "tokens": ["Sohn", "und", "Soh\u00b7nes\u00b7kin\u00b7der", "stehn", "am", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Weihn ein \u00bbVater unser\u00ab still dem Greis,", "tokens": ["Weihn", "ein", "\u00bb", "Va\u00b7ter", "un\u00b7ser", "\u00ab", "still", "dem", "Greis", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "$(", "NN", "PPOSAT", "$(", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ihre Seelen r\u00fchrt's wie Todesfl\u00fcgel,", "tokens": ["Ih\u00b7re", "See\u00b7len", "r\u00fchrt's", "wie", "To\u00b7des\u00b7fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und der Vater spricht im Kinderkreis:", "tokens": ["Und", "der", "Va\u00b7ter", "spricht", "im", "Kin\u00b7der\u00b7kreis", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "\u00bbjene Kugeln fern im B\u00f6hmerlande", "tokens": ["\u00bb", "je\u00b7ne", "Ku\u00b7geln", "fern", "im", "B\u00f6h\u00b7mer\u00b7lan\u00b7de"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PDAT", "NN", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schlugen hier zugleich ins treuste Herz;", "tokens": ["Schlu\u00b7gen", "hier", "zu\u00b7gleich", "ins", "treus\u00b7te", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Br\u00fcder gegen Br\u00fcder! Schmach und Schande!", "tokens": ["Br\u00fc\u00b7der", "ge\u00b7gen", "Br\u00fc\u00b7der", "!", "Schmach", "und", "Schan\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$.", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ueberleben konnt' er nicht den Schmerz.", "tokens": ["Ue\u00b7ber\u00b7le\u00b7ben", "konnt'", "er", "nicht", "den", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Und mir war's, als sollten wir begraben", "tokens": ["Und", "mir", "wa\u00b7r's", ",", "als", "soll\u00b7ten", "wir", "be\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "$,", "KOUS", "VMFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dich, Alt\u00f6sterreich, zur selben Stund!", "tokens": ["Dich", ",", "Al\u00b7t\u00f6s\u00b7ter\u00b7reich", ",", "zur", "sel\u00b7ben", "Stund", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJD", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Doch zu fr\u00fch umkr\u00e4chzten dich die Raben,", "tokens": ["Doch", "zu", "fr\u00fch", "um\u00b7kr\u00e4chz\u00b7ten", "dich", "die", "Ra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKA", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Als gesunken du auf blut'gen Grund.", "tokens": ["Als", "ge\u00b7sun\u00b7ken", "du", "auf", "blut'\u00b7gen", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Denn das blieb dein Loos, da\u00df stets im Fallen", "tokens": ["Denn", "das", "blieb", "dein", "Loos", ",", "da\u00df", "stets", "im", "Fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dir zu neuem Schwung der Fittig sprie\u00dft;", "tokens": ["Dir", "zu", "neu\u00b7em", "Schwung", "der", "Fit\u00b7tig", "sprie\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df wenn sich des Unheils Wetter ballen,", "tokens": ["Da\u00df", "wenn", "sich", "des", "Un\u00b7heils", "Wet\u00b7ter", "bal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PRF", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Deiner Saat nur milder Regen flie\u00dft.", "tokens": ["Dei\u00b7ner", "Saat", "nur", "mil\u00b7der", "Re\u00b7gen", "flie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Willst du danken stets nur der Bedr\u00e4ngni\u00df,", "tokens": ["Willst", "du", "dan\u00b7ken", "stets", "nur", "der", "Be\u00b7dr\u00e4ng\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Zittern stets vor l\u00e4chelndem Geschick?", "tokens": ["Zit\u00b7tern", "stets", "vor", "l\u00e4\u00b7cheln\u00b7dem", "Ge\u00b7schick", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ei so schmiede selber dein Verh\u00e4ngni\u00df,", "tokens": ["Ei", "so", "schmie\u00b7de", "sel\u00b7ber", "dein", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sei dir selbst die Kraft, dir selbst das Gl\u00fcck!", "tokens": ["Sei", "dir", "selbst", "die", "Kraft", ",", "dir", "selbst", "das", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "$,", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Hast ein helles Aug, ins All zu wandern,", "tokens": ["Hast", "ein", "hel\u00b7les", "Aug", ",", "ins", "All", "zu", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,", "APPRART", "NN", "PTKZU", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hast zu Werk und Kampf gar r\u00fcst'gen Arm;", "tokens": ["Hast", "zu", "Werk", "und", "Kampf", "gar", "r\u00fcst'\u00b7gen", "Arm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "KON", "NN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Tr\u00e4gst im Haupt ein Licht auch, wie die Andern,", "tokens": ["Tr\u00e4gst", "im", "Haupt", "ein", "Licht", "auch", ",", "wie", "die", "An\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "ADV", "$,", "PWAV", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hegst ein Herz, wie Wen'ge, frisch und warm.", "tokens": ["Hegst", "ein", "Herz", ",", "wie", "Wen'\u00b7ge", ",", "frisch", "und", "warm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PWAV", "PIS", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Sieh dein Land von Zauberhauch umquollen,", "tokens": ["Sieh", "dein", "Land", "von", "Zau\u00b7ber\u00b7hauch", "um\u00b7quol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sieh dein Volk von jugendfreud'gem Flug;", "tokens": ["Sieh", "dein", "Volk", "von", "ju\u00b7gend\u00b7freu\u00b7d'\u00b7gem", "Flug", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Lasse tief durch Seelen und durch Schollen", "tokens": ["Las\u00b7se", "tief", "durch", "See\u00b7len", "und", "durch", "Schol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPR", "NN", "KON", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Furchen ziehn des Geistes Fl\u00fcgelpflug!", "tokens": ["Fur\u00b7chen", "ziehn", "des", "Geis\u00b7tes", "Fl\u00fc\u00b7gel\u00b7pflug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "Rastlos g\u00e4hrt und dr\u00e4ngt im Erdenboden", "tokens": ["Rast\u00b7los", "g\u00e4hrt", "und", "dr\u00e4ngt", "im", "Er\u00b7den\u00b7bo\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "K