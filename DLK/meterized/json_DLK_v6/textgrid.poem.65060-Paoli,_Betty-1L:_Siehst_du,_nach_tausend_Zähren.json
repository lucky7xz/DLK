{"textgrid.poem.65060": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Siehst du, nach tausend Z\u00e4hren", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siehst du, nach tausend Z\u00e4hren", "tokens": ["Siehst", "du", ",", "nach", "tau\u00b7send", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein selig L\u00e4cheln mir", "tokens": ["Ein", "se\u00b7lig", "L\u00e4\u00b7cheln", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "PPER"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Das Angesicht verkl\u00e4ren,", "tokens": ["Das", "An\u00b7ge\u00b7sicht", "ver\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So gilt die\u00df L\u00e4cheln dir!", "tokens": ["So", "gilt", "die\u00df", "L\u00e4\u00b7cheln", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDS", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und wenn mein Herz, mein wundes,", "tokens": ["Und", "wenn", "mein", "Herz", ",", "mein", "wun\u00b7des", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In banger Sehnsucht schwillt,", "tokens": ["In", "ban\u00b7ger", "Sehn\u00b7sucht", "schwillt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bist du's, dem meines Mundes", "tokens": ["Bist", "du's", ",", "dem", "mei\u00b7nes", "Mun\u00b7des"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PIS", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tief inn'ger Seufzer gilt.", "tokens": ["Tief", "inn'\u00b7ger", "Seuf\u00b7zer", "gilt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}