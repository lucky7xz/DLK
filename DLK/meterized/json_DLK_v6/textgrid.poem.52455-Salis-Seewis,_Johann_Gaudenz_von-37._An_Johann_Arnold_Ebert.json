{"textgrid.poem.52455": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "37. An Johann Arnold Ebert", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein milder Blick fiel, gleich dem Abendstrahle,", "tokens": ["Dein", "mil\u00b7der", "Blick", "fiel", ",", "gleich", "dem", "A\u00b7bend\u00b7strah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der dort auf Alpenbl\u00fcmchen Labung streut,", "tokens": ["Der", "dort", "auf", "Al\u00b7pen\u00b7bl\u00fcm\u00b7chen", "La\u00b7bung", "streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf meine Laute. \u2013 Freund, die Wehmut freut", "tokens": ["Auf", "mei\u00b7ne", "Lau\u00b7te", ".", "\u2013", "Freund", ",", "die", "Weh\u00b7mut", "freut"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "$(", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Kranzes sich um ihre Opferschale.", "tokens": ["Des", "Kran\u00b7zes", "sich", "um", "ih\u00b7re", "Op\u00b7fer\u00b7scha\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vertrauter Youngs! mich rief zum Heimatsthale", "tokens": ["Ver\u00b7trau\u00b7ter", "Y\u00b7o\u00b7ungs", "!", "mich", "rief", "zum", "Hei\u00b7mat\u00b7stha\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "$.", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Genius gerechter Menschlichkeit;", "tokens": ["Der", "Ge\u00b7nius", "ge\u00b7rech\u00b7ter", "Menschlich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis ihm die Freiheit ihre Lorbeern beut,", "tokens": ["Bis", "ihm", "die", "Frei\u00b7heit", "ih\u00b7re", "Lor\u00b7beern", "beut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So lange fei'r ich mit gesenktem Stahle.", "tokens": ["So", "lan\u00b7ge", "fei'r", "ich", "mit", "ge\u00b7senk\u00b7tem", "Stah\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Gleichheit Bild verehrt ein V\u00f6lkchen Hirten:", "tokens": ["Der", "Gleich\u00b7heit", "Bild", "ver\u00b7ehrt", "ein", "V\u00f6lk\u00b7chen", "Hir\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein Bundesbaum gr\u00fcnt still am jungen Rhein;", "tokens": ["Sein", "Bun\u00b7des\u00b7baum", "gr\u00fcnt", "still", "am", "jun\u00b7gen", "Rhein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "APPRART", "ADJA", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da h\u00fcllt ein sanftes Weib mein Schwert in Myrten.", "tokens": ["Da", "h\u00fcllt", "ein", "sanf\u00b7tes", "Weib", "mein", "Schwert", "in", "Myr\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein Laubdach birgt uns; d\u00fcrft' ich dich bewirten,", "tokens": ["Ein", "Laub\u00b7dach", "birgt", "uns", ";", "d\u00fcrft'", "ich", "dich", "be\u00b7wir\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "W\u00fcrd' es mir hehr wie Wingolfs Halle sein...", "tokens": ["W\u00fcrd'", "es", "mir", "hehr", "wie", "Win\u00b7golfs", "Hal\u00b7le", "sein", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "KOKOM", "NN", "NE", "PPOSAT", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu s\u00fc\u00dfer Traum! La\u00df mich im Wehmutshain.", "tokens": ["Zu", "s\u00fc\u00b7\u00dfer", "Traum", "!", "La\u00df", "mich", "im", "Weh\u00b7muts\u00b7hain", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "VVIMP", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}