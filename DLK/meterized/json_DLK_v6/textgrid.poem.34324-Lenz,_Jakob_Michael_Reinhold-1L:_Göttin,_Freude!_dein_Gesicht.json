{"textgrid.poem.34324": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: G\u00f6ttin, Freude! dein Gesicht", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "G\u00f6ttin, Freude! dein Gesicht", "tokens": ["G\u00f6t\u00b7tin", ",", "Freu\u00b7de", "!", "dein", "Ge\u00b7sicht"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "NN", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wendest du vom Edlen nicht.", "tokens": ["Wen\u00b7dest", "du", "vom", "Ed\u00b7len", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn er dich verschwunden meynet,", "tokens": ["Wenn", "er", "dich", "ver\u00b7schwun\u00b7den", "mey\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bist du n\u00e4her als es scheinet,", "tokens": ["Bist", "du", "n\u00e4\u00b7her", "als", "es", "schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KOKOM", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Stehst mit deinem Ungest\u00fcmm", "tokens": ["Stehst", "mit", "dei\u00b7nem", "Un\u00b7ge\u00b7st\u00fcmm"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Des Entz\u00fckens hinter ihm.", "tokens": ["Des", "Ent\u00b7z\u00fc\u00b7kens", "hin\u00b7ter", "ihm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach er klagte, wie verloren,", "tokens": ["Ach", "er", "klag\u00b7te", ",", "wie", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "$,", "PWAV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df er nicht f\u00fcr dich geboren,", "tokens": ["Da\u00df", "er", "nicht", "f\u00fcr", "dich", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df du ihm noch nie gelacht,", "tokens": ["Da\u00df", "du", "ihm", "noch", "nie", "ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil er nicht f\u00fcr dich gemacht.", "tokens": ["Weil", "er", "nicht", "f\u00fcr", "dich", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, er fluchte dem Geschicke!", "tokens": ["Ach", ",", "er", "fluch\u00b7te", "dem", "Ge\u00b7schi\u00b7cke", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mit allem deinem Gl\u00fccke,", "tokens": ["Und", "mit", "al\u00b7lem", "dei\u00b7nem", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Deiner Wonne Ungest\u00fcmm", "tokens": ["Dei\u00b7ner", "Won\u00b7ne", "Un\u00b7ge\u00b7st\u00fcmm"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Stehst du, G\u00f6ttin, hinter ihm.", "tokens": ["Stehst", "du", ",", "G\u00f6t\u00b7tin", ",", "hin\u00b7ter", "ihm", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Da verwandeln denn die Z\u00e4hren,", "tokens": ["Da", "ver\u00b7wan\u00b7deln", "denn", "die", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die dem Schmerz zu kostbar w\u00e4ren,", "tokens": ["Die", "dem", "Schmerz", "zu", "kost\u00b7bar", "w\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PTKA", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Freude Ausdruck sich.", "tokens": ["In", "der", "Freu\u00b7de", "Aus\u00b7druck", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "O da schreyt, da schluchzt er dich!", "tokens": ["O", "da", "schreyt", ",", "da", "schluchzt", "er", "dich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mit aller Wuth der Schmerzen", "tokens": ["Und", "mit", "al\u00b7ler", "Wuth", "der", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tobest du in seinem Herzen,", "tokens": ["To\u00b7best", "du", "in", "sei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bis voll s\u00fcsser Mattigkeit", "tokens": ["Bis", "voll", "s\u00fcs\u00b7ser", "Mat\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Er es f\u00fchlt, da\u00df er sich freut.", "tokens": ["Er", "es", "f\u00fchlt", ",", "da\u00df", "er", "sich", "freut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}