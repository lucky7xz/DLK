{"textgrid.poem.41539": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am Ostermontage", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr, er\u00f6ffne mir die Schrift,", "tokens": ["Herr", ",", "er\u00b7\u00f6ff\u00b7ne", "mir", "die", "Schrift", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner Worte Liebesmorgen,", "tokens": ["Dei\u00b7ner", "Wor\u00b7te", "Lie\u00b7bes\u00b7mor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df er leis im Herzen trifft,", "tokens": ["Da\u00df", "er", "leis", "im", "Her\u00b7zen", "trifft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was gewi\u00dflich drin verborgen.", "tokens": ["Was", "ge\u00b7wi\u00df\u00b7lich", "drin", "ver\u00b7bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wei\u00df es selber nicht zu finden,", "tokens": ["Wei\u00df", "es", "sel\u00b7ber", "nicht", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bin doch aller Hoffnung voll,", "tokens": ["Bin", "doch", "al\u00b7ler", "Hoff\u00b7nung", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "O, die Wolken werden schwinden!", "tokens": ["O", ",", "die", "Wol\u00b7ken", "wer\u00b7den", "schwin\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn die Sonne scheinen soll.", "tokens": ["Wenn", "die", "Son\u00b7ne", "schei\u00b7nen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Soll der Glaube ferne sein?", "tokens": ["Soll", "der", "Glau\u00b7be", "fer\u00b7ne", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da die Liebe nicht verloren!", "tokens": ["Da", "die", "Lie\u00b7be", "nicht", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da in N\u00e4chten stiller Pein", "tokens": ["Da", "in", "N\u00e4ch\u00b7ten", "stil\u00b7ler", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir die Hoffnung neu geboren!", "tokens": ["Mir", "die", "Hoff\u00b7nung", "neu", "ge\u00b7bo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Du, mein Gott der Huld und Treue,", "tokens": ["Du", ",", "mein", "Gott", "der", "Huld", "und", "Treu\u00b7e", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Den des W\u00fcrmleins Kr\u00fcmmen r\u00fchrt,", "tokens": ["Den", "des", "W\u00fcrm\u00b7leins", "Kr\u00fcm\u00b7men", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "H\u00e4ttest du umsonst die Reue", "tokens": ["H\u00e4t\u00b7test", "du", "um\u00b7sonst", "die", "Reu\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In dies starre Herz gef\u00fchrt!", "tokens": ["In", "dies", "star\u00b7re", "Herz", "ge\u00b7f\u00fchrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nein, mein Herr, das hast du nicht,", "tokens": ["Nein", ",", "mein", "Herr", ",", "das", "hast", "du", "nicht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "$,", "PDS", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine Seelen sind dir teuer;", "tokens": ["Dei\u00b7ne", "See\u00b7len", "sind", "dir", "teu\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo nur noch ein F\u00fcnklein spricht,", "tokens": ["Wo", "nur", "noch", "ein", "F\u00fcn\u00b7klein", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Nahst du gern mit deinem Feuer.", "tokens": ["Nahst", "du", "gern", "mit", "dei\u00b7nem", "Feu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "O, ich f\u00fchl' es wohl, wie leise", "tokens": ["O", ",", "ich", "f\u00fchl'", "es", "wohl", ",", "wie", "lei\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,", "PWAV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich das neue Leben regt,", "tokens": ["Sich", "das", "neu\u00b7e", "Le\u00b7ben", "regt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "An der Gnade zarte Speise", "tokens": ["An", "der", "Gna\u00b7de", "zar\u00b7te", "Spei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seine schwachen Lippen legt.", "tokens": ["Sei\u00b7ne", "schwa\u00b7chen", "Lip\u00b7pen", "legt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Manches ist mir wunderbar,", "tokens": ["Man\u00b7ches", "ist", "mir", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Manches mu\u00df mir dunkel scheinen,", "tokens": ["Man\u00b7ches", "mu\u00df", "mir", "dun\u00b7kel", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch in deiner Liebe klar", "tokens": ["Doch", "in", "dei\u00b7ner", "Lie\u00b7be", "klar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird sich alles freudig einen.", "tokens": ["Wird", "sich", "al\u00b7les", "freu\u00b7dig", "ei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "PIS", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "War der Nebel nur des B\u00f6sen,", "tokens": ["War", "der", "Ne\u00b7bel", "nur", "des", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was als Nacht mich zagen lie\u00df:", "tokens": ["Was", "als", "Nacht", "mich", "za\u00b7gen", "lie\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "KOUS", "NN", "PPER", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie sich meine S\u00fcnden l\u00f6sen,", "tokens": ["Wie", "sich", "mei\u00b7ne", "S\u00fcn\u00b7den", "l\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Tret' ich aus der Finsternis.", "tokens": ["Tret'", "ich", "aus", "der", "Fins\u00b7ter\u00b7nis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Herr, mit Tr\u00e4nen dank' ich dir", "tokens": ["Herr", ",", "mit", "Tr\u00e4\u00b7nen", "dank'", "ich", "dir"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "NN", "VVFIN", "PPER", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcr dein \u00fcbergn\u00e4dig Walten,", "tokens": ["F\u00fcr", "dein", "\u00fc\u00b7berg\u00b7n\u00e4\u00b7dig", "Wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df du deinen Glauben mir", "tokens": ["Da\u00df", "du", "dei\u00b7nen", "Glau\u00b7ben", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In der S\u00fcnde vorenthalten:", "tokens": ["In", "der", "S\u00fcn\u00b7de", "vor\u00b7ent\u00b7hal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, ich h\u00e4tte wie im Grimme", "tokens": ["Ach", ",", "ich", "h\u00e4t\u00b7te", "wie", "im", "Grim\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "KOKOM", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Neue Frevel nur ersp\u00e4ht!", "tokens": ["Neu\u00b7e", "Fre\u00b7vel", "nur", "er\u00b7sp\u00e4ht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bis mir des Gewissens Stimme", "tokens": ["Bis", "mir", "des", "Ge\u00b7wis\u00b7sens", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Von dem Sturme \u00fcberweht.", "tokens": ["Von", "dem", "Stur\u00b7me", "\u00fc\u00b7ber\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Deine Gnad' ist weich und warm,", "tokens": ["Dei\u00b7ne", "Gnad'", "ist", "weich", "und", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mag der Sorgfalt nicht entbehren,", "tokens": ["Mag", "der", "Sorg\u00b7falt", "nicht", "ent\u00b7beh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mein Herz war kalt und arm,", "tokens": ["Und", "mein", "Herz", "war", "kalt", "und", "arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Solchen zarten Gast zu n\u00e4hren,", "tokens": ["Sol\u00b7chen", "zar\u00b7ten", "Gast", "zu", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber wie die Quellen springen,", "tokens": ["A\u00b7ber", "wie", "die", "Quel\u00b7len", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Losgerissen von dem Weh,", "tokens": ["Los\u00b7ge\u00b7ris\u00b7sen", "von", "dem", "Weh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Taucht sie sich mit milden Schwingen", "tokens": ["Taucht", "sie", "sich", "mit", "mil\u00b7den", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In den hei\u00dfen roten See.", "tokens": ["In", "den", "hei\u00b7\u00dfen", "ro\u00b7ten", "See", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Herr, ich habe viel geweint,", "tokens": ["Herr", ",", "ich", "ha\u00b7be", "viel", "ge\u00b7weint", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich oft wie zu zergehen,", "tokens": ["Da\u00df", "ich", "oft", "wie", "zu", "zer\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "KOKOM", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Seelennot gemeint,", "tokens": ["In", "der", "See\u00b7len\u00b7not", "ge\u00b7meint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wie ist mir heut geschehen?", "tokens": ["Und", "wie", "ist", "mir", "heut", "ge\u00b7sche\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ich gar so voll der Freuden,", "tokens": ["Da\u00df", "ich", "gar", "so", "voll", "der", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mich keine Angst bezwingt,", "tokens": ["Und", "mich", "kei\u00b7ne", "Angst", "be\u00b7zwingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ob mir gleich das alte Leiden", "tokens": ["Ob", "mir", "gleich", "das", "al\u00b7te", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Riesig an die Seele dringt.", "tokens": ["Rie\u00b7sig", "an", "die", "See\u00b7le", "dringt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und bei deinem heil'gen Buch,", "tokens": ["Und", "bei", "dei\u00b7nem", "heil'\u00b7gen", "Buch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was mir heute fast wie offen,", "tokens": ["Was", "mir", "heu\u00b7te", "fast", "wie", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "KOKOM", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denk' ich keinen einz'gen Fluch,", "tokens": ["Denk'", "ich", "kei\u00b7nen", "einz'\u00b7gen", "Fluch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann nur lieben, kann nur hoffen,", "tokens": ["Kann", "nur", "lie\u00b7ben", ",", "kann", "nur", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVINF", "$,", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seh dich nur als Kindlein neigen,", "tokens": ["Seh", "dich", "nur", "als", "Kin\u00b7dlein", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KOUS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alles lieblich, alles lind,", "tokens": ["Al\u00b7les", "lieb\u00b7lich", ",", "al\u00b7les", "lind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "ADJD", "$,", "PIS", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Deine harten Worte schweigen,", "tokens": ["Dei\u00b7ne", "har\u00b7ten", "Wor\u00b7te", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und ich wei\u00df nicht, wo sie sind.", "tokens": ["Und", "ich", "wei\u00df", "nicht", ",", "wo", "sie", "sind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Das ist nur f\u00fcr diesen Tag,", "tokens": ["Das", "ist", "nur", "f\u00fcr", "die\u00b7sen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "O, viel anders wird es kommen!", "tokens": ["O", ",", "viel", "an\u00b7ders", "wird", "es", "kom\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn zu gro\u00df ist meine Schmach,", "tokens": ["Denn", "zu", "gro\u00df", "ist", "mei\u00b7ne", "Schmach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKA", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Solche Lust kann ihr nicht frommen,", "tokens": ["Sol\u00b7che", "Lust", "kann", "ihr", "nicht", "from\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "PTKNEG", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hast nur deinen Blitz gesendet,", "tokens": ["Hast", "nur", "dei\u00b7nen", "Blitz", "ge\u00b7sen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df nicht irr in meiner Pein", "tokens": ["Da\u00df", "nicht", "irr", "in", "mei\u00b7ner", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ich mich wieder zugewendet", "tokens": ["Ich", "mich", "wie\u00b7der", "zu\u00b7ge\u00b7wen\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PRF", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dem verla\u00dfnen G\u00f6tzenhain.", "tokens": ["Dem", "ver\u00b7la\u00df\u00b7nen", "G\u00f6t\u00b7zen\u00b7hain", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Du unendlich s\u00fc\u00dfes Gl\u00fcck!", "tokens": ["Du", "un\u00b7end\u00b7lich", "s\u00fc\u00b7\u00dfes", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df ich wieder dich verlieren,", "tokens": ["Mu\u00df", "ich", "wie\u00b7der", "dich", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df mir nur dein Bild zur\u00fcck,", "tokens": ["La\u00df", "mir", "nur", "dein", "Bild", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem Grolle mich zu r\u00fchren,", "tokens": ["In", "dem", "Grol\u00b7le", "mich", "zu", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Oder, Herr, soll dieser Stunde", "tokens": ["O\u00b7der", ",", "Herr", ",", "soll", "die\u00b7ser", "Stun\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "VMFIN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00dcberschwenglich Heil erstehn,", "tokens": ["\u00dc\u00b7bersc\u00b7hweng\u00b7lich", "Heil", "er\u00b7stehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "O! so la\u00df des Grolles Wunde", "tokens": ["O", "!", "so", "la\u00df", "des", "Grol\u00b7les", "Wun\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VVIMP", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mir als Trauer offen gehn!", "tokens": ["Mir", "als", "Trau\u00b7er", "of\u00b7fen", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}