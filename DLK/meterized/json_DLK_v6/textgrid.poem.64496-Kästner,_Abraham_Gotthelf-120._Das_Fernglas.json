{"textgrid.poem.64496": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "120. Das Fernglas", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kannst du dir nicht das Fernglas abgew\u00f6hnen,", "tokens": ["Kannst", "du", "dir", "nicht", "das", "Fern\u00b7glas", "ab\u00b7ge\u00b7w\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKNEG", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So richt' es nur nicht nach ", "tokens": ["So", "richt'", "es", "nur", "nicht", "nach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie glaubt, sie wird dadurch entehrt,", "tokens": ["Sie", "glaubt", ",", "sie", "wird", "da\u00b7durch", "ent\u00b7ehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch ist sie ja der M\u00fch' nicht werth.", "tokens": ["Auch", "ist", "sie", "ja", "der", "M\u00fch'", "nicht", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}