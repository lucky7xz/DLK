{"dta.poem.12005": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.85", "fr:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du siehst, Unsichtbarer, du h\u00f6rest, Unvernommner!", "tokens": ["Du", "siehst", ",", "Un\u00b7sicht\u00b7ba\u00b7rer", ",", "du", "h\u00f6\u00b7rest", ",", "Un\u00b7ver\u00b7nomm\u00b7ner", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "PPER", "VVFIN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sehn, h\u00f6ren wird durch dich vollkommen, Allvollkommner.", "tokens": ["Sehn", ",", "h\u00f6\u00b7ren", "wird", "durch", "dich", "voll\u00b7kom\u00b7men", ",", "All\u00b7voll\u00b7komm\u00b7ner", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVINF", "VAFIN", "APPR", "PPER", "ADJD", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du Unverg\u00e4nglichkeit, Verg\u00e4nglichem inwohnend,", "tokens": ["Du", "Un\u00b7ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", ",", "Ver\u00b7g\u00e4ng\u00b7li\u00b7chem", "in\u00b7woh\u00b7nend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Uranf\u00e4nglichkeit, hoch \u00fcberm Wechsel thronend.", "tokens": ["Und", "U\u00b7ran\u00b7f\u00e4ng\u00b7lich\u00b7keit", ",", "hoch", "\u00fc\u00b7berm", "Wech\u00b7sel", "thro\u00b7nend", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Seelen Seele du, Gedanke der Gedanken,", "tokens": ["Der", "See\u00b7len", "See\u00b7le", "du", ",", "Ge\u00b7dan\u00b7ke", "der", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Umfa\u00dft von keines Raums und keines Denkens Schranken.", "tokens": ["Um\u00b7fa\u00dft", "von", "kei\u00b7nes", "Raums", "und", "kei\u00b7nes", "Den\u00b7kens", "Schran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "KON", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dir geht die Wissenschaft vorbei auf dunklen Bahnen,", "tokens": ["Dir", "geht", "die", "Wis\u00b7sen\u00b7schaft", "vor\u00b7bei", "auf", "dunk\u00b7len", "Bah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und um dein Urlicht schwebt der Andacht sel'ges Ahnen.", "tokens": ["Und", "um", "dein", "Ur\u00b7licht", "schwebt", "der", "An\u00b7dacht", "sel'\u00b7ges", "Ah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}