{"textgrid.poem.53223": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du Gesegneter des Herren,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Gesegneter des Herren,", "tokens": ["Du", "Ge\u00b7se\u00b7gne\u00b7ter", "des", "Her\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Komm, zeuch gn\u00e4dig ein! wir sperren", "tokens": ["Komm", ",", "zeuch", "gn\u00e4\u00b7dig", "ein", "!", "wir", "sper\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "VVIMP", "ADJD", "PTKVZ", "$.", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Th\u00f6r und Hertzen Dir weit auff,", "tokens": ["Th\u00f6r", "und", "Hert\u00b7zen", "Dir", "weit", "auff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Komm, Dein Preussen kompt zu hauff,", "tokens": ["Komm", ",", "Dein", "Preus\u00b7sen", "kompt", "zu", "hauff", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "W\u00fcnschet Deiner Herrschaft Segen:", "tokens": ["W\u00fcn\u00b7schet", "Dei\u00b7ner", "Herr\u00b7schaft", "Se\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dir legt K\u00f6nigsberg sich an", "tokens": ["Dir", "legt", "K\u00f6\u00b7nigs\u00b7berg", "sich", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "PRF", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auch so sch\u00f6n es immer kan,", "tokens": ["Auch", "so", "sch\u00f6n", "es", "im\u00b7mer", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "PPER", "ADV", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Aller Pracht ist Deinetwegen,", "tokens": ["Al\u00b7ler", "Pracht", "ist", "Dei\u00b7net\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Der Triumph-Geb\u00e4wde Zier", "tokens": ["Der", "Tri\u00b7um\u00b7ph\u00b7Ge\u00b7b\u00e4w\u00b7de", "Zier"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Pranget, Unserm F\u00fcrsten, Dir.", "tokens": ["Pran\u00b7get", ",", "Un\u00b7serm", "F\u00fcrs\u00b7ten", ",", "Dir", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dieser wehrte Tag wird Preussen,", "tokens": ["Die\u00b7ser", "wehr\u00b7te", "Tag", "wird", "Preus\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil es stehet, heilig heissen,", "tokens": ["Weil", "es", "ste\u00b7het", ",", "hei\u00b7lig", "heis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die wir jetzt am Leben sind", "tokens": ["Die", "wir", "jetzt", "am", "Le\u00b7ben", "sind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "APPRART", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bringen ihn auff Kindes-Kind,", "tokens": ["Brin\u00b7gen", "ihn", "auff", "Kin\u00b7des\u00b7Kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle Nach-Welt wird ihn fassen,", "tokens": ["Al\u00b7le", "Nach\u00b7Welt", "wird", "ihn", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was das Kind die Mutter fragt,", "tokens": ["Was", "das", "Kind", "die", "Mut\u00b7ter", "fragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sie dem Kinde wieder sagt,", "tokens": ["Sie", "dem", "Kin\u00b7de", "wie\u00b7der", "sagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Was man redet auff den Gassen,", "tokens": ["Was", "man", "re\u00b7det", "auff", "den", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Was man hin und her ohn Ruh", "tokens": ["Was", "man", "hin", "und", "her", "ohn", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PIS", "PTKVZ", "KON", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Sorgt und schaffet, das bist Du.", "tokens": ["Sorgt", "und", "schaf\u00b7fet", ",", "das", "bist", "Du", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "$,", "PDS", "VAFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Du bist, dem wir hin und wieder", "tokens": ["Du", "bist", ",", "dem", "wir", "hin", "und", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "PPER", "PTKVZ", "KON", "ADV"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Singen Ehr- und Frewden-Lieder,", "tokens": ["Sin\u00b7gen", "Ehr", "und", "Fre\u00b7wden\u00b7Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "TRUNC", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil Dich auch das Wetter ehrt", "tokens": ["Weil", "Dich", "auch", "das", "Wet\u00b7ter", "ehrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Gesch\u00fctze, die man h\u00f6rt,", "tokens": ["Der", "Ge\u00b7sch\u00fct\u00b7ze", ",", "die", "man", "h\u00f6rt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dir gibt Wall und Schantze Flammen,", "tokens": ["Dir", "gibt", "Wall", "und", "Schant\u00b7ze", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "KON", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Menschen, Wild, Wald, Himmel, Schnee,", "tokens": ["Men\u00b7schen", ",", "Wild", ",", "Wald", ",", "Him\u00b7mel", ",", "Schnee", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "K\u00e4lte, Glut, Lufft, Erde, See", "tokens": ["K\u00e4l\u00b7te", ",", "Glut", ",", "Lufft", ",", "Er\u00b7de", ",", "See"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Tretten Dir in Dienst zusammen:", "tokens": ["Tret\u00b7ten", "Dir", "in", "Dienst", "zu\u00b7sam\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Jedes ehrt, so gut es mag,", "tokens": ["Je\u00b7des", "ehrt", ",", "so", "gut", "es", "mag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "ADV", "ADJD", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Churf\u00fcrst, Deinen Einzugs-Tag.", "tokens": ["Chur\u00b7f\u00fcrst", ",", "Dei\u00b7nen", "Ein\u00b7zugs\u00b7Tag", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Komm, wir sehen umb dich schweben", "tokens": ["Komm", ",", "wir", "se\u00b7hen", "umb", "dich", "schwe\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "APPR", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Billigkeit, Lust, Fried' und Leben,", "tokens": ["Bil\u00b7lig\u00b7keit", ",", "Lust", ",", "Fried'", "und", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+++-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Lauter Gn\u00fcg und Gnaden-Schein", "tokens": ["Lau\u00b7ter", "Gn\u00fcg", "und", "Gna\u00b7den\u00b7Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zeucht mit Unserm F\u00fcrsten ein.", "tokens": ["Zeucht", "mit", "Un\u00b7serm", "F\u00fcrs\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Du wirst Heil dem Lande bringen,", "tokens": ["Du", "wirst", "Heil", "dem", "Lan\u00b7de", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Held, dem Lande, welches fast", "tokens": ["Held", ",", "dem", "Lan\u00b7de", ",", "wel\u00b7ches", "fast"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ART", "NN", "$,", "PRELS", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Durch der Zeiten schwere Last", "tokens": ["Durch", "der", "Zei\u00b7ten", "schwe\u00b7re", "Last"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wil mit seinem Tode ringen,", "tokens": ["Wil", "mit", "sei\u00b7nem", "To\u00b7de", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Hilff ihm, es verl\u00e4sset sich", "tokens": ["Hilff", "ihm", ",", "es", "ver\u00b7l\u00e4s\u00b7set", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "PPER", "VVFIN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Einig noch auff Gott und Dich.", "tokens": ["Ei\u00b7nig", "noch", "auff", "Gott", "und", "Dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "KON", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}