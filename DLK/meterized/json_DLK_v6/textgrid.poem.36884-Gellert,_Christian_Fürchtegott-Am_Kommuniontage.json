{"textgrid.poem.36884": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Am Kommuniontage", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich komme, Herr, und suche dich,", "tokens": ["Ich", "kom\u00b7me", ",", "Herr", ",", "und", "su\u00b7che", "dich", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "M\u00fchselig und beladen.", "tokens": ["M\u00fch\u00b7se\u00b7lig", "und", "be\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVPP", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Gott, mein Erbarmer, w\u00fcrdge mich", "tokens": ["Gott", ",", "mein", "Er\u00b7bar\u00b7mer", ",", "w\u00fcrd\u00b7ge", "mich"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Wunders deiner Gnaden.", "tokens": ["Des", "Wun\u00b7ders", "dei\u00b7ner", "Gna\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich liege hier vor deinem Thron,", "tokens": ["Ich", "lie\u00b7ge", "hier", "vor", "dei\u00b7nem", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sohn Gottes und des Menschen Sohn,", "tokens": ["Sohn", "Got\u00b7tes", "und", "des", "Men\u00b7schen", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mich deiner zu getr\u00f6sten.", "tokens": ["Mich", "dei\u00b7ner", "zu", "ge\u00b7tr\u00f6s\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "PTKA", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich f\u00fchle meiner S\u00fcnden M\u00fch;", "tokens": ["Ich", "f\u00fch\u00b7le", "mei\u00b7ner", "S\u00fcn\u00b7den", "M\u00fch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ich suche Ruh, und finde sie", "tokens": ["Ich", "su\u00b7che", "Ruh", ",", "und", "fin\u00b7de", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Im Glauben der Erl\u00f6sten.", "tokens": ["Im", "Glau\u00b7ben", "der", "Er\u00b7l\u00f6s\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dich bet ich zuversichtlich an,", "tokens": ["Dich", "bet", "ich", "zu\u00b7ver\u00b7sicht\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Du bist das Heil der S\u00fcnder.", "tokens": ["Du", "bist", "das", "Heil", "der", "S\u00fcn\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du hast die Handschrift abgetan,", "tokens": ["Du", "hast", "die", "Hand\u00b7schrift", "ab\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wir sind Gottes Kinder.", "tokens": ["Und", "wir", "sind", "Got\u00b7tes", "Kin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich denk an deines Leidens Macht,", "tokens": ["Ich", "denk", "an", "dei\u00b7nes", "Lei\u00b7dens", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und an dein Wort: Es ist vollbracht!", "tokens": ["Und", "an", "dein", "Wort", ":", "Es", "ist", "voll\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du hast mein Heil verdienet.", "tokens": ["Du", "hast", "mein", "Heil", "ver\u00b7die\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Du hast f\u00fcr mich dich dargestellt.", "tokens": ["Du", "hast", "f\u00fcr", "mich", "dich", "dar\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Gott war in dir, und hat die Welt", "tokens": ["Gott", "war", "in", "dir", ",", "und", "hat", "die", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "PPER", "$,", "KON", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "In dir mit sich vers\u00fchnet.", "tokens": ["In", "dir", "mit", "sich", "ver\u00b7s\u00fch\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So freue dich, mein Herz, in mir!", "tokens": ["So", "freu\u00b7e", "dich", ",", "mein", "Herz", ",", "in", "mir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er tilget deine S\u00fcnden,", "tokens": ["Er", "til\u00b7get", "dei\u00b7ne", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und l\u00e4\u00dft an seiner Tafel hier", "tokens": ["Und", "l\u00e4\u00dft", "an", "sei\u00b7ner", "Ta\u00b7fel", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich Gnad um Gnade finden.", "tokens": ["Dich", "Gnad", "um", "Gna\u00b7de", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du rufst, und er erh\u00f6rt dich schon,", "tokens": ["Du", "rufst", ",", "und", "er", "er\u00b7h\u00f6rt", "dich", "schon", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Spricht liebreich: \u00bbSei getrost, mein Sohn!", "tokens": ["Spricht", "lieb\u00b7reich", ":", "\u00bb", "Sei", "ge\u00b7trost", ",", "mein", "Sohn", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$.", "$(", "VAFIN", "VVPP", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Schuld ist dir vergeben.", "tokens": ["Die", "Schuld", "ist", "dir", "ver\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Du bist in meinen Tod getauft,", "tokens": ["Du", "bist", "in", "mei\u00b7nen", "Tod", "ge\u00b7tauft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und du wirst dem, der dich erkauft,", "tokens": ["Und", "du", "wirst", "dem", ",", "der", "dich", "er\u00b7kauft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Von ganzem Herzen leben.", "tokens": ["Von", "gan\u00b7zem", "Her\u00b7zen", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dein ist das Gl\u00fcck der Seligkeit;", "tokens": ["Dein", "ist", "das", "Gl\u00fcck", "der", "Se\u00b7lig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bewahr es hier im Glauben,", "tokens": ["Be\u00b7wahr", "es", "hier", "im", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und la\u00df durch keine Sicherheit", "tokens": ["Und", "la\u00df", "durch", "kei\u00b7ne", "Si\u00b7cher\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dir deine Krone rauben.", "tokens": ["Dir", "dei\u00b7ne", "Kro\u00b7ne", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sieh, ich vereine mich mit dir;", "tokens": ["Sieh", ",", "ich", "ver\u00b7ei\u00b7ne", "mich", "mit", "dir", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich bin der Weinstock, bleib an mir:", "tokens": ["Ich", "bin", "der", "Wein\u00b7stock", ",", "bleib", "an", "mir", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So wirst du Fr\u00fcchte bringen.", "tokens": ["So", "wirst", "du", "Fr\u00fcch\u00b7te", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich helfe dir, ich st\u00e4rke dich;", "tokens": ["Ich", "hel\u00b7fe", "dir", ",", "ich", "st\u00e4r\u00b7ke", "dich", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und durch die Liebe gegen mich", "tokens": ["Und", "durch", "die", "Lie\u00b7be", "ge\u00b7gen", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wird dir der Sieg gelingen.\u00ab", "tokens": ["Wird", "dir", "der", "Sieg", "ge\u00b7lin\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ja, Herr, mein Gl\u00fcck ist dein Gebot;", "tokens": ["Ja", ",", "Herr", ",", "mein", "Gl\u00fcck", "ist", "dein", "Ge\u00b7bot", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich will es treu erf\u00fcllen,", "tokens": ["Ich", "will", "es", "treu", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und bitte dich, durch deinen Tod,", "tokens": ["Und", "bit\u00b7te", "dich", ",", "durch", "dei\u00b7nen", "Tod", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um Kraft zu meinem Willen.", "tokens": ["Um", "Kraft", "zu", "mei\u00b7nem", "Wil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "La\u00df mich von nun an w\u00fcrdig sein,", "tokens": ["La\u00df", "mich", "von", "nun", "an", "w\u00fcr\u00b7dig", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADV", "APPR", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein ganzes Herz dir, Herr, zu weihn,", "tokens": ["Mein", "gan\u00b7zes", "Herz", "dir", ",", "Herr", ",", "zu", "weihn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "$,", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und deinen Tod zu preisen.", "tokens": ["Und", "dei\u00b7nen", "Tod", "zu", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "La\u00df mich den Ernst der Heiligung", "tokens": ["La\u00df", "mich", "den", "Ernst", "der", "Hei\u00b7li\u00b7gung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Durch eine wahre Besserung", "tokens": ["Durch", "ei\u00b7ne", "wah\u00b7re", "Bes\u00b7se\u00b7rung"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.10": {"text": "Mir und der Welt beweisen!", "tokens": ["Mir", "und", "der", "Welt", "be\u00b7wei\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}