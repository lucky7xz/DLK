{"dta.poem.20315": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Sie w\u00fcnschte ihm gl\u00fcck zum  \n neuen-jahre.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nein! sch\u00f6nste Phillis/ nein! dein wunsch kan nicht bestehn/", "tokens": ["Nein", "!", "sch\u00f6ns\u00b7te", "Phil\u00b7lis", "/", "nein", "!", "dein", "wunsch", "kan", "nicht", "be\u00b7stehn", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "VVFIN", "NE", "$(", "PTKANT", "$.", "PPOSAT", "NN", "VMFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du sprichst mir zwar von gl\u00fcck und s\u00fcssem wohlergehn/", "tokens": ["Du", "sprichst", "mir", "zwar", "von", "gl\u00fcck", "und", "s\u00fcs\u00b7sem", "woh\u00b7ler\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Heist aber di\u00df ein gl\u00fcck/ wann deine blicke t\u00f6dten?", "tokens": ["Heist", "a\u00b7ber", "di\u00df", "ein", "gl\u00fcck", "/", "wann", "dei\u00b7ne", "bli\u00b7cke", "t\u00f6d\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PDS", "ART", "NN", "$(", "PWAV", "PPOSAT", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein hertze steckt voll ha\u00df und zorniger begier/", "tokens": ["Dein", "hert\u00b7ze", "steckt", "voll", "ha\u00df", "und", "zor\u00b7ni\u00b7ger", "be\u00b7gier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gl\u00fcck wird mir nur gew\u00fcnscht/ und ungl\u00fcck giebst du mir.", "tokens": ["Gl\u00fcck", "wird", "mir", "nur", "ge\u00b7w\u00fcnscht", "/", "und", "un\u00b7gl\u00fcck", "giebst", "du", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVPP", "$(", "KON", "ADJD", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wo sind die worte nun/ die mich zuvor erh\u00f6hten?", "tokens": ["Wo", "sind", "die", "wor\u00b7te", "nun", "/", "die", "mich", "zu\u00b7vor", "er\u00b7h\u00f6h\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "$(", "PRELS", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ach nimm den wunsch zur\u00fcck/ und liebe mich daf\u00fcr!", "tokens": ["Ach", "nimm", "den", "wunsch", "zu\u00b7r\u00fcck", "/", "und", "lie\u00b7be", "mich", "da\u00b7f\u00fcr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVIMP", "ART", "ADJD", "PTKVZ", "$(", "KON", "VVFIN", "PRF", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denn wem du g\u00fcnstig bist/ der hat kein gl\u00fcck von n\u00f6then.", "tokens": ["Denn", "wem", "du", "g\u00fcns\u00b7tig", "bist", "/", "der", "hat", "kein", "gl\u00fcck", "von", "n\u00f6\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "VAFIN", "$(", "ART", "VAFIN", "PIAT", "NN", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}