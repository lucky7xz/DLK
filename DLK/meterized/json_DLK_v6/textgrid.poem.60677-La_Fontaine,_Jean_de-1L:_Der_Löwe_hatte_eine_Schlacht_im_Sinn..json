{"textgrid.poem.60677": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der L\u00f6we hatte eine Schlacht im Sinn.", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der L\u00f6we hatte eine Schlacht im Sinn.", "tokens": ["Der", "L\u00f6\u00b7we", "hat\u00b7te", "ei\u00b7ne", "Schlacht", "im", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So h\u00e4lt er Kriegsrat, schickt den Adjutanten", "tokens": ["So", "h\u00e4lt", "er", "Kriegs\u00b7rat", ",", "schickt", "den", "Ad\u00b7ju\u00b7tan\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu allen Tieren hin;", "tokens": ["Zu", "al\u00b7len", "Tie\u00b7ren", "hin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein jedes stelle schnell sich ein,", "tokens": ["Ein", "je\u00b7des", "stel\u00b7le", "schnell", "sich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "ADJD", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit seinen Kr\u00e4ften hilfsbereit zu sein:", "tokens": ["Mit", "sei\u00b7nen", "Kr\u00e4f\u00b7ten", "hilfs\u00b7be\u00b7reit", "zu", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So brauche man den breiten Elefanten,", "tokens": ["So", "brau\u00b7che", "man", "den", "brei\u00b7ten", "E\u00b7lef\u00b7an\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Damit er Munition und Waffen trage", "tokens": ["Da\u00b7mit", "er", "Mu\u00b7ni\u00b7tion", "und", "Waf\u00b7fen", "tra\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und wuchtigen Trittes eine Bresche schlage;", "tokens": ["Und", "wuch\u00b7ti\u00b7gen", "Trit\u00b7tes", "ei\u00b7ne", "Bre\u00b7sche", "schla\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Zum Sturmlauf halte sich der B\u00e4r bereit;", "tokens": ["Zum", "Sturm\u00b7lauf", "hal\u00b7te", "sich", "der", "B\u00e4r", "be\u00b7reit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der Fuchs jedoch soll feine R\u00e4nke spinnen;", "tokens": ["Der", "Fuchs", "je\u00b7doch", "soll", "fei\u00b7ne", "R\u00e4n\u00b7ke", "spin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Der Affe suche nach Gelegenheit,", "tokens": ["Der", "Af\u00b7fe", "su\u00b7che", "nach", "Ge\u00b7le\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Den Feind durch Sp\u00e4\u00dfe zu gewinnen.", "tokens": ["Den", "Feind", "durch", "Sp\u00e4\u00b7\u00dfe", "zu", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "\u00bbschickt nur\u00ab, riet einer, \u00bbden Herrn Esel dort", "tokens": ["\u00bb", "schickt", "nur", "\u00ab", ",", "riet", "ei\u00b7ner", ",", "\u00bb", "den", "Herrn", "E\u00b7sel", "dort"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "ADV", "$(", "$,", "VVFIN", "PIS", "$,", "$(", "ART", "NN", "NN", "ADV"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.14": {"text": "Und auch den allzu feigen Hasen fort,", "tokens": ["Und", "auch", "den", "all\u00b7zu", "fei\u00b7gen", "Ha\u00b7sen", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "PTKA", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Denn jener ist so t\u00f6lpelhaft wie dumm,", "tokens": ["Denn", "je\u00b7ner", "ist", "so", "t\u00f6l\u00b7pel\u00b7haft", "wie", "dumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "ADJD", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und dieser kehrt vorm Feinde sicher um.\u00ab", "tokens": ["Und", "die\u00b7ser", "kehrt", "vorm", "Fein\u00b7de", "si\u00b7cher", "um", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PDS", "VVFIN", "APPRART", "NN", "ADJD", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "\u00bbdurchaus nicht,\u00ab sprach der K\u00f6nig, \u00bbbeide haben", "tokens": ["\u00bb", "durc\u00b7haus", "nicht", ",", "\u00ab", "sprach", "der", "K\u00f6\u00b7nig", ",", "\u00bb", "bei\u00b7de", "ha\u00b7ben"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["$(", "ADV", "PTKNEG", "$,", "$(", "VVFIN", "ART", "NN", "$,", "$(", "PIS", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "F\u00fcr unsern Kriegszug nutzenswerte Gaben:", "tokens": ["F\u00fcr", "un\u00b7sern", "Kriegs\u00b7zug", "nut\u00b7zens\u00b7wer\u00b7te", "Ga\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Der Esel soll mit seinem grimmen Ruf", "tokens": ["Der", "E\u00b7sel", "soll", "mit", "sei\u00b7nem", "grim\u00b7men", "Ruf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Den Feind entsetzen,", "tokens": ["Den", "Feind", "ent\u00b7set\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.21": {"text": "Den Hasen, den Natur zum L\u00e4ufer schuf,", "tokens": ["Den", "Ha\u00b7sen", ",", "den", "Na\u00b7tur", "zum", "L\u00e4u\u00b7fer", "schuf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Wird, denk ich, keiner als Kurier ersetzen.\u00ab", "tokens": ["Wird", ",", "denk", "ich", ",", "kei\u00b7ner", "als", "Ku\u00b7rier", "er\u00b7set\u00b7zen", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "$,", "VVFIN", "PPER", "$,", "PIS", "KOKOM", "NN", "VVINF", "$.", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein weiser F\u00fcrst mu\u00df stets verstehen,", "tokens": ["Ein", "wei\u00b7ser", "F\u00fcrst", "mu\u00df", "stets", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch dem Geringsten seinen Platz zu geben,", "tokens": ["Auch", "dem", "Ge\u00b7rings\u00b7ten", "sei\u00b7nen", "Platz", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und der Verst\u00e4ndige wird nie im Leben", "tokens": ["Und", "der", "Ver\u00b7st\u00e4n\u00b7di\u00b7ge", "wird", "nie", "im", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den kleinsten Vorteil \u00fcbersehen.", "tokens": ["Den", "kleins\u00b7ten", "Vor\u00b7teil", "\u00fc\u00b7ber\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}