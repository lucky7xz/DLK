{"textgrid.poem.57886": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Juli, das ist Volksfestzeit,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Juli, das ist Volksfestzeit,", "tokens": ["Der", "Ju\u00b7li", ",", "das", "ist", "Volks\u00b7fest\u00b7zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hoch her geht's dann im St\u00e4dtchen,", "tokens": ["Hoch", "her", "geht's", "dann", "im", "St\u00e4dt\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APZR", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Es freuen darauf sich schon wochenlang", "tokens": ["Es", "freu\u00b7en", "da\u00b7rauf", "sich", "schon", "wo\u00b7chen\u00b7lang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "PRF", "ADV", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die jungen Burschen und M\u00e4dchen.", "tokens": ["Die", "jun\u00b7gen", "Bur\u00b7schen", "und", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Stadt tr\u00e4gt Kranz- und Flaggenschmuck", "tokens": ["Die", "Stadt", "tr\u00e4gt", "Kranz", "und", "Flag\u00b7gen\u00b7schmuck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Meyers Krischan das Banner,", "tokens": ["Und", "Me\u00b7yers", "Krisc\u00b7han", "das", "Ban\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der B\u00fcrgermeister die Festrede h\u00e4lt,", "tokens": ["Der", "B\u00fcr\u00b7ger\u00b7meis\u00b7ter", "die", "Fest\u00b7re\u00b7de", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Man mu\u00df es ihm lassen, das kann er.", "tokens": ["Man", "mu\u00df", "es", "ihm", "las\u00b7sen", ",", "das", "kann", "er", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "PPER", "VVINF", "$,", "PDS", "VMFIN", "PPER", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Und auch eine Fahnenjungfer ist da,", "tokens": ["Und", "auch", "ei\u00b7ne", "Fah\u00b7nen\u00b7jung\u00b7fer", "ist", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sehr dick, sehr rot, sehr bl\u00f6de,", "tokens": ["Sehr", "dick", ",", "sehr", "rot", ",", "sehr", "bl\u00f6\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie h\u00e4lt mit Angstschwei\u00df im Gesicht", "tokens": ["Sie", "h\u00e4lt", "mit", "A\u00b7ngstschwei\u00df", "im", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eine unverst\u00e4ndliche Rede.", "tokens": ["Ei\u00b7ne", "un\u00b7ver\u00b7st\u00e4nd\u00b7li\u00b7che", "Re\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "In ihren Z\u00fcgen hold und mild", "tokens": ["In", "ih\u00b7ren", "Z\u00fc\u00b7gen", "hold", "und", "mild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kann man es deutlich lesen:", "tokens": ["Kann", "man", "es", "deut\u00b7lich", "le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Halb f\u00fchlt das M\u00e4dchen sich blamiert", "tokens": ["Halb", "f\u00fchlt", "das", "M\u00e4d\u00b7chen", "sich", "bla\u00b7miert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PRF", "VVFIN"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Und halb als h\u00f6heres Wesen.", "tokens": ["Und", "halb", "als", "h\u00f6\u00b7he\u00b7res", "We\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}