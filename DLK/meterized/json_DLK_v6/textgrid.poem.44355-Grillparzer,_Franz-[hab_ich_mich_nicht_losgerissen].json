{"textgrid.poem.44355": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[hab ich mich nicht losgerissen]", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hab ich mich nicht losgerissen,", "tokens": ["Hab", "ich", "mich", "nicht", "los\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht mein Herz von ihr gewandt,", "tokens": ["Nicht", "mein", "Herz", "von", "ihr", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil ich sie verachten m\u00fcssen,", "tokens": ["Weil", "ich", "sie", "ver\u00b7ach\u00b7ten", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "VMINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil ich wertlos sie erkannt?", "tokens": ["Weil", "ich", "wert\u00b7los", "sie", "er\u00b7kannt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Warum steht in holdem Bangen", "tokens": ["Wa\u00b7rum", "steht", "in", "hol\u00b7dem", "Ban\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie denn immer noch vor mir?", "tokens": ["Sie", "denn", "im\u00b7mer", "noch", "vor", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Woher dieses Glutverlangen,", "tokens": ["Wo\u00b7her", "die\u00b7ses", "Glut\u00b7ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Das mich jetzt noch zieht zu ihr?", "tokens": ["Das", "mich", "jetzt", "noch", "zieht", "zu", "ihr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADV", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Tausend alte Bilder kommen", "tokens": ["Tau\u00b7send", "al\u00b7te", "Bil\u00b7der", "kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, und jedes, jedes spricht:", "tokens": ["Ach", ",", "und", "je\u00b7des", ",", "je\u00b7des", "spricht", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "PIAT", "$,", "PIAT", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist der Pfeil auch weggenommen,", "tokens": ["Ist", "der", "Pfeil", "auch", "weg\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ists darum die Wunde nicht.", "tokens": ["Ists", "da\u00b7rum", "die", "Wun\u00b7de", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}