{"textgrid.poem.67772": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Lobgesang", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sing, o meine Seele, Deines", "tokens": ["Sing", ",", "o", "mei\u00b7ne", "See\u00b7le", ",", "Dei\u00b7nes"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "$,", "FM", "PPOSAT", "NN", "$,", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsichtbaren K\u00f6nigs Reich!", "tokens": ["Un\u00b7sicht\u00b7ba\u00b7ren", "K\u00f6\u00b7nigs", "Reich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Von der Erd' hinauf gen Himmel,", "tokens": ["Von", "der", "Erd'", "hin\u00b7auf", "gen", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werd' ihm im Triumphe gleich!", "tokens": ["Werd'", "ihm", "im", "Tri\u00b7um\u00b7phe", "gleich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ADV", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Wie die Unschuld, wie die Wahrheit,", "tokens": ["Wie", "die", "Un\u00b7schuld", ",", "wie", "die", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Still und ewig ist sein Reich.", "tokens": ["Still", "und", "e\u00b7wig", "ist", "sein", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbehre sei Gott in den H\u00f6hen!", "tokens": ["\u00bb", "eh\u00b7re", "sei", "Gott", "in", "den", "H\u00f6\u00b7hen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "VAFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Fried' auf Erden! Aller Welt", "tokens": ["Fried'", "auf", "Er\u00b7den", "!", "Al\u00b7ler", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "NN", "$.", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heil!\u00ab so singen Gottes Engel", "tokens": ["Heil", "!", "\u00ab", "so", "sin\u00b7gen", "Got\u00b7tes", "En\u00b7gel"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "$(", "ADV", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter ihres K\u00f6nigs Zelt.", "tokens": ["Un\u00b7ter", "ih\u00b7res", "K\u00f6\u00b7nigs", "Zelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "N\u00e4chtlich stille, hell im Dunkel,", "tokens": ["N\u00e4cht\u00b7lich", "stil\u00b7le", ",", "hell", "im", "Dun\u00b7kel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Also kommt, so herrscht der Held.", "tokens": ["Al\u00b7so", "kommt", ",", "so", "herrscht", "der", "Held", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Neiget Euch ihm, Erdenkronen,", "tokens": ["Nei\u00b7get", "Euch", "ihm", ",", "Er\u00b7den\u00b7kro\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sinket nieder in den Staub!", "tokens": ["Sin\u00b7ket", "nie\u00b7der", "in", "den", "Staub", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sklavenjoch ist Eure Fessel,", "tokens": ["Skla\u00b7ven\u00b7joch", "ist", "Eu\u00b7re", "Fes\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Euer Lorbeer blutig Laub;", "tokens": ["Eu\u00b7er", "Lor\u00b7beer", "blu\u00b7tig", "Laub", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eure Babel wird zu Tr\u00fcmmer,", "tokens": ["Eu\u00b7re", "Ba\u00b7bel", "wird", "zu", "Tr\u00fcm\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Euer Purpur Wurmesraub!", "tokens": ["Eu\u00b7er", "Pur\u00b7pur", "Wur\u00b7me\u00b7sraub", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber ewig, wie die Sonne,", "tokens": ["A\u00b7ber", "e\u00b7wig", ",", "wie", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist der Wahrheit g\u00fclden Licht.", "tokens": ["Ist", "der", "Wahr\u00b7heit", "g\u00fcl\u00b7den", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch das Schicksal \u00fcberwindet", "tokens": ["Auch", "das", "Schick\u00b7sal", "\u00fc\u00b7berw\u00b7in\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine Ruh und wanket nicht;", "tokens": ["Sei\u00b7ne", "Ruh", "und", "wan\u00b7ket", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seine Demuth, seine Liebe", "tokens": ["Sei\u00b7ne", "De\u00b7muth", ",", "sei\u00b7ne", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist's, die Noth und Tod zerbricht.", "tokens": ["Ist's", ",", "die", "Noth", "und", "Tod", "zer\u00b7bricht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Oeffnet Euch, Ihr ew'gen Pforten!", "tokens": ["Oeff\u00b7net", "Euch", ",", "Ihr", "ew'\u00b7gen", "Pfor\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn ein Sieger zeucht heran.", "tokens": ["Denn", "ein", "Sie\u00b7ger", "zeucht", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "\u00bbwer ist er, der stille Sieger?", "tokens": ["\u00bb", "wer", "ist", "er", ",", "der", "stil\u00b7le", "Sie\u00b7ger", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Blut bezeichnet seine Bahn!\u00ab", "tokens": ["Blut", "be\u00b7zeich\u00b7net", "sei\u00b7ne", "Bahn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Es ist Christ, der Wahrheit K\u00f6nig,", "tokens": ["Es", "ist", "Christ", ",", "der", "Wahr\u00b7heit", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der ein Geistesreich gewann.", "tokens": ["Der", "ein", "Geis\u00b7tes\u00b7reich", "ge\u00b7wann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Alle Weisen, alle Guten", "tokens": ["Al\u00b7le", "Wei\u00b7sen", ",", "al\u00b7le", "Gu\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ziehn ihm im Triumphe nach,", "tokens": ["Ziehn", "ihm", "im", "Tri\u00b7um\u00b7phe", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie, wie er, im Blutespurpur,", "tokens": ["Sie", ",", "wie", "er", ",", "im", "Blu\u00b7tes\u00b7pur\u00b7pur", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAV", "PPER", "$,", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sie, wie er, voll sch\u00f6ner Schmach.", "tokens": ["Sie", ",", "wie", "er", ",", "voll", "sch\u00f6\u00b7ner", "Schmach", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAV", "PPER", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Freiheit ist's, was sie errangen,", "tokens": ["Frei\u00b7heit", "ist's", ",", "was", "sie", "er\u00b7ran\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kette, was ihr Arm zerbrach.", "tokens": ["Ket\u00b7te", ",", "was", "ihr", "Arm", "zer\u00b7brach", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "K\u00f6nig, la\u00df von Deiner Salbung", "tokens": ["K\u00f6\u00b7nig", ",", "la\u00df", "von", "Dei\u00b7ner", "Sal\u00b7bung"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVIMP", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einen Odem mich durchwehn", "tokens": ["Ei\u00b7nen", "O\u00b7dem", "mich", "durch\u00b7wehn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Stille Deines Reiches", "tokens": ["Und", "die", "Stil\u00b7le", "Dei\u00b7nes", "Rei\u00b7ches"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir durch Herz und Seele gehn!", "tokens": ["Mir", "durch", "Herz", "und", "See\u00b7le", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df mich sterbend, la\u00df mich lebend", "tokens": ["La\u00df", "mich", "ster\u00b7bend", ",", "la\u00df", "mich", "le\u00b7bend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVPP", "$,", "VVIMP", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit Dein Reich von ferne sehn!", "tokens": ["Mit", "Dein", "Reich", "von", "fer\u00b7ne", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}