{"textgrid.poem.65581": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "54. Die Taufe von ungef\u00e4hr", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Jude fiel in's Wasser und wollte schier ersaufen,", "tokens": ["Der", "Ju\u00b7de", "fiel", "in's", "Was\u00b7ser", "und", "woll\u00b7te", "schier", "er\u00b7sau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "KON", "VMFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da schrieen b\u00f6se Sp\u00f6tter: Er hat sich lassen taufen.", "tokens": ["Da", "schri\u00b7een", "b\u00f6\u00b7se", "Sp\u00f6t\u00b7ter", ":", "Er", "hat", "sich", "las\u00b7sen", "tau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$.", "PPER", "VAFIN", "PRF", "VVINF", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie ", "tokens": ["Wie"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "So wird Unedel Edel, wenn es geadelt ist.", "tokens": ["So", "wird", "Un\u00b7e\u00b7del", "E\u00b7del", ",", "wenn", "es", "ge\u00b7a\u00b7delt", "ist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "NN", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}