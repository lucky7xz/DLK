{"textgrid.poem.44263": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[zwey Schl\u00fc\u00dfel \u00f6fnen uns das Thor der Ewigkeit]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwey Schl\u00fc\u00dfel \u00f6fnen uns das Thor der Ewigkeit,", "tokens": ["Zwey", "Schl\u00fc\u00b7\u00dfel", "\u00f6f\u00b7nen", "uns", "das", "Thor", "der", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zwey Wege theilen sich, den Berg zu \u00fcberwinden,", "tokens": ["Zwey", "We\u00b7ge", "thei\u00b7len", "sich", ",", "den", "Berg", "zu", "\u00fc\u00b7berw\u00b7in\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PRF", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf welchem Ehr und Ruhm, die Sclaven keiner Zeit,", "tokens": ["Auf", "wel\u00b7chem", "Ehr", "und", "Ruhm", ",", "die", "Scla\u00b7ven", "kei\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "KON", "NN", "$,", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Gro\u00dfmuth und der Kunst den Marmortempel gr\u00fcnden:", "tokens": ["Der", "Gro\u00df\u00b7muth", "und", "der", "Kunst", "den", "Mar\u00b7mor\u00b7tem\u00b7pel", "gr\u00fcn\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hier schwizt die Tapferkeit, dort eilt die Wei\u00dfheit fort,", "tokens": ["Hier", "schwizt", "die", "Tap\u00b7fer\u00b7keit", ",", "dort", "eilt", "die", "Wei\u00df\u00b7heit", "fort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und beide dringen sich an den gestirnten Ort,", "tokens": ["Und", "bei\u00b7de", "drin\u00b7gen", "sich", "an", "den", "ge\u00b7stirn\u00b7ten", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Wo Pallas und ihr Kiel, wo Mavors und sein Degen", "tokens": ["Wo", "Pal\u00b7las", "und", "ihr", "Kiel", ",", "wo", "Ma\u00b7vors", "und", "sein", "De\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "KON", "PPOSAT", "NN", "$,", "PWAV", "NE", "KON", "PPOSAT", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Die Bilder ihrer Zunft auf g\u00f6ldne M\u00fcnzen pr\u00e4gen.", "tokens": ["Die", "Bil\u00b7der", "ih\u00b7rer", "Zunft", "auf", "g\u00f6ld\u00b7ne", "M\u00fcn\u00b7zen", "pr\u00e4\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Vor diesen theuren Lohn zahlt mancher Haut und Blut,", "tokens": ["Vor", "die\u00b7sen", "theu\u00b7ren", "Lohn", "zahlt", "man\u00b7cher", "Haut", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVFIN", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor diese Kostbarkeit wagt C\u00e4sar Leib und Leben.", "tokens": ["Vor", "die\u00b7se", "Kost\u00b7bar\u00b7keit", "wagt", "C\u00e4\u00b7sar", "Leib", "und", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "NE", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Heist nicht, was Cloelia und ihr Geschlechte thut,", "tokens": ["Heist", "nicht", ",", "was", "Clo\u00b7e\u00b7lia", "und", "ihr", "Ge\u00b7schlech\u00b7te", "thut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PRELS", "NE", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch Wa\u00dfer und Gefahr dem Kranz entgegen streben?", "tokens": ["Durch", "Wa\u00b7\u00dfer", "und", "Ge\u00b7fahr", "dem", "Kranz", "ent\u00b7ge\u00b7gen", "stre\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bl\u00e4st nicht die Fama noch der Nachwelt in das Ohr,", "tokens": ["Bl\u00e4st", "nicht", "die", "Fa\u00b7ma", "noch", "der", "Nach\u00b7welt", "in", "das", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Warum Pharsalien Pompejens Kopf verlor?", "tokens": ["Wa\u00b7rum", "Phar\u00b7sa\u00b7li\u00b7en", "Pom\u00b7pe\u00b7jens", "Kopf", "ver\u00b7lor", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NE", "NN", "VVFIN", "$."], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Hat die Verg\u00e4ngligkeit den k\u00fchnen Arm gefre\u00dfen,", "tokens": ["Hat", "die", "Ver\u00b7g\u00e4n\u00b7glig\u00b7keit", "den", "k\u00fch\u00b7nen", "Arm", "ge\u00b7fre\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der bey Porsennens Herd die rechte Hand verge\u00dfen?", "tokens": ["Der", "bey", "Por\u00b7sen\u00b7nens", "Herd", "die", "rech\u00b7te", "Hand", "ver\u00b7ge\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Auf dieser Stra\u00dfe geht ein unerschrockner Held,", "tokens": ["Auf", "die\u00b7ser", "Stra\u00b7\u00dfe", "geht", "ein", "un\u00b7er\u00b7schrock\u00b7ner", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der ihm die Losung nimmt: Wir sterben oder siegen.", "tokens": ["Der", "ihm", "die", "Lo\u00b7sung", "nimmt", ":", "Wir", "ster\u00b7ben", "o\u00b7der", "sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Wahlstatt scheinet ihm ein Thron, der ewig h\u00e4lt;", "tokens": ["Die", "Wahl\u00b7statt", "schei\u00b7net", "ihm", "ein", "Thron", ",", "der", "e\u00b7wig", "h\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So viel er Leichen macht, so viel betritt er Stiegen.", "tokens": ["So", "viel", "er", "Lei\u00b7chen", "macht", ",", "so", "viel", "be\u00b7tritt", "er", "Stie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "NN", "VVFIN", "$,", "ADV", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Spricht man zulezt von ihm: Er focht, er stand, er fiel,", "tokens": ["Spricht", "man", "zu\u00b7lezt", "von", "ihm", ":", "Er", "focht", ",", "er", "stand", ",", "er", "fiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "PPER", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So kommt er allerdings an sein erw\u00fcntschtes Ziel", "tokens": ["So", "kommt", "er", "al\u00b7ler\u00b7dings", "an", "sein", "er\u00b7w\u00fcnt\u00b7schtes", "Ziel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und l\u00e4st den schweren Zeug nach einem halben Blicke,", "tokens": ["Und", "l\u00e4st", "den", "schwe\u00b7ren", "Zeug", "nach", "ei\u00b7nem", "hal\u00b7ben", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als spr\u00e4ch er Gute Nacht, der Eitelkeit zur\u00fccke.", "tokens": ["Als", "spr\u00e4ch", "er", "Gu\u00b7te", "Nacht", ",", "der", "Ei\u00b7tel\u00b7keit", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADJA", "NN", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Noch sichrer f\u00fchrt die Bahn, so uns Minerva bricht;", "tokens": ["Noch", "sich\u00b7rer", "f\u00fchrt", "die", "Bahn", ",", "so", "uns", "Mi\u00b7ner\u00b7va", "bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$,", "ADV", "PPER", "NE", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So fl\u00fcchtig steigt kein Pfeil, den Meroe gefiedert,", "tokens": ["So", "fl\u00fcch\u00b7tig", "steigt", "kein", "Pfeil", ",", "den", "Me\u00b7roe", "ge\u00b7fie\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PIAT", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Als sich ein Geist erhebt, der mit den Musen spricht", "tokens": ["Als", "sich", "ein", "Geist", "er\u00b7hebt", ",", "der", "mit", "den", "Mu\u00b7sen", "spricht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und seiner Glieder Fall durch den Verstand erwiedert.", "tokens": ["Und", "sei\u00b7ner", "Glie\u00b7der", "Fall", "durch", "den", "Ver\u00b7stand", "er\u00b7wie\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er spottet selbst dem Neid, der ihm nicht folgen kan,", "tokens": ["Er", "spot\u00b7tet", "selbst", "dem", "Neid", ",", "der", "ihm", "nicht", "fol\u00b7gen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sieht aus der stolzen H\u00f6h der Misgunst Ohnmacht an,", "tokens": ["Sieht", "aus", "der", "stol\u00b7zen", "H\u00f6h", "der", "Mis\u00b7gunst", "Ohn\u00b7macht", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sucht alles in sich selbst, was ihn zufrieden stelle,", "tokens": ["Sucht", "al\u00b7les", "in", "sich", "selbst", ",", "was", "ihn", "zu\u00b7frie\u00b7den", "stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "PRF", "ADV", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und macht die Finstern\u00fc\u00df in seiner Gruft noch helle.", "tokens": ["Und", "macht", "die", "Fins\u00b7ter\u00b7n\u00fc\u00df", "in", "sei\u00b7ner", "Gruft", "noch", "hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Das Volck, so gerne log, erhob sein Vaterland,", "tokens": ["Das", "Volck", ",", "so", "ger\u00b7ne", "log", ",", "er\u00b7hob", "sein", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil es dem Juppiter die Wiege zugeschnidten;", "tokens": ["Weil", "es", "dem", "Jup\u00b7pi\u00b7ter", "die", "Wie\u00b7ge", "zu\u00b7ge\u00b7schnid\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auch Pella wird der Welt durch die Geburth bekand,", "tokens": ["Auch", "Pel\u00b7la", "wird", "der", "Welt", "durch", "die", "Ge\u00b7burth", "be\u00b7kand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die einen J\u00fcngling gab, der um die Welt gestritten.", "tokens": ["Die", "ei\u00b7nen", "J\u00fcng\u00b7ling", "gab", ",", "der", "um", "die", "Welt", "ge\u00b7strit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch Memphis und Athen verschlungen beider Ruhm:", "tokens": ["Doch", "Mem\u00b7phis", "und", "A\u00b7then", "ver\u00b7schlun\u00b7gen", "bei\u00b7der", "Ruhm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "KON", "NE", "VVFIN", "PIAT", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Dort wird die Wei\u00dfheit jung, hier stund ihr Heiligthum;", "tokens": ["Dort", "wird", "die", "Wei\u00df\u00b7heit", "jung", ",", "hier", "stund", "ihr", "Hei\u00b7lig\u00b7thum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dort ward der Wi\u00dfenschaft das erste Brodt gebrochen,", "tokens": ["Dort", "ward", "der", "Wi\u00b7\u00dfen\u00b7schaft", "das", "ers\u00b7te", "Brodt", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Hier der Gelehrsamkeit der Hauptsiz zugesprochen.", "tokens": ["Hier", "der", "Ge\u00b7lehr\u00b7sam\u00b7keit", "der", "Haupt\u00b7siz", "zu\u00b7ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stan