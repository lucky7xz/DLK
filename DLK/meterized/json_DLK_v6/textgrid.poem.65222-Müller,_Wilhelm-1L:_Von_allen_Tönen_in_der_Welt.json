{"textgrid.poem.65222": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von allen T\u00f6nen in der Welt", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von allen T\u00f6nen in der Welt", "tokens": ["Von", "al\u00b7len", "T\u00f6\u00b7nen", "in", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist keiner, der mir ba\u00df gef\u00e4llt,", "tokens": ["Ist", "kei\u00b7ner", ",", "der", "mir", "ba\u00df", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als voller Gl\u00e4ser Klingen,", "tokens": ["Als", "vol\u00b7ler", "Gl\u00e4\u00b7ser", "Klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn einen Spruch, wie 's Herz ihn meint,", "tokens": ["Wenn", "ei\u00b7nen", "Spruch", ",", "wie", "'s", "Herz", "ihn", "meint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PWAV", "PPER", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Entgegenbringt der Freund dem Freund,", "tokens": ["Ent\u00b7ge\u00b7gen\u00b7bringt", "der", "Freund", "dem", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df hoch die Tropfen springen.", "tokens": ["Da\u00df", "hoch", "die", "Trop\u00b7fen", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Auch h\u00f6r' ich gern des Hammers Schlag,", "tokens": ["Auch", "h\u00f6r'", "ich", "gern", "des", "Ham\u00b7mers", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der aus den Tonnen allgemach", "tokens": ["Der", "aus", "den", "Ton\u00b7nen", "all\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Spund wei\u00df aufzutreiben.", "tokens": ["Den", "Spund", "wei\u00df", "auf\u00b7zu\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und wenn der liebe klare Wein", "tokens": ["Und", "wenn", "der", "lie\u00b7be", "kla\u00b7re", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Rinnt pl\u00e4tschernd in die Flaschen ein,", "tokens": ["Rinnt", "pl\u00e4t\u00b7schernd", "in", "die", "Fla\u00b7schen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Klang ist zum Bet\u00e4uben.", "tokens": ["Der", "Klang", "ist", "zum", "Be\u00b7t\u00e4u\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hoch springt mir gleich mein Herz empor,", "tokens": ["Hoch", "springt", "mir", "gleich", "mein", "Herz", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00f6r' ich der Winzer Jubelchor", "tokens": ["H\u00f6r'", "ich", "der", "Win\u00b7zer", "Ju\u00b7be\u00b7lchor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von einem Berge schallen,", "tokens": ["Von", "ei\u00b7nem", "Ber\u00b7ge", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verk\u00fcndend gute Erntezeit,", "tokens": ["Ver\u00b7k\u00fcn\u00b7dend", "gu\u00b7te", "Ern\u00b7te\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verhei\u00dfend Heil und Seligkeit", "tokens": ["Ver\u00b7hei\u00b7\u00dfend", "Heil", "und", "Se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Uns treuen Zechern allen.", "tokens": ["Uns", "treu\u00b7en", "Ze\u00b7chern", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PIAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wer's also meint, der sto\u00dfe an,", "tokens": ["Wer's", "al\u00b7so", "meint", ",", "der", "sto\u00b7\u00dfe", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "$,", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wer nicht mit mir singen kann,", "tokens": ["Und", "wer", "nicht", "mit", "mir", "sin\u00b7gen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKNEG", "APPR", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Glas das wird doch klingen.", "tokens": ["Sein", "Glas", "das", "wird", "doch", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDS", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und wer den Becherklang nicht liebt,", "tokens": ["Und", "wer", "den", "Be\u00b7cher\u00b7klang", "nicht", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wer sich ohne Schmerz betr\u00fcbt,", "tokens": ["Und", "wer", "sich", "oh\u00b7ne", "Schmerz", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem soll'n die Kauze singen!", "tokens": ["Dem", "soll'n", "die", "Kau\u00b7ze", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}