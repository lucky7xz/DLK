{"textgrid.poem.55267": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Beispiel", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich mal ungeduldig werde,", "tokens": ["Wenn", "ich", "mal", "un\u00b7ge\u00b7dul\u00b7dig", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denk ich an die Geduld der Erde,", "tokens": ["Denk", "ich", "an", "die", "Ge\u00b7duld", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die, wie man sagt, sich t\u00e4glich dreht", "tokens": ["Die", ",", "wie", "man", "sagt", ",", "sich", "t\u00e4g\u00b7lich", "dreht"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "PWAV", "PIS", "VVFIN", "$,", "PRF", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und j\u00e4hrlich so wie j\u00e4hrlich geht.", "tokens": ["Und", "j\u00e4hr\u00b7lich", "so", "wie", "j\u00e4hr\u00b7lich", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "KOKOM", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bin ich denn f\u00fcr was andres da? \u2013", "tokens": ["Bin", "ich", "denn", "f\u00fcr", "was", "and\u00b7res", "da", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PRELS", "PIS", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich folge der lieben Frau Mama.", "tokens": ["Ich", "fol\u00b7ge", "der", "lie\u00b7ben", "Frau", "Ma\u00b7ma", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}