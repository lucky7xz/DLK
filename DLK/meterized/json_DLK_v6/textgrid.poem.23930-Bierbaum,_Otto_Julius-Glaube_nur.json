{"textgrid.poem.23930": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Glaube nur", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn im Sommer der rote Mohn", "tokens": ["Wenn", "im", "Som\u00b7mer", "der", "ro\u00b7te", "Mohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wieder gl\u00fcht im gelben Korn,", "tokens": ["Wie\u00b7der", "gl\u00fcht", "im", "gel\u00b7ben", "Korn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn des Finken s\u00fc\u00dfer Ton", "tokens": ["Wenn", "des", "Fin\u00b7ken", "s\u00fc\u00b7\u00dfer", "Ton"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wieder lockt im Hagedorn,", "tokens": ["Wie\u00b7der", "lockt", "im", "Ha\u00b7ge\u00b7dorn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn es wieder weit und breit", "tokens": ["Wenn", "es", "wie\u00b7der", "weit", "und", "breit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Feierklar und fruchtstill ist,", "tokens": ["Fei\u00b7er\u00b7klar", "und", "frucht\u00b7still", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dann erf\u00fcllt sich uns die Zeit,", "tokens": ["Dann", "er\u00b7f\u00fcllt", "sich", "uns", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Die mit vollen Ma\u00dfen mi\u00dft,", "tokens": ["Die", "mit", "vol\u00b7len", "Ma\u00b7\u00dfen", "mi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Dann verebbt, was uns bedroht,", "tokens": ["Dann", "ver\u00b7ebbt", ",", "was", "uns", "be\u00b7droht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Dann verweht, was uns bedr\u00fcckt,", "tokens": ["Dann", "ver\u00b7weht", ",", "was", "uns", "be\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Ueber dem Schlangenkopf der Not", "tokens": ["Ue\u00b7ber", "dem", "Schlan\u00b7gen\u00b7kopf", "der", "Not"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Ist das Sonnenschwert gez\u00fcckt.", "tokens": ["Ist", "das", "Son\u00b7nen\u00b7schwert", "ge\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Glaube nur! Es wird geschehn!", "tokens": ["Glau\u00b7be", "nur", "!", "Es", "wird", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Wende nicht den Blick zur\u00fcck!", "tokens": ["Wen\u00b7de", "nicht", "den", "Blick", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Wenn die Sommerwinde wehn,", "tokens": ["Wenn", "die", "Som\u00b7mer\u00b7win\u00b7de", "wehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Werden wir in Rosen gehn,", "tokens": ["Wer\u00b7den", "wir", "in", "Ro\u00b7sen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Und die Sonne lacht uns Gl\u00fcck.", "tokens": ["Und", "die", "Son\u00b7ne", "lacht", "uns", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}}}}