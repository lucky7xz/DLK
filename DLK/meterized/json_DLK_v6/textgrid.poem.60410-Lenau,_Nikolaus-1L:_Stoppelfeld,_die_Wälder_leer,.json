{"textgrid.poem.60410": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Stoppelfeld, die W\u00e4lder leer,", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stoppelfeld, die W\u00e4lder leer,", "tokens": ["Stop\u00b7pel\u00b7feld", ",", "die", "W\u00e4l\u00b7der", "leer", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es irrt der Wind verlassen,", "tokens": ["Und", "es", "irrt", "der", "Wind", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil kein Laub zu finden mehr,", "tokens": ["Weil", "kein", "Laub", "zu", "fin\u00b7den", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PTKZU", "VVINF", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Rauschend seinen Gru\u00df zu fassen.", "tokens": ["Rau\u00b7schend", "sei\u00b7nen", "Gru\u00df", "zu", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kranich scheidet von der Flur,", "tokens": ["Kra\u00b7nich", "schei\u00b7det", "von", "der", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der k\u00fchlen, lebensm\u00fcden,", "tokens": ["Von", "der", "k\u00fch\u00b7len", ",", "le\u00b7bens\u00b7m\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Freudig ruft ers, da\u00df die Spur", "tokens": ["Freu\u00b7dig", "ruft", "ers", ",", "da\u00df", "die", "Spur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PIS", "$,", "KOUS", "ART", "NN"], "meter": "+-++--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Er gefunden nach dem S\u00fcden.", "tokens": ["Er", "ge\u00b7fun\u00b7den", "nach", "dem", "S\u00fc\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mitten durch den Herbstesfrost", "tokens": ["Mit\u00b7ten", "durch", "den", "Herbs\u00b7tes\u00b7frost"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schickt der Lenz aus fernen Landen", "tokens": ["Schickt", "der", "Lenz", "aus", "fer\u00b7nen", "Lan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem Zugvogel seinen Trost,", "tokens": ["Dem", "Zug\u00b7vo\u00b7gel", "sei\u00b7nen", "Trost", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Heimlich mit ihm einverstanden.", "tokens": ["Heim\u00b7lich", "mit", "ihm", "ein\u00b7ver\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "O wie mag dem Vogel sein,", "tokens": ["O", "wie", "mag", "dem", "Vo\u00b7gel", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ihm durch das Nebeld\u00fcster", "tokens": ["Wenn", "ihm", "durch", "das", "Ne\u00b7bel\u00b7d\u00fcs\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Z\u00fcckt ins Herz der warme Schein", "tokens": ["Z\u00fcckt", "ins", "Herz", "der", "war\u00b7me", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das ferne Waldgefl\u00fcster!", "tokens": ["Und", "das", "fer\u00b7ne", "Wald\u00b7ge\u00b7fl\u00fcs\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hoch im Fluge \u00fcbers Meer", "tokens": ["Hoch", "im", "Flu\u00b7ge", "\u00fc\u00b7bers", "Meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00e4rket ihn der Duft der Auen;", "tokens": ["St\u00e4r\u00b7ket", "ihn", "der", "Duft", "der", "Au\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O wie s\u00fc\u00df empfindet er", "tokens": ["O", "wie", "s\u00fc\u00df", "emp\u00b7fin\u00b7det", "er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADJD", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ahndung, Sehnsucht und Vertrauen!", "tokens": ["Ahn\u00b7dung", ",", "Sehn\u00b7sucht", "und", "Ver\u00b7trau\u00b7en", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Nebel auf die Stoppeln taut;", "tokens": ["Ne\u00b7bel", "auf", "die", "Stop\u00b7peln", "taut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "D\u00fcrr der Wald; \u2013 ich duld es gerne,", "tokens": ["D\u00fcrr", "der", "Wald", ";", "\u2013", "ich", "duld", "es", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$.", "$(", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seit gegeben seinen Laut", "tokens": ["Seit", "ge\u00b7ge\u00b7ben", "sei\u00b7nen", "Laut"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVPP", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kranich, wandernd in die Ferne.", "tokens": ["Kra\u00b7nich", ",", "wan\u00b7dernd", "in", "die", "Fer\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Hab ich gleich, als ich so sacht", "tokens": ["Hab", "ich", "gleich", ",", "als", "ich", "so", "sacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "$,", "KOUS", "PPER", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die Stoppeln hingeschritten,", "tokens": ["Durch", "die", "Stop\u00b7peln", "hin\u00b7ge\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aller Sensen auch gedacht,", "tokens": ["Al\u00b7ler", "Sen\u00b7sen", "auch", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die ins Leben mir geschnitten;", "tokens": ["Die", "ins", "Le\u00b7ben", "mir", "ge\u00b7schnit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Hab ich gleich am d\u00fcrren Strauch", "tokens": ["Hab", "ich", "gleich", "am", "d\u00fcr\u00b7ren", "Strauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Andres Welk bedauern m\u00fcssen,", "tokens": ["And\u00b7res", "Welk", "be\u00b7dau\u00b7ern", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "VMINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als das Laub, vom Windeshauch", "tokens": ["Als", "das", "Laub", ",", "vom", "Win\u00b7des\u00b7hauch"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aufgewirbelt mir zu F\u00fc\u00dfen:", "tokens": ["Auf\u00b7ge\u00b7wir\u00b7belt", "mir", "zu", "F\u00fc\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Aber ohne Gram und Groll", "tokens": ["A\u00b7ber", "oh\u00b7ne", "Gram", "und", "Groll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Blick ich nach den Freudengr\u00fcften,", "tokens": ["Blick", "ich", "nach", "den", "Freu\u00b7den\u00b7gr\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn das Herz im Busen scholl,", "tokens": ["Denn", "das", "Herz", "im", "Bu\u00b7sen", "scholl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie der Vogel in den L\u00fcften;", "tokens": ["Wie", "der", "Vo\u00b7gel", "in", "den", "L\u00fcf\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Ja, das Herz in meiner Brust", "tokens": ["Ja", ",", "das", "Herz", "in", "mei\u00b7ner", "Brust"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist dem Kranich gleich geartet,", "tokens": ["Ist", "dem", "Kra\u00b7nich", "gleich", "ge\u00b7ar\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihm ist das Land bewu\u00dft,", "tokens": ["Und", "ihm", "ist", "das", "Land", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo mein Fr\u00fchling mich erwartet.", "tokens": ["Wo", "mein", "Fr\u00fch\u00b7ling", "mich", "er\u00b7war\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}