{"dta.poem.13037": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eso schmeichelt jede sich, wie man mir beygebracht,", "tokens": ["\u201e", "so", "schmei\u00b7chelt", "je\u00b7de", "sich", ",", "wie", "man", "mir", "bey\u00b7ge\u00b7bracht", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PIAT", "PRF", "$,", "PWAV", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201edurch ihrer Tugend Amt des Rangs, der Ehren-Pracht?", "tokens": ["\u201e", "durch", "ih\u00b7rer", "Tu\u00b7gend", "Amt", "des", "Rangs", ",", "der", "Eh\u00b7ren\u00b7Pracht", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "NN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "255\u201dMan \u00fcbereilet sich; durch Eilen kann man fehlen:", "tokens": ["\"", "Man", "\u00fc\u00b7be\u00b7rei\u00b7let", "sich", ";", "durch", "Ei\u00b7len", "kann", "man", "feh\u00b7len", ":"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VVFIN", "PRF", "$.", "APPR", "NN", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201egar selten pflegt man recht mit Eil\u2019 und Streit zu w\u00e4hlen:", "tokens": ["\u201e", "gar", "sel\u00b7ten", "pflegt", "man", "recht", "mit", "Eil'", "und", "Streit", "zu", "w\u00e4h\u00b7len", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "VVFIN", "PIS", "ADV", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201enoch weniger wo man den Wiedersinn erweckt,", "tokens": ["\u201e", "noch", "we\u00b7ni\u00b7ger", "wo", "man", "den", "Wie\u00b7der\u00b7sinn", "er\u00b7weckt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "PWAV", "PIS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eund einer Frage Schlu\u00df in tausend Fragen steckt,", "tokens": ["\u201e", "und", "ei\u00b7ner", "Fra\u00b7ge", "Schlu\u00df", "in", "tau\u00b7send", "Fra\u00b7gen", "steckt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ART", "NN", "NN", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ewie diese von dem Frie\u00df, und von dem Ruhm-Geb\u00e4ude.", "tokens": ["\u201e", "wie", "die\u00b7se", "von", "dem", "Frie\u00df", ",", "und", "von", "dem", "Ruhm\u00b7Ge\u00b7b\u00e4u\u00b7de", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PDS", "APPR", "ART", "NN", "$,", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "260\u201dMir scheint da\u00df man so leicht die Frage nicht entscheide.", "tokens": ["\"", "Mir", "scheint", "da\u00df", "man", "so", "leicht", "die", "Fra\u00b7ge", "nicht", "ent\u00b7schei\u00b7de", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "KOUS", "PIS", "ADV", "ADJD", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}