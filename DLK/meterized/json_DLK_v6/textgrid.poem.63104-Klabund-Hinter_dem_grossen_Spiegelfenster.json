{"textgrid.poem.63104": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Hinter dem grossen Spiegelfenster", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hinter dem gro\u00dfen Spiegelfenster des Caf\u00e9s", "tokens": ["Hin\u00b7ter", "dem", "gro\u00b7\u00dfen", "Spie\u00b7gel\u00b7fens\u00b7ter", "des", "Ca\u00b7f\u00e9s"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Sitz ich und sehe hei\u00df auf das Stra\u00dfenpflaster,", "tokens": ["Sitz", "ich", "und", "se\u00b7he", "hei\u00df", "auf", "das", "Stra\u00b7\u00dfen\u00b7pflas\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Suche im Treiben der Farben und K\u00f6rper Heilung meines sentimentalen Weh's,", "tokens": ["Su\u00b7che", "im", "Trei\u00b7ben", "der", "Far\u00b7ben", "und", "K\u00f6r\u00b7per", "Hei\u00b7lung", "mei\u00b7nes", "sen\u00b7ti\u00b7men\u00b7ta\u00b7len", "Weh's", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "KON", "NN", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+-+-+--+-+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Sehe viele Frauen, Fremde, bunte Offiziere, Gauner, Japaner, sogar einen Negermaster.", "tokens": ["Se\u00b7he", "vie\u00b7le", "Frau\u00b7en", ",", "Frem\u00b7de", ",", "bun\u00b7te", "Of\u00b7fi\u00b7zie\u00b7re", ",", "Gau\u00b7ner", ",", "Ja\u00b7pa\u00b7ner", ",", "so\u00b7gar", "ei\u00b7nen", "Ne\u00b7ger\u00b7mas\u00b7ter", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,", "NN", "$,", "ADJA", "NN", "$,", "NN", "$,", "NE", "$,", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+--+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Alle blicken sie zu mir und haben Sehnsucht nach der Musik im Innern,", "tokens": ["Al\u00b7le", "bli\u00b7cken", "sie", "zu", "mir", "und", "ha\u00b7ben", "Sehn\u00b7sucht", "nach", "der", "Mu\u00b7sik", "im", "In\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "PPER", "KON", "VAFIN", "NN", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.6": {"text": "Wollen tr\u00e4umerisch- und sanfter T\u00f6ne sich erinnern.", "tokens": ["Wol\u00b7len", "tr\u00e4u\u00b7me\u00b7risch", "und", "sanf\u00b7ter", "T\u00f6\u00b7ne", "sich", "e\u00b7rin\u00b7nern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "TRUNC", "KON", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "Aber ich, an meinen Stuhl gebannt und gebrannt,", "tokens": ["A\u00b7ber", "ich", ",", "an", "mei\u00b7nen", "Stuhl", "ge\u00b7bannt", "und", "ge\u00b7brannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPR", "PPOSAT", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.8": {"text": "Starre, staune nach drau\u00dfen unverwandt,", "tokens": ["Star\u00b7re", ",", "stau\u00b7ne", "nach", "drau\u00b7\u00dfen", "un\u00b7ver\u00b7wandt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "ADV", "ADJD", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Da\u00df jemand komme, freiwillig, nicht gedr\u00e4ngt,", "tokens": ["Da\u00df", "je\u00b7mand", "kom\u00b7me", ",", "frei\u00b7wil\u00b7lig", ",", "nicht", "ge\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "ADJD", "$,", "PTKNEG", "VVPP", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Ein blondes M\u00e4dchen... eine braune Dirne...", "tokens": ["Ein", "blon\u00b7des", "M\u00e4d\u00b7chen", "...", "ei\u00b7ne", "brau\u00b7ne", "Dir\u00b7ne", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "In rosa, gelber, violetter Taille...", "tokens": ["In", "ro\u00b7sa", ",", "gel\u00b7ber", ",", "vi\u00b7o\u00b7let\u00b7ter", "Tail\u00b7le", "..."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "... Oder meinetwegen eine dicke Rentierkanaille", "tokens": ["...", "O\u00b7der", "mei\u00b7net\u00b7we\u00b7gen", "ei\u00b7ne", "di\u00b7cke", "Ren\u00b7tier\u00b7ka\u00b7nail\u00b7le"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.13": {"text": "Mit schmalzigem, verfettetem Hirne \u2013", "tokens": ["Mit", "schmal\u00b7zi\u00b7gem", ",", "ver\u00b7fet\u00b7te\u00b7tem", "Hir\u00b7ne", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Nur da\u00df er mir f\u00fcr f\u00fcnf Minuten seine Gegenwart schenkt!", "tokens": ["Nur", "da\u00df", "er", "mir", "f\u00fcr", "f\u00fcnf", "Mi\u00b7nu\u00b7ten", "sei\u00b7ne", "Ge\u00b7gen\u00b7wart", "schenkt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPER", "APPR", "CARD", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.15": {"text": "Ich bin so einsam! Einsamer noch macht mich die s\u00fc\u00dfe Operette...", "tokens": ["Ich", "bin", "so", "ein\u00b7sam", "!", "Ein\u00b7sa\u00b7mer", "noch", "macht", "mich", "die", "s\u00fc\u00b7\u00dfe", "O\u00b7per\u00b7et\u00b7te", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$.", "NN", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+-++--+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.16": {"text": "O l\u00e4g ich irgendwo in dunkler Nacht,", "tokens": ["O", "l\u00e4g", "ich", "ir\u00b7gend\u00b7wo", "in", "dunk\u00b7ler", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Ein Kind, in einem Kinderbette,", "tokens": ["Ein", "Kind", ",", "in", "ei\u00b7nem", "Kin\u00b7der\u00b7bet\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Von einer Mutter zart zur Ruh gebracht...", "tokens": ["Von", "ei\u00b7ner", "Mut\u00b7ter", "zart", "zur", "Ruh", "ge\u00b7bracht", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}