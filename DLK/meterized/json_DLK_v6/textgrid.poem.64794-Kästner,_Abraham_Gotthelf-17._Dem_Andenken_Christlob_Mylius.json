{"textgrid.poem.64794": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "17. Dem Andenken Christlob Mylius", "genre": "verse", "period": "N.A.", "pub_year": 1754, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund, den voll Schmerz mehr, als nur Freunde, missen,", "tokens": ["Freund", ",", "den", "voll", "Schmerz", "mehr", ",", "als", "nur", "Freun\u00b7de", ",", "mis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJD", "NN", "ADV", "$,", "KOUS", "ADV", "NN", "$,", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Der, wenn er stirbt, der Weisen Menge kr\u00e4nkt,", "tokens": ["Der", ",", "wenn", "er", "stirbt", ",", "der", "Wei\u00b7sen", "Men\u00b7ge", "kr\u00e4nkt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "VVFIN", "$,", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Deutschland klagt: Mir ward ein Sohn entrissen;", "tokens": ["Den", "Deutschland", "klagt", ":", "Mir", "ward", "ein", "Sohn", "ent\u00b7ris\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Stolz w\u00e4r auf ihn das Land, das ihn versenkt.", "tokens": ["Stolz", "w\u00e4r", "auf", "ihn", "das", "Land", ",", "das", "ihn", "ver\u00b7senkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie wissen wir, wann wir den Sch\u00f6pfer h\u00f6ren?", "tokens": ["Wie", "wis\u00b7sen", "wir", ",", "wann", "wir", "den", "Sch\u00f6p\u00b7fer", "h\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "War nicht dein Trieb, dein starker Trieb, sein Ruf?", "tokens": ["War", "nicht", "dein", "Trieb", ",", "dein", "star\u00b7ker", "Trieb", ",", "sein", "Ruf", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der ri\u00df dich hin, die Wunder uns zu lehren,", "tokens": ["Der", "ri\u00df", "dich", "hin", ",", "die", "Wun\u00b7der", "uns", "zu", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die unser Gott im fernen Westen schuf.", "tokens": ["Die", "un\u00b7ser", "Gott", "im", "fer\u00b7nen", "Wes\u00b7ten", "schuf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und leitet uns, wo uns kein Engel f\u00fchret,", "tokens": ["Und", "lei\u00b7tet", "uns", ",", "wo", "uns", "kein", "En\u00b7gel", "f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein edler Trieb von unbesiegter Macht,", "tokens": ["Ein", "ed\u00b7ler", "Trieb", "von", "un\u00b7be\u00b7sieg\u00b7ter", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zerst\u00f6rt sie denn, die Hand, die uns regieret,", "tokens": ["Zer\u00b7st\u00f6rt", "sie", "denn", ",", "die", "Hand", ",", "die", "uns", "re\u00b7gie\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Werkzeug schon, noch eh' es war verbracht?", "tokens": ["Ihr", "Werk\u00b7zeug", "schon", ",", "noch", "eh'", "es", "war", "ver\u00b7bracht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,", "ADV", "KOUS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So fragen wir, wir noch gefangne Seelen,", "tokens": ["So", "fra\u00b7gen", "wir", ",", "wir", "noch", "ge\u00b7fang\u00b7ne", "See\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Todt ist f\u00fcr uns, was nicht der Erde lebt:", "tokens": ["Todt", "ist", "f\u00fcr", "uns", ",", "was", "nicht", "der", "Er\u00b7de", "lebt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPER", "$,", "PRELS", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer ist so k\u00fchn, die Aemter durchzuz\u00e4hlen,", "tokens": ["Wer", "ist", "so", "k\u00fchn", ",", "die", "A\u00b7em\u00b7ter", "durch\u00b7zu\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+----+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Zu denen Gott befreyte Geister hebt?", "tokens": ["Zu", "de\u00b7nen", "Gott", "be\u00b7frey\u00b7te", "Geis\u00b7ter", "hebt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Als an das Ziel, das ich mir vorgestellt?", "tokens": ["Als", "an", "das", "Ziel", ",", "das", "ich", "mir", "vor\u00b7ge\u00b7stellt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So sprach mein Freund, schon eilt er durch die H\u00f6hen;", "tokens": ["So", "sprach", "mein", "Freund", ",", "schon", "eilt", "er", "durch", "die", "H\u00f6\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fern unter ihm liegt bald die neue Welt:", "tokens": ["Fern", "un\u00b7ter", "ihm", "liegt", "bald", "die", "neu\u00b7e", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Auf die sein Blick, der um die Erde streifet,", "tokens": ["Auf", "die", "sein", "Blick", ",", "der", "um", "die", "Er\u00b7de", "strei\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Blick im Fliehn, nun ohne Sehnsucht schie\u00dft;", "tokens": ["Ein", "Blick", "im", "Fliehn", ",", "nun", "oh\u00b7ne", "Sehn\u00b7sucht", "schie\u00dft", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie, wenn ein Kind zum Manne pl\u00f6tzlich reifet,", "tokens": ["Wie", ",", "wenn", "ein", "Kind", "zum", "Man\u00b7ne", "pl\u00f6tz\u00b7lich", "rei\u00b7fet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "ART", "NN", "APPRART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein Spielzeug sieht, und gleich darauf vergi\u00dft.", "tokens": ["Sein", "Spiel\u00b7zeug", "sieht", ",", "und", "gleich", "da\u00b7rauf", "ver\u00b7gi\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KON", "ADV", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Er sieht des Monds uns nie gesehnen R\u00fccken,", "tokens": ["Er", "sieht", "des", "Monds", "uns", "nie", "ge\u00b7seh\u00b7nen", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und nah' am Mars, noch einmal umgekehrt,", "tokens": ["Und", "nah'", "am", "Mars", ",", "noch", "ein\u00b7mal", "um\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fchlt er f\u00fcr uns: \u00bbWie? jenes Punktes St\u00fccken", "tokens": ["F\u00fchlt", "er", "f\u00fcr", "uns", ":", "\u00bb", "Wie", "?", "je\u00b7nes", "Punk\u00b7tes", "St\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$.", "$(", "PWAV", "$.", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sind Menschenblut, sind ew'ge Seelen werth!", "tokens": ["Sind", "Men\u00b7schen\u00b7blut", ",", "sind", "ew'\u00b7ge", "See\u00b7len", "werth", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "VAFIN", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Im gro\u00dfen Ball, dem vier Begleiter gl\u00e4nzen,", "tokens": ["Im", "gro\u00b7\u00dfen", "Ball", ",", "dem", "vier", "Be\u00b7glei\u00b7ter", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Riesen nach gebundne Zwerge ziehn.", "tokens": ["Wie", "Rie\u00b7sen", "nach", "ge\u00b7bund\u00b7ne", "Zwer\u00b7ge", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was w\u00e4ren da des Schachs und Mogols Grenzen?", "tokens": ["Was", "w\u00e4\u00b7ren", "da", "des", "Schachs", "und", "Mo\u00b7gols", "Gren\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ART", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Menschen Welt w\u00e4r auch ein Zwerg f\u00fcr ihn.\u00ab", "tokens": ["Der", "Men\u00b7schen", "Welt", "w\u00e4r", "auch", "ein", "Zwerg", "f\u00fcr", "ihn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "ART", "NN", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Drauf hat sein Flug den Ring Saturns gefunden,", "tokens": ["Drauf", "hat", "sein", "Flug", "den", "Ring", "Sa\u00b7turns", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPOSAT", "NN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und weil er bald sich senkt, bald wieder steigt,", "tokens": ["Und", "weil", "er", "bald", "sich", "senkt", ",", "bald", "wie\u00b7der", "steigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PRF", "VVFIN", "$,", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wird das f\u00fcr ihn ein Schauspiel von Sekunden,", "tokens": ["Wird", "das", "f\u00fcr", "ihn", "ein", "Schau\u00b7spiel", "von", "Se\u00b7kun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "APPR", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was sp\u00e4t sich uns in funfzehn Jahren zeigt", "tokens": ["Was", "sp\u00e4t", "sich", "uns", "in", "funf\u00b7zehn", "Jah\u00b7ren", "zeigt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "PRF", "PPER", "APPR", "CARD", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Denn, wie ein Sklav, den schwere Fesseln zwingen,", "tokens": ["Denn", ",", "wie", "ein", "Sklav", ",", "den", "schwe\u00b7re", "Fes\u00b7seln", "zwin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit matter Hand ein grobes Werkzeug f\u00fchrt,", "tokens": ["Mit", "mat\u00b7ter", "Hand", "ein", "gro\u00b7bes", "Werk\u00b7zeug", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wirkt hier der Geist, der dort die leichten Schwingen", "tokens": ["Wirkt", "hier", "der", "Geist", ",", "der", "dort", "die", "leich\u00b7ten", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit freyer Kraft im weiten Aether r\u00fchrt.", "tokens": ["Mit", "frey\u00b7er", "Kraft", "im", "wei\u00b7ten", "A\u00b7e\u00b7ther", "r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.11": {"line.1": {"text": "Wo unsichtbar f\u00fcr uns Kometen gehen,", "tokens": ["Wo", "un\u00b7sicht\u00b7bar", "f\u00fcr", "uns", "Ko\u00b7me\u00b7ten", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ward noch sein Blick durchs Sonnenreich gesandt,", "tokens": ["Ward", "noch", "sein", "Blick", "durchs", "Son\u00b7nen\u00b7reich", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als \u00fcbers\u00e4h', von's Bructers kalten H\u00f6hen,", "tokens": ["Als", "\u00fc\u00b7ber\u00b7s\u00e4h'", ",", "von's", "Bruc\u00b7ters", "kal\u00b7ten", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er noch einmal ", "tokens": ["Er", "noch", "ein\u00b7mal"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "Schon ist er hin, wo Wesen h\u00f6h'rer Stufen", "tokens": ["Schon", "ist", "er", "hin", ",", "wo", "We\u00b7sen", "h\u00f6h'\u00b7rer", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$,", "PWAV", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Mensch nicht f\u00fchlt, und nur der Geist erkennt,", "tokens": ["Der", "Mensch", "nicht", "f\u00fchlt", ",", "und", "nur", "der", "Geist", "er\u00b7kennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVFIN", "$,", "KON", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo Seelen sehn, wozu sie Gott berufen,", "tokens": ["Wo", "See\u00b7len", "sehn", ",", "wo\u00b7zu", "sie", "Gott", "be\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$,", "PWAV", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Vorsicht ist, was man hier Schicksal nennt.", "tokens": ["Und", "Vor\u00b7sicht", "ist", ",", "was", "man", "hier", "Schick\u00b7sal", "nennt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "$,", "PRELS", "PIS", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Dort ist vor ihm die K\u00f6rperwelt vergangen,", "tokens": ["Dort", "ist", "vor", "ihm", "die", "K\u00f6r\u00b7per\u00b7welt", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gro\u00df zeigt sich ihm Gott in der wahren Welt,", "tokens": ["Gro\u00df", "zeigt", "sich", "ihm", "Gott", "in", "der", "wah\u00b7ren", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "PPER", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Von ihr wird uns, die wir mit Einsicht prangen,", "tokens": ["Von", "ihr", "wird", "uns", ",", "die", "wir", "mit", "Ein\u00b7sicht", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ein Theil, wie Nichts, umnebelt vorgestellt.", "tokens": ["Ein", "Theil", ",", "wie", "Nichts", ",", "um\u00b7ne\u00b7belt", "vor\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PIS", "$,", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Der mu\u00df f\u00fcr uns, weil wir nichts Bessers kennen,", "tokens": ["Der", "mu\u00df", "f\u00fcr", "uns", ",", "weil", "wir", "nichts", "Bes\u00b7sers", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "PPER", "$,", "KOUS", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Allmacht Reich, der Weisheit Schauplatz seyn;", "tokens": ["Der", "All\u00b7macht", "Reich", ",", "der", "Weis\u00b7heit", "Schau\u00b7platz", "seyn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So wagt ein Hirt", "tokens": ["So", "wagt", "ein", "Hirt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und dachte,