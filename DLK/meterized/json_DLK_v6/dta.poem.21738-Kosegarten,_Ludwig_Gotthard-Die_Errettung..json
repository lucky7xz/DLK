{"dta.poem.21738": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Die Errettung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vor&#252;ber ist der schwere Traum,", "tokens": ["Vor", "&#252;", "ber", "ist", "der", "schwe\u00b7re", "Traum", ","], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$(", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor\u00fcber, und ich glaub' es kaum. . . .", "tokens": ["Vor\u00b7\u00fc\u00b7ber", ",", "und", "ich", "glaub'", "es", "kaum", ".", ".", ".", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["ADV", "$,", "KON", "PPER", "VVFIN", "PPER", "ADV", "$.", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich lebe!", "tokens": ["Ich", "le\u00b7be", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Ich lebe ja! und unentweiht", "tokens": ["Ich", "le\u00b7be", "ja", "!", "und", "un\u00b7ent\u00b7weiht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$.", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Blieb meiner Unschuld weisses Kleid.", "tokens": ["Blieb", "mei\u00b7ner", "Un\u00b7schuld", "weis\u00b7ses", "Kleid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich hebe,", "tokens": ["Ich", "he\u00b7be", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Ihr heil'gen Jungfraun, nach wie vor", "tokens": ["Ihr", "heil'\u00b7gen", "Jung\u00b7fraun", ",", "nach", "wie", "vor"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "APPR", "KOKOM", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den Blick zu eurem reinen Chor", "tokens": ["Den", "Blick", "zu", "eu\u00b7rem", "rei\u00b7nen", "Chor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Rein empor!", "tokens": ["Rein", "em\u00b7por", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Es drohte namenlose Noth,", "tokens": ["Es", "droh\u00b7te", "na\u00b7men\u00b7lo\u00b7se", "Noth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und Schande, herber als der Tod,", "tokens": ["Und", "Schan\u00b7de", ",", "her\u00b7ber", "als", "der", "Tod", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADV", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der Armen! \u2014", "tokens": ["Der", "Ar\u00b7men", "!"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ART", "NN", "$.", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "\u201edie ihr die Unschuld schirmt und sch\u00fctzt,", "tokens": ["\u201e", "die", "ihr", "die", "Un\u00b7schuld", "schirmt", "und", "sch\u00fctzt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "PPER", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "\u201eden Frevler z\u00fcrnend niederblitzt,", "tokens": ["\u201e", "den", "Frev\u00b7ler", "z\u00fcr\u00b7nend", "nie\u00b7der\u00b7blitzt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "\u201eerbarmen!", "tokens": ["\u201e", "er\u00b7bar\u00b7men", "!"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.16": {"text": "\u201each rettet, rettet! Grimmiglich", "tokens": ["\u201e", "ach", "ret\u00b7tet", ",", "ret\u00b7tet", "!", "Grim\u00b7mig\u00b7lich"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["$(", "ITJ", "VVFIN", "$,", "VVFIN", "$.", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "\u201eumgrinsen H\u00f6llenlarven mich!", "tokens": ["\u201e", "um\u00b7grin\u00b7sen", "H\u00f6l\u00b7len\u00b7lar\u00b7ven", "mich", "!"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "\u201erettet mich!\u201c \u2014", "tokens": ["\u201e", "ret\u00b7tet", "mich", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "$.", "$(", "$("], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Und nieder von dem Sitz der Ruh,", "tokens": ["Und", "nie\u00b7der", "von", "dem", "Sitz", "der", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Christus Armen schautest du,", "tokens": ["Aus", "Chris\u00b7tus", "Ar\u00b7men", "schau\u00b7test", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Madonne!", "tokens": ["Ma\u00b7don\u00b7ne", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Es weinte laut der Jungfraun Chor,", "tokens": ["Es", "wein\u00b7te", "laut", "der", "Jung\u00b7fraun", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und gn\u00e4dig neigte Gott sein Ohr.", "tokens": ["Und", "gn\u00e4\u00b7dig", "neig\u00b7te", "Gott", "sein", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O Wonne!", "tokens": ["O", "Won\u00b7ne", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Er schalt. Die H\u00f6ll' entsetzte sich.", "tokens": ["Er", "schalt", ".", "Die", "H\u00f6ll'", "ent\u00b7setz\u00b7te", "sich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Larven grass und grauerlich", "tokens": ["Die", "Lar\u00b7ven", "grass", "und", "grau\u00b7er\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Liessen mich.", "tokens": ["Lies\u00b7sen", "mich", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Du, deren Flehn den Sohn bezwang,", "tokens": ["Du", ",", "de\u00b7ren", "Flehn", "den", "Sohn", "be\u00b7zwang", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Madonna, lass mich meinen Dank", "tokens": ["Ma\u00b7don\u00b7na", ",", "lass", "mich", "mei\u00b7nen", "Dank"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Dir weinen!", "tokens": ["Dir", "wei\u00b7nen", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Die ihr mein Antlitz nicht besch\u00e4mt,", "tokens": ["Die", "ihr", "mein", "Ant\u00b7litz", "nicht", "be\u00b7sch\u00e4mt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Des Treuen treulich euch annehmt,", "tokens": ["Des", "Treu\u00b7en", "treu\u00b7lich", "euch", "an\u00b7nehmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ihr Reinen,", "tokens": ["Ihr", "Rei\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.16": {"text": "Euch soll Biankens Lobgesang,", "tokens": ["Euch", "soll", "Bi\u00b7an\u00b7kens", "Lob\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Euch str\u00f6men soll Biankens Dank", "tokens": ["Euch", "str\u00f6\u00b7men", "soll", "Bi\u00b7an\u00b7kens", "Dank"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Lebenslang.", "tokens": ["Le\u00b7bens\u00b7lang", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}