{"textgrid.poem.53542": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Sch\u00e4ferliedchen", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Kaiser ist ein braver Mann,", "tokens": ["Der", "Kai\u00b7ser", "ist", "ein", "bra\u00b7ver", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "doch leider nicht zu Haus,", "tokens": ["doch", "lei\u00b7der", "nicht", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und mancher gute B\u00fcrgersmann", "tokens": ["und", "man\u00b7cher", "gu\u00b7te", "B\u00fcr\u00b7gers\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "zieht still sein Schnupftuch raus.", "tokens": ["zieht", "still", "sein", "Schnupf\u00b7tuch", "raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und er beweint so tr\u00e4nenna\u00df", "tokens": ["Und", "er", "be\u00b7weint", "so", "tr\u00e4\u00b7nen\u00b7na\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "den kaiserlichen Bann \u2013", "tokens": ["den", "kai\u00b7ser\u00b7li\u00b7chen", "Bann", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "und sonst noch was und sonst noch was,", "tokens": ["und", "sonst", "noch", "was", "und", "sonst", "noch", "was", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PWS", "KON", "ADV", "ADV", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "was ich nicht sagen kann.", "tokens": ["was", "ich", "nicht", "sa\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie war sie sch\u00f6n, die gro\u00dfe Zeit!", "tokens": ["Wie", "war", "sie", "sch\u00f6n", ",", "die", "gro\u00b7\u00dfe", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man f\u00fchlte sich als Gott.", "tokens": ["Man", "f\u00fchl\u00b7te", "sich", "als", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "KOUS", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man nutzte die Gelegenheit", "tokens": ["Man", "nutz\u00b7te", "die", "Ge\u00b7le\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ganz aus, bis zum Bankrott.", "tokens": ["ganz", "aus", ",", "bis", "zum", "Bank\u00b7rott", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "KOUS", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Der Orden reiches \u00dcberma\u00df", "tokens": ["Der", "Or\u00b7den", "rei\u00b7ches", "\u00dc\u00b7berm\u00b7a\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "in manche H\u00e4nde rann", "tokens": ["in", "man\u00b7che", "H\u00e4n\u00b7de", "rann"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "und sonst noch was und sonst noch was,", "tokens": ["und", "sonst", "noch", "was", "und", "sonst", "noch", "was", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PWS", "KON", "ADV", "ADV", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "was ich nicht sagen kann.", "tokens": ["was", "ich", "nicht", "sa\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie standen tief im Flamenland", "tokens": ["Sie", "stan\u00b7den", "tief", "im", "Fla\u00b7men\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und tief im Russenreich.", "tokens": ["und", "tief", "im", "Rus\u00b7sen\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es herrschte dort die starke Hand;", "tokens": ["Es", "herrschte", "dort", "die", "star\u00b7ke", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "bei Panjes galt das gleich.", "tokens": ["bei", "Pan\u00b7jes", "galt", "das", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PDS", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie nahmen mit den tiefen Ha\u00df", "tokens": ["Sie", "nah\u00b7men", "mit", "den", "tie\u00b7fen", "Ha\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "von Weib und Kind und Mann", "tokens": ["von", "Weib", "und", "Kind", "und", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "und sonst noch was und sonst noch was,", "tokens": ["und", "sonst", "noch", "was", "und", "sonst", "noch", "was", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PWS", "KON", "ADV", "ADV", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "was ich nicht sagen kann.", "tokens": ["was", "ich", "nicht", "sa\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und das ist alles nun dahin.", "tokens": ["Und", "das", "ist", "al\u00b7les", "nun", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PIS", "ADV", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was Wunder, da\u00df es klagt:", "tokens": ["Was", "Wun\u00b7der", ",", "da\u00df", "es", "klagt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbweh, da\u00df ich ohne Kaiser bin!", "tokens": ["\u00bb", "weh", ",", "da\u00df", "ich", "oh\u00b7ne", "Kai\u00b7ser", "bin", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "KOUS", "PPER", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie hat mir der behagt!\u00ab", "tokens": ["Wie", "hat", "mir", "der", "be\u00b7hagt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie machen sich die \u00c4uglein na\u00df,", "tokens": ["Sie", "ma\u00b7chen", "sich", "die", "\u00c4ug\u00b7lein", "na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "die Herren um Stresemann,", "tokens": ["die", "Her\u00b7ren", "um", "Stre\u00b7se\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "und sonst noch was und sonst noch was,", "tokens": ["und", "sonst", "noch", "was", "und", "sonst", "noch", "was", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PWS", "KON", "ADV", "ADV", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "was ich nicht sagen kann.", "tokens": ["was", "ich", "nicht", "sa\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}