{"dta.poem.20670": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An die Sonne  \n bei  \n dem Leichenbeg\u00e4ngnisse Friedrichs des Gr\u00f6\u00dften", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geliebte F\u00fcrstin der Natur,                 ", "tokens": ["Ge\u00b7lieb\u00b7te", "F\u00fcrs\u00b7tin", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Sonne! h\u00fclle dich in Schleyerw\u00f6lkchen nur,", "tokens": ["O", "Son\u00b7ne", "!", "h\u00fcl\u00b7le", "dich", "in", "Schley\u00b7er\u00b7w\u00f6lk\u00b7chen", "nur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "PRF", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und nicht in eine schwere finstre Wolke,", "tokens": ["Und", "nicht", "in", "ei\u00b7ne", "schwe\u00b7re", "finst\u00b7re", "Wol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du sch\u00f6ne Himmelsmajest\u00e4t!", "tokens": ["Du", "sch\u00f6\u00b7ne", "Him\u00b7mels\u00b7ma\u00b7jes\u00b7t\u00e4t", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bleib freundlich diesem Trauervolke.", "tokens": ["Bleib", "freund\u00b7lich", "die\u00b7sem", "Trau\u00b7er\u00b7vol\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sieh, dieser Zug, der langsam geht,", "tokens": ["Sieh", ",", "die\u00b7ser", "Zug", ",", "der", "lang\u00b7sam", "geht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PDAT", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der K\u00f6nigliche Leichenwagen,", "tokens": ["Der", "K\u00f6\u00b7nig\u00b7li\u00b7che", "Lei\u00b7chen\u00b7wa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bedeutet mehr, als je dein Blick gesehn,", "tokens": ["Be\u00b7deu\u00b7tet", "mehr", ",", "als", "je", "dein", "Blick", "ge\u00b7sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wenn Weltbeherrscher fortgetragen", "tokens": ["Wenn", "Welt\u00b7be\u00b7herr\u00b7scher", "fort\u00b7ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "In Gr\u00fcfte wurden, wo kein Klagen,", "tokens": ["In", "Gr\u00fcf\u00b7te", "wur\u00b7den", ",", "wo", "kein", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "$,", "PWAV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Kein Opferbringen, und kein Flehn", "tokens": ["Kein", "Op\u00b7fer\u00b7brin\u00b7gen", ",", "und", "kein", "Flehn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "KON", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Den Hingetragnen weckt, wo d\u00fcster die Verwesung", "tokens": ["Den", "Hin\u00b7ge\u00b7trag\u00b7nen", "weckt", ",", "wo", "d\u00fcs\u00b7ter", "die", "Ver\u00b7we\u00b7sung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Auf ewig kaltem Throne sitzt,", "tokens": ["Auf", "e\u00b7wig", "kal\u00b7tem", "Thro\u00b7ne", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wenn Jahr an Jahr zur Neugenesung", "tokens": ["Wenn", "Jahr", "an", "Jahr", "zur", "Neu\u00b7ge\u00b7ne\u00b7sung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Dein milder Fr\u00fchlingsstrahl erhitzt", "tokens": ["Dein", "mil\u00b7der", "Fr\u00fch\u00b7lings\u00b7strahl", "er\u00b7hitzt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Die winterkrank gewesne Erde,", "tokens": ["Die", "win\u00b7ter\u00b7krank", "ge\u00b7wes\u00b7ne", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Da\u00df Baum und Pflanze wieder bl\u00fchn,", "tokens": ["Da\u00df", "Baum", "und", "Pflan\u00b7ze", "wie\u00b7der", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und Berg und Thal bekleidet werde", "tokens": ["Und", "Berg", "und", "Thal", "be\u00b7klei\u00b7det", "wer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Mit wiederfrischem Jugendgr\u00fcn:", "tokens": ["Mit", "wie\u00b7der\u00b7fri\u00b7schem", "Ju\u00b7gend\u00b7gr\u00fcn", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Nur Gras und Blumen kannst du wecken", "tokens": ["Nur", "Gras", "und", "Blu\u00b7men", "kannst", "du", "we\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Und Wurm und Schwalben, die ihr Haupt,", "tokens": ["Und", "Wurm", "und", "Schwal\u00b7ben", ",", "die", "ihr", "Haupt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,", "PRELS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Ihr leblos Haupt, im Sumpf verstecken;", "tokens": ["Ihr", "leb\u00b7los", "Haupt", ",", "im", "Sumpf", "ver\u00b7ste\u00b7cken", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$,", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Mehr ist dir nicht erlaubt \u2014", "tokens": ["Mehr", "ist", "dir", "nicht", "er\u00b7laubt"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die K\u00f6nige, die dir geglichen", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7ge", ",", "die", "dir", "ge\u00b7gli\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An Gr\u00f6\u00dfe, Mildigkeit und Macht,", "tokens": ["An", "Gr\u00f6\u00b7\u00dfe", ",", "Mil\u00b7dig\u00b7keit", "und", "Macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und so wie Laub und Gras verblichen,", "tokens": ["Und", "so", "wie", "Laub", "und", "Gras", "ver\u00b7bli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die werden nicht hervorgebracht", "tokens": ["Die", "wer\u00b7den", "nicht", "her\u00b7vor\u00b7ge\u00b7bracht"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PTKNEG", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aus ihren Gr\u00e4bern, wenn die Schwalbe", "tokens": ["Aus", "ih\u00b7ren", "Gr\u00e4\u00b7bern", ",", "wenn", "die", "Schwal\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch deine W\u00fcrkung wieder lebt,", "tokens": ["Durch", "dei\u00b7ne", "W\u00fcr\u00b7kung", "wie\u00b7der", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und b\u00e4te dich darum die halbe", "tokens": ["Und", "b\u00e4\u00b7te", "dich", "da\u00b7rum", "die", "hal\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "PAV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Verway\u00dfte Welt, die mit begr\u00e4bt", "tokens": ["Ver\u00b7way\u00df\u00b7te", "Welt", ",", "die", "mit", "be\u00b7gr\u00e4bt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "APPR", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ihr bl\u00fchend Gl\u00fcck, und Stolz, und Wonne,", "tokens": ["Ihr", "bl\u00fc\u00b7hend", "Gl\u00fcck", ",", "und", "Stolz", ",", "und", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVPP", "NN", "$,", "KON", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Du bist ohnm\u00e4chtig ihrem Ruf \u2014", "tokens": ["Du", "bist", "ohn\u00b7m\u00e4ch\u00b7tig", "ih\u00b7rem", "Ruf"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Du siehst nicht mehr als Morgensonne", "tokens": ["Du", "siehst", "nicht", "mehr", "als", "Mor\u00b7gen\u00b7son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Den Fr\u00fcherwachten, der schon in Gedanken schuf,", "tokens": ["Den", "Fr\u00fc\u00b7her\u00b7wach\u00b7ten", ",", "der", "schon", "in", "Ge\u00b7dan\u00b7ken", "schuf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Was Millionen Menschen n\u00fctzte,", "tokens": ["Was", "Mil\u00b7lion\u00b7en", "Men\u00b7schen", "n\u00fctz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.14": {"text": "Wenn deinem Glanz die Lerch entgegen sang \u2014", "tokens": ["Wenn", "dei\u00b7nem", "Glanz", "die", "Lerch", "ent\u00b7ge\u00b7gen", "sang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "PTKVZ", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Du siehst nicht mehr den Helden, der uns sch\u00fctzte,", "tokens": ["Du", "siehst", "nicht", "mehr", "den", "Hel\u00b7den", ",", "der", "uns", "sch\u00fctz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Der mit viel Feinden f\u00fcr uns rang.", "tokens": ["Der", "mit", "viel", "Fein\u00b7den", "f\u00fcr", "uns", "rang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Du wirst in Seiner Hand nicht mehr Sein Schwerdt", "tokens": ["Du", "wirst", "in", "Sei\u00b7ner", "Hand", "nicht", "mehr", "Sein", "Schwerdt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "verg\u00fclden,", "tokens": ["ver\u00b7g\u00fcl\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Er gab es Seinem Folgef\u00fcrst,", "tokens": ["Er", "gab", "es", "Sei\u00b7nem", "Fol\u00b7ge\u00b7f\u00fcrst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Den du dereinst in Schlachtgefilden", "tokens": ["Den", "du", "de\u00b7reinst", "in", "Schlacht\u00b7ge\u00b7fil\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Zu Heldenkampf auch wecken wirst,", "tokens": ["Zu", "Hel\u00b7den\u00b7kampf", "auch", "we\u00b7cken", "wirst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Wenn gegen uns ein Feind sich h\u00fcbe", "tokens": ["Wenn", "ge\u00b7gen", "uns", "ein", "Feind", "sich", "h\u00fc\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "ART", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Vom Waffenlager f\u00fcrchterlich \u2014", "tokens": ["Vom", "Waf\u00b7fen\u00b7la\u00b7ger", "f\u00fcrch\u00b7ter\u00b7lich"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Ihn wird auch Landesvaterliebe", "tokens": ["Ihn", "wird", "auch", "Lan\u00b7des\u00b7va\u00b7ter\u00b7lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Nicht ruhen lassen, wenn du dich", "tokens": ["Nicht", "ru\u00b7hen", "las\u00b7sen", ",", "wenn", "du", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "VVINF", "VVINF", "$,", "KOUS", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Schon zeigst im rosenfarbnen Schleyer:", "tokens": ["Schon", "zeigst", "im", "ro\u00b7sen\u00b7farb\u00b7nen", "Schle\u00b7yer", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.27": {"text": "Die\u00df ist Sein Vorsatz k\u00f6niglich \u2014", "tokens": ["Die\u00df", "ist", "Sein", "Vor\u00b7satz", "k\u00f6\u00b7nig\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er weint, Sein Vorbild war so gro\u00df, so lieb, so", "tokens": ["Er", "weint", ",", "Sein", "Vor\u00b7bild", "war", "so", "gro\u00df", ",", "so", "lieb", ",", "so"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "theuer,", "tokens": ["theu\u00b7er", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und ach, du selber tr\u00fcbest ja", "tokens": ["Und", "ach", ",", "du", "sel\u00b7ber", "tr\u00fc\u00b7best", "ja"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "XY", "$,", "PPER", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Antlitz bei der Leichenfeyer,", "tokens": ["Dein", "Ant\u00b7litz", "bei", "der", "Lei\u00b7chen\u00b7fe\u00b7yer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": 