{"textgrid.poem.47032": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bracht' ihr Blumen; als ich die nun immer", "tokens": ["Ich", "bracht'", "ihr", "Blu\u00b7men", ";", "als", "ich", "die", "nun", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$.", "KOUS", "PPER", "ART", "ADV", "ADV"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "An ihrer Brust nicht sah und dr\u00fcber klagte,", "tokens": ["An", "ih\u00b7rer", "Brust", "nicht", "sah", "und", "dr\u00fc\u00b7ber", "klag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "VVFIN", "KON", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Versetzte sie getrost: Weil mir's behagte,", "tokens": ["Ver\u00b7setz\u00b7te", "sie", "ge\u00b7trost", ":", "Weil", "mir's", "be\u00b7hag\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$.", "KOUS", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Recht lang' sie bl\u00fchn zu sehn, bl\u00fchn sie im Zimmer.", "tokens": ["Recht", "lang'", "sie", "bl\u00fchn", "zu", "sehn", ",", "bl\u00fchn", "sie", "im", "Zim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVINF", "PTKZU", "VVINF", "$,", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Band kauft' ich ihr, und als ich das auch nimmer", "tokens": ["Band", "kauft'", "ich", "ihr", ",", "und", "als", "ich", "das", "auch", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PPER", "$,", "KON", "KOUS", "PPER", "PDS", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An ihrem Arm ersp\u00e4ht' und spitzig fragte:", "tokens": ["An", "ih\u00b7rem", "Arm", "er\u00b7sp\u00e4ht'", "und", "spit\u00b7zig", "frag\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo bl\u00fcht nun das? sprach sie: Im Schrank; ich zagte,", "tokens": ["Wo", "bl\u00fcht", "nun", "das", "?", "sprach", "sie", ":", "Im", "Schrank", ";", "ich", "zag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PDS", "$.", "VVFIN", "PPER", "$.", "APPRART", "NN", "$.", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die Sonne bleich' ihm den zu seinen Schimmer.", "tokens": ["Die", "Son\u00b7ne", "bleich'", "ihm", "den", "zu", "sei\u00b7nen", "Schim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun spr\u00e4che jemand, der das nicht verst\u00fcnde:", "tokens": ["Nun", "spr\u00e4\u00b7che", "je\u00b7mand", ",", "der", "das", "nicht", "ver\u00b7st\u00fcn\u00b7de", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PRELS", "PDS", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O welche Liebe, die mit solcher Treue", "tokens": ["O", "wel\u00b7che", "Lie\u00b7be", ",", "die", "mit", "sol\u00b7cher", "Treu\u00b7e"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PWAT", "NN", "$,", "PRELS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bewahrt solch ein verg\u00e4nglich Angedenken.", "tokens": ["Be\u00b7wahrt", "solch", "ein", "ver\u00b7g\u00e4ng\u00b7lich", "An\u00b7ge\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich aber spreche, der ich's wohl ergr\u00fcnde:", "tokens": ["Ich", "a\u00b7ber", "spre\u00b7che", ",", "der", "ich's", "wohl", "er\u00b7gr\u00fcn\u00b7de", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "PRELS", "PIS", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O da\u00df den Neider solche Lieb' erfreue,", "tokens": ["O", "da\u00df", "den", "Nei\u00b7der", "sol\u00b7che", "Lieb'", "er\u00b7freu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ART", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Die, was ans Herz soll, niederlegt in Schr\u00e4nken.", "tokens": ["Die", ",", "was", "ans", "Herz", "soll", ",", "nie\u00b7der\u00b7legt", "in", "Schr\u00e4n\u00b7ken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "APPRART", "NN", "VMFIN", "$,", "VVFIN", "APPR", "NN", "$."], "meter": 