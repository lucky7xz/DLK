{"textgrid.poem.59523": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Marienw\u00fcrmchen", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Marienw\u00fcrmchen, flieg weg!", "tokens": ["Ma\u00b7rien\u00b7w\u00fcrm\u00b7chen", ",", "flieg", "weg", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Fliege nicht zu lange weg!", "tokens": ["Flie\u00b7ge", "nicht", "zu", "lan\u00b7ge", "weg", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PTKA", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcbern Zaun und \u00fcbers Heck,", "tokens": ["\u00dc\u00b7bern", "Zaun", "und", "\u00fc\u00b7bers", "Heck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcber Vaters Garten weg,", "tokens": ["\u00dc\u00b7ber", "Va\u00b7ters", "Gar\u00b7ten", "weg", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In die Welt hinaus \u2013", "tokens": ["In", "die", "Welt", "hin\u00b7aus", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APZR", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Vergi\u00df nicht dein Haus,", "tokens": ["Ver\u00b7gi\u00df", "nicht", "dein", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Vergi\u00df nicht die lieben Kinder.", "tokens": ["Ver\u00b7gi\u00df", "nicht", "die", "lie\u00b7ben", "Kin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Die Blumen sind hier ges\u00fcnder,", "tokens": ["Die", "Blu\u00b7men", "sind", "hier", "ge\u00b7s\u00fcn\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Die Sonne scheint hier heller.", "tokens": ["Die", "Son\u00b7ne", "scheint", "hier", "hel\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Drum, W\u00fcrmchen, fliege schneller", "tokens": ["Drum", ",", "W\u00fcrm\u00b7chen", ",", "flie\u00b7ge", "schnel\u00b7ler"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["PAV", "$,", "NN", "$,", "VVFIN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Zu Haus! Zu Haus! Zu Haus!", "tokens": ["Zu", "Haus", "!", "Zu", "Haus", "!", "Zu", "Haus", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "APPR", "NN", "$.", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Marienw\u00fcrmchen, flieg weg!", "tokens": ["Ma\u00b7rien\u00b7w\u00fcrm\u00b7chen", ",", "flieg", "weg", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Fliege nicht zu lange weg!", "tokens": ["Flie\u00b7ge", "nicht", "zu", "lan\u00b7ge", "weg", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PTKA", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weg ist lang, und weht der Wind \u2013", "tokens": ["Weg", "ist", "lang", ",", "und", "weht", "der", "Wind", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$,", "KON", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Achte die Fl\u00fcgel, liebes Kind,", "tokens": ["Ach\u00b7te", "die", "Fl\u00fc\u00b7gel", ",", "lie\u00b7bes", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Und die F\u00fc\u00dfchen klein", "tokens": ["Und", "die", "F\u00fc\u00df\u00b7chen", "klein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Auch vor Schelmerein:", "tokens": ["Auch", "vor", "Schel\u00b7me\u00b7rein", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Denn Kr\u00f6tent\u00fccken und Spinnen", "tokens": ["Denn", "Kr\u00f6\u00b7ten\u00b7t\u00fc\u00b7cken", "und", "Spin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Auf dein Verderben nur sinnen;", "tokens": ["Auf", "dein", "Ver\u00b7der\u00b7ben", "nur", "sin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Doch hier dich alle lieben \u2013", "tokens": ["Doch", "hier", "dich", "al\u00b7le", "lie\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "PIS", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "O w\u00e4rst du doch geblieben", "tokens": ["O", "w\u00e4rst", "du", "doch", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Zu Haus! Zu Haus! Zu Haus!", "tokens": ["Zu", "Haus", "!", "Zu", "Haus", "!", "Zu", "Haus", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "APPR", "NN", "$.", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Marienw\u00fcrmchen, komm wieder!", "tokens": ["Ma\u00b7rien\u00b7w\u00fcrm\u00b7chen", ",", "komm", "wie\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Dir rufen deine Br\u00fcder,", "tokens": ["Dir", "ru\u00b7fen", "dei\u00b7ne", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Br\u00fcderlein und Schwesterlein", "tokens": ["Br\u00fc\u00b7derl\u00b7ein", "und", "Schwes\u00b7ter\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die kleinen Kinderlein", "tokens": ["Und", "die", "klei\u00b7nen", "Kin\u00b7der\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und Gro\u00dfmutter auch", "tokens": ["Und", "Gro\u00df\u00b7mut\u00b7ter", "auch"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "ADV"], "meter": "-++-+", "measure": "unknown.measure.tri"}, "line.6": {"text": "Und Gro\u00dfvater auch \u2013", "tokens": ["Und", "Gro\u00df\u00b7va\u00b7ter", "auch", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "$("], "meter": "--+-+", "measure": "anapaest.init"}, "line.7": {"text": "Sie sitzen im schwarzen Rocke,", "tokens": ["Sie", "sit\u00b7zen", "im", "schwar\u00b7zen", "Ro\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Der K\u00fcster zieht die Glocke", "tokens": ["Der", "K\u00fcs\u00b7ter", "zieht", "die", "Glo\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und soll die Trauer l\u00e4uten \u2013", "tokens": ["Und", "soll", "die", "Trau\u00b7er", "l\u00e4u\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "O komm zu lieben Leuten,", "tokens": ["O", "komm", "zu", "lie\u00b7ben", "Leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Zu Haus! Zu Haus! Zu Haus!", "tokens": ["Zu", "Haus", "!", "Zu", "Haus", "!", "Zu", "Haus", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "APPR", "NN", "$.", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}