{"textgrid.poem.25786": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Faulbaumduft", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wei\u00df der Park, ein Korallenhain.", "tokens": ["Wei\u00df", "der", "Park", ",", "ein", "Ko\u00b7ral\u00b7len\u00b7hain", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Eisf\u00e4den schneiden den See.", "tokens": ["Eis\u00b7f\u00e4\u00b7den", "schnei\u00b7den", "den", "See", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Gr\u00fcn glei\u00dfen Pfauen im Sternenschein", "tokens": ["Gr\u00fcn", "glei\u00b7\u00dfen", "Pfau\u00b7en", "im", "Ster\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "APPRART", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Auf \u00e4therblauem Schnee.", "tokens": ["Auf", "\u00e4t\u00b7her\u00b7blau\u00b7em", "Schnee", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Scharfblanke H\u00f6hlen,", "tokens": ["Scharf\u00b7blan\u00b7ke", "H\u00f6h\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Gold\u00e4tzend in Helle.", "tokens": ["Gold\u00b7\u00e4t\u00b7zend", "in", "Hel\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Auf hy