{"textgrid.poem.33232": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "[12] Eitelkeit des Zeitlichen", "genre": "verse", "period": "N.A.", "pub_year": 1676, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es eilet unsre Zeit, als wie ein Spiel, dahin,", "tokens": ["Es", "ei\u00b7let", "uns\u00b7re", "Zeit", ",", "als", "wie", "ein", "Spiel", ",", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "KOKOM", "ART", "NN", "$,", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Stunden und der Tag, der Monat und die Jahre", "tokens": ["Die", "Stun\u00b7den", "und", "der", "Tag", ",", "der", "Mo\u00b7nat", "und", "die", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Begleiten insgesammt uns zu der Todten-Bahre;", "tokens": ["Be\u00b7glei\u00b7ten", "ins\u00b7ge\u00b7sammt", "uns", "zu", "der", "Tod\u00b7ten\u00b7Bah\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ich wei\u00df heute nicht, ob ich noch Morgen bin.", "tokens": ["Und", "ich", "wei\u00df", "heu\u00b7te", "nicht", ",", "ob", "ich", "noch", "Mor\u00b7gen", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was n\u00fctzt dir die Gestalt? Was n\u00fctzt dein hoher Sinn,", "tokens": ["Was", "n\u00fctzt", "dir", "die", "Ge\u00b7stalt", "?", "Was", "n\u00fctzt", "dein", "ho\u00b7her", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$.", "PWS", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der nicht an schlechtem Gut sich suchet zu erg\u00f6tzen?", "tokens": ["Der", "nicht", "an", "schlech\u00b7tem", "Gut", "sich", "su\u00b7chet", "zu", "er\u00b7g\u00f6t\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "ADJA", "NN", "PRF", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bestricket ihn der Tod nicht auch mit seinen Netzen?", "tokens": ["Be\u00b7stri\u00b7cket", "ihn", "der", "Tod", "nicht", "auch", "mit", "sei\u00b7nen", "Net\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein Lacken und ein Bret ist endlich der Gewinn.", "tokens": ["Ein", "La\u00b7cken", "und", "ein", "Bret", "ist", "end\u00b7lich", "der", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Spiel noch so lang und gut die Rolle hier auf Erden,", "tokens": ["Spiel", "noch", "so", "lang", "und", "gut", "die", "Rol\u00b7le", "hier", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADJD", "KON", "ADJD", "ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Schau-Platz mu\u00df einmahl doch zugezogen werden.", "tokens": ["Der", "Schau\u00b7Platz", "mu\u00df", "ein\u00b7mahl", "doch", "zu\u00b7ge\u00b7zo\u00b7gen", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}