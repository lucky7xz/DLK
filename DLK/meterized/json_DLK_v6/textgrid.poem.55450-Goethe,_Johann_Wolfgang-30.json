{"textgrid.poem.55450": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "30", "genre": "verse", "period": "N.A.", "pub_year": 1823, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Testament verteilt die holden Gaben,", "tokens": ["Dein", "Tes\u00b7ta\u00b7ment", "ver\u00b7teilt", "die", "hol\u00b7den", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Womit Natur dich m\u00fctterlich vollendet,", "tokens": ["Wo\u00b7mit", "Na\u00b7tur", "dich", "m\u00fct\u00b7ter\u00b7lich", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Verm\u00e4chtnis nach Verm\u00e4chtnis ausgespendet,", "tokens": ["Ver\u00b7m\u00e4cht\u00b7nis", "nach", "Ver\u00b7m\u00e4cht\u00b7nis", "aus\u00b7ge\u00b7spen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zufrieden jeder, seinen Teil zu haben.", "tokens": ["Zu\u00b7frie\u00b7den", "je\u00b7der", ",", "sei\u00b7nen", "Teil", "zu", "ha\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "$,", "PPOSAT", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch wenn du Gl\u00fcckliche zu machen trachtest,", "tokens": ["Doch", "wenn", "du", "Gl\u00fcck\u00b7li\u00b7che", "zu", "ma\u00b7chen", "trach\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So w\u00e4r es der, dem du dich ganz vermachtest.", "tokens": ["So", "w\u00e4r", "es", "der", ",", "dem", "du", "dich", "ganz", "ver\u00b7mach\u00b7test", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "$,", "PRELS", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}