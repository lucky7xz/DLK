{"dta.poem.14011": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Lerche .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ich kann hier nicht singen,", "tokens": ["Ich", "kann", "hier", "nicht", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Aus dieser Mauern dunklen Ringen", "tokens": ["Aus", "die\u00b7ser", "Mau\u00b7ern", "dunk\u00b7len", "Rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mu\u00df ich mich schwingen", "tokens": ["Mu\u00df", "ich", "mich", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Vor Lust und tiefem Weh.", "tokens": ["Vor", "Lust", "und", "tie\u00b7fem", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O Freude, in klarer H\u00f6h", "tokens": ["O", "Freu\u00b7de", ",", "in", "kla\u00b7rer", "H\u00f6h"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Zu sinken und sich zu heben,", "tokens": ["Zu", "sin\u00b7ken", "und", "sich", "zu", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "In Gesang", "tokens": ["In", "Ge\u00b7sang"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Ueber die gr\u00fcne Erde dahin zu schweben,", "tokens": ["Ue\u00b7ber", "die", "gr\u00fc\u00b7ne", "Er\u00b7de", "da\u00b7hin", "zu", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PAV", "PTKZU", "VVINF", "$,"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.9": {"text": "Wie unten die licht' und dunkeln Streifen", "tokens": ["Wie", "un\u00b7ten", "die", "licht'", "und", "dun\u00b7keln", "Strei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "KON", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Wechselnd im Fluge vor\u00fcberschweifen,", "tokens": ["Wech\u00b7selnd", "im", "Flu\u00b7ge", "vor\u00b7\u00fc\u00b7bersc\u00b7hwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.11": {"text": "Aus der Tiefe ein Wirren und Rauschen und H\u00e4mmern,", "tokens": ["Aus", "der", "Tie\u00b7fe", "ein", "Wir\u00b7ren", "und", "Rau\u00b7schen", "und", "H\u00e4m\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.12": {"text": "Die Erde aufschimmernd im Fr\u00fchlingsd\u00e4mmern,", "tokens": ["Die", "Er\u00b7de", "auf\u00b7schim\u00b7mernd", "im", "Fr\u00fch\u00b7lings\u00b7d\u00e4m\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Wie ist die Welt so voller Klang!", "tokens": ["Wie", "ist", "die", "Welt", "so", "vol\u00b7ler", "Klang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Herz, was bist Du bang?", "tokens": ["Herz", ",", "was", "bist", "Du", "bang", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.15": {"text": "Mu\u00dft aufw\u00e4rts dringen!", "tokens": ["Mu\u00dft", "auf\u00b7w\u00e4rts", "drin\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Die Sonne tritt hervor,", "tokens": ["Die", "Son\u00b7ne", "tritt", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Wie gl\u00e4nzen mir Brust und Schwingen,", "tokens": ["Wie", "gl\u00e4n\u00b7zen", "mir", "Brust", "und", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "Wie still und weit ist's droben am Himmelsthor.", "tokens": ["Wie", "still", "und", "weit", "ist's", "dro\u00b7ben", "am", "Him\u00b7mel\u00b7sthor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "VAFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}