{"textgrid.poem.42588": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "12. Gebet eines W\u00fcrttembergers", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der du von deinem ew'gen Thron", "tokens": ["Der", "du", "von", "dei\u00b7nem", "ew'\u00b7gen", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die V\u00f6lker h\u00fctest, gro\u00df' und kleine:", "tokens": ["Die", "V\u00f6l\u00b7ker", "h\u00fc\u00b7test", ",", "gro\u00df'", "und", "klei\u00b7ne", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJD", "KON", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gewi\u00df, du blickst auch auf das meine,", "tokens": ["Ge\u00b7wi\u00df", ",", "du", "blickst", "auch", "auf", "das", "mei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ADV", "APPR", "PRELS", "PPOSAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du siehst das Leiden, siehst den Hohn.", "tokens": ["Du", "siehst", "das", "Lei\u00b7den", ",", "siehst", "den", "Hohn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zu unsrem K\u00f6nig, deinem Knecht,", "tokens": ["Zu", "uns\u00b7rem", "K\u00f6\u00b7nig", ",", "dei\u00b7nem", "Knecht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kann nicht des Volkes Stimme kommen;", "tokens": ["Kann", "nicht", "des", "Vol\u00b7kes", "Stim\u00b7me", "kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00e4tt er sie, wie er will, vernommen,", "tokens": ["H\u00e4tt", "er", "sie", ",", "wie", "er", "will", ",", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir h\u00e4tten l\u00e4ngst das teure Recht.", "tokens": ["Wir", "h\u00e4t\u00b7ten", "l\u00e4ngst", "das", "teu\u00b7re", "Recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch dir ist offen jeglich Tor,", "tokens": ["Doch", "dir", "ist", "of\u00b7fen", "jeg\u00b7lich", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir keine Scheidwand vorgeschoben,", "tokens": ["Dir", "kei\u00b7ne", "Scheid\u00b7wand", "vor\u00b7ge\u00b7scho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Wort ist Donnerhall von oben:", "tokens": ["Dein", "Wort", "ist", "Don\u00b7ner\u00b7hall", "von", "o\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sprich du an unsres K\u00f6nigs Ohr!", "tokens": ["Sprich", "du", "an", "uns\u00b7res", "K\u00f6\u00b7nigs", "Ohr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}