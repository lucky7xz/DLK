{"textgrid.poem.38924": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja, es giebt ein sch\u00f6nes Sehnen,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, es giebt ein sch\u00f6nes Sehnen,", "tokens": ["Ja", ",", "es", "giebt", "ein", "sch\u00f6\u00b7nes", "Seh\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das wie aus der tiefsten Nacht", "tokens": ["Das", "wie", "aus", "der", "tiefs\u00b7ten", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "KOKOM", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In dem Herzen aufgewacht,", "tokens": ["In", "dem", "Her\u00b7zen", "auf\u00b7ge\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Greift nach Waffen, findet Thr\u00e4nen;", "tokens": ["Greift", "nach", "Waf\u00b7fen", ",", "fin\u00b7det", "Thr\u00e4\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Viele lieben, viele w\u00e4hnen,", "tokens": ["Vie\u00b7le", "lie\u00b7ben", ",", "vie\u00b7le", "w\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVINF", "$,", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df Liebe nur Lust dem Herzen", "tokens": ["Da\u00df", "Lie\u00b7be", "nur", "Lust", "dem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "NN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Schenken soll und keine Schmerzen:", "tokens": ["Schen\u00b7ken", "soll", "und", "kei\u00b7ne", "Schmer\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "KON", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Alle Farben m\u00fcssen flie\u00dfen,", "tokens": ["Al\u00b7le", "Far\u00b7ben", "m\u00fcs\u00b7sen", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wenn ein Licht sich soll ergie\u00dfen", "tokens": ["Wenn", "ein", "Licht", "sich", "soll", "er\u00b7gie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PRF", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Aus dem goldnen Brand der Kerzen.", "tokens": ["Aus", "dem", "gold\u00b7nen", "Brand", "der", "Ker\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}