{"dta.poem.8534": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "S\u00fc\u00df .", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf den Gassen der G&#228;rtner rief:", "tokens": ["Auf", "den", "Gas\u00b7sen", "der", "G", "&#228;", "rt\u00b7ner", "rief", ":"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NE", "$(", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Kauft Trauben, kaufet die Trauben!", "tokens": ["Kauft", "Trau\u00b7ben", ",", "kau\u00b7fet", "die", "Trau\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Aber im Herzen die Furcht ihm wohnt,", "tokens": ["A\u00b7ber", "im", "Her\u00b7zen", "die", "Furcht", "ihm", "wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Es m\u00f6chte sie Keiner begehren;", "tokens": ["Es", "m\u00f6ch\u00b7te", "sie", "Kei\u00b7ner", "be\u00b7geh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Sauer waren und trocken sie,", "tokens": ["Sau\u00b7er", "wa\u00b7ren", "und", "tro\u00b7cken", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KON", "ADJD", "PPER", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Sie hatte Mehlthau get\u00f6dtet;", "tokens": ["Sie", "hat\u00b7te", "Mehlt\u00b7hau", "ge\u00b7t\u00f6d\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "VVPP", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Naht ihm Hassan: \u201emein G\u00e4rtner, sprich,", "tokens": ["Naht", "ihm", "Has\u00b7san", ":", "\u201e", "mein", "G\u00e4rt\u00b7ner", ",", "sprich", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "NE", "$.", "$(", "PPOSAT", "NN", "$,", "ADJD", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.8": {"text": "Was willst du f\u00fcr deine Trauben?\u201c", "tokens": ["Was", "willst", "du", "f\u00fcr", "dei\u00b7ne", "Trau\u00b7ben", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "\u201enimm, o Herr, und koste sie,", "tokens": ["\u201e", "nimm", ",", "o", "Herr", ",", "und", "kos\u00b7te", "sie", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$,", "FM", "NN", "$,", "KON", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Und habe meiner Erbarmen!\u201c", "tokens": ["Und", "ha\u00b7be", "mei\u00b7ner", "Er\u00b7bar\u00b7men", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "\u201eo wie k\u00f6stlich, mein G\u00e4rtner, nimm", "tokens": ["\u201e", "o", "wie", "k\u00f6st\u00b7lich", ",", "mein", "G\u00e4rt\u00b7ner", ",", "nimm"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["$(", "FM", "KOKOM", "ADJD", "$,", "PPOSAT", "NN", "$,", "VVIMP"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.12": {"text": "Und m\u00f6ge Allah dich segnen!\u201c", "tokens": ["Und", "m\u00f6\u00b7ge", "Al\u00b7lah", "dich", "seg\u00b7nen", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VMFIN", "NN", "PPER", "VVINF", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Abend naht und der andre Tag:", "tokens": ["A\u00b7bend", "naht", "und", "der", "and\u00b7re", "Tag", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.14": {"text": "\u201eweh mir, wie bin ich betrogen!", "tokens": ["\u201e", "weh", "mir", ",", "wie", "bin", "ich", "be\u00b7tro\u00b7gen", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPER", "$,", "PWAV", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Hat mir gestern Zuleima\u2019s Ku\u00df", "tokens": ["Hat", "mir", "ge\u00b7stern", "Zu\u00b7lei\u00b7ma's", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Denn also vers\u00fc\u00dfet die Lippen?\u201c", "tokens": ["Denn", "al\u00b7so", "ver\u00b7s\u00fc\u00b7\u00dfet", "die", "Lip\u00b7pen", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}