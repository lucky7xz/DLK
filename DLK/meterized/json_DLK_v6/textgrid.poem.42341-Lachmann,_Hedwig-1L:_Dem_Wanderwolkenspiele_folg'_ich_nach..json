{"textgrid.poem.42341": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dem Wanderwolkenspiele folg' ich nach.", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem Wanderwolkenspiele folg' ich nach.", "tokens": ["Dem", "Wan\u00b7der\u00b7wol\u00b7ken\u00b7spie\u00b7le", "fol\u00b7g'", "ich", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Ein Sonnenstreifen dr\u00fcben an der Mauer", "tokens": ["Ein", "Son\u00b7nen\u00b7strei\u00b7fen", "dr\u00fc\u00b7ben", "an", "der", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verlischt und leuchtet auf zu kurzer Dauer", "tokens": ["Ver\u00b7lischt", "und", "leuch\u00b7tet", "auf", "zu", "kur\u00b7zer", "Dau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "APPR", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und schnelle Schatten fliegen \u00fcbers Dach.", "tokens": ["Und", "schnel\u00b7le", "Schat\u00b7ten", "flie\u00b7gen", "\u00fc\u00b7bers", "Dach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie h\u00e4ngt mein Blick an all der dunklen Hast!", "tokens": ["Wie", "h\u00e4ngt", "mein", "Blick", "an", "all", "der", "dunk\u00b7len", "Hast", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In der Sekunde tausendfach geboren", "tokens": ["In", "der", "Se\u00b7kun\u00b7de", "tau\u00b7send\u00b7fach", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wieder tausendfach zur\u00fcck verloren", "tokens": ["Und", "wie\u00b7der", "tau\u00b7send\u00b7fach", "zu\u00b7r\u00fcck", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PTKVZ", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und nie und nirgends diesem Wirbel Rast.", "tokens": ["Und", "nie", "und", "nir\u00b7gends", "die\u00b7sem", "Wir\u00b7bel", "Rast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "ADV", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O w\u00fcchse mir inmitten aller Flucht", "tokens": ["O", "w\u00fcch\u00b7se", "mir", "in\u00b7mit\u00b7ten", "al\u00b7ler", "Flucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und flatterndem verfr\u00fchtem Bl\u00fctenregen", "tokens": ["Und", "flat\u00b7tern\u00b7dem", "ver\u00b7fr\u00fch\u00b7tem", "Bl\u00fc\u00b7ten\u00b7re\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "- F\u00fcr mein Geschick ein noch verschlossner Segen \u2013", "tokens": ["F\u00fcr", "mein", "Ge\u00b7schick", "ein", "noch", "ver\u00b7schloss\u00b7ner", "Se\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "ART", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In zarter Knospenh\u00fclle eine Frucht.", "tokens": ["In", "zar\u00b7ter", "Knos\u00b7pen\u00b7h\u00fcl\u00b7le", "ei\u00b7ne", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}