{"textgrid.poem.43915": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie h\u00f6r ich das von dir, betr\u00fcbte Magdalis,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie h\u00f6r ich das von dir, betr\u00fcbte Magdalis,", "tokens": ["Wie", "h\u00f6r", "ich", "das", "von", "dir", ",", "be\u00b7tr\u00fcb\u00b7te", "Mag\u00b7da\u00b7lis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "APPR", "PPER", "$,", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df deine Sch\u00f6nheit weint und sich dadurch verzehret?", "tokens": ["Da\u00df", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", "weint", "und", "sich", "da\u00b7durch", "ver\u00b7zeh\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "KON", "PRF", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O trauervolle Post, o allzu harter Ri\u00df,", "tokens": ["O", "trau\u00b7er\u00b7vol\u00b7le", "Post", ",", "o", "all\u00b7zu", "har\u00b7ter", "Ri\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "PTKA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der mich in kurzer Zeit dem Tode selbst gew\u00e4hret!", "tokens": ["Der", "mich", "in", "kur\u00b7zer", "Zeit", "dem", "To\u00b7de", "selbst", "ge\u00b7w\u00e4h\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mein Kind, bedencke mich; was beugstu mir das Herz,", "tokens": ["Mein", "Kind", ",", "be\u00b7den\u00b7cke", "mich", ";", "was", "beugs\u00b7tu", "mir", "das", "Herz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "$.", "PWS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil ich erfahren mu\u00df, da\u00df mir dein Ohr nicht glaube?", "tokens": ["Weil", "ich", "er\u00b7fah\u00b7ren", "mu\u00df", ",", "da\u00df", "mir", "dein", "Ohr", "nicht", "glau\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Warum vergr\u00f6\u00dferstu den ungemeinen Schmerz?", "tokens": ["Wa\u00b7rum", "ver\u00b7gr\u00f6\u00b7\u00dfers\u00b7tu", "den", "un\u00b7ge\u00b7mei\u00b7nen", "Schmerz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein Zweifel zwingt mich fast, da\u00df ich mein Leben raube.", "tokens": ["Dein", "Zwei\u00b7fel", "zwingt", "mich", "fast", ",", "da\u00df", "ich", "mein", "Le\u00b7ben", "rau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn lebt ich nicht vor dich, so sucht ich meinen Tod", "tokens": ["Denn", "lebt", "ich", "nicht", "vor", "dich", ",", "so", "sucht", "ich", "mei\u00b7nen", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPR", "PPER", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch Feuer, Me\u00dfer, Strick, Stahl, Brunnen, Gift und Degen,", "tokens": ["Durch", "Feu\u00b7er", ",", "Me\u00b7\u00dfer", ",", "Strick", ",", "Stahl", ",", "Brun\u00b7nen", ",", "Gift", "und", "De\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So k\u00f6nt ich meinen Leib, den die entstandne Noth", "tokens": ["So", "k\u00f6nt", "ich", "mei\u00b7nen", "Leib", ",", "den", "die", "ent\u00b7stand\u00b7ne", "Noth"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von allen Seiten rei\u00dft, auf meine Baare legen.", "tokens": ["Von", "al\u00b7len", "Sei\u00b7ten", "rei\u00dft", ",", "auf", "mei\u00b7ne", "Baa\u00b7re", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So aber, ob mich gleich des Gl\u00fcckes Misgunst hast", "tokens": ["So", "a\u00b7ber", ",", "ob", "mich", "gleich", "des", "Gl\u00fc\u00b7ckes", "Mis\u00b7gunst", "hast"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und alle Wetter sich auf meinen Kopf verschw\u00f6ren,", "tokens": ["Und", "al\u00b7le", "Wet\u00b7ter", "sich", "auf", "mei\u00b7nen", "Kopf", "ver\u00b7schw\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ertrag ich mit Gedult die B\u00fcrde schwerer Last,", "tokens": ["Er\u00b7trag", "ich", "mit", "Ge\u00b7dult", "die", "B\u00fcr\u00b7de", "schwe\u00b7rer", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dir einzig und allein Vergn\u00fcgung zu geb\u00e4hren.", "tokens": ["Dir", "ein\u00b7zig", "und", "al\u00b7lein", "Ver\u00b7gn\u00fc\u00b7gung", "zu", "ge\u00b7b\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADV", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Hingegen deine Brust traut mir die Falschheit zu,", "tokens": ["Hin\u00b7ge\u00b7gen", "dei\u00b7ne", "Brust", "traut", "mir", "die", "Falschheit", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+--++-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Dein Vorwurf sezet mir ein Me\u00dfer an die Kehle;", "tokens": ["Dein", "Vor\u00b7wurf", "se\u00b7zet", "mir", "ein", "Me\u00b7\u00dfer", "an", "die", "Keh\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Glaubstu, da\u00df G\u00fcnther das, was deine Freundschaft, thu,", "tokens": ["Glaubs\u00b7tu", ",", "da\u00df", "G\u00fcn\u00b7ther", "das", ",", "was", "dei\u00b7ne", "Freund\u00b7schaft", ",", "thu", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "KOUS", "NE", "PDS", "$,", "PRELS", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von der ich jeden Tag ein neues Ungl\u00fcck zehle?", "tokens": ["Von", "der", "ich", "je\u00b7den", "Tag", "ein", "neu\u00b7es", "Un\u00b7gl\u00fcck", "zeh\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PIAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Mein Kind, er\u00f6fne mir: wer hat dein Haupt verr\u00fcckt?", "tokens": ["Mein", "Kind", ",", "er\u00b7\u00f6f\u00b7ne", "mir", ":", "wer", "hat", "dein", "Haupt", "ver\u00b7r\u00fcckt", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "$.", "PWS", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat wohl ein Maul voll Gift das Feuer ausgego\u00dfen,", "tokens": ["Hat", "wohl", "ein", "Maul", "voll", "Gift", "das", "Feu\u00b7er", "aus\u00b7ge\u00b7go\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "NN", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das meine Redligkeit in deine Brust geschickt?", "tokens": ["Das", "mei\u00b7ne", "Red\u00b7lig\u00b7keit", "in", "dei\u00b7ne", "Brust", "ge\u00b7schickt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist deiner Schwester Brief ein angestellter Po\u00dfen?", "tokens": ["Ist", "dei\u00b7ner", "Schwes\u00b7ter", "Brief", "ein", "an\u00b7ge\u00b7stell\u00b7ter", "Po\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die Liebe, welche du vergangnes Jahr erkand,", "tokens": ["Die", "Lie\u00b7be", ",", "wel\u00b7che", "du", "ver\u00b7gang\u00b7nes", "Jahr", "er\u00b7kand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erduldet nimmermehr, da\u00df sie dein Herz verdencke,", "tokens": ["E\u00b7rdul\u00b7det", "nim\u00b7mer\u00b7mehr", ",", "da\u00df", "sie", "dein", "Herz", "ver\u00b7den\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und f\u00fchlestu nicht mehr das fest verkn\u00fcpfte Band,", "tokens": ["Und", "f\u00fch\u00b7le\u00b7stu", "nicht", "mehr", "das", "fest", "ver\u00b7kn\u00fcpf\u00b7te", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In dem ich mich je mehr und mehr verschr\u00e4ncke?", "tokens": ["In", "dem", "ich", "mich", "je", "mehr", "und", "mehr", "ver\u00b7schr\u00e4n\u00b7cke", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "ADV", "ADV", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Wo dir ein Tropfen Blut noch in den Adern springt,", "tokens": ["Wo", "dir", "ein", "Trop\u00b7fen", "Blut", "noch", "in", "den", "A\u00b7dern", "springt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der seine Glut beh\u00e4lt und mich sein eigen nennet,", "tokens": ["Der", "sei\u00b7ne", "Glut", "be\u00b7h\u00e4lt", "und", "mich", "sein", "ei\u00b7gen", "nen\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "KON", "PPER", "PPOSAT", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So rei\u00df die Furcht entzwey, die deine Gro\u00dfmuth zwingt,", "tokens": ["So", "rei\u00df", "die", "Furcht", "ent\u00b7zwey", ",", "die", "dei\u00b7ne", "Gro\u00df\u00b7muth", "zwingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wi\u00dfe, da\u00df mein Herz noch in der Asche brennet.", "tokens": ["Und", "wi\u00b7\u00dfe", ",", "da\u00df", "mein", "Herz", "noch", "in", "der", "A\u00b7sche", "bren\u00b7net", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Warum soll dich der Gram mir vor der Zeit entziehn?", "tokens": ["Wa\u00b7rum", "soll", "dich", "der", "Gram", "mir", "vor", "der", "Zeit", "ent\u00b7ziehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PRF", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Womit hab ich verdient, dich bla\u00df und todt zu schauen?", "tokens": ["Wo\u00b7mit", "hab", "ich", "ver\u00b7dient", ",", "dich", "bla\u00df", "und", "todt", "zu", "schau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVPP", "$,", "PRF", "ADJD", "KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mir stand der Hofnungsbaum in allem Wetter gr\u00fcn,", "tokens": ["Mir", "stand", "der", "Hof\u00b7nungs\u00b7baum", "in", "al\u00b7lem", "Wet\u00b7ter", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PIS", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jezt will ihm deine Qual so Stamm als Ast verhauen.", "tokens": ["Jezt", "will", "ihm", "dei\u00b7ne", "Qual", "so", "Stamm", "als", "Ast", "ver\u00b7hau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "ADV", "NN", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "War ich ein Wetterhahn, der ihm die Freude macht,", "tokens": ["War", "ich", "ein", "Wet\u00b7ter\u00b7hahn", ",", "der", "ihm", "die", "Freu\u00b7de", "macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn er ein Frauenbild durch seine List betrogen,", "tokens": ["Wenn", "er", "ein", "Frau\u00b7en\u00b7bild", "durch", "sei\u00b7ne", "List", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So w\u00e4r ich werth, da\u00df mich die Mutter umgebracht,", "tokens": ["So", "w\u00e4r", "ich", "werth", ",", "da\u00df", "mich", "die", "Mut\u00b7ter", "um\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Eh ich die erste Milch aus ihrer Brust gesogen.", "tokens": ["Eh", "ich", "die", "ers\u00b7te", "Milch", "aus", "ih\u00b7rer", "Brust", "ge\u00b7so\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Wo nur des H\u00f6chsten Gunst mir diesen Wuntsch verleiht,", "tokens": ["Wo", "nur", "des", "H\u00f6chs\u00b7ten", "Gunst", "mir", "die\u00b7sen", "Wunt\u00b7sch", "ver\u00b7leiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "So soll der Donner eh mich in die Tiefe st\u00fcrzen,", "tokens": ["So", "soll", "der", "Don\u00b7ner", "eh", "mich", "in", "die", "Tie\u00b7fe", "st\u00fcr\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "KOUS", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als mein vergebner Schwur durch Unbest\u00e4ndigkeit", "tokens": ["Als", "mein", "ver\u00b7geb\u00b7ner", "Schwur", "durch", "Un\u00b7be\u00b7st\u00e4n\u00b7dig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dir, angenehmes Kind, die Lebenslust verk\u00fcrzen.", "tokens": ["Dir", ",", "an\u00b7ge\u00b7neh\u00b7mes", "Kind", ",", "die", "Le\u00b7bens\u00b7lust", "ver\u00b7k\u00fcr\u00b7zen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Wie manche sch\u00f6ne Nacht sieht mich der bla\u00dfe Mond", "tokens": ["Wie", "man\u00b7che", "sch\u00f6\u00b7ne", "Nacht", "sieht", "mich", "der", "bla\u00b7\u00dfe", "Mond"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In stiller Einsamkeit am Kummerfaden spinnen!", "tokens": ["In", "stil\u00b7ler", "Ein\u00b7sam\u00b7keit", "am", "Kum\u00b7mer\u00b7fa\u00b7den", "spin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich fre\u00dfe mir das Herz; die Angst, so mich bewohnt,", "tokens": ["Ich", "fre\u00b7\u00dfe", "mir", "das", "Herz", ";", "die", "Angst", ",", "so", "mich", "be\u00b7wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$.", "ART", "NN", "$,", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "L\u00e4st keinen Augenblick mich Lust und Luft gewinnen.", "tokens": ["L\u00e4st", "kei\u00b7nen", "Au\u00b7gen\u00b7blick", "mich", "Lust", "und", "Luft", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Das macht, weil Wittenberg mir so zuwider scheint,", "tokens": ["Das", "macht", ",", "weil", "Wit\u00b7ten\u00b7berg", "mir", "so", "zu\u00b7wi\u00b7der", "scheint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "NE", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df mir kein Freudenstern darinnen aufgegangen;", "tokens": ["Da\u00df", "mir", "kein", "Freu\u00b7dens\u00b7tern", "da\u00b7rin\u00b7nen", "auf\u00b7ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gott kennet meine Noth; ich habe keinen Freund,", "tokens": ["Gott", "ken\u00b7net", "mei\u00b7ne", "Noth", ";", "ich", "ha\u00b7be", "kei\u00b7nen", "Freund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als den, der \u00fcber mich dergleichen Qual verhangen.", "tokens": ["Als", "den", ",", "der", "\u00fc\u00b7ber", "mich", "derg\u00b7lei\u00b7chen", "Qual", "ver\u00b7han\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "PRELS", "APPR", "PPER", "PIS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Jedoch die Zuversicht, so mein Gem\u00fcthe stillt,", "tokens": ["Je\u00b7doch", "die", "Zu\u00b7ver\u00b7sicht", ",", "so", "mein", "Ge\u00b7m\u00fc\u00b7the", "stillt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sagt mir, es sey noch nicht der Abend aller Tage;", "tokens": ["Sagt", "mir", ",", "es", "sey", "noch", "nicht", "der", "A\u00b7bend", "al\u00b7ler", "Ta\u00b7ge", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VAFIN", "ADV", "PTKNEG", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil nun aus Alo\u00eb ein Schmerzensmittel quillt,", "tokens": ["Weil", "nun", "aus", "A\u00b7lo\u00eb", "ein", "Schmer\u00b7zens\u00b7mit\u00b7tel", "quillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "NE", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "So hab ich einen Trost in meiner steten Plage.", "tokens": ["So", "hab", "ich", "ei\u00b7nen", "Trost", "in", "mei\u00b7ner", "ste\u00b7ten", "Pla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Mein Engel, meine Lust, mein Leben und mein Licht,", "tokens": ["Mein", "En\u00b7gel", ",", "mei\u00b7ne", "Lust", ",", "mein", "Le\u00b7ben", "und", "mein", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor die ich tausenmahl mit Freuden sterben wollte,", "tokens": ["Vor", "die", "ich", "tau\u00b7sen\u00b7mahl", "mit", "Freu\u00b7den", "ster\u00b7ben", "woll\u00b7te", ","], "token_info