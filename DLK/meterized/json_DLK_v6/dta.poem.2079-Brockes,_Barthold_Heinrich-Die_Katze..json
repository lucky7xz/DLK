{"dta.poem.2079": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Katze.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auch die\u00df ist ein besonder Thier, wof\u00fcr dem Sch\u00f6pfer", "tokens": ["Auch", "die\u00df", "ist", "ein", "be\u00b7son\u00b7der", "Thier", ",", "wo\u00b7f\u00fcr", "dem", "Sch\u00f6p\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dank geb\u00fchret,", "tokens": ["Dank", "ge\u00b7b\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Wovon sowohl im Haus als Felde man wahr- und gro\u00dfen", "tokens": ["Wo\u00b7von", "so\u00b7wohl", "im", "Haus", "als", "Fel\u00b7de", "man", "wahr", "und", "gro\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPRART", "NN", "KOUS", "NN", "PIS", "TRUNC", "KON", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Nutzen sp\u00fcret,", "tokens": ["Nut\u00b7zen", "sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Da es den M\u00e4usen und den Ratzen, und anderm Ungeziefer", "tokens": ["Da", "es", "den", "M\u00e4u\u00b7sen", "und", "den", "Rat\u00b7zen", ",", "und", "an\u00b7derm", "Un\u00b7ge\u00b7zie\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "KON", "ART", "NN", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "feind,", "tokens": ["feind", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Die uns dadurch, da\u00df sie so sehr und monatlich fast sich", "tokens": ["Die", "uns", "da\u00b7durch", ",", "da\u00df", "sie", "so", "sehr", "und", "mo\u00b7nat\u00b7lich", "fast", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PAV", "$,", "KOUS", "PPER", "ADV", "ADV", "KON", "ADJD", "VVFIN", "PRF"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "vermehren,", "tokens": ["ver\u00b7meh\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Auf eine Weise, die nicht leidlich, mehr als man fast ge-", "tokens": ["Auf", "ei\u00b7ne", "Wei\u00b7se", ",", "die", "nicht", "leid\u00b7lich", ",", "mehr", "als", "man", "fast", "ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PTKNEG", "ADJD", "$,", "ADV", "KOUS", "PIS", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "denkt, beschweren,", "tokens": ["denkt", ",", "be\u00b7schwe\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Uns schaden und uns plagen w\u00fcrden. Ja gegen diesen", "tokens": ["Uns", "scha\u00b7den", "und", "uns", "pla\u00b7gen", "w\u00fcr\u00b7den", ".", "Ja", "ge\u00b7gen", "die\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVINF", "KON", "PPER", "VVFIN", "VAFIN", "$.", "NN", "APPR", "PDAT"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Zufall scheint", "tokens": ["Zu\u00b7fall", "scheint"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Die Katze recht mit Flei\u00df geschaffen. Derselben Unver-", "tokens": ["Die", "Kat\u00b7ze", "recht", "mit", "Flei\u00df", "ge\u00b7schaf\u00b7fen", ".", "Der\u00b7sel\u00b7ben", "Un\u00b7ver"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "VVPP", "$.", "PDAT", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "drossenheit,", "tokens": ["dros\u00b7sen\u00b7heit", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Gelenker K\u00f6rper, schnelle Glieder, Geduld, beherzte", "tokens": ["Ge\u00b7len\u00b7ker", "K\u00f6r\u00b7per", ",", "schnel\u00b7le", "Glie\u00b7der", ",", "Ge\u00b7duld", ",", "be\u00b7herz\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "NN", "$,", "ADJA", "NN", "$,", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Schlauigkeit,", "tokens": ["Schlau\u00b7ig\u00b7keit", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Ein scharf Geh\u00f6r, nebst dem Geruch, und, \u00fcber alles,", "tokens": ["Ein", "scharf", "Ge\u00b7h\u00f6r", ",", "nebst", "dem", "Ge\u00b7ruch", ",", "und", ",", "\u00fc\u00b7ber", "al\u00b7les", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "APPR", "ART", "NN", "$,", "KON", "$,", "APPR", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "solche Augen,", "tokens": ["sol\u00b7che", "Au\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "Die in der dunklen Nacht zu sehn, nicht minder als am", "tokens": ["Die", "in", "der", "dunk\u00b7len", "Nacht", "zu", "sehn", ",", "nicht", "min\u00b7der", "als", "am"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,", "PTKNEG", "ADV", "KOKOM", "APPRART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Tage, taugen,", "tokens": ["Ta\u00b7ge", ",", "tau\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.21": {"text": "Da\u00df ein verborgner Zug in ihnen, wie wild sie gleich,", "tokens": ["Da\u00df", "ein", "ver\u00b7borg\u00b7ner", "Zug", "in", "ih\u00b7nen", ",", "wie", "wild", "sie", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPER", "$,", "PWAV", "ADJD", "PPER", "ADV", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "sie z\u00e4hmen kann:", "tokens": ["sie", "z\u00e4h\u00b7men", "kann", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.23": {"text": "Die\u00df alles zeiget Absicht, Weisheit und Lieb unwider-", "tokens": ["Die\u00df", "al\u00b7les", "zei\u00b7get", "Ab\u00b7sicht", ",", "Weis\u00b7heit", "und", "Lieb", "un\u00b7wi\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "VVFIN", "NN", "$,", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "sprechlich an", "tokens": ["sprech\u00b7lich", "an"], "token_info": ["word", "word"], "pos": ["ADJD", "PTKVZ"], "meter": "+-+", "measure": "trochaic.di"}, "line.25": {"text": "Von einem allgemeinen Sch\u00f6pfer. Man kann auch an", "tokens": ["Von", "ei\u00b7nem", "all\u00b7ge\u00b7mei\u00b7nen", "Sch\u00f6p\u00b7fer", ".", "Man", "kann", "auch", "an"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "PIS", "VMFIN", "ADV", "APPR"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "den Katzen sehn,", "tokens": ["den", "Kat\u00b7zen", "sehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.27": {"text": "Wie alle Dinge, die geschehen, nach Maa\u00df und Ord-", "tokens": ["Wie", "al\u00b7le", "Din\u00b7ge", ",", "die", "ge\u00b7sche\u00b7hen", ",", "nach", "Maa\u00df", "und", "Ord"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "$,", "PRELS", "VVPP", "$,", "APPR", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "nungen geschehn.", "tokens": ["nun\u00b7gen", "ge\u00b7schehn", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}