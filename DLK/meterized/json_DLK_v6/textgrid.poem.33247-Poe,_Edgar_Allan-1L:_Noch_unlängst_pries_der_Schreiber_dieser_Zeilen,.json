{"textgrid.poem.33247": {"metadata": {"author": {"name": "Poe, Edgar Allan", "birth": "N.A.", "death": "N.A."}, "title": "1L: Noch unl\u00e4ngst pries der Schreiber dieser Zeilen,", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch unl\u00e4ngst pries der Schreiber dieser Zeilen,", "tokens": ["Noch", "un\u00b7l\u00e4ngst", "pries", "der", "Schrei\u00b7ber", "die\u00b7ser", "Zei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich br\u00fcstend mit besonderem Verstand,", "tokens": ["Sich", "br\u00fcs\u00b7tend", "mit", "be\u00b7son\u00b7de\u00b7rem", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbdie Sch\u00f6pferkraft der Worte\u00ab und bestritt,", "tokens": ["\u00bb", "die", "Sch\u00f6p\u00b7fer\u00b7kraft", "der", "Wor\u00b7te", "\u00ab", "und", "be\u00b7stritt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ART", "NN", "$(", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df je Gedanken jenseits des Gebiets", "tokens": ["Da\u00df", "je", "Ge\u00b7dan\u00b7ken", "jen\u00b7seits", "des", "Ge\u00b7biets"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Menschenzunge Menschenhirn entspr\u00e4ngen;", "tokens": ["Der", "Men\u00b7schen\u00b7zun\u00b7ge", "Men\u00b7schen\u00b7hirn", "ent\u00b7spr\u00e4n\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und jetzt gesteht er, seinen Stolz verh\u00f6hnend:", "tokens": ["Und", "jetzt", "ge\u00b7steht", "er", ",", "sei\u00b7nen", "Stolz", "ver\u00b7h\u00f6h\u00b7nend", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Zwei Worte sind, zwei seltsam fremde Silben,", "tokens": ["Zwei", "Wor\u00b7te", "sind", ",", "zwei", "selt\u00b7sam", "frem\u00b7de", "Sil\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "$,", "CARD", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Italiens T\u00f6ne, die von Engeln nur", "tokens": ["I\u00b7ta\u00b7li\u00b7ens", "T\u00f6\u00b7ne", ",", "die", "von", "En\u00b7geln", "nur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "APPR", "NN", "ADV"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "In Mondlichttraum sich fl\u00fcstern lassen, \u00bbTau,", "tokens": ["In", "Mond\u00b7licht\u00b7traum", "sich", "fl\u00fcs\u00b7tern", "las\u00b7sen", ",", "\u00bb", "Tau", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["APPR", "NE", "PRF", "VVINF", "VVINF", "$,", "$(", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der perlengleich auf Hermons H\u00fcgel h\u00e4ngt\u00ab,", "tokens": ["Der", "per\u00b7len\u00b7gleich", "auf", "Her\u00b7mons", "H\u00fc\u00b7gel", "h\u00e4ngt", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "APPR", "NE", "NN", "VVFIN", "$(", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Aus seines Herzens tiefstem Grund bewegte", "tokens": ["Aus", "sei\u00b7nes", "Her\u00b7zens", "tiefs\u00b7tem", "Grund", "be\u00b7weg\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Gedanken, die, wie ungedacht, die Seele", "tokens": ["Ge\u00b7dan\u00b7ken", ",", "die", ",", "wie", "un\u00b7ge\u00b7dacht", ",", "die", "See\u00b7le"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PRELS", "$,", "PWAV", "ADJD", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Nur von Gedanken sind, weit reicher, wilder", "tokens": ["Nur", "von", "Ge\u00b7dan\u00b7ken", "sind", ",", "weit", "rei\u00b7cher", ",", "wil\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "APPR", "NN", "VAFIN", "$,", "ADJD", "ADJD", "$,", "ADJA"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.14": {"text": "Und g\u00f6ttlich-vision\u00e4rer, als sie selbst", "tokens": ["Und", "g\u00f6tt\u00b7lich\u00b7vi\u00b7si\u00b7o\u00b7n\u00e4rer", ",", "als", "sie", "selbst"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Der Seraphharfner Israfel (der doch", "tokens": ["Der", "Se\u00b7raph\u00b7harf\u00b7ner", "Is\u00b7ra\u00b7fel", "(", "der", "doch"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NE", "$(", "ART", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "\u00bbdie s\u00fc\u00dfeste der Stimmen hat von allen", "tokens": ["\u00bb", "die", "s\u00fc\u00b7\u00dfes\u00b7te", "der", "Stim\u00b7men", "hat", "von", "al\u00b7len"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "ART", "NN", "VAFIN", "APPR", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Gesch\u00f6pfen Gottes\u00ab) jemals \u00e4u\u00dfern k\u00f6nnte.", "tokens": ["Ge\u00b7sch\u00f6p\u00b7fen", "Got\u00b7tes", "\u00ab", ")", "je\u00b7mals", "\u00e4u\u00b7\u00dfern", "k\u00f6nn\u00b7te", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$(", "$(", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und ich! Ach, meine Zauber sind gebrochen.", "tokens": ["Und", "ich", "!", "Ach", ",", "mei\u00b7ne", "Zau\u00b7ber", "sind", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$.", "ITJ", "$,", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Kraftlos entsinkt die Feder meiner Hand.", "tokens": ["Kraft\u00b7los", "ent\u00b7sinkt", "die", "Fe\u00b7der", "mei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.20": {"text": "Ob du auch batest drum, ich kann es nicht,", "tokens": ["Ob", "du", "auch", "ba\u00b7test", "drum", ",", "ich", "kann", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "PAV", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Mit deinem teuren Namen etwas schreiben.", "tokens": ["Mit", "dei\u00b7nem", "teu\u00b7ren", "Na\u00b7men", "et\u00b7was", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Ich kann nicht sprechen oder denken, ach,", "tokens": ["Ich", "kann", "nicht", "spre\u00b7chen", "o\u00b7der", "den\u00b7ken", ",", "ach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "KON", "VVINF", "$,", "ITJ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Nicht f\u00fchlen mehr; denn das ist kein Gef\u00fchl,", "tokens": ["Nicht", "f\u00fch\u00b7len", "mehr", ";", "denn", "das", "ist", "kein", "Ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ADV", "$.", "KON", "PDS", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Dies starre Stehen auf der goldnen Schwelle", "tokens": ["Dies", "star\u00b7re", "Ste\u00b7hen", "auf", "der", "gold\u00b7nen", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Weitoffnen Traumtors, da ich regungslos,", "tokens": ["Weit\u00b7off\u00b7nen", "Traum\u00b7tors", ",", "da", "ich", "re\u00b7gungs\u00b7los", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KOUS", "PPER", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Entz\u00fcckt vom pr\u00e4chtigen Ausblick und durchschauert", "tokens": ["Ent\u00b7z\u00fcckt", "vom", "pr\u00e4ch\u00b7ti\u00b7gen", "Aus\u00b7blick", "und", "durch\u00b7schau\u00b7ert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "KON", "VVFIN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "So auf dem rechten wie dem linken Weg,", "tokens": ["So", "auf", "dem", "rech\u00b7ten", "wie", "dem", "lin\u00b7ken", "Weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Weithin den ganzen Weg, in Purpurdunst", "tokens": ["Weit\u00b7hin", "den", "gan\u00b7zen", "Weg", ",", "in", "Pur\u00b7pur\u00b7dunst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Bis fern ans Ende sehe \u2013 ", "tokens": ["Bis", "fern", "ans", "En\u00b7de", "se\u00b7he", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}