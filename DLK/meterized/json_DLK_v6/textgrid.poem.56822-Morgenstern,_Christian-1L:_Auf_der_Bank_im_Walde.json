{"textgrid.poem.56822": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf der Bank im Walde", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf der Bank im Walde", "tokens": ["Auf", "der", "Bank", "im", "Wal\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "haben sich gestern zwei gek\u00fc\u00dft.", "tokens": ["ha\u00b7ben", "sich", "ge\u00b7stern", "zwei", "ge\u00b7k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ADV", "CARD", "VVPP", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Heute kommt die Nachtigall", "tokens": ["Heu\u00b7te", "kommt", "die", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "und holt sich, was geblieben ist.", "tokens": ["und", "holt", "sich", ",", "was", "ge\u00b7blie\u00b7ben", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$,", "PRELS", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das M\u00e4dchen hat beim Scheiden", "tokens": ["Das", "M\u00e4d\u00b7chen", "hat", "beim", "Schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "die Z\u00f6pfe neu sich aufgesteckt ...", "tokens": ["die", "Z\u00f6p\u00b7fe", "neu", "sich", "auf\u00b7ge\u00b7steckt", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ei, wie viel blonde Seide da", "tokens": ["Ei", ",", "wie", "viel", "blon\u00b7de", "Sei\u00b7de", "da"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PIAT", "ADJA", "NN", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "die Nachtigall entdeckt!", "tokens": ["die", "Nach\u00b7ti\u00b7gall", "ent\u00b7deckt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Den Schnabel voller F\u00e4den,", "tokens": ["Den", "Schna\u00b7bel", "vol\u00b7ler", "F\u00e4\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "kehrt Nachtigall nach Haus", "tokens": ["kehrt", "Nach\u00b7ti\u00b7gall", "nach", "Haus"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und legt das zarte Nestchen", "tokens": ["und", "legt", "das", "zar\u00b7te", "Nest\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit ihrem Golde aus.", "tokens": ["Mit", "ih\u00b7rem", "Gol\u00b7de", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Freund Nachtigall, Freund Nachtigall,", "tokens": ["Freund", "Nach\u00b7ti\u00b7gall", ",", "Freund", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "so bleib's in allen Jahren! \u2013:", "tokens": ["so", "bleib's", "in", "al\u00b7len", "Jah\u00b7ren", "!", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN", "$.", "$(", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mir werd ein Schn\u00e4blein voll Gesang,", "tokens": ["Mir", "werd", "ein", "Schn\u00e4b\u00b7lein", "voll", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "dir eins voll Liebchens Haaren!", "tokens": ["dir", "eins", "voll", "Lieb\u00b7chens", "Haa\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "ADJD", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}