{"textgrid.poem.33625": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Weinlese", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Winzer, schwerbeladne, sch\u00fctten", "tokens": ["Win\u00b7zer", ",", "schwer\u00b7be\u00b7lad\u00b7ne", ",", "sch\u00fct\u00b7ten"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "ADJA", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihre Trauben aus den B\u00fctten,", "tokens": ["Ih\u00b7re", "Trau\u00b7ben", "aus", "den", "B\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wir stampfen t\u00fcchtig ein;", "tokens": ["Und", "wir", "stamp\u00b7fen", "t\u00fcch\u00b7tig", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch ein Bienchen, mitgefangen,", "tokens": ["Auch", "ein", "Bien\u00b7chen", ",", "mit\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Das am s\u00fc\u00dfen Saft gehangen,", "tokens": ["Das", "am", "s\u00fc\u00b7\u00dfen", "Saft", "ge\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird mit eingestampft zum Wein.", "tokens": ["Wird", "mit", "ein\u00b7ge\u00b7stampft", "zum", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "VVPP", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da\u00df kein Beerchen wir vergeuden,", "tokens": ["Da\u00df", "kein", "Beer\u00b7chen", "wir", "ver\u00b7geu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stirb im Todbett deiner Freuden,", "tokens": ["Stirb", "im", "Tod\u00b7bett", "dei\u00b7ner", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das Unsterblichkeit dir gibt.", "tokens": ["Das", "U\u00b7nsterb\u00b7lich\u00b7keit", "dir", "gibt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In dem Wein, o Biene, leben", "tokens": ["In", "dem", "Wein", ",", "o", "Bie\u00b7ne", ",", "le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$,", "FM", "NN", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wird dein emsig Honigweben", "tokens": ["Wird", "dein", "em\u00b7sig", "Ho\u00b7nig\u00b7we\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJD", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Mit dem Tau, den du geliebt.", "tokens": ["Mit", "dem", "Tau", ",", "den", "du", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Werde denn ein goldner Funken,", "tokens": ["Wer\u00b7de", "denn", "ein", "gold\u00b7ner", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie du selbst wohl freudetrunken", "tokens": ["Wie", "du", "selbst", "wohl", "freu\u00b7de\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schw\u00e4rmtest in dem Sonnenschein,", "tokens": ["Schw\u00e4rm\u00b7test", "in", "dem", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch dein Stachel wird in Herzen,", "tokens": ["Auch", "dein", "Sta\u00b7chel", "wird", "in", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Geisterblitzen, frohen Scherzen", "tokens": ["Geis\u00b7ter\u00b7blit\u00b7zen", ",", "fro\u00b7hen", "Scher\u00b7zen"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auferstehen aus dem Wein.", "tokens": ["Auf\u00b7er\u00b7ste\u00b7hen", "aus", "dem", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}