{"textgrid.poem.49883": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Am Rheinfall", "genre": "verse", "period": "N.A.", "pub_year": 1846, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Halte dein Herz, o Wanderer, fest in gewaltigen H\u00e4nden!", "tokens": ["Hal\u00b7te", "dein", "Herz", ",", "o", "Wan\u00b7de\u00b7rer", ",", "fest", "in", "ge\u00b7wal\u00b7ti\u00b7gen", "H\u00e4n\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "FM", "NN", "$,", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Mir entst\u00fcrzte vor Lust zitternd das meinige fast.", "tokens": ["Mir", "ent\u00b7st\u00fcrz\u00b7te", "vor", "Lust", "zit\u00b7ternd", "das", "mei\u00b7ni\u00b7ge", "fast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "VVPP", "ART", "PPOSS", "ADV", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Rastlos donnernde Massen auf donnernde Massen geworfen,", "tokens": ["Rast\u00b7los", "don\u00b7nern\u00b7de", "Mas\u00b7sen", "auf", "don\u00b7nern\u00b7de", "Mas\u00b7sen", "ge\u00b7wor\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Ohr und Auge wohin retten sie sich im Tumult?", "tokens": ["Ohr", "und", "Au\u00b7ge", "wo\u00b7hin", "ret\u00b7ten", "sie", "sich", "im", "Tu\u00b7mult", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PWAV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Wahrlich, den eigenen Wutschrei h\u00f6rete nicht der Gigant hier,", "tokens": ["Wahr\u00b7lich", ",", "den", "ei\u00b7ge\u00b7nen", "Wut\u00b7schrei", "h\u00f6\u00b7re\u00b7te", "nicht", "der", "Gi\u00b7gant", "hier", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "ADJA", "NN", "VVFIN", "PTKNEG", "ART", "NN", "ADV", "$,"], "meter": "+--+--+--+-+-+-+", "measure": "dactylic.tri.plus"}, "line.6": {"text": "L\u00e4g er, vom Himmel gest\u00fcrzt, unten am Felsen gekr\u00fcmmt!", "tokens": ["L\u00e4g", "er", ",", "vom", "Him\u00b7mel", "ge\u00b7st\u00fcrzt", ",", "un\u00b7ten", "am", "Fel\u00b7sen", "ge\u00b7kr\u00fcmmt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "APPRART", "NN", "VVPP", "$,", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "++-+---+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Rosse der G\u00f6tter, im Schwung, eins \u00fcber dem R\u00fccken des andern,", "tokens": ["Ros\u00b7se", "der", "G\u00f6t\u00b7ter", ",", "im", "Schwung", ",", "eins", "\u00fc\u00b7ber", "dem", "R\u00fc\u00b7cken", "des", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "APPRART", "NN", "$,", "PIS", "APPR", "ART", "NN", "ART", "ADJA", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.8": {"text": "St\u00fcrmen herunter und streun silberne M\u00e4hnen umher;", "tokens": ["St\u00fcr\u00b7men", "her\u00b7un\u00b7ter", "und", "streun", "sil\u00b7ber\u00b7ne", "M\u00e4h\u00b7nen", "um\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VVFIN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Herrliche Leiber, unz\u00e4hlbare, folgen sich, nimmer dieselben,", "tokens": ["Herr\u00b7li\u00b7che", "Lei\u00b7ber", ",", "un\u00b7z\u00e4hl\u00b7ba\u00b7re", ",", "fol\u00b7gen", "sich", ",", "nim\u00b7mer", "die\u00b7sel\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "$,", "VVFIN", "PRF", "$,", "ADV", "PDAT", "$,"], "meter": "+--+-+-+-+--+--+-", "measure": "iambic.septa.invert"}, "line.10": {"text": "Ewig dieselbigen \u2013 wer wartet das Ende wohl aus?", "tokens": ["E\u00b7wig", "die\u00b7sel\u00b7bi\u00b7gen", "\u2013", "wer", "war\u00b7tet", "das", "En\u00b7de", "wohl", "aus", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "$(", "PWS", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+--+---+--+-+-", "measure": "dactylic.di.plus"}, "line.11": {"text": "Angst umzieht dir den Busen mit eins und, ", "tokens": ["Angst", "um\u00b7zieht", "dir", "den", "Bu\u00b7sen", "mit", "eins", "und", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "APPR", "CARD", "KON", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "\u00dcber das Haupt st\u00fcrzt dir krachend das Himmelsgew\u00f6lb!", "tokens": ["\u00dc\u00b7ber", "das", "Haupt", "st\u00fcrzt", "dir", "kra\u00b7chend", "das", "Him\u00b7mels\u00b7ge\u00b7w\u00f6lb", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}}}}