{"textgrid.poem.66160": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Frischlin", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo liegt ", "tokens": ["Wo", "liegt"], "token_info": ["word", "word"], "pos": ["PWAV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Wo scharrten sie des Edeln Asche hin?", "tokens": ["Wo", "scharr\u00b7ten", "sie", "des", "E\u00b7deln", "A\u00b7sche", "hin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo ist sein Grab mit stillem Moos bewachsen?", "tokens": ["Wo", "ist", "sein", "Grab", "mit", "stil\u00b7lem", "Moos", "be\u00b7wach\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo liegt ", "tokens": ["Wo", "liegt"], "token_info": ["word", "word"], "pos": ["PWAV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Er schlummert nur \u2013 vielleicht auf einem Anger!", "tokens": ["Er", "schlum\u00b7mert", "nur", "\u2013", "viel\u00b7leicht", "auf", "ei\u00b7nem", "An\u00b7ger", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn F\u00fcrstenha\u00df lag auf ihm h\u00fcgelschwer,", "tokens": ["Denn", "F\u00fcrs\u00b7ten\u00b7ha\u00df", "lag", "auf", "ihm", "h\u00fc\u00b7gel\u00b7schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Pfaffen br\u00fcllten \u00fcber seiner Leiche:", "tokens": ["Und", "Pfaf\u00b7fen", "br\u00fcll\u00b7ten", "\u00fc\u00b7ber", "sei\u00b7ner", "Lei\u00b7che", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verdammt ist er!", "tokens": ["Ver\u00b7dammt", "ist", "er", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "$."], "meter": "-+--", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Und doch, ", "tokens": ["Und", "doch", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Der Aetherstrahl des Genius geflammt;", "tokens": ["Der", "A\u00b7e\u00b7ther\u00b7strahl", "des", "Ge\u00b7nius", "ge\u00b7flammt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und besser warst du, als die Hasser alle,", "tokens": ["Und", "bes\u00b7ser", "warst", "du", ",", "als", "die", "Has\u00b7ser", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "$,", "KOUS", "ART", "NN", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die dich verdammt.", "tokens": ["Die", "dich", "ver\u00b7dammt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Als Knabe schon griffst du mit k\u00fchnem Finger", "tokens": ["Als", "Kna\u00b7be", "schon", "griffst", "du", "mit", "k\u00fch\u00b7nem", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ins Saitenspiel. Als J\u00fcngling wirbelst du", "tokens": ["Ins", "Sai\u00b7ten\u00b7spiel", ".", "Als", "J\u00fcng\u00b7ling", "wir\u00b7belst", "du"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$.", "KOUS", "NN", "VAFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Lyra Strahlen. Deine T\u00f6ne flogen", "tokens": ["Der", "Ly\u00b7ra", "Strah\u00b7len", ".", "Dei\u00b7ne", "T\u00f6\u00b7ne", "flo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Wolken zu,", "tokens": ["Den", "Wol\u00b7ken", "zu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Die um den Mond mit Silberduft sich ziehen.", "tokens": ["Die", "um", "den", "Mond", "mit", "Sil\u00b7ber\u00b7duft", "sich", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Versammelt waren Roma's Dichter drauf.", "tokens": ["Ver\u00b7sam\u00b7melt", "wa\u00b7ren", "Ro\u00b7ma's", "Dich\u00b7ter", "drauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie stutzten: Aus den W\u00e4ldern der Barbaren", "tokens": ["Sie", "stutz\u00b7ten", ":", "Aus", "den", "W\u00e4l\u00b7dern", "der", "Bar\u00b7ba\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Steigt Sang herauf?", "tokens": ["Steigt", "Sang", "her\u00b7auf", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Dir hat ", "tokens": ["Dir", "hat"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Mit eigner Hand den Sokkus angeschn\u00fcrt,", "tokens": ["Mit", "eig\u00b7ner", "Hand", "den", "Sok\u00b7kus", "an\u00b7ge\u00b7schn\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Dich selbst gef\u00fchrt.", "tokens": ["Dich", "selbst", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Er reichte dir in einem Faunenhorne", "tokens": ["Er", "reich\u00b7te", "dir", "in", "ei\u00b7nem", "Fau\u00b7nen\u00b7hor\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des G\u00f6ttertrankes viel. Da stieg dein Herz", "tokens": ["Des", "G\u00f6t\u00b7ter\u00b7tran\u00b7kes", "viel", ".", "Da", "stieg", "dein", "Herz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$.", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Herauf ins Antlitz, und die Lippen troffen", "tokens": ["Her\u00b7auf", "ins", "Ant\u00b7litz", ",", "und", "die", "Lip\u00b7pen", "trof\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "KON", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von hellem Scherz.", "tokens": ["Von", "hel\u00b7lem", "Scherz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Dich hat ", "tokens": ["Dich", "hat"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "--", "measure": "unknown.measure.zero"}, "line.2": {"text": "Und selbst der Geist des st\u00fcrmenden ", "tokens": ["Und", "selbst", "der", "Geist", "des", "st\u00fcr\u00b7men\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Es segneten der ", "tokens": ["Es", "seg\u00b7ne\u00b7ten", "der"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Dich unsichtbar.", "tokens": ["Dich", "un\u00b7sicht\u00b7bar", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "ADJD", "$."], "meter": "-+--", "measure": "dactylic.init"}}, "stanza.9": {"line.1": {"text": "Dein Auge sah nicht mit dem Scholiasten", "tokens": ["Dein", "Au\u00b7ge", "sah", "nicht", "mit", "dem", "Scho\u00b7li\u00b7as\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur W\u00f6rterkram und Periodenflu\u00df.", "tokens": ["Nur", "W\u00f6r\u00b7ter\u00b7kram", "und", "Pe\u00b7ri\u00b7o\u00b7den\u00b7flu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es sah das Sch\u00f6ne; sah das Wetterleuchten", "tokens": ["Es", "sah", "das", "Sch\u00f6\u00b7ne", ";", "sah", "das", "Wet\u00b7ter\u00b7leuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Genius.", "tokens": ["Des", "Ge\u00b7nius", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.10": {"line.1": {"text": "Wie silbernes Getr\u00e4ufel aus den Wolken", "tokens": ["Wie", "sil\u00b7ber\u00b7nes", "Ge\u00b7tr\u00e4u\u00b7fel", "aus", "den", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "War deine Red' im vollgedr\u00e4ngten Saal.", "tokens": ["War", "dei\u00b7ne", "Red'", "im", "voll\u00b7ge\u00b7dr\u00e4ng\u00b7ten", "Saal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Wahrheit schien ein Schwert in deinem Munde,", "tokens": ["Die", "Wahr\u00b7heit", "schien", "ein", "Schwert", "in", "dei\u00b7nem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Wetterstrahl.", "tokens": ["Ein", "Wet\u00b7ter\u00b7strahl", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "Als R\u00f6mer schriebst du; aber deine Seele", "tokens": ["Als", "R\u00f6\u00b7mer", "schriebst", "du", ";", "a\u00b7ber", "dei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "$.", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Voll Vaterland, liebt deutschen Biederton.", "tokens": ["Voll", "Va\u00b7ter\u00b7land", ",", "liebt", "deut\u00b7schen", "Bie\u00b7der\u00b7ton", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du sprachst den stolzen purpurnen Tyrannen", "tokens": ["Du", "sprachst", "den", "stol\u00b7zen", "pur\u00b7pur\u00b7nen", "Ty\u00b7ran\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ins Antlitz Hohn.", "tokens": ["Ins", "Ant\u00b7litz", "Hohn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "Da schlug Gewaltthat dich in Eisenfessel;", "tokens": ["Da", "schlug", "Ge\u00b7walt\u00b7that", "dich", "in", "Ei\u00b7sen\u00b7fes\u00b7sel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Sie ging voll Hohn um deine Gruft herum,", "tokens": ["Sie", "ging", "voll", "Hohn", "um", "dei\u00b7ne", "Gruft", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und br\u00fcllte: Ha, da fault er nun, mein Hasser,", "tokens": ["Und", "br\u00fcll\u00b7te", ":", "Ha", ",", "da", "fault", "er", "nun", ",", "mein", "Has\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ITJ", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf ewig stumm.", "tokens": ["Auf", "e\u00b7wig", "stumm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.13": {"line.1": {"text": "Du aber schnellst mit wuthbeflammten H\u00e4nden", "tokens": ["Du", "a\u00b7ber", "schnellst", "mit", "wuth\u00b7be\u00b7flamm\u00b7ten", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die dichtgeringte Eisenlast entzwei;", "tokens": ["Die", "dicht\u00b7ge\u00b7ring\u00b7te", "Ei\u00b7sen\u00b7last", "ent\u00b7zwei", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Entrei\u00dfst dich muthig durch des Kerkers Quader", "tokens": ["Ent\u00b7rei\u00df\u00b7st", "dich", "mut\u00b7hig", "durch", "des", "Ker\u00b7kers", "Qua\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Der Sklaverei.", "tokens": ["Der", "Skla\u00b7ve\u00b7rei", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.14": {"line.1": {"text": "Doch ach! an eines grauen Felsen Wurzel", "tokens": ["Doch", "ach", "!", "an", "ei\u00b7nes", "grau\u00b7en", "Fel\u00b7sen", "Wur\u00b7zel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "XY", "$.", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fand er, der Edle, seinen Martertod.", "tokens": ["Fand", "er", ",", "der", "Ed\u00b7le", ",", "sei\u00b7nen", "Mar\u00b7ter\u00b7tod", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "ADJA", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Winzer sah den Dichter blutig liegen", "tokens": ["Ein", "Win\u00b7zer", "sah", "den", "Dich\u00b7ter", "blu\u00b7tig", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Morgenroth.", "tokens": ["Im", "Mor\u00b7gen\u00b7roth", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.15": {"line.1": {"text": "Wo ruht er nun, der Bruder meines Geistes?", "tokens": ["Wo", "ruht", "er", "nun", ",", "der", "Bru\u00b7der", "mei\u00b7nes", "Geis\u00b7tes", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo scharrten sie des Edlen Tr\u00fcmmer hin?", "tokens":