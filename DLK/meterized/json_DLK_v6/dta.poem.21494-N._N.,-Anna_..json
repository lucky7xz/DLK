{"dta.poem.21494": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Anna .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Die Drossel ruft vom Lindenbaum, die Sonne steigt herauf mit Lust,", "tokens": ["Die", "Dros\u00b7sel", "ruft", "vom", "Lin\u00b7den\u00b7baum", ",", "die", "Son\u00b7ne", "steigt", "her\u00b7auf", "mit", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,", "ART", "NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "La\u00df einmal noch mein blasses Haupt sich lehnen m\u00fcd\u2019 an deine Brust.", "tokens": ["La\u00df", "ein\u00b7mal", "noch", "mein", "blas\u00b7ses", "Haupt", "sich", "leh\u00b7nen", "m\u00fcd'", "an", "dei\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "PRF", "VVINF", "VMFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Noch einmal la\u00df mich deine Hand inbr\u00fcnstig k\u00fcssen hei\u00df und schwer, \u2014", "tokens": ["Noch", "ein\u00b7mal", "la\u00df", "mich", "dei\u00b7ne", "Hand", "in\u00b7br\u00fcns\u00b7tig", "k\u00fcs\u00b7sen", "hei\u00df", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "VVIMP", "PPER", "PPOSAT", "NN", "ADJD", "VVFIN", "ADJD", "KON", "ADJD", "$,", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Nicht deinen Mund \u2014 nicht deinen Mund! ich lie\u00dfe dich sonst nimmermehr.", "tokens": ["Nicht", "dei\u00b7nen", "Mund", "nicht", "dei\u00b7nen", "Mund", "!", "ich", "lie\u00b7\u00dfe", "dich", "sonst", "nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "$(", "PTKNEG", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+--+-+--++--+", "measure": "iambic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "Maimorgenwind lacht heimlich leis\u2019 und raunt im gr\u00fcnenden Spalier,", "tokens": ["Mai\u00b7mor\u00b7gen\u00b7wind", "lacht", "heim\u00b7lich", "leis'", "und", "raunt", "im", "gr\u00fc\u00b7nen\u00b7den", "Spa\u00b7lier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "ADJD", "KON", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.2": {"text": "Doch wenn der Abend niederf\u00e4llt, dann bist du, Heinrich, nicht mehr hier!", "tokens": ["Doch", "wenn", "der", "A\u00b7bend", "nie\u00b7der\u00b7f\u00e4llt", ",", "dann", "bist", "du", ",", "Hein\u00b7rich", ",", "nicht", "mehr", "hier", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "$,", "NE", "$,", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Nein, nein, dein Mund und Auge l\u00fcgt: Es wei\u00df dein Herz so gut wie ich,", "tokens": ["Nein", ",", "nein", ",", "dein", "Mund", "und", "Au\u00b7ge", "l\u00fcgt", ":", "Es", "wei\u00df", "dein", "Herz", "so", "gut", "wie", "ich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "KOKOM", "PPER", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und wenn Du einst auch heimw\u00e4rts kehrst, nie wieder schaut mein Auge dich.", "tokens": ["Und", "wenn", "Du", "einst", "auch", "heim\u00b7w\u00e4rts", "kehrst", ",", "nie", "wie\u00b7der", "schaut", "mein", "Au\u00b7ge", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "ADV", "VVFIN", "$,", "ADV", "ADV", "VVFIN", "PPOSAT", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Sonst logst du nie, ich wei\u00df es wohl, sprachst niemals von dem gold\u2019nen Ring,", "tokens": ["Sonst", "logst", "du", "nie", ",", "ich", "wei\u00df", "es", "wohl", ",", "sprachst", "nie\u00b7mals", "von", "dem", "gold'\u00b7nen", "Ring", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Du, Heinrich, bist so klug und ich ein arm unwissend h\u00e4\u00dflich Ding.", "tokens": ["Du", ",", "Hein\u00b7rich", ",", "bist", "so", "klug", "und", "ich", "ein", "arm", "un\u00b7wis\u00b7send", "h\u00e4\u00df\u00b7lich", "Ding", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$,", "VAFIN", "ADV", "ADJD", "KON", "PPER", "ART", "ADJD", "ADJD", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Ich wu\u00dft\u2019 es wohl, ich w\u00fcrde nie dir dienen treu und still als Frau, \u2014", "tokens": ["Ich", "wu\u00dft'", "es", "wohl", ",", "ich", "w\u00fcr\u00b7de", "nie", "dir", "die\u00b7nen", "treu", "und", "still", "als", "Frau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "ADV", "PPER", "VVINF", "ADJD", "KON", "ADJD", "KOKOM", "NN", "$,", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Denn deine Hand ist weich und zart, und meine ganz von Arbeit rauh.", "tokens": ["Denn", "dei\u00b7ne", "Hand", "ist", "weich", "und", "zart", ",", "und", "mei\u00b7ne", "ganz", "von", "Ar\u00b7beit", "rauh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,", "KON", "PPOSAT", "ADV", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Ich wei\u00df es wohl, wie du dich stolz verzehrst nach Ruhm und Sonnenschein, \u2014", "tokens": ["Ich", "wei\u00df", "es", "wohl", ",", "wie", "du", "dich", "stolz", "ver\u00b7zehrst", "nach", "Ruhm", "und", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "PRF", "ADJD", "VVFIN", "APPR", "NN", "KON", "NN", "$,", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und in der Reichen helles Schlo\u00df, ich Arme, darf nicht mit hinein.", "tokens": ["Und", "in", "der", "Rei\u00b7chen", "hel\u00b7les", "Schlo\u00df", ",", "ich", "Ar\u00b7me", ",", "darf", "nicht", "mit", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$,", "PPER", "NN", "$,", "VMFIN", "PTKNEG", "APPR", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.8": {"line.1": {"text": "Ich wu\u00dft\u2019 es wohl, ich wu\u00dft\u2019 es wohl vom ersten Anfang an, da\u00df du \u2014", "tokens": ["Ich", "wu\u00dft'", "es", "wohl", ",", "ich", "wu\u00dft'", "es", "wohl", "vom", "ers\u00b7ten", "An\u00b7fang", "an", ",", "da\u00df", "du"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "PTKVZ", "$,", "KOUS", "PPER", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Mein Ungl\u00fcck, Schmach und ew\u2019gen Tod, \u2014 ach alles f\u00fcgtest du mir zu!", "tokens": ["Mein", "Un\u00b7gl\u00fcck", ",", "Schmach", "und", "ew'\u00b7gen", "Tod", ",", "ach", "al\u00b7les", "f\u00fcg\u00b7test", "du", "mir", "zu", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "ADJA", "NN", "$,", "$(", "XY", "PIS", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+--+--+-+", "measure": "iambic.septa.relaxed"}}, "stanza.9": {"line.1": {"text": "Ich wu\u00dft\u2019 es wohl, da\u00df so es kam, Elend und Schande \u00fcber mich,", "tokens": ["Ich", "wu\u00dft'", "es", "wohl", ",", "da\u00df", "so", "es", "kam", ",", "E\u00b7lend", "und", "Schan\u00b7de", "\u00fc\u00b7ber", "mich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KOUS", "ADV", "PPER", "VVFIN", "$,", "NN", "KON", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-++--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Und dennoch, dennoch kam\u2019s, denn ach! ich liebte gar zu innig dich!", "tokens": ["Und", "den\u00b7noch", ",", "den\u00b7noch", "kam's", ",", "denn", "ach", "!", "ich", "lieb\u00b7te", "gar", "zu", "in\u00b7nig", "dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "VVFIN", "$,", "KON", "XY", "$.", "PPER", "VVFIN", "ADV", "PTKA", "ADJD", "PPER", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.10": {"line.1": {"text": "Die Drossel ruft vom Lindenbaum, die Sonne kommt herauf mit Lust,", "tokens": ["Die", "Dros\u00b7sel", "ruft", "vom", "Lin\u00b7den\u00b7baum", ",", "die", "Son\u00b7ne", "kommt", "her\u00b7auf", "mit", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,", "ART", "NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "La\u00df einmal noch mein blasses Haupt sich lehnen m\u00fcd\u2019 an deine Brust.", "tokens": ["La\u00df", "ein\u00b7mal", "noch", "mein", "blas\u00b7ses", "Haupt", "sich", "leh\u00b7nen", "m\u00fcd'", "an", "dei\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "PRF", "VVINF", "VMFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.11": {"line.1": {"text": "Weh, meinen Busen pre\u00dft und sprengt\u2019s, ein Feuer lodert schw\u00fcl und hei\u00df,", "tokens": ["Weh", ",", "mei\u00b7nen", "Bu\u00b7sen", "pre\u00dft", "und", "sprengt's", ",", "ein", "Feu\u00b7er", "lo\u00b7dert", "schw\u00fcl", "und", "hei\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$,", "ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und unter meinem Herzen quillt und regt es sich und athmet leis\u2019.", "tokens": ["Und", "un\u00b7ter", "mei\u00b7nem", "Her\u00b7zen", "quillt", "und", "regt", "es", "sich", "und", "ath\u00b7met", "leis'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "PPER", "PRF", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.12": {"line.1": {"text": "Und f\u00e4llt hernieder jene Nacht, und lieg\u2019 ich bla\u00df und leidenswund,", "tokens": ["Und", "f\u00e4llt", "her\u00b7nie\u00b7der", "je\u00b7ne", "Nacht", ",", "und", "lieg'", "ich", "bla\u00df", "und", "lei\u00b7dens\u00b7wund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PDAT", "NN", "$,", "KON", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Dann Heinrich bist du fern und k\u00fc\u00df\u2019st \u2014 ach, k\u00fc\u00df\u2019st wohl einen sch\u00f6n\u2019ren Mund.", "tokens": ["Dann", "Hein\u00b7rich", "bist", "du", "fern", "und", "k\u00fc\u00df'st", "ach", ",", "k\u00fc\u00df'st", "wohl", "ei\u00b7nen", "sch\u00f6n'\u00b7ren", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VAFIN", "PPER", "ADJD", "KON", "VVFIN", "$(", "ITJ", "$,", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}}, "stanza.13": {"line.1": {"text": "Und dennoch ist\u2019s von deinem Fleisch und dennoch lebt\u2019s von deinem Blut,", "tokens": ["Und", "den\u00b7noch", "ist's", "von", "dei\u00b7nem", "Fleisch", "und", "den\u00b7noch", "lebt's", "von", "dei\u00b7nem", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "APPR", "PPOSAT", "NN", "KON", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und dennoch sieht\u2019s dein Auge nie, das treu und z\u00e4rtlich auf ihm ruht.", "tokens": ["Und", "den\u00b7noch", "sieht's", "dein", "Au\u00b7ge", "nie", ",", "das", "treu", "und", "z\u00e4rt\u00b7lich", "auf", "ihm", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "ADV", "$,", "PRELS", "ADJD", "KON", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.14": {"line.1": {"text": "Nur Thr\u00e4nen f\u00fchlt es, fallend schwer, Gl\u00fchtropfen, auf sein Angesicht,", "tokens": ["Nur", "Thr\u00e4\u00b7nen", "f\u00fchlt", "es", ",", "fal\u00b7lend", "schwer", ",", "Gl\u00fch\u00b7trop\u00b7fen", ",", "auf", "sein", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPER", "$,", "ADJD", "ADJD", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Nur Seufzer h\u00f6rt\u2019s und leisen Schlag des Herzens, das im Tode bricht.", "tokens": ["Nur", "Seuf\u00b7zer", "h\u00f6rt's", "und", "lei\u00b7sen", "Schlag", "des", "Her\u00b7zens", ",", "das", "im", "To\u00b7de", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "KON", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.15": {"line.1": {"text": "Und eh\u2019s gebor\u2019n, ert\u00f6nt ihm schon des Vaters und der Mutter Fluch;", "tokens": ["Und", "eh's", "ge\u00b7bor'n", ",", "er\u00b7t\u00f6nt", "ihm", "schon", "des", "Va\u00b7ters", "und", "der", "Mut\u00b7ter", "Fluch", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVPP", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "W\u00e4r\u2019st du doch todt, mein Kind, mein Kind, und l\u00e4gst du stumm im", "tokens": ["W\u00e4r'st", "du", "doch", "todt", ",", "mein", "Kind", ",", "mein", "Kind", ",", "und", "l\u00e4gst", "du", "stumm", "im"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "ADJD", "APPRART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Leichentuch! . . . .", "tokens": ["Lei\u00b7chen\u00b7tuch", "!", ".", ".", ".", "."], "token_info": ["word", "punct", "punct", "punct", "punct", "punct"], "pos": ["NN", "$.", "$.", "$.", "$.", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.16": {"line.1": {"text": "Wir waren lang zusammen nun, Heinrich! ich glaub, \u2019s ist schon ein Jahr", "tokens": ["Wir", "wa\u00b7ren", "lang", "zu\u00b7sam\u00b7men", "nun", ",", "Hein\u00b7rich", "!", "ich", "glaub", ",", "'s", "ist", "schon", "ein", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "ADV", "$,", "NE", "$.", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+---+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Da k\u00fc\u00dftest du zum ersten Mal verstohlen mein lichtblondes Haar.", "tokens": ["Da", "k\u00fc\u00df\u00b7test", "du", "zum", "ers\u00b7ten", "Mal", "ver\u00b7stoh\u00b7len", "mein", "licht\u00b7blon\u00b7des", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.17": {"line.1": {"text": "Nun lacht heimlich Maimorgenwind und raunt im gr\u00fcnenden Spalier,", "tokens": ["Nun", "lacht", "heim\u00b7lich", "Mai\u00b7mor\u00b7gen\u00b7wind", "und", "raunt", "im", "gr\u00fc\u00b7nen\u00b7den", "Spa\u00b7lier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "NN", "KON", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und wenn der Abend niederf\u00e4llt, dann bist du, Heinrich nicht mehr hier.", "tokens": ["Und", "wenn", "der", "A\u00b7bend", "nie\u00b7der\u00b7f\u00e4llt", ",", "dann", "bist", "du", ",", "Hein\u00b7rich", "nicht", "mehr", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "$,", "NE", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.18": {"line.1": {"text": "Und bist du fern, ich will ja nicht, da\u00df Thr\u00e4nen du um mich vergie\u00df\u2019st,", "tokens": ["Und", "bist", "du", "fern", ",", "ich", "will", "ja", "nicht", ",", "da\u00df", "Thr\u00e4\u00b7nen", "du", "um", "mich", "ver\u00b7gie\u00df'st", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "PPER", "VMFIN", "ADV", "PTKNEG", "$,", "KOUS", "NN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Doch denk daran, wie hei\u00df um dich aus meinem Aug\u2019 die Thr\u00e4ne flie\u00dft \u2026", "tokens": ["Doch", "denk", "da\u00b7ran", ",", "wie", "hei\u00df", "um", "dich", "aus", "mei\u00b7nem", "Aug'", "die", "Thr\u00e4\u00b7ne", "flie\u00dft", "\u2026"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "$,", "PWAV", "ADJD", "APPR", "PRF", "APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.19": {"line.1": {"text": "O denk\u2019 zuweilen, wie mich Noth und Ungl\u00fcck packt so rauh und hart,", "tokens": ["O", "denk'", "zu\u00b7wei\u00b7len", ",", "wie", "mich", "Noth", "und", "Un\u00b7gl\u00fcck", "packt", "so", "rauh", "und", "hart", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,", "PWAV", "PPER", "NN", "KON", "NN", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Vergi\u00df es nicht, da\u00df ich aus Liebe zu dir so sehr ungl\u00fccklich ward!", "tokens": ["Ver\u00b7gi\u00df", "es", "nicht", ",", "da\u00df", "ich", "aus", "Lie\u00b7be", "zu", "dir", "so", "sehr", "un\u00b7gl\u00fcck\u00b7lich", "ward", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "APPR", "NN", "APPR", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.20": {"line.1": {"text": "Und f\u00fchrst du einst ein Fr\u00e4ulein dir zur Hochzeit und zur Kirch\u2019 hinab,", "tokens": ["Und", "f\u00fchrst", "du", "einst", "ein", "Fr\u00e4u\u00b7lein", "dir", "zur", "Hoch\u00b7zeit", "und", "zur", "Kirch'", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "PPER", "APPRART", "NN", "KON", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Zum letzten Male denke dann, wie der Wind geht \u00fcber ein fernes Grab.", "tokens": ["Zum", "letz\u00b7ten", "Ma\u00b7le", "den\u00b7ke", "dann", ",", "wie", "der", "Wind", "geht", "\u00fc\u00b7ber", "ein", "fer\u00b7nes", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+--+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.21": {"line.1": {"text": "Doch sage nie, k\u00fc\u00dft du voll Gluth den Mund und ihrer Augen Schein,", "tokens": ["Doch", "sa\u00b7ge", "nie", ",", "k\u00fc\u00dft", "du", "voll", "Gluth", "den", "Mund", "und", "ih\u00b7rer", "Au\u00b7gen", "Schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "VVFIN", "PPER", "ADJD", "NN", "ART", "NN", "KON", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Sag\u2019 nicht, da\u00df du von mir gegangen, weil ich so schlecht und so gemein.", "tokens": ["Sag'", "nicht", ",", "da\u00df", "du", "von", "mir", "ge\u00b7gan\u00b7gen", ",", "weil", "ich", "so", "schlecht", "und", "so", "ge\u00b7mein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "KOUS", "PPER", "APPR", "PPER", "VVPP", "$,", "KOUS", "PPER", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+--+--+--+-+-+-+", "measure": "amphibrach.tetra.plus"}}, "stanza.22": {"line.1": {"text": "Und spotte du am Schenktisch nie, wie man am Schenktisch sonst wohl thut", "tokens": ["Und", "spot\u00b7te", "du", "am", "Schenk\u00b7tisch", "nie", ",", "wie", "man", "am", "Schenk\u00b7tisch", "sonst", "wohl", "thut"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "ADV", "$,", "PWAV", "PIS", "APPRART", "NN", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Der armen Dirne aus dem Volk, die dich so liebte, dir so gut.", "tokens": ["Der", "ar\u00b7men", "Dir\u00b7ne", "aus", "dem", "Volk", ",", "die", "dich", "so", "lieb\u00b7te", ",", "dir", "so", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.23": {"line.1": {"text": "Denn th\u00e4test du\u2019s, denn th\u00e4test du\u2019s, dann wollt ich sprengen wohl mein Grab,", "tokens": ["Denn", "th\u00e4\u00b7test", "du's", ",", "denn", "th\u00e4\u00b7test", "du's", ",", "dann", "wollt", "ich", "spren\u00b7gen", "wohl", "mein", "Grab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$,", "KON", "VVFIN", "PIS", "$,", "ADV", "VMFIN", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und schmetterte Krankheit und Wahnsinn auf dein verfluchtes Haupt herab \u2026", "tokens": ["Und", "schmet\u00b7ter\u00b7te", "Krank\u00b7heit", "und", "Wahn\u00b7sinn", "auf", "dein", "ver\u00b7fluch\u00b7tes", "Haupt", "her\u00b7ab", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "APPR", "PPOSAT", "ADJA", "NN", "ADV", "$("], "meter": "-+--+--+--+-+-+-+", "measure": "amphibrach.tetra.plus"}}, "stanza.24": {"line.1": {"text": "Dann w\u00fcrf ich Blut und Flammengluth wohl auf das Liebste, was du hast,", "tokens": ["Dann", "w\u00fcrf", "ich", "Blut", "und", "Flam\u00b7men\u00b7gluth", "wohl", "auf", "das", "Liebs\u00b7te", ",", "was", "du", "hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "ADV", "APPR", "ART", "NN", "$,", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Dann send\u2019 ich in das Herz und Hirn die ganze H\u00f6lle dir zu Gast \u2026", "tokens": ["Dann", "send'", "ich", "in", "das", "Herz", "und", "Hirn", "die", "gan\u00b7ze", "H\u00f6l\u00b7le", "dir", "zu", "Gast", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "KON", "NN", "ART", "ADJA", "NN", "PPER", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.25": {"line.1": {"text": "O S\u00fc\u00dfer, Liebster z\u00fcrne du, o z\u00fcrn\u2019 nicht \u00fcber solch ein Wort, \u2014", "tokens": ["O", "S\u00fc\u00b7\u00dfer", ",", "Liebs\u00b7ter", "z\u00fcr\u00b7ne", "du", ",", "o", "z\u00fcrn'", "nicht", "\u00fc\u00b7ber", "solch", "ein", "Wort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "$,", "NN", "VVFIN", "PPER", "$,", "FM", "FM", "PTKNEG", "APPR", "PIAT", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Die Sonne steigt, die Stunde naht, und du gehst ewig von mir fort.", "tokens": ["Die", "Son\u00b7ne", "steigt", ",", "die", "Stun\u00b7de", "naht", ",", "und", "du", "gehst", "e\u00b7wig", "von", "mir", "fort", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,", "KON", "PPER", "VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.26": {"line.1": {"text": "Und was ich wollte, Lieber du? Ich wollte nur, sei nicht betr\u00fcbt,", "tokens": ["Und", "was", "ich", "woll\u00b7te", ",", "Lie\u00b7ber", "du", "?", "Ich", "woll\u00b7te", "nur", ",", "sei", "nicht", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VMFIN", "$,", "ADJD", "PPER", "$.", "PPER", "VMFIN", "ADV", "$,", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Du hast nicht Schuld, ich segne dich, ich hab\u2019 dich ja so sehr geliebt!", "tokens": ["Du", "hast", "nicht", "Schuld", ",", "ich", "seg\u00b7ne", "dich", ",", "ich", "hab'", "dich", "ja", "so", "sehr", "ge\u00b7liebt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "$,", "PPER", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "PPER", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.27": {"line.1": {"text": "Ich segne dich f\u00fcr jedes Wort, f\u00fcr jeden Ku\u00df von deinem Mund,", "tokens": ["Ich", "seg\u00b7ne", "dich", "f\u00fcr", "je\u00b7des", "Wort", ",", "f\u00fcr", "je\u00b7den", "Ku\u00df", "von", "dei\u00b7nem", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und treff\u2019 dich nie so harter Schmerz und furche deine Seele wund!", "tokens": ["Und", "treff'", "dich", "nie", "so", "har\u00b7ter", "Schmerz", "und", "fur\u00b7che", "dei\u00b7ne", "See\u00b7le", "wund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "wo