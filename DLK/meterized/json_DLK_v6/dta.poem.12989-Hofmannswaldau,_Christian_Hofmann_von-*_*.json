{"dta.poem.12989": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "* *", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer seine lebens-zeit zu keinem menschen kommen:", "tokens": ["Wer", "sei\u00b7ne", "le\u00b7bens\u00b7zeit", "zu", "kei\u00b7nem", "men\u00b7schen", "kom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die jahre dem Homer\u2019 und Priscian geschenckt:", "tokens": ["Die", "jah\u00b7re", "dem", "Ho\u00b7mer'", "und", "Pri\u00b7sci\u00b7an", "ge\u00b7schenckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NE", "KON", "NE", "VVPP", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Jrrt, wenn er in der welt so fortzukommen denckt.", "tokens": ["Jrrt", ",", "wenn", "er", "in", "der", "welt", "so", "fort\u00b7zu\u00b7kom\u00b7men", "denckt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "ADV", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer hat an seinen hof pedanten aufgenommen?", "tokens": ["Wer", "hat", "an", "sei\u00b7nen", "hof", "pe\u00b7dan\u00b7ten", "auf\u00b7ge\u00b7nom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPOSAT", "NN", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein ignorante zeigt: (wenn er zu leben wei\u00df)", "tokens": ["Ein", "ig\u00b7no\u00b7ran\u00b7te", "zeigt", ":(", "wenn", "er", "zu", "le\u00b7ben", "wei\u00df", ")"], "token_info": ["word", "word", "word", "emoticon", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$(", "KOUS", "PPER", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der umgang mit der welt mit etwas witz und glimpffe", "tokens": ["Der", "um\u00b7gang", "mit", "der", "welt", "mit", "et\u00b7was", "witz", "und", "glimpf\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "APPR", "PIAT", "NN", "KON", "VVFIN"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.7": {"text": "Behalte, dem Latein und Griechischen zum schimpffe,", "tokens": ["Be\u00b7hal\u00b7te", ",", "dem", "La\u00b7tein", "und", "Grie\u00b7chi\u00b7schen", "zum", "schimpf\u00b7fe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "KON", "NN", "APPRART", "VVFIN", "$,"], "meter": "-+-+++-+-+-+-", "measure": "unknown.measure.septa"}, "line.8": {"text": "Was gl\u00fcck und nutz betrifft, noch allezeit den prei\u00df.", "tokens": ["Was", "gl\u00fcck", "und", "nutz", "be\u00b7tr\u00b7ifft", ",", "noch", "al\u00b7le\u00b7zeit", "den", "prei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "NN", "VVFIN", "$,", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}}}}