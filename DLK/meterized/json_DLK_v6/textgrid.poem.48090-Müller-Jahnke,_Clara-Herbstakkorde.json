{"textgrid.poem.48090": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Herbstakkorde", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Laublose Aeste", "tokens": ["Laub\u00b7lo\u00b7se", "A\u00b7es\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.2": {"text": "strecken die B\u00e4ume", "tokens": ["stre\u00b7cken", "die", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "wie flehend erhobene", "tokens": ["wie", "fle\u00b7hend", "er\u00b7ho\u00b7be\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJD", "ADJA"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.4": {"text": "H\u00e4nde gen Himmel,", "tokens": ["H\u00e4n\u00b7de", "gen", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "und wo ich schreite:", "tokens": ["und", "wo", "ich", "schrei\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "zu meinen F\u00fc\u00dfen", "tokens": ["zu", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "ein d\u00fcrres Rascheln,", "tokens": ["ein", "d\u00fcr\u00b7res", "Ra\u00b7scheln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "als glitte zur Seite", "tokens": ["als", "glit\u00b7te", "zur", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "mir leicht der Tod . . .", "tokens": ["mir", "leicht", "der", "Tod", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "ADJD", "ART", "NN", "$.", "$.", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Im Hauch des Nordwinds", "tokens": ["Im", "Hauch", "des", "Nord\u00b7winds"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "flattert hoch oben", "tokens": ["flat\u00b7tert", "hoch", "o\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADJD", "ADV"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "im Wipfel der Eiche", "tokens": ["im", "Wip\u00b7fel", "der", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "das letzte Blatt.", "tokens": ["das", "letz\u00b7te", "Blatt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Wehe hernieder,", "tokens": ["We\u00b7he", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "einsames Blatt!", "tokens": ["ein\u00b7sa\u00b7mes", "Blatt", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Nieder zum Staub", "tokens": ["Nie\u00b7der", "zum", "Staub"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "m\u00fcssen die bunten", "tokens": ["m\u00fcs\u00b7sen", "die", "bun\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "schimmernden Kinder des Lenzes alle;", "tokens": ["schim\u00b7mern\u00b7den", "Kin\u00b7der", "des", "Len\u00b7zes", "al\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "PIS", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "nieder zum Staub", "tokens": ["nie\u00b7der", "zum", "Staub"], "token_info": ["word", "word", "word"], "pos": ["PTKVZ", "APPRART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "m\u00fcssen die seligen", "tokens": ["m\u00fcs\u00b7sen", "die", "se\u00b7li\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Bl\u00fctentr\u00e4ume des Menschenherzens,", "tokens": ["Bl\u00fc\u00b7ten\u00b7tr\u00e4u\u00b7me", "des", "Men\u00b7schen\u00b7her\u00b7zens", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "m\u00fcssen die stolzen", "tokens": ["m\u00fcs\u00b7sen", "die", "stol\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Lichtgedanken der Menschenstirne \u2013 \u2013", "tokens": ["Licht\u00b7ge\u00b7dan\u00b7ken", "der", "Men\u00b7schens\u00b7tir\u00b7ne", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "$(", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "und er selber, der Mensch,", "tokens": ["und", "er", "sel\u00b7ber", ",", "der", "Mensch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "$,", "ART", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.10": {"text": "der hochgewaltige, seelenbegabte", "tokens": ["der", "hoch\u00b7ge\u00b7wal\u00b7ti\u00b7ge", ",", "see\u00b7len\u00b7be\u00b7gab\u00b7te"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "ADJA", "$,", "VVFIN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Erdgebieter,", "tokens": ["Erd\u00b7ge\u00b7bie\u00b7ter", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "nieder mu\u00df er,", "tokens": ["nie\u00b7der", "mu\u00df", "er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKVZ", "VMFIN", "PPER", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "nieder zum Staub!", "tokens": ["nie\u00b7der", "zum", "Staub", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKVZ", "APPRART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Du kennst sie, die ewigen", "tokens": ["Du", "kennst", "sie", ",", "die", "e\u00b7wi\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ART", "ADJA"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "wandellosen Gewalten \u2013 \u2013", "tokens": ["wan\u00b7del\u00b7lo\u00b7sen", "Ge\u00b7wal\u00b7ten", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["VVFIN", "NN", "$(", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "was str\u00e4ubst du dich?!", "tokens": ["was", "str\u00e4ubst", "du", "dich", "?!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Sch\u00e4rfer weht der Nordost.", "tokens": ["Sch\u00e4r\u00b7fer", "weht", "der", "Nord\u00b7ost", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Durch kahles Gezweig", "tokens": ["Durch", "kah\u00b7les", "Ge\u00b7zweig"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "kichert und pfeift", "tokens": ["ki\u00b7chert", "und", "pfeift"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "sein eisiges Gel\u00e4chter . . .", "tokens": ["sein", "ei\u00b7si\u00b7ges", "Ge\u00b7l\u00e4ch\u00b7ter", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Einsames Blatt,", "tokens": ["Ein\u00b7sa\u00b7mes", "Blatt", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "du sinkst!", "tokens": ["du", "sinkst", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}}}}