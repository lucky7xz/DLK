{"dta.poem.9072": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nacht.", "tokens": ["Nacht", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Der Ahorn vor meinem Fenster rauscht,", "tokens": ["Der", "A\u00b7horn", "vor", "mei\u00b7nem", "Fens\u00b7ter", "rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "von seinen Bl\u00e4ttern funkelt der Thau ins Gras,", "tokens": ["von", "sei\u00b7nen", "Bl\u00e4t\u00b7tern", "fun\u00b7kelt", "der", "Thau", "ins", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "und mein Herz", "tokens": ["und", "mein", "Herz"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "schl\u00e4gt.", "tokens": ["schl\u00e4gt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}}, "stanza.3": {"line.1": {"text": "Nacht.", "tokens": ["Nacht", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "Ein Hund . . . bellt, . . . ein Zweig . . . knickt, \u2014 still!", "tokens": ["Ein", "Hund", ".", ".", ".", "bellt", ",", ".", ".", ".", "ein", "Zweig", ".", ".", ".", "knickt", ",", "still", "!"], "token_info": ["word", "word", "punct", "punct", "punct", "word", "punct", "punct", "punct", "punct", "word", "word", "punct", "punct", "punct", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "NN", "$.", "$.", "$.", "VVPP", "$,", "$.", "$.", "$.", "ART", "NN", "$.", "$.", "$.", "VVFIN", "$,", "$(", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Still!!", "tokens": ["Still", "!!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+", "measure": "single.up"}}, "stanza.6": {"line.1": {"text": "Du? . . . Du?", "tokens": ["Du", "?", ".", ".", ".", "Du", "?"], "token_info": ["word", "punct", "punct", "punct", "punct", "word", "punct"], "pos": ["PPER", "$.", "$.", "$.", "$.", "PPER", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.7": {"line.1": {"text": "Ah, deine Hand! Wie kalt, wie kalt!", "tokens": ["Ah", ",", "dei\u00b7ne", "Hand", "!", "Wie", "kalt", ",", "wie", "kalt", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$.", "PWAV", "ADJD", "$,", "PWAV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und . . . deine Augen . . . gebrochen!", "tokens": ["Und", ".", ".", ".", "dei\u00b7ne", "Au\u00b7gen", ".", ".", ".", "ge\u00b7bro\u00b7chen", "!"], "token_info": ["word", "punct", "punct", "punct", "word", "word", "punct", "punct", "punct", "word", "punct"], "pos": ["KON", "$.", "$.", "$.", "PPOSAT", "NN", "$.", "$.", "$.", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Gebrochen!!", "tokens": ["Ge\u00b7bro\u00b7chen", "!!"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.9": {"line.1": {"text": "Nein! Nein! Du darfst es nicht sehn.", "tokens": ["Nein", "!", "Nein", "!", "Du", "darfst", "es", "nicht", "sehn", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PTKANT", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "dass die Lippen mir zucken,", "tokens": ["dass", "die", "Lip\u00b7pen", "mir", "zu\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "und auch die Thr\u00e4nen nicht, die ich kindisch um dich vergiesse \u2014", "tokens": ["und", "auch", "die", "Thr\u00e4\u00b7nen", "nicht", ",", "die", "ich", "kin\u00b7disch", "um", "dich", "ver\u00b7gies\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PTKNEG", "$,", "PRELS", "PPER", "ADJD", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+---+-+-+-", "measure": "unknown.measure.hexa"}}, "stanza.10": {"line.1": {"text": "Du armes Weib!", "tokens": ["Du", "ar\u00b7mes", "Weib", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "Also nachts,", "tokens": ["Al\u00b7so", "nachts", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "nachts nur noch wagst du dich,", "tokens": ["nachts", "nur", "noch", "wagst", "du", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "sch\u00fcchtern,", "tokens": ["sch\u00fcch\u00b7tern", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "aus deinem Sarg?", "tokens": ["aus", "dei\u00b7nem", "Sarg", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Um dich auf Zehen zu mir zu schleichen?", "tokens": ["Um", "dich", "auf", "Ze\u00b7hen", "zu", "mir", "zu", "schlei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APPR", "CARD", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Armes Weib!", "tokens": ["Ar\u00b7mes", "Weib", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}