{"textgrid.poem.67195": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Aussicht", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Menschen fr\u00f6hlich sind, ist dieses vom Gem\u00fcte,", "tokens": ["Wenn", "Men\u00b7schen", "fr\u00f6h\u00b7lich", "sind", ",", "ist", "die\u00b7ses", "vom", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "VAFIN", "$,", "VAFIN", "PDAT", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und aus dem Wohlergehn, doch aus dem Felde kommet,", "tokens": ["Und", "aus", "dem", "Woh\u00b7ler\u00b7gehn", ",", "doch", "aus", "dem", "Fel\u00b7de", "kom\u00b7met", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zu schaun der B\u00e4ume Wuchs, die angenehme Bl\u00fcte,", "tokens": ["Zu", "schaun", "der", "B\u00e4u\u00b7me", "Wuchs", ",", "die", "an\u00b7ge\u00b7neh\u00b7me", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da Frucht der Ernte noch den Menschen w\u00e4chst und frommet.", "tokens": ["Da", "Frucht", "der", "Ern\u00b7te", "noch", "den", "Men\u00b7schen", "w\u00e4chst", "und", "from\u00b7met", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gebirg umgibt das Feld, vom Himmel hoch entstehet", "tokens": ["Ge\u00b7birg", "um\u00b7gibt", "das", "Feld", ",", "vom", "Him\u00b7mel", "hoch", "ent\u00b7ste\u00b7het"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "APPRART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die D\u00e4mmerung und Luft, der Ebnen sanfte Wege", "tokens": ["Die", "D\u00e4m\u00b7me\u00b7rung", "und", "Luft", ",", "der", "Eb\u00b7nen", "sanf\u00b7te", "We\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "$,", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sind in den Feldern fern, und \u00fcber Wasser gehet", "tokens": ["Sind", "in", "den", "Fel\u00b7dern", "fern", ",", "und", "\u00fc\u00b7ber", "Was\u00b7ser", "ge\u00b7het"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "PTKVZ", "$,", "KON", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Mensch zu \u00d6rtern dort die k\u00fchn erh\u00f6hten Stege.", "tokens": ["Der", "Mensch", "zu", "\u00d6r\u00b7tern", "dort", "die", "k\u00fchn", "er\u00b7h\u00f6h\u00b7ten", "Ste\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADV", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Erinnerung ist auch dem Menschen in den Worten,", "tokens": ["E\u00b7rin\u00b7ne\u00b7rung", "ist", "auch", "dem", "Men\u00b7schen", "in", "den", "Wor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und der Zusammenhang der Menschen gilt die Tage", "tokens": ["Und", "der", "Zu\u00b7sam\u00b7men\u00b7hang", "der", "Men\u00b7schen", "gilt", "die", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Des Lebens durch zum Guten in den Orten,", "tokens": ["Des", "Le\u00b7bens", "durch", "zum", "Gu\u00b7ten", "in", "den", "Or\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch zu sich selber macht der Mensch des Wissens Frage.", "tokens": ["Doch", "zu", "sich", "sel\u00b7ber", "macht", "der", "Mensch", "des", "Wis\u00b7sens", "Fra\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRF", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Aussicht scheint Ermunterung, der Mensch erfreuet", "tokens": ["Die", "Aus\u00b7sicht", "scheint", "Er\u00b7mun\u00b7te\u00b7rung", ",", "der", "Mensch", "er\u00b7freu\u00b7et"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am Nutzen sich, mit Tagen dann erneuet", "tokens": ["Am", "Nut\u00b7zen", "sich", ",", "mit", "Ta\u00b7gen", "dann", "er\u00b7neu\u00b7et"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PRF", "$,", "APPR", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich sein Gesch\u00e4ft, und um das Gute waltet", "tokens": ["Sich", "sein", "Ge\u00b7sch\u00e4ft", ",", "und", "um", "das", "Gu\u00b7te", "wal\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PRF", "PPOSAT", "NN", "$,", "KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Vorsicht gut, zu Dank, der nicht veraltet.", "tokens": ["Die", "Vor\u00b7sicht", "gut", ",", "zu", "Dank", ",", "der", "nicht", "ver\u00b7al\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "APPR", "NN", "$,", "PRELS", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}