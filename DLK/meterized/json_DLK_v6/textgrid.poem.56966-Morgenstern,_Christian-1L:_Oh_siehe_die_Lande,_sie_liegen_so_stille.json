{"textgrid.poem.56966": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Oh siehe die Lande, sie liegen so stille", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oh siehe die Lande, sie liegen so stille", "tokens": ["Oh", "sie\u00b7he", "die", "Lan\u00b7de", ",", "sie", "lie\u00b7gen", "so", "stil\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ADV", "ADJA"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "und freun sich der sternigen K\u00fchle entgegen,", "tokens": ["und", "freun", "sich", "der", "ster\u00b7ni\u00b7gen", "K\u00fch\u00b7le", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "es rastet der Sonne gewaltiger Wille,", "tokens": ["es", "ras\u00b7tet", "der", "Son\u00b7ne", "ge\u00b7wal\u00b7ti\u00b7ger", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "und leiser wird alles Bewegen und Regen.", "tokens": ["und", "lei\u00b7ser", "wird", "al\u00b7les", "Be\u00b7we\u00b7gen", "und", "Re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Es baut sich die Nacht auf unz\u00e4hligen S\u00e4ulen", "tokens": ["Es", "baut", "sich", "die", "Nacht", "auf", "un\u00b7z\u00e4h\u00b7li\u00b7gen", "S\u00e4u\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "des Lichtes empor \u00fcber schlafenden Fluren,", "tokens": ["des", "Lich\u00b7tes", "em\u00b7por", "\u00fc\u00b7ber", "schla\u00b7fen\u00b7den", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "und langsam veratmen ihr Jauchzen und Heulen", "tokens": ["und", "lang\u00b7sam", "ver\u00b7at\u00b7men", "ihr", "Jauch\u00b7zen", "und", "Heu\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "KON", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "die tr\u00e4umenden Seelen der Kreaturen.", "tokens": ["die", "tr\u00e4u\u00b7men\u00b7den", "See\u00b7len", "der", "Kre\u00b7a\u00b7tu\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}}}}