{"dta.poem.13947": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der brave Schiffer .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Bald m\u00e4chtiger bald leise,", "tokens": ["Bald", "m\u00e4ch\u00b7ti\u00b7ger", "bald", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit wunderbarem Klang,", "tokens": ["Mit", "wun\u00b7der\u00b7ba\u00b7rem", "Klang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zogst Du Gesanges-Kreise,", "tokens": ["Zogst", "Du", "Ge\u00b7san\u00b7ge\u00b7sKrei\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Da\u00df eine tiefe Weise", "tokens": ["Da\u00df", "ei\u00b7ne", "tie\u00b7fe", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das wilde Meer bezwang.", "tokens": ["Das", "wil\u00b7de", "Meer", "be\u00b7zwang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}