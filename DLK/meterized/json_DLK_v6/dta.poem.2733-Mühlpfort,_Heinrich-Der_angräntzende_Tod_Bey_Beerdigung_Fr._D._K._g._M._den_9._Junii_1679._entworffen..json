{"dta.poem.2733": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der angr\u00e4ntzende Tod/  \n Bey Beerdigung Fr. D. K. g. M. den 9.  \n Junii 1679. entworffen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du letzte Nachbarin von diesen Tugend-Frauen/", "tokens": ["Du", "letz\u00b7te", "Nach\u00b7ba\u00b7rin", "von", "die\u00b7sen", "Tu\u00b7gen\u00b7dFrau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So meine Kindheit noch als Mutter hat gekennt/", "tokens": ["So", "mei\u00b7ne", "Kind\u00b7heit", "noch", "als", "Mut\u00b7ter", "hat", "ge\u00b7kennt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "KOUS", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Soll diese schwache Hand dir auch ein Denckmal bauen", "tokens": ["Soll", "die\u00b7se", "schwa\u00b7che", "Hand", "dir", "auch", "ein", "Denck\u00b7mal", "bau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PDAT", "ADJA", "NN", "PPER", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das nicht der Neid verh\u00f6nt/ und keine Zeit zertrennt?", "tokens": ["Das", "nicht", "der", "Neid", "ver\u00b7h\u00f6nt", "/", "und", "kei\u00b7ne", "Zeit", "zer\u00b7trennt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ART", "NN", "VVPP", "$(", "KON", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach ja! indem die Welt umb ihre Gr\u00e4ntzen streitet/", "tokens": ["Ach", "ja", "!", "in\u00b7dem", "die", "Welt", "umb", "ih\u00b7re", "Gr\u00e4nt\u00b7zen", "strei\u00b7tet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und durch der Waffen Recht sie zu erweitern denckt/", "tokens": ["Und", "durch", "der", "Waf\u00b7fen", "Recht", "sie", "zu", "er\u00b7wei\u00b7tern", "denckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "PPER", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Hat dich der blasse Tod so in ein Haus geleitet/", "tokens": ["Hat", "dich", "der", "blas\u00b7se", "Tod", "so", "in", "ein", "Haus", "ge\u00b7lei\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das Raum die F\u00fclle hat und keine Gr\u00e4ntz umbschrenckt.", "tokens": ["Das", "Raum", "die", "F\u00fcl\u00b7le", "hat", "und", "kei\u00b7ne", "Gr\u00e4ntz", "umbschrenckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "KON", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Du tauscht dein gr\u00fcnes Hau\u00df mit einer schwartzen Kammer/", "tokens": ["Du", "tauscht", "dein", "gr\u00fc\u00b7nes", "Hau\u00df", "mit", "ei\u00b7ner", "schwart\u00b7zen", "Kam\u00b7mer", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das kommt zwar der Vernunfft arg und erschrecklich f\u00fcr:", "tokens": ["Das", "kommt", "zwar", "der", "Ver\u00b7nunfft", "arg", "und", "er\u00b7schreck\u00b7lich", "f\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "ADJD", "KON", "ADJD", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Alleine/ wer erw\u00e4gt/ wie aller Schmertz und Jammer", "tokens": ["Al\u00b7lei\u00b7ne", "/", "wer", "er\u00b7w\u00e4gt", "/", "wie", "al\u00b7ler", "Schmertz", "und", "Jam\u00b7mer"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$(", "PWS", "VVFIN", "$(", "KOKOM", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Durch dieses Wechsels Schlu\u00df entschl\u00e4fft zugleich mit dir/", "tokens": ["Durch", "die\u00b7ses", "Wech\u00b7sels", "Schlu\u00df", "ent\u00b7schl\u00e4fft", "zu\u00b7gleich", "mit", "dir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "VVFIN", "ADV", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der heist es wolgethan/ und wird die Welt beklagen/", "tokens": ["Der", "heist", "es", "wol\u00b7ge\u00b7than", "/", "und", "wird", "die", "Welt", "be\u00b7kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VMFIN", "$(", "KON", "VAFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df sie/ die Th\u00f6richte/ von keinen Gr\u00e4ntzen wei\u00df/", "tokens": ["Da\u00df", "sie", "/", "die", "Th\u00f6\u00b7rich\u00b7te", "/", "von", "kei\u00b7nen", "Gr\u00e4nt\u00b7zen", "wei\u00df", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "ART", "NN", "$(", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.15": {"text": "Und l\u00e4st sich bald den Geitz durch wilde Wellen jagen/", "tokens": ["Und", "l\u00e4st", "sich", "bald", "den", "Geitz", "durch", "wil\u00b7de", "Wel\u00b7len", "ja\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Bald Hochmuth f\u00fchren an auff ein gef\u00e4hrlich Ei\u00df;", "tokens": ["Bald", "Hoch\u00b7muth", "f\u00fch\u00b7ren", "an", "auff", "ein", "ge\u00b7f\u00e4hr\u00b7lich", "Ei\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PTKVZ", "APPR", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Da\u00df sie mehr haben will/ als sie vor kan besitzen/", "tokens": ["Da\u00df", "sie", "mehr", "ha\u00b7ben", "will", "/", "als", "sie", "vor", "kan", "be\u00b7sit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAINF", "VMFIN", "$(", "KOUS", "PPER", "APPR", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da\u00df den Begierden auch die Erde viel zu klein.", "tokens": ["Da\u00df", "den", "Be\u00b7gier\u00b7den", "auch", "die", "Er\u00b7de", "viel", "zu", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ART", "NN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da\u00df sie nur Sch\u00e4tze sucht/ die nicht der Seele n\u00fctzen/", "tokens": ["Da\u00df", "sie", "nur", "Sch\u00e4t\u00b7ze", "sucht", "/", "die", "nicht", "der", "See\u00b7le", "n\u00fct\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VVFIN", "$(", "PRELS", "PTKNEG", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und bey der letzten Farth elende Tr\u00f6ster seyn.", "tokens": ["Und", "bey", "der", "letz\u00b7ten", "Farth", "e\u00b7len\u00b7de", "Tr\u00f6s\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.21": {"text": "Da\u00df sie wie Tantalus nach falschen Aepffeln schnappet/", "tokens": ["Da\u00df", "sie", "wie", "Tan\u00b7ta\u00b7lus", "nach", "fal\u00b7schen", "A\u00b7e\u00b7pf\u00b7feln", "schnap\u00b7pet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOKOM", "NE", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.22": {"text": "Da\u00df sie mit einem Sieb aussch\u00f6pffen will die Fluth/", "tokens": ["Da\u00df", "sie", "mit", "ei\u00b7nem", "Sieb", "aus\u00b7sch\u00f6pf\u00b7fen", "will", "die", "Fluth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVINF", "VMFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Gleich Fechtern/ die geblendt/ nur in den finstern tappet/", "tokens": ["Gleich", "Fech\u00b7tern", "/", "die", "ge\u00b7blendt", "/", "nur", "in", "den", "fins\u00b7tern", "tap\u00b7pet", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$(", "ART", "VVPP", "$(", "ADV", "APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und bey dem h\u00f6chsten Witz recht toll und alber thut.", "tokens": ["Und", "bey", "dem", "h\u00f6chs\u00b7ten", "Witz", "recht", "toll", "und", "al\u00b7ber", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADV", "ADJD", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Da doch der grosse G\u00f6tt hat allen Dingen Gr\u00e4ntzen", "tokens": ["Da", "doch", "der", "gros\u00b7se", "G\u00f6tt", "hat", "al\u00b7len", "Din\u00b7gen", "Gr\u00e4nt\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "VAFIN", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "So wol dem weiten Meer/ als auch der Erd er dacht/", "tokens": ["So", "wol", "dem", "wei\u00b7ten", "Meer", "/", "als", "auch", "der", "Erd", "er", "dacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$(", "KOKOM", "ADV", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und uns/ die wir vielmehr nach seinem Bilde gl\u00e4ntzen/", "tokens": ["Und", "uns", "/", "die", "wir", "viel\u00b7mehr", "nach", "sei\u00b7nem", "Bil\u00b7de", "gl\u00e4nt\u00b7zen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "PRELS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Schon ein vollkommen Ma\u00df und richtig Ziel gemacht.", "tokens": ["Schon", "ein", "voll\u00b7kom\u00b7men", "Ma\u00df", "und", "rich\u00b7tig", "Ziel", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "KON", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Di\u00df kan der arme Mensch nicht haarbreit \u00fcberschreiten/", "tokens": ["Di\u00df", "kan", "der", "ar\u00b7me", "Mensch", "nicht", "haar\u00b7breit", "\u00fc\u00b7bersc\u00b7hrei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "ADJA", "NN", "PTKNEG", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und wenn der blasse Tod schon seine Richtsch nur zeucht/", "tokens": ["Und", "wenn", "der", "blas\u00b7se", "Tod", "schon", "sei\u00b7ne", "Richt\u00b7sch", "nur", "zeucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "ADV", "PPOSAT", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.31": {"text": "So zwingt er den zu gehn/ der k\u00fchn wil wieder streiten/", "tokens": ["So", "zwingt", "er", "den", "zu", "gehn", "/", "der", "k\u00fchn", "wil", "wie\u00b7der", "strei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "PTKZU", "VVINF", "$(", "ART", "ADJD", "VMFIN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und der mu\u00df erstlich dran/ der furchtsam ist und fleucht.", "tokens": ["Und", "der", "mu\u00df", "erst\u00b7lich", "dran", "/", "der", "furcht\u00b7sam", "ist", "und", "fleucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VMFIN", "ADJD", "PAV", "$(", "ART", "ADJD", "VAFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Hingegen wenn der Tod besuchet unsre Gr\u00e4ntzen/", "tokens": ["Hin\u00b7ge\u00b7gen", "wenn", "der", "Tod", "be\u00b7su\u00b7chet", "uns\u00b7re", "Gr\u00e4nt\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und man als einen Gast ihn heist willkommen seyn/", "tokens": ["Und", "man", "als", "ei\u00b7nen", "Gast", "ihn", "heist", "will\u00b7kom\u00b7men", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "ART", "NN", "PPER", "VAFIN", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Als Abgesandten ehrt/ der/ unter Sieges-Kr\u00e4ntzen/", "tokens": ["Als", "Ab\u00b7ge\u00b7sand\u00b7ten", "ehrt", "/", "der", "/", "un\u00b7ter", "Sie\u00b7ges\u00b7Kr\u00e4nt\u00b7zen", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$(", "ART", "$(", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Uns in die lichte Burg des Himmels f\u00fchret ein/", "tokens": ["Uns", "in", "die", "lich\u00b7te", "Burg", "des", "Him\u00b7mels", "f\u00fch\u00b7ret", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "So sind wir ja gewi\u00df/ da\u00df wir au\u00df frembder Erden/", "tokens": ["So", "sind", "wir", "ja", "ge\u00b7wi\u00df", "/", "da\u00df", "wir", "au\u00df", "fremb\u00b7der", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "$(", "KOUS", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Au\u00df strenger Dienstbarkeit in g\u00fcldne Freyheit ziehn:", "tokens": ["Au\u00df", "stren\u00b7ger", "Dienst\u00b7bar\u00b7keit", "in", "g\u00fcld\u00b7ne", "Frey\u00b7heit", "ziehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Da\u00df uns f\u00fcr Babylon Jerusalem mu\u00df werden/", "tokens": ["Da\u00df", "uns", "f\u00fcr", "Ba\u00b7by\u00b7lon", "Je\u00b7ru\u00b7sa\u00b7lem", "mu\u00df", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "NE", "VMFIN", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4