{"textgrid.poem.33543": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Nach dem Gefecht", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Himmel bullern nicht mehr die Haubitzen,", "tokens": ["Am", "Him\u00b7mel", "bul\u00b7lern", "nicht", "mehr", "die", "Hau\u00b7bit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Kanoniere ruhn bei den Gesch\u00fctzen.", "tokens": ["Die", "Ka\u00b7no\u00b7nie\u00b7re", "ruhn", "bei", "den", "Ge\u00b7sch\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Infantrie schl\u00e4gt sich jetzt Zelte auf,", "tokens": ["Die", "In\u00b7fant\u00b7rie", "schl\u00e4gt", "sich", "jetzt", "Zel\u00b7te", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und langsam steigt der blasse Mond herauf.", "tokens": ["Und", "lang\u00b7sam", "steigt", "der", "blas\u00b7se", "Mond", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Auf gelben Feldern lohn in roten Hosen,", "tokens": ["Auf", "gel\u00b7ben", "Fel\u00b7dern", "lohn", "in", "ro\u00b7ten", "Ho\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aschfahl von Tod und Pulver, die Franzosen.", "tokens": ["Aschfahl", "von", "Tod", "und", "Pul\u00b7ver", ",", "die", "Fran\u00b7zo\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Dazwischen hocken deutsche Sanit\u00e4ter.", "tokens": ["Da\u00b7zwi\u00b7schen", "ho\u00b7cken", "deut\u00b7sche", "Sa\u00b7ni\u00b7t\u00e4\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Tag wird grauer, seine Sonne r\u00f6ter.", "tokens": ["Der", "Tag", "wird", "grau\u00b7er", ",", "sei\u00b7ne", "Son\u00b7ne", "r\u00f6\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Feldk\u00fcchen dampfen. D\u00f6rfer sind in Brand.", "tokens": ["Feld\u00b7k\u00fc\u00b7chen", "damp\u00b7fen", ".", "D\u00f6r\u00b7fer", "sind", "in", "Brand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$.", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Zerbrochne Karren stehn am Stra\u00dfenrand.", "tokens": ["Zer\u00b7broch\u00b7ne", "Kar\u00b7ren", "stehn", "am", "Stra\u00b7\u00dfen\u00b7rand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Keuchende Radler lungern hei\u00df und braun", "tokens": ["Keu\u00b7chen\u00b7de", "Rad\u00b7ler", "lun\u00b7gern", "hei\u00df", "und", "braun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "An einem angebrannten Bretterzaun.", "tokens": ["An", "ei\u00b7nem", "an\u00b7ge\u00b7brann\u00b7ten", "Bret\u00b7ter\u00b7zaun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und Ordonnanzen reiten schon", "tokens": ["Und", "Or\u00b7don\u00b7nan\u00b7zen", "rei\u00b7ten", "schon"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Regiment zur Division.", "tokens": ["Vom", "Re\u00b7gi\u00b7ment", "zur", "Di\u00b7vi\u00b7si\u00b7on", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}