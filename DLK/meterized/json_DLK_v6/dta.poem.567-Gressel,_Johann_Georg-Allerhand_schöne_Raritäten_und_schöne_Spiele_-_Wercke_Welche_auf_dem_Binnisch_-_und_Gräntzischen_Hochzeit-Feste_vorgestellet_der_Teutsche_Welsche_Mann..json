{"dta.poem.567": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Allerhand sch\u00f6ne Rarit\u00e4ten und sch\u00f6ne  \n Spiele - Wercke/  \n Welche auf dem  \n Binnisch - und Gr\u00e4ntzischen  \n Hochzeit-Feste vorgestellet  \n der Teutsche Welsche Mann.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "So kommt ihr jungen Herren/ ihr Jungfern/ tret\u2019t heran", "tokens": ["So", "kommt", "ihr", "jun\u00b7gen", "Her\u00b7ren", "/", "ihr", "Jung\u00b7fern", "/", "tret't", "he\u00b7ran"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$(", "PPOSAT", "NN", "$(", "VVFIN", "PTKVZ"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Jhr M\u00e4nner/ eilt herbey mit euren lieben Frauen/", "tokens": ["Ihr", "M\u00e4n\u00b7ner", "/", "eilt", "her\u00b7bey", "mit", "eu\u00b7ren", "lie\u00b7ben", "Frau\u00b7en", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gebt achtung/ was ich sag\u2019/ ich bin der Welsche Mann/", "tokens": ["Gebt", "ach\u00b7tung", "/", "was", "ich", "sag'", "/", "ich", "bin", "der", "Wel\u00b7sche", "Mann", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$(", "PWS", "PPER", "VVFIN", "$(", "PPER", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der sch\u00f6ne Rarit\u00e4t l\u00e4\u00dft ohne Geld beschauen.", "tokens": ["Der", "sch\u00f6\u00b7ne", "Ra\u00b7ri\u00b7t\u00e4t", "l\u00e4\u00dft", "oh\u00b7ne", "Geld", "be\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch eine jede Jungfer mu\u00df", "tokens": ["Doch", "ei\u00b7ne", "je\u00b7de", "Jung\u00b7fer", "mu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PIAT", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mir davor geben einen Ku\u00df.", "tokens": ["Mir", "da\u00b7vor", "ge\u00b7ben", "ei\u00b7nen", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PAV", "VVINF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sch\u00f6ne Rarit\u00e4t/ sch\u00f6ne Spiele-Werckerc.", "tokens": ["Sch\u00f6\u00b7ne", "Ra\u00b7ri\u00b7t\u00e4t", "/", "sch\u00f6\u00b7ne", "Spie\u00b7le\u00b7\u00b7Wer\u00b7ckerc", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADJA", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}}}}