{"textgrid.poem.47070": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "67.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ich er\u00f6ffne mit geheimem Schauer", "tokens": ["Wie", "ich", "er\u00b7\u00f6ff\u00b7ne", "mit", "ge\u00b7hei\u00b7mem", "Schau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die mir so lang' entw\u00f6hnte Th\u00fcr und st\u00f6hne,", "tokens": ["Die", "mir", "so", "lang'", "ent\u00b7w\u00f6hn\u00b7te", "Th\u00fcr", "und", "st\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADJA", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00e4llt in die Augen mir sogleich der sch\u00f6ne", "tokens": ["F\u00e4llt", "in", "die", "Au\u00b7gen", "mir", "sog\u00b7leich", "der", "sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kanarienvogel im geflochtnen Bauer.", "tokens": ["Ka\u00b7na\u00b7ri\u00b7en\u00b7vo\u00b7gel", "im", "ge\u00b7flocht\u00b7nen", "Bau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Sein gelbes H\u00e4lschen recket er mit schlauer", "tokens": ["Sein", "gel\u00b7bes", "H\u00e4l\u00b7schen", "re\u00b7cket", "er", "mit", "schlau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bedeutsamkeit, als ob er mich verh\u00f6hne,", "tokens": ["Be\u00b7deut\u00b7sam\u00b7keit", ",", "als", "ob", "er", "mich", "ver\u00b7h\u00f6h\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOKOM", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und singt, als w\u00e4ren's klare Menschent\u00f6ne:", "tokens": ["Und", "singt", ",", "als", "w\u00e4\u00b7ren's", "kla\u00b7re", "Men\u00b7schen\u00b7t\u00f6\u00b7ne", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOKOM", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbso lang ist also dieser Freiheit Dauer?", "tokens": ["\u00bb", "so", "lang", "ist", "al\u00b7so", "die\u00b7ser", "Frei\u00b7heit", "Dau\u00b7er", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "VAFIN", "ADV", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Klug ist der Vogel, und der Mensch ist th\u00f6richt,", "tokens": ["Klug", "ist", "der", "Vo\u00b7gel", ",", "und", "der", "Mensch", "ist", "th\u00f6\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NE", "$,", "KON", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich hab' in der Gef\u00e4ngnis Lustgemache", "tokens": ["Ich", "hab'", "in", "der", "Ge\u00b7f\u00e4ng\u00b7nis", "Lust\u00b7ge\u00b7ma\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Indes hier fortgesungen meine Lieder,", "tokens": ["In\u00b7des", "hier", "fort\u00b7ge\u00b7sun\u00b7gen", "mei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Als s\u00e4\u00df' ich frei in Teneriffas R\u00f6hricht;", "tokens": ["Als", "s\u00e4\u00df'", "ich", "frei", "in", "Te\u00b7ne\u00b7rif\u00b7fas", "R\u00f6h\u00b7richt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADJD", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du bist der Haft entflohn mit einem Ache", "tokens": ["Du", "bist", "der", "Haft", "ent\u00b7flohn", "mit", "ei\u00b7nem", "A\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und kehrst zur Haft mit einem Ach jetzt wieder.\u00ab", "tokens": ["Und", "kehrst", "zur", "Haft", "mit", "ei\u00b7nem", "Ach", "jetzt", "wie\u00b7der", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}