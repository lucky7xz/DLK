{"textgrid.poem.42473": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im s\u00fcdwestafrikanischen Land,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im s\u00fcdwestafrikanischen Land,", "tokens": ["Im", "s\u00fcd\u00b7west\u00b7af\u00b7ri\u00b7ka\u00b7ni\u00b7schen", "Land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Bei Kalkfontein, im Aubgebiet,", "tokens": ["Bei", "Kalk\u00b7fon\u00b7tein", ",", "im", "Aub\u00b7ge\u00b7biet", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPRART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Liegt im ewig sengenden Sonnenbrand", "tokens": ["Liegt", "im", "e\u00b7wig", "sen\u00b7gen\u00b7den", "Son\u00b7nen\u00b7brand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJD", "ADJA", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Ein k\u00fchler Kolk zwischen R\u00f6hricht und Ried.", "tokens": ["Ein", "k\u00fch\u00b7ler", "Kolk", "zwi\u00b7schen", "R\u00f6h\u00b7richt", "und", "Ried", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Es singen die Quellen, sie bieten den Gru\u00df:", "tokens": ["Es", "sin\u00b7gen", "die", "Quel\u00b7len", ",", "sie", "bie\u00b7ten", "den", "Gru\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Trinkt! Trinkt! und netzt euch den staubm\u00fcden Fu\u00df", "tokens": ["Trinkt", "!", "Trinkt", "!", "und", "netzt", "euch", "den", "staub\u00b7m\u00fc\u00b7den", "Fu\u00df"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "VVFIN", "$.", "KON", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "An der klaren, frischen Wasserstelle.", "tokens": ["An", "der", "kla\u00b7ren", ",", "fri\u00b7schen", "Was\u00b7ser\u00b7stel\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wasser! Die Witbois halten es fest;", "tokens": ["Was\u00b7ser", "!", "Die", "Wit\u00b7bois", "hal\u00b7ten", "es", "fest", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Um den Trunk tobt seit drei Tagen der Tod.", "tokens": ["Um", "den", "Trunk", "tobt", "seit", "drei", "Ta\u00b7gen", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVFIN", "APPR", "CARD", "NN", "ART", "NN", "$."], "meter": "--+--++--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "\u00bbwasser! Dann mag mich fressen die Pest!", "tokens": ["\u00bb", "was\u00b7ser", "!", "Dann", "mag", "mich", "fres\u00b7sen", "die", "Pest", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "$.", "ADV", "VMFIN", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Nur einen Tropfen in letzter Not!\u00ab", "tokens": ["Nur", "ei\u00b7nen", "Trop\u00b7fen", "in", "letz\u00b7ter", "Not", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Es plappern die Wellchen kokett und kalt,", "tokens": ["Es", "plap\u00b7pern", "die", "Well\u00b7chen", "ko\u00b7kett", "und", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Sie pl\u00e4tschern und plauschen: kommt bald, kommt bald", "tokens": ["Sie", "pl\u00e4t\u00b7schern", "und", "plau\u00b7schen", ":", "kommt", "bald", ",", "kommt", "bald"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$.", "VVFIN", "ADV", "$,", "VVFIN", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "An die klare, frische Wasserstelle!", "tokens": ["An", "die", "kla\u00b7re", ",", "fri\u00b7sche", "Was\u00b7ser\u00b7stel\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Vier Tage! Wir st\u00fcrmen zum f\u00fcnften Mal,", "tokens": ["Vier", "Ta\u00b7ge", "!", "Wir", "st\u00fcr\u00b7men", "zum", "f\u00fcnf\u00b7ten", "Mal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$.", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und w\u00e4re das Labsal von Teufeln umringt.", "tokens": ["Und", "w\u00e4\u00b7re", "das", "Lab\u00b7sal", "von", "Teu\u00b7feln", "um\u00b7ringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wasser! Wann endlich endet die Qual!", "tokens": ["Was\u00b7ser", "!", "Wann", "end\u00b7lich", "en\u00b7det", "die", "Qual", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Noch einmal gest\u00fcrmt! Es gelingt, es gelingt!", "tokens": ["Noch", "ein\u00b7mal", "ge\u00b7st\u00fcrmt", "!", "Es", "ge\u00b7lingt", ",", "es", "ge\u00b7lingt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Wie in der Heimat durch Wald und Feld", "tokens": ["Wie", "in", "der", "Hei\u00b7mat", "durch", "Wald", "und", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sprudelt das B\u00e4chlein, o selige Welt,", "tokens": ["Spru\u00b7delt", "das", "B\u00e4ch\u00b7lein", ",", "o", "se\u00b7li\u00b7ge", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "FM", "ADJA", "NN", "$,"], "meter": "+--+-+---+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "An der klaren, frischen Wasserstelle.", "tokens": ["An", "der", "kla\u00b7ren", ",", "fri\u00b7schen", "Was\u00b7ser\u00b7stel\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Umsonst! Nun liegen wir m\u00fcrb und matt,", "tokens": ["Um\u00b7sonst", "!", "Nun", "lie\u00b7gen", "wir", "m\u00fcrb", "und", "matt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verdurstend, die Lippen sind rissig und wund,", "tokens": ["Ver\u00b7durs\u00b7tend", ",", "die", "Lip\u00b7pen", "sind", "ris\u00b7sig", "und", "wund", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Der Wahnsinn h\u00e4lt uns am Boden platt,", "tokens": ["Der", "Wahn\u00b7sinn", "h\u00e4lt", "uns", "am", "Bo\u00b7den", "platt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gl\u00fchhei\u00df ist der Stein dem saugenden Mund.", "tokens": ["Gl\u00fch\u00b7hei\u00df", "ist", "der", "Stein", "dem", "sau\u00b7gen\u00b7den", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Die Nixen winken: Bei uns ist es k\u00fchl,", "tokens": ["Die", "Ni\u00b7xen", "win\u00b7ken", ":", "Bei", "uns", "ist", "es", "k\u00fchl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "APPR", "PPER", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Kommt, badet mit uns im heitern Gesp\u00fcl", "tokens": ["Kommt", ",", "ba\u00b7det", "mit", "uns", "im", "hei\u00b7tern", "Ge\u00b7sp\u00fcl"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+----+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Der klaren, frischen Wasserstelle.", "tokens": ["Der", "kla\u00b7ren", ",", "fri\u00b7schen", "Was\u00b7ser\u00b7stel\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbwasser! Wasser! Nur einen Schluck!\u00ab", "tokens": ["\u00bb", "was\u00b7ser", "!", "Was\u00b7ser", "!", "Nur", "ei\u00b7nen", "Schluck", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "$.", "NN", "$.", "ADV", "ART", "NN", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Einer ruft heilig, schon wirr ist sein Sinn,", "tokens": ["Ei\u00b7ner", "ruft", "hei\u00b7lig", ",", "schon", "wirr", "ist", "sein", "Sinn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJD", "$,", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Das W\u00e4sserchen dr\u00fcben \u00e4fft gluckgluckgluck:", "tokens": ["Das", "W\u00e4s\u00b7ser\u00b7chen", "dr\u00fc\u00b7ben", "\u00e4fft", "gluck\u00b7gluck\u00b7gluck", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "\u00bbgott f\u00fchret zum frischen Wasser mich hin.\u00ab", "tokens": ["\u00bb", "gott", "f\u00fch\u00b7ret", "zum", "fri\u00b7schen", "Was\u00b7ser", "mich", "hin", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "VVFIN", "APPRART", "ADJA", "NN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Das Wellchen schwatzt weiter und kichert und lacht", "tokens": ["Das", "Well\u00b7chen", "schwatzt", "wei\u00b7ter", "und", "ki\u00b7chert", "und", "lacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und hat seine windigen Scherze gemacht", "tokens": ["Und", "hat", "sei\u00b7ne", "win\u00b7di\u00b7gen", "Scher\u00b7ze", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Auf der klaren, frischen Wasserstelle.", "tokens": ["Auf", "der", "kla\u00b7ren", ",", "fri\u00b7schen", "Was\u00b7ser\u00b7stel\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "In der Batterie herrscht Gr\u00e4berruh,", "tokens": ["In", "der", "Bat\u00b7te\u00b7rie", "herrscht", "Gr\u00e4\u00b7ber\u00b7ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Offiziere und Mannschaft sind zermetzt;", "tokens": ["Of\u00b7fi\u00b7zie\u00b7re", "und", "Mann\u00b7schaft", "sind", "zer\u00b7metzt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Kein Schu\u00df mehr, Hans Klapperbein schmunzelt dazu,", "tokens": ["Kein", "Schu\u00df", "mehr", ",", "Hans", "Klap\u00b7per\u00b7bein", "schmun\u00b7zelt", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "$,", "NE", "NE", "VVFIN", "PAV", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Gefallen fast Alles und zerfetzt.", "tokens": ["Ge\u00b7fal\u00b7len", "fast", "Al\u00b7les", "und", "zer\u00b7fetzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIS", "KON", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und dr\u00fcben das Teichlein l\u00e4dt ungest\u00fcm ein:", "tokens": ["Und", "dr\u00fc\u00b7ben", "das", "Teich\u00b7lein", "l\u00e4dt", "un\u00b7ge\u00b7st\u00fcm", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Trinkt doch und wascht euch die Wunden rein", "tokens": ["Trinkt", "doch", "und", "wascht", "euch", "die", "Wun\u00b7den", "rein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "KON", "VVFIN", "PPER", "ART", "NN", "ADJD"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "An der klaren, frischen Wasserstelle.", "tokens": ["An", "der", "kla\u00b7ren", ",", "fri\u00b7schen", "Was\u00b7ser\u00b7stel\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Getroffen im Unterleib, \u00e4chzt der Major,", "tokens": ["Ge\u00b7trof\u00b7fen", "im", "Un\u00b7ter\u00b7leib", ",", "\u00e4chzt", "der", "Ma\u00b7jor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "VVFIN", "ART", "NE", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In der furchtbaren Hitze, drei Tage lang.", "tokens": ["In", "der", "furcht\u00b7ba\u00b7ren", "Hit\u00b7ze", ",", "drei", "Ta\u00b7ge", "lang", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "CARD", "NN", "ADJD", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Kein Arzt. Er rafft sich vergebens empor:", "tokens": ["Kein", "Arzt", ".", "Er", "rafft", "sich", "ver\u00b7ge\u00b7bens", "em\u00b7por", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "PPER", "VVFIN", "PRF", "ADV", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00bbwasser!\u00ab Er h\u00f6rt nur H\u00f6llengesang.", "tokens": ["\u00bb", "was\u00b7ser", "!", "\u00ab", "Er", "h\u00f6rt", "nur", "H\u00f6l\u00b7len\u00b7ge\u00b7sang", "."], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "$.", "$(", "PPER", "VVFIN", "ADV", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Durch Tag und Nacht h\u00f6hnt das Quellengegluck:", "tokens": ["Durch", "Tag", "und", "Nacht", "h\u00f6hnt", "das", "Quel\u00b7len\u00b7ge\u00b7gluck", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+--+", "measur