{"textgrid.poem.38764": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich wu\u00dfte nicht wie mir geschah", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wu\u00dfte nicht wie mir geschah", "tokens": ["Ich", "wu\u00df\u00b7te", "nicht", "wie", "mir", "ge\u00b7schah"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWAV", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als von dem Busch ein Bl\u00e4ttchen th\u00e4t ausscheinen,", "tokens": ["Als", "von", "dem", "Busch", "ein", "Bl\u00e4tt\u00b7chen", "th\u00e4t", "aus\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich mu\u00dfte weinen", "tokens": ["Ich", "mu\u00df\u00b7te", "wei\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Als ich das erste Gr\u00fcn ersah:", "tokens": ["Als", "ich", "das", "ers\u00b7te", "Gr\u00fcn", "er\u00b7sah", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie mu\u00dft du ohne dein Verschulden", "tokens": ["Wie", "mu\u00dft", "du", "oh\u00b7ne", "dein", "Ver\u00b7schul\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den b\u00f6sen Frost, die kalten N\u00e4chte dulden?", "tokens": ["Den", "b\u00f6\u00b7sen", "Frost", ",", "die", "kal\u00b7ten", "N\u00e4ch\u00b7te", "dul\u00b7den", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Du meinst es treu und gut,", "tokens": ["Du", "meinst", "es", "treu", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Du armes Blut,", "tokens": ["Du", "ar\u00b7mes", "Blut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Und mu\u00dft an deiner Lieb und Treu verscheiden;", "tokens": ["Und", "mu\u00dft", "an", "dei\u00b7ner", "Lieb", "und", "Treu", "ver\u00b7schei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Du blickst umher mit Liebes-Augen,", "tokens": ["Du", "blickst", "um\u00b7her", "mit", "Lie\u00b7bes\u00b7Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Den warmen Schimmer einzusaugen,", "tokens": ["Den", "war\u00b7men", "Schim\u00b7mer", "ein\u00b7zu\u00b7sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Ach! dich wird noch die Fr\u00fchlingssonne meiden. \u2013", "tokens": ["Ach", "!", "dich", "wird", "noch", "die", "Fr\u00fch\u00b7lings\u00b7son\u00b7ne", "mei\u00b7den", ".", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "ADV", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "So ist Blicken,", "tokens": ["So", "ist", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "H\u00e4ndedr\u00fccken,", "tokens": ["H\u00e4n\u00b7de\u00b7dr\u00fc\u00b7cken", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Weit von mir.", "tokens": ["Weit", "von", "mir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.16": {"text": "Fr\u00fchlings-Herold, ach! es geht mir so wie dir!", "tokens": ["Fr\u00fch\u00b7lings\u00b7He\u00b7rold", ",", "ach", "!", "es", "geht", "mir", "so", "wie", "dir", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ITJ", "$.", "PPER", "VVFIN", "PPER", "ADV", "KOKOM", "PPER", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Bleibt, ihr freundlichen Kinder, zur\u00fccke!", "tokens": ["Bleibt", ",", "ihr", "freund\u00b7li\u00b7chen", "Kin\u00b7der", ",", "zu\u00b7r\u00fc\u00b7cke", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Bleib in der Erde, du gr\u00fcnes Gras!", "tokens": ["Bleib", "in", "der", "Er\u00b7de", ",", "du", "gr\u00fc\u00b7nes", "Gras", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ihr suchet hier oben ein unbekannt Gl\u00fccke,", "tokens": ["Ihr", "su\u00b7chet", "hier", "o\u00b7ben", "ein", "un\u00b7be\u00b7kannt", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ihr suchet die Liebe und findet wohl Ha\u00df.", "tokens": ["Ihr", "su\u00b7chet", "die", "Lie\u00b7be", "und", "fin\u00b7det", "wohl", "Ha\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "ADV", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Aber keiner h\u00f6rt mein Rathen,", "tokens": ["A\u00b7ber", "kei\u00b7ner", "h\u00f6rt", "mein", "Ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Keiner h\u00f6rt mein bittend Flehen,", "tokens": ["Kei\u00b7ner", "h\u00f6rt", "mein", "bit\u00b7tend", "Fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sie k\u00fcmmert's nicht was ihnen mag geschehen,", "tokens": ["Sie", "k\u00fcm\u00b7mert's", "nicht", "was", "ih\u00b7nen", "mag", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sie eilen nur zu den gewohnten Thaten.", "tokens": ["Sie", "ei\u00b7len", "nur", "zu", "den", "ge\u00b7wohn\u00b7ten", "Tha\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Denn die Knospen schon anschwollen,", "tokens": ["Denn", "die", "Knos\u00b7pen", "schon", "an\u00b7schwol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Durch die B\u00e4ume zog die Luft,", "tokens": ["Durch", "die", "B\u00e4u\u00b7me", "zog", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Gr\u00fcner stets und gr\u00fcner ward der Wald,", "tokens": ["Gr\u00fc\u00b7ner", "stets", "und", "gr\u00fc\u00b7ner", "ward", "der", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Das Lied der wandernden Sommerv\u00f6gel schallt,", "tokens": ["Das", "Lied", "der", "wan\u00b7dern\u00b7den", "Som\u00b7mer\u00b7v\u00f6\u00b7gel", "schallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Von dem Apfelbaum in vollen", "tokens": ["Von", "dem", "Ap\u00b7fel\u00b7baum", "in", "vol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Sternen h\u00e4ngt die Bluth und athmet s\u00fc\u00dfen Duft:", "tokens": ["Ster\u00b7nen", "h\u00e4ngt", "die", "Bluth", "und", "ath\u00b7met", "s\u00fc\u00b7\u00dfen", "Duft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "KON", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.15": {"text": "Und das Fr\u00fchlings-Gesinde,", "tokens": ["Und", "das", "Fr\u00fch\u00b7lings\u00b7Ge\u00b7sin\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.16": {"text": "Die spielenden Winde,", "tokens": ["Die", "spie\u00b7len\u00b7den", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Die Schmetterlinge", "tokens": ["Die", "Schmet\u00b7ter\u00b7lin\u00b7ge"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Mit Farbenglanz auf wiegender Schwinge,", "tokens": ["Mit", "Far\u00b7ben\u00b7glanz", "auf", "wie\u00b7gen\u00b7der", "Schwin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Alles, alles, ist zur\u00fcckgekommen,", "tokens": ["Al\u00b7les", ",", "al\u00b7les", ",", "ist", "zu\u00b7r\u00fcck\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "$,", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Die Wogen, die den Bach hinabgeschwommen,", "tokens": ["Die", "Wo\u00b7gen", ",", "die", "den", "Bach", "hin\u00b7ab\u00b7ge\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Lassen keimen schon die Liebes-Augen kleine,", "tokens": ["Las\u00b7sen", "kei\u00b7men", "schon", "die", "Lie\u00b7bes\u00b7Au\u00b7gen", "klei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN", "ADJA", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.22": {"text": "Die mit dem blauen Scheine", "tokens": ["Die", "mit", "dem", "blau\u00b7en", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Die Fluth sehns\u00fcchtig gr\u00fcssen.", "tokens": ["Die", "Fluth", "sehn\u00b7s\u00fcch\u00b7tig", "gr\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Ach! alle Kinder sprie\u00dfen,", "tokens": ["Ach", "!", "al\u00b7le", "Kin\u00b7der", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Mit denen Sommer spielet,", "tokens": ["Mit", "de\u00b7nen", "Som\u00b7mer", "spie\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Es leuchten alle Gartensterne,", "tokens": ["Es", "leuch\u00b7ten", "al\u00b7le", "Gar\u00b7tens\u00b7ter\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Nur sie ist ferne", "tokens": ["Nur", "sie", "ist", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.28": {"text": "Und wei\u00df es nicht wie einsam sich mein Herz hier f\u00fchlet.", "tokens": ["Und", "wei\u00df", "es", "nicht", "wie", "ein\u00b7sam", "sich", "mein", "Herz", "hier", "f\u00fch\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "KOKOM", "ADJD", "PRF", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "O gr\u00fcnes Laub, o dunkler Wald,", "tokens": ["O", "gr\u00fc\u00b7nes", "Laub", ",", "o", "dunk\u00b7ler", "Wald", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich sehe nirgend ihre Gestalt!", "tokens": ["Ich", "se\u00b7he", "nir\u00b7gend", "ih\u00b7re", "Ge\u00b7stalt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "W\u00e4rt Bl\u00fcmlein ihr zur\u00fcckgeblieben!", "tokens": ["W\u00e4rt", "Bl\u00fcm\u00b7lein", "ihr", "zu\u00b7r\u00fcck\u00b7ge\u00b7blie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr mehret nur die Trauer mein,", "tokens": ["Ihr", "meh\u00b7ret", "nur", "die", "Trau\u00b7er", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie ich sonst euren zarten Schein", "tokens": ["Wie", "ich", "sonst", "eu\u00b7ren", "zar\u00b7ten", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von ganzem Herzen mu\u00dfte lieben.", "tokens": ["Von", "gan\u00b7zem", "Her\u00b7zen", "mu\u00df\u00b7te", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Du Vogelsang,", "tokens": ["Du", "Vo\u00b7gel\u00b7sang", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Du B\u00e4chleinklang,", "tokens": ["Du", "B\u00e4ch\u00b7lein\u00b7klang", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Ihr lacht und k\u00fcmmert euch um mich nicht mehr.", "tokens": ["Ihr", "lacht", "und", "k\u00fcm\u00b7mert", "euch", "um", "mich", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "APPR", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ich schau umher:", "tokens": ["Ich", "schau", "um\u00b7her", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Was willst du in dem bunten Kleide,", "tokens": ["Was", "willst", "du", "in", "dem", "bun\u00b7ten", "Klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Du Sommer