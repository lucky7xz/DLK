"}, "line.10": {"text": "Im Herzen gar zu wehe.", "tokens": ["Im", "Her\u00b7zen", "gar", "zu", "we\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Im Schmerz h\u00e4lt es treu,", "tokens": ["Im", "Schmerz", "h\u00e4lt", "es", "treu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Mag Fr\u00fchling kommen oder Winter kalt,", "tokens": ["Mag", "Fr\u00fch\u00b7ling", "kom\u00b7men", "o\u00b7der", "Win\u00b7ter", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "VVINF", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Stets bl\u00fchet neu", "tokens": ["Stets", "bl\u00fc\u00b7het", "neu"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und wandelt vor mir hin die lieblichste Gestalt,", "tokens": ["Und", "wan\u00b7delt", "vor", "mir", "hin", "die", "lieb\u00b7lichs\u00b7te", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie ist ein Sommer der nie schwindet,", "tokens": ["Sie", "ist", "ein", "Som\u00b7mer", "der", "nie", "schwin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Blumenglanz der nie erblindet:", "tokens": ["Ein", "Blu\u00b7men\u00b7glanz", "de