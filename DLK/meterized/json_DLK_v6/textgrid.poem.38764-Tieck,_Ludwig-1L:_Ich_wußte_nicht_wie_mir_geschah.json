": "iambic.penta"}, "line.12": {"text": "Dann m\u00f6gen Rosen welken oder bl\u00fchen,", "tokens": ["Dann", "m\u00f6\u00b7gen", "Ro\u00b7sen", "wel\u00b7ken", "o\u00b7der", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Der Fr\u00fchling kommen oder fliehen,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "kom\u00b7men", "o\u00b7der", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Ich will nicht klagen", "tokens": ["Ich", "will", "nicht", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Will diesen Sommer auch die Nachtigall nicht schlagen.", "tokens": ["Will", "die\u00b7sen", "Som\u00b7mer", "auch", "die", "Nach\u00b7ti\u00b7gall", "nicht", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "NN", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}