{"textgrid.poem.30299": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Kraut dient f\u00fcr das t\u00f6dten;", "tokens": ["Kein", "Kraut", "dient", "f\u00fcr", "das", "t\u00f6d\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nein, sagen die Poeten:", "tokens": ["Nein", ",", "sa\u00b7gen", "die", "Po\u00b7et\u00b7en", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein Blat von unsrem Krantze", "tokens": ["Ein", "Blat", "von", "uns\u00b7rem", "Krant\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der frischen Lorber-Pflantze,", "tokens": ["Der", "fri\u00b7schen", "Lor\u00b7ber\u00b7Pflant\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Erw\u00e4rmt von unsrer Stirne,", "tokens": ["Er\u00b7w\u00e4rmt", "von", "uns\u00b7rer", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Begeistert vom Gehirne,", "tokens": ["Be\u00b7geis\u00b7tert", "vom", "Ge\u00b7hir\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Gibt Balsam zum genesen", "tokens": ["Gibt", "Bal\u00b7sam", "zum", "ge\u00b7ne\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NE", "APPRART", "VVINF"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.8": {"text": "Und trotzet das verwesen.", "tokens": ["Und", "trot\u00b7zet", "das", "ver\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Nicht anders, ihr Poeten!", "tokens": ["Nicht", "an\u00b7ders", ",", "ihr", "Po\u00b7et\u00b7en", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Der Tod kan keinen n\u00f6then,", "tokens": ["Der", "Tod", "kan", "kei\u00b7nen", "n\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Den ihr und eure Sinnen", "tokens": ["Den", "ihr", "und", "eu\u00b7re", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Nicht lassen wolt von hinnen.", "tokens": ["Nicht", "las\u00b7sen", "wolt", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "APPR", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Die alten, k\u00fchnen Degen", "tokens": ["Die", "al\u00b7ten", ",", "k\u00fch\u00b7nen", "De\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Gehn noch auff unsren Wegen,", "tokens": ["Gehn", "noch", "auff", "un\u00b7sren", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Die ihrer Druden Lieder", "tokens": ["Die", "ih\u00b7rer", "Dru\u00b7den", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Nicht liessen kummen nieder.", "tokens": ["Nicht", "lies\u00b7sen", "kum\u00b7men", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Was w\u00fcsten wir von Helden", "tokens": ["Was", "w\u00fcs\u00b7ten", "wir", "von", "Hel\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Und ihrer Thurst zu melden,", "tokens": ["Und", "ih\u00b7rer", "Thurst", "zu", "mel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Wann nicht Poeten-Geister,", "tokens": ["Wann", "nicht", "Po\u00b7e\u00b7ten\u00b7Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "De\u00df schwartzen Grabes Meister,", "tokens": ["De\u00df", "schwart\u00b7zen", "Gra\u00b7bes", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Die Sterbligkeit verb\u00fcrget,", "tokens": ["Die", "Ster\u00b7blig\u00b7keit", "ver\u00b7b\u00fcr\u00b7get", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Da\u00df sie sie nicht gew\u00fcrget?", "tokens": ["Da\u00df", "sie", "sie", "nicht", "ge\u00b7w\u00fcr\u00b7get", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Was w\u00e4r von tapffren Thaten,", "tokens": ["Was", "w\u00e4r", "von", "tapf\u00b7fren", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Was w\u00e4r von klugem rathen", "tokens": ["Was", "w\u00e4r", "von", "klu\u00b7gem", "ra\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Der Nachwelt k\u00fcndig blieben,", "tokens": ["Der", "Nach\u00b7welt", "k\u00fcn\u00b7dig", "blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Wann diese nicht geschrieben?", "tokens": ["Wann", "die\u00b7se", "nicht", "ge\u00b7schrie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Es macht poetisch Tichten,", "tokens": ["Es", "macht", "po\u00b7e\u00b7tisch", "Tich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Da\u00df alles bleibt im Liechten;", "tokens": ["Da\u00df", "al\u00b7les", "bleibt", "im", "Liech\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Sonst fiel in lauter N\u00e4chte,", "tokens": ["Sonst", "fiel", "in", "lau\u00b7ter", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Was Hertz und Witz verbr\u00e4chte.", "tokens": ["Was", "Hertz", "und", "Witz", "ver\u00b7br\u00e4ch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Es sind zwar mehr der Kielen,", "tokens": ["Es", "sind", "zwar", "mehr", "der", "Kie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Die auff da\u00df ferne spielen,", "tokens": ["Die", "auff", "da\u00df", "fer\u00b7ne", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "KOUS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Die hin nach Ewigkeiten", "tokens": ["Die", "hin", "nach", "E\u00b7wig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Gleichwol die Fahrt bereiten;", "tokens": ["Gleich\u00b7wol", "die", "Fahrt", "be\u00b7rei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Doch d\u00fcnckt mich, da\u00df Poeten", "tokens": ["Doch", "d\u00fcnckt", "mich", ",", "da\u00df", "Po\u00b7et\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "NN"], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.36": {"text": "Noch mehr als andre r\u00f6then,", "tokens": ["Noch", "mehr", "als", "and\u00b7re", "r\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Was Todten-Asche blasset.", "tokens": ["Was", "Tod\u00b7ten\u00b7A\u00b7sche", "blas\u00b7set", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$."], "meter": "-++-+-", "measure": "unknown.measure.tri"}, "line.38": {"text": "Ihr Thun ist so gefasset,", "tokens": ["Ihr", "Thun", "ist", "so", "ge\u00b7fas\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Da\u00df ihre s\u00fcsse Sachen", "tokens": ["Da\u00df", "ih\u00b7re", "s\u00fcs\u00b7se", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "Viel Buler ihnen machen;", "tokens": ["Viel", "Bu\u00b7ler", "ih\u00b7nen", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.41": {"text": "Da\u00df ihre Zierligkeiten", "tokens": ["Da\u00df", "ih\u00b7re", "Zier\u00b7lig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "Die Sinnen m\u00e4chtig leiten.", "tokens": ["Die", "Sin\u00b7nen", "m\u00e4ch\u00b7tig", "lei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.43": {"text": "Sie zuckern alle Worte;", "tokens": ["Sie", "zu\u00b7ckern", "al\u00b7le", "Wor\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "Es bl\u00fcht an allem Orte;", "tokens": ["Es", "bl\u00fcht", "an", "al\u00b7lem", "Or\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIS", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.45": {"text": "Sie schreiben nicht, sie mahlen.", "tokens": ["Sie", "schrei\u00b7ben", "nicht", ",", "sie", "mah\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.46": {"text": "Die ungez\u00e4lten Zahlen", "tokens": ["Die", "un\u00b7ge\u00b7z\u00e4l\u00b7ten", "Zah\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.47": {"text": "Der andren K\u00fcnstligkeiten,", "tokens": ["Der", "an\u00b7dren", "K\u00fcnst\u00b7lig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.48": {"text": "Die k\u00fcnnen so bereiten", "tokens": ["Die", "k\u00fcn\u00b7nen", "so", "be\u00b7rei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.49": {"text": "Gem\u00fcther zum verlieben,", "tokens": ["Ge\u00b7m\u00fc\u00b7ther", "zum", "ver\u00b7lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.50": {"text": "Da\u00df sie stets ihrer blieben,", "tokens": ["Da\u00df", "sie", "stets", "ih\u00b7rer", "blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.51": {"text": "Und die, die ihre bleiben,", "tokens": ["Und", "die", ",", "die", "ih\u00b7re", "blei\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.52": {"text": "Die k\u00fcnnen sich denn schreiben", "tokens": ["Die", "k\u00fcn\u00b7nen", "sich", "denn", "schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "PRF", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.53": {"text": "F\u00fcr Freunde derer Leute,", "tokens": ["F\u00fcr", "Freun\u00b7de", "de\u00b7rer", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PDS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.54": {"text": "Dran Zeit hat keine Beuthe.", "tokens": ["Dran", "Zeit", "hat", "kei\u00b7ne", "Beu\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.55": {"text": "Wie dein Poete singet", "tokens": ["Wie", "dein", "Poe\u00b7te", "sin\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.56": {"text": "Und mit dem Alter dinget,", "tokens": ["Und", "mit", "dem", "Al\u00b7ter", "din\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.57": {"text": "Dich, Brieg, und die darinnen", "tokens": ["Dich", ",", "Brieg", ",", "und", "die", "da\u00b7rin\u00b7nen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "$,", "KON", "ART", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.58": {"text": "Vom sterben zu gewinnen,", "tokens": ["Vom", "ster\u00b7ben", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.59": {"text": "Das zeugen seine Lieder.", "tokens": ["Das", "zeu\u00b7gen", "sei\u00b7ne", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.60": {"text": "Was sonsten hin und wieder", "tokens": ["Was", "sons\u00b7ten", "hin", "und", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.61": {"text": "Er k\u00fcnstlich, artlich spielet,", "tokens": ["Er", "k\u00fcnst\u00b7lich", ",", "art\u00b7lich", "spie\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.62": {"text": "Da\u00df Lust und Nutz man f\u00fchlet:", "tokens": ["Da\u00df", "Lust", "und", "Nutz", "man", "f\u00fch\u00b7let", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.63": {"text": "Di\u00df kan gen\u00fcglich zeigen,", "tokens": ["Di\u00df", "kan", "ge\u00b7n\u00fcg\u00b7lich", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.64": {"text": "Wie hoch Poeten steigen.", "tokens": ["Wie", "hoch", "Po\u00b7et\u00b7en", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.65": {"text": "Brieg, ehre di\u00df Beginnen,", "tokens": ["Brieg", ",", "eh\u00b7re", "di\u00df", "Be\u00b7gin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PDS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.66": {"text": "Wilstu nach dir seyn k\u00fcnnen!", "tokens": ["Wils\u00b7tu", "nach", "dir", "seyn", "k\u00fcn\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.67": {"text": "Zwar k\u00fcnnen ihr Ger\u00fcchte", "tokens": ["Zwar", "k\u00fcn\u00b7nen", "ihr", "Ge\u00b7r\u00fcch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.68": {"text": "Durch eigenes Gewichte", "tokens": ["Durch", "ei\u00b7ge\u00b7nes", "Ge\u00b7wich\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.69": {"text": "Verewigen die Tichter,", "tokens": ["Ve\u00b7re\u00b7wi\u00b7gen", "die", "Tich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.70": {"text": "Doch durch bewogne Richter,", "tokens": ["Doch", "durch", "be\u00b7wog\u00b7ne", "Rich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.71": {"text": "Die ihnen hold und g\u00fcnstig,", "tokens": ["Die", "ih\u00b7nen", "hold", "und", "g\u00fcns\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.72": {"text": "So wird ihr Trieb mehr br\u00fcnstig,", "tokens": ["So", "wird", "ihr", "Trieb", "mehr", "br\u00fcns\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.73": {"text": "Da\u00df sich sie und die Ihren", "tokens": ["Da\u00df", "sich", "sie", "und", "die", "Ih\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PPER", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.74": {"text": "Bi\u00df gar an Himmel f\u00fchren.", "tokens": ["Bi\u00df", "gar", "an", "Him\u00b7mel", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}