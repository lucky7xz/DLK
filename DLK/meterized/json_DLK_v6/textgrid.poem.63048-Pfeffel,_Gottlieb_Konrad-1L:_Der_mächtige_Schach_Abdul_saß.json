{"textgrid.poem.63048": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der m\u00e4chtige Schach Abdul sa\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der m\u00e4chtige Schach Abdul sa\u00df", "tokens": ["Der", "m\u00e4ch\u00b7ti\u00b7ge", "Schach", "Ab\u00b7dul", "sa\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NE", "VVFIN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Auf Cores Thron, als in dem Reiche", "tokens": ["Auf", "Co\u00b7res", "Thron", ",", "als", "in", "dem", "Rei\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "APPR", "ART", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Feuer einer faulen Seuche", "tokens": ["Das", "Feu\u00b7er", "ei\u00b7ner", "fau\u00b7len", "Seu\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Volk bey Miriaden fra\u00df.", "tokens": ["Das", "Volk", "bey", "Mi\u00b7ri\u00b7a\u00b7den", "fra\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Heilkunst emsigstes Bestreben", "tokens": ["Der", "Heil\u00b7kunst", "em\u00b7sigs\u00b7tes", "Be\u00b7stre\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Erhielt nicht eines Kranken Leben:", "tokens": ["Er\u00b7hielt", "nicht", "ei\u00b7nes", "Kran\u00b7ken", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie welkten alle wie das Gras.", "tokens": ["Sie", "welk\u00b7ten", "al\u00b7le", "wie", "das", "Gras", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Um dieses Ungemach zu heben,", "tokens": ["Um", "die\u00b7ses", "Un\u00b7ge\u00b7mach", "zu", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Lud einst der Schach den Divan vor.", "tokens": ["Lud", "einst", "der", "Schach", "den", "Di\u00b7van", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Allein man schwieg zu seinen Fragen.", "tokens": ["Al\u00b7lein", "man", "schwieg", "zu", "sei\u00b7nen", "Fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Der Mufti kratzte sich das Ohr:", "tokens": ["Der", "Muf\u00b7ti", "kratz\u00b7te", "sich", "das", "Ohr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der Kanzler glaubte viel zu sagen,", "tokens": ["Der", "Kanz\u00b7ler", "glaub\u00b7te", "viel", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und sagte nichts. Zuletzt ward auch", "tokens": ["Und", "sag\u00b7te", "nichts", ".", "Zu\u00b7letzt", "ward", "auch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "$.", "ADV", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Der Arzt gefragt: wir wissens alle,", "tokens": ["Der", "Arzt", "ge\u00b7fragt", ":", "wir", "wis\u00b7sens", "al\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$.", "PPER", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Sprach er mit vorgestrecktem Bauch,", "tokens": ["Sprach", "er", "mit", "vor\u00b7ge\u00b7streck\u00b7tem", "Bauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Der Sitz des Uebels ist die Galle:", "tokens": ["Der", "Sitz", "des", "Ue\u00b7bels", "ist", "die", "Gal\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Die zeugt die Krankheit und den Tod;", "tokens": ["Die", "zeugt", "die", "Krank\u00b7heit", "und", "den", "Tod", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Doch wer kann die Natur beschw\u00f6ren? \u2013", "tokens": ["Doch", "wer", "kann", "die", "Na\u00b7tur", "be\u00b7schw\u00f6\u00b7ren", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PWS", "VMFIN", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Freund, damit hat es keine Noth,", "tokens": ["Freund", ",", "da\u00b7mit", "hat", "es", "kei\u00b7ne", "Noth", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PAV", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.20": {"text": "Rief der Monarch, du sollst es h\u00f6ren.", "tokens": ["Rief", "der", "Mon\u00b7arch", ",", "du", "sollst", "es", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.21": {"text": "Sogleich erscheinet ein Mandat,", "tokens": ["Sog\u00b7leich", "er\u00b7schei\u00b7net", "ein", "Man\u00b7dat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Da\u00df jedem Herrn und jedem Sklaven,", "tokens": ["Da\u00df", "je\u00b7dem", "Herrn", "und", "je\u00b7dem", "Skla\u00b7ven", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Bey martervollen Lebensstrafen,", "tokens": ["Bey", "mar\u00b7ter\u00b7vol\u00b7len", "Le\u00b7bens\u00b7stra\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Ein Kind, das eine Galle hat,", "tokens": ["Ein", "Kind", ",", "das", "ei\u00b7ne", "Gal\u00b7le", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Zu zeugen, f\u00f6rmlich untersagte. \u2013", "tokens": ["Zu", "zeu\u00b7gen", ",", "f\u00f6rm\u00b7lich", "un\u00b7ter\u00b7sag\u00b7te", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ADJD", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Ihr Abdul war ein wildes Thier,", "tokens": ["Ihr", "Ab\u00b7dul", "war", "ein", "wil\u00b7des", "Thier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Ein Satan, den die Mordlust plagte!", "tokens": ["Ein", "Sa\u00b7tan", ",", "den", "die", "Mord\u00b7lust", "plag\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "So fiel mir unser Pfarrer hier", "tokens": ["So", "fiel", "mir", "un\u00b7ser", "Pfar\u00b7rer", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Ergrimmt ins Wort. Ich mu\u00dfte lachen:", "tokens": ["Er\u00b7grimmt", "ins", "Wort", ".", "Ich", "mu\u00df\u00b7te", "la\u00b7chen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$.", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Dem guten Mann kam nicht in Sinn,", "tokens": ["Dem", "gu\u00b7ten", "Mann", "kam", "nicht", "in", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Da\u00df er und sein Sankt Augustin", "tokens": ["Da\u00df", "er", "und", "sein", "Sankt", "Au\u00b7gus\u00b7tin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KON", "PPOSAT", "VVFIN", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.32": {"text": "Den lieben Gott zum Abdul machen.", "tokens": ["Den", "lie\u00b7ben", "Gott", "zum", "Ab\u00b7dul", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}