{"textgrid.poem.63852": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich zur Winterflucht mein B\u00fcndel schn\u00fcrte,", "tokens": ["Als", "ich", "zur", "Win\u00b7ter\u00b7flucht", "mein", "B\u00fcn\u00b7del", "schn\u00fcr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Steckt' ich, die schwerlich man im S\u00fcden f\u00e4nde,", "tokens": ["Steckt'", "ich", ",", "die", "schwer\u00b7lich", "man", "im", "S\u00fc\u00b7den", "f\u00e4n\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "ADJD", "PIS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auch meines teuren R\u00fcckert Liederb\u00e4nde", "tokens": ["Auch", "mei\u00b7nes", "teu\u00b7ren", "R\u00fc\u00b7ckert", "Lie\u00b7der\u00b7b\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit ein, dem dieser Vorzug wohl geb\u00fchrte.", "tokens": ["Mit", "ein", ",", "dem", "die\u00b7ser", "Vor\u00b7zug", "wohl", "ge\u00b7b\u00fchr\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PDAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie bin ich froh, da\u00df ich ihn mit mir f\u00fchrte!", "tokens": ["Wie", "bin", "ich", "froh", ",", "da\u00df", "ich", "ihn", "mit", "mir", "f\u00fchr\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn nie lies't diesen Reichen man zu Ende,", "tokens": ["Denn", "nie", "lies't", "die\u00b7sen", "Rei\u00b7chen", "man", "zu", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PDAT", "NN", "PIS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem g\u00fctig so Natur gefeit die H\u00e4nde,", "tokens": ["Dem", "g\u00fc\u00b7tig", "so", "Na\u00b7tur", "ge\u00b7feit", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "NN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df Poesie ward, was er je ber\u00fchrte.", "tokens": ["Da\u00df", "Poe\u00b7sie", "ward", ",", "was", "er", "je", "be\u00b7r\u00fchr\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "$,", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Nun liegt vor unserm Blick auf tausend Seiten", "tokens": ["Nun", "liegt", "vor", "un\u00b7serm", "Blick", "auf", "tau\u00b7send", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein Leben, sein Gem\u00fct, sein tiefstes Denken,", "tokens": ["Sein", "Le\u00b7ben", ",", "sein", "Ge\u00b7m\u00fct", ",", "sein", "tiefs\u00b7tes", "Den\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "All seine Freuden, Schmerzen, Traulichkeiten.", "tokens": ["All", "sei\u00b7ne", "Freu\u00b7den", ",", "Schmer\u00b7zen", ",", "Trau\u00b7lich\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wohl frommt's, in solchen Schatz sich zu versenken", "tokens": ["Wohl", "frommt's", ",", "in", "sol\u00b7chen", "Schatz", "sich", "zu", "ver\u00b7sen\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "APPR", "PIAT", "NN", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und nach der tr\u00fcben Flut der j\u00fcngsten Zeiten", "tokens": ["Und", "nach", "der", "tr\u00fc\u00b7ben", "Flut", "der", "j\u00fcng\u00b7sten", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus diesem reinen Quell sein Herz zu tr\u00e4nken!", "tokens": ["Aus", "die\u00b7sem", "rei\u00b7nen", "Quell", "sein", "Herz", "zu", "tr\u00e4n\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}