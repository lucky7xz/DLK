{"dta.poem.12126": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sieh, alles was dich sonst ge\u00e4rgert hat zu Haus,", "tokens": ["Sieh", ",", "al\u00b7les", "was", "dich", "sonst", "ge\u00b7\u00e4r\u00b7gert", "hat", "zu", "Haus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PIS", "PWS", "PPER", "ADV", "VVPP", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie s\u00f6hnest du damit dich nun auf Reisen aus!", "tokens": ["Wie", "s\u00f6h\u00b7nest", "du", "da\u00b7mit", "dich", "nun", "auf", "Rei\u00b7sen", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PAV", "PPER", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie \u00e4rgerte dich sonst ein grauer Regentag,", "tokens": ["Wie", "\u00e4r\u00b7ger\u00b7te", "dich", "sonst", "ein", "grau\u00b7er", "Re\u00b7gen\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo mit den Blumen matt der Geist danieder lag.", "tokens": ["Wo", "mit", "den", "Blu\u00b7men", "matt", "der", "Geist", "da\u00b7nie\u00b7der", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "APPR", "ART", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nun freut ein solcher dich, an dem du still einmal", "tokens": ["Nun", "freut", "ein", "sol\u00b7cher", "dich", ",", "an", "dem", "du", "still", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "PIAT", "PPER", "$,", "APPR", "PRELS", "PPER", "ADJD", "ADV"], "meter": "-+-+-+++-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Darfst liegen, und nicht mu\u00dft durchschweifen Berg und Thal.", "tokens": ["Darfst", "lie\u00b7gen", ",", "und", "nicht", "mu\u00dft", "durch\u00b7schwei\u00b7fen", "Berg", "und", "Thal", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "KON", "PTKNEG", "VMFIN", "VVINF", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}