{"textgrid.poem.31547": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die Fr\u00f6lich-und Herrliche Auferstehung Christi", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Erde konde nicht den jenigen behalten /", "tokens": ["Die", "Er\u00b7de", "kon\u00b7de", "nicht", "den", "je\u00b7ni\u00b7gen", "be\u00b7hal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ART", "PIAT", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "aus dessen Mund sie ward. Wie kond Verwesung sehn /", "tokens": ["aus", "des\u00b7sen", "Mund", "sie", "ward", ".", "Wie", "kond", "Ver\u00b7we\u00b7sung", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "VAFIN", "$.", "PWAV", "ADJD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "in dem der Erz-Geist pflegt des Lebens zubestehn?", "tokens": ["in", "dem", "der", "Er\u00b7zGeist", "pflegt", "des", "Le\u00b7bens", "zu\u00b7be\u00b7stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "wie kond der Sonnen-Brunn / die Vrhitz selbst erkalten?", "tokens": ["wie", "kond", "der", "Son\u00b7nen\u00b7Brunn", "/", "die", "Vr\u00b7hitz", "selbst", "er\u00b7kal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "$(", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "sie must' in Mittel-Punct / war sie schon Strahlzerspalten:", "tokens": ["sie", "must'", "in", "Mit\u00b7tel\u00b7Punct", "/", "war", "sie", "schon", "Strahl\u00b7zer\u00b7spal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "$(", "VAFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df aus dem Todten-Reich der Lebens-F\u00fcrst k\u00f6nt gehn.", "tokens": ["Da\u00df", "aus", "dem", "Tod\u00b7ten\u00b7Reich", "der", "Le\u00b7bens\u00b7F\u00fcrst", "k\u00f6nt", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sein Mund-Lufft w\u00e4r genug / den Atlas weg zuwehn.", "tokens": ["Sein", "Mun\u00b7dLufft", "w\u00e4r", "ge\u00b7nug", "/", "den", "At\u00b7las", "weg", "zu\u00b7wehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$(", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sein' Allmacht kan so wol in als auf Erden walten.", "tokens": ["Sein'", "All\u00b7macht", "kan", "so", "wol", "in", "als", "auf", "Er\u00b7den", "wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "ADV", "APPR", "KOUS", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was wolt dir / starker Leu / der Tod das M\u00e4u\u00dflein seyn /", "tokens": ["Was", "wolt", "dir", "/", "star\u00b7ker", "Leu", "/", "der", "Tod", "das", "M\u00e4u\u00df\u00b7lein", "seyn", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$(", "ADJA", "NN", "$(", "ART", "NN", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "nach dem du Drachen schon und Tyger \u00fcberwunden /", "tokens": ["nach", "dem", "du", "Dra\u00b7chen", "schon", "und", "Ty\u00b7ger", "\u00fc\u00b7berw\u00b7un\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "ADV", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "der S\u00fcnd und Teuffel Heer? du legst dich nur hinein:", "tokens": ["der", "S\u00fcnd", "und", "Teuf\u00b7fel", "Heer", "?", "du", "legst", "dich", "nur", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NN", "$.", "PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Auf da\u00df wir auch den Tod im Grab belebet funden.", "tokens": ["Auf", "da\u00df", "wir", "auch", "den", "Tod", "im", "Grab", "be\u00b7le\u00b7bet", "fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dein Vrst\u00e4nd / schon mein Grab noch ungemacht aufmacht.", "tokens": ["Dein", "Vr\u00b7st\u00e4nd", "/", "schon", "mein", "Grab", "noch", "un\u00b7ge\u00b7macht", "auf\u00b7macht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Du hast Vnsterblichkeit uns Sterblichen gebracht.", "tokens": ["Du", "hast", "Vns\u00b7terb\u00b7lich\u00b7keit", "uns", "Sterb\u00b7li\u00b7chen", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}