{"textgrid.poem.58800": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "19. Von au\u00dfen eng, von innen weit", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei gegen deine S\u00fcnd' gestrenge,", "tokens": ["Sei", "ge\u00b7gen", "dei\u00b7ne", "S\u00fcnd'", "ge\u00b7stren\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Halt Fleisch und Sinnen in der Enge;", "tokens": ["Halt", "Fleisch", "und", "Sin\u00b7nen", "in", "der", "En\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch innerlich ganz weit und frei", "tokens": ["Doch", "in\u00b7ner\u00b7lich", "ganz", "weit", "und", "frei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und gegen Gott vertraulich sei!", "tokens": ["Und", "ge\u00b7gen", "Gott", "ver\u00b7trau\u00b7lich", "sei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}