{"textgrid.poem.49154": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Sterbenstrost", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was tr\u00f6stet mich doch in der Noth,", "tokens": ["Was", "tr\u00f6s\u00b7tet", "mich", "doch", "in", "der", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn mir der angeerbte Tod", "tokens": ["Wenn", "mir", "der", "an\u00b7ge\u00b7erb\u00b7te", "Tod"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dies Leben endlich ab will k\u00fcrzen,", "tokens": ["Dies", "Le\u00b7ben", "end\u00b7lich", "ab", "will", "k\u00fcr\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADV", "PTKVZ", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn mich mein eigen Herz verklagt,", "tokens": ["Wenn", "mich", "mein", "ei\u00b7gen", "Herz", "ver\u00b7klagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von nichts als von Verdammnis sagt,", "tokens": ["Von", "nichts", "als", "von", "Ver\u00b7damm\u00b7nis", "sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "KOKOM", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn mich die S\u00fcnd' hinab will st\u00fcrzen,", "tokens": ["Wenn", "mich", "die", "S\u00fcnd'", "hin\u00b7ab", "will", "st\u00fcr\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Hinab, wo die verboste Welt", "tokens": ["Hin\u00b7ab", ",", "wo", "die", "ver\u00b7bos\u00b7te", "Welt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den Lohn f\u00fcr all' ihr Thun erh\u00e4lt?", "tokens": ["Den", "Lohn", "f\u00fcr", "all'", "ihr", "Thun", "er\u00b7h\u00e4lt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es giebet Alles mich verlorn,", "tokens": ["Es", "gie\u00b7bet", "Al\u00b7les", "mich", "ver\u00b7lorn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Teufel zeigt mir Gottes Zorn", "tokens": ["Der", "Teu\u00b7fel", "zeigt", "mir", "Got\u00b7tes", "Zorn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schreit von lauter Feuerflammen.", "tokens": ["Und", "schreit", "von", "lau\u00b7ter", "Feu\u00b7er\u00b7flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was vor mir kaum besorglich war,", "tokens": ["Was", "vor", "mir", "kaum", "be\u00b7sorg\u00b7lich", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das beut sich jetzt zum Schrecken dar", "tokens": ["Das", "beut", "sich", "jetzt", "zum", "Schre\u00b7cken", "dar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und schw\u00f6rt auf meinen Tod zusammen,", "tokens": ["Und", "schw\u00f6rt", "auf", "mei\u00b7nen", "Tod", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ja, was von H\u00f6lle pred'gen kann,", "tokens": ["Ja", ",", "was", "von", "H\u00f6l\u00b7le", "pre\u00b7d'\u00b7gen", "kann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "APPR", "NN", "VVFIN", "VMFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Das sagt sich feindlich bei mir an.", "tokens": ["Das", "sagt", "sich", "feind\u00b7lich", "bei", "mir", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nur du, mein Jesus, stehst f\u00fcr mich,", "tokens": ["Nur", "du", ",", "mein", "Je\u00b7sus", ",", "stehst", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PPOSAT", "NE", "$,", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein lieber Vater hat ja dich,", "tokens": ["Dein", "lie\u00b7ber", "Va\u00b7ter", "hat", "ja", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dich, seinen ein'gen Sohn, gegeben,", "tokens": ["Dich", ",", "sei\u00b7nen", "ein'\u00b7gen", "Sohn", ",", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Damit durch dich die ganze Welt,", "tokens": ["Da\u00b7mit", "durch", "dich", "die", "gan\u00b7ze", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die durch die S\u00fcnde war gef\u00e4llt,", "tokens": ["Die", "durch", "die", "S\u00fcn\u00b7de", "war", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht sterben m\u00f6chte, sondern leben;", "tokens": ["Nicht", "ster\u00b7ben", "m\u00f6ch\u00b7te", ",", "son\u00b7dern", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "$,", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es sollte dein Verdienst allein", "tokens": ["Es", "soll\u00b7te", "dein", "Ver\u00b7dienst", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Genug f\u00fcr alle Menschen sein.", "tokens": ["Ge\u00b7nug", "f\u00fcr", "al\u00b7le", "Men\u00b7schen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Das eign' ich mir auch k\u00fchnlich zu;", "tokens": ["Das", "eign'", "ich", "mir", "auch", "k\u00fchn\u00b7lich", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich glaube, da\u00df, mein Heiland, du", "tokens": ["Ich", "glau\u00b7be", ",", "da\u00df", ",", "mein", "Hei\u00b7land", ",", "du"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "$,", "PPOSAT", "NN", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch mich dem Teufel abgestritten.", "tokens": ["Auch", "mich", "dem", "Teu\u00b7fel", "ab\u00b7ge\u00b7strit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Vater hat mich auch geliebt,", "tokens": ["Dein", "Va\u00b7ter", "hat", "mich", "auch", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was ich f\u00fcr Bosheit je ge\u00fcbt,", "tokens": ["Was", "ich", "f\u00fcr", "Bos\u00b7heit", "je", "ge\u00b7\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Daf\u00fcr hast du mit eins gelitten,", "tokens": ["Da\u00b7f\u00fcr", "hast", "du", "mit", "eins", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "APPR", "PIS", "VVPP", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.7": {"text": "Dein Blut ist, wie der ganzen Welt,", "tokens": ["Dein", "Blut", "ist", ",", "wie", "der", "gan\u00b7zen", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wahrhaftig auch mein L\u00f6segeld.", "tokens": ["Wahr\u00b7haf\u00b7tig", "auch", "mein", "L\u00f6\u00b7se\u00b7geld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dein Reich r\u00e4umt mir dein Vater ein,", "tokens": ["Dein", "Reich", "r\u00e4umt", "mir", "dein", "Va\u00b7ter", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie kann ich denn verloren sein?", "tokens": ["Wie", "kann", "ich", "denn", "ver\u00b7lo\u00b7ren", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was darf die S\u00fcnde mich erschrecken,", "tokens": ["Was", "darf", "die", "S\u00fcn\u00b7de", "mich", "er\u00b7schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was l\u00fcget mir der Teufel f\u00fcr,", "tokens": ["Was", "l\u00fc\u00b7get", "mir", "der", "Teu\u00b7fel", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es lieget meine Schuld auf dir,", "tokens": ["Es", "lie\u00b7get", "mei\u00b7ne", "Schuld", "auf", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Grab, das kann mich, Herr, verdecken,", "tokens": ["Dein", "Grab", ",", "das", "kann", "mich", ",", "Herr", ",", "ver\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VMFIN", "PPER", "$,", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn Alles feindlich auf mich hetzt", "tokens": ["Wenn", "Al\u00b7les", "feind\u00b7lich", "auf", "mich", "hetzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADJD", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und mich durchaus verloren sch\u00e4tzt.", "tokens": ["Und", "mich", "durc\u00b7haus", "ver\u00b7lo\u00b7ren", "sch\u00e4tzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Erhalte diesen Trost in mir,", "tokens": ["Er\u00b7hal\u00b7te", "die\u00b7sen", "Trost", "in", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Denn darauf leb' und sterb' ich dir,", "tokens": ["Denn", "da\u00b7rauf", "leb'", "und", "sterb'", "ich", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "KON", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn mich die Seele will verlassen;", "tokens": ["Wenn", "mich", "die", "See\u00b7le", "will", "ver\u00b7las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So lang' ein Glied sich an mir regt,", "tokens": ["So", "lang'", "ein", "Glied", "sich", "an", "mir", "regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So lang' der Tod die Brust mir schl\u00e4gt,", "tokens": ["So", "lang'", "der", "Tod", "die", "Brust", "mir", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So lass' mich dieses Ein'ge fassen,", "tokens": ["So", "lass'", "mich", "die\u00b7ses", "Ein'\u00b7ge", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df du mich von der H\u00f6lle Noth", "tokens": ["Da\u00df", "du", "mich", "von", "der", "H\u00f6l\u00b7le", "Noth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Erl\u00f6set hast durch deinen Tod.", "tokens": ["Er\u00b7l\u00f6\u00b7set", "hast", "durch", "dei\u00b7nen", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}