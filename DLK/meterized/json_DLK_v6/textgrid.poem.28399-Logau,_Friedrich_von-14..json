{"textgrid.poem.28399": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der klare Wein ist au\u00df; die Hefen sind in Fassen;", "tokens": ["Der", "kla\u00b7re", "Wein", "ist", "au\u00df", ";", "die", "He\u00b7fen", "sind", "in", "Fas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKVZ", "$.", "ART", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es hat die gute Zeit uns grosses Leid verlassen.", "tokens": ["Es", "hat", "die", "gu\u00b7te", "Zeit", "uns", "gros\u00b7ses", "Leid", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kan Christus nicht bey uns, was er zu Cana kan?", "tokens": ["Kan", "Chris\u00b7tus", "nicht", "bey", "uns", ",", "was", "er", "zu", "Ca\u00b7na", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "PTKNEG", "APPR", "PPER", "$,", "PWS", "PPER", "APPR", "NE", "VMFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Schweig! thu, was er dir sagt, bi\u00df seine Zeit k\u00fcmmt an.", "tokens": ["Schweig", "!", "thu", ",", "was", "er", "dir", "sagt", ",", "bi\u00df", "sei\u00b7ne", "Zeit", "k\u00fcmmt", "an", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVFIN", "$,", "PWS", "PPER", "PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}