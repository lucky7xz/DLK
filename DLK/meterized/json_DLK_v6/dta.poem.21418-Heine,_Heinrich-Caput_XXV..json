{"dta.poem.21418": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201edu schlenderst dort herum, und hast", "tokens": ["\u201e", "du", "schlen\u00b7derst", "dort", "he\u00b7rum", ",", "und", "hast"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ADV", "PTKVZ", "$,", "KON", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht mahl an deiner Seite", "tokens": ["Nicht", "mahl", "an", "dei\u00b7ner", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Einen treuen deutschen Verleger, der dich", "tokens": ["Ei\u00b7nen", "treu\u00b7en", "deut\u00b7schen", "Ver\u00b7le\u00b7ger", ",", "der", "dich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "PRELS", "PPER"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Als Mentor warne und leite.", "tokens": ["Als", "Men\u00b7tor", "war\u00b7ne", "und", "lei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "\u201eund die Verf\u00fchrung ist dort so gro\u00df,", "tokens": ["\u201e", "und", "die", "Ver\u00b7f\u00fch\u00b7rung", "ist", "dort", "so", "gro\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ART", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dort giebt es so viele Sylphiden,", "tokens": ["Dort", "giebt", "es", "so", "vie\u00b7le", "Syl\u00b7phi\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die ungesund, und gar zu leicht", "tokens": ["Die", "un\u00b7ge\u00b7sund", ",", "und", "gar", "zu", "leicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "$,", "KON", "ADV", "PTKA", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verliert man den Seelenfrieden.", "tokens": ["Ver\u00b7liert", "man", "den", "See\u00b7len\u00b7frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "\u201egeh\u2019 nicht zur\u00fcck und bleib\u2019 bei uns;", "tokens": ["\u201e", "geh'", "nicht", "zu\u00b7r\u00fcck", "und", "bleib'", "bei", "uns", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKNEG", "PTKVZ", "KON", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hier herrschen noch Zucht und Sitte,", "tokens": ["Hier", "herr\u00b7schen", "noch", "Zucht", "und", "Sit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und manches stille Vergn\u00fcgen bl\u00fcht", "tokens": ["Und", "man\u00b7ches", "stil\u00b7le", "Ver\u00b7gn\u00fc\u00b7gen", "bl\u00fcht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Auch hier, in unserer Mitte.", "tokens": ["Auch", "hier", ",", "in", "un\u00b7se\u00b7rer", "Mit\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "\u201ebleib\u2019 bei uns in Deutschland, es wird dir hier", "tokens": ["\u201e", "bleib'", "bei", "uns", "in", "Deutschland", ",", "es", "wird", "dir", "hier"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "APPR", "PPER", "APPR", "NE", "$,", "PPER", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Jetzt besser als eh\u2019mals munden;", "tokens": ["Jetzt", "bes\u00b7ser", "als", "eh'\u00b7mals", "mun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wir schreiten fort, du hast gewi\u00df", "tokens": ["Wir", "schrei\u00b7ten", "fort", ",", "du", "hast", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Fortschritt selbst gefunden.", "tokens": ["Den", "Fort\u00b7schritt", "selbst", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}