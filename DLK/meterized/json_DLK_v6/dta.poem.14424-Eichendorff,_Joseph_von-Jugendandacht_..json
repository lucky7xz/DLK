{"dta.poem.14424": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Jugendandacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und wie so tausend Stimmen ferne sangen,", "tokens": ["Und", "wie", "so", "tau\u00b7send", "Stim\u00b7men", "fer\u00b7ne", "san\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "CARD", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als riefen mich von hinnen seel'ge Br\u00fcder,", "tokens": ["Als", "rie\u00b7fen", "mich", "von", "hin\u00b7nen", "seel'\u00b7ge", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PRF", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fchlt' ich die alten Schmerzen immer wieder,", "tokens": ["F\u00fchlt'", "ich", "die", "al\u00b7ten", "Schmer\u00b7zen", "im\u00b7mer", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Seit Deine Blicke, Jungfrau, mich bezwangen.", "tokens": ["Seit", "Dei\u00b7ne", "Bli\u00b7cke", ",", "Jung\u00b7frau", ",", "mich", "be\u00b7zwan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}