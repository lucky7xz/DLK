{"textgrid.poem.52093": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "39.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Weib hat mehr Credit, dis sagst du f\u00fcr und f\u00fcr,", "tokens": ["Mein", "Weib", "hat", "mehr", "Cre\u00b7dit", ",", "dis", "sagst", "du", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "NN", "$,", "PDS", "VVFIN", "PPER", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als ich an Kauffmannschafft, als ich an Wechselb\u00e4ncken,", "tokens": ["Als", "ich", "an", "Kauf\u00b7fmann\u00b7schafft", ",", "als", "ich", "an", "Wech\u00b7sel\u00b7b\u00e4n\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "$,", "KOUS", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich glaub es mein Cornut, sie wei\u00df sich so zu r\u00e4ncken,", "tokens": ["Ich", "glaub", "es", "mein", "Cor\u00b7nut", ",", "sie", "wei\u00df", "sich", "so", "zu", "r\u00e4n\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PRF", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df sie die Kinder offt auch selbst verwechselt dir.", "tokens": ["Da\u00df", "sie", "die", "Kin\u00b7der", "offt", "auch", "selbst", "ver\u00b7wech\u00b7selt", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADV", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}