{"dta.poem.23686": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Lobredner.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Doris, h\u00f6re doch die Redner,", "tokens": ["Do\u00b7ris", ",", "h\u00f6\u00b7re", "doch", "die", "Red\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6re doch, sie loben M\u00e4dchen!", "tokens": ["H\u00f6\u00b7re", "doch", ",", "sie", "lo\u00b7ben", "M\u00e4d\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PPER", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Chloe reitzt mit schwarzen Lokken,", "tokens": ["Chloe", "reitzt", "mit", "schwar\u00b7zen", "Lok\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dafne mit erhabnem Busen,", "tokens": ["Daf\u00b7ne", "mit", "er\u00b7hab\u00b7nem", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Laura mit vergn\u00fcgten Minen,", "tokens": ["Lau\u00b7ra", "mit", "ver\u00b7gn\u00fcg\u00b7ten", "Mi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sappho mit gelehrten Augen,", "tokens": ["Sap\u00b7pho", "mit", "ge\u00b7lehr\u00b7ten", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Hanchen mit geschwinden F\u00fcssen,", "tokens": ["Han\u00b7chen", "mit", "ge\u00b7schwin\u00b7den", "F\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Galathe mit sch\u00f6nen H\u00e4nden,", "tokens": ["Ga\u00b7la\u00b7the", "mit", "sch\u00f6\u00b7nen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und das braune Kammerfr\u00e4ulein,", "tokens": ["Und", "das", "brau\u00b7ne", "Kam\u00b7mer\u00b7fr\u00e4u\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Mit dem Gr\u00fcbchen in dem Kinne.", "tokens": ["Mit", "dem", "Gr\u00fcb\u00b7chen", "in", "dem", "Kin\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Doris, soll ich dich nicht loben?", "tokens": ["Do\u00b7ris", ",", "soll", "ich", "dich", "nicht", "lo\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "H\u00f6re, Doris, ieder Redner", "tokens": ["H\u00f6\u00b7re", ",", "Do\u00b7ris", ",", "ie\u00b7der", "Red\u00b7ner"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "NE", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Lobte dich in seinem M\u00e4dchen;", "tokens": ["Lob\u00b7te", "dich", "in", "sei\u00b7nem", "M\u00e4d\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Aber deine sch\u00f6ne Seele", "tokens": ["A\u00b7ber", "dei\u00b7ne", "sch\u00f6\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Lobten sie an keinem M\u00e4dchen.", "tokens": ["Lob\u00b7ten", "sie", "an", "kei\u00b7nem", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}