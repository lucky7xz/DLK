{"textgrid.poem.65022": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja, ich habe dir verziehen,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, ich habe dir verziehen,", "tokens": ["Ja", ",", "ich", "ha\u00b7be", "dir", "ver\u00b7zie\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner Schuld gedenk ich nicht;", "tokens": ["Dei\u00b7ner", "Schuld", "ge\u00b7denk", "ich", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber dich auf ewig fliehen", "tokens": ["A\u00b7ber", "dich", "auf", "e\u00b7wig", "flie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hei\u00dft mich eine h\u00f6h're Pflicht.", "tokens": ["Hei\u00dft", "mich", "ei\u00b7ne", "h\u00f6h'\u00b7re", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und das Wort selbst der Vergebung,", "tokens": ["Und", "das", "Wort", "selbst", "der", "Ver\u00b7ge\u00b7bung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Drauf sich nun dein Hoffen baut,", "tokens": ["Drauf", "sich", "nun", "dein", "Hof\u00b7fen", "baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "War in schmerzlicher Erhebung", "tokens": ["War", "in", "schmerz\u00b7li\u00b7cher", "Er\u00b7he\u00b7bung"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ADJA", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Meiner Liebe Sterbelaut.", "tokens": ["Mei\u00b7ner", "Lie\u00b7be", "Ster\u00b7be\u00b7laut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mit emporgewandtem Haupte", "tokens": ["Mit", "em\u00b7por\u00b7ge\u00b7wand\u00b7tem", "Haup\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sank ich einstens vor dir hin,", "tokens": ["Sank", "ich", "eins\u00b7tens", "vor", "dir", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil ich wahnbefangen glaubte", "tokens": ["Weil", "ich", "wahn\u00b7be\u00b7fan\u00b7gen", "glaub\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVINF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df du edler, als ich bin.", "tokens": ["Da\u00df", "du", "ed\u00b7ler", ",", "als", "ich", "bin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "$,", "KOUS", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Von der H\u00f6h' herabgeschmettert,", "tokens": ["Von", "der", "H\u00f6h'", "her\u00b7ab\u00b7ge\u00b7schmet\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ledig der getr\u00e4umten Zier,", "tokens": ["Le\u00b7dig", "der", "ge\u00b7tr\u00e4um\u00b7ten", "Zier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deines Adels baar, entg\u00f6ttert,", "tokens": ["Dei\u00b7nes", "A\u00b7dels", "baar", ",", "ent\u00b7g\u00f6t\u00b7tert", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stehst du nun \u2013 wie klein! \u2013 vor mir.", "tokens": ["Stehst", "du", "nun", "\u2013", "wie", "klein", "!", "\u2013", "vor", "mir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$(", "PWAV", "ADJD", "$.", "$(", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Was mir blieb von meinem Lieben", "tokens": ["Was", "mir", "blieb", "von", "mei\u00b7nem", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stolzes Mitleid ist's allein.", "tokens": ["Stol\u00b7zes", "Mit\u00b7leid", "ist's", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df den Scheidebrief geschrieben", "tokens": ["La\u00df", "den", "Schei\u00b7de\u00b7brief", "ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Drum f\u00fcr alle Zeiten sein.", "tokens": ["Drum", "f\u00fcr", "al\u00b7le", "Zei\u00b7ten", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PIAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Besser ist's dem Gl\u00fcck entsagen", "tokens": ["Bes\u00b7ser", "ist's", "dem", "Gl\u00fcck", "ent\u00b7sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Muthig, ein f\u00fcr alle Mal,", "tokens": ["Mut\u00b7hig", ",", "ein", "f\u00fcr", "al\u00b7le", "Mal", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als, befleckt, im Herzen tragen", "tokens": ["Als", ",", "be\u00b7fleckt", ",", "im", "Her\u00b7zen", "tra\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "$,", "VVPP", "$,", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein entw\u00fcrdigt Ideal!", "tokens": ["Ein", "ent\u00b7w\u00fcr\u00b7digt", "I\u00b7deal", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}