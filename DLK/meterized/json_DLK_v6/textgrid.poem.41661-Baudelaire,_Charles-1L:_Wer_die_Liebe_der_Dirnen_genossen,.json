{"textgrid.poem.41661": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer die Liebe der Dirnen genossen,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer die Liebe der Dirnen genossen,", "tokens": ["Wer", "die", "Lie\u00b7be", "der", "Dir\u00b7nen", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Ist befriedigt und ohne Gram;", "tokens": ["Ist", "be\u00b7frie\u00b7digt", "und", "oh\u00b7ne", "Gram", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "KON", "APPR", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Mein Arm ist gebrochen und lahm,", "tokens": ["Mein", "Arm", "ist", "ge\u00b7bro\u00b7chen", "und", "lahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "KON", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Weil er Wolkengebilde umschlossen.", "tokens": ["Weil", "er", "Wol\u00b7ken\u00b7ge\u00b7bil\u00b7de", "um\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVINF", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Das Sternheer, das vom Himmel gl\u00fcht,", "tokens": ["Das", "Stern\u00b7heer", ",", "das", "vom", "Him\u00b7mel", "gl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist schuld mit seinem seltnen Scheine,", "tokens": ["Ist", "schuld", "mit", "sei\u00b7nem", "selt\u00b7nen", "Schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df mein geblendet Aug alleine", "tokens": ["Da\u00df", "mein", "ge\u00b7blen\u00b7det", "Aug", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "VVPP", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erinnerung von Sonnen sieht.", "tokens": ["E\u00b7rin\u00b7ne\u00b7rung", "von", "Son\u00b7nen", "sieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Umsonst wollt ich den Raum durchdringen,", "tokens": ["Um\u00b7sonst", "wollt", "ich", "den", "Raum", "durch\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Um End und Mitte zu ersp\u00e4hn;", "tokens": ["Um", "End", "und", "Mit\u00b7te", "zu", "er\u00b7sp\u00e4hn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich f\u00fchle, rettungslos zergehn", "tokens": ["Ich", "f\u00fch\u00b7le", ",", "ret\u00b7tungs\u00b7los", "zer\u00b7gehn"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor fremdem Glutblick meine Schwingen.", "tokens": ["Vor", "frem\u00b7dem", "Glut\u00b7blick", "mei\u00b7ne", "Schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mich, den verbrannt der Sehnsucht Glut,", "tokens": ["Mich", ",", "den", "ver\u00b7brannt", "der", "Sehn\u00b7sucht", "Glut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lebendge Sch\u00f6nheit zu erkennen,", "tokens": ["Le\u00b7bend\u00b7ge", "Sch\u00f6n\u00b7heit", "zu", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Kr\u00f6nt nicht der Ruhm, nach mir zu nennen", "tokens": ["Kr\u00f6nt", "nicht", "der", "Ruhm", ",", "nach", "mir", "zu", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "$,", "APPR", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Abgrund, drin mein Leichnam ruht.", "tokens": ["Den", "Ab\u00b7grund", ",", "drin", "mein", "Leich\u00b7nam", "ruht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}