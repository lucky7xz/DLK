{"textgrid.poem.40376": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Lazarus", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin der kleine Lazarus,", "tokens": ["Ich", "bin", "der", "klei\u00b7ne", "La\u00b7za\u00b7rus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "der still zu Bette liegen mu\u00df;", "tokens": ["der", "still", "zu", "Bet\u00b7te", "lie\u00b7gen", "mu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die Nacht ist immer schrecklich lang,", "tokens": ["die", "Nacht", "ist", "im\u00b7mer", "schreck\u00b7lich", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ich bin schon sieben Tage krank.", "tokens": ["ich", "bin", "schon", "sie\u00b7ben", "Ta\u00b7ge", "krank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "CARD", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df, im ganzen Hause gehn", "tokens": ["Ich", "wei\u00df", ",", "im", "gan\u00b7zen", "Hau\u00b7se", "gehn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPRART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die gro\u00dfen Leute auf den Zehn;", "tokens": ["die", "gro\u00b7\u00dfen", "Leu\u00b7te", "auf", "den", "Zehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ich mach mir aber garnichts draus,", "tokens": ["ich", "mach", "mir", "a\u00b7ber", "gar\u00b7nichts", "draus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PIS", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ich packe still mein Spielzeug aus.", "tokens": ["ich", "pa\u00b7cke", "still", "mein", "Spiel\u00b7zeug", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich schicke mein Soldatenheer", "tokens": ["Ich", "schi\u00b7cke", "mein", "Sol\u00b7da\u00b7ten\u00b7heer"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "durch meine Kissen kreuz und quer,", "tokens": ["durch", "mei\u00b7ne", "Kis\u00b7sen", "kreuz", "und", "quer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "von Tal zu Tal, bergauf bergab,", "tokens": ["von", "Tal", "zu", "Tal", ",", "ber\u00b7gauf", "berg\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und manchmal kommt ein tiefes Grab.", "tokens": ["und", "manch\u00b7mal", "kommt", "ein", "tie\u00b7fes", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und auf dem Laken wei\u00df wie Schnee", "tokens": ["Und", "auf", "dem", "La\u00b7ken", "wei\u00df", "wie", "Schnee"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "KOKOM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ziehn meine Schiffe \u00fcber See;", "tokens": ["ziehn", "mei\u00b7ne", "Schif\u00b7fe", "\u00fc\u00b7ber", "See", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und um die Wellen geht ein Wall,", "tokens": ["und", "um", "die", "Wel\u00b7len", "geht", "ein", "Wall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "da bau ich Burgen \u00fcberall.", "tokens": ["da", "bau", "ich", "Bur\u00b7gen", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich bin der Riese gro\u00df und still,", "tokens": ["Ich", "bin", "der", "Rie\u00b7se", "gro\u00df", "und", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "der Alles tun kann, was er will,", "tokens": ["der", "Al\u00b7les", "tun", "kann", ",", "was", "er", "will", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVINF", "VMFIN", "$,", "PWS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "vom Bettberg bis zum Lakenstrand", "tokens": ["vom", "Bett\u00b7berg", "bis", "zum", "La\u00b7ken\u00b7strand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "im Reich der wei\u00dfen Leinewand.", "tokens": ["im", "Reich", "der", "wei\u00b7\u00dfen", "Lei\u00b7ne\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}