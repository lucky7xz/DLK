{"dta.poem.12246": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Die schwarzbraune Hexe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es blies ein J\u00e4ger wohl in sein Horn,               ", "tokens": ["Es", "blies", "ein", "J\u00e4\u00b7ger", "wohl", "in", "sein", "Horn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wohl in sein Horn,", "tokens": ["Wohl", "in", "sein", "Horn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und alles was er blies das war verlorn.", "tokens": ["Und", "al\u00b7les", "was", "er", "blies", "das", "war", "ver\u00b7lorn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PWS", "PPER", "VVFIN", "PDS", "VAFIN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hop sa sa sa,", "tokens": ["Hop", "sa", "sa", "sa", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.it", "FM.it", "FM.it", "FM.it", "$,"], "meter": "OOOO", "measure": "unknown.measure.zero"}, "line.5": {"text": "Dra ra ra ra,", "tokens": ["Dra", "ra", "ra", "ra", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Und alles was er blies das war verlorn.", "tokens": ["Und", "al\u00b7les", "was", "er", "blies", "das", "war", "ver\u00b7lorn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PWS", "PPER", "VVFIN", "PDS", "VAFIN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Soll denn mein Blasen verloren seyn?", "tokens": ["Soll", "denn", "mein", "Bla\u00b7sen", "ver\u00b7lo\u00b7ren", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "VVPP", "VAINF", "$."], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Verloren seyn?", "tokens": ["Ver\u00b7lo\u00b7ren", "seyn", "?"], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ich wollte lieber kein J\u00e4ger seyn.", "tokens": ["Ich", "woll\u00b7te", "lie\u00b7ber", "kein", "J\u00e4\u00b7ger", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hop sa sa sa, u. s. w.", "tokens": ["Hop", "sa", "sa", "sa", ",", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["FM.it", "FM.it", "FM.it", "FM.it", "$,", "KON", "VVIMP", "NN"], "meter": "OOOO", "measure": "unknown.measure.zero"}}, "stanza.3": {"line.1": {"text": "Er zog sein Netz wohl \u00fcber den Strauch,", "tokens": ["Er", "zog", "sein", "Netz", "wohl", "\u00fc\u00b7ber", "den", "Strauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wohl \u00fcber den Strauch,", "tokens": ["Wohl", "\u00fc\u00b7ber", "den", "Strauch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Sprang ein schwarzbraunes M\u00e4del heraus.", "tokens": ["Sprang", "ein", "schwarz\u00b7brau\u00b7nes", "M\u00e4\u00b7del", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Hop sa sa sa, u. s. w.", "tokens": ["Hop", "sa", "sa", "sa", ",", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["FM.it", "FM.it", "FM.it", "FM.it", "$,", "KON", "VVIMP", "NN"], "meter": "OOOO", "measure": "unknown.measure.zero"}}, "stanza.4": {"line.1": {"text": "\u201eschwarzbraunes M\u00e4del entspringe mir nicht,", "tokens": ["\u201e", "schwarz\u00b7brau\u00b7nes", "M\u00e4\u00b7del", "ent\u00b7sprin\u00b7ge", "mir", "nicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "\u201eentspringe mir nicht,", "tokens": ["\u201e", "ent\u00b7sprin\u00b7ge", "mir", "nicht", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "\u201ehab' gro\u00dfe Hunde die holen dich.\u201c", "tokens": ["\u201e", "hab'", "gro\u00b7\u00dfe", "Hun\u00b7de", "die", "ho\u00b7len", "dich", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "ADJA", "NN", "ART", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hop sa sa sa, u. s. w.", "tokens": ["Hop", "sa", "sa", "sa", ",", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["FM.it", "FM.it", "FM.it", "FM.it", "$,", "KON", "VVIMP", "NN"], "meter": "OOOO", "measure": "unknown.measure.zero"}}, "stanza.5": {"line.1": {"text": "\u201edeine gro\u00dfen Hunde die holen mich nicht,", "tokens": ["\u201e", "dei\u00b7ne", "gro\u00b7\u00dfen", "Hun\u00b7de", "die", "ho\u00b7len", "mich", "nicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "ART", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "\u201edie holen mich nicht,", "tokens": ["\u201e", "die", "ho\u00b7len", "mich", "nicht", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "\u201esie wissen meine hohe weite Spr\u00fcnge noch nicht\u201c", "tokens": ["\u201e", "sie", "wis\u00b7sen", "mei\u00b7ne", "ho\u00b7he", "wei\u00b7te", "Spr\u00fcn\u00b7ge", "noch", "nicht", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPOSAT", "ADJA", "ADJA", "NN", "ADV", "PTKNEG", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "Hop sa sa sa, u. s. w.", "tokens": ["Hop", "sa", "sa", "sa", ",", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["FM.it", "FM.it", "FM.it", "FM.it", "$,", "KON", "VVIMP", "NN"], "meter": "OOOO", "measure": "unknown.measure.zero"}}, "stanza.6": {"line.1": {"text": "\u201edeine hohe Spr\u00fcnge die wissen sie wohl,", "tokens": ["\u201e", "dei\u00b7ne", "ho\u00b7he", "Spr\u00fcn\u00b7ge", "die", "wis\u00b7sen", "sie", "wohl", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "ART", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "\u201edie wissen sie wohl,", "tokens": ["\u201e", "die", "wis\u00b7sen", "sie", "wohl", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "\u201esie wissen, da\u00df du heute noch sterben sollst.\u201c", "tokens": ["\u201e", "sie", "wis\u00b7sen", ",", "da\u00df", "du", "heu\u00b7te", "noch", "ster\u00b7ben", "sollst", ".", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVINF", "$,", "KOUS", "PPER", "ADV", "ADV", "VVINF", "VMFIN", "$.", "$("], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hop sa sa sa, u. s. w.", "tokens": ["Hop", "sa", "sa", "sa", ",", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["FM.it", "FM.it", "FM.it", "FM.it", "$,", "KON", "VVIMP", "NN"], "meter": "OOOO", "measure": "unknown.measure.zero"}}, "stanza.7": {"line.1": {"text": "\u201esterben ich nun, so bin ich todt,", "tokens": ["\u201e", "ster\u00b7ben", "ich", "nun", ",", "so", "bin", "ich", "todt", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "\u201eso bin ich todt,", "tokens": ["\u201e", "so", "bin", "ich", "todt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "\u201ebegr\u00e4bt man mich unter die R\u00f6slein roth.\u201c", "tokens": ["\u201e", "be\u00b7gr\u00e4bt", "man", "mich", "un\u00b7ter", "die", "R\u00f6s\u00b7lein", "roth", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PIS", "PRF", "APPR", "ART", "NN", "ADJD", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Hop sa sa sa, u. s. w.", "tokens": ["Hop", "sa", "sa", "sa", ",", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["FM.it", "FM.it", "FM.it", "FM.it", "$,", "KON", "VVIMP", "NN"], "meter": "OOOO", "measure": "unknown.measure.zero"}}, "stanza.8": {"line.1": {"text": "\u201ewohl unter die R\u00f6slein, wohl unter den Klee,", "tokens": ["\u201e", "wohl", "un\u00b7ter", "die", "R\u00f6s\u00b7lein", ",", "wohl", "un\u00b7ter", "den", "Klee", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "\u201ewohl unter den Klee,", "tokens": ["\u201e", "wohl", "un\u00b7ter", "den", "Klee", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "\u201edarunter verderb ich nimmermehr.\u201c", "tokens": ["\u201e", "da\u00b7run\u00b7ter", "ver\u00b7derb", "ich", "nim\u00b7mer\u00b7mehr", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PAV", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hop sa sa sa, u. s. w.", "tokens": ["Hop", "sa", "sa", "sa", ",", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["FM.it", "FM.it", "FM.it", "FM.it", "$,", "KON", "VVIMP", "NN"], "meter": "OOOO", "measure": "unknown.measure.zero"}}, "stanza.9": {"line.1": {"text": "Es wuchsen drey Lilien auf ihrem Grab,", "tokens": ["Es", "wuch\u00b7sen", "drey", "Li\u00b7li\u00b7en", "auf", "ih\u00b7rem", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf ihrem Grab,", "tokens": ["Auf", "ih\u00b7rem", "Grab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die wollte ein Reuter wohl brechen ab.", "tokens": ["Die", "woll\u00b7te", "ein", "Reu\u00b7ter", "wohl", "bre\u00b7chen", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NE", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Hop sa sa sa, u. s. w.", "tokens": ["Hop", "sa", "sa", "sa", ",", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["FM.it", "FM.it", "FM.it", "FM.it", "$,", "KON", "VVIMP", "NN"], "meter": "OOOO", "measure": "unknown.measure.zero"}}, "stanza.10": {"line.1": {"text": "Ach Reuter, la\u00df die drey Lilien stahn,", "tokens": ["Ach", "Reu\u00b7ter", ",", "la\u00df", "die", "drey", "Li\u00b7li\u00b7en", "stahn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VVIMP", "ART", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.2": {"text": "Die Lilien stahn,", "tokens": ["Die", "Li\u00b7li\u00b7en", "stahn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Es soll sie ein junger frischer J\u00e4ger han.", "tokens": ["Es", "soll", "sie", "ein", "jun\u00b7ger", "fri\u00b7scher", "J\u00e4\u00b7ger", "han", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "ADJA", "ADJA", "NN", "VAFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Hop sa sa sa, u. s. w.", "tokens": ["Hop", "sa", "sa", "sa", ",", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["FM.it", "FM.it", "FM.it", "FM.it", "$,", "KON", "VVIMP", "NN"], "meter": "OOOO", "measure": "unknown.measure.zero"}}}}}