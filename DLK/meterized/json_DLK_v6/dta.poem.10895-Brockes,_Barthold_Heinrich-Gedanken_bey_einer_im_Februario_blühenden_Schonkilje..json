{"dta.poem.10895": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gedanken bey einer im  \n Februario bl\u00fchenden Schonkilje.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Liebster Gott! kanns m\u00f6glich seyn! kann der Winter nicht ein-\nmal", "tokens": ["Liebs\u00b7ter", "Gott", "!", "kanns", "m\u00f6g\u00b7lich", "seyn", "!", "kann", "der", "Win\u00b7ter", "nicht", "ein", "mal"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "VMFIN", "ADJD", "VAINF", "$.", "VMFIN", "ART", "NN", "PTKNEG", "TRUNC", "ADV"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Der Natur Formirungskraft, auch bey fernem Sonnenstral,", "tokens": ["Der", "Na\u00b7tur", "For\u00b7mi\u00b7rungs\u00b7kraft", ",", "auch", "bey", "fer\u00b7nem", "Son\u00b7nen\u00b7stral", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+---+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Auch zur allerrauhsten Zeit, mitten im December, tilgen!", "tokens": ["Auch", "zur", "al\u00b7ler\u00b7rauhs\u00b7ten", "Zeit", ",", "mit\u00b7ten", "im", "De\u00b7cem\u00b7ber", ",", "til\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,", "ADV", "APPRART", "NN", "$,", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Also rief ich, als ich j\u00fcngst z\u00e4rtlich bl\u00fchende Schonkiljen,", "tokens": ["Al\u00b7so", "rief", "ich", ",", "als", "ich", "j\u00fcngst", "z\u00e4rt\u00b7lich", "bl\u00fc\u00b7hen\u00b7de", "Schon\u00b7kil\u00b7jen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Ohne Kunst, in meinem Garten, aus der Erde steigen sah.", "tokens": ["Oh\u00b7ne", "Kunst", ",", "in", "mei\u00b7nem", "Gar\u00b7ten", ",", "aus", "der", "Er\u00b7de", "stei\u00b7gen", "sah", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Wie ich ihren unverhofften lieblich-gelben Glanz erblickte;", "tokens": ["Wie", "ich", "ih\u00b7ren", "un\u00b7ver\u00b7hoff\u00b7ten", "lieb\u00b7lich\u00b7gel\u00b7ben", "Glanz", "er\u00b7blick\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Wie ich sie, mit frohen H\u00e4nden, um sie aufzuheben, pfl\u00fcckte:", "tokens": ["Wie", "ich", "sie", ",", "mit", "fro\u00b7hen", "H\u00e4n\u00b7den", ",", "um", "sie", "auf\u00b7zu\u00b7he\u00b7ben", ",", "pfl\u00fcck\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "KOUI", "PPER", "VVIZU", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Waren mir, ob ihrer Sch\u00f6nheit, fast die Freudenthr\u00e4nen nah.", "tokens": ["Wa\u00b7ren", "mir", ",", "ob", "ih\u00b7rer", "Sch\u00f6n\u00b7heit", ",", "fast", "die", "Freu\u00b7dent\u00b7hr\u00e4\u00b7nen", "nah", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "$,", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Ich vermochte, wie ich sie so vollkommen fand, so sch\u00f6n,", "tokens": ["Ich", "ver\u00b7moch\u00b7te", ",", "wie", "ich", "sie", "so", "voll\u00b7kom\u00b7men", "fand", ",", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "PPER", "ADV", "ADJD", "VVFIN", "$,", "ADV", "ADJD", "$,"], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Wie sie so vortrefflich roch, mich daran nicht satt zu sehn,", "tokens": ["Wie", "sie", "so", "vor\u00b7treff\u00b7lich", "roch", ",", "mich", "da\u00b7ran", "nicht", "satt", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VVFIN", "$,", "PRF", "PAV", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-++-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "Mich daran nicht satt zu riechen. Meine Seele lenkte sich,", "tokens": ["Mich", "da\u00b7ran", "nicht", "satt", "zu", "rie\u00b7chen", ".", "Mei\u00b7ne", "See\u00b7le", "lenk\u00b7te", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PAV", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$.", "PPOSAT", "NN", "VVFIN", "PRF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Durch den unvermutheten nie gesehnen Glanz ger\u00fchret,", "tokens": ["Durch", "den", "un\u00b7ver\u00b7mu\u00b7the\u00b7ten", "nie", "ge\u00b7seh\u00b7nen", "Glanz", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.13": {"text": "Zu der Blum und meinem Sch\u00f6pfer; dankt und lobt ihn inniglich,", "tokens": ["Zu", "der", "Blum", "und", "mei\u00b7nem", "Sch\u00f6p\u00b7fer", ";", "dankt", "und", "lobt", "ihn", "in\u00b7nig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "KON", "PPOSAT", "NN", "$.", "VVFIN", "KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Da\u00df er, zu der Menschen Anmuth, ein so sch\u00f6n Gew\u00e4chs for-", "tokens": ["Da\u00df", "er", ",", "zu", "der", "Men\u00b7schen", "An\u00b7muth", ",", "ein", "so", "sch\u00f6n", "Ge\u00b7w\u00e4chs", "for"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "ART", "NN", "NN", "$,", "ART", "ADV", "ADJD", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.15": {"text": "Da\u00df er, da zur Sommerszeit, leider! durch derselben Menge,", "tokens": ["Da\u00df", "er", ",", "da", "zur", "Som\u00b7mers\u00b7zeit", ",", "lei\u00b7der", "!", "durch", "der\u00b7sel\u00b7ben", "Men\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "APPRART", "NN", "$,", "ADV", "$.", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.16": {"text": "Wir, zur Unempfindlichkeit uns ungl\u00fccklich bringen lassen,", "tokens": ["Wir", ",", "zur", "Un\u00b7emp\u00b7find\u00b7lich\u00b7keit", "uns", "un\u00b7gl\u00fcck\u00b7lich", "brin\u00b7gen", "las\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPRART", "NN", "PPER", "ADJD", "VVINF", "VVINF", "$,"], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Uns zu ungewohnter Zeit, (so da\u00df wir die Art nicht fassen)", "tokens": ["Uns", "zu", "un\u00b7ge\u00b7wohn\u00b7ter", "Zeit", ",", "(", "so", "da\u00df", "wir", "die", "Art", "nicht", "fas\u00b7sen", ")"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$,", "$(", "ADV", "KOUS", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Durch derselben s\u00fcssen Balsam, durch ihr schimmerndes Ge-", "tokens": ["Durch", "der\u00b7sel\u00b7ben", "s\u00fcs\u00b7sen", "Bal\u00b7sam", ",", "durch", "ihr", "schim\u00b7mern\u00b7des", "Ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.19": {"text": "Ausserordentlich vergn\u00fcgt. Herr! wenn ich an dieser Gabe,", "tokens": ["Aus\u00b7ser\u00b7or\u00b7dent\u00b7lich", "ver\u00b7gn\u00fcgt", ".", "Herr", "!", "wenn", "ich", "an", "die\u00b7ser", "Ga\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$.", "NN", "$.", "KOUS", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.20": {"text": "Durch dieselbe, durchs Gesicht, und durch den Geruch, mich labe:", "tokens": ["Durch", "die\u00b7sel\u00b7be", ",", "durchs", "Ge\u00b7sicht", ",", "und", "durch", "den", "Ge\u00b7ruch", ",", "mich", "la\u00b7be", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "$,", "APPRART", "NN", "$,", "KON", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.21": {"text": "La\u00df die\u00df liebliche Gesch\u00f6pf mich zu dir, als Sch\u00f6pfer, f\u00fchren;", "tokens": ["La\u00df", "die\u00df", "lieb\u00b7li\u00b7che", "Ge\u00b7sch\u00f6pf", "mich", "zu", "dir", ",", "als", "Sch\u00f6p\u00b7fer", ",", "f\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PDS", "ADJA", "NN", "PPER", "APPR", "PPER", "$,", "KOUS", "NN", "$,", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.22": {"text": "La\u00df mich ein, in dir gegr\u00fcndet, irdisches Vergn\u00fcgen sp\u00fcren!", "tokens": ["La\u00df", "mich", "ein", ",", "in", "dir", "ge\u00b7gr\u00fcn\u00b7det", ",", "ir\u00b7di\u00b7sches", "Ver\u00b7gn\u00fc\u00b7gen", "sp\u00fc\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "APPR", "PPER", "VVPP", "$,", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}