{"dta.poem.11652": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Charis und Penia.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Aber seht,", "tokens": ["A\u00b7ber", "seht", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Wie r\u00fchrend spricht aus diesen fremden Z\u00fcgen", "tokens": ["Wie", "r\u00fch\u00b7rend", "spricht", "aus", "die\u00b7sen", "frem\u00b7den", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "VVFIN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jezt offne, reine Menschlichkeit sich aus!", "tokens": ["Jezt", "off\u00b7ne", ",", "rei\u00b7ne", "Menschlich\u00b7keit", "sich", "aus", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJA", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Bajazzo's rohe Stimme ist entfernt,", "tokens": ["Ba\u00b7jaz\u00b7zo's", "ro\u00b7he", "Stim\u00b7me", "ist", "ent\u00b7fernt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Peitsche, die zum Scherze, doch empfindlich,", "tokens": ["Die", "Peit\u00b7sche", ",", "die", "zum", "Scher\u00b7ze", ",", "doch", "emp\u00b7find\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Kleinen traf, der sich zum Lachen zwang.", "tokens": ["Den", "Klei\u00b7nen", "traf", ",", "der", "sich", "zum", "La\u00b7chen", "zwang", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}