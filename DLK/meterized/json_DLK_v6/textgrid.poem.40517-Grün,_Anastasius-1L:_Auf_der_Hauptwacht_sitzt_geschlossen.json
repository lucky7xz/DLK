{"textgrid.poem.40517": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf der Hauptwacht sitzt geschlossen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf der Hauptwacht sitzt geschlossen", "tokens": ["Auf", "der", "Haupt\u00b7wacht", "sitzt", "ge\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Des Gebirges schlanker Sohn,", "tokens": ["Des", "Ge\u00b7bir\u00b7ges", "schlan\u00b7ker", "Sohn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Morgen fr\u00fche wird erschossen,", "tokens": ["Mor\u00b7gen", "fr\u00fc\u00b7he", "wird", "er\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der dreimal der Fahn' entflohn.", "tokens": ["Der", "drei\u00b7mal", "der", "Fahn'", "ent\u00b7flohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Heute g\u00f6nnten mit Erbarmen", "tokens": ["Heu\u00b7te", "g\u00f6nn\u00b7ten", "mit", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie ihm Wein und Prasserkost;", "tokens": ["Sie", "ihm", "Wein", "und", "Pras\u00b7ser\u00b7kost", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch in seiner Mutter Armen", "tokens": ["Doch", "in", "sei\u00b7ner", "Mut\u00b7ter", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gibt und nimmt er letzten Trost:", "tokens": ["Gibt", "und", "nimmt", "er", "letz\u00b7ten", "Trost", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbmutter, seht, die n\u00e4rr'schen Leute", "tokens": ["\u00bb", "mut\u00b7ter", ",", "seht", ",", "die", "n\u00e4rr'\u00b7schen", "Leu\u00b7te"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "NN", "$,", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heischten Treu' und Eid mir ab,", "tokens": ["Heischten", "Treu'", "und", "Eid", "mir", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die ich doch, und nicht erst heute,", "tokens": ["Die", "ich", "doch", ",", "und", "nicht", "erst", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "$,", "KON", "PTKNEG", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meiner lieben Sennin gab!", "tokens": ["Mei\u00b7ner", "lie\u00b7ben", "Sen\u00b7nin", "gab", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Soll mein Blut dem F\u00fcrsten geben,", "tokens": ["Soll", "mein", "Blut", "dem", "F\u00fcrs\u00b7ten", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mag wohl sein ein guter Mann;", "tokens": ["Mag", "wohl", "sein", "ein", "gu\u00b7ter", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch er fordre nicht mein Leben!", "tokens": ["Doch", "er", "ford\u00b7re", "nicht", "mein", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was blieb' euch, o Mutter, dann?", "tokens": ["Was", "blieb'", "euch", ",", "o", "Mut\u00b7ter", ",", "dann", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "FM", "NN", "$,", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Eures Hauptes Silberflocken,", "tokens": ["Eu\u00b7res", "Haup\u00b7tes", "Sil\u00b7ber\u00b7flo\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Acker schirmen, Hof und Haus", "tokens": ["A\u00b7cker", "schir\u00b7men", ",", "Hof", "und", "Haus"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVINF", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Liebsten goldne Locken,", "tokens": ["Und", "der", "Liebs\u00b7ten", "gold\u00b7ne", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "F\u00fcllt's nicht sch\u00f6n ein Leben aus?", "tokens": ["F\u00fcllt's", "nicht", "sch\u00f6n", "ein", "Le\u00b7ben", "aus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Hoch von langen Stangen wallten", "tokens": ["Hoch", "von", "lan\u00b7gen", "Stan\u00b7gen", "wall\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fetzen Tuchs, drauf sie recht fein", "tokens": ["Fet\u00b7zen", "Tuchs", ",", "drauf", "sie", "recht", "fein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "$,", "PAV", "PPER", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ein gefl\u00fcgelt Raubthier malten;", "tokens": ["Ein", "ge\u00b7fl\u00fc\u00b7gelt", "Raubt\u00b7hier", "mal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und da sollt' ich hinterdrein!", "tokens": ["Und", "da", "sollt'", "ich", "hin\u00b7ter\u00b7drein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PTKVZ", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.7": {"line.1": {"text": "Dem Gev\u00f6gel Adlern, Geiern,", "tokens": ["Dem", "Ge\u00b7v\u00f6\u00b7gel", "Ad\u00b7lern", ",", "Gei\u00b7ern", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War ich doch mein Lebtag gram;", "tokens": ["War", "ich", "doch", "mein", "Leb\u00b7tag", "gram", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPOSAT", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Scho\u00df manch einen, der zu euern", "tokens": ["Scho\u00df", "manch", "ei\u00b7nen", ",", "der", "zu", "eu\u00b7ern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PIAT", "PIS", "$,", "PRELS", "APPR", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Liebsten Heerden kam!", "tokens": ["Und", "der", "Liebs\u00b7ten", "Heer\u00b7den", "kam", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.8": {"line.1": {"text": "Ueber eine blanke Schachtel", "tokens": ["Ue\u00b7ber", "ei\u00b7ne", "blan\u00b7ke", "Schach\u00b7tel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spannten sie ein Eselsfell:", "tokens": ["Spann\u00b7ten", "sie", "ein", "E\u00b7sels\u00b7fell", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welch Gedr\u00f6hn, statt Lerch' und Wachtel,", "tokens": ["Welch", "Ge\u00b7dr\u00f6hn", ",", "statt", "Ler\u00b7ch'", "und", "Wach\u00b7tel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOUI", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Die im Korn einst schlugen hell!", "tokens": ["Die", "im", "Korn", "einst", "schlu\u00b7gen", "hell", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ADV", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Trommell\u00e4rm trieb mich von dannen,", "tokens": ["Trom\u00b7mel\u00b7l\u00e4rm", "trieb", "mich", "von", "dan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alphorn rief mich zu den H\u00f6hn,", "tokens": ["Al\u00b7phorn", "rief", "mich", "zu", "den", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die gr\u00fcnen, duft'gen Tannen,", "tokens": ["Wo", "die", "gr\u00fc\u00b7nen", ",", "duft'\u00b7gen", "Tan\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine echten Fahnen, wehn!", "tokens": ["Mei\u00b7ne", "ech\u00b7ten", "Fah\u00b7nen", ",", "wehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Unserm K\u00fcster lauscht' ich lieber", "tokens": ["Un\u00b7serm", "K\u00fcs\u00b7ter", "lauscht'", "ich", "lie\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem tapfern Fiedelstrich,", "tokens": ["Mit", "dem", "tap\u00b7fern", "Fie\u00b7del\u00b7strich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4hrend vom Gebirg her\u00fcber", "tokens": ["W\u00e4h\u00b7rend", "vom", "Ge\u00b7birg", "her\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00fc\u00df'rer Klang mein Ohr beschlich!", "tokens": ["S\u00fc\u00df'\u00b7rer", "Klang", "mein", "Ohr", "be\u00b7schlich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "In zweifarbig Tuch geschlagen,", "tokens": ["In", "zwei\u00b7far\u00b7big", "Tuch", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Knebelten mich Spang' und Knopf,", "tokens": ["Kne\u00b7bel\u00b7ten", "mich", "Spang'", "und", "Knopf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Einen H\u00f6cker sollt' ich tragen", "tokens": ["Ei\u00b7nen", "H\u00f6\u00b7cker", "sollt'", "ich", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und als Hut solch schwarzen Topf!", "tokens": ["Und", "als", "Hut", "solch", "schwar\u00b7zen", "Topf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Besser l\u00e4\u00dft, das sieht doch Jeder,", "tokens": ["Bes\u00b7ser", "l\u00e4\u00dft", ",", "das", "sieht", "doch", "Je\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "PDS", "VVFIN", "ADV", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir der gr\u00fcne Sch\u00fctzenrock,", "tokens": ["Mir", "der", "gr\u00fc\u00b7ne", "Sch\u00fct\u00b7zen\u00b7rock", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf dem Hut die Schildhahnfeder,", "tokens": ["Auf", "dem", "Hut", "die", "Schild\u00b7hahn\u00b7fe\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stutzen auch und Alpenstock!", "tokens": ["Stut\u00b7zen", "auch", "und", "Al\u00b7pen\u00b7stock", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Wachtstehn sollt' ich Nachts vor Zelten!", "tokens": ["Wacht\u00b7stehn", "sollt'", "ich", "Nachts", "vor", "Zel\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lullt mein Wachen sie in Ruh?", "tokens": ["Lullt", "mein", "Wa\u00b7chen", "sie", "in", "Ruh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Legt der Herr den mir geschm\u00e4lten", "tokens": ["Legt", "der", "Herr", "den", "mir", "ge\u00b7schm\u00e4l\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlummer wohl dem ihren zu?", "tokens": ["Schlum\u00b7mer", "wohl", "dem", "ih\u00b7ren", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "PPOSAT", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Besser als durch mich geborgen", "tokens": ["Bes\u00b7ser", "als", "durch", "mich", "ge\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "APPR", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stellt' in Himmels Schutz ich sie;", "tokens": ["Stellt'", "in", "Him\u00b7mels", "Schutz", "ich", "sie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "NN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und vor Liebchens Haus am Morgen", "tokens": ["Und", "vor", "Lieb\u00b7chens", "Haus", "am", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stand als Ehrenwacht ich fr\u00fch.", "tokens": ["Stand", "als", "Eh\u00b7ren\u00b7wacht", "ich", "fr\u00fch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "NN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Morgen, wenn die Sch\u00fcsse sch\u00fcttern", "tokens": ["Mor\u00b7gen", ",", "wenn", "die", "Sch\u00fcs\u00b7se", "sch\u00fct\u00b7tern"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mutter, denkt, da\u00df fern von 