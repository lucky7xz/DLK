{"textgrid.poem.56026": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Die Gruppe", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als pfl\u00fcckte einer rasch zu einem Strau\u00df:", "tokens": ["Als", "pfl\u00fcck\u00b7te", "ei\u00b7ner", "rasch", "zu", "ei\u00b7nem", "Strau\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "ADJD", "APPR", "ART", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ordnet der Zufall hastig die Gesichter,", "tokens": ["ord\u00b7net", "der", "Zu\u00b7fall", "has\u00b7tig", "die", "Ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "lockert sie auf und dr\u00fcckt sie wieder dichter,", "tokens": ["lo\u00b7ckert", "sie", "auf", "und", "dr\u00fcckt", "sie", "wie\u00b7der", "dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "ergreift zwei ferne, l\u00e4\u00dft ein nahes aus,", "tokens": ["er\u00b7greift", "zwei", "fer\u00b7ne", ",", "l\u00e4\u00dft", "ein", "na\u00b7hes", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "CARD", "ADJA", "$,", "VVFIN", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "tauscht das mit dem, bl\u00e4st irgendeines frisch,", "tokens": ["tauscht", "das", "mit", "dem", ",", "bl\u00e4st", "ir\u00b7gend\u00b7ei\u00b7nes", "frisch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPR", "ART", "$,", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "wirft einen Hund, wie Kraut, aus dem Gemisch", "tokens": ["wirft", "ei\u00b7nen", "Hund", ",", "wie", "Kraut", ",", "aus", "dem", "Ge\u00b7misch"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PWAV", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und zieht, was niedrig schaut, wie durch verworrne", "tokens": ["und", "zieht", ",", "was", "nied\u00b7rig", "schaut", ",", "wie", "durch", "ver\u00b7worr\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PRELS", "ADJD", "VVFIN", "$,", "PWAV", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Stiele und Bl\u00e4tter, an dem Kopf nach vorne", "tokens": ["Stie\u00b7le", "und", "Bl\u00e4t\u00b7ter", ",", "an", "dem", "Kopf", "nach", "vor\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "APPR", "ART", "NN", "APPR", "ADJA"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "und bindet es ganz klein am Rande ein;", "tokens": ["und", "bin\u00b7det", "es", "ganz", "klein", "am", "Ran\u00b7de", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und streckt sich wieder, \u00e4ndert und verstellt", "tokens": ["und", "streckt", "sich", "wie\u00b7der", ",", "\u00e4n\u00b7dert", "und", "ver\u00b7stellt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADV", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und hat nur eben Zeit, zum Augenschein", "tokens": ["und", "hat", "nur", "e\u00b7ben", "Zeit", ",", "zum", "Au\u00b7gen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ADV", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "zur\u00fcckzuspringen mitten auf die Matte,", "tokens": ["zu\u00b7r\u00fcck\u00b7zu\u00b7sprin\u00b7gen", "mit\u00b7ten", "auf", "die", "Mat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "auf der im n\u00e4chsten Augenblick der glatte", "tokens": ["auf", "der", "im", "n\u00e4chs\u00b7ten", "Au\u00b7gen\u00b7blick", "der", "glat\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPRART", "ADJA", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gewichteschwinger seine Schwere schwellt.", "tokens": ["Ge\u00b7wich\u00b7te\u00b7schwin\u00b7ger", "sei\u00b7ne", "Schwe\u00b7re", "schwellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}