{"dta.poem.9641": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der dritte preiset die vollkommenheit der  \n Churf\u00fcrstl. Gemahlin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es zeugt von ihrem hohen stande", "tokens": ["Es", "zeugt", "von", "ih\u00b7rem", "ho\u00b7hen", "stan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Selbst unser F\u00fcrstin geist und leib/", "tokens": ["Selbst", "un\u00b7ser", "F\u00fcrs\u00b7tin", "geist", "und", "leib", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie ist in unserm gantzen lande", "tokens": ["Sie", "ist", "in", "un\u00b7serm", "gant\u00b7zen", "lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die kl\u00fcgste frau/ das sch\u00f6nste weib.", "tokens": ["Die", "kl\u00fcgs\u00b7te", "frau", "/", "das", "sch\u00f6ns\u00b7te", "weib", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer wolte sie nicht herrschen lassen?", "tokens": ["Wer", "wol\u00b7te", "sie", "nicht", "herr\u00b7schen", "las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PTKNEG", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie herrscht mit billiger gewalt.", "tokens": ["Sie", "herrscht", "mit", "bil\u00b7li\u00b7ger", "ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie \u00fcbertrifft die untersassen", "tokens": ["Sie", "\u00fc\u00b7bert\u00b7rifft", "die", "un\u00b7ter\u00b7sas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "An hoheit/ tugend und gestalt.", "tokens": ["An", "ho\u00b7heit", "/", "tu\u00b7gend", "und", "ge\u00b7stalt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "ADJD", "KON", "VVPP", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}}}}}