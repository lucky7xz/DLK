{"textgrid.poem.31222": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Veilgens schlagen au\u00df.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Veilgens schlagen au\u00df.", "tokens": ["Die", "Veil\u00b7gens", "schla\u00b7gen", "au\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sie sind schon halb herau\u00df!", "tokens": ["Sie", "sind", "schon", "halb", "her\u00b7au\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch ihre heitre Bl\u00e4ue", "tokens": ["Durch", "ih\u00b7re", "heit\u00b7re", "Bl\u00e4u\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "dreibt Arkas seine S\u00e4ue.", "tokens": ["dreibt", "Ar\u00b7kas", "sei\u00b7ne", "S\u00e4u\u00b7e", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "die geulen ", "tokens": ["die", "geu\u00b7len"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "stehn fast bi\u00df an den B\u00e4uchen", "tokens": ["stehn", "fast", "bi\u00df", "an", "den", "B\u00e4u\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "in Gr\u00e4hsern und Gestr\u00e4uchen.", "tokens": ["in", "Gr\u00e4h\u00b7sern", "und", "Ge\u00b7str\u00e4u\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Groh\u00df-Vatter ", "tokens": ["Groh\u00df\u00b7Vat\u00b7ter"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "sein sihben stimmigt Rohr/", "tokens": ["sein", "sih\u00b7ben", "stim\u00b7migt", "Rohr", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00fcmb sein verlihbtes Pfeiffen", "tokens": ["\u00fcmb", "sein", "ver\u00b7lihb\u00b7tes", "Pfeif\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zitronen-V\u00f6gel schweiffen.", "tokens": ["Zi\u00b7tro\u00b7nen\u00b7V\u00f6\u00b7gel", "schwei\u00b7ffen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Schon h\u00f6rt man Sylvien schreyn/", "tokens": ["Schon", "h\u00f6rt", "man", "Syl\u00b7vi\u00b7en", "schreyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NE", "NE", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "ach nicht doch/ nein/ ach nein/", "tokens": ["ach", "nicht", "doch", "/", "nein", "/", "ach", "nein", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "$(", "PTKANT", "$(", "XY", "PTKANT", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "die dikken Bokksbaum-H\u00e4kken", "tokens": ["die", "dik\u00b7ken", "Bokks\u00b7baum\u00b7H\u00e4k\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "sie gantz und gar verst\u00e4kken.", "tokens": ["sie", "gantz", "und", "gar", "ver\u00b7st\u00e4k\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "wa\u00df wird dar bloh\u00df gemacht?", "tokens": ["wa\u00df", "wird", "dar", "bloh\u00df", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PAV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bleibt/ Kindgens/ ruhig lihgen/", "tokens": ["Bleibt", "/", "Kind\u00b7gens", "/", "ru\u00b7hig", "lih\u00b7gen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$(", "NN", "$(", "ADJD", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "der Schnizzger schnizzt euch Wihgen!", "tokens": ["der", "Sch\u00b7nizz\u00b7ger", "schnizzt", "euch", "Wih\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}}}}