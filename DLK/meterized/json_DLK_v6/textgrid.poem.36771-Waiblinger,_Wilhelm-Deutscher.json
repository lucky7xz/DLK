{"textgrid.poem.36771": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Deutscher", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Religion ist die Seele der Kunst und heil'ge Geschichte,", "tokens": ["Re\u00b7li\u00b7gi\u00b7on", "ist", "die", "See\u00b7le", "der", "Kunst", "und", "heil'\u00b7ge", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+---+-+--+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Und die Bibel allein bringt ihr Gedeihen und Heil.", "tokens": ["Und", "die", "Bi\u00b7bel", "al\u00b7lein", "bringt", "ihr", "Ge\u00b7dei\u00b7hen", "und", "Heil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Glauben und Fr\u00f6mmigkeit sei's und stille christliche Demuth,", "tokens": ["Glau\u00b7ben", "und", "Fr\u00f6m\u00b7mig\u00b7keit", "sei's", "und", "stil\u00b7le", "christ\u00b7li\u00b7che", "De\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "NE", "KON", "ADJA", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und der heilige Geist, der dich beseele zum Werk.", "tokens": ["Und", "der", "hei\u00b7li\u00b7ge", "Geist", ",", "der", "dich", "be\u00b7see\u00b7le", "zum", "Werk", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Fliehe vor allem den Reiz der Sinnlichkeit, denn der Aesthetik", "tokens": ["Flie\u00b7he", "vor", "al\u00b7lem", "den", "Reiz", "der", "Sinn\u00b7lich\u00b7keit", ",", "denn", "der", "A\u00b7e\u00b7sthe\u00b7tik"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PIS", "ART", "NN", "ART", "NN", "$,", "KON", "ART", "NN"], "meter": "+--+--+-+--+-+--+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Ist sie S\u00fcnd', ist sie Tod, wie der Moral sie es ist.", "tokens": ["Ist", "sie", "S\u00fcnd'", ",", "ist", "sie", "Tod", ",", "wie", "der", "Mo\u00b7ral", "sie", "es", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "VAFIN", "PPER", "NN", "$,", "PWAV", "ART", "NN", "PPER", "PPER", "VAFIN", "$."], "meter": "+-+--+--++--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Geist, unsichtbares Wesen, geheimni\u00dfvolles und tiefes", "tokens": ["Geist", ",", "un\u00b7sicht\u00b7ba\u00b7res", "We\u00b7sen", ",", "ge\u00b7heim\u00b7ni\u00df\u00b7vol\u00b7les", "und", "tie\u00b7fes"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "ADJA", "KON", "ADJA"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Hast du zu malen, und nicht irdische niedre Natur.", "tokens": ["Hast", "du", "zu", "ma\u00b7len", ",", "und", "nicht", "ir\u00b7di\u00b7sche", "nied\u00b7re", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKZU", "VVINF", "$,", "KON", "PTKNEG", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Denn nach ihrem Gesetz, nach ihren lieblichen Formen", "tokens": ["Denn", "nach", "ih\u00b7rem", "Ge\u00b7setz", ",", "nach", "ih\u00b7ren", "lieb\u00b7li\u00b7chen", "For\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Schaue du nicht, das gen\u00fcgt einzig dem heidnischen Sinn.", "tokens": ["Schau\u00b7e", "du", "nicht", ",", "das", "ge\u00b7n\u00fcgt", "ein\u00b7zig", "dem", "heid\u00b7ni\u00b7schen", "Sinn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PDS", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Aus der eigenen Tiefe, dem innern Schauen und F\u00fchlen", "tokens": ["Aus", "der", "ei\u00b7ge\u00b7nen", "Tie\u00b7fe", ",", "dem", "in\u00b7nern", "Schau\u00b7en", "und", "F\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "KON", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.12": {"text": "So empfange dein Bild, schaff' es von innen heraus;", "tokens": ["So", "emp\u00b7fan\u00b7ge", "dein", "Bild", ",", "schaff'", "es", "von", "in\u00b7nen", "he\u00b7raus", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "APPR", "ADV", "PTKVZ", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Und weil wir unsichtbar Unsichtbares bilden nicht k\u00f6nnen,", "tokens": ["Und", "weil", "wir", "un\u00b7sicht\u00b7bar", "Un\u00b7sicht\u00b7ba\u00b7res", "bil\u00b7den", "nicht", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "NN", "VVFIN", "PTKNEG", "VMFIN", "$,"], "meter": "-+-+--+---+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Sei's von der groben Natur wenigstens g\u00e4nzlich entfernt.", "tokens": ["Sei's", "von", "der", "gro\u00b7ben", "Na\u00b7tur", "we\u00b7nigs\u00b7tens", "g\u00e4nz\u00b7lich", "ent\u00b7fernt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Drum mit wenigem Fleisch und himmlischer Magerkeit kleide", "tokens": ["Drum", "mit", "we\u00b7ni\u00b7gem", "Fleisch", "und", "himm\u00b7li\u00b7scher", "Ma\u00b7ger\u00b7keit", "klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "PIAT", "NN", "KON", "ADJA", "NN", "VVFIN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Deine Heil'gen, so da\u00df fast ihre Seelen man sieht.", "tokens": ["Dei\u00b7ne", "Heil'\u00b7gen", ",", "so", "da\u00df", "fast", "ih\u00b7re", "See\u00b7len", "man", "sieht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "KOUS", "ADV", "PPOSAT", "NN", "PIS", "VVFIN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Alte Meister, sie lehren es dich, mit frommer Verehrung", "tokens": ["Al\u00b7te", "Meis\u00b7ter", ",", "sie", "leh\u00b7ren", "es", "dich", ",", "mit", "from\u00b7mer", "Ver\u00b7eh\u00b7rung"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "PRF", "$,", "APPR", "ADJA", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.18": {"text": "Schaue sie an, und es wird dir das Geheimni\u00df enth\u00fcllt.", "tokens": ["Schau\u00b7e", "sie", "an", ",", "und", "es", "wird", "dir", "das", "Ge\u00b7heim\u00b7ni\u00df", "ent\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "KON", "PPER", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.19": {"text": "Besser sind ihre Fehler, als selbst die Tugenden Neu'rer,", "tokens": ["Bes\u00b7ser", "sind", "ih\u00b7re", "Feh\u00b7ler", ",", "als", "selbst", "die", "Tu\u00b7gen\u00b7den", "Neu'\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,", "KOUS", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-++-+--+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.20": {"text": "Bete Fiesole an, Guido verachte mir brav.", "tokens": ["Be\u00b7te", "Fie\u00b7so\u00b7le", "an", ",", "Gui\u00b7do", "ver\u00b7ach\u00b7te", "mir", "brav", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$,", "NE", "VVFIN", "PPER", "ADJD", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Bleibst du in Armuth auch, und sch\u00e4tzt man hienieden dich wenig,", "tokens": ["Bleibst", "du", "in", "Ar\u00b7muth", "auch", ",", "und", "sch\u00e4tzt", "man", "hien\u00b7ie\u00b7den", "dich", "we\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADV", "$,", "KON", "VVFIN", "PIS", "VVFIN", "PPER", "ADV", "$,"], "meter": "+--+-+-+-++--+-", "measure": "iambic.septa.invert"}, "line.22": {"text": "Ist dir die Gnade daf\u00fcr, jene von oben, gewi\u00df.", "tokens": ["Ist", "dir", "die", "Gna\u00b7de", "da\u00b7f\u00fcr", ",", "je\u00b7ne", "von", "o\u00b7ben", ",", "ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PAV", "$,", "PDS", "APPR", "ADV", "$,", "ADV", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}}}}