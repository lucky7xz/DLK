{"textgrid.poem.62822": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein naseweiser Papagey", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein naseweiser Papagey", "tokens": ["Ein", "na\u00b7se\u00b7wei\u00b7ser", "Pa\u00b7pa\u00b7gey"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus unsern aufgekl\u00e4rten Zeiten,", "tokens": ["Aus", "un\u00b7sern", "auf\u00b7ge\u00b7kl\u00e4r\u00b7ten", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da stolzer Wahn und Sp\u00f6tterey", "tokens": ["Da", "stol\u00b7zer", "Wahn", "und", "Sp\u00f6t\u00b7te\u00b7rey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Pygm\u00e4isch das Verdienst bestreiten,", "tokens": ["Pyg\u00b7m\u00e4\u00b7isch", "das", "Ver\u00b7dienst", "be\u00b7strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sah einen Adler, den sein Flug", "tokens": ["Sah", "ei\u00b7nen", "Ad\u00b7ler", ",", "den", "sein", "Flug"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aus dem Revier der Sonne trug,", "tokens": ["Aus", "dem", "Re\u00b7vier", "der", "Son\u00b7ne", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Auf einem Baum sich niederlassen,", "tokens": ["Auf", "ei\u00b7nem", "Baum", "sich", "nie\u00b7der\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und kriegte Lust mit ihm zu spassen.", "tokens": ["Und", "krieg\u00b7te", "Lust", "mit", "ihm", "zu", "spas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Er flattert auf den Weisen zu,", "tokens": ["Er", "flat\u00b7tert", "auf", "den", "Wei\u00b7sen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der die gesenkte Stirne k\u00fchlte", "tokens": ["Der", "die", "ge\u00b7senk\u00b7te", "Stir\u00b7ne", "k\u00fchl\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und in dem Heiligthum der Ruh", "tokens": ["Und", "in", "dem", "Hei\u00b7lig\u00b7thum", "der", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die Wonne seines Daseyns f\u00fchlte.", "tokens": ["Die", "Won\u00b7ne", "sei\u00b7nes", "Da\u00b7seyns", "f\u00fchl\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "\u00bbherr Vetter, sprach der Papagey,", "tokens": ["\u00bb", "herr", "Vet\u00b7ter", ",", "sprach", "der", "Pa\u00b7pa\u00b7gey", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Mich d\u00fcnkt die Zeiten sind vorbey,", "tokens": ["Mich", "d\u00fcnkt", "die", "Zei\u00b7ten", "sind", "vor\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Da man dich als Monarch verehrte;", "tokens": ["Da", "man", "dich", "als", "Mon\u00b7arch", "ver\u00b7ehr\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "KOUS", "NN", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.16": {"text": "Die Zeiten, da des Dichters Witz", "tokens": ["Die", "Zei\u00b7ten", ",", "da", "des", "Dich\u00b7ters", "Witz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Dich mit dem schreckenvollen Blitz", "tokens": ["Dich", "mit", "dem", "schre\u00b7cken\u00b7vol\u00b7len", "Blitz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Des wilden Donnergotts bewehrte;", "tokens": ["Des", "wil\u00b7den", "Don\u00b7ner\u00b7gotts", "be\u00b7wehr\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Und gleichwohl bist du schlau genug,", "tokens": ["Und", "gleich\u00b7wohl", "bist", "du", "schlau", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Noch itzt den P\u00f6bel zu beth\u00f6ren", "tokens": ["Noch", "itzt", "den", "P\u00f6\u00b7bel", "zu", "be\u00b7th\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Und lenkest deinen stolzen Flug", "tokens": ["Und", "len\u00b7kest", "dei\u00b7nen", "stol\u00b7zen", "Flug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Nach mystischen erhabnen Sph\u00e4ren,", "tokens": ["Nach", "mys\u00b7ti\u00b7schen", "er\u00b7hab\u00b7nen", "Sph\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Als w\u00e4r es, wie der Gimpel glaubt", "tokens": ["Als", "w\u00e4r", "es", ",", "wie", "der", "Gim\u00b7pel", "glaubt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Nur deiner Majest\u00e4t erlaubt", "tokens": ["Nur", "dei\u00b7ner", "Ma\u00b7jes\u00b7t\u00e4t", "er\u00b7laubt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Auf des Olymps lazurnen H\u00fcgeln", "tokens": ["Auf", "des", "O\u00b7lymps", "la\u00b7zur\u00b7nen", "H\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-++-+-+-", "measure": "unknown.measure.penta"}, "line.26": {"text": "Sich in dem Quell des Lichts zu spiegeln.\u00ab", "tokens": ["Sich", "in", "dem", "Quell", "des", "Lichts", "zu", "spie\u00b7geln", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Der lose Vogel schweigt und lacht,", "tokens": ["Der", "lo\u00b7se", "Vo\u00b7gel", "schweigt", "und", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "So wie es jeder Sp\u00f6tter macht,", "tokens": ["So", "wie", "es", "je\u00b7der", "Sp\u00f6t\u00b7ter", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Wenn sein bescheidner Gegner g\u00e4hnet.", "tokens": ["Wenn", "sein", "be\u00b7scheid\u00b7ner", "Geg\u00b7ner", "g\u00e4h\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "\u00bbdem Adler ist es einerley,", "tokens": ["\u00bb", "dem", "Ad\u00b7ler", "ist", "es", "ei\u00b7ner\u00b7ley", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Sprach jener, was ein Papagey", "tokens": ["Sprach", "je\u00b7ner", ",", "was", "ein", "Pa\u00b7pa\u00b7gey"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PDAT", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Und was ein Gimpel von ihm w\u00e4hnet.", "tokens": ["Und", "was", "ein", "Gim\u00b7pel", "von", "ihm", "w\u00e4h\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Sein Wesen sagt ihm was er ist;", "tokens": ["Sein", "We\u00b7sen", "sagt", "ihm", "was", "er", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Der k\u00fchne Geist, der ihn belebet,", "tokens": ["Der", "k\u00fch\u00b7ne", "Geist", ",", "der", "ihn", "be\u00b7le\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Das Auge, das die Sonne gr\u00fc\u00dft,", "tokens": ["Das", "Au\u00b7ge", ",", "das", "die", "Son\u00b7ne", "gr\u00fc\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Sobald es nur die Wimper hebet,", "tokens": ["So\u00b7bald", "es", "nur", "die", "Wim\u00b7per", "he\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Der ehrne Fittich, den er tr\u00e4gt,", "tokens": ["Der", "ehr\u00b7ne", "Fit\u00b7tich", ",", "den", "er", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Der kaum bekielt die H\u00fcfte schl\u00e4gt,", "tokens": ["Der", "kaum", "be\u00b7kielt", "die", "H\u00fcf\u00b7te", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Um aus dem Nest sich aufzuraffen,", "tokens": ["Um", "aus", "dem", "Nest", "sich", "auf\u00b7zu\u00b7raf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "NN", "PRF", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Verk\u00fcndigt ihm das stolze Gl\u00fcck,", "tokens": ["Ver\u00b7k\u00fcn\u00b7digt", "ihm", "das", "stol\u00b7ze", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Er sey vom g\u00fctigen Geschick", "tokens": ["Er", "sey", "vom", "g\u00fc\u00b7ti\u00b7gen", "Ge\u00b7schick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Zum B\u00fcrger des Olymps erschaffen.\u00ab", "tokens": ["Zum", "B\u00fcr\u00b7ger", "des", "O\u00b7lymps", "er\u00b7schaf\u00b7fen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}