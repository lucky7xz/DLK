{"textgrid.poem.26333": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Der tote Baum", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Vogel klagt, ich sehe auf.", "tokens": ["Ein", "Vo\u00b7gel", "klagt", ",", "ich", "se\u00b7he", "auf", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Welk steht der Baum vor meiner T\u00fcre.", "tokens": ["Welk", "steht", "der", "Baum", "vor", "mei\u00b7ner", "T\u00fc\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sehe an dem Baum hinauf,", "tokens": ["Ich", "se\u00b7he", "an", "dem", "Baum", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus jedem Zweig den Tod ich sp\u00fcre.", "tokens": ["Aus", "je\u00b7dem", "Zweig", "den", "Tod", "ich", "sp\u00fc\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Bl\u00e4tter, die sonst hochgestellt,", "tokens": ["Die", "Bl\u00e4t\u00b7ter", ",", "die", "sonst", "hoch\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von gr\u00fcnem Lichte frisch erhellt,", "tokens": ["Von", "gr\u00fc\u00b7nem", "Lich\u00b7te", "frisch", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Bl\u00e4tter h\u00e4ngen grau herab.", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "h\u00e4n\u00b7gen", "grau", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es steigt der gro\u00dfe Baum ins Grab.", "tokens": ["Es", "steigt", "der", "gro\u00b7\u00dfe", "Baum", "ins", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Als mir der Vogel ihn gezeigt,", "tokens": ["Als", "mir", "der", "Vo\u00b7gel", "ihn", "ge\u00b7zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Flog er dann fort im Wolkenmeere.", "tokens": ["Flog", "er", "dann", "fort", "im", "Wol\u00b7ken\u00b7mee\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich habe still den Kopf geneigt.", "tokens": ["Ich", "ha\u00b7be", "still", "den", "Kopf", "ge\u00b7neigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Rund um mich w\u00e4chst die Totenleere.", "tokens": ["Rund", "um", "mich", "w\u00e4chst", "die", "To\u00b7ten\u00b7lee\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}