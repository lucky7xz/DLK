{"textgrid.poem.25767": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sehet dort! Aurora schaut", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sehet dort! Aurora schaut", "tokens": ["Se\u00b7het", "dort", "!", "Au\u00b7ro\u00b7ra", "schaut"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "$.", "NE", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber jenes Berges Gipfel,", "tokens": ["Ue\u00b7ber", "je\u00b7nes", "Ber\u00b7ges", "Gip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und nach seines Deckbetts Zipfel", "tokens": ["Und", "nach", "sei\u00b7nes", "Deck\u00b7betts", "Zip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "G\u00e4hnt der alte Vater laut.", "tokens": ["G\u00e4hnt", "der", "al\u00b7te", "Va\u00b7ter", "laut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00f6ret auf! die M\u00fctter schelten!", "tokens": ["H\u00f6\u00b7ret", "auf", "!", "die", "M\u00fct\u00b7ter", "schel\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "H\u00f6ret auf! die Tochter mu\u00df", "tokens": ["H\u00f6\u00b7ret", "auf", "!", "die", "Toch\u00b7ter", "mu\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$.", "ART", "NN", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Jeden Tanz und jeden Ku\u00df", "tokens": ["Je\u00b7den", "Tanz", "und", "je\u00b7den", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Zwiefach sonst noch heut entgelten.", "tokens": ["Zwie\u00b7fach", "sonst", "noch", "heut", "ent\u00b7gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "H\u00fcllet euch, ihr M\u00e4dchen, ein,", "tokens": ["H\u00fcl\u00b7let", "euch", ",", "ihr", "M\u00e4d\u00b7chen", ",", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Horchet nun im bunten Kreise", "tokens": ["Hor\u00b7chet", "nun", "im", "bun\u00b7ten", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Auf der Tassen Klang, und leise", "tokens": ["Auf", "der", "Tas\u00b7sen", "Klang", ",", "und", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "$,", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Stimmt in unsre Lieder ein;", "tokens": ["Stimmt", "in", "uns\u00b7re", "Lie\u00b7der", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Da\u00df des Busens hohe Wellen", "tokens": ["Da\u00df", "des", "Bu\u00b7sens", "ho\u00b7he", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Nicht des Nebels feuchtem Duft'", "tokens": ["Nicht", "des", "Ne\u00b7bels", "feuch\u00b7tem", "Duft'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Und der k\u00fchlen Morgenluft", "tokens": ["Und", "der", "k\u00fch\u00b7len", "Mor\u00b7gen\u00b7luft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Und dem Arzt' entgegen schwellen.", "tokens": ["Und", "dem", "Arzt'", "ent\u00b7ge\u00b7gen", "schwel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKVZ", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Aus der letzten Schaale Punsch", "tokens": ["Aus", "der", "letz\u00b7ten", "Schaa\u00b7le", "Pun\u00b7sch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.18": {"text": "La\u00dft das Glas mit Dank uns f\u00fcllen.", "tokens": ["La\u00dft", "das", "Glas", "mit", "Dank", "uns", "f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Dank den V\u00e4tern, denn sie stillen", "tokens": ["Dank", "den", "V\u00e4\u00b7tern", ",", "denn", "sie", "stil\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Gern der j\u00fcngern Herzen Wunsch.", "tokens": ["Gern", "der", "j\u00fcn\u00b7gern", "Her\u00b7zen", "Wunsch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Lebt denn hoch, ihr guten V\u00e4ter!", "tokens": ["Lebt", "denn", "hoch", ",", "ihr", "gu\u00b7ten", "V\u00e4\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Reibt euch, zweifelnd, nicht den Bart,", "tokens": ["Reibt", "euch", ",", "zwei\u00b7felnd", ",", "nicht", "den", "Bart", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJD", "$,", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Denn an eurer Nachsicht ward", "tokens": ["Denn", "an", "eu\u00b7rer", "Nach\u00b7sicht", "ward"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Keiner von uns zum Verr\u00e4ther.", "tokens": ["Kei\u00b7ner", "von", "uns", "zum", "Ver\u00b7r\u00e4\u00b7ther", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Auch den M\u00fcttern unsern Dank", "tokens": ["Auch", "den", "M\u00fct\u00b7tern", "un\u00b7sern", "Dank"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Aus der bald geleerten Schaale,", "tokens": ["Aus", "der", "bald", "ge\u00b7leer\u00b7ten", "Schaa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Da\u00df aus ihres Auges Strahle", "tokens": ["Da\u00df", "aus", "ih\u00b7res", "Au\u00b7ges", "Strah\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Nicht den T\u00f6chtern drohet Zank.", "tokens": ["Nicht", "den", "T\u00f6ch\u00b7tern", "dro\u00b7het", "Zank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Lebt denn hoch, ihr guten M\u00fctter!", "tokens": ["Lebt", "denn", "hoch", ",", "ihr", "gu\u00b7ten", "M\u00fct\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Aber la\u00dft das Um-sich-schaun.", "tokens": ["A\u00b7ber", "la\u00dft", "das", "Um\u00b7sich\u00b7schaun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Selbst an eurem Unvertraun", "tokens": ["Selbst", "an", "eu\u00b7rem", "Un\u00b7ver\u00b7traun"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Wird kein edler Mann zum Ritter.", "tokens": ["Wird", "kein", "ed\u00b7ler", "Mann", "zum", "Rit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "F\u00fcllet nun zum letztenmal", "tokens": ["F\u00fcl\u00b7let", "nun", "zum", "letz\u00b7ten\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.34": {"text": "Eure Gl\u00e4ser bis zum Rande.", "tokens": ["Eu\u00b7re", "Gl\u00e4\u00b7ser", "bis", "zum", "Ran\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Leb' und schlafe wohl, Amande!", "tokens": ["Leb'", "und", "schla\u00b7fe", "wohl", ",", "A\u00b7man\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "KON", "VVFIN", "ADV", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Und gedenk' an diesen Saal.", "tokens": ["Und", "ge\u00b7denk'", "an", "die\u00b7sen", "Saal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Gute Nacht, ihr Sch\u00f6nen alle!", "tokens": ["Gu\u00b7te", "Nacht", ",", "ihr", "Sch\u00f6\u00b7nen", "al\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN", "PIAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Gleich sey eure Phantasie", "tokens": ["Gleich", "sey", "eu\u00b7re", "Phan\u00b7ta\u00b7sie"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Unsrer T\u00e4nze Melodie,", "tokens": ["Uns\u00b7rer", "T\u00e4n\u00b7ze", "Me\u00b7lo\u00b7die", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "Und an Freude diesem Balle.", "tokens": ["Und", "an", "Freu\u00b7de", "die\u00b7sem", "Bal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}