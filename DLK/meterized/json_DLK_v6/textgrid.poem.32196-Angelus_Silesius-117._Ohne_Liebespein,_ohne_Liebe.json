{"textgrid.poem.32196": {"metadata": {"author": {"name": "Angelus Silesius", "birth": "N.A.", "death": "N.A."}, "title": "117. Ohne Liebespein, ohne Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1650, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verzug ursacht Verdru\u00df. F\u00fchlst du um Gott nicht Pein,", "tokens": ["Ver\u00b7zug", "ur\u00b7sacht", "Ver\u00b7dru\u00df", ".", "F\u00fchlst", "du", "um", "Gott", "nicht", "Pein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$.", "VVFIN", "PPER", "APPR", "NN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So glaub ich nicht, dein Herz in ihn entz\u00fcndt zu sein.", "tokens": ["So", "glaub", "ich", "nicht", ",", "dein", "Herz", "in", "ihn", "ent\u00b7z\u00fcndt", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$,", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}