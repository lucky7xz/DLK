{"textgrid.poem.59058": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "277. Gib dem Einen das Jawort!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hat dich Jesus oft gefragt;", "tokens": ["Es", "hat", "dich", "Je\u00b7sus", "oft", "ge\u00b7fragt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NE", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Warum mit andern sich geplagt?", "tokens": ["Wa\u00b7rum", "mit", "an\u00b7dern", "sich", "ge\u00b7plagt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PIS", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur dumm und herzlich Ja gesagt:", "tokens": ["Nur", "dumm", "und", "herz\u00b7lich", "Ja", "ge\u00b7sagt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hier ist des Herrn elende Magd!", "tokens": ["Hier", "ist", "des", "Herrn", "e\u00b7len\u00b7de", "Magd", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}