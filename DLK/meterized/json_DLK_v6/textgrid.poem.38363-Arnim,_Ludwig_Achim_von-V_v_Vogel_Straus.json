{"textgrid.poem.38363": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "V v Vogel Straus", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Vogel Straus hat gro\u00dfe Bein, doch klein ist sein Verstand,", "tokens": ["Der", "Vo\u00b7gel", "Straus", "hat", "gro\u00b7\u00dfe", "Bein", ",", "doch", "klein", "ist", "sein", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "VAFIN", "ADJA", "NN", "$,", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Es br\u00fctet ihm der Sonnenschein die Eier aus im Sand.", "tokens": ["Es", "br\u00fc\u00b7tet", "ihm", "der", "Son\u00b7nen\u00b7schein", "die", "Ei\u00b7er", "aus", "im", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Oft Stein und Eisen er verschluckt, sein Magen der ist gut,", "tokens": ["Oft", "Stein", "und", "Ei\u00b7sen", "er", "ver\u00b7schluckt", ",", "sein", "Ma\u00b7gen", "der", "ist", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "PPER", "VVPP", "$,", "PPOSAT", "NN", "ART", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Sein Federn sind der Weiber Schmuck, sie steckens auf den Hut.", "tokens": ["Sein", "Fe\u00b7dern", "sind", "der", "Wei\u00b7ber", "Schmuck", ",", "sie", "ste\u00b7ckens", "auf", "den", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "NN", "$,", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}