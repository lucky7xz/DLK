{"textgrid.poem.62912": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Sch\u00fcler a\u00df, wie viele Knaben,", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Sch\u00fcler a\u00df, wie viele Knaben,", "tokens": ["Ein", "Sch\u00fc\u00b7ler", "a\u00df", ",", "wie", "vie\u00b7le", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Datteln f\u00fcr sein Leben gern;", "tokens": ["Die", "Dat\u00b7teln", "f\u00fcr", "sein", "Le\u00b7ben", "gern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und um des Guten viel zu haben,", "tokens": ["Und", "um", "des", "Gu\u00b7ten", "viel", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So pflanzt er einen Dattelkern", "tokens": ["So", "pflanzt", "er", "ei\u00b7nen", "Dat\u00b7tel\u00b7kern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In seines Vaters Blumengarten.", "tokens": ["In", "sei\u00b7nes", "Va\u00b7ters", "Blu\u00b7men\u00b7gar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Vater sah ihm l\u00e4chelnd zu", "tokens": ["Der", "Va\u00b7ter", "sah", "ihm", "l\u00e4\u00b7chelnd", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sagte: Datteln pflanzest du?", "tokens": ["Und", "sag\u00b7te", ":", "Dat\u00b7teln", "pflan\u00b7zest", "du", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "O Kind, da must du lange warten!", "tokens": ["O", "Kind", ",", "da", "must", "du", "lan\u00b7ge", "war\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Denn wisse, dieser edle Baum", "tokens": ["Denn", "wis\u00b7se", ",", "die\u00b7ser", "ed\u00b7le", "Baum"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Tr\u00e4gt oft nach zwanzig Jahren kaum", "tokens": ["Tr\u00e4gt", "oft", "nach", "zwan\u00b7zig", "Jah\u00b7ren", "kaum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "CARD", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die ersten seiner s\u00fc\u00dfen Fr\u00fcchte.", "tokens": ["Die", "ers\u00b7ten", "sei\u00b7ner", "s\u00fc\u00b7\u00dfen", "Fr\u00fcch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Karl, der sich dessen nicht versah,", "tokens": ["Karl", ",", "der", "sich", "des\u00b7sen", "nicht", "ver\u00b7sah", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PRF", "PDS", "PTKNEG", "VVFIN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Hielt ein und r\u00fcmpfte das Gesichte.", "tokens": ["Hielt", "ein", "und", "r\u00fcmpf\u00b7te", "das", "Ge\u00b7sich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Ey, sprach er endlich zum Papa,", "tokens": ["Ey", ",", "sprach", "er", "end\u00b7lich", "zum", "Pa\u00b7pa", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Das Warten soll mich nicht verdrie\u00dfen;", "tokens": ["Das", "War\u00b7ten", "soll", "mich", "nicht", "ver\u00b7drie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Belohnt die Zeit nur meinen Flei\u00df,", "tokens": ["Be\u00b7lohnt", "die", "Zeit", "nur", "mei\u00b7nen", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "So kann ich ja dereinst als Greis,", "tokens": ["So", "kann", "ich", "ja", "de\u00b7reinst", "als", "Greis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Was itzt der Knabe pflanzt, genie\u00dfen.", "tokens": ["Was", "itzt", "der", "Kna\u00b7be", "pflanzt", ",", "ge\u00b7nie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So, holder Liebling, denkst auch du", "tokens": ["So", ",", "hol\u00b7der", "Lieb\u00b7ling", ",", "denkst", "auch", "du"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "PRELS", "ADV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sammelst an Minervens Busen", "tokens": ["Und", "sam\u00b7melst", "an", "Mi\u00b7ner\u00b7vens", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Dir Sch\u00e4tze f\u00fcr die Heldenruh", "tokens": ["Dir", "Sch\u00e4t\u00b7ze", "f\u00fcr", "die", "Hel\u00b7den\u00b7ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und \u2013 triumphieret deutsche Musen! \u2013", "tokens": ["Und", "\u2013", "tri\u00b7um\u00b7phie\u00b7ret", "deut\u00b7sche", "Mu\u00b7sen", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "$(", "VVFIN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Euch hat ein Celte sich gew\u00e4hlt,", "tokens": ["Euch", "hat", "ein", "Cel\u00b7te", "sich", "ge\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der kaum zwey volle Lustern z\u00e4hlt.", "tokens": ["Der", "kaum", "zwey", "vol\u00b7le", "Lus\u00b7tern", "z\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "CARD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Freund, pflanzest du auf deutsche W\u00e4lle", "tokens": ["Freund", ",", "pflan\u00b7zest", "du", "auf", "deut\u00b7sche", "W\u00e4l\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Einst dein Panier mit tapfrer Hand,", "tokens": ["Einst", "dein", "Pa\u00b7nier", "mit", "tapf\u00b7rer", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "So denke stets, auch diese Stelle", "tokens": ["So", "den\u00b7ke", "stets", ",", "auch", "die\u00b7se", "Stel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "$,", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Geh\u00f6rt zu Gellerts Vaterland.", "tokens": ["Ge\u00b7h\u00f6rt", "zu", "Gel\u00b7lerts", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}