{"dta.poem.12399": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Schreiber .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das M\u00e4gdlein will ein Freier habn,               ", "tokens": ["Das", "M\u00e4gd\u00b7lein", "will", "ein", "Frei\u00b7er", "habn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sollt sien aus der Erde grabn,", "tokens": ["Und", "sollt", "sien", "aus", "der", "Er\u00b7de", "grabn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr funfzehn Pfennige.", "tokens": ["F\u00fcr", "funf\u00b7zehn", "Pfen\u00b7ni\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie grub wohl ein, sie grub wohl aus,", "tokens": ["Sie", "grub", "wohl", "ein", ",", "sie", "grub", "wohl", "aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "PPER", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und grub nur einen Schreiber heraus,", "tokens": ["Und", "grub", "nur", "ei\u00b7nen", "Schrei\u00b7ber", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "F\u00fcr funfzehn Pfennige.", "tokens": ["F\u00fcr", "funf\u00b7zehn", "Pfen\u00b7ni\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Schreiber hatt' des Gelds zu viel,", "tokens": ["Der", "Schrei\u00b7ber", "hatt'", "des", "Gelds", "zu", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "APPR", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er kauft dem M\u00e4gdlein, was sie will,", "tokens": ["Er", "kauft", "dem", "M\u00e4gd\u00b7lein", ",", "was", "sie", "will", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr funfzehn Pfennige.", "tokens": ["F\u00fcr", "funf\u00b7zehn", "Pfen\u00b7ni\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er kauft ihr wohl ein'n G\u00fcrtel schmal,", "tokens": ["Er", "kauft", "ihr", "wohl", "ein'n", "G\u00fcr\u00b7tel", "schmal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der starrt von Gold wohl \u00fcberall,", "tokens": ["Der", "starrt", "von", "Gold", "wohl", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr funfzehn Pfennige.", "tokens": ["F\u00fcr", "funf\u00b7zehn", "Pfen\u00b7ni\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er kauft ihr einen breiten Hut,", "tokens": ["Er", "kauft", "ihr", "ei\u00b7nen", "brei\u00b7ten", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der w\u00e4r wohl f\u00fcr die Sonne gut,", "tokens": ["Der", "w\u00e4r", "wohl", "f\u00fcr", "die", "Son\u00b7ne", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr funfzehn Pfennige.", "tokens": ["F\u00fcr", "funf\u00b7zehn", "Pfen\u00b7ni\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wohl f\u00fcr die Sonn', wohl f\u00fcr den Wind,", "tokens": ["Wohl", "f\u00fcr", "die", "Sonn'", ",", "wohl", "f\u00fcr", "den", "Wind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bleib du bei mir, mein liebes Kind,", "tokens": ["Bleib", "du", "bei", "mir", ",", "mein", "lie\u00b7bes", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr funfzehn Pfennige.", "tokens": ["F\u00fcr", "funf\u00b7zehn", "Pfen\u00b7ni\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Bleibst du bei mir, bleib ich bei dir,", "tokens": ["Bleibst", "du", "bei", "mir", ",", "bleib", "ich", "bei", "dir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "All' meine G\u00fcter schenk ich dir,", "tokens": ["All'", "mei\u00b7ne", "G\u00fc\u00b7ter", "schenk", "ich", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sind funfzehn Pfennige.", "tokens": ["Sind", "funf\u00b7zehn", "Pfen\u00b7ni\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Behalt dein Gut, la\u00df mir mein'n Muth,", "tokens": ["Be\u00b7halt", "dein", "Gut", ",", "la\u00df", "mir", "mein'n", "Muth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVIMP", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein andre leicht dich nehmen thut,", "tokens": ["Kein", "and\u00b7re", "leicht", "dich", "neh\u00b7men", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIS", "VVFIN", "PRF", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr funfzehn Pfennige.", "tokens": ["F\u00fcr", "funf\u00b7zehn", "Pfen\u00b7ni\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Dein guten Muth den mag ich nicht,", "tokens": ["Dein", "gu\u00b7ten", "Muth", "den", "mag", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "VMFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat traun von treuer Liebe nicht,", "tokens": ["Hat", "traun", "von", "treu\u00b7er", "Lie\u00b7be", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr funfzehn Pfennige.", "tokens": ["F\u00fcr", "funf\u00b7zehn", "Pfen\u00b7ni\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Dein Herz ist wie ein Taubenhaus,", "tokens": ["Dein", "Herz", "ist", "wie", "ein", "Tau\u00b7ben\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fliegt einer nein, der andre aus,", "tokens": ["Fliegt", "ei\u00b7ner", "nein", ",", "der", "and\u00b7re", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PTKANT", "$,", "PRELS", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr funfzehn Pfennige.", "tokens": ["F\u00fcr", "funf\u00b7zehn", "Pfen\u00b7ni\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}