{"textgrid.poem.45946": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[wohlan! so bin ich deiner los]", "genre": "verse", "period": "N.A.", "pub_year": 1812, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohlan! so bin ich deiner los", "tokens": ["Wo\u00b7hlan", "!", "so", "bin", "ich", "dei\u00b7ner", "los"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "ADV", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du freches l\u00fcderliches Weib!", "tokens": ["Du", "fre\u00b7ches", "l\u00fc\u00b7der\u00b7li\u00b7ches", "Weib", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Fluch \u00fcber deinen s\u00fcndenvollen Scho\u00df", "tokens": ["Fluch", "\u00fc\u00b7ber", "dei\u00b7nen", "s\u00fcn\u00b7den\u00b7vol\u00b7len", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Fluch \u00fcber deinen feilen geilen Leib,", "tokens": ["Fluch", "\u00fc\u00b7ber", "dei\u00b7nen", "fei\u00b7len", "gei\u00b7len", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Fluch \u00fcber deine l\u00fcderlichen Br\u00fcste", "tokens": ["Fluch", "\u00fc\u00b7ber", "dei\u00b7ne", "l\u00fc\u00b7der\u00b7li\u00b7chen", "Br\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von Zucht und Wahrheit leer,", "tokens": ["Von", "Zucht", "und", "Wahr\u00b7heit", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Von Schand' und L\u00fcgen schwer,", "tokens": ["Von", "Schand'", "und", "L\u00fc\u00b7gen", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ein schmutzig Kissen aller eklen L\u00fcste.", "tokens": ["Ein", "schmut\u00b7zig", "Kis\u00b7sen", "al\u00b7ler", "ek\u00b7len", "L\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Fluch \u00fcber jede tote Stunde", "tokens": ["Fluch", "\u00fc\u00b7ber", "je\u00b7de", "to\u00b7te", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die ich an deinem l\u00fcgenvollen Munde,", "tokens": ["Die", "ich", "an", "dei\u00b7nem", "l\u00fc\u00b7gen\u00b7vol\u00b7len", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "In ekelhafter K\u00fcsse Rausch vollbracht,", "tokens": ["In", "e\u00b7kel\u00b7haf\u00b7ter", "K\u00fcs\u00b7se", "Rausch", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Fluch \u00fcber jede gottverge\u00dfne Nacht,", "tokens": ["Fluch", "\u00fc\u00b7ber", "je\u00b7de", "gott\u00b7ver\u00b7ge\u00df\u00b7ne", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Die ich in deinem frechen Bett erhandelt,", "tokens": ["Die", "ich", "in", "dei\u00b7nem", "fre\u00b7chen", "Bett", "er\u00b7han\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Die ich in toller Liebe \u00fcberwacht,", "tokens": ["Die", "ich", "in", "tol\u00b7ler", "Lie\u00b7be", "\u00fc\u00b7ber\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Wohl unter deinem Fenster hingewandelt,", "tokens": ["Wohl", "un\u00b7ter", "dei\u00b7nem", "Fens\u00b7ter", "hin\u00b7ge\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Wenn du mit andern in dem Werk befangen,", "tokens": ["Wenn", "du", "mit", "an\u00b7dern", "in", "dem", "Werk", "be\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Mit andrer L\u00fcg' an anderm Mund gehangen.", "tokens": ["Mit", "an\u00b7drer", "L\u00fcg'", "an", "an\u00b7derm", "Mund", "ge\u00b7han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PIS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Mein Gott, mein Gott, er will sich mein erbarmen,", "tokens": ["Mein", "Gott", ",", "mein", "Gott", ",", "er", "will", "sich", "mein", "er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPER", "VMFIN", "PRF", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Mein Herr hat mich befreit aus deinen Armen,", "tokens": ["Mein", "Herr", "hat", "mich", "be\u00b7freit", "aus", "dei\u00b7nen", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Wohin dein Gott, der Satan mich gef\u00fchrt;", "tokens": ["Wo\u00b7hin", "dein", "Gott", ",", "der", "Sa\u00b7tan", "mich", "ge\u00b7f\u00fchrt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Drum hab' ich nimmer dir dein Herz ger\u00fchrt,", "tokens": ["Drum", "hab'", "ich", "nim\u00b7mer", "dir", "dein", "Herz", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Und wie ich mochte bitten, mochte flehen,", "tokens": ["Und", "wie", "ich", "moch\u00b7te", "bit\u00b7ten", ",", "moch\u00b7te", "fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VMFIN", "VVINF", "$,", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Kein edles Wort h\u00f6rt' ich von dir erstehen,", "tokens": ["Kein", "ed\u00b7les", "Wort", "h\u00f6rt'", "ich", "von", "dir", "er\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Du drohst, du elend Weib, dich zu ermorden,", "tokens": ["Du", "drohst", ",", "du", "e\u00b7lend", "Weib", ",", "dich", "zu", "er\u00b7mor\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "ADJD", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "O k\u00f6nntest du's, es st\u00fcrb' dein ganzer Orden,", "tokens": ["O", "k\u00f6nn\u00b7test", "du's", ",", "es", "st\u00fcrb'", "dein", "gan\u00b7zer", "Or\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PIS", "$,", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Doch spar' die M\u00fche nur, denn du bist l\u00e4ngstens tot,", "tokens": ["Doch", "spa\u00b7r'", "die", "M\u00fc\u00b7he", "nur", ",", "denn", "du", "bist", "l\u00e4ngs\u00b7tens", "tot", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$,", "KON", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "L\u00e4ngst faulst du in dir selbst, in S\u00fcnd' und L\u00fcgenkot.", "tokens": ["L\u00e4ngst", "faulst", "du", "in", "dir", "selbst", ",", "in", "S\u00fcnd'", "und", "L\u00fc\u00b7gen\u00b7kot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "ADV", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Schneidst du den Hals dir ab", "tokens": ["Schneidst", "du", "den", "Hals", "dir", "ab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "PPER", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Und springst du in die Spree,", "tokens": ["Und", "springst", "du", "in", "die", "Spree", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.30": {"text": "Du findest nie ein Grab", "tokens": ["Du", "fin\u00b7dest", "nie", "ein", "Grab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Die Spreu schwimmt in der H\u00f6h'.", "tokens": ["Die", "Spreu", "schwimmt", "in", "der", "H\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.32": {"text": "Des Todes heiliger Traum", "tokens": ["Des", "To\u00b7des", "hei\u00b7li\u00b7ger", "Traum"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.33": {"text": "Wird nimmer dich erl\u00f6sen", "tokens": ["Wird", "nim\u00b7mer", "dich", "er\u00b7l\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Es stirbt ein gr\u00fcner Baum,", "tokens": ["Es", "stirbt", "ein", "gr\u00fc\u00b7ner", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.35": {"text": "Doch nie ein d\u00fcrrer Besen.", "tokens": ["Doch", "nie", "ein", "d\u00fcr\u00b7rer", "Be\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Zur eignen Rute wirst du noch an deinem R\u00fccken,", "tokens": ["Zur", "eig\u00b7nen", "Ru\u00b7te", "wirst", "du", "noch", "an", "dei\u00b7nem", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Und h\u00f6chstens reicht dein Leib dir einstens schlechte Kr\u00fccken.", "tokens": ["Und", "h\u00f6chs\u00b7tens", "reicht", "dein", "Leib", "dir", "eins\u00b7tens", "schlech\u00b7te", "Kr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wohlan, du elend Weib, nun sind wir auf der Stelle", "tokens": ["Wo\u00b7hlan", ",", "du", "e\u00b7lend", "Weib", ",", "nun", "sind", "wir", "auf", "der", "Stel\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "ADJD", "NN", "$,", "ADV", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wo wir zuerst uns sahn, ich, du, und dein Geselle,", "tokens": ["Wo", "wir", "zu\u00b7erst", "uns", "sahn", ",", "ich", ",", "du", ",", "und", "dein", "Ge\u00b7sel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPER", "VVFIN", "$,", "PPER", "$,", "PPER", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ich mein' den Teufel, Weib, der deine Seele reitet,", "tokens": ["Ich", "mein'", "den", "Teu\u00b7fel", ",", "Weib", ",", "der", "dei\u00b7ne", "See\u00b7le", "rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "H\u00f6r' wie sein Fl\u00fcgel rauscht, den \u00fcber dir er breitet,", "tokens": ["H\u00f6r'", "wie", "sein", "Fl\u00fc\u00b7gel", "rauscht", ",", "den", "\u00fc\u00b7ber", "dir", "er", "brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "PPOSAT", "NN", "VVFIN", "$,", "PRELS", "APPR", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Ich h\u00f6r' den dunklen Flu\u00df, es t\u00f6nt die dumpfe Welle,", "tokens": ["Ich", "h\u00f6r'", "den", "dunk\u00b7len", "Flu\u00df", ",", "es", "t\u00f6nt", "die", "dump\u00b7fe", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Du L\u00fcgnerin leb wohl, leb schlecht, hier ist die Schwelle,", "tokens": ["Du", "L\u00fcg\u00b7ne\u00b7rin", "leb", "wohl", ",", "leb", "schlecht", ",", "hier", "ist", "die", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "ADV", "$,", "VVFIN", "ADJD", "$,", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Wo sich mein reuig Herz, von dir du Hexe scheidet,", "tokens": ["Wo", "sich", "mein", "reu\u00b7ig", "Herz", ",", "von", "dir", "du", "He\u00b7xe", "schei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PPOSAT", "ADJD", "NN", "$,", "APPR", "PPER", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Verdorren mag der Fu\u00df, der je dein Bett beschreitet,", "tokens": ["Ver\u00b7dor\u00b7ren", "mag", "der", "Fu\u00df", ",", "der", "je", "dein", "Bett", "be\u00b7schrei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Ich hab' dich nie gekannt, ich hab' dich nie gesehen,", "tokens": ["Ich", "hab'", "dich", "nie", "ge\u00b7kannt", ",", "ich", "hab'", "dich", "nie", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Es war ein b\u00f6ser Traum, er mu\u00df hinuntergehen,", "tokens": ["Es", "war", "ein", "b\u00f6\u00b7ser", "Traum", ",", "er", "mu\u00df", "hin\u00b7un\u00b7ter\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Das l\u00fcderliche Buch, um das du mich betrogen,", "tokens": ["Das", "l\u00fc\u00b7der\u00b7li\u00b7che", "Buch", ",", "um", "das", "du", "mich", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUI", "ART", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Aus dem du geile Brunst f\u00fcr andrer Lust gesogen,", "tokens": ["Aus", "dem", "du", "gei\u00b7le", "Brunst", "f\u00fcr", "an\u00b7drer", "Lust", "ge\u00b7so\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Ich werfe es hinab in diese schmutz'gen Wogen,", "tokens": ["Ich", "wer\u00b7fe", "es", "hin\u00b7ab", "in", "die\u00b7se", "schmutz'\u00b7gen", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Und mit ihm werf' ich hin, was ich f\u00fcr dich gef\u00fchlt,", "tokens": ["Und", "mit", "ihm", "wer\u00b7f'", "ich", "hin", ",", "was", "ich", "f\u00fcr", "dich", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VAFIN", "PPER", "PTKVZ", "$,", "PWS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}, "line.52": {"text": "Da\u00df sich die b\u00f6se Glut, die mir das Herz zerw\u00fchlt,", "tokens": ["Da\u00df", "sich", "die", "b\u00f6\u00b7se", "Glut", ",", "die", "mir", "das", "Herz", "zer\u00b7w\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "In dieses Flusses tr\u00fcber Welle k\u00fchlt.", "tokens": ["In", "die\u00b7ses", "Flus\u00b7ses", "tr\u00fc\u00b7ber", "Wel\u00b7le", "k\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.54": {"text": "Nimm hin den Scheideku\u00df,", "tokens": ["Nimm", "hin", "den", "Schei\u00b7de\u00b7ku\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.55": {"text": "Ich geb' ihn ohn' Verdru\u00df,", "tokens": ["Ich", "geb'", "ihn", "ohn'", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.56": {"text": "Von mir sei dir verziehn,", "tokens": ["Von", "mir", "sei", "dir", "ver\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.57": {"text": "Wend' dich, zu Gott dahin,", "tokens": ["Wend'", "dich", ",", "zu", "Gott", "da\u00b7hin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "NN", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.58": {"text": "Und fleh', da\u00df er verzeih',", "tokens": ["Und", "fleh'", ",", "da\u00df", "er", "ver\u00b7zeih'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.59": {"text": "Dem S\u00fcnder steht es frei.", "tokens": ["Dem", "S\u00fcn\u00b7der", "steht", "es", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.60": {"text": "Er ist f\u00fcr dich, f\u00fcr mich, f\u00fcr alle uns gestorben,", "tokens": ["Er", "ist", "f\u00fcr", "dich", ",", "f\u00fcr", "mich", ",", "f\u00fcr", "al\u00b7le", "uns", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$,", "APPR", "PPER", "$,", "APPR", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Ich habe im Gebet mir Trost von ihm erworben.", "tokens": ["Ich", "ha\u00b7be", "im", "Ge\u00b7bet", "mir", "Trost", "von", "ihm", "er\u00b7wor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "PPER", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Ich gab des Heilands Bild in deine schn\u00f6den H\u00e4nde,", "tokens": ["Ich", "gab", "des", "Hei\u00b7lands", "Bild", "in", "dei\u00b7ne", "schn\u00f6\u00b7den", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "So bin durch dich ich auch zu einem Judas worden,", "tokens": ["So", "bin", "durch", "dich", "ich", "auch", "zu", "ei\u00b7nem", "Ju\u00b7das", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "PPER", "ADV", "APPR", "ART", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Den Herrn hab' ich verkauft, an die, die ihn ermorden,", "tokens": ["Den", "Herrn", "hab'", "ich", "ver\u00b7kauft", ",", "an", "die", ",", "die", "ihn", "er\u00b7mor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,", "APPR", "ART", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Erbarm' dich meiner Seel', und zu dem Kreuz dich wende,", "tokens": ["Er\u00b7barm'", "dich", "mei\u00b7ner", "Seel'", ",", "und", "zu", "dem", "Kreuz", "dich", "wen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$,", "KON", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "O mache, da\u00df an dir dies Bild ein Wunder tut,", "tokens": ["O", "ma\u00b7che", ",", "da\u00df", "an", "dir", "dies", "Bild", "ein", "Wun\u00b7der", "tut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "KOUS", "APPR", "PPER", "PDS", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Und da\u00df er dich erl\u00f6st mit seinem heiligen Blut,", "tokens": ["Und", "da\u00df", "er", "dich", "er\u00b7l\u00f6st", "mit", "sei\u00b7nem", "hei\u00b7li\u00b7gen", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.68": {"text": "So darf ich ruhig sein, da\u00df ich so fromme Gabe", "tokens": ["So", "darf", "ich", "ru\u00b7hig", "sein", ",", "da\u00df", "ich", "so", "from\u00b7me", "Ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VAINF", "$,", "KOUS", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "An dich, du elend Weib, so schn\u00f6d vergeudet habe,", "tokens": ["An", "dich", ",", "du", "e\u00b7lend", "Weib", ",", "so", "schn\u00f6d", "ver\u00b7geu\u00b7det", "ha\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPER", "ADJD", "NN", "$,", "ADV", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Nun wend' ich mich von dir, ich will in Friede gehn,", "tokens": ["Nun", "wend'", "ich", "mich", "von", "dir", ",", "ich", "will", "in", "Frie\u00b7de", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PRF", "APPR", "PPER", "$,", "PPER", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "Ich will unschuldig nun die Sterne wiedersehn,", "tokens": ["Ich", "will", "un\u00b7schul\u00b7dig", "nun", "die", "Ster\u00b7ne", "wie\u00b7der\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Ich will zu Gott dem Herrn um H\u00fclfe f\u00fcr dich flehn,", "tokens": ["Ich", "will", "zu", "Gott", "dem", "Herrn", "um", "H\u00fcl\u00b7fe", "f\u00fcr", "dich", "flehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "ART", "NN", "APPR", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.73": {"text": "Da\u00df dich die Gnade sein barmherzig m\u00f6g' anwehn,", "tokens": ["Da\u00df", "dich", "die", "Gna\u00b7de", "sein", "barm\u00b7her\u00b7zig", "m\u00f6g'", "an\u00b7wehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPOSAT", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.74": {"text": "Da\u00df einen Engel er, zu dir ermahnen sende,", "tokens": ["Da\u00df", "ei\u00b7nen", "En\u00b7gel", "er", ",", "zu", "dir", "er\u00b7mah\u00b7nen", "sen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "$,", "APPR", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.75": {"text": "Da\u00df er dein elend Herz wie meines zu sich wende,", "tokens": ["Da\u00df", "er", "dein", "e\u00b7lend", "Herz", "wie", "mei\u00b7nes", "zu", "sich", "wen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJD", "NN", "KOKOM", "PPOSAT", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.76": {"text": "So gehet nicht mein Schmerz, doch Leid und Lieb' zu Ende.", "tokens": ["So", "ge\u00b7het", "nicht", "mein", "Schmerz", ",", "doch", "Leid", "und", "Lieb'", "zu", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,", "KON", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}