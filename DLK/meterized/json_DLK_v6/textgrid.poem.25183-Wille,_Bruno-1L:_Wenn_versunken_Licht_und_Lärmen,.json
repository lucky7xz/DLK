{"textgrid.poem.25183": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn versunken Licht und L\u00e4rmen,", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn versunken Licht und L\u00e4rmen,", "tokens": ["Wenn", "ver\u00b7sun\u00b7ken", "Licht", "und", "L\u00e4r\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sitz ich bei der Lampe Schimmer", "tokens": ["Sitz", "ich", "bei", "der", "Lam\u00b7pe", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Oft im n\u00e4chtlich stillen Zimmer,", "tokens": ["Oft", "im", "n\u00e4cht\u00b7lich", "stil\u00b7len", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo Gedanken mich umschw\u00e4rmen,", "tokens": ["Wo", "Ge\u00b7dan\u00b7ken", "mich", "um\u00b7schw\u00e4r\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf ein altes Buch gesenkt", "tokens": ["Auf", "ein", "al\u00b7tes", "Buch", "ge\u00b7senkt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meine Stirne ernstbeschwert;", "tokens": ["Mei\u00b7ne", "Stir\u00b7ne", "ernst\u00b7be\u00b7schwert", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "K\u00fchlung mir der Nachtwind schenkt", "tokens": ["K\u00fch\u00b7lung", "mir", "der", "Nacht\u00b7wind", "schenkt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Durch das Fenster unverwehrt.", "tokens": ["Durch", "das", "Fens\u00b7ter", "un\u00b7ver\u00b7wehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wundersame Lieder sausen", "tokens": ["Wun\u00b7der\u00b7sa\u00b7me", "Lie\u00b7der", "sau\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Drau\u00dfen Wind und Wald und Wetter,", "tokens": ["Drau\u00b7\u00dfen", "Wind", "und", "Wald", "und", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es wehn des Buches Bl\u00e4tter;", "tokens": ["Und", "es", "wehn", "des", "Bu\u00b7ches", "Bl\u00e4t\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welch ein feierliches Brausen!", "tokens": ["Welch", "ein", "fei\u00b7er\u00b7li\u00b7ches", "Brau\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ich lausche und ich lausche,", "tokens": ["Und", "ich", "lau\u00b7sche", "und", "ich", "lau\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "KON", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "An ein Ufer fern entr\u00fcckt. \u2013", "tokens": ["An", "ein", "U\u00b7fer", "fern", "ent\u00b7r\u00fcckt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Rausche Strom der Wahrheit, rausche!", "tokens": ["Rau\u00b7sche", "Strom", "der", "Wahr\u00b7heit", ",", "rau\u00b7sche", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Meine Seele lauscht entz\u00fcckt.", "tokens": ["Mei\u00b7ne", "See\u00b7le", "lauscht", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}