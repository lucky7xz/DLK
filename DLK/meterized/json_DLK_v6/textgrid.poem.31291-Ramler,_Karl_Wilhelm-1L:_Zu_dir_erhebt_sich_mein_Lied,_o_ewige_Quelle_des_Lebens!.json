{"textgrid.poem.31291": {"metadata": {"author": {"name": "Ramler, Karl Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zu dir erhebt sich mein Lied, o ewige Quelle des Lebens!", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu dir erhebt sich mein Lied, o ewige Quelle des Lebens!", "tokens": ["Zu", "dir", "er\u00b7hebt", "sich", "mein", "Lied", ",", "o", "e\u00b7wi\u00b7ge", "Quel\u00b7le", "des", "Le\u00b7bens", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PRF", "PPOSAT", "NN", "$,", "FM", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "O, du von den Lippen danksagender Weisen Jehova gegr\u00fc\u00dfet,", "tokens": ["O", ",", "du", "von", "den", "Lip\u00b7pen", "dank\u00b7sa\u00b7gen\u00b7der", "Wei\u00b7sen", "Je\u00b7ho\u00b7va", "ge\u00b7gr\u00fc\u00b7\u00dfet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "APPR", "ART", "NN", "ADJA", "NN", "NE", "VVPP", "$,"], "meter": "+-+-+--+--+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Und Oromazes und ", "tokens": ["Und", "O\u00b7ro\u00b7ma\u00b7zes", "und"], "token_info": ["word", "word", "word"], "pos": ["KON", "NE", "KON"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der hier vom Grase rollt, gleich gro\u00df in der Sonne, die rastlos", "tokens": ["Der", "hier", "vom", "Gra\u00b7se", "rollt", ",", "gleich", "gro\u00df", "in", "der", "Son\u00b7ne", ",", "die", "rast\u00b7los"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADV", "APPRART", "NN", "VVFIN", "$,", "ADV", "ADJD", "APPR", "ART", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Rings um sich an goldenen Seilen gl\u00fcckselige Weiten herumf\u00fchrt;", "tokens": ["Rings", "um", "sich", "an", "gol\u00b7de\u00b7nen", "Sei\u00b7len", "gl\u00fcck\u00b7se\u00b7li\u00b7ge", "Wei\u00b7ten", "her\u00b7um\u00b7f\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "APPR", "ADJA", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "----+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Im Wurme, der einen best\u00e4ubten Erntetag lebt, und im Cherub,", "tokens": ["Im", "Wur\u00b7me", ",", "der", "ei\u00b7nen", "be\u00b7st\u00e4ub\u00b7ten", "Ern\u00b7te\u00b7tag", "lebt", ",", "und", "im", "Che\u00b7rub", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$,", "KON", "APPRART", "NE", "$,"], "meter": "-+--+--+-+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Der alle Naturen durchforscht seit seiner undenklichen Jugend,", "tokens": ["Der", "al\u00b7le", "Na\u00b7tu\u00b7ren", "durch\u00b7forscht", "seit", "sei\u00b7ner", "un\u00b7denk\u00b7li\u00b7chen", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+--+--+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Und viele Glieder bereites an der Kette der Wesen verkn\u00fcpft sieht,", "tokens": ["Und", "vie\u00b7le", "Glie\u00b7der", "be\u00b7rei\u00b7tes", "an", "der", "Ket\u00b7te", "der", "We\u00b7sen", "ver\u00b7kn\u00fcpft", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJA", "APPR", "ART", "NN", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+--+-+-+--+--++", "measure": "iambic.octa.plus.relaxed"}, "line.9": {"text": "Er selbst, der Oberste: doch in deiner Gr\u00f6\u00dfe versinket, \u2013", "tokens": ["Er", "selbst", ",", "der", "O\u00b7bers\u00b7te", ":", "doch", "in", "dei\u00b7ner", "Gr\u00f6\u00b7\u00dfe", "ver\u00b7sin\u00b7ket", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADV", "$,", "ART", "ADJA", "$.", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,", "$("], "meter": "-+----+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Wie soll ich in menschlicher Rede den Kindern der Erde dich nennen, \u2013", "tokens": ["Wie", "soll", "ich", "in", "menschli\u00b7cher", "Re\u00b7de", "den", "Kin\u00b7dern", "der", "Er\u00b7de", "dich", "nen\u00b7nen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "ADJA", "NN", "ART", "NN", "ART", "NN", "PPER", "VVINF", "$,", "$("], "meter": "-+--+-+--+--+-+--", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Dich, deines unendlichen Weltraums allbelebende F\u00fclle? \u2013 \u2013", "tokens": ["Dich", ",", "dei\u00b7nes", "un\u00b7end\u00b7li\u00b7chen", "Welt\u00b7raums", "all\u00b7be\u00b7le\u00b7ben\u00b7de", "F\u00fcl\u00b7le", "?", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "$,", "PPOSAT", "ADJA", "NN", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+--+--+-+-+--+-", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Mit Schaudern versenkt sich in ihn mein Geist in den Tempeln der W\u00e4lder.", "tokens": ["Mit", "Schau\u00b7dern", "ver\u00b7senkt", "sich", "in", "ihn", "mein", "Geist", "in", "den", "Tem\u00b7peln", "der", "W\u00e4l\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "APPR", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-++-+--+--+-", "measure": "iambic.septa.relaxed"}, "line.13": {"text": "Auf himmelstrebenden Felsen am Rande der grausenden Tiefe!", "tokens": ["Auf", "him\u00b7mel\u00b7stre\u00b7ben\u00b7den", "Fel\u00b7sen", "am", "Ran\u00b7de", "der", "grau\u00b7sen\u00b7den", "Tie\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Und o! wie verschwindet mir dann die sinnliche Freude! wie werden", "tokens": ["Und", "o", "!", "wie", "ver\u00b7schwin\u00b7det", "mir", "dann", "die", "sinn\u00b7li\u00b7che", "Freu\u00b7de", "!", "wie", "wer\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "FM", "$.", "PWAV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$.", "PWAV", "VAINF"], "meter": "-+--+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Mir alle Begierden erh\u00f6ht. \u2013 Du Weltgeist, hier steh' ich verloren", "tokens": ["Mir", "al\u00b7le", "Be\u00b7gier\u00b7den", "er\u00b7h\u00f6ht", ".", "\u2013", "Du", "Welt\u00b7geist", ",", "hier", "steh'", "ich", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PIAT", "NN", "VVPP", "$.", "$(", "PPER", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "VVPP"], "meter": "-+--+--+-+--+--+-", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Auf einem Staube des Ganzen, und breite die H\u00e4nde zu dir aus:", "tokens": ["Auf", "ei\u00b7nem", "Stau\u00b7be", "des", "Gan\u00b7zen", ",", "und", "brei\u00b7te", "die", "H\u00e4n\u00b7de", "zu", "dir", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+--+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.17": {"text": "Erh\u00e4ltst du, wenn einst dies zarte Gewebe des Leibes sich aufl\u00f6s't,", "tokens": ["Er\u00b7h\u00e4ltst", "du", ",", "wenn", "einst", "dies", "zar\u00b7te", "Ge\u00b7we\u00b7be", "des", "Lei\u00b7bes", "sich", "auf\u00b7l\u00f6s't", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ADV", "PDS", "ADJA", "NN", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-++-+--+--+--+-", "measure": "iambic.septa.relaxed"}, "line.18": {"text": "Ein h\u00f6heres Antheil von mir, so soll die Bewunderung deiner", "tokens": ["Ein", "h\u00f6\u00b7he\u00b7res", "An\u00b7theil", "von", "mir", ",", "so", "soll", "die", "Be\u00b7wun\u00b7de\u00b7rung", "dei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "$,", "ADV", "VMFIN", "ART", "NN", "PPOSAT"], "meter": "-+--+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Mein langes Gesch\u00e4ft verbleiben, mein langer Gesang .....", "tokens": ["Mein", "lan\u00b7ges", "Ge\u00b7sch\u00e4ft", "ver\u00b7blei\u00b7ben", ",", "mein", "lan\u00b7ger", "Ge\u00b7sang", "....."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}