{"dta.poem.23724": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An die Stadt Prag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und tausend Siegesbogen,", "tokens": ["Und", "tau\u00b7send", "Sie\u00b7ges\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht eher eingezogen.", "tokens": ["Nicht", "e\u00b7her", "ein\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Prag, spare Muth und Hitze,", "tokens": ["Prag", ",", "spa\u00b7re", "Muth", "und", "Hit\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es tobt schon sein Gesch\u00fctze!", "tokens": ["Es", "tobt", "schon", "sein", "Ge\u00b7sch\u00fct\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ach, tr\u00e4fen doch die Waffen,", "tokens": ["Ach", ",", "tr\u00e4\u00b7fen", "doch", "die", "Waf\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nur deine faule Pfaffen,", "tokens": ["Nur", "dei\u00b7ne", "fau\u00b7le", "Pfaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ach, m\u00f6chtet, ihr Kanonen,", "tokens": ["Ach", ",", "m\u00f6ch\u00b7tet", ",", "ihr", "Ka\u00b7no\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "VMFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die M\u00e4dchen nur verschonen!", "tokens": ["Die", "M\u00e4d\u00b7chen", "nur", "ver\u00b7scho\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}