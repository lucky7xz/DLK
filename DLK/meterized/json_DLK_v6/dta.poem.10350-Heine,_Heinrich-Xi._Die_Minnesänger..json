{"dta.poem.10350": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Xi.  \n  Die Minnes\u00e4nger.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Zu dem Wettgesange schreiten", "tokens": ["Zu", "dem", "Wett\u00b7ge\u00b7san\u00b7ge", "schrei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Minnes\u00e4nger jetzt herbei;", "tokens": ["Min\u00b7ne\u00b7s\u00e4n\u00b7ger", "jetzt", "her\u00b7bei", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ei, das giebt ein seltsam Streiten,", "tokens": ["Ei", ",", "das", "giebt", "ein", "selt\u00b7sam", "Strei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein gar seltsames Turnei!", "tokens": ["Ein", "gar", "selt\u00b7sa\u00b7mes", "Tur\u00b7nei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Phantasie, die sch\u00e4umend wilde,", "tokens": ["Phan\u00b7ta\u00b7sie", ",", "die", "sch\u00e4u\u00b7mend", "wil\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist des Minnes\u00e4ngers Pferd,", "tokens": ["Ist", "des", "Min\u00b7ne\u00b7s\u00e4n\u00b7gers", "Pferd", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Kunst dient ihm zum Schilde,", "tokens": ["Und", "die", "Kunst", "dient", "ihm", "zum", "Schil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "--+---+-", "measure": "anapaest.init"}, "line.4": {"text": "Und das Wort, das ist sein Schwert.", "tokens": ["Und", "das", "Wort", ",", "das", "ist", "sein", "Schwert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "H\u00fcbsche Damen schauen munter", "tokens": ["H\u00fcb\u00b7sche", "Da\u00b7men", "schau\u00b7en", "mun\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vom beteppichten Balkon',", "tokens": ["Vom", "be\u00b7tep\u00b7pich\u00b7ten", "Bal\u00b7kon'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Doch die rechte ist nicht drunter", "tokens": ["Doch", "die", "rech\u00b7te", "ist", "nicht", "drun\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "VAFIN", "PTKNEG", "PAV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit des Sieges Myrthenkron'.", "tokens": ["Mit", "des", "Sie\u00b7ges", "Myr\u00b7then\u00b7kron'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Andre Leute, wenn sie springen", "tokens": ["And\u00b7re", "Leu\u00b7te", ",", "wenn", "sie", "sprin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KOUS", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die Schranken, sind gesund;", "tokens": ["In", "die", "Schran\u00b7ken", ",", "sind", "ge\u00b7sund", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber Minnes\u00e4nger bringen", "tokens": ["A\u00b7ber", "Min\u00b7ne\u00b7s\u00e4n\u00b7ger", "brin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort schon mit die Todeswund'.", "tokens": ["Dort", "schon", "mit", "die", "To\u00b7des\u00b7wund'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Und wem dort am besten dringen", "tokens": ["Und", "wem", "dort", "am", "bes\u00b7ten", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "PTKA", "ADJD", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Liedes Blutstr\u00f6m' aus der Brust,", "tokens": ["Lie\u00b7des", "Blut\u00b7str\u00f6m'", "aus", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der wird's beste Lob erringen,", "tokens": ["Der", "wird's", "bes\u00b7te", "Lob", "er\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und sein Weh giebt Andern Lust.", "tokens": ["Und", "sein", "Weh", "giebt", "An\u00b7dern", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}