{"dta.poem.13828": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Soldatenlied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Das Fu\u00dfvolk kommt da geschritten,", "tokens": ["Das", "Fu\u00df\u00b7volk", "kommt", "da", "ge\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Trommeln wirbeln voran,", "tokens": ["Die", "Trom\u00b7meln", "wir\u00b7beln", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Fahne in ihrer Mitten", "tokens": ["Die", "Fah\u00b7ne", "in", "ih\u00b7rer", "Mit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Weht \u00fcber den gr\u00fcnen Plan,", "tokens": ["Weht", "\u00fc\u00b7ber", "den", "gr\u00fc\u00b7nen", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sie prangt in schneewei\u00dfem Kleide", "tokens": ["Sie", "prangt", "in", "schnee\u00b7wei\u00b7\u00dfem", "Klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Als wie eine milde Braut,", "tokens": ["Als", "wie", "ei\u00b7ne", "mil\u00b7de", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die giebt dem hohe Freude,", "tokens": ["Die", "giebt", "dem", "ho\u00b7he", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wen Gott ihr angetraut.", "tokens": ["Wen", "Gott", "ihr", "an\u00b7ge\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Sie haben sie recht umschlossen,", "tokens": ["Sie", "ha\u00b7ben", "sie", "recht", "um\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Dicht Mann an Mann ger\u00fcckt,", "tokens": ["Dicht", "Mann", "an", "Mann", "ge\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "So ziehen die Kriegsgenossen", "tokens": ["So", "zie\u00b7hen", "die", "Kriegs\u00b7ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Streng, schweigend und ungeschm\u00fcckt,", "tokens": ["Streng", ",", "schwei\u00b7gend", "und", "un\u00b7ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "++--+-+", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Wie Gottes dunkeler Wille,", "tokens": ["Wie", "Got\u00b7tes", "dun\u00b7ke\u00b7ler", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Wie ein Gewitter schwer,", "tokens": ["Wie", "ein", "Ge\u00b7wit\u00b7ter", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Da wird es ringsum so stille,", "tokens": ["Da", "wird", "es", "ring\u00b7sum", "so", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Der Tod nur blitzt hin und her.", "tokens": ["Der", "Tod", "nur", "blitzt", "hin", "und", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}