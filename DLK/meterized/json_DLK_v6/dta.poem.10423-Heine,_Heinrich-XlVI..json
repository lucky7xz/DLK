{"dta.poem.10423": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XlVI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Am leuchtenden Sommermorgen", "tokens": ["Am", "leuch\u00b7ten\u00b7den", "Som\u00b7mer\u00b7mor\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Geh' ich im Garten herum.", "tokens": ["Geh'", "ich", "im", "Gar\u00b7ten", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Es fl\u00fcstern und sprechen die Blumen,", "tokens": ["Es", "fl\u00fcs\u00b7tern", "und", "spre\u00b7chen", "die", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ich aber wandle stumm.", "tokens": ["Ich", "a\u00b7ber", "wand\u00b7le", "stumm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es fl\u00fcstern und sprechen die Blumen,", "tokens": ["Es", "fl\u00fcs\u00b7tern", "und", "spre\u00b7chen", "die", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und schau'n mitleidig mich an:", "tokens": ["Und", "schau'n", "mit\u00b7lei\u00b7dig", "mich", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sey unserer Schwester nicht b\u00f6se,", "tokens": ["Sey", "un\u00b7se\u00b7rer", "Schwes\u00b7ter", "nicht", "b\u00f6\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Du trauriger, blasser Mann.", "tokens": ["Du", "trau\u00b7ri\u00b7ger", ",", "blas\u00b7ser", "Mann", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}