{"dta.poem.664": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Zusatz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Geist brennt/ mein Hertze bricht/ ", "tokens": ["Mein", "Geist", "brennt", "/", "mein", "Hert\u00b7ze", "bricht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$(", "PPOSAT", "VVFIN", "VVFIN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wenn ich Ephraim bedencke.", "tokens": ["Wenn", "ich", "Eph\u00b7raim", "be\u00b7den\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn ich meiner Augen li", "tokens": ["Wenn", "ich", "mei\u00b7ner", "Au\u00b7gen", "li"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auff sein threnend Auge lencke.", "tokens": ["Auff", "sein", "thre\u00b7nend", "Au\u00b7ge", "len\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob ich mich auch zuweilen zornig stelle: ", "tokens": ["Ob", "ich", "mich", "auch", "zu\u00b7wei\u00b7len", "zor\u00b7nig", "stel\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Vnd jhn mit finsternus vnd schwartzen schmertzen decke:", "tokens": ["Vnd", "jhn", "mit", "fins\u00b7ter\u00b7nus", "vnd", "schwart\u00b7zen", "schmert\u00b7zen", "de\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NE", "KON", "VVINF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Vnd mit geschwindein blitz/ vnnd harten donner schrecke:", "tokens": ["Vnd", "mit", "ge\u00b7schwin\u00b7de\u00b7in", "blitz", "/", "vnnd", "har\u00b7ten", "don\u00b7ner", "schre\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "NE", "$(", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Jm augenblick wird dennoch alles helle. ", "tokens": ["Jm", "au\u00b7gen\u00b7blick", "wird", "den\u00b7noch", "al\u00b7les", "hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADV", "PIAT", "ADJA", "$."], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Ich mus mich vber jhn erbarmen/ ", "tokens": ["Ich", "mus", "mich", "vber", "jhn", "er\u00b7bar\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Ich mus jhn mit den starcken armen", "tokens": ["Ich", "mus", "jhn", "mit", "den", "star\u00b7cken", "ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "ART", "ADJA", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Aus seiner angst vnd aus dem bande/", "tokens": ["Aus", "sei\u00b7ner", "angst", "vnd", "aus", "dem", "ban\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Aus herber noch vnd harter schande/", "tokens": ["Aus", "her\u00b7ber", "noch", "vnd", "har\u00b7ter", "schan\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "KON", "ADJA", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Aus Kercker/ grufft vnd rodt/ au\u00df glut vnd Wasser f\u00fchren.", "tokens": ["Aus", "Ker\u00b7cker", "/", "grufft", "vnd", "rodt", "/", "au\u00df", "glut", "vnd", "Was\u00b7ser", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "VVFIN", "KON", "ADJD", "$(", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Vnd jhn mit newem schmuck/ vnd newer ehren zihren.", "tokens": ["Vnd", "jhn", "mit", "ne\u00b7wem", "schmuck", "/", "vnd", "ne\u00b7wer", "eh\u00b7ren", "zih\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "$(", "KON", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}}}}}