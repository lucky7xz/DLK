{"textgrid.poem.37043": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Narci\u00df und Echo,", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Fr\u00e4ulein Echo sah einmal", "tokens": ["Das", "Fr\u00e4u\u00b7lein", "E\u00b7cho", "sah", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Ahnherrn der Narci\u00dfen,", "tokens": ["Den", "Ahn\u00b7herrn", "der", "Nar\u00b7ci\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der manches Jungfernherzgen stahl,", "tokens": ["Der", "man\u00b7ches", "Jung\u00b7fern\u00b7herz\u00b7gen", "stahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In gr\u00fcnen Finsterni\u00dfen,", "tokens": ["In", "gr\u00fc\u00b7nen", "Fins\u00b7ter\u00b7ni\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sich einer Badequelle nahn.", "tokens": ["Sich", "ei\u00b7ner", "Ba\u00b7deq\u00b7uel\u00b7le", "nahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Stracks schielten Ihro Gnaden,", "tokens": ["Stracks", "schiel\u00b7ten", "Ih\u00b7ro", "Gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Als sie den sch\u00f6nen J\u00fcngling sahn,", "tokens": ["Als", "sie", "den", "sch\u00f6\u00b7nen", "J\u00fcng\u00b7ling", "sahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nach seinen vollen Waden.", "tokens": ["Nach", "sei\u00b7nen", "vol\u00b7len", "Wa\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der sechzehn Ahnen Dunst verschwand", "tokens": ["Der", "sech\u00b7zehn", "Ah\u00b7nen", "Dunst", "ver\u00b7schwand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "CARD", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gemach aus ihrem Hirne,", "tokens": ["Ge\u00b7mach", "aus", "ih\u00b7rem", "Hir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie bot ihm buhlerisch die Hand,", "tokens": ["Sie", "bot", "ihm", "buh\u00b7le\u00b7risch", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie eine B\u00fcrgerdirne.", "tokens": ["Wie", "ei\u00b7ne", "B\u00fcr\u00b7ger\u00b7dir\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Narci\u00df dreht ihr den R\u00fccken zu,", "tokens": ["Nar\u00b7ci\u00df", "dreht", "ihr", "den", "R\u00fc\u00b7cken", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Und schreit ihr in die Ohren:", "tokens": ["Und", "schreit", "ihr", "in", "die", "Oh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mamsellchen, la\u00df sie mich in Ruh,", "tokens": ["Mam\u00b7sell\u00b7chen", ",", "la\u00df", "sie", "mich", "in", "Ruh", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie hat hier nichts verlohren.", "tokens": ["Sie", "hat", "hier", "nichts", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Drauf schlich das Fr\u00e4ulein in den Wald,", "tokens": ["Drauf", "schlich", "das", "Fr\u00e4u\u00b7lein", "in", "den", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Leben zu verweinen,", "tokens": ["Ihr", "Le\u00b7ben", "zu", "ver\u00b7wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie starb, und ihre Stimme hallt", "tokens": ["Sie", "starb", ",", "und", "ih\u00b7re", "Stim\u00b7me", "hallt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch itzt in unsern Hainen.", "tokens": ["Noch", "itzt", "in", "un\u00b7sern", "Hai\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch soll sie, wie die Rede geht,", "tokens": ["Doch", "soll", "sie", ",", "wie", "die", "Re\u00b7de", "geht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Eh sie im Herrn entschlafen,", "tokens": ["Eh", "sie", "im", "Herrn", "ent\u00b7schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die G\u00f6tter haben angefleht,", "tokens": ["Die", "G\u00f6t\u00b7ter", "ha\u00b7ben", "an\u00b7ge\u00b7fleht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den J\u00fcngling zu bestrafen.", "tokens": ["Den", "J\u00fcng\u00b7ling", "zu", "be\u00b7stra\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der letzte Seufzer ward erf\u00fcllt.", "tokens": ["Der", "letz\u00b7te", "Seuf\u00b7zer", "ward", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er sah in einer Quelle,", "tokens": ["Er", "sah", "in", "ei\u00b7ner", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die silbern rann, sein eigen Bild,", "tokens": ["Die", "sil\u00b7bern", "rann", ",", "sein", "ei\u00b7gen", "Bild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und liebt' es auf der Stelle.", "tokens": ["Und", "liebt'", "es", "auf", "der", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Am Ufer lag er, wie behext,", "tokens": ["Am", "U\u00b7fer", "lag", "er", ",", "wie", "be\u00b7hext", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und flo\u00df in Klagen \u00fcber.", "tokens": ["Und", "flo\u00df", "in", "Kla\u00b7gen", "\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sein Pfarrer las ihm oft den Text,", "tokens": ["Sein", "Pfar\u00b7rer", "las", "ihm", "oft", "den", "Text", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit vielem Ernst, dar\u00fcber.", "tokens": ["Mit", "vie\u00b7lem", "Ernst", ",", "da\u00b7r\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIS", "NE", "$,", "PAV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was halfs? Narci\u00df, der Starrkopf, blieb", "tokens": ["Was", "halfs", "?", "Nar\u00b7ci\u00df", ",", "der", "Starr\u00b7kopf", ",", "blieb"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWS", "VVFIN", "$.", "NN", "$,", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bey seinen sieben Sinnen,", "tokens": ["Bey", "sei\u00b7nen", "sie\u00b7ben", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und lief, wie ein verjagter Dieb,", "tokens": ["Und", "lief", ",", "wie", "ein", "ver\u00b7jag\u00b7ter", "Dieb", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Gucken zu beginnen,", "tokens": ["Sein", "Gu\u00b7cken", "zu", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sobald die liebe Sonne schien,", "tokens": ["So\u00b7bald", "die", "lie\u00b7be", "Son\u00b7ne", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zum Spiegel seiner Quelle,", "tokens": ["Zum", "Spie\u00b7gel", "sei\u00b7ner", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und sah, bedeckt vom Baldachin", "tokens": ["Und", "sah", ",", "be\u00b7deckt", "vom", "Bal\u00b7da\u00b7ch\u00b7in"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des Hains, in ", "tokens": ["Des", "Hains", ",", "in"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.6": {"line.1": {"text": "Er machte, wenn er nahe war,", "tokens": ["Er", "mach\u00b7te", ",", "wenn", "er", "na\u00b7he", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verliebte Reverenze,", "tokens": ["Ver\u00b7lieb\u00b7te", "Re\u00b7ve\u00b7ren\u00b7ze", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bot dem Phantom Geschenke dar,", "tokens": ["Bot", "dem", "Phan\u00b7tom", "Ge\u00b7schen\u00b7ke", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Bald Str\u00e4u\u00dfer, und bald Kr\u00e4nze.", "tokens": ["Bald", "Str\u00e4u\u00b7\u00dfer", ",", "und", "bald", "Kr\u00e4n\u00b7ze", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "KON", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er reichte seiner Abg\u00f6ttin", "tokens": ["Er", "reich\u00b7te", "sei\u00b7ner", "Ab\u00b7g\u00f6t\u00b7tin"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Einst eine Purpurrose.", "tokens": ["Einst", "ei\u00b7ne", "Pur\u00b7pur\u00b7ro\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sie hielt ihm auch ein R\u00f6schen hin,", "tokens": ["Sie", "hielt", "ihm", "auch", "ein", "R\u00f6\u00b7schen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und l\u00e4chelte, die Lose.", "tokens": ["Und", "l\u00e4\u00b7chel\u00b7te", ",", "die", "Lo\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sein R\u00f6schen fiel ihm in den Bach,", "tokens": ["Sein", "R\u00f6\u00b7schen", "fiel", "ihm", "in", "den", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich wei\u00df nicht, wie's gekommen,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wie's", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Stracks fiel das andre R\u00f6schen nach,", "tokens": ["Stracks", "fiel", "das", "and\u00b7re", "R\u00f6\u00b7schen", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch kams nicht angeschwommen.", "tokens": ["Doch", "kams", "nicht", "an\u00b7ge\u00b7schwom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er gab dem Bache Ku\u00df auf Ku\u00df.", "tokens": ["Er", "gab", "dem", "Ba\u00b7che", "Ku\u00df", "auf", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So liebt' er, wie Poeten,", "tokens": ["So", "liebt'", "er", ",", "wie", "Po\u00b7et\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ein Ideal, fern vom Genu\u00df", "tokens": ["Ein", "I\u00b7deal", ",", "fern", "vom", "Ge\u00b7nu\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADJD", "APPRART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Und den Realit\u00e4ten.", "tokens": ["Und", "den", "Re\u00b7a\u00b7li\u00b7t\u00e4\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "Drauf macht' er, im Gehirn verr\u00fcckt,", "tokens": ["Drauf", "macht'", "er", ",", "im", "Ge\u00b7hirn", "ver\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Ding noch immer bunter,", "tokens": ["Das", "Ding", "noch", "im\u00b7mer", "bun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sprang, nachdem er gnug geguckt,", "tokens": ["Und", "sprang", ",", "nach\u00b7dem", "er", "gnug", "ge\u00b7gu