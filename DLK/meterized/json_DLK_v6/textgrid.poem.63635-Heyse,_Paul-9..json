{"textgrid.poem.63635": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Leben ist ein Meer voll wilder Klippen,", "tokens": ["Das", "Le\u00b7ben", "ist", "ein", "Meer", "voll", "wil\u00b7der", "Klip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Fischblut gilt es glatt sich durchzuwinden,", "tokens": ["Mit", "Fischblut", "gilt", "es", "glatt", "sich", "durch\u00b7zu\u00b7win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "VVFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Niemals sein Herz zu tragen auf den Lippen,", "tokens": ["Nie\u00b7mals", "sein", "Herz", "zu", "tra\u00b7gen", "auf", "den", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Niemals an andrer Gl\u00fcck sein Herz zu binden.", "tokens": ["Nie\u00b7mals", "an", "an\u00b7drer", "Gl\u00fcck", "sein", "Herz", "zu", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Du lerntest viel zu fr\u00fch an andre denken,", "tokens": ["Du", "lern\u00b7test", "viel", "zu", "fr\u00fch", "an", "and\u00b7re", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKA", "ADJD", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An ihrem Wohl und Weh dich freun und kr\u00e4nken.", "tokens": ["An", "ih\u00b7rem", "Wohl", "und", "Weh", "dich", "freun", "und", "kr\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ach, viel zu fr\u00fche singst du an zu lieben:", "tokens": ["Ach", ",", "viel", "zu", "fr\u00fc\u00b7he", "singst", "du", "an", "zu", "lie\u00b7ben", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "APPR", "ADJA", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du w\u00e4rst nicht lang ein froher Mensch geblieben!", "tokens": ["Du", "w\u00e4rst", "nicht", "lang", "ein", "fro\u00b7her", "Mensch", "ge\u00b7blie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}