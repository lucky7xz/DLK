{"textgrid.poem.49283": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "De\u00df Propheten Jeremias Gebett", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Herr, bedencke doch, Herr, nim dir doch zu Hertzen", "tokens": ["O", "Herr", ",", "be\u00b7den\u00b7cke", "doch", ",", "Herr", ",", "nim", "dir", "doch", "zu", "Hert\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVFIN", "ADV", "$,", "NN", "$,", "VVIMP", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Leyd, so uns betrifft, schau' unsre Schmach und Schmertzen", "tokens": ["Das", "Leyd", ",", "so", "uns", "be\u00b7tr\u00b7ifft", ",", "schau'", "uns\u00b7re", "Schmach", "und", "Schmert\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+----+-+-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Mit Vatteraugen an, sieh' unser g\u00fcld'nes Land,", "tokens": ["Mit", "Vat\u00b7ter\u00b7au\u00b7gen", "an", ",", "sieh'", "un\u00b7ser", "g\u00fcld'\u00b7nes", "Land", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "VVIMP", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Milch und Honig trug, ist nun in frembder Hand", "tokens": ["Das", "Milch", "und", "Ho\u00b7nig", "trug", ",", "ist", "nun", "in", "fremb\u00b7der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$,", "VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und unsre H\u00e4user auch; wir sind zu Waisen worden", "tokens": ["Und", "uns\u00b7re", "H\u00e4u\u00b7ser", "auch", ";", "wir", "sind", "zu", "Wai\u00b7sen", "wor\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADV", "$.", "PPER", "VAFIN", "APPR", "NN", "VAPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und unsre M\u00fctter sind im armen Widwen Orden,", "tokens": ["Und", "uns\u00b7re", "M\u00fct\u00b7ter", "sind", "im", "ar\u00b7men", "Wid\u00b7wen", "Or\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPRART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie haben keinen Mann, wir keine V\u00e4tter nicht,", "tokens": ["Sie", "ha\u00b7ben", "kei\u00b7nen", "Mann", ",", "wir", "kei\u00b7ne", "V\u00e4t\u00b7ter", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PPER", "PIAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und sind ohn H\u00fclff' und Trost; wann Wasser uns gebricht,", "tokens": ["Und", "sind", "ohn", "H\u00fclff'", "und", "Trost", ";", "wann", "Was\u00b7ser", "uns", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "KON", "NN", "$.", "PWAV", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das dennoch unser ist, so lassen wir es bringen", "tokens": ["Das", "den\u00b7noch", "un\u00b7ser", "ist", ",", "so", "las\u00b7sen", "wir", "es", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADJD", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Umb unser eygnes Geld, und m\u00fcssen Holtz erzwingen,", "tokens": ["Umb", "un\u00b7ser", "eyg\u00b7nes", "Geld", ",", "und", "m\u00fcs\u00b7sen", "Holtz", "er\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "$,", "KON", "VMFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das uns im Pusche w\u00e4chst, umb grossen Werth und Prei\u00df.", "tokens": ["Das", "uns", "im", "Pu\u00b7sche", "w\u00e4chst", ",", "umb", "gros\u00b7sen", "Werth", "und", "Prei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPRART", "NN", "VVFIN", "$,", "KOUI", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Man treibt uns \u00fcber Hal\u00df, und wann wir allen Flei\u00df", "tokens": ["Man", "treibt", "uns", "\u00fc\u00b7ber", "Hal\u00df", ",", "und", "wann", "wir", "al\u00b7len", "Flei\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "NN", "$,", "KON", "PWAV", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Zur Arbeit angewandt, so da\u00df wir fast erliegen,", "tokens": ["Zur", "Ar\u00b7beit", "an\u00b7ge\u00b7wandt", ",", "so", "da\u00df", "wir", "fast", "er\u00b7lie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "ADV", "KOUS", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Jedennoch sind wir faul und thun nicht nach Gen\u00fcgen.", "tokens": ["Je\u00b7den\u00b7noch", "sind", "wir", "faul", "und", "thun", "nicht", "nach", "Ge\u00b7n\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "VVFIN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wir m\u00fcssen jetzt au\u00df Noth Egypten dienstbar sein", "tokens": ["Wir", "m\u00fcs\u00b7sen", "jetzt", "au\u00df", "Noth", "E\u00b7gyp\u00b7ten", "dienst\u00b7bar", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "NN", "NE", "ADJD", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und Assur Gutes thun, damit uns Brot allein", "tokens": ["Und", "As\u00b7sur", "Gu\u00b7tes", "thun", ",", "da\u00b7mit", "uns", "Brot", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "NN", "VVINF", "$,", "KOUS", "PPER", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Zu essen wird gereicht; es wird an uns gerochen", "tokens": ["Zu", "es\u00b7sen", "wird", "ge\u00b7reicht", ";", "es", "wird", "an", "uns", "ge\u00b7ro\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "VAFIN", "VVPP", "$.", "PPER", "VAFIN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Was unsre V\u00e4tter schon f\u00fcr langer Zeit verbrochen.", "tokens": ["Was", "uns\u00b7re", "V\u00e4t\u00b7ter", "schon", "f\u00fcr", "lan\u00b7ger", "Zeit", "ver\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADV", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Sie schlaffen unbesorgt und dencken nicht mehr her,", "tokens": ["Sie", "schlaf\u00b7fen", "un\u00b7be\u00b7sorgt", "und", "den\u00b7cken", "nicht", "mehr", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ihr Uebel aber wird uns Kindern nun zu schwer.", "tokens": ["Ihr", "Ue\u00b7bel", "a\u00b7ber", "wird", "uns", "Kin\u00b7dern", "nun", "zu", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "PPER", "NN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wir dienen Knechten jetzt, Schand \u00fcber alle Schande,", "tokens": ["Wir", "die\u00b7nen", "Knech\u00b7ten", "jetzt", ",", "Schand", "\u00fc\u00b7ber", "al\u00b7le", "Schan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "$,", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und niemand rettet uns in diesem gantzen Lande", "tokens": ["Und", "nie\u00b7mand", "ret\u00b7tet", "uns", "in", "die\u00b7sem", "gant\u00b7zen", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "PPER", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Von ihrer Dienstbarkeit; wir holen unser Brod", "tokens": ["Von", "ih\u00b7rer", "Dienst\u00b7bar\u00b7keit", ";", "wir", "ho\u00b7len", "un\u00b7ser", "Brod"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Mit Aengsten und Gefahr, weil beydes Schwerd und Todt", "tokens": ["Mit", "A\u00b7engs\u00b7ten", "und", "Ge\u00b7fahr", ",", "weil", "bey\u00b7des", "Schwerd", "und", "Todt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "KOUS", "PIS", "NN", "KON", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.25": {"text": "Hier in der W\u00fcsten sind, da\u00df Fleisch und Beine knacken,", "tokens": ["Hier", "in", "der", "W\u00fcs\u00b7ten", "sind", ",", "da\u00df", "Fleisch", "und", "Bei\u00b7ne", "kna\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN", "$,", "KOUS", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die schwartze Haut sieht au\u00df, als were sie gebacken", "tokens": ["Die", "schwart\u00b7ze", "Haut", "sieht", "au\u00df", ",", "als", "we\u00b7re", "sie", "ge\u00b7ba\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,", "KOKOM", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Im Ofen und ged\u00f6rt f\u00fcr Mangel an der Kost", "tokens": ["Im", "O\u00b7fen", "und", "ge\u00b7d\u00f6rt", "f\u00fcr", "Man\u00b7gel", "an", "der", "Kost"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "VVPP", "APPR", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und Hunger der uns plagt. Sie haben schn\u00f6de Lust", "tokens": ["Und", "Hun\u00b7ger", "der", "uns", "plagt", ".", "Sie", "ha\u00b7ben", "schn\u00f6\u00b7de", "Lust"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "ART", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und Ueppigkeit ver\u00fcbt mit Zions sch\u00f6nen Weibern,", "tokens": ["Und", "Uep\u00b7pig\u00b7keit", "ver\u00b7\u00fcbt", "mit", "Zi\u00b7ons", "sch\u00f6\u00b7nen", "Wei\u00b7bern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Den Jungfraun Juda auch von ihren zarten Leibern", "tokens": ["Den", "Jung\u00b7fraun", "Ju\u00b7da", "auch", "von", "ih\u00b7ren", "zar\u00b7ten", "Lei\u00b7bern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Den besten Schatz geraubt, die F\u00fcrsten auffgeschn\u00fcrt,", "tokens": ["Den", "bes\u00b7ten", "Schatz", "ge\u00b7raubt", ",", "die", "F\u00fcrs\u00b7ten", "auff\u00b7ge\u00b7schn\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die Alten nicht geehrt, als wie sich wol geb\u00fchrt.", "tokens": ["Die", "Al\u00b7ten", "nicht", "ge\u00b7ehrt", ",", "als", "wie", "sich", "wol", "ge\u00b7b\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVPP", "$,", "KOUS", "PWAV", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Das arme junge Volck hat M\u00fcller m\u00fcssen werden,", "tokens": ["Das", "ar\u00b7me", "jun\u00b7ge", "Volck", "hat", "M\u00fcl\u00b7ler", "m\u00fcs\u00b7sen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VAFIN", "NE", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die Knaben bleiben schier erliegen auff der Erden,", "tokens": ["Die", "Kna\u00b7ben", "blei\u00b7ben", "schier", "er\u00b7lie\u00b7gen", "auff", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So das ihr Hals f\u00fcr Last de\u00df Holtzes brechen wil.", "tokens": ["So", "das", "ihr", "Hals", "f\u00fcr", "Last", "de\u00df", "Holt\u00b7zes", "bre\u00b7chen", "wil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PPOSAT", "NN", "APPR", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Kein Alter sitzt am Thor', es wird kein Seitenspiel", "tokens": ["Kein", "Al\u00b7ter", "sitzt", "am", "Thor'", ",", "es", "wird", "kein", "Sei\u00b7ten\u00b7spiel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "APPRART", "NN", "$,", "PPER", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Von unsrer Jugend mehr, wie zwar zuvor, ger\u00fchret,", "tokens": ["Von", "uns\u00b7rer", "Ju\u00b7gend", "mehr", ",", "wie", "zwar", "zu\u00b7vor", ",", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$,", "PWAV", "ADV", "ADV", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die Laut' ist ohne Laut; kein Neigen wird gef\u00fchret,", "tokens": ["Die", "Laut'", "ist", "oh\u00b7ne", "Laut", ";", "kein", "Nei\u00b7gen", "wird", "ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "$.", "PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Kein Tantz nicht mehr gehegt; die Freuden sind nun au\u00df,", "tokens": ["Kein", "Tantz", "nicht", "mehr", "ge\u00b7hegt", ";", "die", "Freu\u00b7den", "sind", "nun", "au\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKNEG", "ADV", "VVPP", "$.", "ART", "NN", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und unser Schauspielplatz wird jetzt ein Klagehau\u00df;", "tokens": ["Und", "un\u00b7ser", "Schau\u00b7spiel\u00b7platz", "wird", "jetzt", "ein", "Kla\u00b7ge\u00b7hau\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Die Cron ist auch dahin; ach, ach, da\u00df unser Leben.", "tokens": ["Die", "Cron", "ist", "auch", "da\u00b7hin", ";", "ach", ",", "ach", ",", "da\u00df", "un\u00b7ser", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PAV", "$.", "XY", "$,", "ITJ", "$,", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Sich also gantz und gar den S\u00fcnden hat ergeben!", "tokens": ["Sich", "al\u00b7so", "gantz", "und", "gar", "den", "S\u00fcn\u00b7den", "hat", "er\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "KON", "ADV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Drumb sind wir jetzt betr\u00fcbt, drumb bricht das Hertz uns schier", "tokens": ["Drumb", "sind", "wir", "jetzt", "be\u00b7tr\u00fcbt", ",", "drumb", "bricht", "das", "Hertz", "uns", "schier"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "VVPP", "$,", "PAV", "VVFIN", "ART", "NN", "PPER", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Und unsern Augen k\u00f6mpt fast alles finster f\u00fcr,", "tokens": ["Und", "un\u00b7sern", "Au\u00b7gen", "k\u00f6mpt", "fast", "al\u00b7les", "fins\u00b7ter", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "PIS", "ADJD", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Weil Zion w\u00fcste liegt, weil F\u00fcchs' ihn u