, "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Und l\u00e4nger als du pflegst, den harten Sinn zu fassen?", "tokens": ["Und", "l\u00e4n\u00b7ger", "als", "du", "pflegst", ",", "den", "har\u00b7ten", "Sinn", "zu", "fas\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Ists m\u00f6glich, da\u00df dein Zorn noch weiter brennen kan?", "tokens": ["Ists", "m\u00f6g\u00b7lich", ",", "da\u00df", "dein", "Zorn", "noch", "wei\u00b7ter", "bren\u00b7nen", "kan", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Herr, bring' uns doch nun heim und nim uns wider an;", "tokens": ["Herr", ",", "bring'", "uns", "doch", "nun", "heim", "und", "nim", "uns", "wi\u00b7der", "an", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "KON", "VVIMP", "PPER", "PTKVZ", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Verj\u00fcng' uns widerumb, beschenck uns mit den Jahren,", "tokens": ["Ver\u00b7j\u00fcng'", "uns", "wi\u00b7de\u00b7rumb", ",", "be\u00b7schenck", "uns", "mit", "den", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "VVIMP", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Die also gl\u00fccklich sind, als jene vormals waren;", "tokens": ["Die", "al\u00b7so", "gl\u00fcck\u00b7lich", "sind", ",", "als", "je\u00b7ne", "vor\u00b7mals", "wa\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VAFIN", "$,", "KOUS", "PDS", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Dann, Herr, du bist uns feind, hast keine Masse mehr", "tokens": ["Dann", ",", "Herr", ",", "du", "bist", "uns", "feind", ",", "hast", "kei\u00b7ne", "Mas\u00b7se", "mehr"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "$,", "PPER", "VAFIN", "PPER", "NN", "$,", "VAFIN", "PIAT", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Zu straffen uns, dein Volck, und z\u00fcrnest gar zu sehr.", "tokens": ["Zu", "straf\u00b7fen", "uns", ",", "dein", "Volck", ",", "und", "z\u00fcr\u00b7nest", "gar", "zu", "sehr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "ADV", "PTKA", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}