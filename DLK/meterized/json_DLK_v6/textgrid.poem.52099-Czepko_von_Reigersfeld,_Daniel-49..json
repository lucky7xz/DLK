{"textgrid.poem.52099": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "49.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie \u00e4ngstet sich das Hertz in der Camill\u00e6 Brust,", "tokens": ["Wie", "\u00e4ngs\u00b7tet", "sich", "das", "Hertz", "in", "der", "Ca\u00b7mill\u00e6", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ART", "NN", "APPR", "ART", "NE", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dieweil ihr liebster Mann Binor\u00e6us gestorben,", "tokens": ["Die\u00b7weil", "ihr", "liebs\u00b7ter", "Mann", "Bi\u00b7nor\u00e6us", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich wundre mich noch itzt, da\u00df es nicht gantz vertorben,", "tokens": ["Ich", "wund\u00b7re", "mich", "noch", "itzt", ",", "da\u00df", "es", "nicht", "gantz", "ver\u00b7tor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df es nicht gantz vor Leid zum Leibe raus gemust.", "tokens": ["Da\u00df", "es", "nicht", "gantz", "vor", "Leid", "zum", "Lei\u00b7be", "raus", "ge\u00b7must", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "APPR", "NN", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das heist voll Lieb, das heist voll Treu an Mann gedacht,", "tokens": ["Das", "heist", "voll", "Lieb", ",", "das", "heist", "voll", "Treu", "an", "Mann", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "NN", "$,", "PRELS", "ADJD", "ADJD", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann wi\u00dft, dieweil ihr Leid mit nichts sonst war zu stillen,", "tokens": ["Dann", "wi\u00dft", ",", "die\u00b7weil", "ihr", "Leid", "mit", "nichts", "sonst", "war", "zu", "stil\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "APPR", "PIS", "ADV", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Li\u00df \u00fcber Jahres Frist sie sich in Schleyer h\u00fcllen,", "tokens": ["Li\u00df", "\u00fc\u00b7ber", "Jah\u00b7res", "Frist", "sie", "sich", "in", "Schle\u00b7yer", "h\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PPER", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und hat vor Angst darzu ein Kind zur Welt gebracht.", "tokens": ["Und", "hat", "vor", "Angst", "dar\u00b7zu", "ein", "Kind", "zur", "Welt", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "PAV", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}