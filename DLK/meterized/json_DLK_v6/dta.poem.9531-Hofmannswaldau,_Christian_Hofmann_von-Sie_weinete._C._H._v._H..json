{"dta.poem.9531": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sie weinete.  \n C. H. v. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es brach der Lesbie das seufzen durch den mund/", "tokens": ["Es", "brach", "der", "Les\u00b7bie", "das", "seuf\u00b7zen", "durch", "den", "mund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die rosen hatten hier den liljen weichen m\u00fcssen.", "tokens": ["Die", "ro\u00b7sen", "hat\u00b7ten", "hier", "den", "lil\u00b7jen", "wei\u00b7chen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man sah der thr\u00e4nen bach auf beyden wangen fl\u00fcssen/", "tokens": ["Man", "sah", "der", "thr\u00e4\u00b7nen", "bach", "auf", "bey\u00b7den", "wan\u00b7gen", "fl\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein heisses ach und weh quall aus des hertzens grund.", "tokens": ["Ein", "heis\u00b7ses", "ach", "und", "weh", "quall", "aus", "des", "hert\u00b7zens", "grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "XY", "KON", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich schaute/ wie der schmertz in ihren augen stund/", "tokens": ["Ich", "schau\u00b7te", "/", "wie", "der", "schmertz", "in", "ih\u00b7ren", "au\u00b7gen", "stund", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOKOM", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie ihre strahlen sich durch angst verdecken liessen/", "tokens": ["Wie", "ih\u00b7re", "strah\u00b7len", "sich", "durch", "angst", "ver\u00b7de\u00b7cken", "lies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "VVFIN", "PRF", "APPR", "NN", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es lag die freundlichkeit in ohnmacht zu den f\u00fcssen.", "tokens": ["Es", "lag", "die", "freund\u00b7lich\u00b7keit", "in", "ohn\u00b7macht", "zu", "den", "f\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJD", "APPR", "ART", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Und ihr verworren haar that ihre wehmuth kund.", "tokens": ["Und", "ihr", "ver\u00b7wor\u00b7ren", "haar", "that", "ih\u00b7re", "weh\u00b7muth", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich f\u00fchlte diese noth auf meine seele dringen/", "tokens": ["Ich", "f\u00fchl\u00b7te", "die\u00b7se", "noth", "auf", "mei\u00b7ne", "see\u00b7le", "drin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Es grif die kalte pein auch meine geister an/", "tokens": ["Es", "grif", "die", "kal\u00b7te", "pein", "auch", "mei\u00b7ne", "geis\u00b7ter", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und weil die wehmuth nicht mit freyheit reden kan/", "tokens": ["Und", "weil", "die", "weh\u00b7muth", "nicht", "mit", "frey\u00b7heit", "re\u00b7den", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PTKNEG", "APPR", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So kont ich endlich nichts als diesen reim erzwingen:", "tokens": ["So", "kont", "ich", "end\u00b7lich", "nichts", "als", "die\u00b7sen", "reim", "er\u00b7zwin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PIS", "KOKOM", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie meinen geist belebt der sch\u00f6nen augenschein/", "tokens": ["Wie", "mei\u00b7nen", "geist", "be\u00b7lebt", "der", "sch\u00f6\u00b7nen", "au\u00b7gen\u00b7schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So soll ihr weinen itzt auch meine s\u00fcndflut seyn.", "tokens": ["So", "soll", "ihr", "wei\u00b7nen", "itzt", "auch", "mei\u00b7ne", "s\u00fcnd\u00b7flut", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}