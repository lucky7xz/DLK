{"textgrid.poem.44359": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Kuss", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf die H\u00e4nde k\u00fc\u00dft die Achtung,", "tokens": ["Auf", "die", "H\u00e4n\u00b7de", "k\u00fc\u00dft", "die", "Ach\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freundschaft auf die offne Stirne,", "tokens": ["Freund\u00b7schaft", "auf", "die", "off\u00b7ne", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf die Wange Wohlgefallen,", "tokens": ["Auf", "die", "Wan\u00b7ge", "Wohl\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selge Liebe auf den Mund;", "tokens": ["Sel\u00b7ge", "Lie\u00b7be", "auf", "den", "Mund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aufs geschlo\u00dfne Aug die Sehnsucht,", "tokens": ["Aufs", "ge\u00b7schlo\u00df\u00b7ne", "Aug", "die", "Sehn\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.6": {"text": "In die hohle Hand Verlangen,", "tokens": ["In", "die", "hoh\u00b7le", "Hand", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Arm und Nacken die Begierde,", "tokens": ["Arm", "und", "Na\u00b7cken", "die", "Be\u00b7gier\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "\u00dcberall sonst hin Raserei.", "tokens": ["\u00dc\u00b7be\u00b7rall", "sonst", "hin", "Ra\u00b7se\u00b7rei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}