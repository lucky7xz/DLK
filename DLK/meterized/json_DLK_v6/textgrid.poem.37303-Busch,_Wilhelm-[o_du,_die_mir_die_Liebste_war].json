{"textgrid.poem.37303": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[o du, die mir die Liebste war]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du, die mir die Liebste war,", "tokens": ["O", "du", ",", "die", "mir", "die", "Liebs\u00b7te", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du schl\u00e4fst nun schon so manches Jahr.", "tokens": ["Du", "schl\u00e4fst", "nun", "schon", "so", "man\u00b7ches", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So manches Jahr, da ich allein,", "tokens": ["So", "man\u00b7ches", "Jahr", ",", "da", "ich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "KOUS", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du gutes Herz, gedenk ich dein.", "tokens": ["Du", "gu\u00b7tes", "Herz", ",", "ge\u00b7denk", "ich", "dein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gedenk ich dein, von Nacht umh\u00fcllt,", "tokens": ["Ge\u00b7denk", "ich", "dein", ",", "von", "Nacht", "um\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So tritt zu mir dein treues Bild.", "tokens": ["So", "tritt", "zu", "mir", "dein", "treu\u00b7es", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dein treues Bild, was ich auch tu,", "tokens": ["Dein", "treu\u00b7es", "Bild", ",", "was", "ich", "auch", "tu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PWS", "PPER", "ADV", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es winkt mir ab, es winkt mir zu.", "tokens": ["Es", "winkt", "mir", "ab", ",", "es", "winkt", "mir", "zu", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und scheint mein Wort dir gar zu k\u00fchn,", "tokens": ["Und", "scheint", "mein", "Wort", "dir", "gar", "zu", "k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Nicht gut mein Tun,", "tokens": ["Nicht", "gut", "mein", "Tun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Du hast mir einst so oft verziehn,", "tokens": ["Du", "hast", "mir", "einst", "so", "oft", "ver\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Verzeih auch nun.", "tokens": ["Ver\u00b7zeih", "auch", "nun", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}