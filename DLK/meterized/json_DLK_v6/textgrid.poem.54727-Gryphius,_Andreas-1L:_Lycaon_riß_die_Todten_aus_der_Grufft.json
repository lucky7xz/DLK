{"textgrid.poem.54727": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lycaon ri\u00df die Todten aus der Grufft/", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lycaon ri\u00df die Todten aus der Grufft/", "tokens": ["Ly\u00b7ca\u00b7on", "ri\u00df", "die", "Tod\u00b7ten", "aus", "der", "Grufft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Beraubte S\u00e4rg' und brach Stein/ Creutz und Baare.", "tokens": ["Be\u00b7raub\u00b7te", "S\u00e4r\u00b7g'", "und", "brach", "Stein", "/", "Creutz", "und", "Baa\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "VVFIN", "NN", "$(", "NN", "KON", "NE", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Damit ihm nicht dergleichen widerfahre;", "tokens": ["Da\u00b7mit", "ihm", "nicht", "derg\u00b7lei\u00b7chen", "wi\u00b7der\u00b7fah\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bestelt das Recht ein Grab ihm in der Lufft.", "tokens": ["Be\u00b7stelt", "das", "Recht", "ein", "Grab", "ihm", "in", "der", "Lufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}