{"dta.poem.11675": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Bei Gelegenheit eines Kinderspielzeugs,  \n vorstellend:  \n  Hanswurst an der Sandm\u00fchle.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Ein h\u00fcbsch Gesch\u00e4ft wird hier betrieben.", "tokens": ["Ein", "h\u00fcbsch", "Ge\u00b7sch\u00e4ft", "wird", "hier", "be\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geht wohl einem M\u00fcller im ganzen Land", "tokens": ["Geht", "wohl", "ei\u00b7nem", "M\u00fcl\u00b7ler", "im", "gan\u00b7zen", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NE", "APPRART", "ADJA", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Sein Metier so lustig aus der Hand?", "tokens": ["Sein", "Me\u00b7tier", "so", "lus\u00b7tig", "aus", "der", "Hand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Zwar das bekenn' ich frank und frei,", "tokens": ["Zwar", "das", "be\u00b7kenn'", "ich", "frank", "und", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "PPER", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Besonderer Segen ist nicht dabei:", "tokens": ["Be\u00b7son\u00b7de\u00b7rer", "Se\u00b7gen", "ist", "nicht", "da\u00b7bei", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PTKNEG", "PAV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Sand gie\u00dft man ein, Sand kommt heraus;", "tokens": ["Sand", "gie\u00dft", "man", "ein", ",", "Sand", "kommt", "he\u00b7raus", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "PTKVZ", "$,", "NN", "VVFIN", "PTKVZ", "$."], "meter": "++--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Man d\u00e4chte fast, hier w\u00e4r' ein Narr zu Haus.", "tokens": ["Man", "d\u00e4ch\u00b7te", "fast", ",", "hier", "w\u00e4r'", "ein", "Narr", "zu", "Haus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "$,", "ADV", "VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sobald ich \u00fcbrigens insoweit fertig bin,", "tokens": ["So\u00b7bald", "ich", "\u00fcb\u00b7ri\u00b7gens", "in\u00b7so\u00b7weit", "fer\u00b7tig", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Hab' ich etwas wirklich Gemeinn\u00fctziges im Sinn.", "tokens": ["Hab'", "ich", "et\u00b7was", "wirk\u00b7lich", "Ge\u00b7mein\u00b7n\u00fct\u00b7zi\u00b7ges", "im", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADJD", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}}}}