{"dta.poem.19367": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "100.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wol gibt es zwischen Recht und Unrecht scharfe Gr\u00e4nzen,", "tokens": ["Wol", "gibt", "es", "zwi\u00b7schen", "Recht", "und", "Un\u00b7recht", "schar\u00b7fe", "Gr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch deinen Scharfsinn la\u00df nicht in der Sch\u00e4rfe gl\u00e4nzen.", "tokens": ["Doch", "dei\u00b7nen", "Scharf\u00b7sinn", "la\u00df", "nicht", "in", "der", "Sch\u00e4r\u00b7fe", "gl\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVIMP", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gewis bestimmter als dis zweifelhaft Gebiet", "tokens": ["Ge\u00b7wis", "be\u00b7stimm\u00b7ter", "als", "dis", "zwei\u00b7fel\u00b7haft", "Ge\u00b7biet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "PDS", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist zwischen Acker hier und dort der Unterschied.", "tokens": ["Ist", "zwi\u00b7schen", "A\u00b7cker", "hier", "und", "dort", "der", "Un\u00b7ter\u00b7schied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "ADV", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch h\u00e4lt der Ackersmann von hier und der von dort", "tokens": ["Doch", "h\u00e4lt", "der", "A\u00b7ckers\u00b7mann", "von", "hier", "und", "der", "von", "dort"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADV", "KON", "ART", "APPR", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein wenig seinen Pflug zur\u00fcck vom \u00e4u\u00dfern Ort;", "tokens": ["Ein", "we\u00b7nig", "sei\u00b7nen", "Pflug", "zu\u00b7r\u00fcck", "vom", "\u00e4u\u00b7\u00dfern", "Ort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PPOSAT", "NN", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da\u00df lieber ungebaut ein Streifchen zwischenliege,", "tokens": ["Da\u00df", "lie\u00b7ber", "un\u00b7ge\u00b7baut", "ein", "Streif\u00b7chen", "zwi\u00b7schen\u00b7lie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als da\u00df sich Pflug und Pflug begegnen dort zum Kriege.", "tokens": ["Als", "da\u00df", "sich", "Pflug", "und", "Pflug", "be\u00b7geg\u00b7nen", "dort", "zum", "Krie\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PRF", "NN", "KON", "NN", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "So halt den Fu\u00df zur\u00fcck von der Versuchung Rand,", "tokens": ["So", "halt", "den", "Fu\u00df", "zu\u00b7r\u00fcck", "von", "der", "Ver\u00b7su\u00b7chung", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und setz' im Zweifelsfall in Ruhstand deine Hand.", "tokens": ["Und", "setz'", "im", "Zwei\u00b7fels\u00b7fall", "in", "Ruh\u00b7stand", "dei\u00b7ne", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}