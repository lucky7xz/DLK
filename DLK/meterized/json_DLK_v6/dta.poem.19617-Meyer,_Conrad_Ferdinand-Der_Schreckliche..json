{"dta.poem.19617": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der Schreckliche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Benvenuto, sprich, was schmiedest", "tokens": ["Ben\u00b7ve\u00b7nu\u00b7to", ",", "sprich", ",", "was", "schmie\u00b7dest"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ADJD", "$,", "PWS", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du wie rasend in der Werkstatt?", "tokens": ["Du", "wie", "ra\u00b7send", "in", "der", "Werk\u00b7statt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOKOM", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welches ungeheure Kunstwerk?", "tokens": ["Wel\u00b7ches", "un\u00b7ge\u00b7heu\u00b7re", "Kunst\u00b7werk", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u2014 \u201eMesser! Scharfe feine Messer!\u201c", "tokens": ["\u201e", "Mes\u00b7ser", "!", "Schar\u00b7fe", "fei\u00b7ne", "Mes\u00b7ser", "!", "\u201c"], "token_info": ["punct", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "$(", "NN", "$.", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Benvenuto, sprich, was prahlst du?", "tokens": ["Ben\u00b7ve\u00b7nu\u00b7to", ",", "sprich", ",", "was", "prahlst", "du", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "PWS", "VVFIN", "PPER", "$."], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.2": {"text": "Welche ungeheure L\u00fcge", "tokens": ["Wel\u00b7che", "un\u00b7ge\u00b7heu\u00b7re", "L\u00fc\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["PWAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tischest auf du den Gesellen?", "tokens": ["Ti\u00b7schest", "auf", "du", "den", "Ge\u00b7sel\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "\u2014 \u201eIch bin stummer als ein Fischchen.\u201c", "tokens": ["\u201e", "Ich", "bin", "stum\u00b7mer", "als", "ein", "Fischchen", ".", "\u201c"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "$(", "PPER", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Benvenuto, sprich, was drohst du?", "tokens": ["Ben\u00b7ve\u00b7nu\u00b7to", ",", "sprich", ",", "was", "drohst", "du", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "PWS", "VVFIN", "PPER", "$."], "meter": "+---+---", "measure": "dactylic.init"}, "line.2": {"text": "Welche ungeheure Mordthat,", "tokens": ["Wel\u00b7che", "un\u00b7ge\u00b7heu\u00b7re", "Mord\u00b7that", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die vor Abend du begehn wirst?", "tokens": ["Die", "vor", "A\u00b7bend", "du", "be\u00b7gehn", "wirst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPER", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u2014 \u201eIch bin fr\u00f6mmer als ein L\u00e4mmlein.\u201c", "tokens": ["\u201e", "Ich", "bin", "fr\u00f6m\u00b7mer", "als", "ein", "L\u00e4mm\u00b7lein", ".", "\u201c"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "$(", "PPER", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Benvenuto bringt die Eisen", "tokens": ["Ben\u00b7ve\u00b7nu\u00b7to", "bringt", "die", "Ei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meister Jakob von Perugia,", "tokens": ["Meis\u00b7ter", "Ja\u00b7kob", "von", "Pe\u00b7ru\u00b7gia", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NE", "$,"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.3": {"text": "Der den kranken Finger schneidet", "tokens": ["Der", "den", "kran\u00b7ken", "Fin\u00b7ger", "schnei\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem geduld'gen Kind des Goldschmieds.", "tokens": ["Dem", "ge\u00b7duld'\u00b7gen", "Kind", "des", "Gold\u00b7schmieds", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Benvenuto's gl\u00fchnde Blicke", "tokens": ["Ben\u00b7ve\u00b7nu\u00b7to's", "gl\u00fchn\u00b7de", "Bli\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Folgen jedem Schnitt des Stahles.", "tokens": ["Fol\u00b7gen", "je\u00b7dem", "Schnitt", "des", "Stah\u00b7les", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201erafaella, schmerzt mein Messer?\u201c", "tokens": ["\u201e", "ra\u00b7fael\u00b7la", ",", "schmerzt", "mein", "Mes\u00b7ser", "?", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NE", "$,", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201ebenvenuto, nein, es schmerzt nicht.\u201c", "tokens": ["\u201e", "ben\u00b7ve\u00b7nu\u00b7to", ",", "nein", ",", "es", "schmerzt", "nicht", ".", "\u201c"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$,", "PTKANT", "$,", "PPER", "VVFIN", "PTKNEG", "$.", "$("], "meter": "---+--+-", "measure": "iambic.di.relaxed"}}}}}