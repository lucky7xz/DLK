{"dta.poem.23454": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das allgemeine Beste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr V\u00f6lker, la\u00dft doch euer Klagen!", "tokens": ["Ihr", "V\u00f6l\u00b7ker", ",", "la\u00dft", "doch", "eu\u00b7er", "Kla\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVIMP", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00dft euer Zweifeln, euer Zagen!", "tokens": ["La\u00dft", "eu\u00b7er", "Zwei\u00b7feln", ",", "eu\u00b7er", "Za\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df sich f\u00fcr euch die F\u00fcrsten plagen,", "tokens": ["Da\u00df", "sich", "f\u00fcr", "euch", "die", "F\u00fcrs\u00b7ten", "pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das soll euch allen wohlbehagen.", "tokens": ["Das", "soll", "euch", "al\u00b7len", "wohl\u00b7be\u00b7ha\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die F\u00fcrsten sind bei Tag und Nacht", "tokens": ["Die", "F\u00fcrs\u00b7ten", "sind", "bei", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf euer Bestes nur bedacht.", "tokens": ["Auf", "eu\u00b7er", "Bes\u00b7tes", "nur", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr sollt nicht schm\u00e4hen, sollt nicht schmollen,", "tokens": ["Ihr", "sollt", "nicht", "schm\u00e4\u00b7hen", ",", "sollt", "nicht", "schmol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr sollt nicht euren F\u00fcrsten grollen!", "tokens": ["Ihr", "sollt", "nicht", "eu\u00b7ren", "F\u00fcrs\u00b7ten", "grol\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sollt ihnen Dank und Ehrfurcht zollen,", "tokens": ["Sollt", "ih\u00b7nen", "Dank", "und", "Ehr\u00b7furcht", "zol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil sie nur euer Bestes wollen!", "tokens": ["Weil", "sie", "nur", "eu\u00b7er", "Bes\u00b7tes", "wol\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zwar ist das Beste von der Welt", "tokens": ["Zwar", "ist", "das", "Bes\u00b7te", "von", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vorl\u00e4ufig immer noch das Geld.", "tokens": ["Vor\u00b7l\u00e4u\u00b7fig", "im\u00b7mer", "noch", "das", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}