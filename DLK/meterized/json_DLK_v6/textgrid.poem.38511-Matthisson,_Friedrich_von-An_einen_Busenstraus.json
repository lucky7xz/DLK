{"textgrid.poem.38511": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An einen Busenstraus", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine Gottheit erkohr dich unter Tausenden", "tokens": ["Ei\u00b7ne", "Got\u00b7theit", "er\u00b7kohr", "dich", "un\u00b7ter", "Tau\u00b7sen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Von den S\u00f6hnen des Mays, an der gehobnen Brust", "tokens": ["Von", "den", "S\u00f6h\u00b7nen", "des", "Mays", ",", "an", "der", "ge\u00b7hob\u00b7nen", "Brust"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NE", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Meines M\u00e4dchens zu beben,", "tokens": ["Mei\u00b7nes", "M\u00e4d\u00b7chens", "zu", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Lieblichduftender Blumenstraus!", "tokens": ["Lieb\u00b7lich\u00b7duf\u00b7ten\u00b7der", "Blu\u00b7men\u00b7straus", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Neidensw\u00fcrdiges Loos! schauest das Heiligthum,", "tokens": ["Nei\u00b7dens\u00b7w\u00fcr\u00b7di\u00b7ges", "Loos", "!", "schau\u00b7est", "das", "Hei\u00b7lig\u00b7thum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-++--+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Das des rosigen Flors wallender Schleyer h\u00fcllt!", "tokens": ["Das", "des", "ro\u00b7si\u00b7gen", "Flors", "wal\u00b7len\u00b7der", "Schle\u00b7yer", "h\u00fcllt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ruhst am Throne der Liebe,", "tokens": ["Ruhst", "am", "Thro\u00b7ne", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Sonnst im Glanze der Sch\u00f6nheit dich!", "tokens": ["Sonnst", "im", "Glan\u00b7ze", "der", "Sch\u00f6n\u00b7heit", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Wohl mir! wiegte wie du, gl\u00fccklicher Blumenstraus,", "tokens": ["Wohl", "mir", "!", "wieg\u00b7te", "wie", "du", ",", "gl\u00fcck\u00b7li\u00b7cher", "Blu\u00b7men\u00b7straus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "$.", "VVFIN", "KOKOM", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Liebetrunken mein Haupt, unter berauschenden", "tokens": ["Lie\u00b7be\u00b7trun\u00b7ken", "mein", "Haupt", ",", "un\u00b7ter", "be\u00b7rau\u00b7schen\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "APPR", "ADJA"], "meter": "+-+--++--+--", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wonnetr\u00e4umen des Himmels,", "tokens": ["Won\u00b7net\u00b7r\u00e4u\u00b7men", "des", "Him\u00b7mels", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "An der klopfenden M\u00e4dchenbrust!", "tokens": ["An", "der", "klop\u00b7fen\u00b7den", "M\u00e4d\u00b7chen\u00b7brust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Ha! dann lacht' ich der Welt, lachte des Stadtgew\u00fchls,", "tokens": ["Ha", "!", "dann", "lacht'", "ich", "der", "Welt", ",", "lach\u00b7te", "des", "Stadt\u00b7ge\u00b7w\u00fchls", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Und des gauckelnden Tand's, welchen die Mod' ersann!", "tokens": ["Und", "des", "gau\u00b7ckeln\u00b7den", "Tan\u00b7d's", ",", "wel\u00b7chen", "die", "Mod'", "er\u00b7sann", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PWAT", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "W\u00e4re froher als Adam", "tokens": ["W\u00e4\u00b7re", "fro\u00b7her", "als", "A\u00b7dam"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "KOKOM", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "In den Lauben von Eden war!", "tokens": ["In", "den", "Lau\u00b7ben", "von", "E\u00b7den", "war", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "VAFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}