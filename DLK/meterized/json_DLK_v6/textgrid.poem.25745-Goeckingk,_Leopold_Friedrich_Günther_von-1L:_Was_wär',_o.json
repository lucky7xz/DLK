{"textgrid.poem.25745": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was w\u00e4r', o ", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was w\u00e4r', o ", "tokens": ["Was", "w\u00e4r'", ",", "o"], "token_info": ["word", "word", "punct", "word"], "pos": ["PWS", "VAFIN", "$,", "FM"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "F\u00fcr den gequ\u00e4lten Weisen, wenn die Seele", "tokens": ["F\u00fcr", "den", "ge\u00b7qu\u00e4l\u00b7ten", "Wei\u00b7sen", ",", "wenn", "die", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht \u00fcber der Verwesung Knochenfeld", "tokens": ["Nicht", "\u00fc\u00b7ber", "der", "Ver\u00b7we\u00b7sung", "Kno\u00b7chen\u00b7feld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hinaus oft blickte? Eine M\u00f6rderh\u00f6hle,", "tokens": ["Hin\u00b7aus", "oft", "blick\u00b7te", "?", "Ei\u00b7ne", "M\u00f6r\u00b7der\u00b7h\u00f6h\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vom matten Strahl' der Hoffnung kaum erhellt.", "tokens": ["Vom", "mat\u00b7ten", "Strahl'", "der", "Hoff\u00b7nung", "kaum", "er\u00b7hellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Doch h\u00e4tte die Religion", "tokens": ["Doch", "h\u00e4t\u00b7te", "die", "Re\u00b7li\u00b7gi\u00b7on"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wohlth\u00e4tig nicht zerstreut die dicken Schatten:", "tokens": ["Wohlt\u00b7h\u00e4\u00b7tig", "nicht", "zer\u00b7streut", "die", "di\u00b7cken", "Schat\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKNEG", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Willkommen w\u00e4re mir die T\u00e4uschung schon,", "tokens": ["Will\u00b7kom\u00b7men", "w\u00e4\u00b7re", "mir", "die", "T\u00e4u\u00b7schung", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Da\u00df unsre Seelen wandern! Denn was hatten", "tokens": ["Da\u00df", "uns\u00b7re", "See\u00b7len", "wan\u00b7dern", "!", "Denn", "was", "hat\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$.", "KON", "PWS", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die Weisen sonst f\u00fcr Trost, f\u00fcr Schmerzenlohn?", "tokens": ["Die", "Wei\u00b7sen", "sonst", "f\u00fcr", "Trost", ",", "f\u00fcr", "Schmer\u00b7zen\u00b7lohn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und, lieber Freund, was w\u00e4r' es mehr,", "tokens": ["Und", ",", "lie\u00b7ber", "Freund", ",", "was", "w\u00e4r'", "es", "mehr", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "NN", "$,", "PWS", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Wenn ich zuweilen tr\u00e4umte, da\u00df die Seelen", "tokens": ["Wenn", "ich", "zu\u00b7wei\u00b7len", "tr\u00e4um\u00b7te", ",", "da\u00df", "die", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Fortwanderten, bis zu dem Endverh\u00f6r'?", "tokens": ["Fort\u00b7wan\u00b7der\u00b7ten", ",", "bis", "zu", "dem", "End\u00b7ver\u00b7h\u00f6r'", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "APPR", "ART", "NN", "$."], "meter": "-+-----+-+", "measure": "dactylic.init"}, "line.14": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Sein Lied uns s\u00e4ng'? O Freund, was w\u00e4r' es mehr?", "tokens": ["Sein", "Lied", "uns", "s\u00e4ng'", "?", "O", "Freund", ",", "was", "w\u00e4r'", "es", "mehr", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$.", "NE", "NN", "$,", "PWS", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Wirst du den edlen Hirsch nicht jetzt", "tokens": ["Wirst", "du", "den", "ed\u00b7len", "Hirsch", "nicht", "jetzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Bedauren, dich die tolle Wuth nicht kr\u00e4nken,", "tokens": ["Be\u00b7dau\u00b7ren", ",", "dich", "die", "tol\u00b7le", "Wuth", "nicht", "kr\u00e4n\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRF", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Die ihn zum Scherz' abmartert und zerfetzt?", "tokens": ["Die", "ihn", "zum", "Scher\u00b7z'", "ab\u00b7mar\u00b7tert", "und", "zer\u00b7fetzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "La\u00df mich ein Thor seyn, ", "tokens": ["La\u00df", "mich", "ein", "Thor", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.20": {"text": "Da\u00df man in ihm den Kaiser ", "tokens": ["Da\u00df", "man", "in", "ihm", "den", "Kai\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "So oft als meine Augen sahn,", "tokens": ["So", "oft", "als", "mei\u00b7ne", "Au\u00b7gen", "sahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Wie an des Fischers Angel sich mit Schmerzen", "tokens": ["Wie", "an", "des", "Fi\u00b7schers", "An\u00b7gel", "sich", "mit", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Ein W\u00fcrmchen kr\u00fcmmte: Balsam war der Wahn", "tokens": ["Ein", "W\u00fcrm\u00b7chen", "kr\u00fcmm\u00b7te", ":", "Bal\u00b7sam", "war", "der", "Wahn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "NE", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Dann immer meinem wundgefre\u00dfnen Herzen:", "tokens": ["Dann", "im\u00b7mer", "mei\u00b7nem", "wund\u00b7ge\u00b7fre\u00df\u00b7nen", "Her\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Es sey vielleicht ein H\u00f6fling wie ", "tokens": ["Es", "sey", "viel\u00b7leicht", "ein", "H\u00f6f\u00b7ling", "wie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "KOKOM"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Welch Thier der Erde hat den Muth", "tokens": ["Welch", "Thier", "der", "Er\u00b7de", "hat", "den", "Muth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Des Rosses? Dennoch mu\u00df es jungen Thoren", "tokens": ["Des", "Ros\u00b7ses", "?", "Den\u00b7noch", "mu\u00df", "es", "jun\u00b7gen", "Tho\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ADV", "VMFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Gehorchen; dennoch st\u00f6\u00dft ihr Uebermuth", "tokens": ["Ge\u00b7hor\u00b7chen", ";", "den\u00b7noch", "st\u00f6\u00dft", "ihr", "Ue\u00b7ber\u00b7muth"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "In seine Seite scharfgespitzte Sporen;", "tokens": ["In", "sei\u00b7ne", "Sei\u00b7te", "scharf\u00b7ge\u00b7spitz\u00b7te", "Spo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Doch was da flie\u00dft, sey eines M\u00f6rders Blut.", "tokens": ["Doch", "was", "da", "flie\u00dft", ",", "sey", "ei\u00b7nes", "M\u00f6r\u00b7ders", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVFIN", "$,", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Sieh dieser Wachtel Schmeichelein!", "tokens": ["Sieh", "die\u00b7ser", "Wach\u00b7tel", "Schmei\u00b7che\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Auf meiner Schulter, gleich als wenn sie lange", "tokens": ["Auf", "mei\u00b7ner", "Schul\u00b7ter", ",", "gleich", "als", "wenn", "sie", "lan\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADV", "KOKOM", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Mich schon gekannt, steht sie auf einem Bein',", "tokens": ["Mich", "schon", "ge\u00b7kannt", ",", "steht", "sie", "auf", "ei\u00b7nem", "Bein'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Und kratzt mir mit dem andern sanft die Wange;", "tokens": ["Und", "kratzt", "mir", "mit", "dem", "an\u00b7dern", "sanft", "die", "Wan\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Ach! k\u00f6nnt' es nicht mein Sohn, mein ", "tokens": ["Ach", "!", "k\u00f6nnt'", "es", "nicht", "mein", "Sohn", ",", "mein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ITJ", "$.", "VMFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "$,", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Und warum sollt' ich nicht dereinst", "tokens": ["Und", "wa\u00b7rum", "sollt'", "ich", "nicht", "de\u00b7reinst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VMFIN", "PPER", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Als Lerche dann mich in die L\u00fcfte schwingen,", "tokens": ["Als", "Ler\u00b7che", "dann", "mich", "in", "die", "L\u00fcf\u00b7te", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Wenn du um mich auf meinem H\u00fcgel weinst?", "tokens": ["Wenn", "du", "um", "mich", "auf", "mei\u00b7nem", "H\u00fc\u00b7gel", "weinst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Nicht dir auch dann noch s\u00fc\u00dfe Lieder singen?", "tokens": ["Nicht", "dir", "auch", "dann", "noch", "s\u00fc\u00b7\u00dfe", "Lie\u00b7der", "sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "ADV", "ADV", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Der Wahn ist s\u00fc\u00df! Still! wenn du's nicht so meinst.", "tokens": ["Der", "Wahn", "ist", "s\u00fc\u00df", "!", "Still", "!", "wenn", "du's", "nicht", "so", "meinst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "ADJD", "$.", "KOUS", "PIS", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}