{"textgrid.poem.63683": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut, da ich mittags mich im Wald erging,", "tokens": ["Heut", ",", "da", "ich", "mit\u00b7tags", "mich", "im", "Wald", "er\u00b7ging", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sah ich ein Tierlein mitten auf dem Wege,", "tokens": ["Sah", "ich", "ein", "Tier\u00b7lein", "mit\u00b7ten", "auf", "dem", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das regungslos sich sonnte, weich und tr\u00e4ge,", "tokens": ["Das", "re\u00b7gungs\u00b7los", "sich", "sonn\u00b7te", ",", "weich", "und", "tr\u00e4\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PRF", "VVFIN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In einem breiten, goldnen Sonnenring.", "tokens": ["In", "ei\u00b7nem", "brei\u00b7ten", ",", "gold\u00b7nen", "Son\u00b7nen\u00b7ring", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wird dir auch wohl im k\u00fchlen Waldgehege?", "tokens": ["Wird", "dir", "auch", "wohl", "im", "k\u00fch\u00b7len", "Wald\u00b7ge\u00b7he\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entbehrst du nicht im rauhen Nord der Pflege,", "tokens": ["Ent\u00b7behrst", "du", "nicht", "im", "rau\u00b7hen", "Nord", "der", "Pfle\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die dein Geschlecht im S\u00fcden stets empfing?", "tokens": ["Die", "dein", "Ge\u00b7schlecht", "im", "S\u00fc\u00b7den", "stets", "emp\u00b7fing", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hast du, wie dort der Brauch, in Gartenmauern", "tokens": ["Hast", "du", ",", "wie", "dort", "der", "Brauch", ",", "in", "Gar\u00b7ten\u00b7mau\u00b7ern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "PWAV", "ADV", "ART", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein K\u00e4mmerchen und kannst, vor Regeng\u00fcssen", "tokens": ["Ein", "K\u00e4m\u00b7mer\u00b7chen", "und", "kannst", ",", "vor", "Re\u00b7gen\u00b7g\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "KON", "VMFIN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Schnee gesch\u00fctzt, den Winter \u00fcberdauern?", "tokens": ["Und", "Schnee", "ge\u00b7sch\u00fctzt", ",", "den", "Win\u00b7ter", "\u00fc\u00b7berd\u00b7au\u00b7ern", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch, wie du durchkommst, wirst du selber wissen,", "tokens": ["Doch", ",", "wie", "du", "durch\u00b7kommst", ",", "wirst", "du", "sel\u00b7ber", "wis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Indes wir gro\u00dfen Menschen in den Schauern", "tokens": ["In\u00b7des", "wir", "gro\u00b7\u00dfen", "Men\u00b7schen", "in", "den", "Schau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Ostwinds uns den Schnupfen holen m\u00fcssen.", "tokens": ["Des", "Ost\u00b7winds", "uns", "den", "Schnup\u00b7fen", "ho\u00b7len", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}