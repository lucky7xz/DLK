{"dta.poem.8432": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An den Mond.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Durch diese Apfelb\u00e4ume,", "tokens": ["Durch", "die\u00b7se", "Ap\u00b7fel\u00b7b\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo einst dein Freund so selig war,", "tokens": ["Wo", "einst", "dein", "Freund", "so", "se\u00b7lig", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und tr\u00e4umte s\u00fcsse Tr\u00e4ume?", "tokens": ["Und", "tr\u00e4um\u00b7te", "s\u00fcs\u00b7se", "Tr\u00e4u\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verh\u00fclle deinen Silberglanz,", "tokens": ["Ver\u00b7h\u00fcl\u00b7le", "dei\u00b7nen", "Sil\u00b7ber\u00b7glanz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und schimmre, wie du schimmerst,", "tokens": ["Und", "schimm\u00b7re", ",", "wie", "du", "schim\u00b7merst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wenn du den fr\u00fchen Todtenkranz", "tokens": ["Wenn", "du", "den", "fr\u00fc\u00b7hen", "Tod\u00b7ten\u00b7kranz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der jungen Braut beflimmerst!", "tokens": ["Der", "jun\u00b7gen", "Braut", "be\u00b7flim\u00b7merst", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du blikst umsonst so hell und klar", "tokens": ["Du", "blikst", "um\u00b7sonst", "so", "hell", "und", "klar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In diese Laube nieder;", "tokens": ["In", "die\u00b7se", "Lau\u00b7be", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nie findest du das frohe Paar", "tokens": ["Nie", "fin\u00b7dest", "du", "das", "fro\u00b7he", "Paar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In ihrem Schatten wieder!", "tokens": ["In", "ih\u00b7rem", "Schat\u00b7ten", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein schwarzes feindliches Geschick", "tokens": ["Ein", "schwar\u00b7zes", "feind\u00b7li\u00b7ches", "Ge\u00b7schick"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Entriss mir meine Sch\u00f6ne!", "tokens": ["Ent\u00b7riss", "mir", "mei\u00b7ne", "Sch\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Kein Seufzer zaubert sie zur\u00fcck,", "tokens": ["Kein", "Seuf\u00b7zer", "zau\u00b7bert", "sie", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und keine Sehnsuchtsthr\u00e4ne!", "tokens": ["Und", "kei\u00b7ne", "Sehn\u00b7suchtst\u00b7hr\u00e4\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O wandelt sie hinfort einmal", "tokens": ["O", "wan\u00b7delt", "sie", "hin\u00b7fort", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An meiner Ruhestelle,", "tokens": ["An", "mei\u00b7ner", "Ru\u00b7he\u00b7stel\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann mache flugs mit tr\u00fcbem Stral", "tokens": ["Dann", "ma\u00b7che", "flugs", "mit", "tr\u00fc\u00b7bem", "Stral"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Grabes Blumen helle!", "tokens": ["Des", "Gra\u00b7bes", "Blu\u00b7men", "hel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie seze weinend sich aufs Grab,", "tokens": ["Sie", "se\u00b7ze", "wei\u00b7nend", "sich", "aufs", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo Rosen niederhangen,", "tokens": ["Wo", "Ro\u00b7sen", "nie\u00b7der\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und pfl\u00fccke sich ein Bl\u00fcmchen ab,", "tokens": ["Und", "pfl\u00fc\u00b7cke", "sich", "ein", "Bl\u00fcm\u00b7chen", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und dr\u00fcck' es an die Wangen.", "tokens": ["Und", "dr\u00fcck", "es", "an", "die", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}