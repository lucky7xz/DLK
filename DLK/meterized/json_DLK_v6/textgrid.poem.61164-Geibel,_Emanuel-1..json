{"textgrid.poem.61164": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Tr\u00f6pflein mehr im Becher!", "tokens": ["Kein", "Tr\u00f6pf\u00b7lein", "mehr", "im", "Be\u00b7cher", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kein Geld im S\u00e4ckel mehr!", "tokens": ["Kein", "Geld", "im", "S\u00e4\u00b7ckel", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da wird mir armem Zecher", "tokens": ["Da", "wird", "mir", "ar\u00b7mem", "Ze\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Herze gar so schwer.", "tokens": ["Das", "Her\u00b7ze", "gar", "so", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Wandern macht mir Pein,", "tokens": ["Das", "Wan\u00b7dern", "macht", "mir", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wei\u00df nicht, wo aus, noch ein;", "tokens": ["Wei\u00df", "nicht", ",", "wo", "aus", ",", "noch", "ein", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "PTKVZ", "$,", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ins Kloster m\u00f6cht' ich gehen,", "tokens": ["Ins", "Klos\u00b7ter", "m\u00f6cht'", "ich", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da liegt ein k\u00fchler Wein.", "tokens": ["Da", "liegt", "ein", "k\u00fch\u00b7ler", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich zieh' auf d\u00fcrrem Wege,", "tokens": ["Ich", "zieh'", "auf", "d\u00fcr\u00b7rem", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Rock ist arg bestaubt,", "tokens": ["Mein", "Rock", "ist", "arg", "be\u00b7staubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wei\u00df nicht, wohin ich lege", "tokens": ["Wei\u00df", "nicht", ",", "wo\u00b7hin", "ich", "le\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In dieser Nacht mein Haupt.", "tokens": ["In", "die\u00b7ser", "Nacht", "mein", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Herberg' ist die Welt,", "tokens": ["Mein", "Her\u00b7ber\u00b7g'", "ist", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Mein Dach das Himmelszelt,", "tokens": ["Mein", "Dach", "das", "Him\u00b7mels\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das Bett, darauf ich schlafe,", "tokens": ["Das", "Bett", ",", "da\u00b7rauf", "ich", "schla\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das ist das breite Feld.", "tokens": ["Das", "ist", "das", "brei\u00b7te", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich geh' auf flinken Sohlen,", "tokens": ["Ich", "geh'", "auf", "flin\u00b7ken", "Soh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch schneller reit't das Gl\u00fcck;", "tokens": ["Doch", "schnel\u00b7ler", "reit't", "das", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich mag es nicht einholen,", "tokens": ["Ich", "mag", "es", "nicht", "ein\u00b7ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es l\u00e4\u00dft mich arg zur\u00fcck.", "tokens": ["Es", "l\u00e4\u00dft", "mich", "arg", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Komm' ich an einen Ort,", "tokens": ["Komm'", "ich", "an", "ei\u00b7nen", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So war es eben dort,", "tokens": ["So", "war", "es", "e\u00b7ben", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da kommt der Wind geflogen,", "tokens": ["Da", "kommt", "der", "Wind", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der pfeift mich aus sofort.", "tokens": ["Der", "pfeift", "mich", "aus", "so\u00b7fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich wollt', ich l\u00e4g' zur Stunde", "tokens": ["Ich", "wollt'", ",", "ich", "l\u00e4g'", "zur", "Stun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am Heidelberger Fa\u00df,", "tokens": ["Am", "Hei\u00b7del\u00b7ber\u00b7ger", "Fa\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den offnen Mund am Spunde,", "tokens": ["Den", "off\u00b7nen", "Mund", "am", "Spun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und tr\u00e4umt' ich wei\u00df nicht was.", "tokens": ["Und", "tr\u00e4umt'", "ich", "wei\u00df", "nicht", "was", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "PTKNEG", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wollt' ein Dirnlein fein", "tokens": ["Und", "wollt'", "ein", "Dirn\u00b7lein", "fein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mir gar die Schenkin sein:", "tokens": ["Mir", "gar", "die", "Schen\u00b7kin", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mir w\u00e4r's, als schw\u00e4mmen Rosen", "tokens": ["Mir", "w\u00e4r's", ",", "als", "schw\u00e4m\u00b7men", "Ro\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wohl auf dem klaren Wein.", "tokens": ["Wohl", "auf", "dem", "kla\u00b7ren", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ach wer den Weg doch w\u00fc\u00dfte", "tokens": ["Ach", "wer", "den", "Weg", "doch", "w\u00fc\u00df\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PWS", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In das Schlaraffenland!", "tokens": ["In", "das", "Schla\u00b7raf\u00b7fen\u00b7land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Mir d\u00fcnket wohl, ich m\u00fc\u00dfte", "tokens": ["Mir", "d\u00fcn\u00b7ket", "wohl", ",", "ich", "m\u00fc\u00df\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dort finden Ehr' und Stand.", "tokens": ["Dort", "fin\u00b7den", "Ehr'", "und", "Stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Mut ist gar so schlecht,", "tokens": ["Mein", "Mut", "ist", "gar", "so", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df ich ihn tauschen m\u00f6cht';", "tokens": ["Da\u00df", "ich", "ihn", "tau\u00b7schen", "m\u00f6cht'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und so's Dukaten schneite,", "tokens": ["Und", "so's", "Du\u00b7ka\u00b7ten", "schnei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das w\u00e4r' mir eben recht.", "tokens": ["Das", "w\u00e4r'", "mir", "e\u00b7ben", "recht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}