{"textgrid.poem.63215": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Winteranfang", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alle Welt ist, voll Wind.", "tokens": ["Al\u00b7le", "Welt", "ist", ",", "voll", "Wind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "$,", "ADJD", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Der Herbst fallt von den B\u00e4umen.", "tokens": ["Der", "Herbst", "fallt", "von", "den", "B\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir sind", "tokens": ["Wir", "sind"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "In Tr\u00e4umen.", "tokens": ["In", "Tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Der erste weisse Schnee ...", "tokens": ["Der", "ers\u00b7te", "weis\u00b7se", "Schnee", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wer auf ihn tritt, tritt ihn zu Dreck.", "tokens": ["Wer", "auf", "ihn", "tritt", ",", "tritt", "ihn", "zu", "Dreck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sehe weg,", "tokens": ["Ich", "se\u00b7he", "weg", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Weil ich mein Herz seh.", "tokens": ["Weil", "ich", "mein", "Herz", "seh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}