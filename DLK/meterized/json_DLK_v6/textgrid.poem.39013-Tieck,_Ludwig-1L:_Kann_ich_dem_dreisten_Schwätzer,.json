{"textgrid.poem.39013": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kann ich dem dreisten Schw\u00e4tzer,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kann ich dem dreisten Schw\u00e4tzer,", "tokens": ["Kann", "ich", "dem", "dreis\u00b7ten", "Schw\u00e4t\u00b7zer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem bettelnden Redner,", "tokens": ["Dem", "bet\u00b7teln\u00b7den", "Red\u00b7ner", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Dem ich stets heimkehrend vor\u00fcber wandle,", "tokens": ["Dem", "ich", "stets", "heim\u00b7keh\u00b7rend", "vor\u00b7\u00fc\u00b7ber", "wand\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "ADV", "VVFIN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Nimmer entgehn?", "tokens": ["Nim\u00b7mer", "ent\u00b7gehn", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Arm ist er nicht,", "tokens": ["Arm", "ist", "er", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Und dennoch bin ich gezwungen", "tokens": ["Und", "den\u00b7noch", "bin", "ich", "ge\u00b7zwun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Ihm mehr zu reichen", "tokens": ["Ihm", "mehr", "zu", "rei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Als dem Elend-D\u00fcrftigen?", "tokens": ["Als", "dem", "E\u00b7len\u00b7dD\u00fcrf\u00b7ti\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Soll ich dort die Stra\u00dfe w\u00e4hlen?", "tokens": ["Soll", "ich", "dort", "die", "Stra\u00b7\u00dfe", "w\u00e4h\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Nein! sch\u00e4me dich dieser Schw\u00e4che!", "tokens": ["Nein", "!", "sch\u00e4\u00b7me", "dich", "die\u00b7ser", "Schw\u00e4\u00b7che", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "VVFIN", "PRF", "PDAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Mag er doch reden,", "tokens": ["Mag", "er", "doch", "re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Bitten und beten,", "tokens": ["Bit\u00b7ten", "und", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Dreister Stirne geh' ich", "tokens": ["Dreis\u00b7ter", "Stir\u00b7ne", "geh'", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Ihm fest vor\u00fcber,", "tokens": ["Ihm", "fest", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Und keine M\u00fcnze, kein Kupfer,", "tokens": ["Und", "kei\u00b7ne", "M\u00fcn\u00b7ze", ",", "kein", "Kup\u00b7fer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Soll seiner Redekunst ein Opfer fallen. \u2013", "tokens": ["Soll", "sei\u00b7ner", "Re\u00b7de\u00b7kunst", "ein", "Op\u00b7fer", "fal\u00b7len", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Schon gewahrt er mich von fern,", "tokens": ["Schon", "ge\u00b7wahrt", "er", "mich", "von", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "PPER", "PRF", "APPR", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Er schwenkt den gro\u00dfen dreikantigen Hut,", "tokens": ["Er", "schwenkt", "den", "gro\u00b7\u00dfen", "drei\u00b7kan\u00b7ti\u00b7gen", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.3": {"text": "Und seine wohllautende volle Stimme t\u00f6nt:", "tokens": ["Und", "sei\u00b7ne", "wohl\u00b7lau\u00b7ten\u00b7de", "vol\u00b7le", "Stim\u00b7me", "t\u00f6nt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Gebenedeit sei dort, der Edle,", "tokens": ["Ge\u00b7be\u00b7ne\u00b7deit", "sei", "dort", ",", "der", "Ed\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "$,", "ART", "ADJA", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.5": {"text": "Der t\u00e4glich leichteren Schrittes schon", "tokens": ["Der", "t\u00e4g\u00b7lich", "leich\u00b7te\u00b7ren", "Schrit\u00b7tes", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Durch unsre ber\u00fchmten Gassen wandelt!", "tokens": ["Durch", "uns\u00b7re", "be\u00b7r\u00fchm\u00b7ten", "Gas\u00b7sen", "wan\u00b7delt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Wohl haben meine frommen Gebete", "tokens": ["Wohl", "ha\u00b7ben", "mei\u00b7ne", "from\u00b7men", "Ge\u00b7be\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Dem Trefflichen gen\u00fctzt:", "tokens": ["Dem", "Treff\u00b7li\u00b7chen", "ge\u00b7n\u00fctzt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Wie krank und schwach", "tokens": ["Wie", "krank", "und", "schwach"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Schritt er mir \u00e4chzend das erstemal vor\u00fcber!", "tokens": ["Schritt", "er", "mir", "\u00e4ch\u00b7zend", "das", "ers\u00b7te\u00b7mal", "vor\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "ART", "ADV", "PTKVZ", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.11": {"text": "R\u00fcstigen Ganges, ohne Stab,", "tokens": ["R\u00fcs\u00b7ti\u00b7gen", "Gan\u00b7ges", ",", "oh\u00b7ne", "Stab", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KOUI", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Seh' ich ihn bald in voller Gesundheit prangen.", "tokens": ["Seh'", "ich", "ihn", "bald", "in", "vol\u00b7ler", "Ge\u00b7sund\u00b7heit", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.13": {"text": "Wer bin ich Aermster,", "tokens": ["Wer", "bin", "ich", "A\u00b7erms\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Der ich hier als ekler Kr\u00fcppel", "tokens": ["Der", "ich", "hier", "als", "ek\u00b7ler", "Kr\u00fcp\u00b7pel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "KOUS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Auf der Gasse liegen mu\u00df,", "tokens": ["Auf", "der", "Gas\u00b7se", "lie\u00b7gen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Da\u00df ein solcher lieber, theurer Mann,", "tokens": ["Da\u00df", "ein", "sol\u00b7cher", "lie\u00b7ber", ",", "theu\u00b7rer", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIAT", "ADV", "$,", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "Je um diese verzerrte Figur sich gek\u00fcmmert? \u2013", "tokens": ["Je", "um", "die\u00b7se", "ver\u00b7zerr\u00b7te", "Fi\u00b7gur", "sich", "ge\u00b7k\u00fcm\u00b7mert", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "PDAT", "ADJA", "NN", "PRF", "VVPP", "$.", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "N\u00e4her kommt er und mir n\u00e4her.", "tokens": ["N\u00e4\u00b7her", "kommt", "er", "und", "mir", "n\u00e4\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "PPER", "ADJD", "$."], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.19": {"text": "Ei! welch mildes Antlitz!", "tokens": ["Ei", "!", "welch", "mil\u00b7des", "Ant\u00b7litz", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAT", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.20": {"text": "W\u00e4r' ich nicht ein Verworfner,", "tokens": ["W\u00e4r'", "ich", "nicht", "ein", "Ver\u00b7worf\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Wenn meine fromme stille Freude", "tokens": ["Wenn", "mei\u00b7ne", "from\u00b7me", "stil\u00b7le", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Nur dahin zielte,", "tokens": ["Nur", "da\u00b7hin", "ziel\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PAV", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "Eine Gabe von ihm zu empfangen?", "tokens": ["Ei\u00b7ne", "Ga\u00b7be", "von", "ihm", "zu", "emp\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Fern sei von mir so niedrer Gedanke!", "tokens": ["Fern", "sei", "von", "mir", "so", "nie\u00b7drer", "Ge\u00b7dan\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Nein, Belobtester, Wackerster,", "tokens": ["Nein", ",", "Be\u00b7lob\u00b7tes\u00b7ter", ",", "Wa\u00b7ckers\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.26": {"text": "Schreitet, schreitet dreist vor\u00fcber,", "tokens": ["Schrei\u00b7tet", ",", "schrei\u00b7tet", "dreist", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Seht nicht her nach dem \u00e4rmsten eurer Verehrer,", "tokens": ["Seht", "nicht", "her", "nach", "dem", "\u00e4rms\u00b7ten", "eu\u00b7rer", "Ver\u00b7eh\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "APPR", "ART", "ADJA", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Der doch f\u00fcr euch beten und w\u00fcnschen wird:", "tokens": ["Der", "doch", "f\u00fcr", "euch", "be\u00b7ten", "und", "w\u00fcn\u00b7schen", "wird", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "VVINF", "KON", "VVINF", "VAFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.29": {"text": "Bettl' ich gleich, Eigennutz ist mir fremd,", "tokens": ["Bettl'", "ich", "gleich", ",", "Ei\u00b7gen\u00b7nutz", "ist", "mir", "fremd", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.30": {"text": "Doch kann ich nicht so ver\u00e4chtlich seyn", "tokens": ["Doch", "kann", "ich", "nicht", "so", "ver\u00b7\u00e4cht\u00b7lich", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ADV", "ADJD", "VAINF"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "Abzuweisen und zu verschm\u00e4hn,", "tokens": ["Ab\u00b7zu\u00b7wei\u00b7sen", "und", "zu", "ver\u00b7schm\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.32": {"text": "Was solch Alexander mir bietet. \u2013", "tokens": ["Was", "solch", "A\u00b7lex\u00b7an\u00b7der", "mir", "bie\u00b7tet", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PIAT", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Schon hat er den Paul,", "tokens": ["Schon", "hat", "er", "den", "Paul", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NE", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Und l\u00e4chelt dankend", "tokens": ["Und", "l\u00e4\u00b7chelt", "dan\u00b7kend"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Mit seltsamen Blick.", "tokens": ["Mit", "selt\u00b7sa\u00b7men", "Blick", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}