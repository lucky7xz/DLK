{"dta.poem.17457": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "78.  Warumb wenig zur Th\u00fcr de\u00df Lebens  \n eingehn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df nach der Him\u0304elth\u00fcr so wenig Menschen greiffen!", "tokens": ["Da\u00df", "nach", "der", "Him\u0304el\u00b7th\u00fcr", "so", "we\u00b7nig", "Men\u00b7schen", "greif\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Es wil jhm keiner dran den alten Balg abstreiffen.", "tokens": ["Es", "wil", "jhm", "kei\u00b7ner", "dran", "den", "al\u00b7ten", "Balg", "ab\u00b7streif\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "PAV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}