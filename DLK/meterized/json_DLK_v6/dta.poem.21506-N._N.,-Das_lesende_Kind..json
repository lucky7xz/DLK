{"dta.poem.21506": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Das lesende Kind.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Auf den Schoo\u00df das Buch gebreitet,", "tokens": ["Auf", "den", "Schoo\u00df", "das", "Buch", "ge\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Scheinst du nichts um dich zu missen,", "tokens": ["Scheinst", "du", "nichts", "um", "dich", "zu", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Starrst hinein, inde\u00df beflissen", "tokens": ["Starrst", "hin\u00b7ein", ",", "in\u00b7de\u00df", "be\u00b7flis\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueber\u2019s Blatt der Finger gleitet.", "tokens": ["Ue\u00b7ber's", "Blatt", "der", "Fin\u00b7ger", "glei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In das Meer der Zeichen leitet", "tokens": ["In", "das", "Meer", "der", "Zei\u00b7chen", "lei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich kein K\u00f6nnen noch und Wissen,", "tokens": ["Dich", "kein", "K\u00f6n\u00b7nen", "noch", "und", "Wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "ADV", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unbeschr\u00e4nkt, in schwanken Rissen", "tokens": ["Un\u00b7be\u00b7schr\u00e4nkt", ",", "in", "schwan\u00b7ken", "Ris\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich dein junges Sinnen weitet.", "tokens": ["Sich", "dein", "jun\u00b7ges", "Sin\u00b7nen", "wei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "S\u00fc\u00dfes D\u00e4mmern! Traumumwoben", "tokens": ["S\u00fc\u00b7\u00dfes", "D\u00e4m\u00b7mern", "!", "Trau\u00b7mum\u00b7wo\u00b7ben"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$.", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schl\u00e4ft das Denken noch im Neste,", "tokens": ["Schl\u00e4ft", "das", "Den\u00b7ken", "noch", "im", "Nes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur das F\u00fchlen schwebt nach oben.", "tokens": ["Nur", "das", "F\u00fch\u00b7len", "schwebt", "nach", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach, des Lebens tr\u00fcbe Reste", "tokens": ["Ach", ",", "des", "Le\u00b7bens", "tr\u00fc\u00b7be", "Res\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bleiben, wenn der Flor gehoben \u2014", "tokens": ["Blei\u00b7ben", ",", "wenn", "der", "Flor", "ge\u00b7ho\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das Geheimni\u00df ist das Beste.", "tokens": ["Das", "Ge\u00b7heim\u00b7ni\u00df", "ist", "das", "Bes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}