{"dta.poem.21718": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Das Lenzgef\u00fchl.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Lenz, der Lenz ist erschienen.", "tokens": ["Der", "Lenz", ",", "der", "Lenz", "ist", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Es ergr\u00fcnen die Maale der H\u00fcnen,", "tokens": ["Es", "er\u00b7gr\u00fc\u00b7nen", "die", "Maa\u00b7le", "der", "H\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Es belaubt sich der heilige Hain.", "tokens": ["Es", "be\u00b7laubt", "sich", "der", "hei\u00b7li\u00b7ge", "Hain", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Es erhellt sich der dunkele Eppich.", "tokens": ["Es", "er\u00b7hellt", "sich", "der", "dun\u00b7ke\u00b7le", "Ep\u00b7pich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "--+--+--++", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Ein gr\u00fcner geschorener Teppich,", "tokens": ["Ein", "gr\u00fc\u00b7ner", "ge\u00b7scho\u00b7re\u00b7ner", "Tep\u00b7pich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Gestickt mit g\u00fcldenen Blumen,", "tokens": ["Ge\u00b7stickt", "mit", "g\u00fcl\u00b7de\u00b7nen", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "So funkelt der Anger, der Rain.", "tokens": ["So", "fun\u00b7kelt", "der", "An\u00b7ger", ",", "der", "Rain", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Die Blumen stehen wie trunken", "tokens": ["Die", "Blu\u00b7men", "ste\u00b7hen", "wie", "trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ADJD"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Herab auf die Erde gesunken,", "tokens": ["Her\u00b7ab", "auf", "die", "Er\u00b7de", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Umarmt sie der br\u00e4utliche May.", "tokens": ["Um\u00b7armt", "sie", "der", "br\u00e4ut\u00b7li\u00b7che", "May", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Entgegen dem Br\u00fcnstigen bebet", "tokens": ["Ent\u00b7ge\u00b7gen", "dem", "Br\u00fcns\u00b7ti\u00b7gen", "be\u00b7bet"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ihr schwellender Schoos. Es entstrebet", "tokens": ["Ihr", "schwel\u00b7len\u00b7der", "Schoos", ".", "Es", "ent\u00b7stre\u00b7bet"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "PPER", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Der Kraisenden dr\u00e4ngend die F\u00fclle", "tokens": ["Der", "Krai\u00b7sen\u00b7den", "dr\u00e4n\u00b7gend", "die", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Der Leben jung und neu.", "tokens": ["Der", "Le\u00b7ben", "jung", "und", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auf brechen die Brunnen der Tiefen.", "tokens": ["Auf", "bre\u00b7chen", "die", "Brun\u00b7nen", "der", "Tie\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Es erwachen die Seelen, die schliefen.", "tokens": ["Es", "er\u00b7wa\u00b7chen", "die", "See\u00b7len", ",", "die", "schlie\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Es bev\u00f6lkern sich Matten und Wald.", "tokens": ["Es", "be\u00b7v\u00f6l\u00b7kern", "sich", "Mat\u00b7ten", "und", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "NN", "KON", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Wie brausen die K\u00e4fer! Wie schwirren", "tokens": ["Wie", "brau\u00b7sen", "die", "K\u00e4\u00b7fer", "!", "Wie", "schwir\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$.", "PWAV", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Die zarten Libellen! Wie girren", "tokens": ["Die", "zar\u00b7ten", "Li\u00b7bel\u00b7len", "!", "Wie", "gir\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PWAV", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Die T\u00e4ubchen, derweil in den L\u00fcften", "tokens": ["Die", "T\u00e4ub\u00b7chen", ",", "der\u00b7weil", "in", "den", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Lerche Brautlied schallt.", "tokens": ["Der", "Ler\u00b7che", "Braut\u00b7lied", "schallt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Vom Hauche des Fr\u00fchlings geschwollen,", "tokens": ["Vom", "Hau\u00b7che", "des", "Fr\u00fch\u00b7lings", "ge\u00b7schwol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Vom Athem der Liebe gequollen,", "tokens": ["Vom", "A\u00b7them", "der", "Lie\u00b7be", "ge\u00b7quol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Schwelgt jeglicher Busen in Lust.", "tokens": ["Schwelgt", "jeg\u00b7li\u00b7cher", "Bu\u00b7sen", "in", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Nur mir will das Herz sich nicht weiten.", "tokens": ["Nur", "mir", "will", "das", "Herz", "sich", "nicht", "wei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "ART", "NN", "PRF", "PTKNEG", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Nur mir sich der Busen nicht breiten.", "tokens": ["Nur", "mir", "sich", "der", "Bu\u00b7sen", "nicht", "brei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PRF", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "----+--+-", "measure": "iambic.di.relaxed"}, "line.6": {"text": "Wann, ach, wann f\u00fcllt sich die Leere", "tokens": ["Wann", ",", "ach", ",", "wann", "f\u00fcllt", "sich", "die", "Lee\u00b7re"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "ITJ", "$,", "PWAV", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "In dieser verschmachtenden Brust.", "tokens": ["In", "die\u00b7ser", "ver\u00b7schmach\u00b7ten\u00b7den", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Ich sp\u00e4h' in die N\u00e4h' und die Ferne,", "tokens": ["Ich", "sp\u00e4h'", "in", "die", "N\u00e4h'", "und", "die", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ich frage die sinkenden Sterne,", "tokens": ["Ich", "fra\u00b7ge", "die", "sin\u00b7ken\u00b7den", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ich rufe die Blumen der Flur,", "tokens": ["Ich", "ru\u00b7fe", "die", "Blu\u00b7men", "der", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Ich belausche die Echo der Haine.", "tokens": ["Ich", "be\u00b7lau\u00b7sche", "die", "E\u00b7cho", "der", "Hai\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Wo wandelt, wo hauset die Eine,", "tokens": ["Wo", "wan\u00b7delt", ",", "wo", "hau\u00b7set", "die", "Ei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "PWAV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Die das schmerzliche Sehnen befriedigt? \u2014", "tokens": ["Die", "das", "schmerz\u00b7li\u00b7che", "Seh\u00b7nen", "be\u00b7frie\u00b7digt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Ach zeigt mir der Einzigen Spur!", "tokens": ["Ach", "zeigt", "mir", "der", "Ein\u00b7zi\u00b7gen", "Spur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}