{"textgrid.poem.34625": {"metadata": {"author": {"name": "Neukirch, Benjamin", "birth": "N.A.", "death": "N.A."}, "title": "Auff die kr\u00f6nung des R\u00f6mischen K\u00f6nigs Josephi", "genre": "verse", "period": "N.A.", "pub_year": 1697, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Europa zanckte sich und wolte gerne wissen/", "tokens": ["Eu\u00b7ro\u00b7pa", "zanck\u00b7te", "sich", "und", "wol\u00b7te", "ger\u00b7ne", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "KON", "VMFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer in Germanien noch w\u00fcrde k\u00f6nig seyn.", "tokens": ["Wer", "in", "Ger\u00b7ma\u00b7ni\u00b7en", "noch", "w\u00fcr\u00b7de", "k\u00f6\u00b7nig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NE", "ADV", "VAFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der stoltze Ludewig war \u00e4usserst drauff beflissen/", "tokens": ["Der", "stolt\u00b7ze", "Lu\u00b7de\u00b7wig", "war", "\u00e4us\u00b7serst", "drauff", "be\u00b7flis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VAFIN", "ADV", "PAV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie er das deutsche reich m\u00f6cht auseinander streun:", "tokens": ["Wie", "er", "das", "deut\u00b7sche", "reich", "m\u00f6cht", "aus\u00b7ein\u00b7an\u00b7der", "streun", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "ADJD", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum spahrt' er weder gelt/ noch m\u00fch und schmeicheleyen/", "tokens": ["Drum", "spahrt'", "er", "we\u00b7der", "gelt", "/", "noch", "m\u00fch", "und", "schmei\u00b7che\u00b7le\u00b7yen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "KON", "ADJD", "$(", "ADV", "ADJD", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "Und bot sein eignes kind zu einem k\u00e4yser an:", "tokens": ["Und", "bot", "sein", "eig\u00b7nes", "kind", "zu", "ei\u00b7nem", "k\u00e4y\u00b7ser", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer/ sprach er/ wird euch mehr als dieser Printz erfreuen/", "tokens": ["Wer", "/", "sprach", "er", "/", "wird", "euch", "mehr", "als", "die\u00b7ser", "Printz", "er\u00b7freu\u00b7en", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$(", "VVFIN", "PPER", "$(", "VAFIN", "PPER", "ADV", "KOUS", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der so/ wie ich/ die kunst sich zu vergr\u00f6ssern kan?", "tokens": ["Der", "so", "/", "wie", "ich", "/", "die", "kunst", "sich", "zu", "ver\u00b7gr\u00f6s\u00b7sern", "kan", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$(", "PWAV", "PPER", "$(", "ART", "NN", "PRF", "PTKZU", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Allein der himmel rieff: Behalte deine Gaben/", "tokens": ["Al\u00b7lein", "der", "him\u00b7mel", "rieff", ":", "Be\u00b7hal\u00b7te", "dei\u00b7ne", "Ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$.", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich will ein Josephs-Hertz und keinen Nero haben.", "tokens": ["Ich", "will", "ein", "Jo\u00b7se\u00b7phs\u00b7Hertz", "und", "kei\u00b7nen", "Ne\u00b7ro", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "KON", "PIAT", "NE", "VAFIN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}