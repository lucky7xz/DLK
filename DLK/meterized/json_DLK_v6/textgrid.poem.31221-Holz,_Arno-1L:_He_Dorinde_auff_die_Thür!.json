{"textgrid.poem.31221": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: He/ Dorinde/ auff die Th\u00fcr!", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "He/ Dorinde/ auff die Th\u00fcr!", "tokens": ["He", "/", "Do\u00b7rin\u00b7de", "/", "auff", "die", "Th\u00fcr", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NE", "$(", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "itzt auff nichts al\u00df Sterne dritt.", "tokens": ["itzt", "auff", "nichts", "al\u00df", "Ster\u00b7ne", "dritt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIS", "KOKOM", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "sazzt sich in das dikke Gr\u00fcn/", "tokens": ["sazzt", "sich", "in", "das", "dik\u00b7ke", "Gr\u00fcn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "wo die Zokker-R\u00f6sgens bl\u00fchn.", "tokens": ["wo", "die", "Zok\u00b7ker\u00b7R\u00f6s\u00b7gens", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Selbst ", "tokens": ["Selbst"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "lachend ihren Threnen-Krug.", "tokens": ["la\u00b7chend", "ih\u00b7ren", "Thre\u00b7nen\u00b7Krug", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "kreucht ihr fast bi\u00df untern Rokk.", "tokens": ["kreucht", "ihr", "fast", "bi\u00df", "un\u00b7tern", "Rokk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "hindter seiner ", "tokens": ["hind\u00b7ter", "sei\u00b7ner"], "token_info": ["word", "word"], "pos": ["APPR", "PPOSAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "bi\u00df er bruhstend sie erwischt", "tokens": ["bi\u00df", "er", "bruh\u00b7stend", "sie", "er\u00b7wischt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und mit ihr sein Hertz erfrischt.", "tokens": ["und", "mit", "ihr", "sein", "Hertz", "er\u00b7frischt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Dr\u00fcmb so la\u00df uns ohngesehn", "tokens": ["Dr\u00fcmb", "so", "la\u00df", "uns", "ohn\u00b7ge\u00b7sehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVIMP", "PPER", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "in die Heidel-P\u00fcschgens gehn", "tokens": ["in", "die", "Hei\u00b7del\u00b7P\u00fcschgens", "gehn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "und uns k\u00fcssen/ da\u00df es knallt/", "tokens": ["und", "uns", "k\u00fcs\u00b7sen", "/", "da\u00df", "es", "knallt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$(", "KOUS", "PPER", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00fcberall/ wo's uns gefallt!", "tokens": ["\u00fc\u00b7be\u00b7rall", "/", "wo's", "uns", "ge\u00b7fallt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PWAV", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}