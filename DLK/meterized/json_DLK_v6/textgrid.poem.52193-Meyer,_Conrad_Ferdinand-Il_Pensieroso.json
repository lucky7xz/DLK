{"textgrid.poem.52193": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Il Pensieroso", "genre": "verse", "period": "N.A.", "pub_year": 1861, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einem Winkel seiner Werkstatt las", "tokens": ["In", "ei\u00b7nem", "Win\u00b7kel", "sei\u00b7ner", "Werk\u00b7statt", "las"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Buonarotti, da es d\u00e4mmerte;", "tokens": ["Buo\u00b7na\u00b7rot\u00b7ti", ",", "da", "es", "d\u00e4m\u00b7mer\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Allm\u00e4hlich vor dem Blicke schwand die Schrift...", "tokens": ["All\u00b7m\u00e4h\u00b7lich", "vor", "dem", "Bli\u00b7cke", "schwand", "die", "Schrift", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da schlich sich Julianus ein, der Tr\u00e4umer,", "tokens": ["Da", "schlich", "sich", "Ju\u00b7li\u00b7a\u00b7nus", "ein", ",", "der", "Tr\u00e4u\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NE", "PTKVZ", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der einzige der heitern Medici,", "tokens": ["Der", "ein\u00b7zi\u00b7ge", "der", "hei\u00b7tern", "Me\u00b7di\u00b7ci", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "ADJA", "NE", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Der Schwermut kannte. Dieser glaubte sich", "tokens": ["Der", "Schwer\u00b7mut", "kann\u00b7te", ".", "Die\u00b7ser", "glaub\u00b7te", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PDS", "VVFIN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Allein. Er setzte sich und in der Hand", "tokens": ["Al\u00b7lein", ".", "Er", "setz\u00b7te", "sich", "und", "in", "der", "Hand"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "PPER", "VVFIN", "PRF", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Barg er das Kinn und hielt gesenkt das Haupt.", "tokens": ["Barg", "er", "das", "Kinn", "und", "hielt", "ge\u00b7senkt", "das", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "KON", "VVFIN", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So sa\u00df er schweigend bei den Marmorbildern,", "tokens": ["So", "sa\u00df", "er", "schwei\u00b7gend", "bei", "den", "Mar\u00b7mor\u00b7bil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die durch das Dunkel leise schimmerten,", "tokens": ["Die", "durch", "das", "Dun\u00b7kel", "lei\u00b7se", "schim\u00b7mer\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und kam mit ihnen murmelnd ins Gespr\u00e4ch,", "tokens": ["Und", "kam", "mit", "ih\u00b7nen", "mur\u00b7melnd", "ins", "Ge\u00b7spr\u00e4ch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Geheim belauscht von Michelangelo:", "tokens": ["Ge\u00b7heim", "be\u00b7lauscht", "von", "Mi\u00b7che\u00b7lan\u00b7ge\u00b7lo", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "\u00bbfeigheit ist's nicht und stammt von Feigheit nicht,", "tokens": ["\u00bb", "feig\u00b7heit", "ist's", "nicht", "und", "stammt", "von", "Feig\u00b7heit", "nicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PTKNEG", "KON", "VVFIN", "APPR", "NN", "PTKNEG", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.14": {"text": "Wenn einer seinem Erdenlos mi\u00dftraut,", "tokens": ["Wenn", "ei\u00b7ner", "sei\u00b7nem", "Er\u00b7den\u00b7los", "mi\u00df\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Sich sehnend nach dem letzten Atemzug,", "tokens": ["Sich", "seh\u00b7nend", "nach", "dem", "letz\u00b7ten", "A\u00b7tem\u00b7zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Denn auch ein Gl\u00fccklicher wei\u00df nicht, was kommt", "tokens": ["Denn", "auch", "ein", "Gl\u00fcck\u00b7li\u00b7cher", "wei\u00df", "nicht", ",", "was", "kommt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PTKNEG", "$,", "PWS", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Und v\u00f6llig unertr\u00e4glich werden kann \u2013", "tokens": ["Und", "v\u00f6l\u00b7lig", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "wer\u00b7den", "kann", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VAINF", "VMFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Leidlose Steine, wie beneid ich euch!\u00ab", "tokens": ["Leid\u00b7lo\u00b7se", "Stei\u00b7ne", ",", "wie", "be\u00b7neid", "ich", "euch", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "VVFIN", "PPER", "PPER", "$.", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.19": {"text": "Er ging und aus dem Leben schwand er dann", "tokens": ["Er", "ging", "und", "aus", "dem", "Le\u00b7ben", "schwand", "er", "dann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Fast unbemerkt. Nach einem Zeitverlauf", "tokens": ["Fast", "un\u00b7be\u00b7merkt", ".", "Nach", "ei\u00b7nem", "Zeit\u00b7ver\u00b7lauf"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Bestellten sie bei Michelangelo", "tokens": ["Be\u00b7stell\u00b7ten", "sie", "bei", "Mi\u00b7che\u00b7lan\u00b7ge\u00b7lo"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Das Grabbild ihm und brachten emsig her,", "tokens": ["Das", "Grab\u00b7bild", "ihm", "und", "brach\u00b7ten", "em\u00b7sig", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "KON", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Was noch in Schilderein vorhanden war", "tokens": ["Was", "noch", "in", "Schil\u00b7de\u00b7rein", "vor\u00b7han\u00b7den", "war"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "NN", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Von schwachen Spuren seines Angesichts.", "tokens": ["Von", "schwa\u00b7chen", "Spu\u00b7ren", "sei\u00b7nes", "An\u00b7ge\u00b7sichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "So waren seine Z\u00fcge, sagten sie.", "tokens": ["So", "wa\u00b7ren", "sei\u00b7ne", "Z\u00fc\u00b7ge", ",", "sag\u00b7ten", "sie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Der Meister schob es mit der Hand zur\u00fcck:", "tokens": ["Der", "Meis\u00b7ter", "schob", "es", "mit", "der", "Hand", "zu\u00b7r\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "\u00bbnehmt weg! Ich sehe, wie er sitzt und sinnt", "tokens": ["\u00bb", "nehmt", "weg", "!", "Ich", "se\u00b7he", ",", "wie", "er", "sitzt", "und", "sinnt"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Und kenne seine Seele. Das gen\u00fcgt.\u00ab", "tokens": ["Und", "ken\u00b7ne", "sei\u00b7ne", "See\u00b7le", ".", "Das", "ge\u00b7n\u00fcgt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$.", "PDS", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}