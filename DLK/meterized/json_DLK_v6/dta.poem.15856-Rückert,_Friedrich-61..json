{"dta.poem.15856": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "61.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du hast gewis dein Theil von Lust, was du genossen,", "tokens": ["Du", "hast", "ge\u00b7wis", "dein", "Theil", "von", "Lust", ",", "was", "du", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "APPR", "NN", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vergessen, da\u00df du nun dreinblickest so verdrossen.", "tokens": ["Ver\u00b7ges\u00b7sen", ",", "da\u00df", "du", "nun", "drein\u00b7bli\u00b7ckest", "so", "ver\u00b7dros\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPER", "ADV", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Erinnre dich, wie sch\u00f6n einmal die Welt dir war!", "tokens": ["E\u00b7rinn\u00b7re", "dich", ",", "wie", "sch\u00f6n", "ein\u00b7mal", "die", "Welt", "dir", "war", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADJD", "ADV", "ART", "NN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ist sie andern jetzt, so ist sie immerdar.", "tokens": ["So", "ist", "sie", "an\u00b7dern", "jetzt", ",", "so", "ist", "sie", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "ADV", "$,", "ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So ist sie immerdar, nur immer andern Augen,", "tokens": ["So", "ist", "sie", "im\u00b7mer\u00b7dar", ",", "nur", "im\u00b7mer", "an\u00b7dern", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr die sie grade taugt, die f\u00fcr sie grade taugen.", "tokens": ["F\u00fcr", "die", "sie", "gra\u00b7de", "taugt", ",", "die", "f\u00fcr", "sie", "gra\u00b7de", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "VVFIN", "$,", "PRELS", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und taugt sie dir nicht mehr, so taugt sie andern noch;", "tokens": ["Und", "taugt", "sie", "dir", "nicht", "mehr", ",", "so", "taugt", "sie", "an\u00b7dern", "noch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKNEG", "ADV", "$,", "ADV", "VVFIN", "PPER", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und taugst du selbst ihr nicht, so taugst du anderm doch.", "tokens": ["Und", "taugst", "du", "selbst", "ihr", "nicht", ",", "so", "taugst", "du", "an\u00b7derm", "doch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPER", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die Welt ist ewig sch\u00f6n, die Welt ist ewig jung,", "tokens": ["Die", "Welt", "ist", "e\u00b7wig", "sch\u00f6n", ",", "die", "Welt", "ist", "e\u00b7wig", "jung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht im Genusse, nur in der Erinnerung.", "tokens": ["Nicht", "im", "Ge\u00b7nus\u00b7se", ",", "nur", "in", "der", "E\u00b7rin\u00b7ne\u00b7rung", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "$,", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}