{"dta.poem.5555": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Weltweisen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erw\u00e4hl' ich denen beyzupflichten,", "tokens": ["Er\u00b7w\u00e4hl'", "ich", "de\u00b7nen", "bey\u00b7zu\u00b7pflich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihren weisen Mann mit GOTT vergleichen wollen;", "tokens": ["Die", "ih\u00b7ren", "wei\u00b7sen", "Mann", "mit", "GoTT", "ver\u00b7glei\u00b7chen", "wol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "APPR", "NE", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die alles blos nach blinder Ordnung richten,", "tokens": ["Die", "al\u00b7les", "blos", "nach", "blin\u00b7der", "Ord\u00b7nung", "rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auch da\u00df die Dinge, welche seyn,", "tokens": ["Auch", "da\u00df", "die", "Din\u00b7ge", ",", "wel\u00b7che", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "$,", "PRELS", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur blos allein", "tokens": ["Nur", "blos", "al\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Dem unvermeidlichen Berh\u00e4ngni\u00df folgen sollen?", "tokens": ["Dem", "un\u00b7ver\u00b7meid\u00b7li\u00b7chen", "Ber\u00b7h\u00e4ng\u00b7ni\u00df", "fol\u00b7gen", "sol\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}}}}