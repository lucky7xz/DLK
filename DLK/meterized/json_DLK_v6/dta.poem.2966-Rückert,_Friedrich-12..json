{"dta.poem.2966": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Raum wird in der Welt nach Eisenbahnanlegung", "tokens": ["Der", "Raum", "wird", "in", "der", "Welt", "nach", "Ei\u00b7sen\u00b7ba\u00b7hnan\u00b7le\u00b7gung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In der Geschwindigkeit verschwinden der Bewegung.", "tokens": ["In", "der", "Ge\u00b7schwin\u00b7dig\u00b7keit", "ver\u00b7schwin\u00b7den", "der", "Be\u00b7we\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dann wird uns in der Welt die Zeit nur \u00fcbrig bleiben,", "tokens": ["Dann", "wird", "uns", "in", "der", "Welt", "die", "Zeit", "nur", "\u00fcb\u00b7rig", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die durch Umtreiben dann im Raum wir auch vertreiben.", "tokens": ["Die", "durch", "Um\u00b7trei\u00b7ben", "dann", "im", "Raum", "wir", "auch", "ver\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADV", "APPRART", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So la\u00dft uns, \u00fcber Zeit und Raum durch Dampf und Eisen", "tokens": ["So", "la\u00dft", "uns", ",", "\u00fc\u00b7ber", "Zeit", "und", "Raum", "durch", "Dampf", "und", "Ei\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "$,", "APPR", "NN", "KON", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erhoben, nun den Kreis der Ewigkeit durchkreisen.", "tokens": ["Er\u00b7ho\u00b7ben", ",", "nun", "den", "Kreis", "der", "E\u00b7wig\u00b7keit", "durch\u00b7krei\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}