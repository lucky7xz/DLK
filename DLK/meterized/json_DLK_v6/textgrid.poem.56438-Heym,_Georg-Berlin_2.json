{"textgrid.poem.56438": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Berlin 2", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Beteerte F\u00e4sser rollten von den Schwellen", "tokens": ["Be\u00b7teer\u00b7te", "F\u00e4s\u00b7ser", "roll\u00b7ten", "von", "den", "Schwel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der dunklen Speicher auf die hohen K\u00e4hne.", "tokens": ["Der", "dunk\u00b7len", "Spei\u00b7cher", "auf", "die", "ho\u00b7hen", "K\u00e4h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Schlepper zogen an. Des Rauches M\u00e4hne", "tokens": ["Die", "Schlep\u00b7per", "zo\u00b7gen", "an", ".", "Des", "Rau\u00b7ches", "M\u00e4h\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hing ru\u00dfig nieder auf die \u00f6ligen Wellen.", "tokens": ["Hing", "ru\u00b7\u00dfig", "nie\u00b7der", "auf", "die", "\u00f6\u00b7li\u00b7gen", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Zwei Dampfer kamen mit Musikkapellen.", "tokens": ["Zwei", "Damp\u00b7fer", "ka\u00b7men", "mit", "Mu\u00b7sik\u00b7ka\u00b7pel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Schornstein kappten sie am Br\u00fcckenbogen.", "tokens": ["Den", "Schorn\u00b7stein", "kapp\u00b7ten", "sie", "am", "Br\u00fc\u00b7cken\u00b7bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Rauch, Ru\u00df, Gestank lag auf den schmutzigen Wogen", "tokens": ["Rauch", ",", "Ru\u00df", ",", "Ge\u00b7stank", "lag", "auf", "den", "schmut\u00b7zi\u00b7gen", "Wo\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "++-+-+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Der Gerbereien mit den braunen Fellen.", "tokens": ["Der", "Ger\u00b7be\u00b7rei\u00b7en", "mit", "den", "brau\u00b7nen", "Fel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "In allen Br\u00fccken, drunter uns die Zille", "tokens": ["In", "al\u00b7len", "Br\u00fc\u00b7cken", ",", "drun\u00b7ter", "uns", "die", "Zil\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PAV", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hindurchgebracht, ert\u00f6nten die Signale", "tokens": ["Hin\u00b7durch\u00b7ge\u00b7bracht", ",", "er\u00b7t\u00f6n\u00b7ten", "die", "Sig\u00b7na\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gleichwie in Trommeln wachsend in der Stille.", "tokens": ["Gleich\u00b7wie", "in", "Trom\u00b7meln", "wach\u00b7send", "in", "der", "Stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wir lie\u00dfen los und trieben im Kanale", "tokens": ["Wir", "lie\u00b7\u00dfen", "los", "und", "trie\u00b7ben", "im", "Ka\u00b7na\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An G\u00e4rten langsam hin. In dem Idylle", "tokens": ["An", "G\u00e4r\u00b7ten", "lang\u00b7sam", "hin", ".", "In", "dem", "I\u00b7dyl\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "PTKVZ", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sahn wir der Riesenschlote Nachtfanale.", "tokens": ["Sahn", "wir", "der", "Rie\u00b7sen\u00b7schlo\u00b7te", "Nacht\u00b7fa\u00b7na\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}