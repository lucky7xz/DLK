{"textgrid.poem.52833": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Politisches Gez\u00e4nk in Ehren,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Politisches Gez\u00e4nk in Ehren,", "tokens": ["Po\u00b7li\u00b7ti\u00b7sches", "Ge\u00b7z\u00e4nk", "in", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ein wenig Kunst auch will der Tag,", "tokens": ["Ein", "we\u00b7nig", "Kunst", "auch", "will", "der", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So pfeif' ich auf der Syrinx R\u00f6hren,", "tokens": ["So", "pfeif'", "ich", "auf", "der", "Sy\u00b7rinx", "R\u00f6h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und bringe was gefallen mag,", "tokens": ["Und", "brin\u00b7ge", "was", "ge\u00b7fal\u00b7len", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Euch, die im Schwei\u00df des Angesichtes", "tokens": ["Euch", ",", "die", "im", "Schwei\u00df", "des", "An\u00b7ge\u00b7sich\u00b7tes"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr euer Brod verdient und e\u00dft,", "tokens": ["Ihr", "eu\u00b7er", "Brod", "ver\u00b7dient", "und", "e\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Euch, die im Schein des Lampenlichtes", "tokens": ["Euch", ",", "die", "im", "Schein", "des", "Lam\u00b7pen\u00b7lich\u00b7tes"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ihr uns'res Wissens Tiefen me\u00dft.", "tokens": ["Ihr", "un\u00b7s'\u00b7res", "Wis\u00b7sens", "Tie\u00b7fen", "me\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "In schweren Wolken zieht das Leben,", "tokens": ["In", "schwe\u00b7ren", "Wol\u00b7ken", "zieht", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Weise schaudert vor der Nacht,", "tokens": ["Der", "Wei\u00b7se", "schau\u00b7dert", "vor", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch eine Waff' ist ihm gegeben,", "tokens": ["Doch", "ei\u00b7ne", "Waff'", "ist", "ihm", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er widerspricht, indem er lacht;", "tokens": ["Er", "wi\u00b7der\u00b7spricht", ",", "in\u00b7dem", "er", "lacht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn auch dem Priester strengster Musen,", "tokens": ["Denn", "auch", "dem", "Pries\u00b7ter", "strengs\u00b7ter", "Mu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Mann vom n\u00fcchternsten Bedacht", "tokens": ["Dem", "Mann", "vom", "n\u00fcch\u00b7terns\u00b7ten", "Be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.7": {"text": "Sitzt der geheime Schalk im Busen", "tokens": ["Sitzt", "der", "ge\u00b7hei\u00b7me", "Schalk", "im", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und trotzt der grauenvollen Macht.", "tokens": ["Und", "trotzt", "der", "grau\u00b7en\u00b7vol\u00b7len", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihr freilich, zehnfach Ungeplagte", "tokens": ["Ihr", "frei\u00b7lich", ",", "zehn\u00b7fach", "Un\u00b7ge\u00b7plag\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "$,", "VVFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Lebensernste, seid schon hold,", "tokens": ["Vom", "Le\u00b7ben\u00b7serns\u00b7te", ",", "seid", "schon", "hold", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich an Ph\u00f6bus Thron mich wagte,", "tokens": ["Da\u00df", "ich", "an", "Ph\u00f6\u00b7bus", "Thron", "mich", "wag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu zerren an dem Quastengold;", "tokens": ["Zu", "zer\u00b7ren", "an", "dem", "Quas\u00b7ten\u00b7gold", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df seine Glorien ich fange", "tokens": ["Da\u00df", "sei\u00b7ne", "Glo\u00b7ri\u00b7en", "ich", "fan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im Prisma bunter Ironie,", "tokens": ["Im", "Pris\u00b7ma", "bun\u00b7ter", "I\u00b7ro\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und steure seinem Lyraklange", "tokens": ["Und", "steu\u00b7re", "sei\u00b7nem", "Ly\u00b7ra\u00b7klan\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit wilder Faschingsmelodie.", "tokens": ["Mit", "wil\u00b7der", "Fa\u00b7schings\u00b7me\u00b7lo\u00b7die", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich m\u00f6cht' erheitern meine Freunde,", "tokens": ["Ich", "m\u00f6cht'", "er\u00b7hei\u00b7tern", "mei\u00b7ne", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O k\u00f6nnt' ich stillen ihre ", "tokens": ["O", "k\u00f6nnt'", "ich", "stil\u00b7len", "ih\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "VVFIN", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich m\u00f6chte necken meine Feinde,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "ne\u00b7cken", "mei\u00b7ne", "Fein\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Anderen willkommen sein.", "tokens": ["Und", "An\u00b7de\u00b7ren", "will\u00b7kom\u00b7men", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch kann ich nimmer euch die Rippen", "tokens": ["Doch", "kann", "ich", "nim\u00b7mer", "euch", "die", "Rip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ersch\u00fcttern, m\u00f6ge mein Gedicht", "tokens": ["Er\u00b7sch\u00fct\u00b7tern", ",", "m\u00f6\u00b7ge", "mein", "Ge\u00b7dicht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein L\u00e4cheln zwingen auf die Lippen", "tokens": ["Ein", "L\u00e4\u00b7cheln", "zwin\u00b7gen", "auf", "die", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Euch, die ihr's danket oder nicht.", "tokens": ["Euch", ",", "die", "ih\u00b7r's", "dan\u00b7ket", "o\u00b7der", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "KON", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Ihr aber, die des Daseins B\u00fcrde", "tokens": ["Ihr", "a\u00b7ber", ",", "die", "des", "Da\u00b7seins", "B\u00fcr\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "PRELS", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bis an das st\u00fcrmelose Grab", "tokens": ["Bis", "an", "das", "st\u00fcr\u00b7me\u00b7lo\u00b7se", "Grab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hinschleppt mit staunenswerther W\u00fcrde,", "tokens": ["Hin\u00b7schleppt", "mit", "stau\u00b7nens\u00b7wert\u00b7her", "W\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Legt eurer Gr\u00f6\u00dfe Togen ab!", "tokens": ["Legt", "eu\u00b7rer", "Gr\u00f6\u00b7\u00dfe", "To\u00b7gen", "ab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur, wer im Innersten zu hegen,", "tokens": ["Nur", ",", "wer", "im", "In\u00b7ners\u00b7ten", "zu", "he\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Zu tragen hat gerechten Schmerz,", "tokens": ["Zu", "tra\u00b7gen", "hat", "ge\u00b7rech\u00b7ten", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Soll still mein Buch zur Seite legen,", "tokens": ["Soll", "still", "mein", "Buch", "zur", "Sei\u00b7te", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Denn einfach ist das Menschenherz.", "tokens": ["Denn", "ein\u00b7fach", "ist", "das", "Men\u00b7schen\u00b7herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und mag ich zunftlos euch erscheinen,", "tokens": ["Und", "mag", "ich", "zunft\u00b7los", "euch", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihr zum Musenreigen schwebt,", "tokens": ["Die", "ihr", "zum", "Mu\u00b7sen\u00b7rei\u00b7gen", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und in den hehren, ewigreinen,", "tokens": ["Und", "in", "den", "heh\u00b7ren", ",", "e\u00b7wig\u00b7rei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den wunderbaren Rythmen lebt -", "tokens": ["Den", "wun\u00b7der\u00b7ba\u00b7ren", "Ry\u00b7th\u00b7men", "lebt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Schlingt eure seelenvollen T\u00e4nze,", "tokens": ["Schlingt", "eu\u00b7re", "see\u00b7len\u00b7vol\u00b7len", "T\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Noch ragt in's Blaue der Parna\u00df,", "tokens": ["Noch", "ragt", "in's", "Blau\u00b7e", "der", "Par\u00b7na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wer da berufen ist, kredenze", "tokens": ["Wer", "da", "be\u00b7ru\u00b7fen", "ist", ",", "kre\u00b7den\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "ADV", "VVPP", "VAFIN", "$,", "VVFIN"], "meter": "-+-+-++--", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Die Schale mit kastal'schem Na\u00df!", "tokens": ["Die", "Scha\u00b7le", "mit", "ka\u00b7stal'\u00b7schem", "Na\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch nicht das Zauberspiel der Grazien", "tokens": ["Doch", "nicht", "das", "Zau\u00b7ber\u00b7spiel", "der", "Gra\u00b7zi\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist Alles, was dem Dunkel wehrt,", "tokens": ["Ist", "Al\u00b7les", ",", "was", "dem", "Dun\u00b7kel", "wehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was von Silesien bis Alsazien,", "tokens": ["Was", "von", "Si\u00b7le\u00b7si\u00b7en", "bis", "Al\u00b7sa\u00b7zi\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NE", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom Golf zum Belt das Herz begehrt;", "tokens": ["Vom", "Golf", "zum", "Belt", "das", "Herz", "be\u00b7gehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bachant'scher Taumel packt zuweilen", "tokens": ["Bachant'\u00b7scher", "Tau\u00b7mel", "packt", "zu\u00b7wei\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Uns an, die Schale weicht dem Krug,", "tokens": ["Uns", "an", ",", "die", "Scha\u00b7le", "weicht", "dem", "Krug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Luft mu\u00df Fidelstrich zertheilen,", "tokens": ["Die", "Luft", "mu\u00df", "Fi\u00b7del\u00b7strich", "zer\u00b7thei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Purzelbaum nur thut genug.", "tokens": ["Ein", "Pur\u00b7zel\u00b7baum", "nur", "thut", "ge\u00b7nug", "."], "token_i