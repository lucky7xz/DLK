{"textgrid.poem.60341": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wild verwachsne dunkle Fichten,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wild verwachsne dunkle Fichten,", "tokens": ["Wild", "ver\u00b7wachs\u00b7ne", "dunk\u00b7le", "Fich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leise klagt die Quelle fort;", "tokens": ["Lei\u00b7se", "klagt", "die", "Quel\u00b7le", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herz, das ist der rechte Ort", "tokens": ["Herz", ",", "das", "ist", "der", "rech\u00b7te", "Ort"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr dein schmerzliches Verzichten!", "tokens": ["F\u00fcr", "dein", "schmerz\u00b7li\u00b7ches", "Ver\u00b7zich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Grauer Vogel in den Zweigen!", "tokens": ["Grau\u00b7er", "Vo\u00b7gel", "in", "den", "Zwei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einsam deine Klage singt,", "tokens": ["Ein\u00b7sam", "dei\u00b7ne", "Kla\u00b7ge", "singt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und auf deine Frage bringt", "tokens": ["Und", "auf", "dei\u00b7ne", "Fra\u00b7ge", "bringt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Antwort nicht des Waldes Schweigen.", "tokens": ["Ant\u00b7wort", "nicht", "des", "Wal\u00b7des", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenns auch immer schweigen bliebe,", "tokens": ["Wenns", "auch", "im\u00b7mer", "schwei\u00b7gen", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klage, klage fort; es weht,", "tokens": ["Kla\u00b7ge", ",", "kla\u00b7ge", "fort", ";", "es", "weht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der dich h\u00f6ret und versteht,", "tokens": ["Der", "dich", "h\u00f6\u00b7ret", "und", "ver\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Stille hier der Geist der Liebe.", "tokens": ["Stil\u00b7le", "hier", "der", "Geist", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nicht verloren hier im Moose,", "tokens": ["Nicht", "ver\u00b7lo\u00b7ren", "hier", "im", "Moo\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herz, dein heimlich Weinen geht,", "tokens": ["Herz", ",", "dein", "heim\u00b7lich", "Wei\u00b7nen", "geht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Liebe Gott versteht,", "tokens": ["Dei\u00b7ne", "Lie\u00b7be", "Gott", "ver\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine tiefe, hoffnungslose!", "tokens": ["Dei\u00b7ne", "tie\u00b7fe", ",", "hoff\u00b7nungs\u00b7lo\u00b7se", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}