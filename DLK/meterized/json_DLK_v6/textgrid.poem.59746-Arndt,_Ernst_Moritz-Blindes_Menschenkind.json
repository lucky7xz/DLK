{"textgrid.poem.59746": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Blindes Menschenkind", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O krankes, blindes Menschenkind,", "tokens": ["O", "kran\u00b7kes", ",", "blin\u00b7des", "Men\u00b7schen\u00b7kind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie wehet dich des Tages Wind", "tokens": ["Wie", "we\u00b7het", "dich", "des", "Ta\u00b7ges", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PRF", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein steuerloses Schiff im Meer", "tokens": ["Ein", "steu\u00b7er\u00b7lo\u00b7ses", "Schiff", "im", "Meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf wilden Wogen hin und her!", "tokens": ["Auf", "wil\u00b7den", "Wo\u00b7gen", "hin", "und", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vom bunten Schein, der immer log,", "tokens": ["Vom", "bun\u00b7ten", "Schein", ",", "der", "im\u00b7mer", "log", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verlockt, du w\u00e4hnst dich himmelhoch", "tokens": ["Ver\u00b7lockt", ",", "du", "w\u00e4hnst", "dich", "him\u00b7mel\u00b7hoch"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "PPER", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wei\u00dft mit aller Kunst und List", "tokens": ["Und", "wei\u00dft", "mit", "al\u00b7ler", "Kunst", "und", "List"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch nimmer, was und wo du bist.", "tokens": ["Doch", "nim\u00b7mer", ",", "was", "und", "wo", "du", "bist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWS", "KON", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O sei, o werde wieder dein!", "tokens": ["O", "sei", ",", "o", "wer\u00b7de", "wie\u00b7der", "dein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$,", "FM", "VAFIN", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und gleich wird dir gesunder sein;", "tokens": ["Und", "gleich", "wird", "dir", "ge\u00b7sun\u00b7der", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O kehre bei dir selbst doch ein!", "tokens": ["O", "keh\u00b7re", "bei", "dir", "selbst", "doch", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da leuchtet dir der rechte Schein.", "tokens": ["Da", "leuch\u00b7tet", "dir", "der", "rech\u00b7te", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Den Schl\u00fcssel nimm der linken Brust;", "tokens": ["Den", "Schl\u00fcs\u00b7sel", "nimm", "der", "lin\u00b7ken", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIMP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da liegt dein Schatz von Mut und Lust,", "tokens": ["Da", "liegt", "dein", "Schatz", "von", "Mut", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da schlie\u00dft dein Gl\u00fcck sich auf und zu:", "tokens": ["Da", "schlie\u00dft", "dein", "Gl\u00fcck", "sich", "auf", "und", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PRF", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ist dein Selbst, ja das bist du.", "tokens": ["Das", "ist", "dein", "Selbst", ",", "ja", "das", "bist", "du", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "ADV", "PDS", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da tief geheim liegt der Magnet,", "tokens": ["Da", "tief", "ge\u00b7heim", "liegt", "der", "Mag\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der ewig unverr\u00fccklich steht,", "tokens": ["Der", "e\u00b7wig", "un\u00b7ver\u00b7r\u00fcck\u00b7lich", "steht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Hauch, gehaucht vom h\u00f6chsten Geist,", "tokens": ["Der", "Hauch", ",", "ge\u00b7haucht", "vom", "h\u00f6chs\u00b7ten", "Geist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der ewig hin zum Himmel weist.", "tokens": ["Der", "e\u00b7wig", "hin", "zum", "Him\u00b7mel", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Das ist dein Evangelienbuch,", "tokens": ["Das", "ist", "dein", "E\u00b7van\u00b7ge\u00b7lien\u00b7buch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das spricht zu dir wie Gottes Spruch,", "tokens": ["Das", "spricht", "zu", "dir", "wie", "Got\u00b7tes", "Spruch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "KOKOM", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Angeld auf Unsterblichkeit,", "tokens": ["Dein", "An\u00b7geld", "auf", "U\u00b7nsterb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Anweisung auf die Ewigkeit.", "tokens": ["An\u00b7wei\u00b7sung", "auf", "die", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Das ist der Gottesstrahl und Blitz,", "tokens": ["Das", "ist", "der", "Got\u00b7tes\u00b7strahl", "und", "Blitz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zermalmend Trug und L\u00fcgenblitz,", "tokens": ["Zer\u00b7mal\u00b7mend", "Trug", "und", "L\u00fc\u00b7gen\u00b7blitz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Freudenschein und Schreckenschein,", "tokens": ["Der", "Freu\u00b7den\u00b7schein", "und", "Schre\u00b7cken\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der z\u00fcndend schl\u00e4gt durch Mark und Bein.", "tokens": ["Der", "z\u00fcn\u00b7dend", "schl\u00e4gt", "durch", "Mark", "und", "Bein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wo diese heil'ge Flamme brennt,", "tokens": ["Wo", "die\u00b7se", "heil'\u00b7ge", "Flam\u00b7me", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da brennt das Licht, das Gott erkennt,", "tokens": ["Da", "brennt", "das", "Licht", ",", "das", "Gott", "er\u00b7kennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Heldenkraft, die M\u00e4nnerkraft,", "tokens": ["Die", "Hel\u00b7den\u00b7kraft", ",", "die", "M\u00e4n\u00b7ner\u00b7kraft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Welten denkt und Welten schafft.", "tokens": ["Die", "Wel\u00b7ten", "denkt", "und", "Wel\u00b7ten", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Da bete an, da kniee hin,", "tokens": ["Da", "be\u00b7te", "an", ",", "da", "kni\u00b7ee", "hin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da st\u00e4hle frisch dir Herz und Sinn", "tokens": ["Da", "st\u00e4h\u00b7le", "frisch", "dir", "Herz", "und", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schau' und sieh, ob dein Magnet", "tokens": ["Und", "schau'", "und", "sieh", ",", "ob", "dein", "Mag\u00b7net"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zu seinem Nordpol richtig steht.", "tokens": ["Zu", "sei\u00b7nem", "Nord\u00b7pol", "rich\u00b7tig", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}