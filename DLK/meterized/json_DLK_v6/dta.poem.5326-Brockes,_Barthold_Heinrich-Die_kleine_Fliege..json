{"dta.poem.5326": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die kleine Fliege.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Neulich sah ich, mit Ergetzen,", "tokens": ["Neu\u00b7lich", "sah", "ich", ",", "mit", "Er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine kleine Fliege sich,", "tokens": ["Ei\u00b7ne", "klei\u00b7ne", "Flie\u00b7ge", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf ein Erlen-Bl\u00e4ttchen setzen,", "tokens": ["Auf", "ein", "Er\u00b7len\u00b7Bl\u00e4tt\u00b7chen", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deren Form verwunderlich", "tokens": ["De\u00b7ren", "Form", "ver\u00b7wun\u00b7der\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["PDS", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Von den Fingern der Natur,", "tokens": ["Von", "den", "Fin\u00b7gern", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "So an Farb\u2019, als an Figur,", "tokens": ["So", "an", "Fa\u00b7rb'", ",", "als", "an", "Fi\u00b7gur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "KOUS", "APPR", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.7": {"text": "Und an bunten Glantz gebildet.", "tokens": ["Und", "an", "bun\u00b7ten", "Glantz", "ge\u00b7bil\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Es war ihr klein K\u00f6pfgen gr\u00fcn,", "tokens": ["Es", "war", "ihr", "klein", "K\u00f6pf\u00b7gen", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "NN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Und ihr C\u00f6rperchen verg\u00fcldet,", "tokens": ["Und", "ihr", "C\u00f6r\u00b7per\u00b7chen", "ver\u00b7g\u00fcl\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Jhrer klaren Fl\u00fcgel Par,", "tokens": ["Ih\u00b7rer", "kla\u00b7ren", "Fl\u00fc\u00b7gel", "Par", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Wenn die Sonne sie beschien,", "tokens": ["Wenn", "die", "Son\u00b7ne", "sie", "be\u00b7schien", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "F\u00e4rbt\u2019 ein Roth fast wie Rubin,", "tokens": ["F\u00e4rbt'", "ein", "Roth", "fast", "wie", "Ru\u00b7bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "KOKOM", "NE", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.13": {"text": "Das, indem es wandelbar,", "tokens": ["Das", ",", "in\u00b7dem", "es", "wan\u00b7del\u00b7bar", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Auch zuweilen bl\u00e4ulich war.", "tokens": ["Auch", "zu\u00b7wei\u00b7len", "bl\u00e4u\u00b7lich", "war", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Liebster GOtt! wie kann doch hier", "tokens": ["Liebs\u00b7ter", "Gott", "!", "wie", "kann", "doch", "hier"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PWAV", "VMFIN", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Sich so mancher Farben Zier", "tokens": ["Sich", "so", "man\u00b7cher", "Far\u00b7ben", "Zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "PIAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Auf so kleinem Platz vereinen,", "tokens": ["Auf", "so", "klei\u00b7nem", "Platz", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Und mit solchem Glantz verm\u00e4hlen,", "tokens": ["Und", "mit", "sol\u00b7chem", "Glantz", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Da\u00df sie wie Metallen scheinen!", "tokens": ["Da\u00df", "sie", "wie", "Me\u00b7tal\u00b7len", "schei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOKOM", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Rief ich, mit vergn\u00fcgter Seelen.", "tokens": ["Rief", "ich", ",", "mit", "ver\u00b7gn\u00fcg\u00b7ter", "See\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Wie so k\u00fcnstlich! fiel mir ein,", "tokens": ["Wie", "so", "k\u00fcnst\u00b7lich", "!", "fiel", "mir", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "$.", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "M\u00fcssen hier die kleinen Theile", "tokens": ["M\u00fcs\u00b7sen", "hier", "die", "klei\u00b7nen", "Thei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "In einander eingeschrenckt,", "tokens": ["In", "ein\u00b7an\u00b7der", "ein\u00b7ge\u00b7schrenckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Durch einander hergelenckt,", "tokens": ["Durch", "ein\u00b7an\u00b7der", "her\u00b7ge\u00b7lenckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Wunderbar verbunden seyn!", "tokens": ["Wun\u00b7der\u00b7bar", "ver\u00b7bun\u00b7den", "seyn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Zu dem Endzweck, da\u00df der Schein", "tokens": ["Zu", "dem", "End\u00b7zweck", ",", "da\u00df", "der", "Schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Unsrer Sonnen und ihr Licht,", "tokens": ["Uns\u00b7rer", "Son\u00b7nen", "und", "ihr", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Das so wunderbarlich-sch\u00f6n,", "tokens": ["Das", "so", "wun\u00b7der\u00b7ba\u00b7rlich\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Und von uns sonst nicht zu sehn,", "tokens": ["Und", "von", "uns", "sonst", "nicht", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Unserm forschenden Gesicht", "tokens": ["Un\u00b7serm", "for\u00b7schen\u00b7den", "Ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}