{"textgrid.poem.34028": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Bei der Bestattung des Herzogs von Augustenburg", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Volk, was tummelst du dich? \u00bbDer Herzog wird ja begraben!", "tokens": ["Volk", ",", "was", "tum\u00b7melst", "du", "dich", "?", "\u00bb", "Der", "Her\u00b7zog", "wird", "ja", "be\u00b7gra\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "PPER", "PRF", "$.", "$(", "ART", "NE", "VAFIN", "ADV", "VVPP", "$."], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "H\u00f6rst du die Glocken denn nicht? Laut genug hallen sie doch!\u00ab", "tokens": ["H\u00f6rst", "du", "die", "Glo\u00b7cken", "denn", "nicht", "?", "Laut", "ge\u00b7nug", "hal\u00b7len", "sie", "doch", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "PTKNEG", "$.", "APPR", "ADV", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "+--+-+-+-+---+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "O, du gl\u00fcckliche Menge, dir kann es nimmermehr fehlen,", "tokens": ["O", ",", "du", "gl\u00fcck\u00b7li\u00b7che", "Men\u00b7ge", ",", "dir", "kann", "es", "nim\u00b7mer\u00b7mehr", "feh\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJA", "NN", "$,", "PPER", "VMFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Alles wird dir zum Fest, ganz, wie die Hochzeit, der Tod.", "tokens": ["Al\u00b7les", "wird", "dir", "zum", "Fest", ",", "ganz", ",", "wie", "die", "Hoch\u00b7zeit", ",", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "APPRART", "NN", "$,", "ADV", "$,", "PWAV", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "M\u00e4nner und Weiber, der Greis mit silberhaarigem Scheitel,", "tokens": ["M\u00e4n\u00b7ner", "und", "Wei\u00b7ber", ",", "der", "Greis", "mit", "sil\u00b7ber\u00b7haa\u00b7ri\u00b7gem", "Schei\u00b7tel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Und das quellende Kind dort auf dem Arme der Magd,", "tokens": ["Und", "das", "quel\u00b7len\u00b7de", "Kind", "dort", "auf", "dem", "Ar\u00b7me", "der", "Magd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Alle str\u00f6men herbei, sie wollen die stolzen Carossen", "tokens": ["Al\u00b7le", "str\u00f6\u00b7men", "her\u00b7bei", ",", "sie", "wol\u00b7len", "die", "stol\u00b7zen", "Ca\u00b7ros\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PTKVZ", "$,", "PPER", "VMFIN", "ART", "ADJA", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Sehen, die Pferde im Putz und die Lakaien im Staat.", "tokens": ["Se\u00b7hen", ",", "die", "Pfer\u00b7de", "im", "Putz", "und", "die", "La\u00b7kai\u00b7en", "im", "Staat", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "APPRART", "NN", "KON", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "W\u00fcrde er selbst hier, der Tod, bestattet, und l\u00e4ge die Hippe", "tokens": ["W\u00fcr\u00b7de", "er", "selbst", "hier", ",", "der", "Tod", ",", "be\u00b7stat\u00b7tet", ",", "und", "l\u00e4\u00b7ge", "die", "Hip\u00b7pe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "$,", "ART", "NN", "$,", "VVPP", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "+--+--+-+--+-+++", "measure": "dactylic.di.plus"}, "line.10": {"text": "Statt des Schwerts, auf dem Sarg, welche uns Alle bedroht,", "tokens": ["Statt", "des", "Schwerts", ",", "auf", "dem", "Sarg", ",", "wel\u00b7che", "uns", "Al\u00b7le", "be\u00b7droht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "PIS", "VVFIN", "$,"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Bunter w\u00e4re es nicht, und lustiger kaum, das Gefolge,", "tokens": ["Bun\u00b7ter", "w\u00e4\u00b7re", "es", "nicht", ",", "und", "lus\u00b7ti\u00b7ger", "kaum", ",", "das", "Ge\u00b7fol\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PTKNEG", "$,", "KON", "ADJD", "ADV", "$,", "ART", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Traun, sie blicken darein, wie es Unsterblichen ziemt.", "tokens": ["Traun", ",", "sie", "bli\u00b7cken", "da\u00b7rein", ",", "wie", "es", "U\u00b7nsterb\u00b7li\u00b7chen", "ziemt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PAV", "$,", "PWAV", "PPER", "NN", "VVFIN", "$."], "meter": "+-+--+--+---+", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Jener Greis, er z\u00e4hlt sein Alter von Hundert herunter,", "tokens": ["Je\u00b7ner", "Greis", ",", "er", "z\u00e4hlt", "sein", "Al\u00b7ter", "von", "Hun\u00b7dert", "her\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NE", "PTKVZ", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Und da bleiben ihm noch zwanzig der Jahre, und mehr,", "tokens": ["Und", "da", "blei\u00b7ben", "ihm", "noch", "zwan\u00b7zig", "der", "Jah\u00b7re", ",", "und", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "CARD", "ART", "NN", "$,", "KON", "ADV", "$,"], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Dieses Kind, es l\u00e4\u00dft gar von der Magd sich versprechen,", "tokens": ["Die\u00b7ses", "Kind", ",", "es", "l\u00e4\u00dft", "gar", "von", "der", "Magd", "sich", "ver\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Da\u00df man ihm morgen zur Lust Kaiser und K\u00f6nig begr\u00e4bt.", "tokens": ["Da\u00df", "man", "ihm", "mor\u00b7gen", "zur", "Lust", "Kai\u00b7ser", "und", "K\u00f6\u00b7nig", "be\u00b7gr\u00e4bt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "APPRART", "NN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Ich nur sehe den Todten mit seinem geschlossenen Auge,", "tokens": ["Ich", "nur", "se\u00b7he", "den", "Tod\u00b7ten", "mit", "sei\u00b7nem", "ge\u00b7schlos\u00b7se\u00b7nen", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+---+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Ich nur das lehmerne Bild, welches der Deckel verbirgt.", "tokens": ["Ich", "nur", "das", "leh\u00b7mer\u00b7ne", "Bild", ",", "wel\u00b7ches", "der", "De\u00b7ckel", "ver\u00b7birgt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Doch schon sehe auch ich ihn nicht mehr, dort l\u00e4chelt ein M\u00e4dchen,", "tokens": ["Doch", "schon", "se\u00b7he", "auch", "ich", "ihn", "nicht", "mehr", ",", "dort", "l\u00e4\u00b7chelt", "ein", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "PPER", "PPER", "PTKNEG", "ADV", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Und es kommt mir so vor, da\u00df sie mich kennt und mir winkt.", "tokens": ["Und", "es", "kommt", "mir", "so", "vor", ",", "da\u00df", "sie", "mich", "kennt", "und", "mir", "winkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}}}}}