{"textgrid.poem.26634": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja, schon schwillt und reift am Rebengel\u00e4nde der Donau", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, schon schwillt und reift am Rebengel\u00e4nde der Donau", "tokens": ["Ja", ",", "schon", "schwillt", "und", "reift", "am", "Re\u00b7ben\u00b7ge\u00b7l\u00e4n\u00b7de", "der", "Do\u00b7nau"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "ART", "NE"], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Saftig die Traube und blinkt unter den Blattern hervor.", "tokens": ["Saf\u00b7tig", "die", "Trau\u00b7be", "und", "blinkt", "un\u00b7ter", "den", "Blat\u00b7tern", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Bald auch naht sich der Winzer und h\u00e4lt ergiebige Lese,", "tokens": ["Bald", "auch", "naht", "sich", "der", "Win\u00b7zer", "und", "h\u00e4lt", "er\u00b7gie\u00b7bi\u00b7ge", "Le\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "ART", "NN", "KON", "VVFIN", "ADJA", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Die im Korb und im Fa\u00df S\u00e4ckel und Keller ihm f\u00fcllt.", "tokens": ["Die", "im", "Korb", "und", "im", "Fa\u00df", "S\u00e4\u00b7ckel", "und", "Kel\u00b7ler", "ihm", "f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "KON", "APPRART", "NN", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und nun zieht es hinaus in Schaaren nach Grinzing und Nu\u00dfdorf,", "tokens": ["Und", "nun", "zieht", "es", "hin\u00b7aus", "in", "Schaa\u00b7ren", "nach", "Grin\u00b7zing", "und", "Nu\u00df\u00b7dorf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "APPR", "NE", "KON", "NE", "$,"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Oder nach Sievering, wo delphisch das \u00bbBr\u00fcnndl\u00ab entspringt.", "tokens": ["O\u00b7der", "nach", "Sie\u00b7ve\u00b7ring", ",", "wo", "del\u00b7phisch", "das", "\u00bb", "Br\u00fcnndl", "\u00ab", "ent\u00b7springt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "NE", "$,", "PWAV", "ADJD", "ART", "$(", "NE", "$(", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "Lauter, lebendiger wird's in den bunt sich f\u00e4rbenden W\u00e4ldern:", "tokens": ["Lau\u00b7ter", ",", "le\u00b7ben\u00b7di\u00b7ger", "wird's", "in", "den", "bunt", "sich", "f\u00e4r\u00b7ben\u00b7den", "W\u00e4l\u00b7dern", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "VAFIN", "APPR", "ART", "ADJD", "PRF", "ADJA", "NN", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Fr\u00f6hliche Stimmen, Gesang \u2013 schweifende Menschen ringsum.", "tokens": ["Fr\u00f6h\u00b7li\u00b7che", "Stim\u00b7men", ",", "Ge\u00b7sang", "\u2013", "schwei\u00b7fen\u00b7de", "Men\u00b7schen", "ring\u00b7sum", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$(", "ADJA", "NN", "ADV", "$."], "meter": "+--+--+---+-+-", "measure": "dactylic.tri.plus"}, "line.9": {"text": "Hier gelagerte Gruppen \u2013 und dort im sch\u00fctzenden Dickicht", "tokens": ["Hier", "ge\u00b7la\u00b7ger\u00b7te", "Grup\u00b7pen", "\u2013", "und", "dort", "im", "sch\u00fct\u00b7zen\u00b7den", "Di\u00b7ckicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$(", "KON", "ADV", "APPRART", "ADJA", "NN"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.10": {"text": "Liebende Paare, die sich seliger Einsamkeit freu'n.", "tokens": ["Lie\u00b7ben\u00b7de", "Paa\u00b7re", ",", "die", "sich", "se\u00b7li\u00b7ger", "Ein\u00b7sam\u00b7keit", "freu'", "n."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADJA", "NN", "$,", "PRELS", "PRF", "ADJA", "NN", "VVFIN", "NE"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.11": {"text": "Aber sie Alle gewahrt man zuletzt in G\u00e4rten und Stuben,", "tokens": ["A\u00b7ber", "sie", "Al\u00b7le", "ge\u00b7wahrt", "man", "zu\u00b7letzt", "in", "G\u00e4r\u00b7ten", "und", "Stu\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PIS", "VVFIN", "PIS", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Wo, am Eingang gesteckt, lockend der \u00bbBuschen\u00ab ergr\u00fcnt.", "tokens": ["Wo", ",", "am", "Ein\u00b7gang", "ge\u00b7steckt", ",", "lo\u00b7ckend", "der", "\u00bb", "Bu\u00b7schen", "\u00ab", "er\u00b7gr\u00fcnt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "$,", "APPRART", "NN", "VVPP", "$,", "VVPP", "ART", "$(", "NN", "$(", "VVPP", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Sieh', da sitzen gedr\u00e4ngt sie an roh gezimmerten Tischen", "tokens": ["Sieh'", ",", "da", "sit\u00b7zen", "ge\u00b7dr\u00e4ngt", "sie", "an", "roh", "ge\u00b7zim\u00b7mer\u00b7ten", "Ti\u00b7schen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VVINF", "VVFIN", "PPER", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Bunt durcheinander: der Greis lockigem J\u00fcngling gesellt;", "tokens": ["Bunt", "durch\u00b7ein\u00b7an\u00b7der", ":", "der", "Greis", "lo\u00b7cki\u00b7gem", "J\u00fcng\u00b7ling", "ge\u00b7sellt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$.", "ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "M\u00fctter den T\u00f6chtern, und V\u00e4ter den Knaben, die m\u00fcd' sich gelaufen \u2013", "tokens": ["M\u00fct\u00b7ter", "den", "T\u00f6ch\u00b7tern", ",", "und", "V\u00e4\u00b7ter", "den", "Kna\u00b7ben", ",", "die", "m\u00fcd'", "sich", "ge\u00b7lau\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KON", "NN", "ART", "NN", "$,", "PRELS", "ADJD", "PRF", "VVPP", "$("], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Selbst der S\u00e4ugling liegt dort an der n\u00e4hrenden Brust.", "tokens": ["Selbst", "der", "S\u00e4ug\u00b7ling", "liegt", "dort", "an", "der", "n\u00e4h\u00b7ren\u00b7den", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.17": {"text": "Fr\u00f6hlich kredenzt, hemd\u00e4rmlig, der \u00bbHauer\u00ab den labenden Tropfen,", "tokens": ["Fr\u00f6h\u00b7lich", "kre\u00b7denzt", ",", "hemd\u00b7\u00e4rm\u00b7lig", ",", "der", "\u00bb", "Hau\u00b7er", "\u00ab", "den", "la\u00b7ben\u00b7den", "Trop\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "ADJD", "$,", "ART", "$(", "NN", "$(", "ART", "ADJA", "NN", "$,"], "meter": "+-+-++--+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Der als \u00bbHeuriger\u00ab licht blinkt im gehenkelten Glas.", "tokens": ["Der", "als", "\u00bb", "Heu\u00b7ri\u00b7ger", "\u00ab", "licht", "blinkt", "im", "ge\u00b7hen\u00b7kel\u00b7ten", "Glas", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "$(", "NN", "$(", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "--+--++-+-+-+", "measure": "anapaest.di.plus"}, "line.19": {"text": "O wie mundet der jetzt zu salzigem K\u00e4se und Rauchfleisch,", "tokens": ["O", "wie", "mun\u00b7det", "der", "jetzt", "zu", "sal\u00b7zi\u00b7gem", "K\u00e4\u00b7se", "und", "Rauch\u00b7fleisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "ART", "ADV", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Bei der \u00bbBretzen\u00ab Geknack, die man an St\u00e4ben verkauft!", "tokens": ["Bei", "der", "\u00bb", "Bret\u00b7zen", "\u00ab", "Ge\u00b7knack", ",", "die", "man", "an", "St\u00e4\u00b7ben", "ver\u00b7kauft", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$(", "NN", "$(", "NN", "$,", "PRELS", "PIS", "APPR", "NN", "VVPP", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.21": {"text": "Und man h\u00f6rt auch Musik: Harmonika, \u00bbKlampfe\u00ab und Geige \u2013", "tokens": ["Und", "man", "h\u00f6rt", "auch", "Mu\u00b7sik", ":", "Har\u00b7mo\u00b7ni\u00b7ka", ",", "\u00bb", "Klamp\u00b7fe", "\u00ab", "und", "Gei\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "NN", "$.", "NE", "$,", "$(", "NN", "$(", "KON", "NN", "$("], "meter": "--+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Rasender T\u00f6ne Gemisch schrillt in den Abend hinaus.", "tokens": ["Ra\u00b7sen\u00b7der", "T\u00f6\u00b7ne", "Ge\u00b7misch", "schrillt", "in", "den", "A\u00b7bend", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.23": {"text": "Lieder erschallen, urw\u00fcchsig und derb, mit verf\u00e4nglichen Texten,", "tokens": ["Lie\u00b7der", "er\u00b7schal\u00b7len", ",", "ur\u00b7w\u00fcch\u00b7sig", "und", "derb", ",", "mit", "ver\u00b7f\u00e4ng\u00b7li\u00b7chen", "Tex\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "ADJD", "KON", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.24": {"text": "Wie sie, satirischen Hangs, drastisch der Wiener ersinnt;", "tokens": ["Wie", "sie", ",", "sa\u00b7ti\u00b7ri\u00b7schen", "Hangs", ",", "dras\u00b7tisch", "der", "Wie\u00b7ner", "er\u00b7sinnt", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ADJA", "NN", "$,", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+---+--+", "measure": "iambic.hexa.chol"}, "line.25": {"text": "Wasserverschm\u00e4hende Oden manch eines volksth\u00fcmlichen Pindar,", "tokens": ["Was\u00b7ser\u00b7ver\u00b7schm\u00e4\u00b7hen\u00b7de", "O\u00b7den", "manch", "ei\u00b7nes", "volkst\u00b7h\u00fcm\u00b7li\u00b7chen", "Pin\u00b7dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.26": {"text": "Welcher den Pegasus nicht, aber den Kutschbock besteigt.", "tokens": ["Wel\u00b7cher", "den", "Pe\u00b7ga\u00b7sus", "nicht", ",", "a\u00b7ber", "den", "Kutschbock", "be\u00b7steigt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ART", "NE", "PTKNEG", "$,", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+---+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.27": {"text": "Ja, hier lebt noch das Volk! Hier schmausen die letzten Ph\u00e4aken,", "tokens": ["Ja", ",", "hier", "lebt", "noch", "das", "Volk", "!", "Hier", "schmau\u00b7sen", "die", "letz\u00b7ten", "Ph\u00e4\u00b7a\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "ADV", "ART", "NN", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+--+-+--", "measure": "anapaest.di.plus"}, "line.28": {"text": "Denen hohl\u00e4ugige Noth noch den \u00bbHamur\u00ab nicht verdarb.", "tokens": ["De\u00b7nen", "hoh\u00b7l\u00e4u\u00b7gi\u00b7ge", "Noth", "noch", "den", "\u00bb", "Ha\u00b7mur", "\u00ab", "nicht", "ver\u00b7darb", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "ADV", "ART", "$(", "NE", "$(", "PTKNEG", "VVFIN", "$."], "meter": "+-+---+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.29": {"text": "Wahrlich, ihr geht nicht unter, ihr Wiener! Dreht sich auch nicht mehr", "tokens": ["Wahr\u00b7lich", ",", "ihr", "geht", "nicht", "un\u00b7ter", ",", "ihr", "Wie\u00b7ner", "!", "Dreht", "sich", "auch", "nicht", "mehr"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "VVFIN", "PTKNEG", "APPR", "$,", "PPOSAT", "NN", "$.", "VVFIN", "PRF", "ADV", "PTKNEG", "ADV"], "meter": "+--+-+--+-+-+-+", "measure": "iambic.septa.invert"}, "line.30": {"text": "An dem Spie\u00dfe das Huhn \u2013 br\u00e4telt noch immer die Wurst.", "tokens": ["An", "dem", "Spie\u00b7\u00dfe", "das", "Huhn", "\u2013", "br\u00e4\u00b7telt", "noch", "im\u00b7mer", "die", "Wurst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$(", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "--+--++-+-+-+", "measure": "anapaest.di.plus"}}}}}