{"textgrid.poem.33990": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Eine Mondnacht in Rom", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bei'm D\u00e4mmerlicht des Mondes schau' ich gerne", "tokens": ["Bei'm", "D\u00e4m\u00b7mer\u00b7licht", "des", "Mon\u00b7des", "schau'", "ich", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der grauen Weltstadt br\u00f6ckelnde Ruinen,", "tokens": ["Der", "grau\u00b7en", "Welt\u00b7stadt", "br\u00f6\u00b7ckeln\u00b7de", "Ru\u00b7i\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Die uns als Maa\u00df f\u00fcr ihre Gr\u00f6\u00dfe dienen,", "tokens": ["Die", "uns", "als", "Maa\u00df", "f\u00fcr", "ih\u00b7re", "Gr\u00f6\u00b7\u00dfe", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOUS", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Woran der Mensch sich selber messen lerne;", "tokens": ["Wo\u00b7ran", "der", "Mensch", "sich", "sel\u00b7ber", "mes\u00b7sen", "ler\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn dieses Licht, das einem tr\u00fcben Sterne", "tokens": ["Denn", "die\u00b7ses", "Licht", ",", "das", "ei\u00b7nem", "tr\u00fc\u00b7ben", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entflie\u00dft, hat ihre Schlachten nie beschienen,", "tokens": ["Ent\u00b7flie\u00dft", ",", "hat", "ih\u00b7re", "Schlach\u00b7ten", "nie", "be\u00b7schie\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VAFIN", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur die Gefall'nen mit den eh'rnen Mienen,", "tokens": ["Nur", "die", "Ge\u00b7fall'\u00b7nen", "mit", "den", "eh'r\u00b7nen", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Umstanden von des Heeres bestem Kerne.", "tokens": ["Um\u00b7stan\u00b7den", "von", "des", "Hee\u00b7res", "bes\u00b7tem", "Ker\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Jetzt tr\u00e4gt sie selbst, wie die, den Todesstempel,", "tokens": ["Jetzt", "tr\u00e4gt", "sie", "selbst", ",", "wie", "die", ",", "den", "To\u00b7dess\u00b7tem\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PWAV", "ART", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Drum ziemt sich's, da\u00df dasselbe Licht ihr leuchte,", "tokens": ["Drum", "ziemt", "sich's", ",", "da\u00df", "das\u00b7sel\u00b7be", "Licht", "ihr", "leuch\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "$,", "KOUS", "PDAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann tr\u00e4umt vielleicht ein Dichter, da\u00df die Sonnen", "tokens": ["Dann", "tr\u00e4umt", "viel\u00b7leicht", "ein", "Dich\u00b7ter", ",", "da\u00df", "die", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Erl\u00f6schen, wie Pall\u00e4ste hier und Tempel", "tokens": ["Er\u00b7l\u00f6\u00b7schen", ",", "wie", "Pal\u00b7l\u00e4s\u00b7te", "hier", "und", "Tem\u00b7pel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zusammenst\u00fcrzen, und der oft verscheuchte", "tokens": ["Zu\u00b7sam\u00b7men\u00b7st\u00fcr\u00b7zen", ",", "und", "der", "oft", "ver\u00b7scheuch\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "ART", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vernichtungsengel jetzt den Sieg gewonnen!", "tokens": ["Ver\u00b7nich\u00b7tungs\u00b7en\u00b7gel", "jetzt", "den", "Sieg", "ge\u00b7won\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}