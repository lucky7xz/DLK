{"dta.poem.13334": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von der verbl\u00fcmten Schreibart. Ist die Fertsetzung der Abhandl. von dem Sinnr.  \n und Scharssinn, aus der Ankl. des verd. Geschmacks.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1742", "urn": "urn:nbn:de:kobv:b4-200905198519", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die dunckel-rothe Glut der Aramanthen,", "tokens": ["Die", "dun\u00b7ckel\u00b7ro\u00b7the", "Glut", "der", "A\u00b7ra\u00b7man\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der bunte Mahn, worauf, wie Diamanten,", "tokens": ["Der", "bun\u00b7te", "Mahn", ",", "wo\u00b7rauf", ",", "wie", "Di\u00b7a\u00b7man\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Tropfen Menge lag,", "tokens": ["Der", "Trop\u00b7fen", "Men\u00b7ge", "lag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Samt der Peonien Blut-rothem Funckeln;", "tokens": ["Samt", "der", "Pe\u00b7o\u00b7ni\u00b7en", "Blut\u00b7ro\u00b7them", "Fun\u00b7ckeln", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Convolvulen, Violen und Ranunckeln,", "tokens": ["Con\u00b7vol\u00b7vu\u00b7len", ",", "Vi\u00b7o\u00b7len", "und", "Ra\u00b7nun\u00b7ckeln", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Die theils, wie Himmel-Blau und Silber, theils verg\u00fcldet,", "tokens": ["Die", "theils", ",", "wie", "Him\u00b7mel\u00b7Blau", "und", "Sil\u00b7ber", ",", "theils", "ver\u00b7g\u00fcl\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "PWAV", "NN", "KON", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und theils in rothen Flammen gl\u00fch\u2019n. rc.", "tokens": ["Und", "theils", "in", "ro\u00b7then", "Flam\u00b7men", "gl\u00fch'", "n.", "rc", "."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "VVFIN", "APPR", "NE", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}