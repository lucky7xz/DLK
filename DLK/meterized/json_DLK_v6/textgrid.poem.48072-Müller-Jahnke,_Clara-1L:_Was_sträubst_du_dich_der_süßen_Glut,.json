{"textgrid.poem.48072": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was str\u00e4ubst du dich der s\u00fc\u00dfen Glut,", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was str\u00e4ubst du dich der s\u00fc\u00dfen Glut,", "tokens": ["Was", "str\u00e4ubst", "du", "dich", "der", "s\u00fc\u00b7\u00dfen", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die z\u00fcngelnd schon dein Haupt versengt,", "tokens": ["die", "z\u00fcn\u00b7gelnd", "schon", "dein", "Haupt", "ver\u00b7sengt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die liebehei\u00dfen Atems dich", "tokens": ["die", "lie\u00b7be\u00b7hei\u00b7\u00dfen", "A\u00b7tems", "dich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "mit Flammenarmen eng umdr\u00e4ngt?!", "tokens": ["mit", "Flam\u00b7me\u00b7nar\u00b7men", "eng", "um\u00b7dr\u00e4ngt", "?!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Glut bin ich \u2013 und du bist mein!", "tokens": ["Die", "Glut", "bin", "ich", "\u2013", "und", "du", "bist", "mein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$(", "KON", "PPER", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wirf ab, wirf ab das Alltagskleid:", "tokens": ["wirf", "ab", ",", "wirf", "ab", "das", "All\u00b7tags\u00b7kleid", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "VVIMP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "gib deine ganze Seele hin", "tokens": ["gib", "dei\u00b7ne", "gan\u00b7ze", "See\u00b7le", "hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "in ihrer nackten Herrlichkeit!", "tokens": ["in", "ih\u00b7rer", "nack\u00b7ten", "Herr\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Umschlingen will ich gl\u00fchend dich", "tokens": ["Um\u00b7schlin\u00b7gen", "will", "ich", "gl\u00fc\u00b7hend", "dich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "ADJD", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und pressen dich ans hei\u00dfe Herz,", "tokens": ["und", "pres\u00b7sen", "dich", "ans", "hei\u00b7\u00dfe", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die Kette schmelzen, die dich band,", "tokens": ["die", "Ket\u00b7te", "schmel\u00b7zen", ",", "die", "dich", "band", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "in meinem Ku\u00df wie tropfend Erz!", "tokens": ["in", "mei\u00b7nem", "Ku\u00df", "wie", "trop\u00b7fend", "Erz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KOKOM", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und fl\u00fcstern will ich dir ins Ohr", "tokens": ["Und", "fl\u00fcs\u00b7tern", "will", "ich", "dir", "ins", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ein W\u00f6rtlein, zaub'risch wunderfein,", "tokens": ["ein", "W\u00f6rt\u00b7lein", ",", "zaub'\u00b7risch", "wun\u00b7der\u00b7fein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "da\u00df du nichts andres denken sollst,", "tokens": ["da\u00df", "du", "nichts", "and\u00b7res", "den\u00b7ken", "sollst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "als mich allein, als mich allein . . .", "tokens": ["als", "mich", "al\u00b7lein", ",", "als", "mich", "al\u00b7lein", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "KOUS", "PPER", "ADV", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}