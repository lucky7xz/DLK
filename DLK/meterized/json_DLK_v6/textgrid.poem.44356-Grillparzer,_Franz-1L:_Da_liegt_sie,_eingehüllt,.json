{"textgrid.poem.44356": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da liegt sie, eingeh\u00fcllt,", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da liegt sie, eingeh\u00fcllt,", "tokens": ["Da", "liegt", "sie", ",", "ein\u00b7ge\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die hilflose Kleine,", "tokens": ["Die", "hilf\u00b7lo\u00b7se", "Klei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Eine Blume an Sch\u00f6nheit", "tokens": ["Ei\u00b7ne", "Blu\u00b7me", "an", "Sch\u00f6n\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und an Bewu\u00dftlosigkeit, da\u00df sie sch\u00f6n;", "tokens": ["Und", "an", "Be\u00b7wu\u00dft\u00b7lo\u00b7sig\u00b7keit", ",", "da\u00df", "sie", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "KOUS", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein leeres Blatt die Seele,", "tokens": ["Ein", "lee\u00b7res", "Blatt", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Sinne Griffel ohne F\u00fchrer,", "tokens": ["Die", "Sin\u00b7ne", "Grif\u00b7fel", "oh\u00b7ne", "F\u00fch\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Verstand ein Schreiber tief im Schlaf.", "tokens": ["Der", "Ver\u00b7stand", "ein", "Schrei\u00b7ber", "tief", "im", "Schlaf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Kein Geist rief noch: es werde Licht!", "tokens": ["Kein", "Geist", "rief", "noch", ":", "es", "wer\u00b7de", "Licht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "$.", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "\u00dcber der dunkeln Urnacht,", "tokens": ["\u00dc\u00b7ber", "der", "dun\u00b7keln", "Ur\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.10": {"text": "Und Mensch- und Tierheit streiten,", "tokens": ["Und", "Men\u00b7sch", "und", "Tier\u00b7heit", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "TRUNC", "KON", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Wem sie geh\u00f6rt.", "tokens": ["Wem", "sie", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Sie l\u00e4chelt! \u2013 Warum?", "tokens": ["Sie", "l\u00e4\u00b7chelt", "!", "\u2013", "Wa\u00b7rum", "?"], "token_info": ["word", "word", "punct", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PWAV", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Sie weint! \u2013 Weswegen?", "tokens": ["Sie", "weint", "!", "\u2013", "Wes\u00b7we\u00b7gen", "?"], "token_info": ["word", "word", "punct", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "O, la\u00dft sie weinen, l\u00e4cheln ohne Grund!", "tokens": ["O", ",", "la\u00dft", "sie", "wei\u00b7nen", ",", "l\u00e4\u00b7cheln", "oh\u00b7ne", "Grund", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "PPER", "VVINF", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gebt diese Kunst ihr mit ins Leben!", "tokens": ["Gebt", "die\u00b7se", "Kunst", "ihr", "mit", "ins", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDAT", "NN", "PPER", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der beste Grund zum Frohsinn ist der Frohsinn,", "tokens": ["Der", "bes\u00b7te", "Grund", "zum", "Froh\u00b7sinn", "ist", "der", "Froh\u00b7sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und m\u00f6g auch k\u00fcnftig, wenn sie weint,", "tokens": ["Und", "m\u00f6g", "auch", "k\u00fcnf\u00b7tig", ",", "wenn", "sie", "weint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nie das Bewu\u00dftsein sagen ihr, warum.", "tokens": ["Nie", "das", "Be\u00b7wu\u00df\u00b7tsein", "sa\u00b7gen", "ihr", ",", "wa\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "$,", "PWAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie rein die Stirn sich hebt,", "tokens": ["Wie", "rein", "die", "Stirn", "sich", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Wangen strotzend leuchten,", "tokens": ["Die", "Wan\u00b7gen", "strot\u00b7zend", "leuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Unterlippe, als zum Ku\u00df geformt,", "tokens": ["Die", "Un\u00b7ter\u00b7lip\u00b7pe", ",", "als", "zum", "Ku\u00df", "ge\u00b7formt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Rosenblatt, sich schwellend hebt,", "tokens": ["Ein", "Ro\u00b7sen\u00b7blatt", ",", "sich", "schwel\u00b7lend", "hebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vom Oberlippchen zierlich \u00fcberrandet", "tokens": ["Vom", "O\u00b7berl\u00b7ipp\u00b7chen", "zier\u00b7lich", "\u00fc\u00b7berr\u00b7an\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und Wang und Kinn mit ihren Gr\u00fcbchen", "tokens": ["Und", "Wang", "und", "Kinn", "mit", "ih\u00b7ren", "Gr\u00fcb\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zur strengen Sch\u00f6nheit f\u00fcgen s\u00fc\u00dfen Reiz!", "tokens": ["Zur", "stren\u00b7gen", "Sch\u00f6n\u00b7heit", "f\u00fc\u00b7gen", "s\u00fc\u00b7\u00dfen", "Reiz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Du bist sch\u00f6n, o Kleine!", "tokens": ["Du", "bist", "sch\u00f6n", ",", "o", "Klei\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "FM", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Und wirst es mehr noch sein, wenn nicht mehr klein.", "tokens": ["Und", "wirst", "es", "mehr", "noch", "sein", ",", "wenn", "nicht", "mehr", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "VAINF", "$,", "KOUS", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sei mir gegr\u00fc\u00dft, Gesegnete der G\u00f6tter!", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "Ge\u00b7se\u00b7gne\u00b7te", "der", "G\u00f6t\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "NN", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Denn, wahrlich, Sch\u00f6nheit ist der G\u00f6tter Segen!", "tokens": ["Denn", ",", "wahr\u00b7lich", ",", "Sch\u00f6n\u00b7heit", "ist", "der", "G\u00f6t\u00b7ter", "Se\u00b7gen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "$,", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So ausgeschieden sein vom Niedern und Gemeinen,", "tokens": ["So", "aus\u00b7ge\u00b7schie\u00b7den", "sein", "vom", "Nie\u00b7dern", "und", "Ge\u00b7mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAINF", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Am Fu\u00df der Himmelsleiter hingestellt,", "tokens": ["Am", "Fu\u00df", "der", "Him\u00b7mels\u00b7lei\u00b7ter", "hin\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die von der Erde aufsteigt zu den G\u00f6ttern", "tokens": ["Die", "von", "der", "Er\u00b7de", "auf\u00b7steigt", "zu", "den", "G\u00f6t\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und einen ewgen Mahner an der Seite,", "tokens": ["Und", "ei\u00b7nen", "ew\u00b7gen", "Mah\u00b7ner", "an", "der", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der leise ruft: Zerst\u00f6r mich nicht!", "tokens": ["Der", "lei\u00b7se", "ruft", ":", "Zer\u00b7st\u00f6r", "mich", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$.", "VVIMP", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Sch\u00f6ne, es ist gut und sch\u00f6n das Gute.", "tokens": ["Das", "Sch\u00f6\u00b7ne", ",", "es", "ist", "gut", "und", "sch\u00f6n", "das", "Gu\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VAFIN", "ADJD", "KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und so wirst du auch gut sein, gut wie sch\u00f6n,", "tokens": ["Und", "so", "wirst", "du", "auch", "gut", "sein", ",", "gut", "wie", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "VAINF", "$,", "ADJD", "KOKOM", "ADJD", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Und klug wie beides und verst\u00e4ndig.", "tokens": ["Und", "klug", "wie", "bei\u00b7des", "und", "ver\u00b7st\u00e4n\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "PIS", "KON", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Vaters Aug in deiner klaren Stirn,", "tokens": ["Des", "Va\u00b7ters", "Aug", "in", "dei\u00b7ner", "kla\u00b7ren", "Stirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es wird von Recht einst sprechen wie in seiner,", "tokens": ["Es", "wird", "von", "Recht", "einst", "spre\u00b7chen", "wie", "in", "sei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADV", "VVINF", "KOKOM", "APPR", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Mutter Mund ob deinem weichen Kinn,", "tokens": ["Der", "Mut\u00b7ter", "Mund", "ob", "dei\u00b7nem", "wei\u00b7chen", "Kinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KOUS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Er wird von Geist ert\u00f6nen wie bei ihr,", "tokens": ["Er", "wird", "von", "Geist", "er\u00b7t\u00f6\u00b7nen", "wie", "bei", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVINF", "KOKOM", "APPR", "PPOSAT", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und fester Sinn wird thronen in den Brauen.", "tokens": ["Und", "fes\u00b7ter", "Sinn", "wird", "thro\u00b7nen", "in", "den", "Brau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Was l\u00e4chelst du? als h\u00e4ttest du vernommen", "tokens": ["Was", "l\u00e4\u00b7chelst", "du", "?", "als", "h\u00e4t\u00b7test", "du", "ver\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$.", "KOKOM", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der allzuraschen Lippe weihend Lob.", "tokens": ["Der", "all\u00b7zu\u00b7ra\u00b7schen", "Lip\u00b7pe", "wei\u00b7hend", "Lob", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich sage dir, die G\u00fcte, die dich schm\u00fcckt,", "tokens": ["Ich", "sa\u00b7ge", "dir", ",", "die", "G\u00fc\u00b7te", ",", "die", "dich", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie wird dir einst der Tr\u00e4nen mehr entpressen,", "tokens": ["Sie", "wird", "dir", "einst", "der", "Tr\u00e4\u00b7nen", "mehr", "ent\u00b7pres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Als die Vergehung weinet und der Schmerz,", "tokens": ["Als", "die", "Ver\u00b7ge\u00b7hung", "wei\u00b7net", "und", "der", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und des Verstandes Fackel wird dir leuchten,", "tokens": ["Und", "des", "Ver\u00b7stan\u00b7des", "Fa\u00b7ckel", "wird", "dir", "leuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da, wo du w\u00fcnschtest lieber blind zu sein,", "tokens": ["Da", ",", "wo", "du", "w\u00fcnschtest", "lie\u00b7ber", "blind", "zu", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "VVFIN", "ADV", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Und spotten werden dein die andern Blinden.", "tokens": ["Und", "spot\u00b7ten", "wer\u00b7den", "dein", "die", "an\u00b7dern", "Blin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VAFIN", "PPOSAT", "ART", "ADJA", "NN", "$."], "meter": "-