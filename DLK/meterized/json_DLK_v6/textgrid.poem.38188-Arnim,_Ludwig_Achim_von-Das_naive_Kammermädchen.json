{"textgrid.poem.38188": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Das naive Kammerm\u00e4dchen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jezt bin ich wiederum recht vergn\u00fcgt,", "tokens": ["Jezt", "bin", "ich", "wie\u00b7de\u00b7rum", "recht", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Weil mein Schatz bei mir ist,", "tokens": ["Weil", "mein", "Schatz", "bei", "mir", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPER", "VAFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und so viele Treu verspricht,", "tokens": ["Und", "so", "vie\u00b7le", "Treu", "ver\u00b7spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Redet mit mir,", "tokens": ["Re\u00b7det", "mit", "mir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Redet von der Wahrheit,", "tokens": ["Re\u00b7det", "von", "der", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Redet von der Treuheit,", "tokens": ["Re\u00b7det", "von", "der", "Treu\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Redt von der Welt S\u00fc\u00dfigkeit,", "tokens": ["Redt", "von", "der", "Welt", "S\u00fc\u00b7\u00dfig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Redt von der Welt End.", "tokens": ["Redt", "von", "der", "Welt", "End", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wollt ihr wissen was es macht,", "tokens": ["Wollt", "ihr", "wis\u00b7sen", "was", "es", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df mich mein Schatz verlacht,", "tokens": ["Da\u00df", "mich", "mein", "Schatz", "ver\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ich im schwarzen Register mu\u00df stehn?", "tokens": ["Und", "ich", "im", "schwar\u00b7zen", "Re\u00b7gis\u00b7ter", "mu\u00df", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Du kannst leicht denken,", "tokens": ["Du", "kannst", "leicht", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Wie es mich thut kr\u00e4nken,", "tokens": ["Wie", "es", "mich", "thut", "kr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wenn ich eine andere mu\u00df bei dir sehn stehn;", "tokens": ["Wenn", "ich", "ei\u00b7ne", "an\u00b7de\u00b7re", "mu\u00df", "bei", "dir", "sehn", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "PIS", "VMFIN", "APPR", "PPER", "VVINF", "VVINF", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Denn du bist hochgesinnt,", "tokens": ["Denn", "du", "bist", "hoch\u00b7ge\u00b7sinnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Hast doch nichts hinter dir,", "tokens": ["Hast", "doch", "nichts", "hin\u00b7ter", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Als nur die Kleider, die du tr\u00e4gst,", "tokens": ["Als", "nur", "die", "Klei\u00b7der", ",", "die", "du", "tr\u00e4gst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wirst ausgelacht,", "tokens": ["Wirst", "aus\u00b7ge\u00b7lacht", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Eine Arme, die magst du nicht,", "tokens": ["Ei\u00b7ne", "Ar\u00b7me", ",", "die", "magst", "du", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.12": {"text": "Eine Reiche, die kriegst du nicht;", "tokens": ["Ei\u00b7ne", "Rei\u00b7che", ",", "die", "kriegst", "du", "nicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.13": {"text": "O weh, wie wird dirs noch gehn!", "tokens": ["O", "weh", ",", "wie", "wird", "dirs", "noch", "gehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PWAV", "VAFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}