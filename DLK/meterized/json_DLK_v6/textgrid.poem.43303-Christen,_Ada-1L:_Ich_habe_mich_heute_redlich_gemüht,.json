{"textgrid.poem.43303": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich habe mich heute redlich gem\u00fcht,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe mich heute redlich gem\u00fcht,", "tokens": ["Ich", "ha\u00b7be", "mich", "heu\u00b7te", "red\u00b7lich", "ge\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Schl\u00e4fe pochen, die Stirne gl\u00fcht,", "tokens": ["Die", "Schl\u00e4\u00b7fe", "po\u00b7chen", ",", "die", "Stir\u00b7ne", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So lange bin ich gesessen.", "tokens": ["So", "lan\u00b7ge", "bin", "ich", "ge\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich f\u00fcgte Rad und R\u00e4dchen geschwind,", "tokens": ["Ich", "f\u00fcg\u00b7te", "Rad", "und", "R\u00e4d\u00b7chen", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ich sprach mit Meister und Gesind,", "tokens": ["Ich", "sprach", "mit", "Meis\u00b7ter", "und", "Ge\u00b7sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lern' ich also vergessen? ...", "tokens": ["Lern'", "ich", "al\u00b7so", "ver\u00b7ges\u00b7sen", "?", "..."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "ADV", "VVPP", "$.", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}