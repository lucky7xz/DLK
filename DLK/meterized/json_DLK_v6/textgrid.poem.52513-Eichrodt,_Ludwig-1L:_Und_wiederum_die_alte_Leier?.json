{"textgrid.poem.52513": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und wiederum die alte Leier?", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wiederum die alte Leier?", "tokens": ["Und", "wie\u00b7de\u00b7rum", "die", "al\u00b7te", "Lei\u00b7er", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Poeten, stimmt ein Neues an!", "tokens": ["Po\u00b7et\u00b7en", ",", "stimmt", "ein", "Neu\u00b7es", "an", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist ja doch das beste Feuer", "tokens": ["Es", "ist", "ja", "doch", "das", "bes\u00b7te", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Minnesingen schon verthan.", "tokens": ["Im", "Min\u00b7ne\u00b7sin\u00b7gen", "schon", "ver\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, ihre Helden, ihre Dichter", "tokens": ["Ach", ",", "ih\u00b7re", "Hel\u00b7den", ",", "ih\u00b7re", "Dich\u00b7ter"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gefunden hat die Liebe l\u00e4ngst,", "tokens": ["Ge\u00b7fun\u00b7den", "hat", "die", "Lie\u00b7be", "l\u00e4ngst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und einem zeitgem\u00e4\u00dfen Richter", "tokens": ["Und", "ei\u00b7nem", "zeit\u00b7ge\u00b7m\u00e4\u00b7\u00dfen", "Rich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gefiele, da\u00df du frischer s\u00e4ngst.", "tokens": ["Ge\u00b7fie\u00b7le", ",", "da\u00df", "du", "fri\u00b7scher", "s\u00e4ngst", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Seit jedes s\u00fc\u00dfe Wort verwerthet,", "tokens": ["Seit", "je\u00b7des", "s\u00fc\u00b7\u00dfe", "Wort", "ver\u00b7wert\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Seit jede Wendung fein erlauscht,", "tokens": ["Seit", "je\u00b7de", "Wen\u00b7dung", "fein", "er\u00b7lauscht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "W\u00e4rs l\u00f6blich, da\u00df ihr euch bekehrtet,", "tokens": ["W\u00e4rs", "l\u00f6b\u00b7lich", ",", "da\u00df", "ihr", "euch", "be\u00b7kehr\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Poeten, die ihr seid berauscht!", "tokens": ["Po\u00b7et\u00b7en", ",", "die", "ihr", "seid", "be\u00b7rauscht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Reimt lieber Ungereimtes wieder,", "tokens": ["Reimt", "lie\u00b7ber", "Un\u00b7ge\u00b7reim\u00b7tes", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir sind auch des Vollkommnen m\u00fcd,", "tokens": ["Wir", "sind", "auch", "des", "Voll\u00b7komm\u00b7nen", "m\u00fcd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Versungen ist das Lied der Lieder,", "tokens": ["Ver\u00b7sun\u00b7gen", "ist", "das", "Lied", "der", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das hohe Lied, der Liebe Lied!", "tokens": ["Das", "ho\u00b7he", "Lied", ",", "der", "Lie\u00b7be", "Lied", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Versungen ist das Lied der Lieder,", "tokens": ["Ver\u00b7sun\u00b7gen", "ist", "das", "Lied", "der", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Menschenherzen einst durchbebt!", "tokens": ["Das", "Men\u00b7schen\u00b7her\u00b7zen", "einst", "durch\u00b7bebt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, dennoch, neu und ewig wieder!", "tokens": ["Und", ",", "den\u00b7noch", ",", "neu", "und", "e\u00b7wig", "wie\u00b7der", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "$,", "ADJD", "KON", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So es ein Menschenherz \u2013 erlebt.", "tokens": ["So", "es", "ein", "Men\u00b7schen\u00b7herz", "\u2013", "er\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "$(", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}