{"textgrid.poem.24139": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich fuhr ins fremde, weite Land; es war", "tokens": ["Ich", "fuhr", "ins", "frem\u00b7de", ",", "wei\u00b7te", "Land", ";", "es", "war"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "$,", "ADJA", "NN", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Fliehn vor mir, vor dir, vor allem, was", "tokens": ["Ein", "Fliehn", "vor", "mir", ",", "vor", "dir", ",", "vor", "al\u00b7lem", ",", "was"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "APPR", "PPER", "$,", "APPR", "PIS", "$,", "PWS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mich t\u00e4glich qu\u00e4lt und treibt und freudlos macht.", "tokens": ["Mich", "t\u00e4g\u00b7lich", "qu\u00e4lt", "und", "treibt", "und", "freud\u00b7los", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "KON", "VVFIN", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich wollte frei sein und Zuschauer sein,", "tokens": ["Ich", "woll\u00b7te", "frei", "sein", "und", "Zu\u00b7schau\u00b7er", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VAINF", "KON", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die H\u00e4nde auf dem R\u00fccken fremd das Fremde sehn.", "tokens": ["Die", "H\u00e4n\u00b7de", "auf", "dem", "R\u00fc\u00b7cken", "fremd", "das", "Frem\u00b7de", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und sieh, ich sehe nur zur\u00fcck und, ach!", "tokens": ["Und", "sieh", ",", "ich", "se\u00b7he", "nur", "zu\u00b7r\u00fcck", "und", ",", "ach", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "PTKVZ", "KON", "$,", "ITJ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In mich hinein und qu\u00e4le mich noch mehr", "tokens": ["In", "mich", "hin\u00b7ein", "und", "qu\u00e4\u00b7le", "mich", "noch", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und bin unruhiger, als je ich war.", "tokens": ["Und", "bin", "un\u00b7ru\u00b7hi\u00b7ger", ",", "als", "je", "ich", "war", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "$,", "KOUS", "ADV", "PPER", "VAFIN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Die bunte Welt umrauscht den Sinnenden,", "tokens": ["Die", "bun\u00b7te", "Welt", "um\u00b7rauscht", "den", "Sin\u00b7nen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.10": {"text": "Der immer nur den Nebelz\u00fcgen folgt,", "tokens": ["Der", "im\u00b7mer", "nur", "den", "Ne\u00b7bel\u00b7z\u00fc\u00b7gen", "folgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die innen unaufh\u00f6rlich hin und her,", "tokens": ["Die", "in\u00b7nen", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Tr\u00fcbselge Schatten, ziehn, wie im Gebirg", "tokens": ["Tr\u00fcb\u00b7sel\u00b7ge", "Schat\u00b7ten", ",", "ziehn", ",", "wie", "im", "Ge\u00b7birg"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVINF", "$,", "PWAV", "APPRART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.13": {"text": "Die grauen Wolken wandern. Wehe mir!", "tokens": ["Die", "grau\u00b7en", "Wol\u00b7ken", "wan\u00b7dern", ".", "We\u00b7he", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "In meinem Auge ist nicht mehr das Bild", "tokens": ["In", "mei\u00b7nem", "Au\u00b7ge", "ist", "nicht", "mehr", "das", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PTKNEG", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Der reichen Welt. Dem Maulwurf ward ich gleich,", "tokens": ["Der", "rei\u00b7chen", "Welt", ".", "Dem", "Maul\u00b7wurf", "ward", "ich", "gleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Der nur die engen G\u00e4nge sieht, die er durchw\u00fchlt.", "tokens": ["Der", "nur", "die", "en\u00b7gen", "G\u00e4n\u00b7ge", "sieht", ",", "die", "er", "durch\u00b7w\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}