{"textgrid.poem.47536": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[komm, komm, o du, der Anmuth]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm, komm, o du, der Anmuth", "tokens": ["Komm", ",", "komm", ",", "o", "du", ",", "der", "An\u00b7muth"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "FM", "PPER", "$,", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hellleuchtendes Gestirne,", "tokens": ["Hell\u00b7leuch\u00b7ten\u00b7des", "Ge\u00b7stir\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Entsende deine Strahlen,", "tokens": ["Ent\u00b7sen\u00b7de", "dei\u00b7ne", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Entfalte deine Zauber,", "tokens": ["Ent\u00b7fal\u00b7te", "dei\u00b7ne", "Zau\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Entraffe die Besinnung", "tokens": ["E\u00b7ntraf\u00b7fe", "die", "Be\u00b7sin\u00b7nung"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Selbstbeherrschungseitlen,", "tokens": ["Den", "Selbst\u00b7be\u00b7herr\u00b7schungs\u00b7eit\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Beweise, da\u00df die Sch\u00f6nheit", "tokens": ["Be\u00b7wei\u00b7se", ",", "da\u00df", "die", "Sch\u00f6n\u00b7heit"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gewaltiger, denn Alles,", "tokens": ["Ge\u00b7wal\u00b7ti\u00b7ger", ",", "denn", "Al\u00b7les", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Was sich hienieden stark nennt,", "tokens": ["Was", "sich", "hien\u00b7ie\u00b7den", "stark", "nennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Ein \u00fcberm\u00e4cht'ger Fallstrick", "tokens": ["Ein", "\u00fc\u00b7ber\u00b7m\u00e4cht'\u00b7ger", "Fall\u00b7strick"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "F\u00fcr jede stolze Tugend", "tokens": ["F\u00fcr", "je\u00b7de", "stol\u00b7ze", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Und steife Sitte sei!", "tokens": ["Und", "stei\u00b7fe", "Sit\u00b7te", "sei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Besch\u00e4me mir der Schule", "tokens": ["Be\u00b7sch\u00e4\u00b7me", "mir", "der", "Schu\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Gespreizte Majest\u00e4ten,", "tokens": ["Ge\u00b7spreiz\u00b7te", "Ma\u00b7jes\u00b7t\u00e4\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Verwirre mir der Fr\u00f6mmler", "tokens": ["Ver\u00b7wir\u00b7re", "mir", "der", "Fr\u00f6mm\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Geschraubte Purit\u00e4ten,", "tokens": ["Ge\u00b7schraub\u00b7te", "Pu\u00b7ri\u00b7t\u00e4\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Vernichte mir der Heuchler", "tokens": ["Ver\u00b7nich\u00b7te", "mir", "der", "Heuch\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Erlogne Dignit\u00e4ten,", "tokens": ["Er\u00b7log\u00b7ne", "Dig\u00b7ni\u00b7t\u00e4\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Zu deinen F\u00fc\u00dfen liege", "tokens": ["Zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", "lie\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Weltoffenbar und bettle", "tokens": ["Welt\u00b7of\u00b7fen\u00b7bar", "und", "bett\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Um einen Blick der Gnade", "tokens": ["Um", "ei\u00b7nen", "Blick", "der", "Gna\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Die ganze Clerisei!", "tokens": ["Die", "gan\u00b7ze", "Cle\u00b7ri\u00b7sei", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}