{"textgrid.poem.31218": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schon rasen \u00fcmb die Erde", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon rasen \u00fcmb die Erde", "tokens": ["Schon", "ra\u00b7sen", "\u00fcmb", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Herrn ", "tokens": ["Herrn"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "schon bohrt sich durch den dikken Schnee", "tokens": ["schon", "bohrt", "sich", "durch", "den", "dik\u00b7ken", "Schnee"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "der angenehme Mertzen-Klee.", "tokens": ["der", "an\u00b7ge\u00b7neh\u00b7me", "Mert\u00b7zen\u00b7Klee", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Darzwischen/ spizz und munter/", "tokens": ["Darz\u00b7wi\u00b7schen", "/", "spizz", "und", "mun\u00b7ter", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$(", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "steht g\u00f6lber Krokus drunter/", "tokens": ["steht", "g\u00f6l\u00b7ber", "Kro\u00b7kus", "drun\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PAV", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "darzu so plinckert schon durchs Gra\u00df", "tokens": ["dar\u00b7zu", "so", "plin\u00b7ckert", "schon", "durchs", "Gra\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "der Teich fast wie au\u00df Spihgel-Gla\u00df.", "tokens": ["der", "Teich", "fast", "wie", "au\u00df", "Spih\u00b7gel\u00b7Gla\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KOKOM", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "die H\u00e4sgens legen Eyer/", "tokens": ["die", "H\u00e4s\u00b7gens", "le\u00b7gen", "Ey\u00b7er", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "die Tichter tichten allbereit/", "tokens": ["die", "Tich\u00b7ter", "tich\u00b7ten", "all\u00b7be\u00b7reit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Bald nahn nun sampt den St\u00f6rchen", "tokens": ["Bald", "nahn", "nun", "sampt", "den", "St\u00f6r\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "die st\u00e4hts verlihbten L\u00f6rchen/", "tokens": ["die", "st\u00e4hts", "ver\u00b7lihb\u00b7ten", "L\u00f6r\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "schon traumt es ihnen jeden-falls", "tokens": ["schon", "traumt", "es", "ih\u00b7nen", "je\u00b7den\u00b7falls"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "von Krebs-Bluht und von M\u00fckken-Schmaltz", "tokens": ["von", "Krebs\u00b7Bluht", "und", "von", "M\u00fc\u00b7kken\u00b7Schmaltz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NE"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "in ", "tokens": ["in"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "der Himmel ferbt sich sanfften Blaus/", "tokens": ["der", "Him\u00b7mel", "ferbt", "sich", "sanff\u00b7ten", "Blaus", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "itzt/ Winter/ zeuch den Harnisch au\u00df!", "tokens": ["itzt", "/", "Win\u00b7ter", "/", "zeuch", "den", "Har\u00b7nisch", "au\u00df", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "NN", "$(", "VVIMP", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du hast uns sehr gefallen/", "tokens": ["Du", "hast", "uns", "sehr", "ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "dr\u00fcmb lohbt ich dich f\u00fcr allen/", "tokens": ["dr\u00fcmb", "lohbt", "ich", "dich", "f\u00fcr", "al\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "APPR", "PIAT", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "doch itzt/ so mu\u00df ich dir gestehn/", "tokens": ["doch", "itzt", "/", "so", "mu\u00df", "ich", "dir", "ge\u00b7stehn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "ADV", "VMFIN", "PPER", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "m\u00f6cht ich dich gern von Hindten sehn.", "tokens": ["m\u00f6cht", "ich", "dich", "gern", "von", "Hind\u00b7ten", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}