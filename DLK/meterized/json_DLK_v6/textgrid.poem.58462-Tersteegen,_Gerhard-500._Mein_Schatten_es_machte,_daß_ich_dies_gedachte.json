{"textgrid.poem.58462": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "500. Mein Schatten es machte, da\u00df ich dies gedachte", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Leib im Paradiesesstand", "tokens": ["Mein", "Leib", "im", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7stand"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ganz keinen Schatten sollte geben;", "tokens": ["Ganz", "kei\u00b7nen", "Schat\u00b7ten", "soll\u00b7te", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nun lebt er hier in Kreuz und Schand',", "tokens": ["Nun", "lebt", "er", "hier", "in", "Kreuz", "und", "Schand'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fchrt nur ein rechtes Schattenleben.", "tokens": ["F\u00fchrt", "nur", "ein", "rech\u00b7tes", "Schat\u00b7ten\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Drum ob der Schattenleib zerbricht,", "tokens": ["Drum", "ob", "der", "Schat\u00b7ten\u00b7leib", "zer\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich werd' ein edler Haus erlangen,", "tokens": ["Ich", "werd'", "ein", "ed\u00b7ler", "Haus", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Drin ich, durchstrahlt mit Gottes Licht,", "tokens": ["Drin", "ich", ",", "durch\u00b7strahlt", "mit", "Got\u00b7tes", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Werd' ewig ohne Schatten prangen.", "tokens": ["Werd'", "e\u00b7wig", "oh\u00b7ne", "Schat\u00b7ten", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}