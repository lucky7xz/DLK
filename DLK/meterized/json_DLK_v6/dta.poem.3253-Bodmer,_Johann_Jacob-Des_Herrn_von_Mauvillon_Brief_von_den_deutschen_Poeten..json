{"dta.poem.3253": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Des Herrn von Mauvillon Brief  \n von den deutschen Poeten.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1742", "urn": "urn:nbn:de:kobv:b4-200905198490", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was willst du mit dem Schatten zancken?", "tokens": ["Was", "willst", "du", "mit", "dem", "Schat\u00b7ten", "zan\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bewei\u00df an st\u00e4rckern deine Macht;", "tokens": ["Be\u00b7wei\u00df", "an", "st\u00e4r\u00b7ckern", "dei\u00b7ne", "Macht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer wird dir in der H\u00f6lle dancken?", "tokens": ["Wer", "wird", "dir", "in", "der", "H\u00f6l\u00b7le", "dan\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ach! Hast du die\u00df noch nicht bedacht? ", "tokens": ["Ach", "!", "Hast", "du", "die\u00df", "noch", "nicht", "be\u00b7dacht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VAFIN", "PPER", "PDS", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du kommst nut Donner, Blitz und Sturm;", "tokens": ["Du", "kommst", "nut", "Don\u00b7ner", ",", "Blitz", "und", "Sturm", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wer ist der grosse Feind? Ein Wurm.", "tokens": ["Wer", "ist", "der", "gros\u00b7se", "Feind", "?", "Ein", "Wurm", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}