{"dta.poem.7645": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von Ruhm und Ehre wird das Herz durchaus nicht satt;", "tokens": ["Von", "Ruhm", "und", "Eh\u00b7re", "wird", "das", "Herz", "durc\u00b7haus", "nicht", "satt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "ART", "NN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ehr hat es Ueberdru\u00df, ehr es Gen\u00fcge hat.", "tokens": ["Ehr", "hat", "es", "Ue\u00b7berd\u00b7ru\u00df", ",", "ehr", "es", "Ge\u00b7n\u00fc\u00b7ge", "hat", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "NN", "$,", "KOUS", "PPER", "NN", "VAFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Man sagt: Es klingt dein Ohr, wenn fern dein Ruhm ert\u00f6nt;", "tokens": ["Man", "sagt", ":", "Es", "klingt", "dein", "Ohr", ",", "wenn", "fern", "dein", "Ruhm", "er\u00b7t\u00f6nt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch schwache Dumpfheit ists, wenn es von selber dr\u00f6hnt.", "tokens": ["Doch", "schwa\u00b7che", "Dum\u00b7pf\u00b7heit", "ists", ",", "wenn", "es", "von", "sel\u00b7ber", "dr\u00f6hnt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "$,", "KOUS", "PPER", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Dir m\u00f6g' es weder so noch so im Ohre gellen;", "tokens": ["Dir", "m\u00f6g'", "es", "we\u00b7der", "so", "noch", "so", "im", "Oh\u00b7re", "gel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "KON", "ADV", "ADV", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zufriedne Stille wohn' in deines Herzens Zellen!", "tokens": ["Zu\u00b7fried\u00b7ne", "Stil\u00b7le", "wohn'", "in", "dei\u00b7nes", "Her\u00b7zens", "Zel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}