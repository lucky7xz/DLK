{"dta.poem.23594": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Europam\u00fcden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den M\u00e4dchen und den Flaschen", "tokens": ["Den", "M\u00e4d\u00b7chen", "und", "den", "Fla\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ward eure Jugendfrische;", "tokens": ["Ward", "eu\u00b7re", "Ju\u00b7gend\u00b7fri\u00b7sche", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr geht mit leeren Taschen", "tokens": ["Ihr", "geht", "mit", "lee\u00b7ren", "Ta\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Beim Alter jetzt zu Tische.", "tokens": ["Beim", "Al\u00b7ter", "jetzt", "zu", "Ti\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sehns\u00fcchtig nach dem Schlummer", "tokens": ["Sehn\u00b7s\u00fcch\u00b7tig", "nach", "dem", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sitzt ihr in eurer Kammer,", "tokens": ["Sitzt", "ihr", "in", "eu\u00b7rer", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und euer letzter Kummer \u2014", "tokens": ["Und", "eu\u00b7er", "letz\u00b7ter", "Kum\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es ist ein Katzenjammer.", "tokens": ["Es", "ist", "ein", "Kat\u00b7zen\u00b7jam\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}