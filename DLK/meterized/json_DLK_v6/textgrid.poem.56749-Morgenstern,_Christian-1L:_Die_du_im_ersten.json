{"textgrid.poem.56749": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die du im ersten", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die du im ersten", "tokens": ["Die", "du", "im", "ers\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "APPRART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "jungfr\u00e4ulichen Schnee", "tokens": ["jung\u00b7fr\u00e4u\u00b7li\u00b7chen", "Schnee"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "dort am fallenden Hang", "tokens": ["dort", "am", "fal\u00b7len\u00b7den", "Hang"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "ahnungsvoll schl\u00e4fst,", "tokens": ["ah\u00b7nungs\u00b7voll", "schl\u00e4fst", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "talbr\u00fcnstige Lawine!", "tokens": ["tal\u00b7br\u00fcns\u00b7ti\u00b7ge", "La\u00b7wi\u00b7ne", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wach auf!", "tokens": ["Wach", "auf", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Und trage mich!", "tokens": ["Und", "tra\u00b7ge", "mich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "wildestes Ro\u00df,", "tokens": ["wil\u00b7des\u00b7tes", "Ro\u00df", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "wieder hinab", "tokens": ["wie\u00b7der", "hin\u00b7ab"], "token_info": ["word", "word"], "pos": ["ADV", "PTKVZ"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "in der Menschen Gefilde!", "tokens": ["in", "der", "Men\u00b7schen", "Ge\u00b7fil\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.11": {"text": ". . . . . . . . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$."]}}, "stanza.2": {"line.1": {"text": "Die zierliche Flocke", "tokens": ["Die", "zier\u00b7li\u00b7che", "Flo\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "bewegt sich ... w\u00e4chst ...", "tokens": ["be\u00b7wegt", "sich", "...", "w\u00e4chst", "..."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PRF", "$(", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und st\u00fcrmt immer toller", "tokens": ["Und", "st\u00fcrmt", "im\u00b7mer", "tol\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "von Fels zu Fels ...", "tokens": ["von", "Fels", "zu", "Fels", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NE", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ich springe ihr nach", "tokens": ["Ich", "sprin\u00b7ge", "ihr", "nach"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "und fasse beherzt", "tokens": ["und", "fas\u00b7se", "be\u00b7herzt"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "in ihr wei\u00dfes,", "tokens": ["in", "ihr", "wei\u00b7\u00dfes", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "wehendes M\u00e4hnenhaar,", "tokens": ["we\u00b7hen\u00b7des", "M\u00e4h\u00b7nen\u00b7haar", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "indessen Phanta", "tokens": ["in\u00b7des\u00b7sen", "Phan\u00b7ta"], "token_info": ["word", "word"], "pos": ["ADV", "NE"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "den Renner lenkt,", "tokens": ["den", "Ren\u00b7ner", "lenkt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "wie auf rollender Kugel", "tokens": ["wie", "auf", "rol\u00b7len\u00b7der", "Ku\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "die G\u00f6ttin des Gl\u00fccks,", "tokens": ["die", "G\u00f6t\u00b7tin", "des", "Gl\u00fccks", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "hochaufgerichtet", "tokens": ["hoch\u00b7auf\u00b7ge\u00b7rich\u00b7tet"],