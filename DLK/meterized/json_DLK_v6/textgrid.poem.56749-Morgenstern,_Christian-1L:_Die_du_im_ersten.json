": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "und l\u00e4chelnd", "tokens": ["und", "l\u00e4\u00b7chelnd"], "token_info": ["word", "word"], "pos": ["KON", "ADJD"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "hilft mir Phanta auf.", "tokens": ["hilft", "mir", "Phan\u00b7ta", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Und dann \u2013 zerbricht sie", "tokens": ["Und", "dann", "\u2013", "zer\u00b7bricht", "sie"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$(", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "ihren Stab.", "tokens": ["ih\u00b7ren", "Stab", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": ". . . . . . . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$."]}}}}}