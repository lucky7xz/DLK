{"textgrid.poem.39193": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tr\u00e4nen, herzbluttriefende, gie\u00dfe stromweis,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00e4nen, herzbluttriefende, gie\u00dfe stromweis,", "tokens": ["Tr\u00e4\u00b7nen", ",", "herz\u00b7blut\u00b7trie\u00b7fen\u00b7de", ",", "gie\u00b7\u00dfe", "strom\u00b7weis", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Dichteraug', aufs Gr\u00fcn der Geschichte Deutschlands,", "tokens": ["Dich\u00b7ter\u00b7aug'", ",", "aufs", "Gr\u00fcn", "der", "Ge\u00b7schich\u00b7te", "Deutschlands", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "ART", "NN", "NE", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Drauf kolossisch f\u00e4llt des geborst'nen Hochbaus", "tokens": ["Drauf", "ko\u00b7los\u00b7sisch", "f\u00e4llt", "des", "ge\u00b7bor\u00b7st'\u00b7nen", "Hoch\u00b7baus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--++-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Taumelnder Schatten.", "tokens": ["Tau\u00b7meln\u00b7der", "Schat\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Nicht, o Deutschland, l\u00e4chelte Dein der Fremdling,", "tokens": ["Nicht", ",", "o", "Deutschland", ",", "l\u00e4\u00b7chel\u00b7te", "Dein", "der", "Fremd\u00b7ling", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "FM", "NE", "$,", "VVFIN", "PPOSAT", "ART", "NN", "$,"], "meter": "+-++--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Als vor Arnulfs Hiebe gest\u00fcrzt der Normann,", "tokens": ["Als", "vor", "Ar\u00b7nulfs", "Hie\u00b7be", "ge\u00b7st\u00fcrzt", "der", "Nor\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NE", "NN", "VVPP", "ART", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und auf Mers'burgs Au des Kosaken Ahn sich", "tokens": ["Und", "auf", "Mer\u00b7s'\u00b7burgs", "Au", "des", "Ko\u00b7sa\u00b7ken", "Ahn", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "NN", "ART", "NN", "NN", "PRF"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Kr\u00fcmmte, der Hunne.", "tokens": ["Kr\u00fcmm\u00b7te", ",", "der", "Hun\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Als zugleich drei P\u00e4psten das Diadem nahm", "tokens": ["Als", "zu\u00b7gleich", "drei", "P\u00e4ps\u00b7ten", "das", "Di\u00b7a\u00b7dem", "nahm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "CARD", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Auf Roncaglias Fluren der dritte Heinrich,", "tokens": ["Auf", "Ron\u00b7cag\u00b7li\u00b7as", "Flu\u00b7ren", "der", "drit\u00b7te", "Hein\u00b7rich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "ART", "ADJA", "NE", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Und geschleift am B\u00fcgel des Barbarossa", "tokens": ["Und", "ge\u00b7schleift", "am", "B\u00fc\u00b7gel", "des", "Bar\u00b7ba\u00b7ros\u00b7sa"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wimmerte Mailand.", "tokens": ["Wim\u00b7mer\u00b7te", "Mai\u00b7land", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Keinen Mann aufst\u00f6rt die gewaltige M\u00e4re; \u2013", "tokens": ["Kei\u00b7nen", "Mann", "auf\u00b7st\u00f6rt", "die", "ge\u00b7wal\u00b7ti\u00b7ge", "M\u00e4\u00b7re", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "+---+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Doch im Ost aufglimmend, ein rotes Sternbild,", "tokens": ["Doch", "im", "Ost", "auf\u00b7glim\u00b7mend", ",", "ein", "ro\u00b7tes", "Stern\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVPP", "$,", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Flammt ob Deutschlands Haupte des Moskowiten", "tokens": ["Flammt", "ob", "Deutschlands", "Haup\u00b7te", "des", "Mos\u00b7ko\u00b7wi\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOUS", "NE", "NN", "ART", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Kreisender S\u00e4bel.", "tokens": ["Krei\u00b7sen\u00b7der", "S\u00e4\u00b7bel", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}