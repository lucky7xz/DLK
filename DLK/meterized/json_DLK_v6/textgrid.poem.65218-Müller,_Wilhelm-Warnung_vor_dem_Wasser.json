{"textgrid.poem.65218": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Warnung vor dem Wasser", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Guckt nicht in Wasserquellen,", "tokens": ["Guckt", "nicht", "in", "Was\u00b7ser\u00b7quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr lustigen Gesellen,", "tokens": ["Ihr", "lus\u00b7ti\u00b7gen", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Guckt lieber in den Wein!", "tokens": ["Guckt", "lie\u00b7ber", "in", "den", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das Wasser ist betr\u00fcglich,", "tokens": ["Das", "Was\u00b7ser", "ist", "be\u00b7tr\u00fcg\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Quellen sind anz\u00fcglich:", "tokens": ["Die", "Quel\u00b7len", "sind", "an\u00b7z\u00fcg\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Guckt lieber in den Wein!", "tokens": ["Guckt", "lie\u00b7ber", "in", "den", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Narci\u00df, der hat's erfahren", "tokens": ["Nar\u00b7ci\u00df", ",", "der", "hat's", "er\u00b7fah\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In seinen sch\u00f6nsten Jahren:", "tokens": ["In", "sei\u00b7nen", "sch\u00f6ns\u00b7ten", "Jah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er sah nicht in dem Wein,", "tokens": ["Er", "sah", "nicht", "in", "dem", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nein, in dem Quell der Wildni\u00df,", "tokens": ["Nein", ",", "in", "dem", "Quell", "der", "Wild\u00b7ni\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Sein allerliebstes Bildni\u00df:", "tokens": ["Sein", "al\u00b7ler\u00b7liebs\u00b7tes", "Bild\u00b7ni\u00df", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Guckt lieber in den Wein!", "tokens": ["Guckt", "lie\u00b7ber", "in", "den", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Trink' ich aus vollem Glase,", "tokens": ["Trink'", "ich", "aus", "vol\u00b7lem", "Gla\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da spiegelt meine Nase", "tokens": ["Da", "spie\u00b7gelt", "mei\u00b7ne", "Na\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sich lang und roth im Wein", "tokens": ["Sich", "lang", "und", "roth", "im", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "KON", "ADJD", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie ist nicht zum Verlieben,", "tokens": ["Sie", "ist", "nicht", "zum", "Ver\u00b7lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie ist nicht zum Betr\u00fcben,", "tokens": ["Sie", "ist", "nicht", "zum", "Be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Drum guck' ich in den Wein.", "tokens": ["Drum", "guck'", "ich", "in", "den", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Schon Mancher ist versunken,", "tokens": ["Schon", "Man\u00b7cher", "ist", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch Keiner ist ertrunken", "tokens": ["Noch", "Kei\u00b7ner", "ist", "er\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PIS", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In einem Becher Wein.", "tokens": ["In", "ei\u00b7nem", "Be\u00b7cher", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die sich darin betrachten,", "tokens": ["Die", "sich", "da\u00b7rin", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PAV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie k\u00f6nnen nicht verschmachten,", "tokens": ["Sie", "k\u00f6n\u00b7nen", "nicht", "ver\u00b7schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Drum guck' ich in den Wein.", "tokens": ["Drum", "guck'", "ich", "in", "den", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ihr lustigen Gesellen,", "tokens": ["Ihr", "lus\u00b7ti\u00b7gen", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Guckt nicht in Wasserquellen,", "tokens": ["Guckt", "nicht", "in", "Was\u00b7ser\u00b7quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Guckt lieber in den Wein!", "tokens": ["Guckt", "lie\u00b7ber", "in", "den", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch \u00fcber euer Gucken", "tokens": ["Doch", "\u00fc\u00b7ber", "eu\u00b7er", "Gu\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Verge\u00dft auch nicht zu schlucken:", "tokens": ["Ver\u00b7ge\u00dft", "auch", "nicht", "zu", "schlu\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Trinkt aus, trinkt aus den Wein!", "tokens": ["Trinkt", "aus", ",", "trinkt", "aus", "den", "Wein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}}}}