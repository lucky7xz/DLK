{"dta.poem.8398": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "IiII.  \n  Opitz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1630", "urn": "urn:nbn:de:kobv:b4-200905197863", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Sch\u00e4fferey Von der Nimfen Hercinie. Breslau, 1630."}, "poem": {"stanza.1": {"line.1": {"text": "Auff jhr klugen Pierinnen/\nLa\u00dfet vns ein liedt beginnen\nEinem Helden der euch liebt/\nDer bey seinen sch\u00f6nen fl\u00fc\u00dfen/\nWelche sich herumb ergie\u00dfen/\nVns auch eine stelle giebt.", "tokens": ["Auff", "jhr", "klu\u00b7gen", "Pie\u00b7rin\u00b7nen", "/", "La\u00b7\u00dfet", "vns", "ein", "liedt", "be\u00b7gin\u00b7nen", "Ei\u00b7nem", "Hel\u00b7den", "der", "euch", "liebt", "/", "Der", "bey", "sei\u00b7nen", "sch\u00f6\u00b7nen", "fl\u00fc\u00b7\u00dfen", "/", "Wel\u00b7che", "sich", "he\u00b7rumb", "er\u00b7gie\u00b7\u00dfen", "/", "Vns", "auch", "ei\u00b7ne", "stel\u00b7le", "giebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$(", "VVIMP", "PPER", "ART", "ADJD", "VVFIN", "ART", "NN", "ART", "PPER", "VVFIN", "$(", "ART", "APPR", "PPOSAT", "ADJA", "VVINF", "$(", "PWS", "PRF", "APZR", "VVINF", "$(", "PPER", "ADV", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+--+-+-+-+-+-+-+-+-+--+-+-+-+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Wei\u00df er gleich mitt rittersachen\nIhm ein solches lob zue machen\nDas der alten namen gleicht/\nSo erkennt er doch da\u00df thaten\nIn die lange nacht gerhaten/\nWann jhr nicht die h\u00e4nde reicht.", "tokens": ["Wei\u00df", "er", "gleich", "mitt", "rit\u00b7ter\u00b7sa\u00b7chen", "Ihm", "ein", "sol\u00b7