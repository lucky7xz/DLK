{"textgrid.poem.65268": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fliegt der Schnee mir in's Gesicht,", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fliegt der Schnee mir in's Gesicht,", "tokens": ["Fliegt", "der", "Schnee", "mir", "in's", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00fcttl' ich ihn herunter.", "tokens": ["Sch\u00fcttl'", "ich", "ihn", "her\u00b7un\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn mein Herz im Busen spricht,", "tokens": ["Wenn", "mein", "Herz", "im", "Bu\u00b7sen", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sing' ich hell und munter.", "tokens": ["Sing'", "ich", "hell", "und", "mun\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "H\u00f6re nicht, was es mir sagt,", "tokens": ["H\u00f6\u00b7re", "nicht", ",", "was", "es", "mir", "sagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$,", "PWS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Habe keine Ohren.", "tokens": ["Ha\u00b7be", "kei\u00b7ne", "Oh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "F\u00fchle nicht, was es mir klagt,", "tokens": ["F\u00fch\u00b7le", "nicht", ",", "was", "es", "mir", "klagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PWS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Klagen ist f\u00fcr Thoren.", "tokens": ["Kla\u00b7gen", "ist", "f\u00fcr", "Tho\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Lustig in die Welt hinein", "tokens": ["Lus\u00b7tig", "in", "die", "Welt", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "APZR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gegen Wind und Wetter!", "tokens": ["Ge\u00b7gen", "Wind", "und", "Wet\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Will kein Gott auf Erden sein,", "tokens": ["Will", "kein", "Gott", "auf", "Er\u00b7den", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "APPR", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind wir selber G\u00f6tter.", "tokens": ["Sind", "wir", "sel\u00b7ber", "G\u00f6t\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}