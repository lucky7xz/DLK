{"textgrid.poem.37427": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Wanderlust", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Zeit, sie orgelt emsig weiter,", "tokens": ["Die", "Zeit", ",", "sie", "or\u00b7gelt", "em\u00b7sig", "wei\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Liedchen singt dir jeder Tag,", "tokens": ["Sein", "Lied\u00b7chen", "singt", "dir", "je\u00b7der", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vermischt mit T\u00f6nen, die nicht heiter,", "tokens": ["Ver\u00b7mischt", "mit", "T\u00f6\u00b7nen", ",", "die", "nicht", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "PRELS", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo keiner was von h\u00f6ren mag.", "tokens": ["Wo", "kei\u00b7ner", "was", "von", "h\u00f6\u00b7ren", "mag."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PWAV", "PIS", "PWS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie klingen fort. Und mit den Jahren", "tokens": ["Sie", "klin\u00b7gen", "fort", ".", "Und", "mit", "den", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wird draus ein voller Singverein.", "tokens": ["Wird", "draus", "ein", "vol\u00b7ler", "Sing\u00b7ver\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist, um aus der Haut zu fahren.", "tokens": ["Es", "ist", ",", "um", "aus", "der", "Haut", "zu", "fah\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUI", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du m\u00f6chtest gern wo anders sein.", "tokens": ["Du", "m\u00f6ch\u00b7test", "gern", "wo", "an\u00b7ders", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PWAV", "ADV", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun gut. Du mu\u00dft ja doch verreisen.", "tokens": ["Nun", "gut", ".", "Du", "mu\u00dft", "ja", "doch", "ver\u00b7rei\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "PPER", "VMFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So f\u00fclle denn den Wanderschlauch.", "tokens": ["So", "f\u00fcl\u00b7le", "denn", "den", "Wan\u00b7der\u00b7schlauch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vielleicht vernimmst du neue Weisen,", "tokens": ["Viel\u00b7leicht", "ver\u00b7nimmst", "du", "neu\u00b7e", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und H\u00fchneraugen kriegst du auch.", "tokens": ["Und", "H\u00fch\u00b7ner\u00b7au\u00b7gen", "kriegst", "du", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}