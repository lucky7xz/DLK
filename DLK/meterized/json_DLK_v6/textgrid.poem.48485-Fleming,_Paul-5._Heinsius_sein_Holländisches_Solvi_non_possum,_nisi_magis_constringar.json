{"textgrid.poem.48485": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "5. Heinsius sein Holl\u00e4ndisches Solvi non possum, nisi magis constringar", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wol dem, der in dem Feld' immitten unter Lanzen", "tokens": ["Wol", "dem", ",", "der", "in", "dem", "Feld'", "im\u00b7mit\u00b7ten", "un\u00b7ter", "Lan\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "$,", "PRELS", "APPR", "ART", "NN", "ADV", "APPR", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "und Degen, die man bl\u00f6\u00dft, den tr\u00fcben Tanz mag tanzen,", "tokens": ["und", "De\u00b7gen", ",", "die", "man", "bl\u00f6\u00dft", ",", "den", "tr\u00fc\u00b7ben", "Tanz", "mag", "tan\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "den Alle m\u00fcssen tun, der sinkend in den Sand", "tokens": ["den", "Al\u00b7le", "m\u00fcs\u00b7sen", "tun", ",", "der", "sin\u00b7kend", "in", "den", "Sand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VMFIN", "VVINF", "$,", "PRELS", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "den letzten Fu\u00dffall tut f\u00fcr Gott und vor sein Land!", "tokens": ["den", "letz\u00b7ten", "Fu\u00df\u00b7fall", "tut", "f\u00fcr", "Gott", "und", "vor", "sein", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "KON", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich aber armer Mensch hab' einen Krieg gewaget,", "tokens": ["Ich", "a\u00b7ber", "ar\u00b7mer", "Mensch", "hab'", "ei\u00b7nen", "Krieg", "ge\u00b7wa\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "da mir bei Furcht und Angst der Tod auch wird versaget.", "tokens": ["da", "mir", "bei", "Furcht", "und", "Angst", "der", "Tod", "auch", "wird", "ver\u00b7sa\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN", "ART", "NN", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mein Feind, der ist mein Lieb; die mir den Tod antut,", "tokens": ["Mein", "Feind", ",", "der", "ist", "mein", "Lieb", ";", "die", "mir", "den", "Tod", "an\u00b7tut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VAFIN", "PPOSAT", "NN", "$.", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "die acht' ich Allem vor und bin ihr mehr als gut.", "tokens": ["die", "acht'", "ich", "Al\u00b7lem", "vor", "und", "bin", "ihr", "mehr", "als", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PPER", "PIS", "PTKVZ", "KON", "VAFIN", "PPER", "PIAT", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ihr Antlitz ist ihr Schwert, die Worte sind die Klingen,", "tokens": ["Ihr", "Ant\u00b7litz", "ist", "ihr", "Schwert", ",", "die", "Wor\u00b7te", "sind", "die", "Klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "darmit sie mich verletzt, die Arme starke Schlingen,", "tokens": ["dar\u00b7mit", "sie", "mich", "ver\u00b7letzt", ",", "die", "Ar\u00b7me", "star\u00b7ke", "Schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVPP", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "darein sie mich verstrickt. Die Pfeile, mein Verdru\u00df,", "tokens": ["da\u00b7rein", "sie", "mich", "ver\u00b7strickt", ".", "Die", "Pfei\u00b7le", ",", "mein", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "PPER", "PRF", "VVPP", "$.", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "das sind die Augen selbst, die ich doch lieben mu\u00df.", "tokens": ["das", "sind", "die", "Au\u00b7gen", "selbst", ",", "die", "ich", "doch", "lie\u00b7ben", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "$,", "PRELS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "O freundliche Gewalt, wie soll ich mich doch retten,", "tokens": ["O", "freund\u00b7li\u00b7che", "Ge\u00b7walt", ",", "wie", "soll", "ich", "mich", "doch", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PWAV", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "der ich gebunden bin mit solchen s\u00fc\u00dfen Ketten?", "tokens": ["der", "ich", "ge\u00b7bun\u00b7den", "bin", "mit", "sol\u00b7chen", "s\u00fc\u00b7\u00dfen", "Ket\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "O Feind, den ich mir such', o Leiden ohne Pein,", "tokens": ["O", "Feind", ",", "den", "ich", "mir", "such'", ",", "o", "Lei\u00b7den", "oh\u00b7ne", "Pein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,", "FM", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "ich mu\u00df um los zu gehn noch mehr gebunden sein.", "tokens": ["ich", "mu\u00df", "um", "los", "zu", "gehn", "noch", "mehr", "ge\u00b7bun\u00b7den", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJD", "PTKZU", "VVINF", "ADV", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}