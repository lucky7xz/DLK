{"textgrid.poem.56283": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Der Kranke", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei mir gegr\u00fc\u00dft, o mein geliebter Wald!", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "o", "mein", "ge\u00b7lieb\u00b7ter", "Wald", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "FM", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du Schauplatz meiner Kindheit froher Spiele,", "tokens": ["Du", "Schau\u00b7platz", "mei\u00b7ner", "Kind\u00b7heit", "fro\u00b7her", "Spie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum letzten Mal gegr\u00fc\u00dft! ich scheide bald. \u2013", "tokens": ["Zum", "letz\u00b7ten", "Mal", "ge\u00b7gr\u00fc\u00dft", "!", "ich", "schei\u00b7de", "bald", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$.", "PPER", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So jung annoch, und schon am letzten Ziele!", "tokens": ["So", "jung", "an\u00b7noch", ",", "und", "schon", "am", "letz\u00b7ten", "Zie\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "$,", "KON", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dein Laub wird gelb und gelber, f\u00e4llt schon ab,", "tokens": ["Dein", "Laub", "wird", "gelb", "und", "gel\u00b7ber", ",", "f\u00e4llt", "schon", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJA", "$,", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich seh es wohl, und f\u00fchle mich gebrochen,", "tokens": ["Ich", "seh", "es", "wohl", ",", "und", "f\u00fch\u00b7le", "mich", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KON", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und blicke trauernd in mein fr\u00fches Grab.", "tokens": ["Und", "bli\u00b7cke", "trau\u00b7ernd", "in", "mein", "fr\u00fc\u00b7hes", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im Sommer hat der Arzt zu mir gesprochen:", "tokens": ["Im", "Som\u00b7mer", "hat", "der", "Arzt", "zu", "mir", "ge\u00b7spro\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbes prangt der Wald im gr\u00fcnen Schmuck noch heut,", "tokens": ["\u00bb", "es", "prangt", "der", "Wald", "im", "gr\u00fc\u00b7nen", "Schmuck", "noch", "heut", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du siehst ihn bald sich einmal noch entf\u00e4rben,", "tokens": ["Du", "siehst", "ihn", "bald", "sich", "ein\u00b7mal", "noch", "ent\u00b7f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PRF", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wann der Herbst sein falbes Laub verstreut,", "tokens": ["Und", "wann", "der", "Herbst", "sein", "fal\u00b7bes", "Laub", "ver\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So wirst du, Fr\u00fch-Verwelkter, selber sterben.\u00ab", "tokens": ["So", "wirst", "du", ",", "Fr\u00fch\u00b7Ver\u00b7welk\u00b7ter", ",", "sel\u00b7ber", "ster\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "NN", "$,", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es ist ein Gestern worden, unerh\u00f6rt!", "tokens": ["Es", "ist", "ein", "Ge\u00b7stern", "wor\u00b7den", ",", "un\u00b7er\u00b7h\u00f6rt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VAPP", "$,", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das Heut, wo du im gr\u00fcnen Schmuck gepranget;", "tokens": ["Das", "Heut", ",", "wo", "du", "im", "gr\u00fc\u00b7nen", "Schmuck", "ge\u00b7pran\u00b7get", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Herbst ist's: es f\u00e4llt dein Laub, wie sich's geh\u00f6rt,", "tokens": ["Herbst", "ist's", ":", "es", "f\u00e4llt", "dein", "Laub", ",", "wie", "sich's", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und mahnt mich, da\u00df der Tod nach mir verlanget.", "tokens": ["Und", "mahnt", "mich", ",", "da\u00df", "der", "Tod", "nach", "mir", "ver\u00b7lan\u00b7get", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O falle, Laub! ich kenne ja mein Los,", "tokens": ["O", "fal\u00b7le", ",", "Laub", "!", "ich", "ken\u00b7ne", "ja", "mein", "Los", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "NN", "$.", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu sterben ohne noch gelebt zu haben;", "tokens": ["Zu", "ster\u00b7ben", "oh\u00b7ne", "noch", "ge\u00b7lebt", "zu", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ADV", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie werden klanglos bald und namenlos", "tokens": ["Sie", "wer\u00b7den", "klang\u00b7los", "bald", "und", "na\u00b7men\u00b7los"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Am Fu\u00dfe dieser Eiche mich vergraben.", "tokens": ["Am", "Fu\u00b7\u00dfe", "die\u00b7ser", "Ei\u00b7che", "mich", "ver\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "O falle, Laub! dem Aug entziehe du", "tokens": ["O", "fal\u00b7le", ",", "Laub", "!", "dem", "Aug", "ent\u00b7zie\u00b7he", "du"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "NN", "$.", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Mutter, die mit Schmerzen mich geboren,", "tokens": ["Der", "Mut\u00b7ter", ",", "die", "mit", "Schmer\u00b7zen", "mich", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die schmerzlich stille St\u00e4tte meiner Ruh!", "tokens": ["Die", "schmerz\u00b7lich", "stil\u00b7le", "St\u00e4t\u00b7te", "mei\u00b7ner", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie hat die Hoffnung, unerf\u00fcllt, verloren.", "tokens": ["Sie", "hat", "die", "Hoff\u00b7nung", ",", "un\u00b7er\u00b7f\u00fcllt", ",", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wenn aber Eine kommt, die ich gemeint,", "tokens": ["Wenn", "a\u00b7ber", "Ei\u00b7ne", "kommt", ",", "die", "ich", "ge\u00b7meint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "VVFIN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sucht den kleinen Platz in Waldesr\u00e4umen,", "tokens": ["Und", "sucht", "den", "klei\u00b7nen", "Platz", "in", "Wal\u00b7des\u00b7r\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und auf den H\u00fcgel sie sich wirft und weint,", "tokens": ["Und", "auf", "den", "H\u00fc\u00b7gel", "sie", "sich", "wirft", "und", "weint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "PRF", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "O rausche, Laub! ich werde von ihr tr\u00e4umen.", "tokens": ["O", "rau\u00b7sche", ",", "Laub", "!", "ich", "wer\u00b7de", "von", "ihr", "tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "NN", "$.", "PPER", "VAFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Er lieget nun am Fu\u00df der Eiche dort,", "tokens": ["Er", "lie\u00b7get", "nun", "am", "Fu\u00df", "der", "Ei\u00b7che", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht aber ist, die er gemeint, gekommen,", "tokens": ["Nicht", "a\u00b7ber", "ist", ",", "die", "er", "ge\u00b7meint", ",", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "ADV", "VAFIN", "$,", "PRELS", "PPER", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es \u00fcberdecken Laub und Schnee den Ort,", "tokens": ["Es", "\u00fc\u00b7berd\u00b7e\u00b7cken", "Laub", "und", "Schnee", "den", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und weit umher wird nur das Wild vernommen.", "tokens": ["Und", "weit", "um\u00b7her", "wird", "nur", "das", "Wild", "ver\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKVZ", "VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}