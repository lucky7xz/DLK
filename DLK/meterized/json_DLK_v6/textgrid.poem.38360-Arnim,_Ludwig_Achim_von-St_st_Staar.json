{"textgrid.poem.38360": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "St st Staar", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Staar schw\u00e4zt, pfeift und singet, er ists, der alles kann,", "tokens": ["Der", "Staar", "schw\u00e4zt", ",", "pfeift", "und", "sin\u00b7get", ",", "er", "ists", ",", "der", "al\u00b7les", "kann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,", "PPER", "VAFIN", "$,", "PRELS", "PIS", "VMFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "In Kopf er alles bringet, nimmt, was er h\u00f6ret, an,", "tokens": ["In", "Kopf", "er", "al\u00b7les", "brin\u00b7get", ",", "nimmt", ",", "was", "er", "h\u00f6\u00b7ret", ",", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "PPER", "PIS", "VVFIN", "$,", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Er ist gar schlau und lose, und merket auf mit Flei\u00df,", "tokens": ["Er", "ist", "gar", "schlau", "und", "lo\u00b7se", ",", "und", "mer\u00b7ket", "auf", "mit", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "VVFIN", "$,", "KON", "VVFIN", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "W\u00e4scht oft sein schwarze Hose, und bringt sie nimmer wei\u00df.", "tokens": ["W\u00e4scht", "oft", "sein", "schwar\u00b7ze", "Ho\u00b7se", ",", "und", "bringt", "sie", "nim\u00b7mer", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "ADJA", "NN", "$,", "KON", "VVFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}