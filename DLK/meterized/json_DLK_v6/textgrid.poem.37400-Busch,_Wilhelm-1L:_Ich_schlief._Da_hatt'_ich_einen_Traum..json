{"textgrid.poem.37400": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich schlief. Da hatt' ich einen Traum.", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich schlief. Da hatt' ich einen Traum.", "tokens": ["Ich", "schlief", ".", "Da", "hatt'", "ich", "ei\u00b7nen", "Traum", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Ich verlie\u00df den Seelenraum.", "tokens": ["Mein", "Ich", "ver\u00b7lie\u00df", "den", "See\u00b7len\u00b7raum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Frei vom gemeinen Tagesleben,", "tokens": ["Frei", "vom", "ge\u00b7mei\u00b7nen", "Ta\u00b7ges\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vermocht ich leicht dahinzuschweben.", "tokens": ["Ver\u00b7mocht", "ich", "leicht", "da\u00b7hin\u00b7zu\u00b7schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So, angenehm mich fortbewegend,", "tokens": ["So", ",", "an\u00b7ge\u00b7nehm", "mich", "fort\u00b7be\u00b7we\u00b7gend", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erreicht ich eine sch\u00f6ne Gegend.", "tokens": ["Er\u00b7reicht", "ich", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Ge\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wohin ich schwebte, wuchs empor", "tokens": ["Wo\u00b7hin", "ich", "schweb\u00b7te", ",", "wuchs", "em\u00b7por"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Alsbald ein bunter Blumenflor,", "tokens": ["Als\u00b7bald", "ein", "bun\u00b7ter", "Blu\u00b7men\u00b7flor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und lustig schw\u00e4rmten um die Dolden", "tokens": ["Und", "lus\u00b7tig", "schw\u00e4rm\u00b7ten", "um", "die", "Dol\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Viel tausend Falter, rot und golden.", "tokens": ["Viel", "tau\u00b7send", "Fal\u00b7ter", ",", "rot", "und", "gol\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ganz nah auf einem Lilienstengel,", "tokens": ["Ganz", "nah", "auf", "ei\u00b7nem", "Li\u00b7li\u00b7ens\u00b7ten\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Einsam und sinnend, sa\u00df ein Engel,", "tokens": ["Ein\u00b7sam", "und", "sin\u00b7nend", ",", "sa\u00df", "ein", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Und weil das Land mir unbekannt,", "tokens": ["Und", "weil", "das", "Land", "mir", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Fragt ich: Wie nennt sich dieses Land?", "tokens": ["Fragt", "ich", ":", "Wie", "nennt", "sich", "die\u00b7ses", "Land", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "PWAV", "VVFIN", "PRF", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Hier, sprach er, \u00e4ndern sich die Dinge.", "tokens": ["Hier", ",", "sprach", "er", ",", "\u00e4n\u00b7dern", "sich", "die", "Din\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Du bist im Reich der Schmetterlinge.", "tokens": ["Du", "bist", "im", "Reich", "der", "Schmet\u00b7ter\u00b7lin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Ich aber, wohlgemut und heiter,", "tokens": ["Ich", "a\u00b7ber", ",", "wohl\u00b7ge\u00b7mut", "und", "hei\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Zog achtlos meines Weges weiter.", "tokens": ["Zog", "acht\u00b7los", "mei\u00b7nes", "We\u00b7ges", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Da kam, wie ich so weiter glitt,", "tokens": ["Da", "kam", ",", "wie", "ich", "so", "wei\u00b7ter", "glitt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Ein Frauenbild und schwebte mit", "tokens": ["Ein", "Frau\u00b7en\u00b7bild", "und", "schweb\u00b7te", "mit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "VVFIN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Als ein willkommenes Geleite,", "tokens": ["Als", "ein", "will\u00b7kom\u00b7me\u00b7nes", "Ge\u00b7lei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.22": {"text": "Anmutig l\u00e4chelnd mir zur Seite,", "tokens": ["An\u00b7mu\u00b7tig", "l\u00e4\u00b7chelnd", "mir", "zur", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PPER", "APPRART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.23": {"text": "Und um sie nie mehr loszulassen,", "tokens": ["Und", "um", "sie", "nie", "mehr", "los\u00b7zu\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Dacht ich die Holde zu umfassen;", "tokens": ["Dacht", "ich", "die", "Hol\u00b7de", "zu", "um\u00b7fas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Doch eh ich Zeit dazu gefunden,", "tokens": ["Doch", "eh", "ich", "Zeit", "da\u00b7zu", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Schl\u00fcpft sie hinweg und ist verschwunden.", "tokens": ["Schl\u00fcpft", "sie", "hin\u00b7weg", "und", "ist", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KON", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Mir war so schw\u00fcl. Ich mu\u00dfte trinken.", "tokens": ["Mir", "war", "so", "schw\u00fcl", ".", "Ich", "mu\u00df\u00b7te", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$.", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Nicht fern sah ich ein B\u00e4chlein blinken.", "tokens": ["Nicht", "fern", "sah", "ich", "ein", "B\u00e4ch\u00b7lein", "blin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Ich b\u00fcckte mich hinab zum Wasser.", "tokens": ["Ich", "b\u00fcck\u00b7te", "mich", "hin\u00b7ab", "zum", "Was\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Gleich fa\u00dft ein Arm, ein kalter, blasser,", "tokens": ["Gleich", "fa\u00dft", "ein", "Arm", ",", "ein", "kal\u00b7ter", ",", "blas\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Vom Grund herauf mich beim Genick.", "tokens": ["Vom", "Grund", "her\u00b7auf", "mich", "beim", "Ge\u00b7nick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Zwar zog ich eilig mich zur\u00fcck,", "tokens": ["Zwar", "zog", "ich", "ei\u00b7lig", "mich", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Allein der Hals war steif und krumm,", "tokens": ["Al\u00b7lein", "der", "Hals", "war", "steif", "und", "krumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Nur m\u00fchsam dreht ich ihn herum,", "tokens": ["Nur", "m\u00fch\u00b7sam", "dreht", "ich", "ihn", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Und ach, wie war es rings umher", "tokens": ["Und", "ach", ",", "wie", "war", "es", "rings", "um\u00b7her"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "XY", "$,", "PWAV", "VAFIN", "PPER", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Auf einmal traurig, \u00f6d und leer.", "tokens": ["Auf", "ein\u00b7mal", "trau\u00b7rig", ",", "\u00f6d", "und", "leer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Von Schmetterlingen nichts zu sehn,", "tokens": ["Von", "Schmet\u00b7ter\u00b7lin\u00b7gen", "nichts", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Die Blumen, eben noch so sch\u00f6n,", "tokens": ["Die", "Blu\u00b7men", ",", "e\u00b7ben", "noch", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "S\u00e4mtlich verdorrt, zerknickt, verkrumpelt.", "tokens": ["S\u00e4mt\u00b7lich", "ver\u00b7dorrt", ",", "zer\u00b7knickt", ",", "ver\u00b7krum\u00b7pelt", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "VVPP", "$,", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.40": {"text": "So bin ich seufzend fortgehumpelt,", "tokens": ["So", "bin", "ich", "seuf\u00b7zend", "fort\u00b7ge\u00b7hum\u00b7pelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Denn mit dem Fliegen, leicht und frei,", "tokens": ["Denn", "mit", "dem", "Flie\u00b7gen", ",", "leicht", "und", "frei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.42": {"text": "War es nun leider auch vorbei.", "tokens": ["War", "es", "nun", "lei\u00b7der", "auch", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Urpl\u00f6tzlich springt aus einem Graben,", "tokens": ["Ur\u00b7pl\u00f6tz\u00b7lich", "springt", "aus", "ei\u00b7nem", "Gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Begleitet vom Geschrei der Raben,", "tokens": ["Be\u00b7glei\u00b7tet", "vom", "Ge\u00b7schrei", "der", "Ra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Mir eine Hexe auf den Nacken", "tokens": ["Mir", "ei\u00b7ne", "He\u00b7xe", "auf", "den", "Na\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Und spornt mich an mit ihren Hacken", "tokens": ["Und", "spornt", "mich", "an", "mit", "ih\u00b7ren", "Ha\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Und macht sich schwer wie Bleigewichte", "tokens": ["Und", "macht", "sich", "schwer", "wie", "Blei\u00b7ge\u00b7wich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "KOKOM", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Und dr\u00fcckt und zwickt mich fast zunichte,", "tokens": ["Und", "dr\u00fcckt", "und", "zwickt", "mich", "fast", "zu\u00b7nich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Bis da\u00df ich matt und lendenlahm", "tokens": ["Bis", "da\u00df", "ich", "matt", "und", "len\u00b7den\u00b7lahm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Zu einem finstern Walde kam.", "tokens": ["Zu", "ei\u00b7nem", "fins\u00b7tern", "Wal\u00b7de", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Ein J\u00e4gersmann, d\u00fcrr von Gestalt,", "tokens": ["Ein", "J\u00e4\u00b7gers\u00b7mann", ",", "d\u00fcrr", "von", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.52": {"text": "Trat vor und rief ein dumpfes Halt.", "tokens": ["Trat", "vor", "und", "rief", "ein", "dum\u00b7pfes", "Halt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Schon liegt ein Pfeil auf seinem Bogen,", "tokens": ["Schon", "liegt", "ein", "Pfeil", "auf", "sei\u00b7nem", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "Schon ist die Sehne straff gezogen.", "tok