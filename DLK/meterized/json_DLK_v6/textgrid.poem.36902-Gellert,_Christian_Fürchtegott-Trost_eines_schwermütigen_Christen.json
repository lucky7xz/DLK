{"textgrid.poem.36902": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Trost eines schwerm\u00fctigen Christen", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du klagst, o Christ! in schweren Leiden,", "tokens": ["Du", "klagst", ",", "o", "Christ", "!", "in", "schwe\u00b7ren", "Lei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "$.", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und seufzest, da\u00df der Geist der Freuden", "tokens": ["Und", "seuf\u00b7zest", ",", "da\u00df", "der", "Geist", "der", "Freu\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von dir gewichen ist.", "tokens": ["Von", "dir", "ge\u00b7wi\u00b7chen", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du klagst und rufst: Herr, wie so lange?", "tokens": ["Du", "klagst", "und", "rufst", ":", "Herr", ",", "wie", "so", "lan\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$.", "NN", "$,", "PWAV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und Gott verzeiht, und dir wird bange,", "tokens": ["Und", "Gott", "ver\u00b7zeiht", ",", "und", "dir", "wird", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "KON", "PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df du von Gott verlassen bist.", "tokens": ["Da\u00df", "du", "von", "Gott", "ver\u00b7las\u00b7sen", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sind meine S\u00fcnden mir vergeben;", "tokens": ["Sind", "mei\u00b7ne", "S\u00fcn\u00b7den", "mir", "ver\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hat Gott mir S\u00fcnder Heil und Leben", "tokens": ["Hat", "Gott", "mir", "S\u00fcn\u00b7der", "Heil", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "PPER", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In seinem Sohn verliehn:", "tokens": ["In", "sei\u00b7nem", "Sohn", "ver\u00b7liehn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wo sind denn seines Geistes Triebe?", "tokens": ["Wo", "sind", "denn", "sei\u00b7nes", "Geis\u00b7tes", "Trie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Warum empfind ich nicht die Liebe,", "tokens": ["Wa\u00b7rum", "emp\u00b7find", "ich", "nicht", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und hoffe nicht getrost auf ihn?", "tokens": ["Und", "hof\u00b7fe", "nicht", "ge\u00b7trost", "auf", "ihn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "M\u00fchselig, sprichst du, und beladen", "tokens": ["M\u00fch\u00b7se\u00b7lig", ",", "sprichst", "du", ",", "und", "be\u00b7la\u00b7den"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "$,", "VVFIN", "PPER", "$,", "KON", "ADJA"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "H\u00f6r ich den Trost vom Wort der Gnaden,", "tokens": ["H\u00f6r", "ich", "den", "Trost", "vom", "Wort", "der", "Gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und ich empfind ihn nicht;", "tokens": ["Und", "ich", "emp\u00b7find", "ihn", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bin abgeneigt, vor Gott zu treten;", "tokens": ["Bin", "ab\u00b7ge\u00b7neigt", ",", "vor", "Gott", "zu", "tre\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bet, und kann nicht gl\u00e4ubig beten;", "tokens": ["Ich", "bet", ",", "und", "kann", "nicht", "gl\u00e4u\u00b7big", "be\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich denke Gott, doch ohne Licht.", "tokens": ["Ich", "den\u00b7ke", "Gott", ",", "doch", "oh\u00b7ne", "Licht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sonst war mir's Freude, seinen Willen", "tokens": ["Sonst", "war", "mir's", "Freu\u00b7de", ",", "sei\u00b7nen", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "NE", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von ganzem Herzen zu erf\u00fcllen;", "tokens": ["Von", "gan\u00b7zem", "Her\u00b7zen", "zu", "er\u00b7f\u00fcl\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Wort war mir gewi\u00df.", "tokens": ["Sein", "Wort", "war", "mir", "ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Itzt kann ich's nicht zu Herzen fassen,", "tokens": ["Itzt", "kann", "ich's", "nicht", "zu", "Her\u00b7zen", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PTKNEG", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und meine Kraft hat mich verlassen,", "tokens": ["Und", "mei\u00b7ne", "Kraft", "hat", "mich", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und meinen Geist deckt Finsternis.", "tokens": ["Und", "mei\u00b7nen", "Geist", "deckt", "Fins\u00b7ter\u00b7nis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Oft f\u00fchl ich Zweifel, die mich qu\u00e4len,", "tokens": ["Oft", "f\u00fchl", "ich", "Zwei\u00b7fel", ",", "die", "mich", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Heul oft vor Unruh meiner Seelen;", "tokens": ["Heul", "oft", "vor", "Un\u00b7ruh", "mei\u00b7ner", "See\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und meine H\u00fclf ist fern.", "tokens": ["Und", "mei\u00b7ne", "H\u00fclf", "ist", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich suche Ruh, die ich nicht finde;", "tokens": ["Ich", "su\u00b7che", "Ruh", ",", "die", "ich", "nicht", "fin\u00b7de", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In meinem Herzen wohnt nur S\u00fcnde,", "tokens": ["In", "mei\u00b7nem", "Her\u00b7zen", "wohnt", "nur", "S\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nur Unmut, keine Furcht des Herrn.", "tokens": ["Nur", "Un\u00b7mut", ",", "kei\u00b7ne", "Furcht", "des", "Herrn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Zag nicht, o Christ! denn deine Schmerzen,", "tokens": ["Zag", "nicht", ",", "o", "Christ", "!", "denn", "dei\u00b7ne", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "FM", "NN", "$.", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sind sichre Zeugen be\u00dfrer Herzen,", "tokens": ["Sind", "sich\u00b7re", "Zeu\u00b7gen", "be\u00df\u00b7rer", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als dir das deine scheint.", "tokens": ["Als", "dir", "das", "dei\u00b7ne", "scheint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie k\u00f6nntest du dich so betr\u00fcben,", "tokens": ["Wie", "k\u00f6nn\u00b7test", "du", "dich", "so", "be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df dir die Kraft fehlt, Gott zu lieben,", "tokens": ["Da\u00df", "dir", "die", "Kraft", "fehlt", ",", "Gott", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "W\u00e4r nicht dein Herz mit ihm vereint?", "tokens": ["W\u00e4r", "nicht", "dein", "Herz", "mit", "ihm", "ver\u00b7eint", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Kein Mensch vermag Gott zu erkennen,", "tokens": ["Kein", "Mensch", "ver\u00b7mag", "Gott", "zu", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch Jesum einen Herrn zu nennen,", "tokens": ["Noch", "Je\u00b7sum", "ei\u00b7nen", "Herrn", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als durch den heilgen Geist.", "tokens": ["Als", "durch", "den", "heil\u00b7gen", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hast du nicht diesen Geist empfangen?", "tokens": ["Hast", "du", "nicht", "die\u00b7sen", "Geist", "emp\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er ist's, der dich nach Gott verlangen,", "tokens": ["Er", "ist's", ",", "der", "dich", "nach", "Gott", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und sein Erbarmen suchen hei\u00dft.", "tokens": ["Und", "sein", "Er\u00b7bar\u00b7men", "su\u00b7chen", "hei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Vertrau auf Gott. Er wohnt bei denen,", "tokens": ["Ver\u00b7trau", "auf", "Gott", ".", "Er", "wohnt", "bei", "de\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$.", "PPER", "VVFIN", "APPR", "PRELS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die sich nach seiner H\u00fclfe sehnen;", "tokens": ["Die", "sich", "nach", "sei\u00b7ner", "H\u00fcl\u00b7fe", "seh\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er kennt und will dein Gl\u00fcck.", "tokens": ["Er", "kennt", "und", "will", "dein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VMFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er h\u00f6ret deines Weinens Stimme;", "tokens": ["Er", "h\u00f6\u00b7ret", "dei\u00b7nes", "Wei\u00b7nens", "Stim\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Verbirgt er gleich in seinem Grimme", "tokens": ["Ver\u00b7birgt", "er", "gleich", "in", "sei\u00b7nem", "Grim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sich einen kleinen Augenblick.", "tokens": ["Sich", "ei\u00b7nen", "klei\u00b7nen", "Au\u00b7gen\u00b7blick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Gott lie\u00df so manchen seiner Frommen", "tokens": ["Gott", "lie\u00df", "so", "man\u00b7chen", "sei\u00b7ner", "From\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In dies Gef\u00fchl des Elends kommen,", "tokens": ["In", "dies", "Ge\u00b7f\u00fchl", "des", "E\u00b7lends", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und stund ihm m\u00e4chtig bei.", "tokens": ["Und", "stund", "ihm", "m\u00e4ch\u00b7tig", "bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du sollst dein Nichts erkennen lernen,", "tokens": ["Du", "sollst", "dein", "Nichts", "er\u00b7ken\u00b7nen", "ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sollst das Vertraun auf dich entfernen,", "tokens": ["Sollst", "das", "Ver\u00b7traun", "auf", "dich", "ent\u00b7fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und sehn, was Gottes Gnade sei.", "tokens": ["Und", "sehn", ",", "was", "Got\u00b7tes", "Gna\u00b7de", "sei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PRELS", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Vor Sicherheit dich zu bewahren,", "tokens": ["Vor", "Si\u00b7cher\u00b7heit", "dich", "zu", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "L\u00e4\u00dft er dich seine Streng erfahren,", "tokens": ["L\u00e4\u00dft", "er", "dich", "sei\u00b7ne", "Streng", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und schickt dir diese Last.", "tokens": ["Und", "schickt", "dir", "die\u00b7se", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er reinigt dich wie Gold im Feuer,", "tokens": ["Er", "rei\u00b7nigt", "dich", "wie", "Gold", "im", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Macht dir das Heil der Seele teuer,", "tokens": ["Macht", "dir", "das", "Heil", "der", "See\u00b7le", "teu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Damit du haltest, was du hast.", "tokens": ["Da\u00b7mit", "du", "hal\u00b7test", ",", "was", "du", "hast", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "So wie ein Vater \u00fcber Kinder,", "tokens": ["So", "wie", "ein", "Va\u00b7ter", "\u00fc\u00b7ber", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erbarmet Gott sich \u00fcber S\u00fcnder,", "tokens": ["Er\u00b7bar\u00b7met", "Gott", "sich", "\u00fc\u00b7ber", "S\u00fcn\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die seinen Namen scheun.", "tokens": ["Die", "sei\u00b7nen", "Na\u00b7men", "scheun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dein Seufzen ist ihm nicht verborgen.", "tokens": ["Dein", "Seuf\u00b7zen", "ist", "ihm", "nicht", "ver\u00b7bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So fern der Abend ist vom Morgen,", "tokens": ["So", "fern", "der", "A\u00b7bend", "ist", "vom", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "L\u00e4\u00dft er von dir die S\u00fcnde sein.", "tokens": ["L\u00e4\u00dft", "er", "von", "dir", "die", "S\u00fcn\u00b7de", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Zwar ist um Trost dir itzo bange;", "tokens": ["Zwar", "ist", "um", "Trost", "dir", "it\u00b7zo", "ban\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denn alle Z\u00fcchtigung, so lange", "tokens": ["Denn", "al\u00b7le", "Z\u00fcch\u00b7ti\u00b7gung", ",", "so", "lan\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PIAT", "NN", "$,", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie da ist, scheint uns hart.", "tokens": ["Sie", "da", "ist", ",", "scheint", "uns", "hart", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch nachmals wird sie friedsam geben", "tokens": ["Doch", "nach\u00b7mals", "wird", "sie", "fried\u00b7sam", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Frucht der Gerechtigkeit und Leben", "tokens": ["Frucht", "der", "Ge\u00b7rech\u00b7tig\u00b7keit", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "KON", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Dem, der durch sie ge\u00fcbet ward