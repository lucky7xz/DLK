."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich st\u00e4rke dich, ich helfe dir!", "tokens": ["Ich", "st\u00e4r\u00b7ke", "dich", ",", "ich", "hel\u00b7fe", "dir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Auf, fasse dich in deinen N\u00f6ten!", "tokens": ["Auf", ",", "fas\u00b7se", "dich", "in", "dei\u00b7nen", "N\u00f6\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sprich: Wollte mich der Herr auch t\u00f6ten:", "tokens": ["Sprich", ":", "Woll\u00b7te", "mich", "der", "Herr", "auch", "t\u00f6\u00b7ten", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "VMFIN", "PRF", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So harr ich dennoch sein.", "tokens": ["So", "harr", "ich", "den\u00b7noch", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mir bleibt das Erbteil der Erl\u00f6sten;", "tokens": ["Mir", "bleibt", "das", "Erb\u00b7teil", "der", "Er\u00b7l\u00f6s\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.5": {"text": "Und will mich Gott nicht eher tr\u00f6sten,", "tokens": ["Und", "will", "mich", "Gott", "nicht", "e\u00b7her", "tr\u00f6s\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "NN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wird er mich doch im Tod erfreun.", "tokens": ["Wird", "er", "mich", "doch", "im", "Tod", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}