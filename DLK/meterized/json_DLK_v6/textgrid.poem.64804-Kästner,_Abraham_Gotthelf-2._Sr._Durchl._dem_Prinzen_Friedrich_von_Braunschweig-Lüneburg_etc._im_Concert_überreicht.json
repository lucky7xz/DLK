{"textgrid.poem.64804": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "2. Sr. Durchl. dem Prinzen Friedrich von Braunschweig-L\u00fcneburg etc. im Concert \u00fcberreicht", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst hing ein traurig Volk die Harfen an die Weiden,", "tokens": ["Einst", "hing", "ein", "trau\u00b7rig", "Volk", "die", "Har\u00b7fen", "an", "die", "Wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sang nur, wenn es sang, Gefangenschaft und Leiden.", "tokens": ["Und", "sang", "nur", ",", "wenn", "es", "sang", ",", "Ge\u00b7fan\u00b7gen\u00b7schaft", "und", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir, die ein gleicher Schmerz zwey Jahre lang gekr\u00e4nkt,", "tokens": ["Wir", ",", "die", "ein", "glei\u00b7cher", "Schmerz", "zwey", "Jah\u00b7re", "lang", "ge\u00b7kr\u00e4nkt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "ADJA", "NN", "CARD", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir haben an die Wand die Harfen nur geh\u00e4ngt:", "tokens": ["Wir", "ha\u00b7ben", "an", "die", "Wand", "die", "Har\u00b7fen", "nur", "ge\u00b7h\u00e4ngt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die B\u00e4ume konnten wir mit ihnen nicht beladen,", "tokens": ["Die", "B\u00e4u\u00b7me", "konn\u00b7ten", "wir", "mit", "ih\u00b7nen", "nicht", "be\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "APPR", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die senkten sich um uns, gespitzt zu Palissaden.", "tokens": ["Die", "senk\u00b7ten", "sich", "um", "uns", ",", "ge\u00b7spitzt", "zu", "Pa\u00b7lis\u00b7sa\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "PPER", "$,", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Noch tr\u00e4umend f\u00fchlen wir das Wohl, das uns geschehn:", "tokens": ["Noch", "tr\u00e4u\u00b7mend", "f\u00fch\u00b7len", "wir", "das", "Wohl", ",", "das", "uns", "ge\u00b7schehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O ", "tokens": ["O"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Der siegreich Friede bringt, und Hoffnung bessrer Zeiten,", "tokens": ["Der", "sieg\u00b7reich", "Frie\u00b7de", "bringt", ",", "und", "Hoff\u00b7nung", "bess\u00b7rer", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$,", "KON", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verzeih' den falschen Ton lang' ungestimmten Saiten.", "tokens": ["Ver\u00b7zeih'", "den", "fal\u00b7schen", "Ton", "lang'", "un\u00b7ge\u00b7stimm\u00b7ten", "Sai\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Herrscht Ruh' und Freyheit bald, f\u00fcr die ", "tokens": ["Herrscht", "Ruh'", "und", "Frey\u00b7heit", "bald", ",", "f\u00fcr", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "KON", "NN", "ADV", "$,", "APPR", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr die ", "tokens": ["F\u00fcr", "die"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "So wird der Dankbarkeit manch frohes Lied gelingen,", "tokens": ["So", "wird", "der", "Dank\u00b7bar\u00b7keit", "manch", "fro\u00b7hes", "Lied", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den Schutz Germaniens, die ", "tokens": ["Den", "Schutz", "Ger\u00b7ma\u00b7ni\u00b7ens", ",", "die"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "NN", "$,", "PRELS"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}