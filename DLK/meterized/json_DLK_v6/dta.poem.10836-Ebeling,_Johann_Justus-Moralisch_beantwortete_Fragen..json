{"dta.poem.10836": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Moralisch  \n beantwortete Fragen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Warum hat GOtt dir Mensch! zwo Au-\ngen und zwo Ohren,", "tokens": ["Wa\u00b7rum", "hat", "Gott", "dir", "Mensch", "!", "zwo", "Au", "gen", "und", "zwo", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NN", "PPER", "NN", "$.", "CARD", "NN", "VVPP", "KON", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dabei vor dem Mund nur eine", "tokens": ["Und", "da\u00b7bei", "vor", "dem", "Mund", "nur", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "APPR", "ART", "NN", "ADV", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Sch\u00f6pfer lehrt dadurch da\u00df es sey deine\nPflicht,", "tokens": ["Der", "Sch\u00f6p\u00b7fer", "lehrt", "da\u00b7durch", "da\u00df", "es", "sey", "dei\u00b7ne", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "KOUS", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+++-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Weil deine Zung offt mehr, als wie es billig", "tokens": ["Weil", "dei\u00b7ne", "Zung", "offt", "mehr", ",", "als", "wie", "es", "bil\u00b7lig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV", "$,", "KOUS", "PWAV", "PPER", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du solt mehr h\u00f6rn und sehn, als deine Zunge r\u00fch-", "tokens": ["Du", "solt", "mehr", "h\u00f6rn", "und", "sehn", ",", "als", "dei\u00b7ne", "Zun\u00b7ge", "r\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "KON", "VVINF", "$,", "KOUS", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn du dich, wie ein Mensch wilst weis und klug", "tokens": ["Wenn", "du", "dich", ",", "wie", "ein", "Mensch", "wilst", "weis", "und", "klug"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "$,", "PWAV", "ART", "NN", "ADJD", "PTKVZ", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Warum hat GOtt dem Mensch, nur einen Mund\ngeschenkt,", "tokens": ["Wa\u00b7rum", "hat", "Gott", "dem", "Mensch", ",", "nur", "ei\u00b7nen", "Mund", "ge\u00b7schenkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NN", "ART", "NN", "$,", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hingegen Hand und Fu\u00df, die an dem Leib ver-", "tokens": ["Hin\u00b7ge\u00b7gen", "Hand", "und", "Fu\u00df", ",", "die", "an", "dem", "Leib", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "NN", "$,", "PRELS", "APPR", "ART", "NN", "TRUNC"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Gedoppelt mitgetheilt? ", "tokens": ["Ge\u00b7dop\u00b7pelt", "mit\u00b7ge\u00b7theilt", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df er durch Hand und Fu\u00df, uns wolle Brodt", "tokens": ["Da\u00df", "er", "durch", "Hand", "und", "Fu\u00df", ",", "uns", "wol\u00b7le", "Brodt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN", "$,", "PPER", "VMFIN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Mensch soll flei\u00dfig seyn, um sich recht zu ern\u00e4hrn,", "tokens": ["Ein", "Mensch", "soll", "flei\u00b7\u00dfig", "seyn", ",", "um", "sich", "recht", "zu", "er\u00b7n\u00e4hrn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADJD", "VAINF", "$,", "KOUI", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch soll er so viel nicht, als er gewinnt, ver-", "tokens": ["Doch", "soll", "er", "so", "viel", "nicht", ",", "als", "er", "ge\u00b7winnt", ",", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "PTKNEG", "$,", "KOUS", "PPER", "VVFIN", "$,", "TRUNC"], "meter": "-+--+-++-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Warum fehln unserm Leib, die H\u00f6rner und die\nKlauen,", "tokens": ["Wa\u00b7rum", "fehln", "un\u00b7serm", "Leib", ",", "die", "H\u00f6r\u00b7ner", "und", "die", "Klau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das starke Schuzgewehr, das wir an Thieren", "tokens": ["Das", "star\u00b7ke", "Schuz\u00b7ge\u00b7wehr", ",", "das", "wir", "an", "Thie\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Weil unser Sch\u00f6pfer uns nicht auf die Welt\ngesezt,", "tokens": ["Weil", "un\u00b7ser", "Sch\u00f6p\u00b7fer", "uns", "nicht", "auf", "die", "Welt", "ge\u00b7sezt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "PTKNEG", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df unser N\u00e4chster w\u00fcrd durch unsern Grimm ver-", "tokens": ["Da\u00df", "un\u00b7ser", "N\u00e4chs\u00b7ter", "w\u00fcrd", "durch", "un\u00b7sern", "Grimm", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "APPR", "PPOSAT", "NE", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Liebe sol allein in unsern Staat regieren,", "tokens": ["Die", "Lie\u00b7be", "sol", "al\u00b7lein", "in", "un\u00b7sern", "Staat", "re\u00b7gie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die wilde Grausamkeit, bei denen wilden Thieren.", "tokens": ["Die", "wil\u00b7de", "Grau\u00b7sam\u00b7keit", ",", "bei", "de\u00b7nen", "wil\u00b7den", "Thie\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELS", "ADJA", "NN", "$."], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}}, "stanza.6": {"line.1": {"text": "Warum ist Ha\u00df und Neid, die Rachbegier, der\nZorn,", "tokens": ["Wa\u00b7rum", "ist", "Ha\u00df", "und", "Neid", ",", "die", "Rach\u00b7be\u00b7gier", ",", "der", "Zorn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NN", "KON", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wobei das Blut aufsch\u00e4ummt in unsrer Adern", "tokens": ["Wo\u00b7bei", "das", "Blut", "auf\u00b7sch\u00e4ummt", "in", "uns\u00b7rer", "A\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So sch\u00e4dlich der Natur? Warum sind wir ge-", "tokens": ["So", "sch\u00e4d\u00b7lich", "der", "Na\u00b7tur", "?", "Wa\u00b7rum", "sind", "wir", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "$.", "PWAV", "VAFIN", "PPER", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Da\u00df die Affecten uns an Leib und Leben straffen?", "tokens": ["Da\u00df", "die", "Af\u00b7fec\u00b7ten", "uns", "an", "Leib", "und", "Le\u00b7ben", "straf\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Darum da\u00df du vielmehr dieselben meiden solst,", "tokens": ["Da\u00b7rum", "da\u00df", "du", "viel\u00b7mehr", "die\u00b7sel\u00b7ben", "mei\u00b7den", "solst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "PPER", "ADV", "PDAT", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil du, wenn du das Blut, schnell durch die A-", "tokens": ["Weil", "du", ",", "wenn", "du", "das", "Blut", ",", "schnell", "durch", "die", "A"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "$,", "ADJD", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gleich einer ofnen See, die Koth und Unflat sch\u00e4u-", "tokens": ["Gleich", "ei\u00b7ner", "of\u00b7nen", "See", ",", "die", "Koth", "und", "Un\u00b7flat", "sch\u00e4u"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "ART", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn sich im starken Sturm das Heer der Wellen", "tokens": ["Wenn", "sich", "im", "star\u00b7ken", "Sturm", "das", "Heer", "der", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPRART", "ADJA", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Sch\u00f6pfer will: der Mensch sol sanfft und stille", "tokens": ["Der", "Sch\u00f6p\u00b7fer", "will", ":", "der", "Mensch", "sol", "sanfft", "und", "stil\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "$.", "ART", "NN", "VMFIN", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So spiegelt sich in ihm des Himmels Gnaden-", "tokens": ["So", "spie\u00b7gelt", "sich", "in", "ihm", "des", "Him\u00b7mels", "Gna\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PPER", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}