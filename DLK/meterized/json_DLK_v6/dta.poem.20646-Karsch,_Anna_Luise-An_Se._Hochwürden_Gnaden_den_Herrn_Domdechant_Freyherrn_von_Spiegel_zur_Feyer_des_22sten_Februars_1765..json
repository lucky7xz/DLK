{"dta.poem.20646": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  Se. Hochw\u00fcrden Gnaden  \n den  \n Herrn Domdechant  \n  Freyherrn von Spiegel  \n zur Feyer des 22sten Februars 1765.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Siehst Du den alten hochbeschneyten Brocken                 ", "tokens": ["Siehst", "Du", "den", "al\u00b7ten", "hoch\u00b7be\u00b7schney\u00b7ten", "Bro\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Freund? sein Haupt, so blendend wei\u00df,", "tokens": ["O", "Freund", "?", "sein", "Haupt", ",", "so", "blen\u00b7dend", "wei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPOSAT", "NN", "$,", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Nestors dreymal hundertj\u00e4hrge Locken,", "tokens": ["Wie", "Nes\u00b7tors", "drey\u00b7mal", "hun\u00b7dert\u00b7j\u00e4hr\u00b7ge", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verh\u00fcllt sich jezt in wolkig Eis.", "tokens": ["Ver\u00b7h\u00fcllt", "sich", "jezt", "in", "wol\u00b7kig", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Jezt spare nicht der grau gewordnen Eichen", "tokens": ["Jezt", "spa\u00b7re", "nicht", "der", "grau", "ge\u00b7word\u00b7nen", "Ei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKNEG", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zerspaltne Wipfel am Kamin,", "tokens": ["Zer\u00b7spalt\u00b7ne", "Wip\u00b7fel", "am", "Ka\u00b7min", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wirf Knoten nach, vom Stamme, der den Streichen", "tokens": ["Wirf", "Kno\u00b7ten", "nach", ",", "vom", "Stam\u00b7me", ",", "der", "den", "Strei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "PTKVZ", "$,", "APPRART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des sch\u00e4rfsten Beils zu trotzen schien,", "tokens": ["Des", "sch\u00e4rfs\u00b7ten", "Beils", "zu", "trot\u00b7zen", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und fordre Wein, den Hochheims Kelter pre\u00dfte,", "tokens": ["Und", "ford\u00b7re", "Wein", ",", "den", "Hoch\u00b7heims", "Kel\u00b7ter", "pre\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als Friedrichs Stirn drey Kr\u00e4nze trug,", "tokens": ["Als", "Fried\u00b7richs", "Stirn", "drey", "Kr\u00e4n\u00b7ze", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Er den Feind wie trockne Fichten\u00e4ste", "tokens": ["Und", "Er", "den", "Feind", "wie", "trock\u00b7ne", "Fich\u00b7ten\u00b7\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bey Kesselsdorf zu Boden schlug.", "tokens": ["Bey", "Kes\u00b7sels\u00b7dorf", "zu", "Bo\u00b7den", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Genie\u00df des gegenw\u00e4rtgen Tages Stunden,", "tokens": ["Ge\u00b7nie\u00df", "des", "ge\u00b7gen\u00b7w\u00e4rt\u00b7gen", "Ta\u00b7ges", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der k\u00fcnftge bleibt noch ungewi\u00df.", "tokens": ["Der", "k\u00fcnft\u00b7ge", "bleibt", "noch", "un\u00b7ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hast Du nicht schon des Schicksals Arm empfunden,", "tokens": ["Hast", "Du", "nicht", "schon", "des", "Schick\u00b7sals", "Arm", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der grimmig Dir am Herzen ri\u00df?", "tokens": ["Der", "grim\u00b7mig", "Dir", "am", "Her\u00b7zen", "ri\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ward nicht Dein Auge zweymal schon verschleyert", "tokens": ["Ward", "nicht", "Dein", "Au\u00b7ge", "zwey\u00b7mal", "schon", "ver\u00b7schley\u00b7ert"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "NN", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Todes Dunkel? sah nicht j\u00fcngst", "tokens": ["Vom", "To\u00b7des", "Dun\u00b7kel", "?", "sah", "nicht", "j\u00fcngst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "NN", "$.", "VVFIN", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein G *, der Dich bey Hundert Bechern feyert,", "tokens": ["Dein", "G", "*", ",", "der", "Dich", "bey", "Hun\u00b7dert", "Be\u00b7chern", "fe\u00b7yert", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "XY", "$,", "PRELS", "PRF", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Den Weg, den Du beynahe gingst?", "tokens": ["Den", "Weg", ",", "den", "Du", "bey\u00b7na\u00b7he", "gingst", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sah nicht Dein Geist schon jene Lorbeerhaine,", "tokens": ["Sah", "nicht", "Dein", "Geist", "schon", "je\u00b7ne", "Lor\u00b7beer\u00b7hai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "NN", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo Pindar an Homerens Hand", "tokens": ["Wo", "Pin\u00b7dar", "an", "Ho\u00b7me\u00b7rens", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "APPR", "NE", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Vertraulich geht, und Sapho\u2019s Schatten keine", "tokens": ["Ver\u00b7trau\u00b7lich", "geht", ",", "und", "Sa\u00b7pho's", "Schat\u00b7ten", "kei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "KON", "NE", "NN", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ganz d\u00fcstre Trauergrotte fand?", "tokens": ["Ganz", "d\u00fcst\u00b7re", "Trau\u00b7er\u00b7grot\u00b7te", "fand", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ein Gott, ein Gott befreyte von dem Grabe", "tokens": ["Ein", "Gott", ",", "ein", "Gott", "be\u00b7frey\u00b7te", "von", "dem", "Gra\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den deutschen Tirteus, welcher nur", "tokens": ["Den", "deut\u00b7schen", "Tir\u00b7teus", ",", "wel\u00b7cher", "nur"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Leben sch\u00e4tzt, als eine neue Gabe", "tokens": ["Sein", "Le\u00b7ben", "sch\u00e4tzt", ",", "als", "ei\u00b7ne", "neu\u00b7e", "Ga\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der allbeseelenden Natur,", "tokens": ["Der", "all\u00b7be\u00b7see\u00b7len\u00b7den", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Weil er Dich funfzig Lenze zu genie\u00dfen", "tokens": ["Weil", "er", "Dich", "funf\u00b7zig", "Len\u00b7ze", "zu", "ge\u00b7nie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "CARD", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch hoffet, und von Dir gef\u00fchrt", "tokens": ["Noch", "hof\u00b7fet", ",", "und", "von", "Dir", "ge\u00b7f\u00fchrt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KON", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durchs Blumenthal den Balsamduft der s\u00fc\u00dfen", "tokens": ["Durchs", "Blu\u00b7ment\u00b7hal", "den", "Bal\u00b7sam\u00b7duft", "der", "s\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bethauten Rose st\u00e4rker sp\u00fcrt.", "tokens": ["Be\u00b7thau\u00b7ten", "Ro\u00b7se", "st\u00e4r\u00b7ker", "sp\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}