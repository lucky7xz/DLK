{"textgrid.poem.42604": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Rosengarten", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom sch\u00f6nen Rosengarten", "tokens": ["Vom", "sch\u00f6\u00b7nen", "Ro\u00b7sen\u00b7gar\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Will ich mit Sang euch melden.", "tokens": ["Will", "ich", "mit", "Sang", "euch", "mel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Am Morgen lustwandelten Fraun,", "tokens": ["Am", "Mor\u00b7gen", "lust\u00b7wan\u00b7del\u00b7ten", "Fraun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Am Abend fochten die Helden.", "tokens": ["Am", "A\u00b7bend", "foch\u00b7ten", "die", "Hel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbmein Herr ist K\u00f6nig im Land,", "tokens": ["\u00bb", "mein", "Herr", "ist", "K\u00f6\u00b7nig", "im", "Land", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ich herrsch im Garten der Rosen,", "tokens": ["Ich", "herrsch", "im", "Gar\u00b7ten", "der", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er hat sich die g\u00fcldene Kron,", "tokens": ["Er", "hat", "sich", "die", "g\u00fcl\u00b7de\u00b7ne", "Kron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Ich den Blumenkranz mir erkosen.", "tokens": ["Ich", "den", "Blu\u00b7men\u00b7kranz", "mir", "er\u00b7ko\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PPER", "VVINF", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "So h\u00f6rt, ihr junge Recken,", "tokens": ["So", "h\u00f6rt", ",", "ihr", "jun\u00b7ge", "Re\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr lieben drei W\u00e4chter mein!", "tokens": ["Ihr", "lie\u00b7ben", "drei", "W\u00e4ch\u00b7ter", "mein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "CARD", "NN", "PPOSAT", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "La\u00dft alle zarten Jungfr\u00e4ulein,", "tokens": ["La\u00dft", "al\u00b7le", "zar\u00b7ten", "Jung\u00b7fr\u00e4u\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00dft keinen Ritter herein!", "tokens": ["La\u00dft", "kei\u00b7nen", "Rit\u00b7ter", "her\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Sie m\u00f6chten die Rosen verderben;", "tokens": ["Sie", "m\u00f6ch\u00b7ten", "die", "Ro\u00b7sen", "ver\u00b7der\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Das br\u00e4chte mir gro\u00dfe Sorgen.\u00ab", "tokens": ["Das", "br\u00e4ch\u00b7te", "mir", "gro\u00b7\u00dfe", "Sor\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So sprach die sch\u00f6ne K\u00f6nigin,", "tokens": ["So", "sprach", "die", "sch\u00f6\u00b7ne", "K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als sie dannen ging am Morgen.", "tokens": ["Als", "sie", "dan\u00b7nen", "ging", "am", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Da wandelten die drei W\u00e4chter", "tokens": ["Da", "wan\u00b7del\u00b7ten", "die", "drei", "W\u00e4ch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "CARD", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gar treulich vor der T\u00fcr.", "tokens": ["Gar", "treu\u00b7lich", "vor", "der", "T\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die R\u00f6slein dufteten stille", "tokens": ["Die", "R\u00f6s\u00b7lein", "duf\u00b7te\u00b7ten", "stil\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und blickten lieblich herf\u00fcr.", "tokens": ["Und", "blick\u00b7ten", "lieb\u00b7lich", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Und kamen des Wegs mit Sitten", "tokens": ["Und", "ka\u00b7men", "des", "Wegs", "mit", "Sit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Drei zarte Jungfr\u00e4ulein:", "tokens": ["Drei", "zar\u00b7te", "Jung\u00b7fr\u00e4u\u00b7lein", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbihr W\u00e4chter, liebe drei W\u00e4chter,", "tokens": ["\u00bb", "ihr", "W\u00e4ch\u00b7ter", ",", "lie\u00b7be", "drei", "W\u00e4ch\u00b7ter", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "VVFIN", "CARD", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "La\u00dft uns in den Garten ein!\u00ab", "tokens": ["La\u00dft", "uns", "in", "den", "Gar\u00b7ten", "ein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Als die Jungfraun Rosen gebrochen,", "tokens": ["Als", "die", "Jung\u00b7fraun", "Ro\u00b7sen", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVPP", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da haben sie all gesprochen:", "tokens": ["Da", "ha\u00b7ben", "sie", "all", "ge\u00b7spro\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbwas blutet mir so die Hand?", "tokens": ["\u00bb", "was", "blu\u00b7tet", "mir", "so", "die", "Hand", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Hat mich das R\u00f6slein gestochen?\u00ab", "tokens": ["Hat", "mich", "das", "R\u00f6s\u00b7lein", "ge\u00b7sto\u00b7chen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Da wandelten die drei W\u00e4chter", "tokens": ["Da", "wan\u00b7del\u00b7ten", "die", "drei", "W\u00e4ch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "CARD", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gar treulich vor der T\u00fcr.", "tokens": ["Gar", "treu\u00b7lich", "vor", "der", "T\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die R\u00f6slein dufteten stille", "tokens": ["Die", "R\u00f6s\u00b7lein", "duf\u00b7te\u00b7ten", "stil\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und blickten lieblich herf\u00fcr.", "tokens": ["Und", "blick\u00b7ten", "lieb\u00b7lich", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Und kamen des Wegs auf Rossen", "tokens": ["Und", "ka\u00b7men", "des", "Wegs", "auf", "Ros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Drei freche Rittersleut:", "tokens": ["Drei", "fre\u00b7che", "Rit\u00b7ters\u00b7leut", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbihr W\u00e4chter, schn\u00f6de drei W\u00e4chter,", "tokens": ["\u00bb", "ihr", "W\u00e4ch\u00b7ter", ",", "schn\u00f6\u00b7de", "drei", "W\u00e4ch\u00b7ter", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "VVFIN", "CARD", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sperret auf die T\u00fcre weit!\u00ab", "tokens": ["Sper\u00b7ret", "auf", "die", "T\u00fc\u00b7re", "weit", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "\u00bbdie T\u00fcre, die bleibet zu,", "tokens": ["\u00bb", "die", "T\u00fc\u00b7re", ",", "die", "blei\u00b7bet", "zu", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Schwerter, die sind blo\u00df,", "tokens": ["Die", "Schwer\u00b7ter", ",", "die", "sind", "blo\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Rosen, die sind teuer,", "tokens": ["Die", "Ro\u00b7sen", ",", "die", "sind", "teu\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Eine Wund gilt jegliche Ros.\u00ab", "tokens": ["Ei\u00b7ne", "Wund", "gilt", "jeg\u00b7li\u00b7che", "Ros", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$.", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.11": {"line.1": {"text": "Da stritten die Ritter und W\u00e4chter,", "tokens": ["Da", "strit\u00b7ten", "die", "Rit\u00b7ter", "und", "W\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "NE", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Ritter den Sieg erwarben,", "tokens": ["Die", "Rit\u00b7ter", "den", "Sieg", "er\u00b7war\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zertraten die R\u00f6slein all,", "tokens": ["Zer\u00b7tra\u00b7ten", "die", "R\u00f6s\u00b7lein", "all", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PIAT", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit den Rosen die W\u00e4chter starben.", "tokens": ["Mit", "den", "Ro\u00b7sen", "die", "W\u00e4ch\u00b7ter", "star\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NE", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.12": {"line.1": {"text": "Und als es war am Abend,", "tokens": ["Und", "als", "es", "war", "am", "A\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Frau K\u00f6nigin kam herbei:", "tokens": ["Frau", "K\u00f6\u00b7ni\u00b7gin", "kam", "her\u00b7bei", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "\u00bbund sind meine Rosen zertreten,", "tokens": ["\u00bb", "und", "sind", "mei\u00b7ne", "Ro\u00b7sen", "zer\u00b7tre\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Erschlagen die J\u00fcnglinge treu,", "tokens": ["Er\u00b7schla\u00b7gen", "die", "J\u00fcng\u00b7lin\u00b7ge", "treu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}}, "stanza.13": {"line.1": {"text": "So will ich auf Rosenbl\u00e4tter", "tokens": ["So", "will", "ich", "auf", "Ro\u00b7sen\u00b7bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie legen in die Erden,", "tokens": ["Sie", "le\u00b7gen", "in", "die", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wo der Rosengarten war,", "tokens": ["Und", "wo", "der", "Ro\u00b7sen\u00b7gar\u00b7ten", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Soll der Liliengarten werden.", "tokens": ["Soll", "der", "Li\u00b7li\u00b7en\u00b7gar\u00b7ten", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VAINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.14": {"line.1": {"text": "Wer ist es, der die Lilien", "tokens": ["Wer", "ist", "es", ",", "der", "die", "Li\u00b7li\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir treulich nun bewacht?", "tokens": ["Mir", "treu\u00b7lich", "nun", "be\u00b7wacht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bei Tage die liebe Sonne,", "tokens": ["Bei", "Ta\u00b7ge", "die", "lie\u00b7be", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Mond und die Sterne bei Nacht.\u00ab", "tokens": ["Der", "Mond", "und", "die", "Ster\u00b7ne", "bei", "Nacht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPR", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}