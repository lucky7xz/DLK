{"textgrid.poem.26505": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: J\u00fcngst wollte rasch dein Aug' sich feuchten \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst wollte rasch dein Aug' sich feuchten \u2013", "tokens": ["J\u00fcngst", "woll\u00b7te", "rasch", "dein", "Aug'", "sich", "feuch\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADJD", "PPOSAT", "NN", "PRF", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es wu\u00dfte Niemand wohl den Grund:", "tokens": ["Es", "wu\u00df\u00b7te", "Nie\u00b7mand", "wohl", "den", "Grund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man sah nur still die Thr\u00e4ne leuchten", "tokens": ["Man", "sah", "nur", "still", "die", "Thr\u00e4\u00b7ne", "leuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "ADJD", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und leise zucken deinen Mund.", "tokens": ["Und", "lei\u00b7se", "zu\u00b7cken", "dei\u00b7nen", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das aber f\u00fchlt' ich: eig'ner Leiden", "tokens": ["Das", "a\u00b7ber", "f\u00fchlt'", "ich", ":", "eig'\u00b7ner", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "ADV", "VVFIN", "PPER", "$.", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Warst du nicht bange dir bewu\u00dft;", "tokens": ["Warst", "du", "nicht", "ban\u00b7ge", "dir", "be\u00b7wu\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du lerntest ja dich stets bescheiden,", "tokens": ["Du", "lern\u00b7test", "ja", "dich", "stets", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil du f\u00fcr And're zittern mu\u00dft.", "tokens": ["Weil", "du", "f\u00fcr", "An\u00b7d'\u00b7re", "zit\u00b7tern", "mu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "In deiner Mitgebor'nen Kreise", "tokens": ["In", "dei\u00b7ner", "Mit\u00b7ge\u00b7bor'\u00b7nen", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00e4llt jedes Weh' auf dich zur\u00fcck;", "tokens": ["F\u00e4llt", "je\u00b7des", "Weh'", "auf", "dich", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du nimmst es hin in deiner Weise,", "tokens": ["Du", "nimmst", "es", "hin", "in", "dei\u00b7ner", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du nimmst es hin, als w\u00e4r's ein Gl\u00fcck.", "tokens": ["Du", "nimmst", "es", "hin", ",", "als", "w\u00e4r's", "ein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KOKOM", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nur wenn du des Vergang'nen Schwere \u2013", "tokens": ["Nur", "wenn", "du", "des", "Ver\u00b7gang'\u00b7nen", "Schwe\u00b7re", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und was noch kommen kann, erw\u00e4gst:", "tokens": ["Und", "was", "noch", "kom\u00b7men", "kann", ",", "er\u00b7w\u00e4gst", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVINF", "VMFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verr\u00e4th in deinem Aug' die Z\u00e4hre,", "tokens": ["Ver\u00b7r\u00e4\u00b7th", "in", "dei\u00b7nem", "Aug'", "die", "Z\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie gro\u00df die Last ist, die du tr\u00e4gst.", "tokens": ["Wie", "gro\u00df", "die", "Last", "ist", ",", "die", "du", "tr\u00e4gst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}