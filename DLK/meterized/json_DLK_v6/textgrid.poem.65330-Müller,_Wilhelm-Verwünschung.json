{"textgrid.poem.65330": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Verw\u00fcnschung", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00f6ge dessen b\u00f6se Zunge stets mit Blasen sein geplagt,", "tokens": ["M\u00f6\u00b7ge", "des\u00b7sen", "b\u00f6\u00b7se", "Zun\u00b7ge", "stets", "mit", "Bla\u00b7sen", "sein", "ge\u00b7plagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "ADJA", "NN", "ADV", "APPR", "NN", "PPOSAT", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Der dir, da\u00df ich treulos w\u00e4re, selber treulos hat gesagt!", "tokens": ["Der", "dir", ",", "da\u00df", "ich", "treu\u00b7los", "w\u00e4\u00b7re", ",", "sel\u00b7ber", "treu\u00b7los", "hat", "ge\u00b7sagt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "ADV", "ADJD", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Ist's ein Stern, fall' er vom Himmel, ist's der Mond, er m\u00fcss' erblinden,", "tokens": ["Ist's", "ein", "Stern", ",", "fall'", "er", "vom", "Him\u00b7mel", ",", "ist's", "der", "Mond", ",", "er", "m\u00fcss'", "er\u00b7blin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "KOUS", "PPER", "APPRART", "NN", "$,", "VAFIN", "ART", "NN", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Ist's ein heirathsfrohes M\u00e4dchen, m\u00fcsse keinen Mann sie finden.", "tokens": ["Ist's", "ein", "hei\u00b7raths\u00b7fro\u00b7hes", "M\u00e4d\u00b7chen", ",", "m\u00fcs\u00b7se", "kei\u00b7nen", "Mann", "sie", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,", "VMFIN", "PIAT", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}