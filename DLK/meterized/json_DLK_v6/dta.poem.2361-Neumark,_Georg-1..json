{"dta.poem.2361": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Ich danke Dir mein starker Retter/", "tokens": ["Ich", "dan\u00b7ke", "Dir", "mein", "star\u00b7ker", "Ret\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich danke Dir mein liebster ", "tokens": ["Ich", "dan\u00b7ke", "Dir", "mein", "liebs\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df du in diesem ", "tokens": ["Da\u00df", "du", "in", "die\u00b7sem"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PDAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Mich hast beh\u00fctet f\u00fcr dem Spott/", "tokens": ["Mich", "hast", "be\u00b7h\u00fc\u00b7tet", "f\u00fcr", "dem", "Spott", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Jene mit vergifften Sinnen/", "tokens": ["Den", "Je\u00b7ne", "mit", "ver\u00b7giff\u00b7ten", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gedachten hier auf Mich zu spinnen.", "tokens": ["Ge\u00b7dach\u00b7ten", "hier", "auf", "Mich", "zu", "spin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich danke dir von meiner Seelen/", "tokens": ["Ich", "dan\u00b7ke", "dir", "von", "mei\u00b7ner", "See\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "da\u00df du mich nun erl\u00f6set hast/", "tokens": ["da\u00df", "du", "mich", "nun", "er\u00b7l\u00f6\u00b7set", "hast", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Au\u00df jener wilden ", "tokens": ["Au\u00df", "je\u00b7ner", "wil\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und von der schweren Kreutzeslast/", "tokens": ["Und", "von", "der", "schwe\u00b7ren", "Kreut\u00b7zes\u00b7last", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "die mir so schwer lag auff dem R\u00fckken/", "tokens": ["die", "mir", "so", "schwer", "lag", "auff", "dem", "R\u00fck\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "da\u00df ich davon fast must, erstikken.", "tokens": ["da\u00df", "ich", "da\u00b7von", "fast", "must", ",", "ers\u00b7tik\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "ADV", "VMFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich wurde hin und her verschlagen/", "tokens": ["Ich", "wur\u00b7de", "hin", "und", "her", "ver\u00b7schla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KON", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gleich wie ein Schifchen auf der See/", "tokens": ["Gleich", "wie", "ein", "Schifc\u00b7hen", "auf", "der", "See", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich fast wer' in solchen Plagen", "tokens": ["Da\u00df", "ich", "fast", "wer'", "in", "sol\u00b7chen", "Pla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vergangen/ mit viel Ach und Weh/", "tokens": ["Ver\u00b7gan\u00b7gen", "/", "mit", "viel", "Ach", "und", "Weh", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "APPR", "PIAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es schmissen mich die Ungl\u00fckswellen/", "tokens": ["Es", "schmis\u00b7sen", "mich", "die", "Un\u00b7gl\u00fcks\u00b7wel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bald Himmelauf/ bald zu der Hellen.", "tokens": ["Bald", "Him\u00b7me\u00b7lauf", "/", "bald", "zu", "der", "Hel\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$(", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nun aber hat es sich gestillet/", "tokens": ["Nun", "a\u00b7ber", "hat", "es", "sich", "ge\u00b7stil\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dr\u00fcm sag' ich/ Gott/ dir hertzlich Dank;\nDa\u00df du mir bist sowol gewillet", "tokens": ["Dr\u00fcm", "sag'", "ich", "/", "Gott", "/", "dir", "hertz\u00b7lich", "Dank", ";", "Da\u00df", "du", "mir", "bist", "so\u00b7wol", "ge\u00b7wil\u00b7let"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$(", "NN", "$(", "PPER", "ADJD", "NN", "$.", "KOUS", "PPER", "PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.3": {"text": "Vergess\u2019 ich nicht mein lebelang.", "tokens": ["Ver\u00b7ge\u00b7ss'", "ich", "nicht", "mein", "le\u00b7be\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "PPOSAT", "VVFIN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Nach diesen rauhen Nordenwinden/", "tokens": ["Nach", "die\u00b7sen", "rau\u00b7hen", "Nor\u00b7den\u00b7win\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich danke ", "tokens": ["Ich", "dan\u00b7ke"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Auf Mich/ dein ", "tokens": ["Auf", "Mich", "/", "dein"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPR", "PPER", "$(", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und da\u00df nun deine gro\u00dfe G\u00fcte/", "tokens": ["Und", "da\u00df", "nun", "dei\u00b7ne", "gro\u00b7\u00dfe", "G\u00fc\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich hat gesetzt aus der Gefahr.", "tokens": ["Mich", "hat", "ge\u00b7setzt", "aus", "der", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df du nach diesem gro\u00dfen Schaden", "tokens": ["Da\u00df", "du", "nach", "die\u00b7sem", "gro\u00b7\u00dfen", "Scha\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mich wieder tr\u00f6stest mit Genaden.", "tokens": ["Mich", "wie\u00b7der", "tr\u00f6s\u00b7test", "mit", "Ge\u00b7na\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wolan! Ich wil mich nicht entsetzen/", "tokens": ["Wo\u00b7lan", "!", "Ich", "wil", "mich", "nicht", "ent\u00b7set\u00b7zen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VMFIN", "PRF", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es komm' auch was nur immer woll'", "tokens": ["Es", "komm'", "auch", "was", "nur", "im\u00b7mer", "woll'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PWS", "ADV", "ADV", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es kan kein Unfall mich verletzen/", "tokens": ["Es", "kan", "kein", "Un\u00b7fall", "mich", "ver\u00b7let\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es ist nichts das Mir schaden soll.", "tokens": ["Es", "ist", "nichts", "das", "Mir", "scha\u00b7den", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ART", "NE", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn du bist selbst mit Mir zu Werke/", "tokens": ["Denn", "du", "bist", "selbst", "mit", "Mir", "zu", "Wer\u00b7ke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "APPR", "NE", "APPR", "NN", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Mein ", "tokens": ["Mein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "-", "measure": "single.down"}}}}}