{"textgrid.poem.42990": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Meine alte Schiffsuhr", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In meinem Zimmer h\u00e4ngt eine runde,", "tokens": ["In", "mei\u00b7nem", "Zim\u00b7mer", "h\u00e4ngt", "ei\u00b7ne", "run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Alte, achteckige Segelschiffsuhr.", "tokens": ["Al\u00b7te", ",", "acht\u00b7ec\u00b7ki\u00b7ge", "Se\u00b7gel\u00b7schiff\u00b7suhr", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Sie schl\u00e4gt weder Glasen noch Stunde.", "tokens": ["Sie", "schl\u00e4gt", "we\u00b7der", "Gla\u00b7sen", "noch", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "NN", "ADV", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sie schl\u00e4gt, wie sie will, und auch nur,", "tokens": ["Sie", "schl\u00e4gt", ",", "wie", "sie", "will", ",", "und", "auch", "nur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "VMFIN", "$,", "KON", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wann sie will. Die Uhrmacher gaben", "tokens": ["Wann", "sie", "will", ".", "Die", "Uhr\u00b7ma\u00b7cher", "ga\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "VMFIN", "$.", "ART", "NN", "VVFIN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie alle ratlos mir zur\u00fcck;", "tokens": ["Sie", "al\u00b7le", "rat\u00b7los", "mir", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie wollten mit solchem Teufelsst\u00fcck", "tokens": ["Sie", "woll\u00b7ten", "mit", "sol\u00b7chem", "Teu\u00b7fels\u00b7st\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gar nichts zu tun haben.", "tokens": ["Gar", "nichts", "zu", "tun", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und gehe sie, wie sie wolle,", "tokens": ["Und", "ge\u00b7he", "sie", ",", "wie", "sie", "wol\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich freue mich, weil sie noch lebt.", "tokens": ["Ich", "freu\u00b7e", "mich", ",", "weil", "sie", "noch", "lebt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur schade, da\u00df nie eine tolle", "tokens": ["Nur", "scha\u00b7de", ",", "da\u00df", "nie", "ei\u00b7ne", "tol\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "ADV", "ART", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "D\u00fcnung sie senkt oder hebt", "tokens": ["D\u00fc\u00b7nung", "sie", "senkt", "o\u00b7der", "hebt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Oder sch\u00fcttert. Nein, sie h\u00e4ngt sicher", "tokens": ["O\u00b7der", "sch\u00fct\u00b7tert", ".", "Nein", ",", "sie", "h\u00e4ngt", "si\u00b7cher"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "PTKANT", "$,", "PPER", "VVFIN", "ADJD"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Geborgen. Doch in ihr kreist", "tokens": ["Ge\u00b7bor\u00b7gen", ".", "Doch", "in", "ihr", "kreist"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "KON", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein ruhelos wunderlicher", "tokens": ["Ein", "ru\u00b7he\u00b7los", "wun\u00b7der\u00b7li\u00b7cher"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Freibeuter-Klabautergeist.", "tokens": ["Frei\u00b7beu\u00b7ter\u00b7Kla\u00b7bau\u00b7ter\u00b7geist", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Nachts, wenn ich still vor ihr hocke,", "tokens": ["Nachts", ",", "wenn", "ich", "still", "vor", "ihr", "ho\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dann h\u00f6re ich mehr als Ticktack.", "tokens": ["Dann", "h\u00f6\u00b7re", "ich", "mehr", "als", "Tick\u00b7tack", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KOUS", "NN", "$."], "meter": "-+-+++-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Dann klingt was wie Nebelglocke", "tokens": ["Dann", "klingt", "was", "wie", "Ne\u00b7bel\u00b7glo\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "KOKOM", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und ferner Hunds wachenschnack.", "tokens": ["Und", "fer\u00b7ner", "Hunds", "wa\u00b7chen\u00b7schnack", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Und manche Zeit vers\u00e4ume", "tokens": ["Und", "man\u00b7che", "Zeit", "ver\u00b7s\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich vor der spukenden, unkenden Uhr,", "tokens": ["Ich", "vor", "der", "spu\u00b7ken\u00b7den", ",", "un\u00b7ken\u00b7den", "Uhr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Indem ich davon tr\u00e4ume,", "tokens": ["In\u00b7dem", "ich", "da\u00b7von", "tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie ich mit ihr nach Westindien fuhr.", "tokens": ["Wie", "ich", "mit", "ihr", "nach", "West\u00b7in\u00b7di\u00b7en", "fuhr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}