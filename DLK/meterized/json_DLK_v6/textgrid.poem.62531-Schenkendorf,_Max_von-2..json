{"textgrid.poem.62531": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum willst du von uns fliehen?", "tokens": ["Wa\u00b7rum", "willst", "du", "von", "uns", "flie\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weile, weile, sch\u00f6ner Stern!", "tokens": ["Wei\u00b7le", ",", "wei\u00b7le", ",", "sch\u00f6\u00b7ner", "Stern", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, so sch\u00f6n noch im Vergl\u00fchen,", "tokens": ["Ach", ",", "so", "sch\u00f6n", "noch", "im", "Ver\u00b7gl\u00fc\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Voll der Herrlichkeit des Herrn.", "tokens": ["Voll", "der", "Herr\u00b7lich\u00b7keit", "des", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Labtest uns mit deinem Schimmer,", "tokens": ["Lab\u00b7test", "uns", "mit", "dei\u00b7nem", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie ein heller Ostertag,", "tokens": ["Wie", "ein", "hel\u00b7ler", "Os\u00b7ter\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Liebten also dich, da\u00df nimmer", "tokens": ["Lieb\u00b7ten", "al\u00b7so", "dich", ",", "da\u00df", "nim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "PPER", "$,", "KOUS", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unser Herz dich missen mag.", "tokens": ["Un\u00b7ser", "Herz", "dich", "mis\u00b7sen", "mag."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "NE"], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}