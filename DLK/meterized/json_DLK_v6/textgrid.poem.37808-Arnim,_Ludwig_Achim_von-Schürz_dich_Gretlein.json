{"textgrid.poem.37808": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Sch\u00fcrz dich Gretlein", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbnun sch\u00fcrz dich Gretlein sch\u00fcrz dich,", "tokens": ["\u00bb", "nun", "sch\u00fcrz", "dich", "Gret\u00b7lein", "sch\u00fcrz", "dich", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "PPER", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohl auf mit mir davon,", "tokens": ["Wohl", "auf", "mit", "mir", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "APPR", "PPER", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Korn ist abgeschnitten,", "tokens": ["Das", "Korn", "ist", "ab\u00b7ge\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Wein ist eingethan.\u00ab", "tokens": ["Der", "Wein", "ist", "ein\u00b7ge\u00b7than", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbach H\u00e4n\u00dflein, liebes H\u00e4n\u00dflein,", "tokens": ["\u00bb", "ach", "H\u00e4n\u00df\u00b7lein", ",", "lie\u00b7bes", "H\u00e4n\u00df\u00b7lein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "XY", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.2": {"text": "So la\u00df mich bey dir sein,", "tokens": ["So", "la\u00df", "mich", "bey", "dir", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "PPER", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Wochen auf dem Felde,", "tokens": ["Die", "Wo\u00b7chen", "auf", "dem", "Fel\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Feiertag beim Wein.\u00ab", "tokens": ["Den", "Fei\u00b7er\u00b7tag", "beim", "Wein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da nahm ers bey den H\u00e4nden,", "tokens": ["Da", "nahm", "ers", "bey", "den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bey ihrer schneeweissen Hand", "tokens": ["Bey", "ih\u00b7rer", "schnee\u00b7weis\u00b7sen", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Er f\u00fchrt sie an ein Ende,", "tokens": ["Er", "f\u00fchrt", "sie", "an", "ein", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da er ein Wirthshaus fand.", "tokens": ["Da", "er", "ein", "Wirths\u00b7haus", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbnun Wirthin, liebe Wirthin,", "tokens": ["\u00bb", "nun", "Wirt\u00b7hin", ",", "lie\u00b7be", "Wirt\u00b7hin", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schaut um nach k\u00fchlem Wein,", "tokens": ["Schaut", "um", "nach", "k\u00fch\u00b7lem", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Kleider dieses Gretlein", "tokens": ["Die", "Klei\u00b7der", "die\u00b7ses", "Gret\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "M\u00fcssen verschlemmet sein.\u00ab", "tokens": ["M\u00fcs\u00b7sen", "ver\u00b7schlem\u00b7met", "sein", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NE", "VVPP", "VAINF", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Die Gret hub an zu weinen,", "tokens": ["Die", "Gret", "hub", "an", "zu", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Unmuth der war gro\u00df,", "tokens": ["Ihr", "Un\u00b7muth", "der", "war", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df Ihr die lichten Z\u00e4hren", "tokens": ["Da\u00df", "Ihr", "die", "lich\u00b7ten", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ueber ihr Wenglein flo\u00df.", "tokens": ["Ue\u00b7ber", "ihr", "Wen\u00b7glein", "flo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "\u00bbach H\u00e4n\u00dflein, liebes H\u00e4n\u00dflein,", "tokens": ["\u00bb", "ach", "H\u00e4n\u00df\u00b7lein", ",", "lie\u00b7bes", "H\u00e4n\u00df\u00b7lein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "XY", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.2": {"text": "Du redtest nicht also,", "tokens": ["Du", "red\u00b7test", "nicht", "al\u00b7so", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als du mich heim ausf\u00fchrest", "tokens": ["Als", "du", "mich", "heim", "aus\u00b7f\u00fch\u00b7rest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "PTKVZ", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus meines Vaters Hof.\u00ab", "tokens": ["Aus", "mei\u00b7nes", "Va\u00b7ters", "Hof", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Er nahm sie bey den H\u00e4nden,", "tokens": ["Er", "nahm", "sie", "bey", "den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bey ihrer schneeweissen Hand,", "tokens": ["Bey", "ih\u00b7rer", "schnee\u00b7weis\u00b7sen", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Er f\u00fchrt sie an ein Ende,", "tokens": ["Er", "f\u00fchrt", "sie", "an", "ein", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da er ein G\u00e4rtlein fand.", "tokens": ["Da", "er", "ein", "G\u00e4rt\u00b7lein", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbach Gretlein, liebes Gretlein,", "tokens": ["\u00bb", "ach", "Gret\u00b7lein", ",", "lie\u00b7bes", "Gret\u00b7lein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "XY", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Warum weinst du so sehr,", "tokens": ["Wa\u00b7rum", "weinst", "du", "so", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Reuet dich dein freier Muth,", "tokens": ["Reu\u00b7et", "dich", "dein", "frei\u00b7er", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder reut dich dein Ehr?\u00ab", "tokens": ["O\u00b7der", "reut", "dich", "dein", "Ehr", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "\u00bbes reut mich nicht mein freier Muth,", "tokens": ["\u00bb", "es", "reut", "mich", "nicht", "mein", "frei\u00b7er", "Muth", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PTKNEG", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Darzu auch nicht mein Ehr;", "tokens": ["Dar\u00b7zu", "auch", "nicht", "mein", "Ehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es reuen mich mein Kleider,", "tokens": ["Es", "reu\u00b7en", "mich", "mein", "Klei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die werden mir nimmermehr.\u00ab", "tokens": ["Die", "wer\u00b7den", "mir", "nim\u00b7mer\u00b7mehr", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}