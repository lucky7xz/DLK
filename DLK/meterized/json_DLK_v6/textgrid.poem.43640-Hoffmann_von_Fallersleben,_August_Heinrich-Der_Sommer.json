{"textgrid.poem.43640": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Sommer", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Sommer, der Sommer,", "tokens": ["Der", "Som\u00b7mer", ",", "der", "Som\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Das ist die sch\u00f6nste Zeit:", "tokens": ["Das", "ist", "die", "sch\u00f6ns\u00b7te", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir ziehen in die W\u00e4lder", "tokens": ["Wir", "zie\u00b7hen", "in", "die", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und durch die Au'n und Felder", "tokens": ["Und", "durch", "die", "Au'n", "und", "Fel\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Voll Lust und Fr\u00f6hlichkeit.", "tokens": ["Voll", "Lust", "und", "Fr\u00f6h\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Sommer, der Sommer,", "tokens": ["Der", "Som\u00b7mer", ",", "der", "Som\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der schenkt uns Freuden viel:", "tokens": ["Der", "schenkt", "uns", "Freu\u00b7den", "viel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir jagen dann und springen", "tokens": ["Wir", "ja\u00b7gen", "dann", "und", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach bunten Schmetterlingen", "tokens": ["Nach", "bun\u00b7ten", "Schmet\u00b7ter\u00b7lin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und spielen manches Spiel.", "tokens": ["Und", "spie\u00b7len", "man\u00b7ches", "Spiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Sommer, der Sommer,", "tokens": ["Der", "Som\u00b7mer", ",", "der", "Som\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der schenkt uns manchen Fund:", "tokens": ["Der", "schenkt", "uns", "man\u00b7chen", "Fund", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erdbeeren wir uns suchen", "tokens": ["Erd\u00b7bee\u00b7ren", "wir", "uns", "su\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Schatten hoher Buchen", "tokens": ["Im", "Schat\u00b7ten", "ho\u00b7her", "Bu\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und laben Herz und Mund.", "tokens": ["Und", "la\u00b7ben", "Herz", "und", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Sommer, der Sommer,", "tokens": ["Der", "Som\u00b7mer", ",", "der", "Som\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der hei\u00dft uns lustig sein:", "tokens": ["Der", "hei\u00dft", "uns", "lus\u00b7tig", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir winden Blumenkr\u00e4nze", "tokens": ["Wir", "win\u00b7den", "Blu\u00b7men\u00b7kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und halten Reigent\u00e4nze", "tokens": ["Und", "hal\u00b7ten", "Rei\u00b7gen\u00b7t\u00e4n\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Beim Abendsonnenschein.", "tokens": ["Beim", "A\u00b7bend\u00b7son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}