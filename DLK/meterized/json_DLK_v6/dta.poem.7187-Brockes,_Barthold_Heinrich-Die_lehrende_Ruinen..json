{"dta.poem.7187": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die lehrende Ruinen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich habe drauf die gantze Nacht", "tokens": ["Ich", "ha\u00b7be", "drauf", "die", "gant\u00b7ze", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit tausend Tr\u00e4umen zugebracht.", "tokens": ["Mit", "tau\u00b7send", "Tr\u00e4u\u00b7men", "zu\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jtzt, da ich meinen Sinn auf das vergangne lencke,", "tokens": ["Jtzt", ",", "da", "ich", "mei\u00b7nen", "Sinn", "auf", "das", "ver\u00b7gang\u00b7ne", "len\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und, was ich gestern sah, noch einmahl \u00fcberdencke,", "tokens": ["Und", ",", "was", "ich", "ge\u00b7stern", "sah", ",", "noch", "ein\u00b7mahl", "\u00fc\u00b7ber\u00b7den\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Wunder der Natur, das Wunder-Werck der Kunst;", "tokens": ["Die", "Wun\u00b7der", "der", "Na\u00b7tur", ",", "das", "Wun\u00b7der\u00b7\u00b7Werck", "der", "Kunst", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Erhebet sich in mir ein tr\u00fcber Schwermuths-Dunst,", "tokens": ["Er\u00b7he\u00b7bet", "sich", "in", "mir", "ein", "tr\u00fc\u00b7ber", "Schwer\u00b7muths\u00b7Dunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Benebelt mein Gehirn, wird der erstaunten Geister", "tokens": ["Be\u00b7ne\u00b7belt", "mein", "Ge\u00b7hirn", ",", "wird", "der", "er\u00b7staun\u00b7ten", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "$,", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In einen Augenblick Tyrann und Ober-Meister:", "tokens": ["In", "ei\u00b7nen", "Au\u00b7gen\u00b7blick", "Ty\u00b7rann", "und", "O\u00b7ber\u00b7Meis\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ein kalter Schauder presst und \u00e4ngstet Hertz und Sinn,", "tokens": ["Ein", "kal\u00b7ter", "Schau\u00b7der", "presst", "und", "\u00e4ngs\u00b7tet", "Hertz", "und", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und meine Seele selbst empfand ein sch\u00fcchtern Grauen,", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "selbst", "emp\u00b7fand", "ein", "sch\u00fcch\u00b7tern", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So viele Wunder-Werck im Staub und Graus zu schauen.", "tokens": ["So", "vie\u00b7le", "Wun\u00b7der\u00b7\u00b7Werck", "im", "Staub", "und", "Graus", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPRART", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}