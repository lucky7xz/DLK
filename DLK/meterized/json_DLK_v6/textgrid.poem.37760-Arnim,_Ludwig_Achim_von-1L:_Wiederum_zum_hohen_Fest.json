{"textgrid.poem.37760": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wiederum zum hohen Fest", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wiederum zum hohen Fest", "tokens": ["Wie\u00b7de\u00b7rum", "zum", "ho\u00b7hen", "Fest"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Klingt des Tisches Glocke,", "tokens": ["Klingt", "des", "Ti\u00b7sches", "Glo\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Doch viel wenger sind der G\u00e4st,", "tokens": ["Doch", "viel", "wen\u00b7ger", "sind", "der", "G\u00e4st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Manchen h\u00e4lt das Grab schon fest", "tokens": ["Man\u00b7chen", "h\u00e4lt", "das", "Grab", "schon", "fest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In dem h\u00f6lzern Rocke.", "tokens": ["In", "dem", "h\u00f6l\u00b7zern", "Ro\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wer zum Feste kommen will", "tokens": ["Wer", "zum", "Fes\u00b7te", "kom\u00b7men", "will"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPRART", "NN", "VVINF", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Komm' im Freudenkleide,", "tokens": ["Komm'", "im", "Freu\u00b7den\u00b7klei\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schweig mein Herz von Todten still,", "tokens": ["Schweig", "mein", "Herz", "von", "Tod\u00b7ten", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn ich meinen Becher f\u00fcll", "tokens": ["Wenn", "ich", "mei\u00b7nen", "Be\u00b7cher", "f\u00fcll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zu der Tafelweide.", "tokens": ["Zu", "der", "Ta\u00b7fel\u00b7wei\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Zwingen l\u00e4\u00dft sich nicht das Herz,", "tokens": ["Zwin\u00b7gen", "l\u00e4\u00dft", "sich", "nicht", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nichts von ihm erzwingen,", "tokens": ["Nichts", "von", "ihm", "er\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "G\u00f6nnt ihm den geliebten Schmerz,", "tokens": ["G\u00f6nnt", "ihm", "den", "ge\u00b7lieb\u00b7ten", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis sich in des Lebens Scherz", "tokens": ["Bis", "sich", "in", "des", "Le\u00b7bens", "Scherz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lebende umschlingen.", "tokens": ["Le\u00b7ben\u00b7de", "um\u00b7schlin\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+---+-", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Unsern Todten dieser Wein,", "tokens": ["Un\u00b7sern", "Tod\u00b7ten", "die\u00b7ser", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Den die Thr\u00e4ne weihte,", "tokens": ["Den", "die", "Thr\u00e4\u00b7ne", "weih\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nun ich nicht mit mir allein,", "tokens": ["Nun", "ich", "nicht", "mit", "mir", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKNEG", "APPR", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist auch die Versammlung klein,", "tokens": ["Ist", "auch", "die", "Ver\u00b7samm\u00b7lung", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "R\u00fcckt der Schmerz zur Weite.", "tokens": ["R\u00fcckt", "der", "Schmerz", "zur", "Wei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Weit und \u00f6de wird die Welt,", "tokens": ["Weit", "und", "\u00f6\u00b7de", "wird", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn so viele fallen,", "tokens": ["Wenn", "so", "vie\u00b7le", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nicht mehr fest zusammenh\u00e4lt,", "tokens": ["Nicht", "mehr", "fest", "zu\u00b7sam\u00b7men\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was der leichte Scherz gesellt", "tokens": ["Was", "der", "leich\u00b7te", "Scherz", "ge\u00b7sellt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Freunden zum Gefallen.", "tokens": ["Freun\u00b7den", "zum", "Ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Fremde wurden wir uns nicht,", "tokens": ["Frem\u00b7de", "wur\u00b7den", "wir", "uns", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PRF", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sahen uns doch selten,", "tokens": ["Sa\u00b7hen", "uns", "doch", "sel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn Gewohnheit heut gebricht,", "tokens": ["Wenn", "Ge\u00b7wohn\u00b7heit", "heut", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Guter Wein vertraulich spricht,", "tokens": ["Gu\u00b7ter", "Wein", "ver\u00b7trau\u00b7lich", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und das soll heut gelten.", "tokens": ["Und", "das", "soll", "heut", "gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "ADV", "VVINF", "$."], "meter": "--+-+-", "measure": "anapaest.init"}}, "stanza.7": {"line.1": {"text": "Nahe schien, was jetzt erreicht", "tokens": ["Na\u00b7he", "schien", ",", "was", "jetzt", "er\u00b7reicht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PRELS", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Uns beim vorgen Feste,", "tokens": ["Uns", "beim", "vor\u00b7gen", "Fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df der Feind aus Deutschland weicht,", "tokens": ["Da\u00df", "der", "Feind", "aus", "Deutschland", "weicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Schien uns damals schon so leicht,", "tokens": ["Schien", "uns", "da\u00b7mals", "schon", "so", "leicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und vollbracht das Beste.", "tokens": ["Und", "voll\u00b7bracht", "das", "Bes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "--+-+-", "measure": "anapaest.init"}}, "stanza.8": {"line.1": {"text": "Wer den Becher trinken soll,", "tokens": ["Wer", "den", "Be\u00b7cher", "trin\u00b7ken", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df ihn vorher f\u00fcllen;", "tokens": ["Mu\u00df", "ihn", "vor\u00b7her", "f\u00fcl\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Bis das Maa\u00df des Guten voll,", "tokens": ["Bis", "das", "Maa\u00df", "des", "Gu\u00b7ten", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Pr\u00fcft die Zeit uns zweifeltoll,", "tokens": ["Pr\u00fcft", "die", "Zeit", "uns", "zwei\u00b7fel\u00b7toll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer von echtem Willen.", "tokens": ["Wer", "von", "ech\u00b7tem", "Wil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Wer vor sich bestanden hat,", "tokens": ["Wer", "vor", "sich", "be\u00b7stan\u00b7den", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PRF", "VVPP", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dem kann sie auch glauben,", "tokens": ["Dem", "kann", "sie", "auch", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sei erkannt die Ehrenthat,", "tokens": ["Sei", "er\u00b7kannt", "die", "Eh\u00b7ren\u00b7that", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch vergebner M\u00fche Saat", "tokens": ["Auch", "ver\u00b7geb\u00b7ner", "M\u00fc\u00b7he", "Saat"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "L\u00e4\u00dft sich keiner rauben.", "tokens": ["L\u00e4\u00dft", "sich", "kei\u00b7ner", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIS", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Gro\u00dfes hat die Zeit gethan,", "tokens": ["Gro\u00b7\u00dfes", "hat", "die", "Zeit", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00f6\u00dfres zu verlangen,", "tokens": ["Gr\u00f6\u00df\u00b7res", "zu", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "F\u00fchlet jedem auf den Zahn,", "tokens": ["F\u00fch\u00b7let", "je\u00b7dem", "auf", "den", "Zahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Pr\u00fcft, ob er auf steilrer Bahn", "tokens": ["Pr\u00fcft", ",", "ob", "er", "auf", "steil\u00b7rer", "Bahn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "W\u00fcrde schwindelnd bangen.", "tokens": ["W\u00fcr\u00b7de", "schwin\u00b7delnd", "ban\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "Gl\u00e4nzend zu uns nieder,", "tokens": ["Gl\u00e4n\u00b7zend", "zu", "uns", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Denn er gr\u00fcndete von fern,", "tokens": ["Denn", "er", "gr\u00fcn\u00b7de\u00b7te", "von", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als Magnet den Eisenkern,", "tokens": ["Als", "Mag\u00b7net", "den", "Ei\u00b7sen\u00b7kern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gab dem Pfeil Gefieder.", "tokens": ["Gab", "dem", "Pfeil", "Ge\u00b7fie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "W\u00e4hrend alle Deutsche schon", "tokens": ["W\u00e4h\u00b7rend", "al\u00b7le", "Deut\u00b7sche", "schon"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich dem Joche beugten,", "tokens": ["Sich", "dem", "Jo\u00b7che", "beug\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sahn wir noch auf seinen Thron,", "tokens": ["Sahn", "wir", "noch", "auf", "sei\u00b7nen", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sprachen allem Ungl\u00fcck Hohn,", "tokens": ["Spra\u00b7chen", "al\u00b7lem", "Un\u00b7gl\u00fcck", "Hohn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Muth in Hoffnung zeugten.", "tokens": ["Muth", "in", "Hoff\u00b7nung", "zeug\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.13": {"line.1": {"text": "Und an Preu\u00dfens Hand sich hob", "tokens": ["Und", "an", "Preu\u00b7\u00dfens", "Hand", "sich", "hob"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "NN", "PRF", "VVFIN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Deutschland aus den Ketten,", "tokens": ["Deutschland", "aus", "den", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Das ist Friedrichs h\u00f6chstes Lob,", "tokens": ["Das", "ist", "Fried\u00b7richs", "h\u00f6chs\u00b7tes", "Lob", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NE", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Da\u00df sein Kriegsgei