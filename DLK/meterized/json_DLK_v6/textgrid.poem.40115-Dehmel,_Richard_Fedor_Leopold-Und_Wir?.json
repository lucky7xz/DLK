{"textgrid.poem.40115": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Und Wir?", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Felsen her zum gr\u00fcnen Holze,", "tokens": ["Vom", "Fel\u00b7sen", "her", "zum", "gr\u00fc\u00b7nen", "Hol\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APZR", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "vom gr\u00fcnen Holze hin zum H\u00fcgel", "tokens": ["vom", "gr\u00fc\u00b7nen", "Hol\u00b7ze", "hin", "zum", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "eilt, V\u00f6gelchen, dein freier Fl\u00fcgel:", "tokens": ["eilt", ",", "V\u00f6\u00b7gel\u00b7chen", ",", "dein", "frei\u00b7er", "Fl\u00fc\u00b7gel", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ziel nicht, Weg nicht ist Dir bekannt.", "tokens": ["Ziel", "nicht", ",", "Weg", "nicht", "ist", "Dir", "be\u00b7kannt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "NN", "PTKNEG", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Und Wir? wir Menschen mit unserm Stolze?", "tokens": ["Und", "Wir", "?", "wir", "Men\u00b7schen", "mit", "un\u00b7serm", "Stol\u00b7ze", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$.", "PPER", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie R\u00e4der um eiserne Zapfen schwirren,", "tokens": ["Wie", "R\u00e4\u00b7der", "um", "ei\u00b7ser\u00b7ne", "Zap\u00b7fen", "schwir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "im ewig gleichen Kreis wir irren:", "tokens": ["im", "e\u00b7wig", "glei\u00b7chen", "Kreis", "wir", "ir\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "immer getrieben, immer gebannt.", "tokens": ["im\u00b7mer", "ge\u00b7trie\u00b7ben", ",", "im\u00b7mer", "ge\u00b7bannt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "VVPP", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}}}}