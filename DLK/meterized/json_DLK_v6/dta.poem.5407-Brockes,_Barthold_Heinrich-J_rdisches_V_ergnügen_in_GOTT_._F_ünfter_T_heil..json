{"dta.poem.5407": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was k\u00f6nnen wir, bey so viel Gaben,", "tokens": ["Was", "k\u00f6n\u00b7nen", "wir", ",", "bey", "so", "viel", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$,", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die uns von GOtt allein geschenckt,", "tokens": ["Die", "uns", "von", "Gott", "al\u00b7lein", "ge\u00b7schenckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn man nicht an den Geber denckt,", "tokens": ["Wenn", "man", "nicht", "an", "den", "Ge\u00b7ber", "denckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch f\u00fcr Entschuldigungen haben?", "tokens": ["Doch", "f\u00fcr", "Ent\u00b7schul\u00b7di\u00b7gun\u00b7gen", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich mag so viel ich immer kann", "tokens": ["Ich", "mag", "so", "viel", "ich", "im\u00b7mer", "kann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PPER", "ADV", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Geist auf alle Dinge lencken;", "tokens": ["Den", "Geist", "auf", "al\u00b7le", "Din\u00b7ge", "len\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So treff ich nichts so w\u00fcrdig an", "tokens": ["So", "treff", "ich", "nichts", "so", "w\u00fcr\u00b7dig", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ADV", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und kann, mit allem meinen Dencken,", "tokens": ["Und", "kann", ",", "mit", "al\u00b7lem", "mei\u00b7nen", "Den\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$,", "APPR", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Nichts seeliger befinden,", "tokens": ["Nichts", "see\u00b7li\u00b7ger", "be\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Um uns mit GOtt selbst zu verbinden,", "tokens": ["Um", "uns", "mit", "Gott", "selbst", "zu", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "APPR", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Als wenn wir seine Gr\u00f6\u00df\u2019 in seinem Werck\u2019 ergr\u00fcnden.", "tokens": ["Als", "wenn", "wir", "sei\u00b7ne", "Gr\u00f6\u00df'", "in", "sei\u00b7nem", "Werck", "er\u00b7gr\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Es kann ein fr\u00f6liches Gem\u00fcthe,", "tokens": ["Es", "kann", "ein", "fr\u00f6\u00b7li\u00b7ches", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Wenn wir den Ausbruch seiner G\u00fcte", "tokens": ["Wenn", "wir", "den", "Aus\u00b7bruch", "sei\u00b7ner", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und seiner Macht und Weisheit sehn,", "tokens": ["Und", "sei\u00b7ner", "Macht", "und", "Weis\u00b7heit", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und zu betrachten uns bestreben,", "tokens": ["Und", "zu", "be\u00b7trach\u00b7ten", "uns", "be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Nicht nur des Sch\u00f6pfers Ruhm am herrlichsten erheben,", "tokens": ["Nicht", "nur", "des", "Sch\u00f6p\u00b7fers", "Ruhm", "am", "herr\u00b7lichs\u00b7ten", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "NN", "APPRART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Nein, gar sich selbst in ihm erh\u00f6hn.", "tokens": ["Nein", ",", "gar", "sich", "selbst", "in", "ihm", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "PRF", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}