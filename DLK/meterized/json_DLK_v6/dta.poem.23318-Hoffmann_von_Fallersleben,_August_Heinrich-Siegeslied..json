{"dta.poem.23318": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Siegeslied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcrchtet nicht die Bajonnette,", "tokens": ["F\u00fcrch\u00b7tet", "nicht", "die", "Ba\u00b7jon\u00b7net\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht den Uniformentand,", "tokens": ["Nicht", "den", "U\u00b7nif\u00b7or\u00b7men\u00b7tand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hof und Adel, Etiquette,", "tokens": ["Hof", "und", "A\u00b7del", ",", "E\u00b7ti\u00b7quet\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "KON", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Titel, Orden, Rang und Stand!", "tokens": ["Ti\u00b7tel", ",", "Or\u00b7den", ",", "Rang", "und", "Stand", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tsching tsching. ", "tokens": ["Tsching", "tsching", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "He, juchhe! sie sind gefallen,", "tokens": ["He", ",", "juch\u00b7he", "!", "sie", "sind", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "$.", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und zertr\u00fcmmert ist ihr Sitz!", "tokens": ["Und", "zer\u00b7tr\u00fcm\u00b7mert", "ist", "ihr", "Sitz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Endlich ist erlaubt uns allen", "tokens": ["End\u00b7lich", "ist", "er\u00b7laubt", "uns", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "VVPP", "PPER", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wieder doch einmal ein Witz.", "tokens": ["Wie\u00b7der", "doch", "ein\u00b7mal", "ein", "Witz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.5": {"text": "Tsching tsching.", "tokens": ["Tsching", "tsching", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Welche Zukunft! o ihr Lieben,", "tokens": ["Wel\u00b7che", "Zu\u00b7kunft", "!", "o", "ihr", "Lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "$.", "FM", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihr Beamten, singt und lacht!", "tokens": ["Ihr", "Be\u00b7am\u00b7ten", ",", "singt", "und", "lacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ihr habt gar viel geschrieben,", "tokens": ["Denn", "ihr", "habt", "gar", "viel", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch nie einen Witz gemacht.", "tokens": ["Doch", "nie", "ei\u00b7nen", "Witz", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tsching tsching.", "tokens": ["Tsching", "tsching", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Die Conduitenlisten geben", "tokens": ["Die", "Con\u00b7dui\u00b7ten\u00b7lis\u00b7ten", "ge\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun anjetzt Bericht sogar,", "tokens": ["Nun", "an\u00b7jetzt", "Be\u00b7richt", "so\u00b7gar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wenn in seinem magern Leben", "tokens": ["Wenn", "in", "sei\u00b7nem", "ma\u00b7gern", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein Beamter witzig war.", "tokens": ["Ein", "Be\u00b7am\u00b7ter", "wit\u00b7zig", "war", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tsching tsching.", "tokens": ["Tsching", "tsching", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.5": {"line.1": {"text": "La\u00dft uns feiern in den Schenken", "tokens": ["La\u00dft", "uns", "fei\u00b7ern", "in", "den", "Schen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "J\u00e4hrlich ein Erinnrungsfest,", "tokens": ["J\u00e4hr\u00b7lich", "ein", "E\u00b7rinn\u00b7rungs\u00b7fest", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,"], "meter": "+--+---", "measure": "dactylic.di.plus"}, "line.3": {"text": "Denn wir d\u00fcrfen wieder denken", "tokens": ["Denn", "wir", "d\u00fcr\u00b7fen", "wie\u00b7der", "den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles was sich drucken l\u00e4\u00dft.", "tokens": ["Al\u00b7les", "was", "sich", "dru\u00b7cken", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PRELS", "PRF", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tsching tsching.", "tokens": ["Tsching", "tsching", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}