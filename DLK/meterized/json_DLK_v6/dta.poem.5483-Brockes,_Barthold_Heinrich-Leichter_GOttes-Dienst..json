{"dta.poem.5483": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Leichter GOttes-Dienst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist nicht nur jedwede Stunde;", "tokens": ["Es", "ist", "nicht", "nur", "jed\u00b7we\u00b7de", "Stun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es ist ein jeder Augenblick,", "tokens": ["Es", "ist", "ein", "je\u00b7der", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ja jede fl\u00fcchtige Secunde", "tokens": ["Ja", "je\u00b7de", "fl\u00fcch\u00b7ti\u00b7ge", "Se\u00b7cun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKANT", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von unsrer Lebens-Zeit ein St\u00fcck.", "tokens": ["Von", "uns\u00b7rer", "Le\u00b7bens\u00b7Zeit", "ein", "St\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du l\u00e4ssest eine nach der andern", "tokens": ["Du", "l\u00e4s\u00b7sest", "ei\u00b7ne", "nach", "der", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verfliegen, schwinden und vergehn,", "tokens": ["Ver\u00b7flie\u00b7gen", ",", "schwin\u00b7den", "und", "ver\u00b7gehn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "KON", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die k\u00fcnst\u2019ge zur vergangnen wandern,", "tokens": ["Die", "k\u00fcnst'\u00b7ge", "zur", "ver\u00b7gang\u00b7nen", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ohn, was du guts hast, anzusehn;", "tokens": ["Ohn", ",", "was", "du", "guts", "hast", ",", "an\u00b7zu\u00b7sehn", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "NN", "VAFIN", "$,", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wie lange denckst du dein Ergetzen", "tokens": ["Wie", "lan\u00b7ge", "denckst", "du", "dein", "Er\u00b7get\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ob allem, was dir GOtt geschenckt,", "tokens": ["Ob", "al\u00b7lem", ",", "was", "dir", "Gott", "ge\u00b7schenckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PWS", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und deinen Danck hinauszusetzen?", "tokens": ["Und", "dei\u00b7nen", "Danck", "hin\u00b7aus\u00b7zu\u00b7set\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Du lebst in Friede, bist geehret,", "tokens": ["Du", "lebst", "in", "Frie\u00b7de", ",", "bist", "ge\u00b7eh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Du bist gesund, du hast dein Brodt,", "tokens": ["Du", "bist", "ge\u00b7sund", ",", "du", "hast", "dein", "Brodt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und was zur Nothdurft dir geh\u00f6ret:", "tokens": ["Und", "was", "zur", "Noth\u00b7durft", "dir", "ge\u00b7h\u00f6\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Wilst du denn, bi\u00df an deinen Todt,", "tokens": ["Wilst", "du", "denn", ",", "bi\u00df", "an", "dei\u00b7nen", "Todt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "$,", "KOUS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Den, der dir alles gab, zu lieben,", "tokens": ["Den", ",", "der", "dir", "al\u00b7les", "gab", ",", "zu", "lie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "PIS", "VVFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "In deiner Lust ihn zu erh\u00f6hn,", "tokens": ["In", "dei\u00b7ner", "Lust", "ihn", "zu", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Bi\u00df (sag ich) an den Todt verschieben,", "tokens": ["Bi\u00df", "(", "sag", "ich", ")", "an", "den", "Todt", "ver\u00b7schie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$(", "VVIMP", "PPER", "$(", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Und da\u00df nur er dirs gab, verstehn,", "tokens": ["Und", "da\u00df", "nur", "er", "dirs", "gab", ",", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PPER", "PIS", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Um dann den Rest von deinem Leben,", "tokens": ["Um", "dann", "den", "Rest", "von", "dei\u00b7nem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Mit inniglicher Danckbarkeit,", "tokens": ["Mit", "in\u00b7nig\u00b7li\u00b7cher", "Dan\u00b7ck\u00b7bar\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.22": {"text": "Den grossen Geber zu erheben?", "tokens": ["Den", "gros\u00b7sen", "Ge\u00b7ber", "zu", "er\u00b7he\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Jetzt thu es! jetzo ist es Zeit!", "tokens": ["Jetzt", "thu", "es", "!", "jet\u00b7zo", "ist", "es", "Zeit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}