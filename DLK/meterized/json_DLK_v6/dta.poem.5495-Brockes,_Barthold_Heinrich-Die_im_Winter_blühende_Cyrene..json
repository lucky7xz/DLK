{"dta.poem.5495": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die im Winter bl\u00fchende Cyrene.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sehe dich, mit recht ger\u00fchretem Gem\u00fcthe,", "tokens": ["Ich", "se\u00b7he", "dich", ",", "mit", "recht", "ge\u00b7r\u00fch\u00b7re\u00b7tem", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Ja w\u00fcrcklich ohn Erstaunen kaum,", "tokens": ["Ja", "w\u00fcrck\u00b7lich", "ohn", "Er\u00b7stau\u00b7nen", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bepurpurter Cyrenen-Baum,", "tokens": ["Be\u00b7pur\u00b7pur\u00b7ter", "Cy\u00b7re\u00b7nen\u00b7Baum", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Jm Winter voll der sch\u00f6nsten Bl\u00fcthe!", "tokens": ["Jm", "Win\u00b7ter", "voll", "der", "sch\u00f6ns\u00b7ten", "Bl\u00fc\u00b7the", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich sehe dich, als wie im Lentzen,", "tokens": ["Ich", "se\u00b7he", "dich", ",", "als", "wie", "im", "Lent\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Jm Januario schon lieblich gl\u00e4ntzen.", "tokens": ["Jm", "Ja\u00b7nu\u00b7a\u00b7rio", "schon", "lieb\u00b7lich", "gl\u00e4nt\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Wie kanst du Nas\u2019 und Aug\u2019 erfrischen!", "tokens": ["Wie", "kanst", "du", "Nas'", "und", "Aug'", "er\u00b7fri\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich seh\u2019 in dir, fast ohne Gr\u00fcn,", "tokens": ["Ich", "seh'", "in", "dir", ",", "fast", "oh\u00b7ne", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "In mehr als hundert Blumen-B\u00fcschen,", "tokens": ["In", "mehr", "als", "hun\u00b7dert", "Blu\u00b7men\u00b7B\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "KOKOM", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Mehr als f\u00fcnf tausend Blumen bl\u00fchn,", "tokens": ["Mehr", "als", "f\u00fcnf", "tau\u00b7send", "Blu\u00b7men", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "CARD", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die all\u2019 im sch\u00f6nsten Purpur gl\u00fch\u2019n.", "tokens": ["Die", "all'", "im", "sch\u00f6ns\u00b7ten", "Pur\u00b7pur", "gl\u00fch'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "PIS", "APPRART", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Es stutzt ein jeder der dich sieht,", "tokens": ["Es", "stutzt", "ein", "je\u00b7der", "der", "dich", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PIS", "ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und l\u00e4\u00dft, zu deines Sch\u00f6pfers Ehren,", "tokens": ["Und", "l\u00e4\u00dft", ",", "zu", "dei\u00b7nes", "Sch\u00f6p\u00b7fers", "Eh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Ein Lob, fast wieder Willen, h\u00f6ren,", "tokens": ["Ein", "Lob", ",", "fast", "wie\u00b7der", "Wil\u00b7len", ",", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Wann ein: ", "tokens": ["Wann", "ein", ":"], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.16": {"text": "Da er kaum selbst wei\u00df, was er spricht.", "tokens": ["Da", "er", "kaum", "selbst", "wei\u00df", ",", "was", "er", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Wenn ich die\u00df h\u00f6re, k\u00f6mmt es mir", "tokens": ["Wenn", "ich", "die\u00df", "h\u00f6\u00b7re", ",", "k\u00f6mmt", "es", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "$,", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als wenn der Ausbruch seiner Lust,", "tokens": ["Als", "wenn", "der", "Aus\u00b7bruch", "sei\u00b7ner", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob sie gleich leider kurtz, mir doch entdecke,", "tokens": ["Ob", "sie", "gleich", "lei\u00b7der", "kurtz", ",", "mir", "doch", "ent\u00b7de\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "$,", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie in der Menschen kalten Brust", "tokens": ["Wie", "in", "der", "Men\u00b7schen", "kal\u00b7ten", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Etwas doch verborgen stecke,", "tokens": ["Ein", "Et\u00b7was", "doch", "ver\u00b7bor\u00b7gen", "ste\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df unsers Sch\u00f6pfers Macht, wie sie es wehrt,", "tokens": ["Da\u00df", "un\u00b7sers", "Sch\u00f6p\u00b7fers", "Macht", ",", "wie", "sie", "es", "wehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Beym Anblick seiner Wunder ehrt:", "tokens": ["Beym", "An\u00b7blick", "sei\u00b7ner", "Wun\u00b7der", "ehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und da\u00df wir, durch Gewohnheit blos allein,", "tokens": ["Und", "da\u00df", "wir", ",", "durch", "Ge\u00b7wohn\u00b7heit", "blos", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "$,", "APPR", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Umnebelt und geblendet seyn.", "tokens": ["Um\u00b7ne\u00b7belt", "und", "ge\u00b7blen\u00b7det", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "VAINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Wenn ich in diesem Baum den Purpur-Glantz erblicke,", "tokens": ["Wenn", "ich", "in", "die\u00b7sem", "Baum", "den", "Pur\u00b7pur\u00b7Glantz", "er\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Deucht mich, als ob auch er (so wie, nach dunckler Nacht", "tokens": ["Deucht", "mich", ",", "als", "ob", "auch", "er", "(", "so", "wie", ",", "nach", "dunck\u00b7ler", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KOKOM", "KOUS", "ADV", "PPER", "$(", "ADV", "KOKOM", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Morgen-R\u00f6the Purpur Pracht", "tokens": ["Der", "Mor\u00b7gen\u00b7R\u00f6\u00b7the", "Pur\u00b7pur", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die graue D\u00e4mmrung f\u00e4rbt) die graue D\u00e4mmrung schm\u00fccke,", "tokens": ["Die", "grau\u00b7e", "D\u00e4mm\u00b7rung", "f\u00e4rbt", ")", "die", "grau\u00b7e", "D\u00e4mm\u00b7rung", "schm\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die uns im Winter deckt, und ich des Fr\u00fchlings Morgen,", "tokens": ["Die", "uns", "im", "Win\u00b7ter", "deckt", ",", "und", "ich", "des", "Fr\u00fch\u00b7lings", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,", "KON", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der uns annoch durch Frost und Duft verborgen,", "tokens": ["Der", "uns", "an\u00b7noch", "durch", "Frost", "und", "Duft", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Nicht mehr entfernt, und in der N\u00e4he", "tokens": ["Nicht", "mehr", "ent\u00b7fernt", ",", "und", "in", "der", "N\u00e4\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ADJD", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schon seine Morgenr\u00f6hte sehe.", "tokens": ["Schon", "sei\u00b7ne", "Mor\u00b7gen\u00b7r\u00f6h\u00b7te", "se\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich seh, geliebter Baum, in dir zugleich die Spur,", "tokens": ["Ich", "seh", ",", "ge\u00b7lieb\u00b7ter", "Baum", ",", "in", "dir", "zu\u00b7gleich", "die", "Spur", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,", "APPR", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df die gesch\u00e4ftige Natur", "tokens": ["Da\u00df", "die", "ge\u00b7sch\u00e4f\u00b7ti\u00b7ge", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht schlaffe, wie es scheint; nein da\u00df sie immer kr\u00e4ftig", "tokens": ["Nicht", "schlaf\u00b7fe", ",", "wie", "es", "scheint", ";", "nein", "da\u00df", "sie", "im\u00b7mer", "kr\u00e4f\u00b7tig"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$.", "PTKANT", "KOUS", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und, wenn sie nichts verhindert, stets gesch\u00e4ftig,", "tokens": ["Und", ",", "wenn", "sie", "nichts", "ver\u00b7hin\u00b7dert", ",", "stets", "ge\u00b7sch\u00e4f\u00b7tig", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PIS", "VVPP", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und nimmer m\u00fc\u00dfig sey. Es reitzt mich deine Pracht,", "tokens": ["Und", "nim\u00b7mer", "m\u00fc\u00b7\u00dfig", "sey", ".", "Es", "reitzt", "mich", "dei\u00b7ne", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "$.", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In meiner Lust, zum Ruhm de\u00df, welcher dich gemacht,", "tokens": ["In", "mei\u00b7ner", "Lust", ",", "zum", "Ruhm", "de\u00df", ",", "wel\u00b7cher", "dich", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPRART", "NN", "ART", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und preis\u2019 ich auch in dir, mit br\u00fcnstigem Gem\u00fcthe,", "tokens": ["Und", "preis'", "ich", "auch", "in", "dir", ",", "mit", "br\u00fcns\u00b7ti\u00b7gem", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den Ausbruch seiner Macht und G\u00fcte.", "tokens": ["Den", "Aus\u00b7bruch", "sei\u00b7ner", "Macht", "und", "G\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Nun fehlet nichts, als da\u00df ich dich nunmehr,", "tokens": ["Nun", "feh\u00b7let", "nichts", ",", "als", "da\u00df", "ich", "dich", "nun\u00b7mehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "KOKOM", "KOUS", "PPER", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu mehr Verbreitung noch von deines Sch\u00f6pfers Ehr,", "tokens": ["Zu", "mehr", "Ver\u00b7brei\u00b7tung", "noch", "von", "dei\u00b7nes", "Sch\u00f6p\u00b7fers", "Ehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dem Auszug aller klugen Geister,", "tokens": ["Dem", "Aus\u00b7zug", "al\u00b7ler", "klu\u00b7gen", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hammoniens so w\u00fcrd\u2019gem B\u00fcrgermeister,", "tokens": ["Ham\u00b7mo\u00b7ni\u00b7ens", "so", "w\u00fcrd'\u00b7gem", "B\u00fcr\u00b7ger\u00b7meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dem theuren ", "tokens": ["Dem", "theu\u00b7ren"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Damit Er sich, an deiner Pracht,", "tokens": ["Da\u00b7mit", "Er", "sich", ",", "an", "dei\u00b7ner", "Pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie Er es sonst mit GOttes Wercken macht,", "tokens": ["Wie", "Er", "es", "sonst", "mit", "Got\u00b7tes", "Wer\u00b7cken", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "APPR", "NN", "NN", "VVFIN", "$,"], "met