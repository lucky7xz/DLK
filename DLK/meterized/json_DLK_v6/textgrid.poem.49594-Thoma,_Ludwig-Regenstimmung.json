{"textgrid.poem.49594": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Regenstimmung", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Papa sitzt in der kurzen Hos", "tokens": ["Pa\u00b7pa", "sitzt", "in", "der", "kur\u00b7zen", "Hos"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Mit blau gefrornem Knie.", "tokens": ["Mit", "blau", "ge\u00b7fror\u00b7nem", "Knie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gott, ist denn hier auch gar nischt los?", "tokens": ["Gott", ",", "ist", "denn", "hier", "auch", "gar", "nischt", "los", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ADV", "ADV", "ADV", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht eine Skatpartie?", "tokens": ["Nicht", "ei\u00b7ne", "Skat\u00b7par\u00b7tie", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mama hat zehn Pfund Schwabbelherz", "tokens": ["Ma\u00b7ma", "hat", "zehn", "Pfund", "Schwab\u00b7bel\u00b7herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "CARD", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Mieder eingeschn\u00fcrt,", "tokens": ["Im", "Mie\u00b7der", "ein\u00b7ge\u00b7schn\u00fcrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wodurch sie einen leisen Schmerz", "tokens": ["Wo\u00b7durch", "sie", "ei\u00b7nen", "lei\u00b7sen", "Schmerz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis an den Nabel sp\u00fcrt.", "tokens": ["Bis", "an", "den", "Na\u00b7bel", "sp\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was soll sie tun? Nu Gott, sie nimmt", "tokens": ["Was", "soll", "sie", "tun", "?", "Nu", "Gott", ",", "sie", "nimmt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$.", "ADV", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was S\u00fc\u00dfes zu sich ein,", "tokens": ["Was", "S\u00fc\u00b7\u00dfes", "zu", "sich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und was ihr auch nicht gut bekimmt,", "tokens": ["Und", "was", "ihr", "auch", "nicht", "gut", "be\u00b7kimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie f\u00fchlt sich so allein.", "tokens": ["Sie", "f\u00fchlt", "sich", "so", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Tochter sitzt auf der Altan'", "tokens": ["Die", "Toch\u00b7ter", "sitzt", "auf", "der", "Al\u00b7tan'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Alpenm\u00e4dchentracht,", "tokens": ["In", "Al\u00b7pen\u00b7m\u00e4d\u00b7chen\u00b7tracht", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wodurch ihr gleich ein junger Mann", "tokens": ["Wo\u00b7durch", "ihr", "gleich", "ein", "jun\u00b7ger", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Courbeschneidung macht.", "tokens": ["Die", "Cour\u00b7be\u00b7schnei\u00b7dung", "macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Gott! Wenn's nicht f\u00e4schon\u00e4bl w\u00e4r!", "tokens": ["Gott", "!", "Wenn's", "nicht", "f\u00e4sc\u00b7ho\u00b7n\u00e4bl", "w\u00e4r", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was tut mer auf dem Platz?", "tokens": ["Was", "tut", "mer", "auf", "dem", "Platz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Unterhaltung is prek\u00e4r", "tokens": ["Die", "Un\u00b7ter\u00b7hal\u00b7tung", "is", "pre\u00b7k\u00e4r"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "FM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wirklich f\u00fcr die Katz.", "tokens": ["Und", "wirk\u00b7lich", "f\u00fcr", "die", "Katz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}