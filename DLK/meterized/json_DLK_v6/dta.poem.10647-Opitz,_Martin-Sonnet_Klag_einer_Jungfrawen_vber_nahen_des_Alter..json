{"dta.poem.10647": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet  \n  Klag einer Jungfrawen vber nahen des Alter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ach wo ist jetzt die Zeit/ da jederman thet gleichen", "tokens": ["Ach", "wo", "ist", "jetzt", "die", "Zeit", "/", "da", "je\u00b7der\u00b7man", "thet", "glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "PWAV", "VAFIN", "ADV", "ART", "NN", "$(", "KOUS", "PIS", "VVFIN", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Rosen sch\u00f6ne Zier mein edele Gestallt?", "tokens": ["Der", "Ro\u00b7sen", "sch\u00f6\u00b7ne", "Zier", "mein", "e\u00b7de\u00b7le", "Ge\u00b7stallt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ja wol ich bin wie sie/ nun ich bin worden alt/", "tokens": ["Ja", "wol", "ich", "bin", "wie", "sie", "/", "nun", "ich", "bin", "wor\u00b7den", "alt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "PPER", "VAFIN", "KOKOM", "PPER", "$(", "ADV", "PPER", "VAFIN", "VAPP", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Eh sie der Sonnen glantz des Morgens kan erreichen/", "tokens": ["Eh", "sie", "der", "Son\u00b7nen", "glantz", "des", "Mor\u00b7gens", "kan", "er\u00b7rei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "ART", "ADV", "VMFIN", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Mu\u00df sie durch k\u00fchle Lufft der kalten Nacht verbleichen/", "tokens": ["Mu\u00df", "sie", "durch", "k\u00fch\u00b7le", "Lufft", "der", "kal\u00b7ten", "Nacht", "ver\u00b7blei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vnd hat nur von dem Taw noch jhren vnderhalt/", "tokens": ["Vnd", "hat", "nur", "von", "dem", "Taw", "noch", "jhren", "vn\u00b7der\u00b7halt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "ART", "NN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "So netzen mich jetzt auch die Thr\u00e4nen manigfalt/", "tokens": ["So", "net\u00b7zen", "mich", "jetzt", "auch", "die", "Thr\u00e4\u00b7nen", "ma\u00b7nig\u00b7falt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil ich die junge Zeit fast habe lassen schleichen/", "tokens": ["Weil", "ich", "die", "jun\u00b7ge", "Zeit", "fast", "ha\u00b7be", "las\u00b7sen", "schlei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "VAFIN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Kompt dann die Morgenr\u00f6th/ so wirdt die Rose roth/", "tokens": ["Kompt", "dann", "die", "Mor\u00b7gen\u00b7r\u00f6th", "/", "so", "wirdt", "die", "Ro\u00b7se", "roth", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$(", "ADV", "VAFIN", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich werde schamroth auch wann ich denck an die Noth/", "tokens": ["Ich", "wer\u00b7de", "scham\u00b7roth", "auch", "wann", "ich", "denck", "an", "die", "Noth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "PWAV", "PPER", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Doch hab ich diesen Trost/ da\u00df gleich wie von den Winden", "tokens": ["Doch", "hab", "ich", "die\u00b7sen", "Trost", "/", "da\u00df", "gleich", "wie", "von", "den", "Win\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PDAT", "NN", "$(", "KOUS", "ADV", "KOKOM", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Rose/ wann der Tag sich neigt/ wirdt abgemeyt/", "tokens": ["Die", "Ro\u00b7se", "/", "wann", "der", "Tag", "sich", "neigt", "/", "wirdt", "ab\u00b7ge\u00b7meyt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAV", "ART", "NN", "PRF", "VVFIN", "$(", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So werdt ich auch/ weil nun mein Abendt nicht ist weit/", "tokens": ["So", "werdt", "ich", "auch", "/", "weil", "nun", "mein", "A\u00b7bendt", "nicht", "ist", "weit", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$(", "KOUS", "ADV", "PPOSAT", "NN", "PTKNEG", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wanns hie ja nicht kan sein/ doch Ruh im Grabe finden.", "tokens": ["Wanns", "hie", "ja", "nicht", "kan", "sein", "/", "doch", "Ruh", "im", "Gra\u00b7be", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "PTKNEG", "VMFIN", "VAINF", "$(", "ADV", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}