{"dta.poem.18695": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Lieblich ist des M\u00e4dchens Blick der winket,", "tokens": ["Lieb\u00b7lich", "ist", "des", "M\u00e4d\u00b7chens", "Blick", "der", "win\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "NN", "ART", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Trinkers Blick ist lieblich eh er trinket,", "tokens": ["Trin\u00b7kers", "Blick", "ist", "lieb\u00b7lich", "eh", "er", "trin\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADJD", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Gruss des Herren der befehlen konnte,", "tokens": ["Gruss", "des", "Her\u00b7ren", "der", "be\u00b7feh\u00b7len", "konn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sonnenschein im Herbst der dich besonnte.", "tokens": ["Son\u00b7nen\u00b7schein", "im", "Herbst", "der", "dich", "be\u00b7sonn\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Lieblicher als alles dieses habe", "tokens": ["Lieb\u00b7li\u00b7cher", "als", "al\u00b7les", "die\u00b7ses", "ha\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "PIS", "PDS", "VAFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Stets vor Augen, wie sich kleiner Gabe", "tokens": ["Stets", "vor", "Au\u00b7gen", ",", "wie", "sich", "klei\u00b7ner", "Ga\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "$,", "PWAV", "PRF", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "D\u00fcrft\u2019ge Hand so h\u00fcbsch entgegen dr\u00e4nget,", "tokens": ["D\u00fcrft'\u00b7ge", "Hand", "so", "h\u00fcbsch", "ent\u00b7ge\u00b7gen", "dr\u00e4n\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "ADJD", "PTKVZ", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Zierlich dankbar was du reichst empf\u00e4nget.", "tokens": ["Zier\u00b7lich", "dank\u00b7bar", "was", "du", "reichst", "emp\u00b7f\u00e4n\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PWS", "PPER", "VVFIN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Welch ein Blick! ein Gruss! ein sprechend Streben!", "tokens": ["Welch", "ein", "Blick", "!", "ein", "Gruss", "!", "ein", "spre\u00b7chend", "Stre\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "$.", "ART", "NN", "$.", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Schau es recht und du wirst immer geben.", "tokens": ["Schau", "es", "recht", "und", "du", "wirst", "im\u00b7mer", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "KON", "PPER", "VAFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}