{"textgrid.poem.41896": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du holder Geist der Lieder, den hienieden", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du holder Geist der Lieder, den hienieden", "tokens": ["Du", "hol\u00b7der", "Geist", "der", "Lie\u00b7der", ",", "den", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJA", "NN", "ART", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Troste mir ein milder Gott verliehn,", "tokens": ["Zum", "Tros\u00b7te", "mir", "ein", "mil\u00b7der", "Gott", "ver\u00b7liehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du Einziger, der nie von mir geschieden,", "tokens": ["Du", "Ein\u00b7zi\u00b7ger", ",", "der", "nie", "von", "mir", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Der freundlich oft, bekr\u00e4nzt mit Immergr\u00fcn", "tokens": ["Der", "freund\u00b7lich", "oft", ",", "be\u00b7kr\u00e4nzt", "mit", "Im\u00b7mer\u00b7gr\u00fcn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "ADV", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und angethan mit tr\u00e4umerischem Frieden,", "tokens": ["Und", "an\u00b7ge\u00b7than", "mit", "tr\u00e4u\u00b7me\u00b7ri\u00b7schem", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein rettend Licht im Sturme mir erschien,", "tokens": ["Ein", "ret\u00b7tend", "Licht", "im", "Stur\u00b7me", "mir", "er\u00b7schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Noch einmal la\u00df in wunderbaren Weisen", "tokens": ["Noch", "ein\u00b7mal", "la\u00df", "in", "wun\u00b7der\u00b7ba\u00b7ren", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVIMP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Durch dich dich selbst und, die dich weckt, mich preisen!", "tokens": ["Durch", "dich", "dich", "selbst", "und", ",", "die", "dich", "weckt", ",", "mich", "prei\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "ADV", "KON", "$,", "PRELS", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Verworren liegt das unbest\u00e4nd'ge Leben", "tokens": ["Ver\u00b7wor\u00b7ren", "liegt", "das", "un\u00b7be\u00b7st\u00e4n\u00b7d'\u00b7ge", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vor unserm Blick und ungestaltet da,", "tokens": ["Vor", "un\u00b7serm", "Blick", "und", "un\u00b7ge\u00b7stal\u00b7tet", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Keiner kann's entwirren und entweben,", "tokens": ["Und", "Kei\u00b7ner", "kann's", "ent\u00b7wir\u00b7ren", "und", "ent\u00b7we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "VVINF", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wer nicht die Welt in deinem Spiegel sah.", "tokens": ["Wer", "nicht", "die", "Welt", "in", "dei\u00b7nem", "Spie\u00b7gel", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du machst das Harte mild, das Rauhe eben,", "tokens": ["Du", "machst", "das", "Har\u00b7te", "mild", ",", "das", "Rau\u00b7he", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das Dunkle hell, das Weitentfernte nah", "tokens": ["Das", "Dunk\u00b7le", "hell", ",", "das", "Wei\u00b7tent\u00b7fern\u00b7te", "nah"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und wei\u00dft allein in lieblichen Gestalten", "tokens": ["Und", "wei\u00dft", "al\u00b7lein", "in", "lieb\u00b7li\u00b7chen", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den kurzen Traum des Sch\u00f6nen festzuhalten.", "tokens": ["Den", "kur\u00b7zen", "Traum", "des", "Sch\u00f6\u00b7nen", "fest\u00b7zu\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So f\u00fchrtest du in jenen holden Tagen,", "tokens": ["So", "f\u00fchr\u00b7test", "du", "in", "je\u00b7nen", "hol\u00b7den", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als noch das Gl\u00fcck sich freundlich mir gesellt,", "tokens": ["Als", "noch", "das", "Gl\u00fcck", "sich", "freund\u00b7lich", "mir", "ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PRF", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Irrenden auf leichtem Zauberwagen", "tokens": ["Den", "Ir\u00b7ren\u00b7den", "auf", "leich\u00b7tem", "Zau\u00b7ber\u00b7wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit raschem Flug durch deine Wunderwelt.", "tokens": ["Mit", "ra\u00b7schem", "Flug", "durch", "dei\u00b7ne", "Wun\u00b7der\u00b7welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und wie ein Blatt, das linde L\u00fcfte tragen,", "tokens": ["Und", "wie", "ein", "Blatt", ",", "das", "lin\u00b7de", "L\u00fcf\u00b7te", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Silberflor des Herbstes flatternd h\u00e4lt,", "tokens": ["Der", "Sil\u00b7ber\u00b7flor", "des", "Herbs\u00b7tes", "flat\u00b7ternd", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So kettet' ich, noch eh die Bilder schwanden,", "tokens": ["So", "ket\u00b7tet'", "ich", ",", "noch", "eh", "die", "Bil\u00b7der", "schwan\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die L\u00e4chelnden mit zarten Liedesbanden.", "tokens": ["Die", "L\u00e4\u00b7cheln\u00b7den", "mit", "zar\u00b7ten", "Lie\u00b7des\u00b7ban\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch wie gemach bey fl\u00fccht'ger Weste Scherzen", "tokens": ["Doch", "wie", "ge\u00b7mach", "bey", "fl\u00fccht'\u00b7ger", "Wes\u00b7te", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die keusche Ros' im heil'gen Glanz entgl\u00fcht,", "tokens": ["Die", "keu\u00b7sche", "Ros'", "im", "heil'\u00b7gen", "Glanz", "ent\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So war auch mir im leichtbewegten Herzen", "tokens": ["So", "war", "auch", "mir", "im", "leicht\u00b7be\u00b7weg\u00b7ten", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein sel'ges Bild allm\u00e4hlig aufgebl\u00fcht.", "tokens": ["Ein", "sel'\u00b7ges", "Bild", "all\u00b7m\u00e4h\u00b7lig", "auf\u00b7ge\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Tief wogte jetzt in Freuden und in Schmerzen,", "tokens": ["Tief", "wog\u00b7te", "jetzt", "in", "Freu\u00b7den", "und", "in", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In Wahn und Wunsch das tr\u00e4umende Gem\u00fcth,", "tokens": ["In", "Wahn", "und", "Wunsch", "das", "tr\u00e4u\u00b7men\u00b7de", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.7": {"text": "Und nur in dir konnt' ich das Leid enth\u00fcllen,", "tokens": ["Und", "nur", "in", "dir", "konnt'", "ich", "das", "Leid", "ent\u00b7h\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Lust verstehn, die gl\u00fch'nde Sehnsucht stillen.", "tokens": ["Die", "Lust", "ver\u00b7stehn", ",", "die", "gl\u00fch'n\u00b7de", "Sehn\u00b7sucht", "stil\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Da nahten sich des Lebens tr\u00fcbste Stunden,", "tokens": ["Da", "nah\u00b7ten", "sich", "des", "Le\u00b7bens", "tr\u00fcbs\u00b7te", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und eisern hielt das Schicksal sein Gericht.", "tokens": ["Und", "ei\u00b7sern", "hielt", "das", "Schick\u00b7sal", "sein", "Ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hei\u00df bluteten die nie geschlo\u00dfnen Wunden,", "tokens": ["Hei\u00df", "blu\u00b7te\u00b7ten", "die", "nie", "ge\u00b7schlo\u00df\u00b7nen", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und n\u00e4chtlich sank der Jugend heitres Licht.", "tokens": ["Und", "n\u00e4cht\u00b7lich", "sank", "der", "Ju\u00b7gend", "heit\u00b7res", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Treue, die mein Herz in sich gefunden,", "tokens": ["Die", "Treu\u00b7e", ",", "die", "mein", "Herz", "in", "sich", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die fand es jetzt in andern Herzen nicht,", "tokens": ["Die", "fand", "es", "jetzt", "in", "an\u00b7dern", "Her\u00b7zen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und dessen Hand, den alles Gl\u00fcck verlassen,", "tokens": ["Und", "des\u00b7sen", "Hand", ",", "den", "al\u00b7les", "Gl\u00fcck", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "$,", "ART", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nicht wagte sie der Gl\u00fcckliche zu fassen.", "tokens": ["Nicht", "wag\u00b7te", "sie", "der", "Gl\u00fcck\u00b7li\u00b7che", "zu", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Nur du, der sonst mit jedem Hauch entflogen,", "tokens": ["Nur", "du", ",", "der", "sonst", "mit", "je\u00b7dem", "Hauch", "ent\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PRELS", "ADV", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der nur am Scherz, am Spiele sich erfreut,", "tokens": ["Der", "nur", "am", "Scherz", ",", "am", "Spie\u00b7le", "sich", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "$,", "APPRART", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du bliebst allein dem Traurenden gewogen", "tokens": ["Du", "bliebst", "al\u00b7lein", "dem", "Trau\u00b7ren\u00b7den", "ge\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und theiltest gern des Freundes Einsamkeit.", "tokens": ["Und", "theil\u00b7test", "gern", "des", "Freun\u00b7des", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und wie der Wein, der gr\u00fcn den Baum umzogen,", "tokens": ["Und", "wie", "der", "Wein", ",", "der", "gr\u00fcn", "den", "Baum", "um\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dem welken selbst der Jugend Anmuth leiht,", "tokens": ["Dem", "wel\u00b7ken", "selbst", "der", "Ju\u00b7gend", "An\u00b7muth", "leiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So sah ich dich um mein erstorbnes Leben", "tokens": ["So", "sah", "ich", "dich", "um", "mein", "ers\u00b7torb\u00b7nes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zum ew'gen Schmuck holdbl\u00fch'nde Kr\u00e4nze weben.