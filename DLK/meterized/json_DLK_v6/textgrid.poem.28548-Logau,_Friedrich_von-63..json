{"textgrid.poem.28548": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "63.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil die Venus Abend-Stern und auch Morgen-Stern verbleibt,", "tokens": ["Weil", "die", "Ve\u00b7nus", "A\u00b7ben\u00b7dS\u00b7tern", "und", "auch", "Mor\u00b7gen\u00b7S\u00b7tern", "ver\u00b7bleibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "KON", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+---+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Wie da\u00df sie die gantze Nacht ihre W\u00fcrckung dann nicht treibt?", "tokens": ["Wie", "da\u00df", "sie", "die", "gant\u00b7ze", "Nacht", "ih\u00b7re", "W\u00fcr\u00b7ckung", "dann", "nicht", "treibt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "ART", "ADJA", "NN", "PPOSAT", "NN", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "----+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Also wird gefragt ein Mann, der sich neulich hat beweibt.", "tokens": ["Al\u00b7so", "wird", "ge\u00b7fragt", "ein", "Mann", ",", "der", "sich", "neu\u00b7lich", "hat", "be\u00b7weibt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "ART", "NN", "$,", "PRELS", "PRF", "ADV", "VAFIN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}}}}}