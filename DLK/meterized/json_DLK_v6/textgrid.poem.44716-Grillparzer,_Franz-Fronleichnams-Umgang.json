{"textgrid.poem.44716": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Fronleichnams-Umgang", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie gehen um, sie gehen um,", "tokens": ["Sie", "ge\u00b7hen", "um", ",", "sie", "ge\u00b7hen", "um", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verstorbener Jahrhunderte", "tokens": ["Ver\u00b7stor\u00b7be\u00b7ner", "Jahr\u00b7hun\u00b7der\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+---", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Nacht-Trug-Gespenster gehen um,", "tokens": ["Nacht\u00b7Trug\u00b7Ge\u00b7spens\u00b7ter", "ge\u00b7hen", "um", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie leben nicht, sie atmen nicht", "tokens": ["Sie", "le\u00b7ben", "nicht", ",", "sie", "at\u00b7men", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und spotten uns ins Angesicht.", "tokens": ["Und", "spot\u00b7ten", "uns", "ins", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}