{"textgrid.poem.58889": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "108. Trau doch!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man glaubt wohl Menschen, was sie sagen,", "tokens": ["Man", "glaubt", "wohl", "Men\u00b7schen", ",", "was", "sie", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An Gottes Treu man zweifelt noch;", "tokens": ["An", "Got\u00b7tes", "Treu", "man", "zwei\u00b7felt", "noch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PIS", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hilft er nicht bald, so will man klagen.", "tokens": ["Hilft", "er", "nicht", "bald", ",", "so", "will", "man", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "$,", "ADV", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kleingl\u00e4ubiger, ei merke doch!", "tokens": ["Klein\u00b7gl\u00e4u\u00b7bi\u00b7ger", ",", "ei", "mer\u00b7ke", "doch", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "VVFIN", "ADV", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}}}}}