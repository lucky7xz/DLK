{"textgrid.poem.39861": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unsel'ger Fluch der Endlichkeit: gespalten", "tokens": ["Un\u00b7sel'\u00b7ger", "Fluch", "der", "End\u00b7lich\u00b7keit", ":", "ge\u00b7spal\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "ART", "NN", "$.", "VVPP"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ist Form und Wesen, ewig abgetrennt!", "tokens": ["Ist", "Form", "und", "We\u00b7sen", ",", "e\u00b7wig", "ab\u00b7ge\u00b7trennt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der stolzen Wohnung, die sich Sch\u00f6nheit nennt,", "tokens": ["Der", "stol\u00b7zen", "Woh\u00b7nung", ",", "die", "sich", "Sch\u00f6n\u00b7heit", "nennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PRF", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ist stets der Fremdling Seele fern gehalten.", "tokens": ["Ist", "stets", "der", "Fremd\u00b7ling", "See\u00b7le", "fern", "ge\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das Nichts schl\u00e4ft in der Wunderblume Falten,", "tokens": ["Das", "Nichts", "schl\u00e4ft", "in", "der", "Wun\u00b7der\u00b7blu\u00b7me", "Fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Lieb' und Lied verherrlicht und nicht kennt;", "tokens": ["Die", "Lieb'", "und", "Lied", "ver\u00b7herr\u00b7licht", "und", "nicht", "kennt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "KON", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Suchst du der Tugend heilig Licht? es brennt", "tokens": ["Suchst", "du", "der", "Tu\u00b7gend", "hei\u00b7lig", "Licht", "?", "es", "brennt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verborgen vor der Welt, in Mi\u00dfgestalten!", "tokens": ["Ver\u00b7bor\u00b7gen", "vor", "der", "Welt", ",", "in", "Mi\u00df\u00b7ge\u00b7stal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So sprach der Zweifel, und ein Wolkenschleier", "tokens": ["So", "sprach", "der", "Zwei\u00b7fel", ",", "und", "ein", "Wol\u00b7ken\u00b7schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zog tr\u00fcb sich \u00fcber der Verkl\u00e4rung Bild,", "tokens": ["Zog", "tr\u00fcb", "sich", "\u00fc\u00b7ber", "der", "Ver\u00b7kl\u00e4\u00b7rung", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PRF", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und eingesch\u00fcchtert floh der Mond von dannen.", "tokens": ["Und", "ein\u00b7ge\u00b7sch\u00fcch\u00b7tert", "floh", "der", "Mond", "von", "dan\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VVFIN", "ART", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch Sterne blieben, und mein Geist ward freier,", "tokens": ["Doch", "Ster\u00b7ne", "blie\u00b7ben", ",", "und", "mein", "Geist", "ward", "frei\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn die Erinnrung l\u00e4chelte noch mild,", "tokens": ["Denn", "die", "E\u00b7rinn\u00b7rung", "l\u00e4\u00b7chel\u00b7te", "noch", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und lie\u00df sich nicht aus meinen Tr\u00e4umen bannen.", "tokens": ["Und", "lie\u00df", "sich", "nicht", "aus", "mei\u00b7nen", "Tr\u00e4u\u00b7men", "ban\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}