{"textgrid.poem.57465": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "1L: So wahr ich redlich bin,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So wahr ich redlich bin,", "tokens": ["So", "wahr", "ich", "red\u00b7lich", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Entfernte Sch\u00e4ferinn:", "tokens": ["Ent\u00b7fern\u00b7te", "Sch\u00e4\u00b7fe\u00b7rinn", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bin ich, es bleibt dabey!", "tokens": ["Bin", "ich", ",", "es", "bleibt", "da\u00b7bey", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PPER", "VVFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dir bis zur Grube treu.", "tokens": ["Dir", "bis", "zur", "Gru\u00b7be", "treu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach f\u00fchlte nur mein Herz", "tokens": ["Ach", "f\u00fchl\u00b7te", "nur", "mein", "Herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Nicht st\u00fcndlich einen Schmerz,", "tokens": ["Nicht", "st\u00fcnd\u00b7lich", "ei\u00b7nen", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der t\u00e4glich weiter geht,", "tokens": ["Der", "t\u00e4g\u00b7lich", "wei\u00b7ter", "geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und blo\u00df daher entsteht;", "tokens": ["Und", "blo\u00df", "da\u00b7her", "ent\u00b7steht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PAV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Da\u00df ich den ersten Ku\u00df", "tokens": ["Da\u00df", "ich", "den", "ers\u00b7ten", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Von dir entbehren mu\u00df.", "tokens": ["Von", "dir", "ent\u00b7beh\u00b7ren", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zwar als es mir gegl\u00fcckt,", "tokens": ["Zwar", "als", "es", "mir", "ge\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df ich dich einst erblickt;", "tokens": ["Da\u00df", "ich", "dich", "einst", "er\u00b7blickt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dir in kurzer Zeit", "tokens": ["Und", "dir", "in", "kur\u00b7zer", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mein ganzes Herz geweiht:", "tokens": ["Mein", "gan\u00b7zes", "Herz", "ge\u00b7weiht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da that mein bl\u00f6der Mund", "tokens": ["Da", "that", "mein", "bl\u00f6\u00b7der", "Mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Dir noch so viel nicht kund.", "tokens": ["Dir", "noch", "so", "viel", "nicht", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich hie\u00df es ein Vergehn,", "tokens": ["Ich", "hie\u00df", "es", "ein", "Ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und freches Unterstehn;", "tokens": ["Und", "fre\u00b7ches", "Un\u00b7ter\u00b7stehn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Aus Furcht: Ihr strenger Muth", "tokens": ["Aus", "Furcht", ":", "Ihr", "stren\u00b7ger", "Muth"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Hei\u00dft dirs unm\u00f6glich gut.", "tokens": ["Hei\u00dft", "dirs", "un\u00b7m\u00f6g\u00b7lich", "gut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "ADJD", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.3": {"line.1": {"text": "Denn da ichs einst gewagt,", "tokens": ["Denn", "da", "ichs", "einst", "ge\u00b7wagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und dir auch ungefragt,", "tokens": ["Und", "dir", "auch", "un\u00b7ge\u00b7fragt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit gro\u00dfer List einmal", "tokens": ["Mit", "gro\u00b7\u00dfer", "List", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein halbes M\u00e4ulchen stahl:", "tokens": ["Ein", "hal\u00b7bes", "M\u00e4ul\u00b7chen", "stahl", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hilf Himmel! wie erhitzt", "tokens": ["Hilf", "Him\u00b7mel", "!", "wie", "er\u00b7hitzt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$.", "PWAV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Hast du auf mich geblitzt;", "tokens": ["Hast", "du", "auf", "mich", "ge\u00b7blitzt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Und mir so sehr gedroht,", "tokens": ["Und", "mir", "so", "sehr", "ge\u00b7droht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Als ob der \u00e4rgste Tod", "tokens": ["Als", "ob", "der", "\u00e4rgs\u00b7te", "Tod"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Noch lange nicht zu schwer", "tokens": ["Noch", "lan\u00b7ge", "nicht", "zu", "schwer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PTKNEG", "PTKA", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "F\u00fcr meinen Fehler w\u00e4r.", "tokens": ["F\u00fcr", "mei\u00b7nen", "Feh\u00b7ler", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Drum hab ich nach der Zeit,", "tokens": ["Drum", "hab", "ich", "nach", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mit mehr Bescheidenheit,", "tokens": ["Mit", "mehr", "Be\u00b7schei\u00b7den\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur deiner sch\u00f6nen Hand", "tokens": ["Nur", "dei\u00b7ner", "sch\u00f6\u00b7nen", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die K\u00fcsse zugewandt.", "tokens": ["Die", "K\u00fcs\u00b7se", "zu\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das lie\u00dfest du zwar zu,", "tokens": ["Das", "lie\u00b7\u00dfest", "du", "zwar", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Doch meiner Seelen Ruh", "tokens": ["Doch", "mei\u00b7ner", "See\u00b7len", "Ruh"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ward dadurch nicht gestillt:", "tokens": ["Ward", "da\u00b7durch", "nicht", "ge\u00b7stillt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Obgleich dein Engelsbild", "tokens": ["Ob\u00b7gleich", "dein", "En\u00b7gels\u00b7bild"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Mir, bis auf diesen Tag", "tokens": ["Mir", ",", "bis", "auf", "die\u00b7sen", "Tag"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Noch stets im Sinne lag.", "tokens": ["Noch", "stets", "im", "Sin\u00b7ne", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ward mirs hernach erlaubt,", "tokens": ["Ward", "mirs", "her\u00b7nach", "er\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Was ich sonst nie geglaubt,", "tokens": ["Was", "ich", "sonst", "nie", "ge\u00b7glaubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu sagen, Sch\u00e4ferinn!", "tokens": ["Zu", "sa\u00b7gen", ",", "Sch\u00e4\u00b7fe\u00b7rinn", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ich der Deine bin:", "tokens": ["Da\u00df", "ich", "der", "Dei\u00b7ne", "bin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "PPOSAT", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O was f\u00fcr Himmelslust", "tokens": ["O", "was", "f\u00fcr", "Him\u00b7mels\u00b7lust"], "token_info": ["word", "word", "word", "word"], "pos": ["ITJ", "PWS", "APPR", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Ergetzte meine Brust!", "tokens": ["Er\u00b7getz\u00b7te", "mei\u00b7ne", "Brust", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Allein, was half es mir?", "tokens": ["Al\u00b7lein", ",", "was", "half", "es", "mir", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ich war entfernt von dir;", "tokens": ["Ich", "war", "ent\u00b7fernt", "von", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Drum konnte meine Pein", "tokens": ["Drum", "konn\u00b7te", "mei\u00b7ne", "Pein"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Noch nicht gestillet seyn.", "tokens": ["Noch", "nicht", "ge\u00b7stil\u00b7let", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Oft geb ich zwar im Traum", "tokens": ["Oft", "geb", "ich", "zwar", "im", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den Fantaseyen Raum;", "tokens": ["Den", "Fan\u00b7ta\u00b7se\u00b7yen", "Raum", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da stellt dich Morpheus mir", "tokens": ["Da", "stellt", "dich", "Mor\u00b7pheus", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "NE", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nach Herzenswunsche f\u00fcr.", "tokens": ["Nach", "Her\u00b7zens\u00b7wun\u00b7sche", "f\u00fcr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch alle Lust ist hin,", "tokens": ["Doch", "al\u00b7le", "Lust", "ist", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So bald ich munter bin:", "tokens": ["So", "bald", "ich", "mun\u00b7ter", "bin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da seh ich, was mir fehlt,", "tokens": ["Da", "seh", "ich", ",", "was", "mir", "fehlt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und mich auch schlafend qu\u00e4lt;", "tokens": ["Und", "mich", "auch", "schla\u00b7fend", "qu\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Weil mich des Schicksals Macht", "tokens": ["Weil", "mich", "des", "Schick\u00b7sals", "Macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "So weit von dir gebracht.", "tokens": ["So", "weit", "von", "dir", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Verh\u00e4ngni\u00df, \u00e4ndre dich!", "tokens": ["Ver\u00b7h\u00e4ng\u00b7ni\u00df", ",", "\u00e4nd\u00b7re", "dich", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "O Sch\u00f6nste! tr\u00f6ste mich:", "tokens": ["O", "Sch\u00f6ns\u00b7te", "!", "tr\u00f6s\u00b7te", "mich", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn denke nur einmal,", "tokens": ["Denn", "den\u00b7ke", "nur", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Was hilft dir meine Qual?", "tokens": ["Was", "hilft", "dir", "mei\u00b7ne", "Qual", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach gieb hinfort nicht mehr", "tokens": ["Ach", "gieb", "hin\u00b7fort", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "VVIMP", "ADV", "PTKNEG", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der Spr\u00f6digkeit Geh\u00f6r;", "tokens": ["Der", "Spr\u00f6\u00b7dig\u00b7keit", "Ge\u00b7h\u00f6r", ";"], "token_info": ["word", 