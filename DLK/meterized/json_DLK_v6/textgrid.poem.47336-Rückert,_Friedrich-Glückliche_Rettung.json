{"textgrid.poem.47336": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcckliche Rettung", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe fiel ins Gr\u00fcbchen am Kinn", "tokens": ["Die", "Lie\u00b7be", "fiel", "ins", "Gr\u00fcb\u00b7chen", "am", "Kinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und war unendlich erschrocken.", "tokens": ["Und", "war", "un\u00b7end\u00b7lich", "er\u00b7schro\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie langte mit entschlossenem Sinn", "tokens": ["Sie", "lang\u00b7te", "mit", "ent\u00b7schlos\u00b7se\u00b7nem", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Nach einer der flatternden Locken", "tokens": ["Nach", "ei\u00b7ner", "der", "flat\u00b7tern\u00b7den", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und zog sich mit Geschicke", "tokens": ["Und", "zog", "sich", "mit", "Ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Heraus am artigen Stricke,", "tokens": ["He\u00b7raus", "am", "ar\u00b7ti\u00b7gen", "Stri\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Sonst l\u00e4ge sie, glaub' ich, noch darin.", "tokens": ["Sonst", "l\u00e4\u00b7ge", "sie", ",", "glaub'", "ich", ",", "noch", "da\u00b7rin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "ADV", "PAV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}