{"dta.poem.10034": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fcher Fr\u00fchling.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Knospen Fr\u00fchling war annoch:", "tokens": ["Der", "Knos\u00b7pen", "Fr\u00fch\u00b7ling", "war", "an\u00b7noch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man sah fast sichtbarlich, wie bey dem lauen Wetter,", "tokens": ["Man", "sah", "fast", "sicht\u00b7bar\u00b7lich", ",", "wie", "bey", "dem", "lau\u00b7en", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADJD", "$,", "PWAV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das gr\u00fcne Heer der jungen Bl\u00e4tter", "tokens": ["Das", "gr\u00fc\u00b7ne", "Heer", "der", "jun\u00b7gen", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus ihren r\u00f6thlichen Beh\u00e4ltern kroch.", "tokens": ["Aus", "ih\u00b7ren", "r\u00f6th\u00b7li\u00b7chen", "Be\u00b7h\u00e4l\u00b7tern", "kroch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie hingen erst annoch verwickelt unter sich;", "tokens": ["Sie", "hin\u00b7gen", "erst", "an\u00b7noch", "ver\u00b7wi\u00b7ckelt", "un\u00b7ter", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "VVPP", "APPR", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Entwickelten sich aber nach und nach,", "tokens": ["Ent\u00b7wi\u00b7ckel\u00b7ten", "sich", "a\u00b7ber", "nach", "und", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "KON", "APPR", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.7": {"text": "Und fingen allgemach,", "tokens": ["Und", "fin\u00b7gen", "all\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "An allen Seiten,", "tokens": ["An", "al\u00b7len", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Sich auszudehnen, auszubreiten,", "tokens": ["Sich", "aus\u00b7zu\u00b7deh\u00b7nen", ",", "aus\u00b7zu\u00b7brei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PRF", "VVIZU", "$,", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sich auszuspannen an, und sanft sich zu erh\u00f6hn.", "tokens": ["Sich", "aus\u00b7zu\u00b7span\u00b7nen", "an", ",", "und", "sanft", "sich", "zu", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "PTKVZ", "$,", "KON", "ADJD", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der allerd\u00fcnnste Tafft, ist nicht so sanft, so sch\u00f6n,", "tokens": ["Der", "al\u00b7ler\u00b7d\u00fcnns\u00b7te", "Tafft", ",", "ist", "nicht", "so", "sanft", ",", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VAFIN", "PTKNEG", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So klar, so glatt, so gl\u00e4ntzend, zart und fein,", "tokens": ["So", "klar", ",", "so", "glatt", ",", "so", "gl\u00e4nt\u00b7zend", ",", "zart", "und", "fein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als neu-gebohrne Bl\u00e4tter seyn.", "tokens": ["Als", "neu\u00b7ge\u00b7bohr\u00b7ne", "Bl\u00e4t\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Aederchen sind selbst durchsichtig, noch vielmehr", "tokens": ["Die", "A\u00b7e\u00b7der\u00b7chen", "sind", "selbst", "durch\u00b7sich\u00b7tig", ",", "noch", "viel\u00b7mehr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADV"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Das noch viel z\u00e4rtere Gespinnst. Das Sonnen-Licht,", "tokens": ["Das", "noch", "viel", "z\u00e4r\u00b7te\u00b7re", "Ge\u00b7spinnst", ".", "Das", "Son\u00b7nen\u00b7Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "ADV", "PIAT", "ADJA", "NN", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So ungehemmt fast, durch sie bricht,", "tokens": ["So", "un\u00b7ge\u00b7hemmt", "fast", ",", "durch", "sie", "bricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "$,", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und durch ihr zart Gewebe strahlet;", "tokens": ["Und", "durch", "ihr", "zart", "Ge\u00b7we\u00b7be", "strah\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wird, recht als fiel es durch ein gr\u00fcnes Glas,", "tokens": ["Wird", ",", "recht", "als", "fiel", "es", "durch", "ein", "gr\u00fc\u00b7nes", "Glas", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ADJD", "KOKOM", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Auch gr\u00fcn gemahlet.", "tokens": ["Auch", "gr\u00fcn", "ge\u00b7mah\u00b7let", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Hiedurch entstehen klare Schatten,", "tokens": ["Hie\u00b7durch", "ent\u00b7ste\u00b7hen", "kla\u00b7re", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die Wald und Garten, Lufft und Matten", "tokens": ["Die", "Wald", "und", "Gar\u00b7ten", ",", "Lufft", "und", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Fast unaussprechlich lieblich f\u00fcllen,", "tokens": ["Fast", "un\u00b7aus\u00b7sprech\u00b7lich", "lieb\u00b7lich", "f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Sie zeigen manchen Schmuck, auch wann sie ihn verh\u00fcllen.", "tokens": ["Sie", "zei\u00b7gen", "man\u00b7chen", "Schmuck", ",", "auch", "wann", "sie", "ihn", "ver\u00b7h\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "ADV", "PWAV", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "In diesem Spiel unschuldger Lieblichkeit,", "tokens": ["In", "die\u00b7sem", "Spiel", "un\u00b7schuld\u00b7ger", "Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Womit sich gleichsam noch des Jahres Kindheit schm\u00fcckte,", "tokens": ["Wo\u00b7mit", "sich", "gleich\u00b7sam", "noch", "des", "Jah\u00b7res", "Kind\u00b7heit", "schm\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADJD", "ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie in der g\u00fcldnen Zeit,", "tokens": ["Wie", "in", "der", "g\u00fcld\u00b7nen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "War, was man nur erblickte,", "tokens": ["War", ",", "was", "man", "nur", "er\u00b7blick\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PRELS", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Unsch\u00e4dlich, Anmuth-voll, Lust- und Vergn\u00fcgen-reich.", "tokens": ["Un\u00b7sch\u00e4d\u00b7lich", ",", "An\u00b7muth\u00b7voll", ",", "Lust", "und", "Ver\u00b7gn\u00fc\u00b7gen\u00b7reich", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "$,", "TRUNC", "KON", "NN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}, "stanza.4": {"line.1": {"text": "Die Spinne, deren Leib fast einer offnen Klauen", "tokens": ["Die", "Spin\u00b7ne", ",", "de\u00b7ren", "Leib", "fast", "ei\u00b7ner", "off\u00b7nen", "Klau\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und B\u00e4ren-Tatzen gleich,", "tokens": ["Und", "B\u00e4\u00b7ren\u00b7Tat\u00b7zen", "gleich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erweckt uns noch kein eckelhaftes Grauen,", "tokens": ["Er\u00b7weckt", "uns", "noch", "kein", "ec\u00b7kel\u00b7haf\u00b7tes", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kein Ohr-Wurm schreckt uns noch.", "tokens": ["Kein", "Ohr\u00b7Wurm", "schreckt", "uns", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Bienen, welche zwar nie sonder Stachel fliegen,", "tokens": ["Die", "Bie\u00b7nen", ",", "wel\u00b7che", "zwar", "nie", "son\u00b7der", "Sta\u00b7chel", "flie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erregen durch ihr sumsendes Geth\u00f6n,", "tokens": ["Er\u00b7re\u00b7gen", "durch", "ihr", "sum\u00b7sen\u00b7des", "Ge\u00b7th\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit welchem wir sie schw\u00e4rmen sehn,", "tokens": ["Mit", "wel\u00b7chem", "wir", "sie", "schw\u00e4r\u00b7men", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sie auf Bluhmen sich, blos uns zum Nutzen, setzen,", "tokens": ["Wenn", "sie", "auf", "Bluh\u00b7men", "sich", ",", "blos", "uns", "zum", "Nut\u00b7zen", ",", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PRF", "$,", "ADV", "PPER", "APPRART", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "Mit Recht uns keine Furcht, nur Anmuth und Ergetzen,", "tokens": ["Mit", "Recht", "uns", "kei\u00b7ne", "Furcht", ",", "nur", "An\u00b7muth", "und", "Er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "PIAT", "NN", "$,", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und, durch den muntern Flei\u00df, ein inniges Vergn\u00fcgen.", "tokens": ["Und", ",", "durch", "den", "mun\u00b7tern", "Flei\u00df", ",", "ein", "in\u00b7ni\u00b7ges", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der Blut-begiergen M\u00fccke", "tokens": ["Der", "Blut\u00b7be\u00b7gier\u00b7gen", "M\u00fc\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verdrie\u00dflicher Gesang, st\u00f6hrt unsre Ruh noch nicht.", "tokens": ["Ver\u00b7drie\u00df\u00b7li\u00b7cher", "Ge\u00b7sang", ",", "st\u00f6hrt", "uns\u00b7re", "Ruh", "noch", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man schl\u00e4gt, geneckt durch ihre dreiste T\u00fccke,", "tokens": ["Man", "schl\u00e4gt", ",", "ge\u00b7neckt", "durch", "ih\u00b7re", "dreis\u00b7te", "T\u00fc\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Nachts, mit Recht erz\u00fcrnt, sich noch nicht ins Gesicht,", "tokens": ["Des", "Nachts", ",", "mit", "Recht", "er\u00b7z\u00fcrnt", ",", "sich", "noch", "nicht", "ins", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "APPR", "NN", "VVPP", "$,", "PRF", "ADV", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Voll Schlaf und Ungeduld; die unversch\u00e4mten Fliegen", "tokens": ["Voll", "Schlaf", "und", "Un\u00b7ge\u00b7duld", ";", "die", "un\u00b7ver\u00b7sch\u00e4m\u00b7ten", "Flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "NN", "KON", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Best\u00fcrmen unsern Tisch noch nicht so Hauffen-weise,", "tokens": ["Be\u00b7st\u00fcr\u00b7men", "un\u00b7sern", "Tisch", "noch", "nicht", "so", "Hauf\u00b7fen\u00b7wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "PTKNEG", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie wol im Herbst geschicht.", "tokens": ["Wie", "wol", "im", "Herbst", "ge\u00b7schicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Kein sch\u00e4dlich gifftig Thier", "tokens": ["Kein", "sch\u00e4d\u00b7lich", "giff\u00b7tig", "Thier"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJD", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Thut sich annoch herf\u00fcr.", "tokens": ["Thut", "sich", "an\u00b7noch", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Und kurtz: in allen dem, was man erblickt\u2019, empfand", "tokens": ["Und", "kurtz", ":", "in", "al\u00b7len", "dem", ",", "was", "man", "er\u00b7blickt'", ",", "emp\u00b7fand"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADJD", "$.", "APPR", "PIAT", "PDS", "$,", "PRELS", "PIS", "VVFIN", "$,", "VVFIN"], "me