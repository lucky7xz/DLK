{"dta.poem.6510": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Licht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von aller Himmels-Lufft ist ihre Gluht umringt,", "tokens": ["Von", "al\u00b7ler", "Him\u00b7mels\u00b7Lufft", "ist", "ih\u00b7re", "Gluht", "um\u00b7ringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die sie umgrentzt und ihr sich stets entgegen leget,", "tokens": ["Die", "sie", "um\u00b7grentzt", "und", "ihr", "sich", "stets", "ent\u00b7ge\u00b7gen", "le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "KON", "PPER", "PRF", "ADV", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df sie nicht aus einander dringt.", "tokens": ["Da\u00df", "sie", "nicht", "aus", "ein\u00b7an\u00b7der", "dringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jhr w\u00fcrckend Feur, da es gedrenget wird, erreget", "tokens": ["Ihr", "w\u00fcr\u00b7ckend", "Feur", ",", "da", "es", "ge\u00b7dren\u00b7get", "wird", ",", "er\u00b7re\u00b7get"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "NN", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein Zittern in der Lufft und in des Himmels Feld,", "tokens": ["Ein", "Zit\u00b7tern", "in", "der", "Lufft", "und", "in", "des", "Him\u00b7mels", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wodurch denn alles sich in einer R\u00fcnd\u2019 erhellt.", "tokens": ["Wo\u00b7durch", "denn", "al\u00b7les", "sich", "in", "ei\u00b7ner", "R\u00fcnd'", "er\u00b7hellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIS", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Vom andern Element, die K\u00fcgelchen gedruckt,", "tokens": ["Vom", "an\u00b7dern", "E\u00b7le\u00b7ment", ",", "die", "K\u00fc\u00b7gel\u00b7chen", "ge\u00b7druckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und ausgedehnt, und von sich weggeschickt,", "tokens": ["Und", "aus\u00b7ge\u00b7dehnt", ",", "und", "von", "sich", "weg\u00b7ge\u00b7schickt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "$,", "KON", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die letzten \u00f6ffnen sich in unsre Augen-Th\u00fcren,", "tokens": ["Die", "letz\u00b7ten", "\u00f6ff\u00b7nen", "sich", "in", "uns\u00b7re", "Au\u00b7gen\u00b7T\u00b7h\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und wie es blos in einem Druck besteht,", "tokens": ["Und", "wie", "es", "blos", "in", "ei\u00b7nem", "Druck", "be\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Der vom gespanneten zu der Bewegung geht,", "tokens": ["Der", "vom", "ge\u00b7span\u00b7ne\u00b7ten", "zu", "der", "Be\u00b7we\u00b7gung", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So ohne Zeit-Verlust geschwinde zu versp\u00fcren;", "tokens": ["So", "oh\u00b7ne", "Zeit\u00b7Ver\u00b7lust", "ge\u00b7schwin\u00b7de", "zu", "ver\u00b7sp\u00fc\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Daher erleuchtet uns der Sonnen Glantz so schnell,", "tokens": ["Da\u00b7her", "er\u00b7leuch\u00b7tet", "uns", "der", "Son\u00b7nen", "Glantz", "so", "schnell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und wird im Augenblick der Creis der Erden hell.", "tokens": ["Und", "wird", "im", "Au\u00b7gen\u00b7blick", "der", "Creis", "der", "Er\u00b7den", "hell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}