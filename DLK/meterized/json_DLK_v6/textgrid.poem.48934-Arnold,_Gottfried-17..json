{"textgrid.poem.48934": {"metadata": {"author": {"name": "Arnold, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1690, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn nur mein Hertze brennend ist/", "tokens": ["Wenn", "nur", "mein", "Hert\u00b7ze", "bren\u00b7nend", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "VVFIN", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da ich das Wort des Lebens fasse/", "tokens": ["Da", "ich", "das", "Wort", "des", "Le\u00b7bens", "fas\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So wei\u00df ich/ da\u00df dus selber bist/", "tokens": ["So", "wei\u00df", "ich", "/", "da\u00df", "dus", "sel\u00b7ber", "bist", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PIS", "ADV", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Vatters Wort/ das ich nicht lasse.", "tokens": ["Des", "Vat\u00b7ters", "Wort", "/", "das", "ich", "nicht", "las\u00b7se", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "PRELS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Flamme deiner Liebe macht/", "tokens": ["Die", "Flam\u00b7me", "dei\u00b7ner", "Lie\u00b7be", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich mich immer h\u00f6her schwinge:", "tokens": ["Da\u00df", "ich", "mich", "im\u00b7mer", "h\u00f6\u00b7her", "schwin\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und wenn ich deine Treu betracht/", "tokens": ["Und", "wenn", "ich", "dei\u00b7ne", "Treu", "be\u00b7tracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Liebes-Feur zum Opffer bringe.", "tokens": ["Ein", "Lie\u00b7bes\u00b7Feur", "zum", "Opf\u00b7fer", "brin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die Flamm ist ja von dir entz\u00fcndt/", "tokens": ["Die", "Flamm", "ist", "ja", "von", "dir", "ent\u00b7z\u00fcndt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Drum eilt sie zu dem Ursprung wieder/", "tokens": ["Drum", "eilt", "sie", "zu", "dem", "Ur\u00b7sprung", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und wenn sie unterwegs dich findt/", "tokens": ["Und", "wenn", "sie", "un\u00b7ter\u00b7wegs", "dich", "findt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "So kehrt sie nie zur Erden nieder.", "tokens": ["So", "kehrt", "sie", "nie", "zur", "Er\u00b7den", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "O la\u00df mich stille stehn und h\u00f6ren/", "tokens": ["O", "la\u00df", "mich", "stil\u00b7le", "stehn", "und", "h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVFIN", "VVINF", "KON", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Wenn du willt in mein Hertz einkehren.", "tokens": ["Wenn", "du", "willt", "in", "mein", "Hertz", "ein\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "Du ziehst allein bey denen ein/", "tokens": ["Du", "ziehst", "al\u00b7lein", "bey", "de\u00b7nen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PRELS", "ART", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.16": {"text": "Die dir im Grund gelassen seyn.", "tokens": ["Die", "dir", "im", "Grund", "ge\u00b7las\u00b7sen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Wie gerne w\u00e4r ich doch in dieser Flamme rein!", "tokens": ["Wie", "ger\u00b7ne", "w\u00e4r", "ich", "doch", "in", "die\u00b7ser", "Flam\u00b7me", "rein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}