{"textgrid.poem.63783": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Siena", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah dich hellgeschm\u00fcckt vom jungen Lenz,", "tokens": ["Ich", "sah", "dich", "hell\u00b7ge\u00b7schm\u00fcckt", "vom", "jun\u00b7gen", "Lenz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du h\u00f6chstget\u00fcrmte von Toskanas St\u00e4dten", "tokens": ["Du", "h\u00f6chst\u00b7ge\u00b7t\u00fcrm\u00b7te", "von", "Tos\u00b7ka\u00b7nas", "St\u00e4d\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und Bl\u00fctenbanner friedenvoll umwehten", "tokens": ["Und", "Bl\u00fc\u00b7ten\u00b7ban\u00b7ner", "frie\u00b7den\u00b7voll", "um\u00b7weh\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die einst'ge Nebenbuhlin von Florenz.", "tokens": ["Die", "einst'\u00b7ge", "Ne\u00b7ben\u00b7buh\u00b7lin", "von", "Flo\u00b7renz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Dein Ruhmesanrecht \u2013 nur der Forscher kennt's.", "tokens": ["Dein", "Ruh\u00b7me\u00b7san\u00b7recht", "\u2013", "nur", "der", "For\u00b7scher", "kennt'", "s."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["PPOSAT", "NN", "$(", "ADV", "ART", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Wettstreit ruht; du bist zur\u00fcckgetreten.", "tokens": ["Der", "Wett\u00b7streit", "ruht", ";", "du", "bist", "zu\u00b7r\u00fcck\u00b7ge\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch Aug und Herz der K\u00fcnstler und Poeten", "tokens": ["Doch", "Aug", "und", "Herz", "der", "K\u00fcnst\u00b7ler", "und", "Po\u00b7et\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bestreiten der Jahrhunderte Sentenz.", "tokens": ["Be\u00b7strei\u00b7ten", "der", "Jahr\u00b7hun\u00b7der\u00b7te", "Sen\u00b7tenz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "-+--++---+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Hier folg' ich gerne jener Heil'gen Spuren,", "tokens": ["Hier", "fol\u00b7g'", "ich", "ger\u00b7ne", "je\u00b7ner", "Heil'\u00b7gen", "Spu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PDAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die r\u00fchrend edel Welt und Himmel ma\u00df", "tokens": ["Die", "r\u00fch\u00b7rend", "e\u00b7del", "Welt", "und", "Him\u00b7mel", "ma\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "KON", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit reinstem Blick begnadeter Naturen.", "tokens": ["Mit", "reins\u00b7tem", "Blick", "be\u00b7gna\u00b7de\u00b7ter", "Na\u00b7tu\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wer, der jemals sie geschaut, verga\u00df", "tokens": ["Und", "wer", ",", "der", "je\u00b7mals", "sie", "ge\u00b7schaut", ",", "ver\u00b7ga\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PWS", "$,", "PRELS", "ADV", "PPER", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die andern Wunderwerke dieser Fluren,", "tokens": ["Die", "an\u00b7dern", "Wun\u00b7der\u00b7wer\u00b7ke", "die\u00b7ser", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die wonnigen Gestalten Sodomas!", "tokens": ["Die", "won\u00b7ni\u00b7gen", "Ge\u00b7stal\u00b7ten", "So\u00b7do\u00b7mas", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}