{"textgrid.poem.48034": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Dornen", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Fr\u00fchling meines Lebens", "tokens": ["Der", "Fr\u00fch\u00b7ling", "mei\u00b7nes", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "bl\u00fchte lang schon ab \u2013", "tokens": ["bl\u00fch\u00b7te", "lang", "schon", "ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "PTKVZ", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "nun trug ich auch den leuchtenden", "tokens": ["nun", "trug", "ich", "auch", "den", "leuch\u00b7ten\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sommer zu Grab . . . .", "tokens": ["Som\u00b7mer", "zu", "Grab", ".", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["NN", "APPR", "NN", "$.", "$.", "$.", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Die bl\u00fchende Rosenkrone", "tokens": ["Die", "bl\u00fc\u00b7hen\u00b7de", "Ro\u00b7sen\u00b7kro\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "verlor nun Duft und Glanz . . .", "tokens": ["ver\u00b7lor", "nun", "Duft", "und", "Glanz", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "ADV", "NN", "KON", "NN", "$.", "$.", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "auf meine blutende Stirne dr\u00fcckt", "tokens": ["auf", "mei\u00b7ne", "blu\u00b7ten\u00b7de", "Stir\u00b7ne", "dr\u00fcckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "der nackte Dornenkranz.", "tokens": ["der", "nack\u00b7te", "Dor\u00b7nen\u00b7kranz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}