{"textgrid.poem.50781": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da lieg ich nun, ohnm\u00e4chtiger Geselle,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da lieg ich nun, ohnm\u00e4chtiger Geselle,", "tokens": ["Da", "lieg", "ich", "nun", ",", "ohn\u00b7m\u00e4ch\u00b7ti\u00b7ger", "Ge\u00b7sel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geschieden von der ganzen, weiten Welt!", "tokens": ["Ge\u00b7schie\u00b7den", "von", "der", "gan\u00b7zen", ",", "wei\u00b7ten", "Welt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Versprengter Tropfen von der Lebensquelle,", "tokens": ["Ver\u00b7spreng\u00b7ter", "Trop\u00b7fen", "von", "der", "Le\u00b7bens\u00b7quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Baum, noch gr\u00fcnend, ist er auch gef\u00e4llt!", "tokens": ["Ein", "Baum", ",", "noch", "gr\u00fc\u00b7nend", ",", "ist", "er", "auch", "ge\u00b7f\u00e4llt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "$,", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wohlan! ich will, was kommen soll, erwarten;", "tokens": ["Wo\u00b7hlan", "!", "ich", "will", ",", "was", "kom\u00b7men", "soll", ",", "er\u00b7war\u00b7ten", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VMFIN", "$,", "PWS", "VVINF", "VMFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Still und behaglich ist's im Grabe hier,", "tokens": ["Still", "und", "be\u00b7hag\u00b7lich", "ist's", "im", "Gra\u00b7be", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich f\u00fchle nicht die Glieder, die erstarrten;", "tokens": ["Ich", "f\u00fch\u00b7le", "nicht", "die", "Glie\u00b7der", ",", "die", "er\u00b7starr\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch hell und heiter glimmt die Seele mir.", "tokens": ["Doch", "hell", "und", "hei\u00b7ter", "glimmt", "die", "See\u00b7le", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "H\u00e4tt ich nun einen ewigen Gedanken,", "tokens": ["H\u00e4tt", "ich", "nun", "ei\u00b7nen", "e\u00b7wi\u00b7gen", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An dem man endlos sich erproben mag,", "tokens": ["An", "dem", "man", "end\u00b7los", "sich", "er\u00b7pro\u00b7ben", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "ADJD", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So m\u00f6cht ich liegen in den engen Schranken,", "tokens": ["So", "m\u00f6cht", "ich", "lie\u00b7gen", "in", "den", "en\u00b7gen", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Still und behaglich, bis zum J\u00fcngsten Tag.", "tokens": ["Still", "und", "be\u00b7hag\u00b7lich", ",", "bis", "zum", "J\u00fcng\u00b7sten", "Tag", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "KOUS", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Vielleicht, wer wei\u00df, w\u00fcchs er zu solcher Gr\u00f6\u00dfe,", "tokens": ["Viel\u00b7leicht", ",", "wer", "wei\u00df", ",", "w\u00fcchs", "er", "zu", "sol\u00b7cher", "Gr\u00f6\u00b7\u00dfe", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu solcher St\u00e4rke, da\u00df er, ein Vulkan,", "tokens": ["Zu", "sol\u00b7cher", "St\u00e4r\u00b7ke", ",", "da\u00df", "er", ",", "ein", "Vul\u00b7kan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "KOUS", "PPER", "$,", "ART", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Flammenausbruch dieses Grab erschl\u00f6sse,", "tokens": ["Im", "Flam\u00b7men\u00b7aus\u00b7bruch", "die\u00b7ses", "Grab", "er\u00b7schl\u00f6s\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vorleuchtend mir auf neuer Lebensbahn.", "tokens": ["Vor\u00b7leuch\u00b7tend", "mir", "auf", "neu\u00b7er", "Le\u00b7bens\u00b7bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wie wundersam, wenn \u00fcber meinem Haupte", "tokens": ["Wie", "wun\u00b7der\u00b7sam", ",", "wenn", "\u00fc\u00b7ber", "mei\u00b7nem", "Haup\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Abendtau die matten Blumen k\u00fchlt:", "tokens": ["Der", "A\u00b7bend\u00b7tau", "die", "mat\u00b7ten", "Blu\u00b7men", "k\u00fchlt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ob wohl, lustwandelnd dann, der Pfarrherr glaubte,", "tokens": ["Ob", "wohl", ",", "lust\u00b7wan\u00b7delnd", "dann", ",", "der", "Pfar\u00b7rherr", "glaub\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "ADJD", "ADV", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df unter ihm ein Wetterleuchten spielt?", "tokens": ["Da\u00df", "un\u00b7ter", "ihm", "ein", "Wet\u00b7ter\u00b7leuch\u00b7ten", "spielt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Da\u00df, gl\u00e4nzend in des eignen Geistes Strahlen,", "tokens": ["Da\u00df", ",", "gl\u00e4n\u00b7zend", "in", "des", "eig\u00b7nen", "Geis\u00b7tes", "Strah\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADJD", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hier unten eine Menschenseele denkt?", "tokens": ["Hier", "un\u00b7ten", "ei\u00b7ne", "Men\u00b7schen\u00b7see\u00b7le", "denkt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vielleicht sind dieses der Verdammung Qualen:", "tokens": ["Viel\u00b7leicht", "sind", "die\u00b7ses", "der", "Ver\u00b7dam\u00b7mung", "Qua\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDAT", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Heimlich zu leuchten, ewiglich versenkt.", "tokens": ["Heim\u00b7lich", "zu", "leuch\u00b7ten", ",", "e\u00b7wig\u00b7lich", "ver\u00b7senkt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$,", "ADJD", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}