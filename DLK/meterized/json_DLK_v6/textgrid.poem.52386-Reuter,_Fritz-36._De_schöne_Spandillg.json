{"textgrid.poem.52386": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "36. De sch\u00f6ne Spandillg", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.71", "sv:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Majur von Vo\u00df tau Grabowh\u00e4w'", "tokens": ["Ma\u00b7jur", "von", "Vo\u00df", "tau", "Gra\u00b7bowh\u00e4w'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NE", "NE", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hadd mal 'n Holl\u00e4nner bi sin K\u00e4uh,", "tokens": ["Hadd", "mal", "'n", "Hol\u00b7l\u00e4n\u00b7ner", "bi", "sin", "K\u00e4uh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "De sihr dat Solospill bedrew.", "tokens": ["De", "sihr", "dat", "So\u00b7lo\u00b7spill", "be\u00b7drew", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "De Mann was s\u00fcs nich tau verachten", "tokens": ["De", "Mann", "was", "s\u00fcs", "nich", "tau", "ver\u00b7ach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "PTKNEG", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Un was ok idel gaud bi't Veih;", "tokens": ["Un", "was", "ok", "i\u00b7del", "gaud", "bi't", "Veih", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Doch as hei wull von Frischen pachten", "tokens": ["Doch", "as", "hei", "wull", "von", "Fri\u00b7schen", "pach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "FM", "FM", "VMFIN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Un dor\u00fcm kamm taum Herrn Majuren,", "tokens": ["Un", "do\u00b7r\u00fcm", "kamm", "taum", "Herrn", "Ma\u00b7ju\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "ADV", "NN", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Dunn seggt de Herr Majur: \u00bbBollhagen\u00ab,", "tokens": ["Dunn", "seggt", "de", "Herr", "Ma\u00b7jur", ":", "\u00bb", "Boll\u00b7ha\u00b7gen", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$(", "NN", "$(", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Seggt hei, \u00bbde groten Fragen,", "tokens": ["Seggt", "hei", ",", "\u00bb", "de", "gro\u00b7ten", "Fra\u00b7gen", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "$(", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "De Solos un de Mataduren,", "tokens": ["De", "So\u00b7los", "un", "de", "Ma\u00b7ta\u00b7du\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "De hewwen m\u00e4nnigeinen all", "tokens": ["De", "hew\u00b7wen", "m\u00e4n\u00b7ni\u00b7gei\u00b7nen", "all"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "ADJA", "PIAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Bet up dat blanke Hemd uttagen.", "tokens": ["Bet", "up", "dat", "blan\u00b7ke", "Hemd", "ut\u00b7ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Wenn ick Sei wedder nemen sall,", "tokens": ["Wenn", "ick", "Sei", "wed\u00b7der", "ne\u00b7men", "sall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Denn m\u00f6ten S' mi dat fast verspreken,", "tokens": ["Denn", "m\u00f6\u00b7ten", "S'", "mi", "dat", "fast", "ver\u00b7spre\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NE", "NE", "ART", "ADV", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Dat S' mit dat Solospill afbreken.\u00ab", "tokens": ["Dat", "S'", "mit", "dat", "So\u00b7lo\u00b7spill", "af\u00b7bre\u00b7ken", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "VMFIN", "VVINF", "$.", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Bollhagen, de versprekt ok fast;", "tokens": ["Boll\u00b7ha\u00b7gen", ",", "de", "ver\u00b7sprekt", "ok", "fast", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.17": {"text": "Doch knapp nah drei, vier Wochen was't,", "tokens": ["Doch", "knapp", "nah", "drei", ",", "vier", "Wo\u00b7chen", "was't", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "CARD", "$,", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Dunn gung dat wedder: \u00bbSolo kl\u00fcr,", "tokens": ["Dunn", "gung", "dat", "wed\u00b7der", ":", "\u00bb", "So\u00b7lo", "kl\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "$.", "$(", "FM.la", "FM.la", "$,"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.19": {"text": "Drei Matadur un denn de Irsten!\u00ab", "tokens": ["Drei", "Ma\u00b7ta\u00b7dur", "un", "denn", "de", "Irs\u00b7ten", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["CARD", "NN", "FM", "KON", "NE", "NE", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Dat argert den Majuren sihr,", "tokens": ["Dat", "ar\u00b7gert", "den", "Ma\u00b7ju\u00b7ren", "sihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch wat em argert noch am mihrsten,", "tokens": ["Doch", "wat", "em", "ar\u00b7gert", "noch", "am", "mihrs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "FM", "FM", "ADV", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dat was, dat hei tau weiten kreg,", "tokens": ["Dat", "was", ",", "dat", "hei", "tau", "wei\u00b7ten", "kreg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "$,", "FM.nl", "FM.nl", "FM.nl", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dat sick sin leiw Bollhagen \u00fcmmer", "tokens": ["Dat", "sick", "sin", "leiw", "Boll\u00b7ha\u00b7gen", "\u00fcm\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Mit Korten in de Tasch r\u00fcm dr\u00f6g.", "tokens": ["Mit", "Kor\u00b7ten", "in", "de", "Tasch", "r\u00fcm", "dr\u00f6g", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NE", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbne\u00ab, seggt de Herr Majur tau sick,", "tokens": ["\u00bb", "ne", "\u00ab", ",", "seggt", "de", "Herr", "Ma\u00b7jur", "tau", "sick", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$(", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbbollhagen, ne, so geiht dat nich!", "tokens": ["\u00bb", "boll\u00b7ha\u00b7gen", ",", "ne", ",", "so", "geiht", "dat", "nich", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NE", "$,", "ADV", "VVFIN", "ART", "PTKNEG", "$."], "meter": "+----+-+", "measure": "dactylic.init"}, "line.8": {"text": "Na, \u00e4werst marken sall hei't doch,", "tokens": ["Na", ",", "\u00e4\u00b7werst", "mar\u00b7ken", "sall", "hei't", "doch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dat ick em up de Spr\u00fcngen b\u00fcn\u00ab,", "tokens": ["Dat", "ick", "em", "up", "de", "Spr\u00fcn\u00b7gen", "b\u00fcn", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Un geiht dorbi in't Veihhus rin,", "tokens": ["Un", "geiht", "dor\u00b7bi", "in't", "Veih\u00b7hus", "rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.11": {"text": "Kickt up de Del, kickt in den Trog", "tokens": ["Kickt", "up", "de", "Del", ",", "kickt", "in", "den", "Trog"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "NE", "NE", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Un fr\u00f6ggt nah dit und fr\u00f6ggt nah dat,", "tokens": ["Un", "fr\u00f6ggt", "nah", "dit", "und", "fr\u00f6ggt", "nah", "dat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "ADJD", "KON", "VVFIN", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Ob't Veih dat Fauder ok woll fratt,", "tokens": ["Ob't", "Veih", "dat", "Fau\u00b7der", "ok", "woll", "fratt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "NE", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Un fr\u00f6ggt den oll'n Bollhagen, wenn'e", "tokens": ["Un", "fr\u00f6ggt", "den", "oll'n", "Boll\u00b7ha\u00b7gen", ",", "wenn'e"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,", "KOUS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Mit lang Heu an tau faudern f\u00fcng,", "tokens": ["Mit", "lang", "Heu", "an", "tau", "fau\u00b7dern", "f\u00fcng", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "NN", "APPR", "NE", "VVINF", "VVFIN", "$,"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Un k\u00fcmmt so nah den Middelst\u00e4nner,", "tokens": ["Un", "k\u00fcmmt", "so", "nah", "den", "Mid\u00b7del\u00b7st\u00e4n\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Wo Haud un Rock von den Holl\u00e4nner,", "tokens": ["Wo", "Haud", "un", "Rock", "von", "den", "Hol\u00b7l\u00e4n\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "FM", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "As \u00fcmmer, an den Nagel h\u00fcng,", "tokens": ["As", "\u00fcm\u00b7mer", ",", "an", "den", "Na\u00b7gel", "h\u00fcng", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "APPR", "ART", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Un grawwelt lis' de Slipp hendal,", "tokens": ["Un", "graw\u00b7welt", "lis'", "de", "Slipp", "hen\u00b7dal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "NE", "NE", "NE", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.20": {"text": "Ahn dat de Melkers dat gewohrten.", "tokens": ["Ahn", "dat", "de", "Mel\u00b7kers", "dat", "ge\u00b7wohr\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NE", "NE", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "\u00bbwohrhaftig ja, dor s\u00fcnd de Korten!\u00ab", "tokens": ["\u00bb", "wohr\u00b7haf\u00b7tig", "ja", ",", "dor", "s\u00fcnd", "de", "Kor\u00b7ten", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "ADV", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Un halt s' herut. \u00bbNa, t\u00e4uw nu mal!\u00ab", "tokens": ["Un", "halt", "s'", "he\u00b7rut", ".", "\u00bb", "Na", ",", "t\u00e4uw", "nu", "mal", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "NE", "VVPP", "$.", "$(", "ITJ", "$,", "VVFIN", "ADV", "ADV", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.23": {"text": "Un geiht ganz sachten ut de D\u00f6r", "tokens": ["Un", "geiht", "ganz", "sach\u00b7ten", "ut", "de", "D\u00f6r"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "VVFIN", "APPR", "NE", "NE"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.24": {"text": "Den Weg entlang de Gorenheck", "tokens": ["Den", "Weg", "ent\u00b7lang", "de", "Go\u00b7ren\u00b7heck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPO", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Un streu't de Korten hen un her", "tokens": ["Un", "streu't", "de", "Kor\u00b7ten", "hen", "un", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "APZR"], "meter": "+--+---+", "measure": "dactylic.di.plus"}, "line.26": {"text": "Recht in den Veihhus-Winterdreck", "tokens": ["Recht", "in", "den", "Veih\u00b7hus\u00b7Win\u00b7ter\u00b7dreck"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Un stellt sick in dat Wagenschur", "tokens": ["Un", "stellt", "sick", "in", "dat", "Wa\u00b7gen\u00b7schur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure"