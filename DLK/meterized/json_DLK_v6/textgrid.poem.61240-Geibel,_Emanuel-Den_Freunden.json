{"textgrid.poem.61240": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Den Freunden", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Endlich hatt' ich mich beschieden,", "tokens": ["End\u00b7lich", "hatt'", "ich", "mich", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lebte sonder Wunsch und Kummer,", "tokens": ["Leb\u00b7te", "son\u00b7der", "Wunsch", "und", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der lang entbehrte Frieden", "tokens": ["Und", "der", "lang", "ent\u00b7behr\u00b7te", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kehrte schon in diese Brust;", "tokens": ["Kehr\u00b7te", "schon", "in", "die\u00b7se", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, da weckt ihr das Verlangen,", "tokens": ["Ach", ",", "da", "weckt", "ihr", "das", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weckt die Hoffnung aus dem Schlummer;", "tokens": ["Weckt", "die", "Hoff\u00b7nung", "aus", "dem", "Schlum\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wieder zweifeln, f\u00fcrchten, bangen", "tokens": ["Wie\u00b7der", "zwei\u00b7feln", ",", "f\u00fcrch\u00b7ten", ",", "ban\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "VVINF", "$,", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mu\u00df ich unter Qual und Lust.", "tokens": ["Mu\u00df", "ich", "un\u00b7ter", "Qual", "und", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Soll ich z\u00fcrnen, soll ich danken?", "tokens": ["Soll", "ich", "z\u00fcr\u00b7nen", ",", "soll", "ich", "dan\u00b7ken", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus des Hafens sichern Schranken", "tokens": ["Aus", "des", "Ha\u00b7fens", "si\u00b7chern", "Schran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Treibt ihr mich aufs Meer zur\u00fcck.", "tokens": ["Treibt", "ihr", "mich", "aufs", "Meer", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Manches wohl erringt der Wille,", "tokens": ["Man\u00b7ches", "wohl", "er\u00b7ringt", "der", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die stolzen Segel schwanken -", "tokens": ["Wo", "die", "stol\u00b7zen", "Se\u00b7gel", "schwan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber jene tiefe Stille,", "tokens": ["A\u00b7ber", "je\u00b7ne", "tie\u00b7fe", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freunde, war doch auch ein Gl\u00fcck.", "tokens": ["Freun\u00b7de", ",", "war", "doch", "auch", "ein", "Gl\u00fcck", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}