{"textgrid.poem.59979": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Kein Preu\u00dfen und kein \u00d6sterreich!", "genre": "verse", "period": "N.A.", "pub_year": 1848, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Preu\u00dfen und kein \u00d6sterreich!", "tokens": ["Kein", "Preu\u00b7\u00dfen", "und", "kein", "\u00d6s\u00b7ter\u00b7reich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Deutschland! wie vermessen!", "tokens": ["Ein", "Deutschland", "!", "wie", "ver\u00b7mes\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PWAV", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der Jungfer wurd das Herz so weich,", "tokens": ["Der", "Jung\u00b7fer", "wurd", "das", "Herz", "so", "weich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie freut sich wie besessen;", "tokens": ["Sie", "freut", "sich", "wie", "be\u00b7ses\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "KOKOM", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein Prinz hat ihr den Hof gemacht", "tokens": ["Ein", "Prinz", "hat", "ihr", "den", "Hof", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und beim Dessert an sie gedacht.", "tokens": ["Und", "beim", "Des\u00b7sert", "an", "sie", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Steh auf, Germania,", "tokens": ["Steh", "auf", ",", "Ger\u00b7ma\u00b7nia", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "NE", "$,"], "meter": "-+---", "measure": "dactylic.init"}, "line.8": {"text": "Dein Br\u00e4utigam ist da!", "tokens": ["Dein", "Br\u00e4u\u00b7ti\u00b7gam", "ist", "da", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "VAFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Kein Preu\u00dfen und kein \u00d6sterreich!", "tokens": ["Kein", "Preu\u00b7\u00dfen", "und", "kein", "\u00d6s\u00b7ter\u00b7reich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und \u00d6sterreich soll thronen?", "tokens": ["Und", "\u00d6s\u00b7ter\u00b7reich", "soll", "thro\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er ist ein Mann \u2013 wir sind ihm gleich,", "tokens": ["Er", "ist", "ein", "Mann", "\u2013", "wir", "sind", "ihm", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "PPER", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wir \u2013 sind Millionen.", "tokens": ["Und", "wir", "\u2013", "sind", "Mil\u00b7lion\u00b7en", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "VAFIN", "NN", "$."], "meter": "--+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Und Millionen schw\u00f6ren hoch", "tokens": ["Und", "Mil\u00b7lion\u00b7en", "schw\u00f6\u00b7ren", "hoch"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "ADJD"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Und rufen laut: Kein neues Joch", "tokens": ["Und", "ru\u00b7fen", "laut", ":", "Kein", "neu\u00b7es", "Joch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "$.", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und keine F\u00fcrsten mehr!", "tokens": ["Und", "kei\u00b7ne", "F\u00fcrs\u00b7ten", "mehr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Dem Volk allein die Ehr!", "tokens": ["Dem", "Volk", "al\u00b7lein", "die", "Ehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Kein Preu\u00dfen und kein \u00d6sterreich!", "tokens": ["Kein", "Preu\u00b7\u00dfen", "und", "kein", "\u00d6s\u00b7ter\u00b7reich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was helfen uns die beiden?", "tokens": ["Was", "hel\u00b7fen", "uns", "die", "bei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "PIAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das eine ist schon totenbleich,", "tokens": ["Das", "ei\u00b7ne", "ist", "schon", "to\u00b7ten\u00b7bleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das andre am Verscheiden.", "tokens": ["Das", "and\u00b7re", "am", "Ver\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wir brauchen solche Sonnen nicht", "tokens": ["Wir", "brau\u00b7chen", "sol\u00b7che", "Son\u00b7nen", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und folgen unserm eignen Licht,", "tokens": ["Und", "fol\u00b7gen", "un\u00b7serm", "eig\u00b7nen", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In unsrer Brust dem Stern;", "tokens": ["In", "uns\u00b7rer", "Brust", "dem", "Stern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wir wollen keinen Herrn.", "tokens": ["Wir", "wol\u00b7len", "kei\u00b7nen", "Herrn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Kein Preu\u00dfen und kein \u00d6sterreich!", "tokens": ["Kein", "Preu\u00b7\u00dfen", "und", "kein", "\u00d6s\u00b7ter\u00b7reich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und tr\u00e4nk er ganze B\u00e4che", "tokens": ["Und", "tr\u00e4nk", "er", "gan\u00b7ze", "B\u00e4\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf unser Wohl \u2013 o Schelmenstreich!", "tokens": ["Auf", "un\u00b7ser", "Wohl", "\u2013", "o", "Schel\u00b7men\u00b7streich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Volk bezahlt die Zeche.", "tokens": ["Das", "Volk", "be\u00b7zahlt", "die", "Ze\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und F\u00fcrstenwein ist teurer Wein,", "tokens": ["Und", "F\u00fcrs\u00b7ten\u00b7wein", "ist", "teu\u00b7rer", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drum schenkt uns einen andern ein:", "tokens": ["Drum", "schenkt", "uns", "ei\u00b7nen", "an\u00b7dern", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gut Wind und gut Geschick", "tokens": ["Gut", "Wind", "und", "gut", "Ge\u00b7schick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "KON", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Der deutschen Republik!", "tokens": ["Der", "deut\u00b7schen", "Re\u00b7pub\u00b7lik", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Kein Preu\u00dfen und kein \u00d6sterreich!", "tokens": ["Kein", "Preu\u00b7\u00dfen", "und", "kein", "\u00d6s\u00b7ter\u00b7reich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und geht's uns schief, so wolln wir gleich", "tokens": ["Und", "geht's", "uns", "schief", ",", "so", "wolln", "wir", "gleich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "$,", "ADV", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch Thurn und Taxis schreiben.", "tokens": ["Durch", "Thurn", "und", "Ta\u00b7xis", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Indes, Herr Johann ohne Land,", "tokens": ["In\u00b7des", ",", "Herr", "Jo\u00b7hann", "oh\u00b7ne", "Land", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "NE", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verzeiht der Deutschen Unverstand", "tokens": ["Ver\u00b7zeiht", "der", "Deut\u00b7schen", "Un\u00b7ver\u00b7stand"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und denkt beim n\u00e4chsten Glas:", "tokens": ["Und", "denkt", "beim", "n\u00e4chs\u00b7ten", "Glas", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In vino veritas!", "tokens": ["In", "vi\u00b7no", "ve\u00b7ri\u00b7tas", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}