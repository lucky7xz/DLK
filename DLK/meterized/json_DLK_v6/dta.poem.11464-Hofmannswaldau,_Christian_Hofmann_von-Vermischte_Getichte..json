{"dta.poem.11464": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vermischte Getichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der geitz bringt eben nicht viel ein:", "tokens": ["Der", "geitz", "bringt", "e\u00b7ben", "nicht", "viel", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was mu\u00df er nicht vor reden h\u00f6ren?", "tokens": ["Was", "mu\u00df", "er", "nicht", "vor", "re\u00b7den", "h\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PTKNEG", "APPR", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist viel besser arm mit ehren,", "tokens": ["Es", "ist", "viel", "bes\u00b7ser", "arm", "mit", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ADJD", "APPR", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als reich, mit spott und schande seyn.", "tokens": ["Als", "reich", ",", "mit", "spott", "und", "schan\u00b7de", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "$,", "APPR", "ADJD", "KON", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}