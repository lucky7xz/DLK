{"dta.poem.19206": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "53.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist unwandelbar in der Verwandlung Reich?", "tokens": ["Was", "ist", "un\u00b7wan\u00b7del\u00b7bar", "in", "der", "Ver\u00b7wand\u00b7lung", "Reich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Ew'ge selber bleibt sich selbst nicht ewig gleich.", "tokens": ["Das", "Ew'\u00b7ge", "sel\u00b7ber", "bleibt", "sich", "selbst", "nicht", "e\u00b7wig", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VVFIN", "PRF", "ADV", "PTKNEG", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der L\u00e4nder Grenzen r\u00fcckt nicht V\u00f6lkerwechsel nur,", "tokens": ["Der", "L\u00e4n\u00b7der", "Gren\u00b7zen", "r\u00fcckt", "nicht", "V\u00f6l\u00b7ker\u00b7wech\u00b7sel", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKNEG", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es r\u00fcckt durch Str\u00f6m' und Meer sie selber die Natur.", "tokens": ["Es", "r\u00fcckt", "durch", "Str\u00f6m'", "und", "Meer", "sie", "sel\u00b7ber", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und jene Bahnen auch, die unver\u00e4nderlichen,", "tokens": ["Und", "je\u00b7ne", "Bah\u00b7nen", "auch", ",", "die", "un\u00b7ver\u00b7\u00e4n\u00b7der\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "ADV", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wovon die Wandelstern' im Wandel nie abwichen;", "tokens": ["Wo\u00b7von", "die", "Wan\u00b7del\u00b7stern'", "im", "Wan\u00b7del", "nie", "ab\u00b7wi\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da\u00df sie von Zeit zu Zeit ein wenig doch ausbeugen,", "tokens": ["Da\u00df", "sie", "von", "Zeit", "zu", "Zeit", "ein", "we\u00b7nig", "doch", "aus\u00b7beu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "APPR", "NN", "ART", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mu\u00df jede Sternkart' uns, die nicht mehr pa\u00dft, bezeugen.", "tokens": ["Mu\u00df", "je\u00b7de", "Stern\u00b7kart'", "uns", ",", "die", "nicht", "mehr", "pa\u00dft", ",", "be\u00b7zeu\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "PPER", "$,", "PRELS", "PTKNEG", "ADV", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}