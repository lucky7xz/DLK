{"dta.poem.12925": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Er findet in ihrer gegenwart \u00fcber-  \n all den himmel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Fahr' ich de\u00dfwegen in die h\u00f6lle,", "tokens": ["Fahr'", "ich", "de\u00df\u00b7we\u00b7gen", "in", "die", "h\u00f6l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PAV", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weil ich, was sterblich ist,", "tokens": ["Weil", "ich", ",", "was", "sterb\u00b7lich", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In dir, Calpurnia! zur g\u00f6ttin auserkiest?", "tokens": ["In", "dir", ",", "Cal\u00b7pur\u00b7nia", "!", "zur", "g\u00f6t\u00b7tin", "aus\u00b7er\u00b7kiest", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "NE", "$.", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "So ist doch auch vorl\u00e4ngst schon eine stelle,", "tokens": ["So", "ist", "doch", "auch", "vor\u00b7l\u00e4ngst", "schon", "ei\u00b7ne", "stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ADV", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Von wegen deiner grausamkeit,", "tokens": ["Von", "we\u00b7gen", "dei\u00b7ner", "grau\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Allda vor dich bereit,", "tokens": ["All\u00b7da", "vor", "dich", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ja deine qvaal wird gr\u00f6\u00dfer seyn, als meine;", "tokens": ["Ja", "dei\u00b7ne", "qvaal", "wird", "gr\u00f6\u00b7\u00dfer", "seyn", ",", "als", "mei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "PPOSAT", "NN", "VAFIN", "ADJD", "VAINF", "$,", "KOUS", "PPOSAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Denn einmal strafet dich die h\u00f6ll\u2019, in der ich bin;", "tokens": ["Denn", "ein\u00b7mal", "stra\u00b7fet", "dich", "die", "h\u00f6ll'", ",", "in", "der", "ich", "bin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dann zieht mein anblick dich in neue marter hin.", "tokens": ["Dann", "zieht", "mein", "an\u00b7blick", "dich", "in", "neu\u00b7e", "mar\u00b7ter", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PRF", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich aber, weil mein aug\u2019 an deiner anmuth scheine", "tokens": ["Ich", "a\u00b7ber", ",", "weil", "mein", "aug'", "an", "dei\u00b7ner", "an\u00b7muth", "schei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sich hier erqvicken kan,", "tokens": ["Sich", "hier", "er\u00b7qvi\u00b7cken", "kan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Treff in der h\u00f6lle selbst noch meinen himmel an.", "tokens": ["Treff", "in", "der", "h\u00f6l\u00b7le", "selbst", "noch", "mei\u00b7nen", "him\u00b7mel", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "ADV", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}