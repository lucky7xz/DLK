{"textgrid.poem.32096": {"metadata": {"author": {"name": "Angelus Silesius", "birth": "N.A.", "death": "N.A."}, "title": "98. Gott kann dem Willen nicht steuern", "genre": "verse", "period": "N.A.", "pub_year": 1650, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nichts St\u00e4rkers ist als Gott; doch kann er nicht verwehren,", "tokens": ["Nichts", "St\u00e4r\u00b7kers", "ist", "als", "Gott", ";", "doch", "kann", "er", "nicht", "ver\u00b7weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "VAFIN", "KOKOM", "NN", "$.", "ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich nicht, was ich will, soll wollen und begehren.", "tokens": ["Da\u00df", "ich", "nicht", ",", "was", "ich", "will", ",", "soll", "wol\u00b7len", "und", "be\u00b7geh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "$,", "PWS", "PPER", "VMFIN", "$,", "VMFIN", "VMFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}