{"dta.poem.19459": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die Dryas.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "O Liebe, wie schnell verrinnest du,", "tokens": ["O", "Lie\u00b7be", ",", "wie", "schnell", "ver\u00b7rin\u00b7nest", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "ADJD", "VVFIN", "PPER", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du fl\u00fcchtige, sch\u00f6ne Stunde,", "tokens": ["Du", "fl\u00fcch\u00b7ti\u00b7ge", ",", "sch\u00f6\u00b7ne", "Stun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mit einer Wunde beginnest du", "tokens": ["Mit", "ei\u00b7ner", "Wun\u00b7de", "be\u00b7gin\u00b7nest", "du"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und endest mit einer Wunde.", "tokens": ["Und", "en\u00b7dest", "mit", "ei\u00b7ner", "Wun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein J\u00fcngling irrt in Waldesraum,", "tokens": ["Ein", "J\u00fcng\u00b7ling", "irrt", "in", "Wal\u00b7des\u00b7raum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Umspielt von goldnen Schimmern,", "tokens": ["Um\u00b7spielt", "von", "gold\u00b7nen", "Schim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sp\u00e4ht nach einem sch\u00f6nen Baum,", "tokens": ["Und", "sp\u00e4ht", "nach", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich draus ein Boot zu zimmern.", "tokens": ["Sich", "draus", "ein", "Boot", "zu", "zim\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PAV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Jungeiche mit dem stolzen Wuchs,", "tokens": ["Jun\u00b7gei\u00b7che", "mit", "dem", "stol\u00b7zen", "Wuchs", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist mir gleich die rechte,", "tokens": ["Du", "bist", "mir", "gleich", "die", "rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dich zeichn' ich mit dem Beile flugs,", "tokens": ["Dich", "zeichn'", "ich", "mit", "dem", "Bei\u00b7le", "flugs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann ruf ich meine Knechte.", "tokens": ["Dann", "ruf", "ich", "mei\u00b7ne", "Knech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er f\u00fchrt den Streich. Ein schmerzlich Ach", "tokens": ["Er", "f\u00fchrt", "den", "Streich", ".", "Ein", "schmerz\u00b7lich", "Ach"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Macht j\u00e4hlings ihn erbleichen.", "tokens": ["Macht", "j\u00e4h\u00b7lings", "ihn", "er\u00b7blei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201eich sterbe!\u201c st\u00f6hnt's im Stamme schwach,", "tokens": ["\u201e", "ich", "ster\u00b7be", "!", "\u201c", "st\u00f6hnt's", "im", "Stam\u00b7me", "schwach", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$.", "$(", "VVFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201edie j\u00fcngste dieser Eichen!\u201c", "tokens": ["\u201e", "die", "j\u00fcngs\u00b7te", "die\u00b7ser", "Ei\u00b7chen", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ein Tr\u00f6pfchen Blutes oder zwei", "tokens": ["Ein", "Tr\u00f6pf\u00b7chen", "Blu\u00b7tes", "o\u00b7der", "zwei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sieht er am Beile hangen", "tokens": ["Sieht", "er", "am", "Bei\u00b7le", "han\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und schleudert's weg mit einem Schrei,", "tokens": ["Und", "schleu\u00b7dert's", "weg", "mit", "ei\u00b7nem", "Schrei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als h\u00e4tt' er Mord begangen.", "tokens": ["Als", "h\u00e4tt'", "er", "Mord", "be\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Schnell fl\u00fcstert's aus dem Baume jetzt:", "tokens": ["Schnell", "fl\u00fcs\u00b7tert's", "aus", "dem", "Bau\u00b7me", "jetzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eder Mord ist nicht vollendet!", "tokens": ["\u201e", "der", "Mord", "ist", "nicht", "voll\u00b7en\u00b7det", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich bin nur leicht am Arm verletzt.", "tokens": ["Ich", "bin", "nur", "leicht", "am", "Arm", "ver\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich hatt' mich umgewendet.\u201c", "tokens": ["Ich", "hatt'", "mich", "um\u00b7ge\u00b7wen\u00b7det", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u201ekomm, G\u00f6ttin\u201c, fleht er, \u201eWaldeskind,", "tokens": ["\u201e", "komm", ",", "G\u00f6t\u00b7tin", "\u201c", ",", "fleht", "er", ",", "\u201e", "Wal\u00b7des\u00b7kind", ","], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich Vergebung finde!\u201c", "tokens": ["Da\u00df", "ich", "Ver\u00b7ge\u00b7bung", "fin\u00b7de", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Schultern schmiegend schl\u00fcpft geschwind", "tokens": ["Die", "Schul\u00b7tern", "schmie\u00b7gend", "schl\u00fcpft", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Dryas aus der Rinde.", "tokens": ["Die", "Dryas", "aus", "der", "Rin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Ein D\u00e4mmer lag auf Stirn und Haar,", "tokens": ["Ein", "D\u00e4m\u00b7mer", "lag", "auf", "Stirn", "und", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Br\u00fcten und ein Weben,", "tokens": ["Ein", "Br\u00fc\u00b7ten", "und", "ein", "We\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von gr\u00fcnem Bl\u00e4tterschatten war", "tokens": ["Von", "gr\u00fc\u00b7nem", "Bl\u00e4t\u00b7ter\u00b7schat\u00b7ten", "war"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der schlanke Wuchs umgeben.", "tokens": ["Der", "schlan\u00b7ke", "Wuchs", "um\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Er fing den Arm zu k\u00fcssen an,", "tokens": ["Er", "fing", "den", "Arm", "zu", "k\u00fcs\u00b7sen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Stelle mit dem Hiebe,", "tokens": ["Die", "Stel\u00b7le", "mit", "dem", "Hie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und, der er viel zu Leid gethan,", "tokens": ["Und", ",", "der", "er", "viel", "zu", "Leid", "ge\u00b7than", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "PPER", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die that ihm viel zu Liebe.", "tokens": ["Die", "that", "ihm", "viel", "zu", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "\u201ein meinem Baum \u2014 ist lauter Traum\u201c ...", "tokens": ["\u201e", "in", "mei\u00b7nem", "Baum", "ist", "lau\u00b7ter", "Traum", "\u201c", "..."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "$(", "VAFIN", "PIAT", "NN", "$(", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie schl\u00fcpft zur\u00fcck behende", "tokens": ["Sie", "schl\u00fcpft", "zu\u00b7r\u00fcck", "be\u00b7hen\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und lispelt in den Waldesraum:", "tokens": ["Und", "lis\u00b7pelt", "in", "den", "Wal\u00b7des\u00b7raum", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eich wei\u00df, wen ich dir sende!\u201c", "tokens": ["\u201e", "ich", "wei\u00df", ",", "wen", "ich", "dir", "sen\u00b7de", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PWS", "PPER", "PPER", "VVFIN", "$.", "$("], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Der Botin Biene Dienst ist schwer,", "tokens": ["Der", "Bo\u00b7tin", "Bie\u00b7ne", "Dienst", "ist", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie mu\u00df sich redlich plagen,", "tokens": ["Sie", "mu\u00df", "sich", "red\u00b7lich", "pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Honig und Wermuth hin und her,", "tokens": ["Ho\u00b7nig", "und", "Wer\u00b7muth", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Waldaus, waldein zu tragen.", "tokens": ["Wald\u00b7aus", ",", "wal\u00b7dein", "zu", "tra\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PTKZU", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.12": {"line.1": {"text": "Einmal kam Bienchen wild gebrummt.", "tokens": ["Ein\u00b7mal", "kam", "Bien\u00b7chen", "wild", "ge\u00b7brummt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ADJD", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Dryas, mich kann's entr\u00fcsten!\u201c", "tokens": ["Dryas", ",", "mich", "kann's", "ent\u00b7r\u00fcs\u00b7ten", "!", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "PPER", "VMFIN", "VVINF", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Es setzt sich an den Stamm und summt:", "tokens": ["Es", "setzt", "sich", "an", "den", "Stamm", "und", "summt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eich sah's wie sie sich k\u00fc\u00dften!", "tokens": ["\u201e", "ich", "sah's", "wie", "sie", "sich", "k\u00fc\u00df\u00b7ten", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "PIS", "KOKOM", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Sie ist ein bl\u00fchend Nachbarkind,", "tokens": ["Sie", "ist", "ein", "bl\u00fc\u00b7hend", "Nach\u00b7bar\u00b7kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00df ihn best\u00e4ndig necken \u2014", "tokens": ["Mu\u00df", "ihn", "be\u00b7st\u00e4n\u00b7dig", "ne\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dich l\u00e4\u00dft er nun bei Wetter und Wind", "tokens": ["Dich", "l\u00e4\u00dft", "er", "nun", "bei", "Wet\u00b7ter", "und", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "In deinem Baume stecken!\u201c", "tokens": ["In", "dei\u00b7nem", "Bau\u00b7me", "ste\u00b7cken", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Ein schmerzlich Ach, als w\u00e4nde sich", "tokens": ["Ein", "schmerz\u00b7lich", "Ach", ",", "als", "w\u00e4n\u00b7de", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "ITJ", "$,", "KOUS", "ADJA", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein schlanker Leib und st\u00fcrbe!", "tokens": ["Ein", "schlan\u00b7ker", "Leib", "und", "st\u00fcr\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Laub vergilbt, die Krone blich,", "tokens": ["Das", "Laub", "ver\u00b7gilbt", ",", "die", "Kro\u00b7ne", "blich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Rinde br\u00f6ckelt m\u00fcrbe.", "tokens": ["Die", "Rin\u00b7de", "br\u00f6\u00b7ckelt", "m\u00fcr\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}