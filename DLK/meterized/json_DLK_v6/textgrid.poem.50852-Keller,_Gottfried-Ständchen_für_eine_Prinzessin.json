{"textgrid.poem.50852": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "St\u00e4ndchen f\u00fcr eine Prinzessin", "genre": "verse", "period": "N.A.", "pub_year": 1848, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6ne B\u00fcrgerin, sieh, der Mai", "tokens": ["Sch\u00f6\u00b7ne", "B\u00fcr\u00b7ge\u00b7rin", ",", "sieh", ",", "der", "Mai"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Flutet um deine Fenster!", "tokens": ["Flu\u00b7tet", "um", "dei\u00b7ne", "Fens\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Alle Seelen sind nun frei,", "tokens": ["Al\u00b7le", "See\u00b7len", "sind", "nun", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es zerflie\u00dfen der Phantasei", "tokens": ["Und", "es", "zer\u00b7flie\u00b7\u00dfen", "der", "Phan\u00b7ta\u00b7sei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Luftige Gespenster.", "tokens": ["Luf\u00b7ti\u00b7ge", "Ge\u00b7spens\u00b7ter", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Liebliche B\u00fcrgerin Klara!", "tokens": ["Lieb\u00b7li\u00b7che", "B\u00fcr\u00b7ge\u00b7rin", "Kla\u00b7ra", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "In die Tiefe tauche k\u00fchn,", "tokens": ["In", "die", "Tie\u00b7fe", "tau\u00b7che", "k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Jugend und Liebe zu werben,", "tokens": ["Ju\u00b7gend", "und", "Lie\u00b7be", "zu", "wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Wo die B\u00e4ume des Lebens bl\u00fchn", "tokens": ["Wo", "die", "B\u00e4u\u00b7me", "des", "Le\u00b7bens", "bl\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVINF"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und die Augen wie Sterne gl\u00fchn!", "tokens": ["Und", "die", "Au\u00b7gen", "wie", "Ster\u00b7ne", "gl\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KOKOM", "NN", "VVINF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Droben bei dir ist Sterben,", "tokens": ["Dro\u00b7ben", "bei", "dir", "ist", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VAFIN", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Liebliche B\u00fcrgerin Klara!", "tokens": ["Lieb\u00b7li\u00b7che", "B\u00fcr\u00b7ge\u00b7rin", "Kla\u00b7ra", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "L\u00f6se den Reif von goldenem Glanz", "tokens": ["L\u00f6\u00b7se", "den", "Reif", "von", "gol\u00b7de\u00b7nem", "Glanz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Aus den Lockenringen!", "tokens": ["Aus", "den", "Lo\u00b7cken\u00b7rin\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wirf ihn herab, im klingenden Tanz", "tokens": ["Wirf", "ihn", "her\u00b7ab", ",", "im", "klin\u00b7gen\u00b7den", "Tanz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Einen bl\u00fchenden Myrtenkranz", "tokens": ["Ei\u00b7nen", "bl\u00fc\u00b7hen\u00b7den", "Myr\u00b7ten\u00b7kranz"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Wollen wir froh dir schlingen,", "tokens": ["Wol\u00b7len", "wir", "froh", "dir", "schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Liebliche B\u00fcrgerin Klara!", "tokens": ["Lieb\u00b7li\u00b7che", "B\u00fcr\u00b7ge\u00b7rin", "Kla\u00b7ra", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "F\u00fchle, du Engel, dies heilige Wehn,", "tokens": ["F\u00fch\u00b7le", ",", "du", "En\u00b7gel", ",", "dies", "hei\u00b7li\u00b7ge", "Wehn", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "NE", "$,", "PDS", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Das allm\u00e4chtige Treiben!", "tokens": ["Das", "all\u00b7m\u00e4ch\u00b7ti\u00b7ge", "Trei\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "O dein Himmel wird untergehn", "tokens": ["O", "dein", "Him\u00b7mel", "wird", "un\u00b7ter\u00b7gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "VAFIN", "VVINF"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Und ein sch\u00f6nerer auferstehn \u2013", "tokens": ["Und", "ein", "sch\u00f6\u00b7ne\u00b7rer", "auf\u00b7er\u00b7stehn", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVINF", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Willst du ein Engel bleiben,", "tokens": ["Willst", "du", "ein", "En\u00b7gel", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Liebliche B\u00fcrgerin Klara?", "tokens": ["Lieb\u00b7li\u00b7che", "B\u00fcr\u00b7ge\u00b7rin", "Kla\u00b7ra", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Nicht wie Luna in schweigender Nacht", "tokens": ["Nicht", "wie", "Lu\u00b7na", "in", "schwei\u00b7gen\u00b7der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "KOKOM", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "K\u00fc\u00dfte den tr\u00e4umenden Schl\u00e4fer:", "tokens": ["K\u00fc\u00df\u00b7te", "den", "tr\u00e4u\u00b7men\u00b7den", "Schl\u00e4\u00b7fer", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Komm in der Sonne strahlender Pracht,", "tokens": ["Komm", "in", "der", "Son\u00b7ne", "strah\u00b7len\u00b7der", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Da\u00df das sch\u00f6ne Lied erwacht:", "tokens": ["Da\u00df", "das", "sch\u00f6\u00b7ne", "Lied", "er\u00b7wacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00f6nigstochter und Sch\u00e4fer!", "tokens": ["K\u00f6\u00b7nig\u00b7stoch\u00b7ter", "und", "Sch\u00e4\u00b7fer", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Liebliche B\u00fcrgerin Klara!", "tokens": ["Lieb\u00b7li\u00b7che", "B\u00fcr\u00b7ge\u00b7rin", "Kla\u00b7ra", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}