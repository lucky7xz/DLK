{"textgrid.poem.24685": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du hast mich an Dinge gemahnet,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast mich an Dinge gemahnet,", "tokens": ["Du", "hast", "mich", "an", "Din\u00b7ge", "ge\u00b7mah\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die heimlich in mir sind,", "tokens": ["Die", "heim\u00b7lich", "in", "mir", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du warst f\u00fcr die Saiten der Seele", "tokens": ["Du", "warst", "f\u00fcr", "die", "Sai\u00b7ten", "der", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der n\u00e4chtige fl\u00fcsternde Wind", "tokens": ["Der", "n\u00e4ch\u00b7ti\u00b7ge", "fl\u00fcs\u00b7tern\u00b7de", "Wind"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Und wie das r\u00e4tselhafte,", "tokens": ["Und", "wie", "das", "r\u00e4t\u00b7sel\u00b7haf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Rufen der atmenden Nacht,", "tokens": ["Das", "Ru\u00b7fen", "der", "at\u00b7men\u00b7den", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wenn drau\u00dfen die Wolken gleiten", "tokens": ["Wenn", "drau\u00b7\u00dfen", "die", "Wol\u00b7ken", "glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und man aus dem Traum erwacht,", "tokens": ["Und", "man", "aus", "dem", "Traum", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Zu blauer weicher Weite", "tokens": ["Zu", "blau\u00b7er", "wei\u00b7cher", "Wei\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die enge N\u00e4he schwillt,", "tokens": ["Die", "en\u00b7ge", "N\u00e4\u00b7he", "schwillt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch Zweige vor dem Monde", "tokens": ["Durch", "Zwei\u00b7ge", "vor", "dem", "Mon\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein leises Zittern quillt.", "tokens": ["Ein", "lei\u00b7ses", "Zit\u00b7tern", "quillt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}