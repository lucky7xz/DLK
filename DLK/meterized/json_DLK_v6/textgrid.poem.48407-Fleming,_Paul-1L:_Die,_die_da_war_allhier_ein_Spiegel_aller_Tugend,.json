{"textgrid.poem.48407": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die, die da war allhier ein Spiegel aller Tugend,", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die, die da war allhier ein Spiegel aller Tugend,", "tokens": ["Die", ",", "die", "da", "war", "all\u00b7hier", "ein", "Spie\u00b7gel", "al\u00b7ler", "Tu\u00b7gend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "ADV", "VAFIN", "ADV", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ist mit der Fr\u00fclingszeit im Fr\u00fcling ihrer Jugend", "tokens": ["ist", "mit", "der", "Fr\u00fc\u00b7lings\u00b7zeit", "im", "Fr\u00fc\u00b7ling", "ih\u00b7rer", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "von uns gerissen hin, wie wann von zarter Hand", "tokens": ["von", "uns", "ge\u00b7ris\u00b7sen", "hin", ",", "wie", "wann", "von", "zar\u00b7ter", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVPP", "PTKVZ", "$,", "PWAV", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ein blaues Veiligen dem Garten wird entwandt,", "tokens": ["ein", "blau\u00b7es", "Vei\u00b7li\u00b7gen", "dem", "Gar\u00b7ten", "wird", "ent\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "wie wenn auf Ph\u00f6bus Schein erfolget Regenwetter,", "tokens": ["wie", "wenn", "auf", "Ph\u00f6\u00b7bus", "Schein", "er\u00b7fol\u00b7get", "Re\u00b7gen\u00b7wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "APPR", "NE", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "wie wenn ein sch\u00f6ner Baum verleust die gr\u00fcnen Bl\u00e4tter.", "tokens": ["wie", "wenn", "ein", "sch\u00f6\u00b7ner", "Baum", "ver\u00b7leust", "die", "gr\u00fc\u00b7nen", "Bl\u00e4t\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch Fr\u00fcling, Veiligen, Schein, Bl\u00e4tter finden sich", "tokens": ["Doch", "Fr\u00fc\u00b7ling", ",", "Vei\u00b7li\u00b7gen", ",", "Schein", ",", "Bl\u00e4t\u00b7ter", "fin\u00b7den", "sich"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "NE", "$,", "NN", "$,", "NN", "VVFIN", "PRF"], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "mit Zeit: o welche Zeit wird wiederbringen dich?", "tokens": ["mit", "Zeit", ":", "o", "wel\u00b7che", "Zeit", "wird", "wie\u00b7der\u00b7brin\u00b7gen", "dich", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "FM", "PWAT", "NN", "VAFIN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}