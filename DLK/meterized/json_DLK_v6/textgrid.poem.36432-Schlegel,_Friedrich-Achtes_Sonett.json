{"textgrid.poem.36432": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Sonett", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ward erl\u00f6st, zum Weltweib umgeschaffen,", "tokens": ["Ich", "ward", "er\u00b7l\u00f6st", ",", "zum", "Welt\u00b7weib", "um\u00b7ge\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des irren Wanderns letzte h\u00f6chste Feier.", "tokens": ["Des", "ir\u00b7ren", "Wan\u00b7derns", "letz\u00b7te", "h\u00f6chs\u00b7te", "Fei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich rag' ins D\u00e4mmerlicht, verh\u00fcllt vom Schleier", "tokens": ["Ich", "rag'", "ins", "D\u00e4m\u00b7mer\u00b7licht", ",", "ver\u00b7h\u00fcllt", "vom", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Sterne mit den bleichen Mondagraffen.", "tokens": ["Der", "Ster\u00b7ne", "mit", "den", "blei\u00b7chen", "Mon\u00b7da\u00b7graf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zur Erde send' ich meinen Himmelsgeier,", "tokens": ["Zur", "Er\u00b7de", "send'", "ich", "mei\u00b7nen", "Him\u00b7mels\u00b7gei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der ruft die letzten geilen Menschenaffen.", "tokens": ["Der", "ruft", "die", "letz\u00b7ten", "gei\u00b7len", "Men\u00b7schen\u00b7af\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich werde meine R\u00f6cke h\u00f6her raffen", "tokens": ["Ich", "wer\u00b7de", "mei\u00b7ne", "R\u00f6\u00b7cke", "h\u00f6\u00b7her", "raf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und alle gr\u00fc\u00dfen als willkomm'ne Freier.", "tokens": ["Und", "al\u00b7le", "gr\u00fc\u00b7\u00dfen", "als", "will\u00b7kom\u00b7m'\u00b7ne", "Frei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVINF", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich h\u00f6re schon ihr heis'res Brunstgeschrei.", "tokens": ["Ich", "h\u00f6\u00b7re", "schon", "ihr", "heis'\u00b7res", "Brunst\u00b7ge\u00b7schrei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Schw\u00e4nze zucken und die Zungen lallen.", "tokens": ["Die", "Schw\u00e4n\u00b7ze", "zu\u00b7cken", "und", "die", "Zun\u00b7gen", "lal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Begattend d\u00fcnken sie sich schicksalsfrei.", "tokens": ["Be\u00b7gat\u00b7tend", "d\u00fcn\u00b7ken", "sie", "sich", "schick\u00b7sals\u00b7frei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch werden sie in meine Scheide fallen,", "tokens": ["Doch", "wer\u00b7den", "sie", "in", "mei\u00b7ne", "Schei\u00b7de", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann will ich sie Kometen gleich mit kurzen", "tokens": ["Dann", "will", "ich", "sie", "Ko\u00b7me\u00b7ten", "gleich", "mit", "kur\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVFIN", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und hellen Knallen in den Weltraum furzen.", "tokens": ["Und", "hel\u00b7len", "Knal\u00b7len", "in", "den", "Welt\u00b7raum", "fur\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}