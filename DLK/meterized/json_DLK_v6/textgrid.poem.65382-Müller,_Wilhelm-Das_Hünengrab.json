{"textgrid.poem.65382": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Das H\u00fcnengrab", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon wieder hundert Jahre!", "tokens": ["Schon", "wie\u00b7der", "hun\u00b7dert", "Jah\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich darf aus meiner Gruft", "tokens": ["Ich", "darf", "aus", "mei\u00b7ner", "Gruft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Heraus die Blicke senden", "tokens": ["He\u00b7raus", "die", "Bli\u00b7cke", "sen\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sch\u00f6pfen frische Luft.", "tokens": ["Und", "sch\u00f6p\u00b7fen", "fri\u00b7sche", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Luft so frisch wie immer,", "tokens": ["Die", "Luft", "so", "frisch", "wie", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KOKOM", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Meer noch dunkelblau,", "tokens": ["Das", "Meer", "noch", "dun\u00b7kel\u00b7blau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die alten wei\u00dfen D\u00fcnen,", "tokens": ["Die", "al\u00b7ten", "wei\u00b7\u00dfen", "D\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die junge gr\u00fcne Au'!", "tokens": ["Die", "jun\u00b7ge", "gr\u00fc\u00b7ne", "Au'", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du, Mensch, nur immer kleiner,", "tokens": ["Du", ",", "Mensch", ",", "nur", "im\u00b7mer", "klei\u00b7ner", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und gr\u00f6\u00dfer stets dein Haus,", "tokens": ["Und", "gr\u00f6\u00b7\u00dfer", "stets", "dein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Gr\u00e4ber immer enger \u2013", "tokens": ["Die", "Gr\u00e4\u00b7ber", "im\u00b7mer", "en\u00b7ger", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo denkst du, Mensch, hinaus?", "tokens": ["Wo", "denkst", "du", ",", "Mensch", ",", "hin\u00b7aus", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die erste Ruhest\u00e4tte", "tokens": ["Die", "ers\u00b7te", "Ru\u00b7he\u00b7st\u00e4t\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fcr eine Spanne Zeit,", "tokens": ["F\u00fcr", "ei\u00b7ne", "Span\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die bauest auf der H\u00f6he", "tokens": ["Die", "bau\u00b7est", "auf", "der", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So pr\u00e4chtig und so weit.", "tokens": ["So", "pr\u00e4ch\u00b7tig", "und", "so", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und l\u00e4\u00dft dein Grab dir graben", "tokens": ["Und", "l\u00e4\u00dft", "dein", "Grab", "dir", "gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So eng', so kurz, so schmal,", "tokens": ["So", "eng'", ",", "so", "kurz", ",", "so", "schmal", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dort zwischen dumpfen Mauern,", "tokens": ["Dort", "zwi\u00b7schen", "dum\u00b7pfen", "Mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im tief versteckten Thal.", "tokens": ["Im", "tief", "ver\u00b7steck\u00b7ten", "Thal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dort mu\u00dft du lange wohnen,", "tokens": ["Dort", "mu\u00dft", "du", "lan\u00b7ge", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dort ist dein rechtes Haus,", "tokens": ["Dort", "ist", "dein", "rech\u00b7tes", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und darfst aus dem nicht gehen", "tokens": ["Und", "darfst", "aus", "dem", "nicht", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "APPR", "PRELS", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf Berg und Strand hinaus.", "tokens": ["Auf", "Berg", "und", "Strand", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Schau' ich aus meinem Grabe,", "tokens": ["Schau'", "ich", "aus", "mei\u00b7nem", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich schaue weit umher", "tokens": ["Ich", "schau\u00b7e", "weit", "um\u00b7her"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den hohen blauen Himmel,", "tokens": ["Den", "ho\u00b7hen", "blau\u00b7en", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die K\u00fcsten und das Meer.", "tokens": ["Die", "K\u00fcs\u00b7ten", "und", "das", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Das Meer, das ich durchschwommen", "tokens": ["Das", "Meer", ",", "das", "ich", "durch\u00b7schwom\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit meinem starken Arm,", "tokens": ["Mit", "mei\u00b7nem", "star\u00b7ken", "Arm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Strand, wo ich gestanden", "tokens": ["Den", "Strand", ",", "wo", "ich", "ge\u00b7stan\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In meiner Feinde Schwarm", "tokens": ["In", "mei\u00b7ner", "Fein\u00b7de", "Schwarm"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Du guckst aus deiner Grube.", "tokens": ["Du", "guckst", "aus", "dei\u00b7ner", "Gru\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Wust und Graus hinein,", "tokens": ["In", "Wust", "und", "Graus", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In schwarze F\u00f6hrenschatten,", "tokens": ["In", "schwar\u00b7ze", "F\u00f6h\u00b7ren\u00b7schat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf deinen Leichenstein.", "tokens": ["Auf", "dei\u00b7nen", "Lei\u00b7chen\u00b7stein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}