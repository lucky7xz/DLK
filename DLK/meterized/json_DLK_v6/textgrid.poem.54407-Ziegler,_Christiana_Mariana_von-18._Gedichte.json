{"textgrid.poem.54407": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "18. Gedichte", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6r! Sch\u00e4fer kannst du nicht stets deine Chloris k\u00fcssen,", "tokens": ["H\u00f6r", "!", "Sch\u00e4\u00b7fer", "kannst", "du", "nicht", "stets", "dei\u00b7ne", "Chlo\u00b7ris", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "VMFIN", "PPER", "PTKNEG", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So denke, da\u00df sie dich auch \u00f6fters mu\u00df vermissen.", "tokens": ["So", "den\u00b7ke", ",", "da\u00df", "sie", "dich", "auch", "\u00f6f\u00b7ters", "mu\u00df", "ver\u00b7mis\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "PRF", "ADV", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie theilet Scherz und Lust, Betr\u00fcben, Furcht und Pein", "tokens": ["Sie", "thei\u00b7let", "Scherz", "und", "Lust", ",", "Be\u00b7tr\u00fc\u00b7ben", ",", "Furcht", "und", "Pein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit dir, drum mu\u00df davon die Helfte deine seyn.", "tokens": ["Mit", "dir", ",", "drum", "mu\u00df", "da\u00b7von", "die", "Helf\u00b7te", "dei\u00b7ne", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PAV", "VMFIN", "PAV", "ART", "NN", "PPOSAT", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Will sich, schau ihre Treu, zu ihr ein Hirte wagen,", "tokens": ["Will", "sich", ",", "schau", "ih\u00b7re", "Treu", ",", "zu", "ihr", "ein", "Hir\u00b7te", "wa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "$,", "ADJD", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "ART", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Ergreift sie gleich den Stock, den N\u00e4scher weg zu jagen.", "tokens": ["Er\u00b7greift", "sie", "gleich", "den", "Stock", ",", "den", "N\u00e4\u00b7scher", "weg", "zu", "ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie scherzt, sie singt und spielt, und alles was sie macht,", "tokens": ["Sie", "scherzt", ",", "sie", "singt", "und", "spielt", ",", "und", "al\u00b7les", "was", "sie", "macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "$,", "KON", "PIS", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da wird gewi\u00df an dich zugleich auch mit gedacht.", "tokens": ["Da", "wird", "ge\u00b7wi\u00df", "an", "dich", "zu\u00b7gleich", "auch", "mit", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "PPER", "ADV", "ADV", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Auch die Entfernung trennt dich nicht von ihrem Herzen,", "tokens": ["Auch", "die", "Ent\u00b7fer\u00b7nung", "trennt", "dich", "nicht", "von", "ih\u00b7rem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Zuweilen mu\u00df sie wohl mit guten Freunden scherzen,", "tokens": ["Zu\u00b7wei\u00b7len", "mu\u00df", "sie", "wohl", "mit", "gu\u00b7ten", "Freun\u00b7den", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Denn dies erfordert ja der Wohlstand und die Zeit;", "tokens": ["Denn", "dies", "er\u00b7for\u00b7dert", "ja", "der", "Wohl\u00b7stand", "und", "die", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Doch brennt sie nur vor dich in reiner Z\u00e4rtlichkeit.", "tokens": ["Doch", "brennt", "sie", "nur", "vor", "dich", "in", "rei\u00b7ner", "Z\u00e4rt\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Was w\u00fcrde, wollte sie die Reihen unterbrechen,", "tokens": ["Was", "w\u00fcr\u00b7de", ",", "woll\u00b7te", "sie", "die", "Rei\u00b7hen", "un\u00b7ter\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Des Satyrs freches Maul zu ihrem Nachtheil sprechen,", "tokens": ["Des", "Sa\u00b7tyrs", "fre\u00b7ches", "Maul", "zu", "ih\u00b7rem", "Nacht\u00b7heil", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ein jeder sp\u00fcrte gleich, g\u00e4b sie niemand Geh\u00f6r,", "tokens": ["Ein", "je\u00b7der", "sp\u00fcr\u00b7te", "gleich", ",", "g\u00e4b", "sie", "nie\u00b7mand", "Ge\u00b7h\u00f6r", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "$,", "VVFIN", "PPER", "PIS", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Da\u00df sie aus Furcht vor dir so bl\u00f6d und ekel w\u00e4r.", "tokens": ["Da\u00df", "sie", "aus", "Furcht", "vor", "dir", "so", "bl\u00f6d", "und", "e\u00b7kel", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "APPR", "PPER", "ADV", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Nein, eine Sch\u00e4ferinn mu\u00df nicht durch Wort und Thaten,", "tokens": ["Nein", ",", "ei\u00b7ne", "Sch\u00e4\u00b7fe\u00b7rinn", "mu\u00df", "nicht", "durch", "Wort", "und", "Tha\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VMFIN", "PTKNEG", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und falsche Sittsamkeit es vor der Welt verrathen,", "tokens": ["Und", "fal\u00b7sche", "Sitt\u00b7sam\u00b7keit", "es", "vor", "der", "Welt", "ver\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da\u00df sie was liebes hat, sonst ist das R\u00e4thsel aus,", "tokens": ["Da\u00df", "sie", "was", "lie\u00b7bes", "hat", ",", "sonst", "ist", "das", "R\u00e4th\u00b7sel", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ADJA", "VAFIN", "$,", "ADV", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und ieder Hirte macht sich nur ein Liedchen draus.", "tokens": ["Und", "ie\u00b7der", "Hir\u00b7te", "macht", "sich", "nur", "ein", "Lied\u00b7chen", "draus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PRF", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}