{"textgrid.poem.66135": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Aus einem Brief Schubarts an seinen Schwager B\u00f6ckh, damals Rektor in E\u00dflingen", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund Jonathan,", "tokens": ["Freund", "Jo\u00b7na\u00b7than", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Sieh deinen Bruder an,", "tokens": ["Sieh", "dei\u00b7nen", "Bru\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der hier im Thal des finstern Grames wallet,", "tokens": ["Der", "hier", "im", "Thal", "des", "fins\u00b7tern", "Gra\u00b7mes", "wal\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo Scherz und Freude weit entflieht,", "tokens": ["Wo", "Scherz", "und", "Freu\u00b7de", "weit", "ent\u00b7flieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wo sein hypochondrisch Lied", "tokens": ["Und", "wo", "sein", "hy\u00b7po\u00b7chond\u00b7risch", "Lied"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aus Drachenh\u00f6hlen wiederhallet.", "tokens": ["Aus", "Dra\u00b7chen\u00b7h\u00f6h\u00b7len", "wie\u00b7der\u00b7hal\u00b7let", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hier, wo mein Lebenspfad", "tokens": ["Hier", ",", "wo", "mein", "Le\u00b7ben\u00b7spfad"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Statt Rosen lauter Dornen hat,", "tokens": ["Statt", "Ro\u00b7sen", "lau\u00b7ter", "Dor\u00b7nen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wo bei jedem Schritt, zu dem mein Fu\u00df sich hebet,", "tokens": ["Und", "wo", "bei", "je\u00b7dem", "Schritt", ",", "zu", "dem", "mein", "Fu\u00df", "sich", "he\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "PIAT", "NN", "$,", "APPR", "ART", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mir Blut an meinen Fersen klebet.", "tokens": ["Mir", "Blut", "an", "mei\u00b7nen", "Fer\u00b7sen", "kle\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Hier, du mein Jonathan,", "tokens": ["Hier", ",", "du", "mein", "Jo\u00b7na\u00b7than", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Auf dieser finsteren und klippenvollen Bahn", "tokens": ["Auf", "die\u00b7ser", "fins\u00b7te\u00b7ren", "und", "klip\u00b7pen\u00b7vol\u00b7len", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "VVINF", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sieh deinen Bruder an.", "tokens": ["Sieh", "dei\u00b7nen", "Bru\u00b7der", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}