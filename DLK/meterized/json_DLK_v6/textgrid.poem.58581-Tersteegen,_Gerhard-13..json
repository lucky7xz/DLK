{"textgrid.poem.58581": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein treuer Untertan betr\u00fcbet sich von Herzen,", "tokens": ["Ein", "treu\u00b7er", "Un\u00b7ter\u00b7tan", "be\u00b7tr\u00fc\u00b7bet", "sich", "von", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn er gefangen sitzt in fremder Herren Macht;", "tokens": ["Wenn", "er", "ge\u00b7fan\u00b7gen", "sitzt", "in", "frem\u00b7der", "Her\u00b7ren", "Macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein rechter Herr und Gott, du kennest meine Schmerzen,", "tokens": ["Mein", "rech\u00b7ter", "Herr", "und", "Gott", ",", "du", "ken\u00b7nest", "mei\u00b7ne", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die andre Herren oft mir haben zugebracht.", "tokens": ["Die", "and\u00b7re", "Her\u00b7ren", "oft", "mir", "ha\u00b7ben", "zu\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn S\u00fcnd' und Satans Macht in Angst und Not mich treiben,", "tokens": ["Wenn", "S\u00fcnd'", "und", "Sa\u00b7tans", "Macht", "in", "Angst", "und", "Not", "mich", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "ADJA", "NN", "APPR", "NN", "KON", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So denkt mein armes Herz alleine doch an dich;", "tokens": ["So", "denkt", "mein", "ar\u00b7mes", "Herz", "al\u00b7lei\u00b7ne", "doch", "an", "dich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es gehe wie es geh', ich will der Deine bleiben,", "tokens": ["Es", "ge\u00b7he", "wie", "es", "geh'", ",", "ich", "will", "der", "Dei\u00b7ne", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "VVFIN", "$,", "PPER", "VMFIN", "ART", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mein Wille bleibet fest, du bist es ewiglich.", "tokens": ["Mein", "Wil\u00b7le", "blei\u00b7bet", "fest", ",", "du", "bist", "es", "e\u00b7wig\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$,", "PPER", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}