{"textgrid.poem.59995": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1866, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vielverschlagner Richard Wagner,", "tokens": ["Viel\u00b7ver\u00b7schlag\u00b7ner", "Ric\u00b7hard", "Wag\u00b7ner", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem Schiffbruch von Paris", "tokens": ["Aus", "dem", "Schiff\u00b7bruch", "von", "Pa\u00b7ris"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach der Isarstadt getragner,", "tokens": ["Nach", "der", "Is\u00b7ar\u00b7stadt", "ge\u00b7trag\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sangeskundiger Uly\u00df!", "tokens": ["San\u00b7ges\u00b7kun\u00b7di\u00b7ger", "U\u00b7ly\u00df", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "Ungest\u00fcmer Wegebahner,", "tokens": ["Un\u00b7ge\u00b7st\u00fc\u00b7mer", "We\u00b7ge\u00b7bah\u00b7ner", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deutscher Tonkunst Pionier,", "tokens": ["Deut\u00b7scher", "Ton\u00b7kunst", "Pi\u00b7o\u00b7nier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter welche Insulaner,", "tokens": ["Un\u00b7ter", "wel\u00b7che", "In\u00b7su\u00b7la\u00b7ner", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Teurer Freund, gerietst du hier!", "tokens": ["Teu\u00b7rer", "Freund", ",", "ge\u00b7rietst", "du", "hier", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und was hilft dir alle Gnade", "tokens": ["Und", "was", "hilft", "dir", "al\u00b7le", "Gna\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihres Herrn Alkinous?", "tokens": ["Ih\u00b7res", "Herrn", "Al\u00b7kin\u00b7ous", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Auf der Lebenspromenade", "tokens": ["Auf", "der", "Le\u00b7bens\u00b7pro\u00b7me\u00b7na\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser erste Sonnenku\u00df?", "tokens": ["Die\u00b7ser", "ers\u00b7te", "Son\u00b7nen\u00b7ku\u00df", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Philister, scheelen Blickes,", "tokens": ["Die", "Phi\u00b7lis\u00b7ter", ",", "schee\u00b7len", "Bli\u00b7ckes", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spucken in den reinsten Quell;", "tokens": ["Spu\u00b7cken", "in", "den", "reins\u00b7ten", "Quell", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kleine Sch\u00f6nheit r\u00fchrt ihr dickes,", "tokens": ["Klei\u00b7ne", "Sch\u00f6n\u00b7heit", "r\u00fchrt", "ihr", "di\u00b7ckes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Undurchdringlich dickes Fell.", "tokens": ["Un\u00b7durch\u00b7dring\u00b7lich", "di\u00b7ckes", "Fell", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ihres Hofbr\u00e4uhorizontes", "tokens": ["Ih\u00b7res", "Hof\u00b7br\u00e4u\u00b7ho\u00b7ri\u00b7zon\u00b7tes"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Grenzen \u00fcberstiegst du keck,", "tokens": ["Gren\u00b7zen", "\u00fc\u00b7bers\u00b7tiegst", "du", "keck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und du bist wie Lola Montez", "tokens": ["Und", "du", "bist", "wie", "Lo\u00b7la", "Mon\u00b7tez"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "KOKOM", "NE", "NE"], "meter": "-+-+++-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Dieser Biederm\u00e4nner Schreck.", "tokens": ["Die\u00b7ser", "Bie\u00b7der\u00b7m\u00e4n\u00b7ner", "Schreck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbsolche Summen zu verplempern,", "tokens": ["\u00bb", "sol\u00b7che", "Sum\u00b7men", "zu", "ver\u00b7plem\u00b7pern", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimmt der Fremdling sich heraus!", "tokens": ["Nimmt", "der", "Fremd\u00b7ling", "sich", "he\u00b7raus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Er bestellte sich bei Sempern", "tokens": ["Er", "be\u00b7stell\u00b7te", "sich", "bei", "Sem\u00b7pern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gar ein neu Kom\u00f6dienhaus!", "tokens": ["Gar", "ein", "neu", "Ko\u00b7m\u00f6\u00b7di\u00b7en\u00b7haus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.7": {"line.1": {"text": "Ist die B\u00fchne, drauf der Robert,", "tokens": ["Ist", "die", "B\u00fch\u00b7ne", ",", "drauf", "der", "Ro\u00b7bert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PAV", "ART", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Prophet, der Troubadour", "tokens": ["Der", "Pro\u00b7phet", ",", "der", "Trou\u00b7ba\u00b7dour"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "M\u00fcnchens Publikum erobert,", "tokens": ["M\u00fcn\u00b7chens", "Pub\u00b7li\u00b7kum", "er\u00b7o\u00b7bert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine Bretterbude nur?", "tokens": ["Ei\u00b7ne", "Bret\u00b7ter\u00b7bu\u00b7de", "nur", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Schreitet nicht der gro\u00dfe Vasco", "tokens": ["Schrei\u00b7tet", "nicht", "der", "gro\u00b7\u00dfe", "Va\u00b7sco"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weltumsegelnd \u00fcber sie?", "tokens": ["Welt\u00b7um\u00b7se\u00b7gelnd", "\u00fc\u00b7ber", "sie", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch Geduld \u2013 du machst Fiasko,", "tokens": ["Doch", "Ge\u00b7duld", "\u2013", "du", "machst", "Fi\u00b7as\u00b7ko", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$(", "PPER", "VVFIN", "NE", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Hergelaufenes Genie!", "tokens": ["Her\u00b7ge\u00b7lau\u00b7fe\u00b7nes", "Ge\u00b7nie", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+---+", "measure": "unknown.measure.tri"}}, "stanza.9": {"line.1": {"text": "Ja, trotz allen deinen Kniffen,", "tokens": ["Ja", ",", "trotz", "al\u00b7len", "dei\u00b7nen", "Knif\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wir versalzen dir die Supp;", "tokens": ["Wir", "ver\u00b7sal\u00b7zen", "dir", "die", "Supp", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Morgen wirst du ausgepfiffen \u2013", "tokens": ["Mor\u00b7gen", "wirst", "du", "aus\u00b7ge\u00b7pfif\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vorw\u00e4rts, Franziskanerklub!\u00ab", "tokens": ["Vor\u00b7w\u00e4rts", ",", "Fran\u00b7zis\u00b7ka\u00b7ner\u00b7klub", "!", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "punct"], "pos": ["ADV", "$,", "NE", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}