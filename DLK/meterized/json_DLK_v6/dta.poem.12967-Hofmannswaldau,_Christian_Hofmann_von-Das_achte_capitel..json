{"dta.poem.12967": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Das achte capitel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man darff sein hertze nicht stets auf der zunge tragen:", "tokens": ["Man", "darff", "sein", "hert\u00b7ze", "nicht", "stets", "auf", "der", "zun\u00b7ge", "tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein weiser schleu\u00dft die zung\u2019 in seinem hertzen ein.", "tokens": ["Ein", "wei\u00b7ser", "schleu\u00dft", "die", "zung'", "in", "sei\u00b7nem", "hert\u00b7zen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drum must du, was dich dr\u00fcckt, nicht einem ieden sagen;", "tokens": ["Drum", "must", "du", ",", "was", "dich", "dr\u00fcckt", ",", "nicht", "ei\u00b7nem", "ie\u00b7den", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,", "PTKNEG", "ART", "PIAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn dein gewissens-rath kan nur ein frommer seyn.", "tokens": ["Denn", "dein", "ge\u00b7wis\u00b7sens\u00b7rath", "kan", "nur", "ein", "from\u00b7mer", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "ADV", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein frommer, welchen Gott und die erfahrung weise", "tokens": ["Ein", "from\u00b7mer", ",", "wel\u00b7chen", "Gott", "und", "die", "er\u00b7fah\u00b7rung", "wei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "PWAT", "NN", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und in geheimni\u00dfen still und getreu gemacht:", "tokens": ["Und", "in", "ge\u00b7heim\u00b7ni\u00b7\u00dfen", "still", "und", "ge\u00b7treu", "ge\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Drum halt dich iederzeit zu einem solchen greise;", "tokens": ["Drum", "halt", "dich", "ie\u00b7der\u00b7zeit", "zu", "ei\u00b7nem", "sol\u00b7chen", "grei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPR", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Deun bey der jugend wird nichts sicher angebracht.", "tokens": ["Deun", "bey", "der", "ju\u00b7gend", "wird", "nichts", "si\u00b7cher", "an\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VAFIN", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Lern eingezogen seyn, und fleuch die frechen buben,", "tokens": ["Lern", "ein\u00b7ge\u00b7zo\u00b7gen", "seyn", ",", "und", "fleuch", "die", "fre\u00b7chen", "bu\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAINF", "$,", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mit denen du zuvor bekant gewesen bist.", "tokens": ["Mit", "de\u00b7nen", "du", "zu\u00b7vor", "be\u00b7kant", "ge\u00b7we\u00b7sen", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADJD", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Fleuch gro\u00dfer herren h\u00f6f\u2019, und alle diese stuben,", "tokens": ["Fleuch", "gro\u00b7\u00dfer", "her\u00b7ren", "h\u00f6f'", ",", "und", "al\u00b7le", "die\u00b7se", "stu\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "$,", "KON", "PIAT", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wo man der redligkeit und menschen-furcht vergi\u00dft.", "tokens": ["Wo", "man", "der", "red\u00b7lig\u00b7keit", "und", "men\u00b7schen\u00b7furcht", "ver\u00b7gi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Denn trug und heucheley geziemet keinen Christen.", "tokens": ["Denn", "trug", "und", "heu\u00b7che\u00b7ley", "ge\u00b7zie\u00b7met", "kei\u00b7nen", "Chris\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die einfalt stehet dir am allerbesten an.", "tokens": ["Die", "ein\u00b7falt", "ste\u00b7het", "dir", "am", "al\u00b7ler\u00b7bes\u00b7ten", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Geselle dich allein zu rechten pietisten,", "tokens": ["Ge\u00b7sel\u00b7le", "dich", "al\u00b7lein", "zu", "rech\u00b7ten", "pie\u00b7tis\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Ob sie die rohe welt schon nicht vertragen kan.", "tokens": ["Ob", "sie", "die", "ro\u00b7he", "welt", "schon", "nicht", "ver\u00b7tra\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "PTKNEG", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Mit diesen mache dich, durch unschuld-volle sitten", "tokens": ["Mit", "die\u00b7sen", "ma\u00b7che", "dich", ",", "durch", "un\u00b7schuld\u00b7vol\u00b7le", "sit\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und unterredungen nach Christi sinn, gemein.", "tokens": ["Und", "un\u00b7ter\u00b7re\u00b7dun\u00b7gen", "nach", "Chris\u00b7ti", "sinn", ",", "ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "NN", "$,", "ADJD", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.19": {"text": "Der weisheit sch\u00fcler sind bey weisen wohl gelitten,", "tokens": ["Der", "weis\u00b7heit", "sch\u00fc\u00b7ler", "sind", "bey", "wei\u00b7sen", "wohl", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "APPR", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wenn sie nur in der zucht still und gehorsam seyn.", "tokens": ["Wenn", "sie", "nur", "in", "der", "zucht", "still", "und", "ge\u00b7hor\u00b7sam", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "PTKVZ", "KON", "ADJD", "VAINF", "$."], "meter": "--+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Bewahre deinen schritt, und geh der weiber m\u00fc\u00dfig:", "tokens": ["Be\u00b7wah\u00b7re", "dei\u00b7nen", "schritt", ",", "und", "geh", "der", "wei\u00b7ber", "m\u00fc\u00b7\u00dfig", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "VVFIN", "$,", "KON", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Jhr umgang st\u00fcrtzet dich sonst in gefahr und spott:", "tokens": ["Ihr", "um\u00b7gang", "st\u00fcrt\u00b7zet", "dich", "sonst", "in", "ge\u00b7fahr", "und", "spott", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Bey ihnen wird man bald der keuschheit \u00fcberdr\u00fc\u00dfig;", "tokens": ["Bey", "ih\u00b7nen", "wird", "man", "bald", "der", "keuschheit", "\u00fc\u00b7berd\u00b7r\u00fc\u00b7\u00dfig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIS", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Drum fleuch sie, und befiehl die frommen deinem Gott.", "tokens": ["Drum", "fleuch", "sie", ",", "und", "be\u00b7fiehl", "die", "from\u00b7men", "dei\u00b7nem", "Gott", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "KON", "ADJD", "ART", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Mit engeln und mit Gott allein gemeinschafft pflegen", "tokens": ["Mit", "en\u00b7geln", "und", "mit", "Gott", "al\u00b7lein", "ge\u00b7mein\u00b7schafft", "pfle\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "ADV", "VVPP", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ist wol die sicherste gesellschafft auf der welt.", "tokens": ["Ist", "wol", "die", "si\u00b7chers\u00b7te", "ge\u00b7sell\u00b7schafft", "auf", "der", "welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.27": {"text": "Wer andre kundschafft sucht, der wagt es sehr verwegen;", "tokens": ["Wer", "and\u00b7re", "kund\u00b7schafft", "sucht", ",", "der", "wagt", "es", "sehr", "ver\u00b7we\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Drum sey dein hertze stets dem himmel zugesellt.", "tokens": ["Drum", "sey", "dein", "hert\u00b7ze", "stets", "dem", "him\u00b7mel", "zu\u00b7ge\u00b7sellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPOSAT", "NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die liebe gegen all\u2019 und iede menschen \u00fcben,", "tokens": ["Die", "lie\u00b7be", "ge\u00b7gen", "all'", "und", "ie\u00b7de", "men\u00b7schen", "\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "APPR", "PIAT", "KON", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Steht Christlich; doch zu viel gemeinschafft ist nicht gut.", "tokens": ["Steht", "Christ\u00b7lich", ";", "doch", "zu", "viel", "ge\u00b7mein\u00b7schafft", "ist", "nicht", "gut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$.", "ADV", "PTKA", "PIS", "VVPP", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Man kan die br\u00fcder auch in seiner kammer lieben,", "tokens": ["Man", "kan", "die", "br\u00fc\u00b7der", "auch", "in", "sei\u00b7ner", "kam\u00b7mer", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "ADJA", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Indem man vor ihr heyl bitt\u2019 und gebete thut.", "tokens": ["In\u00b7dem", "man", "vor", "ihr", "heyl", "bitt'", "und", "ge\u00b7be\u00b7te", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPOSAT", "NN", "PTKANT", "KON", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ein mann, der in der fern als eine sonne scheinet,", "tokens": ["Ein", "mann", ",", "der", "in", "der", "fern", "als", "ei\u00b7ne", "son\u00b7ne", "schei\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "ADJD", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Verliehret in der n\u00e4h offt allen glantz und schein;", "tokens": ["Ver\u00b7lieh\u00b7ret", "in", "der", "n\u00e4h", "offt", "al\u00b7len", "glantz", "und", "schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJD", "ADV", "PIAT", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Denn was der kluge mund gleich auszurichten meynet,", "tokens": ["Denn", "was", "der", "klu\u00b7ge", "mund", "gleich", "aus\u00b7zu\u00b7rich\u00b7ten", "mey\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "NN", "ADV", "VVIZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Das reissen meistentheils die fremden sitten ein.", "tokens": ["Das", "reis\u00b7sen", "meis\u00b7ten\u00b7theils", "die", "frem\u00b7den", "sit\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}