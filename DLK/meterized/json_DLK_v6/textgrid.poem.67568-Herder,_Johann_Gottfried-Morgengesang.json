{"textgrid.poem.67568": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Morgengesang", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erwach, erwach am neuen Morgen", "tokens": ["Er\u00b7wach", ",", "er\u00b7wach", "am", "neu\u00b7en", "Mor\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit allem neuen fr\u00fchen Morgenchor,", "tokens": ["Mit", "al\u00b7lem", "neu\u00b7en", "fr\u00fc\u00b7hen", "Mor\u00b7gen\u00b7chor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du meine Harf', und t\u00f6n' ins frohe Weltget\u00fcmmel", "tokens": ["Du", "mei\u00b7ne", "Ha\u00b7rf'", ",", "und", "t\u00f6n'", "ins", "fro\u00b7he", "Welt\u00b7ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "$,", "KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Mit voller Sait' hinein!", "tokens": ["Mit", "vol\u00b7ler", "Sait'", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Denn in das frohe Weltget\u00fcmmel", "tokens": ["Denn", "in", "das", "fro\u00b7he", "Welt\u00b7ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geh\u00f6rst auch, schwachbesaitet, Du, ins Chor", "tokens": ["Ge\u00b7h\u00f6rst", "auch", ",", "schwach\u00b7be\u00b7sai\u00b7tet", ",", "Du", ",", "ins", "Chor"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "$,", "VVFIN", "$,", "PPER", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der sch\u00f6nen Morgenstern' und fr\u00fcher Lerchenstimmen", "tokens": ["Der", "sch\u00f6\u00b7nen", "Mor\u00b7gen\u00b7stern'", "und", "fr\u00fc\u00b7her", "Ler\u00b7chen\u00b7stim\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und alles Sph\u00e4renklangs.", "tokens": ["Und", "al\u00b7les", "Sph\u00e4\u00b7ren\u00b7klangs", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie wandeln dort, die S\u00e4ngerinnen,", "tokens": ["Sie", "wan\u00b7deln", "dort", ",", "die", "S\u00e4n\u00b7ge\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Morgenstern', und singen ihn heran,", "tokens": ["Die", "Mor\u00b7gen\u00b7stern'", ",", "und", "sin\u00b7gen", "ihn", "he\u00b7ran", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der sie mit Vaterblicken segnet, todte Welten", "tokens": ["Der", "sie", "mit", "Va\u00b7ter\u00b7bli\u00b7cken", "seg\u00b7net", ",", "tod\u00b7te", "Wel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vom Schlummer l\u00e4chelt auf.", "tokens": ["Vom", "Schlum\u00b7mer", "l\u00e4\u00b7chelt", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du auch ein Morgenstern, o Harfe,", "tokens": ["Du", "auch", "ein", "Mor\u00b7gens\u00b7tern", ",", "o", "Har\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Empfang ihn, der ein J\u00fcngling kommen wird", "tokens": ["Emp\u00b7fang", "ihn", ",", "der", "ein", "J\u00fcng\u00b7ling", "kom\u00b7men", "wird"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "PRELS", "ART", "NN", "VVINF", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und g\u00fcldne Strahlen Dir auf Deine Saiten klingen", "tokens": ["Und", "g\u00fcld\u00b7ne", "Strah\u00b7len", "Dir", "auf", "Dei\u00b7ne", "Sai\u00b7ten", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wecken Deine Welt.", "tokens": ["Und", "we\u00b7cken", "Dei\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Erde T\u00f6chter wird er wecken,", "tokens": ["Der", "Er\u00b7de", "T\u00f6ch\u00b7ter", "wird", "er", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Blumen, mit der s\u00fc\u00dfen Liebe Pfeil,", "tokens": ["Die", "Blu\u00b7men", ",", "mit", "der", "s\u00fc\u00b7\u00dfen", "Lie\u00b7be", "Pfeil", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df sie sich wundern ihres neuen sch\u00f6nen Schmuckes", "tokens": ["Da\u00df", "sie", "sich", "wun\u00b7dern", "ih\u00b7res", "neu\u00b7en", "sch\u00f6\u00b7nen", "Schmu\u00b7ckes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und weinen Freudenthau.", "tokens": ["Und", "wei\u00b7nen", "Freu\u00b7dent\u00b7hau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Des Himmels Ch\u00f6re wird er wecken,", "tokens": ["Des", "Him\u00b7mels", "Ch\u00f6\u00b7re", "wird", "er", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die singenden Gefieder, da\u00df sie hoch", "tokens": ["Die", "sin\u00b7gen\u00b7den", "Ge\u00b7fie\u00b7der", ",", "da\u00df", "sie", "hoch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf L\u00fcften schweben und den Flug mit T\u00f6nen steuern", "tokens": ["Auf", "L\u00fcf\u00b7ten", "schwe\u00b7ben", "und", "den", "Flug", "mit", "T\u00f6\u00b7nen", "steu\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVINF", "KON", "ART", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und f\u00fcllen Wald und Thal.", "tokens": ["Und", "f\u00fcl\u00b7len", "Wald", "und", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und Alle sollst Du sie beleben,", "tokens": ["Und", "Al\u00b7le", "sollst", "Du", "sie", "be\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Stimmen Erstgeborne, Tochter Du", "tokens": ["Der", "Stim\u00b7men", "Erst\u00b7ge\u00b7bor\u00b7ne", ",", "Toch\u00b7ter", "Du"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NE", "$,", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Ewigen! Sieh, wie dort schon die Himmelsschwinge,", "tokens": ["Des", "E\u00b7wi\u00b7gen", "!", "Sieh", ",", "wie", "dort", "schon", "die", "Him\u00b7mels\u00b7schwin\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$.", "NE", "$,", "PWAV", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die Lerche, Dir entsteigt!", "tokens": ["Die", "Ler\u00b7che", ",", "Dir", "ent\u00b7steigt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und jene Gipfel, wie sie rauschen", "tokens": ["Und", "je\u00b7ne", "Gip\u00b7fel", ",", "wie", "sie", "rau\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "$,", "PWAV", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Kommenden! Entz\u00fcckungsschauer flie\u00dft", "tokens": ["Dem", "Kom\u00b7men\u00b7den", "!", "Ent\u00b7z\u00fc\u00b7ckungs\u00b7schau\u00b7er", "flie\u00dft"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durch alle Wesen, und in schwarzen, schweren Wellen", "tokens": ["Durch", "al\u00b7le", "We\u00b7sen", ",", "und", "in", "schwar\u00b7zen", ",", "schwe\u00b7ren", "Wel\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "KON", "APPR", "VVINF", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erhebt die Nacht sich fort.", "tokens": ["Er\u00b7hebt", "die", "Nacht", "sich", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "O herrsch umher, Du Harfe Gottes,", "tokens": ["O", "herrsch", "um\u00b7her", ",", "Du", "Har\u00b7fe", "Got\u00b7tes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PTKVZ", "$,", "PPER", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So weit der sch\u00f6ne Rosenj\u00fcngling strahlt;", "tokens": ["So", "weit", "der", "sch\u00f6\u00b7ne", "Ro\u00b7sen\u00b7j\u00fcng\u00b7ling", "strahlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er herrscht am weiten Himmel, und die Dich beseelet,", "tokens": ["Er", "herrscht", "am", "wei\u00b7ten", "Him\u00b7mel", ",", "und", "die", "Dich", "be\u00b7see\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,", "KON", "ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist Erdek\u00f6nigin.", "tokens": ["Ist", "Er\u00b7de\u00b7k\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wohin er g\u00fcldne Strahlen sendet,", "tokens": ["Wo\u00b7hin", "er", "g\u00fcld\u00b7ne", "Strah\u00b7len", "sen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie weit sein Zelt der blaue Himmel zieht,", "tokens": ["Wie", "weit", "sein", "Zelt", "der", "blau\u00b7e", "Him\u00b7mel", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist Dein Gebiet, o Seele; jene sch\u00f6ne H\u00fctte", "tokens": ["Ist", "Dein", "Ge\u00b7biet", ",", "o", "See\u00b7le", ";", "je\u00b7ne", "sch\u00f6\u00b7ne", "H\u00fct\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "FM", "NN", "$.", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist hoch f\u00fcr Dich gew\u00f6lbt.", "tokens": ["Ist", "hoch", "f\u00fcr", "Dich", "ge\u00b7w\u00f6lbt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "All Deines Blickes hohes Ende,", "tokens": ["All", "Dei\u00b7nes", "Bli\u00b7ckes", "ho\u00b7hes", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "All Deines Ganges End' ist Himmel nur;", "tokens": ["All", "Dei\u00b7nes", "Gan\u00b7ges", "End'", "ist", "Him\u00b7mel", "nur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "VAFIN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Du, die in mir denkt, bist Sonne; was Du denkest,", "tokens": ["Und", "Du", ",", "die", "in", "mir", "denkt", ",", "bist", "Son\u00b7ne", ";", "was", "Du", "den\u00b7kest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,", "VAFIN", "NN", "$.", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ist mehr als Lichtesstrahl.", "tokens": ["Ist", "mehr", "als", "Lich\u00b7tes\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KOUS", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Wer bist Du, neuerwachte Seele,", "tokens": ["Wer", "bist", "Du", ",", "neu\u00b7er\u00b7wach\u00b7te", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die in sich selbst als eine Sonne blickt", "tokens": ["Die", "in", "sich", "selbst", "als", "ei\u00b7ne", "Son\u00b7ne", "blickt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PRF", "ADV", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und gie\u00dft in ", "tokens": ["Und", "gie\u00dft", "in"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Der Farben ganzes Meer?", "tokens": ["Der", "Far\u00b7ben", "gan\u00b7zes", "Meer", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Wer bist Du, die auf Welten blicket", "tokens": ["Wer", "bist", "Du", ",", "die", "auf", "Wel\u00b7ten", "bli\u00b7cket"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "$,", "PRELS", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und aus sich selber neue Welten schafft", "tokens": ["Und", "aus", "sich", "sel\u00b7ber", "neu\u00b7e", "Wel\u00b7ten", "schafft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PRF", "ADV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und, wie die Sonne dort, die Wesen rings begl\u00e4nzet", "tokens": ["Und", ",", "wie", "die", "Son\u00b7ne", "dort", ",", "die", "We\u00b7sen", "rings", "be\u00b7gl\u00e4n\u00b7zet"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "ADV", "$,", "ART", "NN", "ADV", "VVFIN