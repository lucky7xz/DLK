{"textgrid.poem.31088": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "202.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Thummius wil alle Tage sich in Ritterstieffeln weisen;", "tokens": ["Thum\u00b7mi\u00b7us", "wil", "al\u00b7le", "Ta\u00b7ge", "sich", "in", "Rit\u00b7ter\u00b7stief\u00b7feln", "wei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PIAT", "NN", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Denn er pflegt durch Stub und Kammer t\u00e4glich au\u00df und ein zu reisen.", "tokens": ["Denn", "er", "pflegt", "durch", "Stub", "und", "Kam\u00b7mer", "t\u00e4g\u00b7lich", "au\u00df", "und", "ein", "zu", "rei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "ADJD", "PTKVZ", "KON", "ART", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}