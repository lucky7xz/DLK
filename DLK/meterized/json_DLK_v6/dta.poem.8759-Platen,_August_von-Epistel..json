{"dta.poem.8759": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Epistel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Freund, du, welchen der Kunst gl\u00fchende Liebe be-", "tokens": ["Freund", ",", "du", ",", "wel\u00b7chen", "der", "Kunst", "gl\u00fc\u00b7hen\u00b7de", "Lie\u00b7be", "be"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "$,", "PWAT", "ART", "NN", "ADJA", "NN", "TRUNC"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wirst mit dem Tadel mich nicht unw\u00fcrdiger Musse", "tokens": ["Wirst", "mit", "dem", "Ta\u00b7del", "mich", "nicht", "un\u00b7w\u00fcr\u00b7di\u00b7ger", "Mus\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "PPER", "PTKNEG", "ADJA", "NN"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Die ich im stillen Bezirk dieser Gefilde gesucht.", "tokens": ["Die", "ich", "im", "stil\u00b7len", "Be\u00b7zirk", "die\u00b7ser", "Ge\u00b7fil\u00b7de", "ge\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "ADJA", "NN", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie mir aber allein hingehn die gefl\u00fcgelten Tage", "tokens": ["Wie", "mir", "a\u00b7ber", "al\u00b7lein", "hin\u00b7gehn", "die", "ge\u00b7fl\u00fc\u00b7gel\u00b7ten", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+-+-+---+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Fragst du, w\u00e4hrend ich fern lebe der st\u00e4dtischen Welt?", "tokens": ["Fragst", "du", ",", "w\u00e4h\u00b7rend", "ich", "fern", "le\u00b7be", "der", "st\u00e4d\u00b7ti\u00b7schen", "Welt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "H\u00e4ufig bewund'r ich rings, ausruhend am H\u00fcgel, die", "tokens": ["H\u00e4u\u00b7fig", "be\u00b7wun\u00b7d'r", "ich", "rings", ",", "aus\u00b7ru\u00b7hend", "am", "H\u00fc\u00b7gel", ",", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "$,", "VVPP", "APPRART", "NN", "$,", "PRELS"], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Wo den beweglichen Schirm Buche mir, Esche mir beut;", "tokens": ["Wo", "den", "be\u00b7weg\u00b7li\u00b7chen", "Schirm", "Bu\u00b7che", "mir", ",", "E\u00b7sche", "mir", "beut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "NN", "PPER", "$,", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "S\u00fc\u00dfe, doch seltene Thr\u00e4nen, wie liebende J\u00fcnglinge", "tokens": ["S\u00fc\u00b7\u00dfe", ",", "doch", "sel\u00b7te\u00b7ne", "Thr\u00e4\u00b7nen", ",", "wie", "lie\u00b7ben\u00b7de", "J\u00fcng\u00b7lin\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "ADJA", "NN", "$,", "PWAV", "ADJA", "NN"], "meter": "+--+--+--+--++-", "measure": "diphilius"}, "line.4": {"text": "Seh' ich, des Thals Fr\u00fchthau, hangen am Rosenge-", "tokens": ["Seh'", "ich", ",", "des", "Thals", "Fr\u00fch\u00b7thau", ",", "han\u00b7gen", "am", "Ro\u00b7sen\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "NN", "$,", "VVFIN", "APPRART", "TRUNC"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Wenn ich zur\u00fcck von dem Wallfahrtsort, von der bunten", "tokens": ["Wenn", "ich", "zu\u00b7r\u00fcck", "von", "dem", "Wall\u00b7fahr\u00b7tsort", ",", "von", "der", "bun\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKVZ", "APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Kehre, dem heitersten Sitz, w\u00e4hrend die Sonne sich", "tokens": ["Keh\u00b7re", ",", "dem", "hei\u00b7ters\u00b7ten", "Sitz", ",", "w\u00e4h\u00b7rend", "die", "Son\u00b7ne", "sich"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "PRF"], "meter": "+--+--++--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Zweyfach l\u00e4chelt mich dann dies gartenumzingelte Dorf an,", "tokens": ["Zwey\u00b7fach", "l\u00e4\u00b7chelt", "mich", "dann", "dies", "gar\u00b7ten\u00b7um\u00b7zin\u00b7gel\u00b7te", "Dorf", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PDS", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Bald am Wiesengestad, bald im gegl\u00e4tteten See;", "tokens": ["Bald", "am", "Wie\u00b7sen\u00b7ge\u00b7stad", ",", "bald", "im", "ge\u00b7gl\u00e4t\u00b7te\u00b7ten", "See", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Oft auch freu' ich mich dann in dem Kahne des trau-", "tokens": ["Oft", "auch", "freu'", "ich", "mich", "dann", "in", "dem", "Kah\u00b7ne", "des", "trau"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "ART", "TRUNC"], "meter": "+-+-+++-+--+", "measure": "iambic.septa.chol"}, "line.10": {"text": "Wenn auf flachem Krystall Zirkel an Zirkel sich reiht,", "tokens": ["Wenn", "auf", "fla\u00b7chem", "Krys\u00b7tall", "Zir\u00b7kel", "an", "Zir\u00b7kel", "sich", "reiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "NN", "APPR", "NN", "PRF", "VVFIN", "$,"], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Oefter des seltenen Flors gro\u00dfblumiger Alpengew\u00e4chse,", "tokens": ["Oef\u00b7ter", "des", "sel\u00b7te\u00b7nen", "Flors", "gro\u00df\u00b7blu\u00b7mi\u00b7ger", "Al\u00b7pen\u00b7ge\u00b7w\u00e4ch\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.12": {"text": "Wenn ich bewaldeter H\u00f6h'n ruhige Gipfel erstieg.", "tokens": ["Wenn", "ich", "be\u00b7wal\u00b7de\u00b7ter", "H\u00f6h'n", "ru\u00b7hi\u00b7ge", "Gip\u00b7fel", "er\u00b7stieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch wer ist's, der sich zu dem einsam wallenden J\u00fcng-", "tokens": ["Doch", "wer", "ist's", ",", "der", "sich", "zu", "dem", "ein\u00b7sam", "wal\u00b7len\u00b7den", "J\u00fcng"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VAFIN", "$,", "PRELS", "PRF", "APPR", "ART", "ADJD", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Als willkommener Freund, bildend und liebend gesellt?", "tokens": ["Als", "will\u00b7kom\u00b7me\u00b7ner", "Freund", ",", "bil\u00b7dend", "und", "lie\u00b7bend", "ge\u00b7sellt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "+--+-++--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Flaccus, apulischer S\u00e4nger, du bist's! Frohsinnige Weisheit", "tokens": ["Flac\u00b7cus", ",", "a\u00b7pu\u00b7li\u00b7scher", "S\u00e4n\u00b7ger", ",", "du", "bist's", "!", "Froh\u00b7sin\u00b7ni\u00b7ge", "Weis\u00b7heit"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "PPER", "NE", "$.", "ADJA", "NN"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Lehren, und gl\u00fccklichen Muth, deine Ges\u00e4nge das", "tokens": ["Leh\u00b7ren", ",", "und", "gl\u00fcck\u00b7li\u00b7chen", "Muth", ",", "dei\u00b7ne", "Ge\u00b7s\u00e4n\u00b7ge", "das"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KON", "ADJA", "NN", "$,", "PPOSAT", "NN", "ART"], "meter": "+--+--++--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "M\u00e4\u00dfig im Lauf der verg\u00e4nglichen Zeit zu genie\u00dfen ge-", "tokens": ["M\u00e4\u00b7\u00dfig", "im", "Lauf", "der", "ver\u00b7g\u00e4ng\u00b7li\u00b7chen", "Zeit", "zu", "ge\u00b7nie\u00b7\u00dfen", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "TRUNC"], "meter": "+--+--+--+--+-+", "measure": "diphilius"}, "line.6": {"text": "Neben die Bilder des Tods stellst du der Freude", "tokens": ["Ne\u00b7ben", "die", "Bil\u00b7der", "des", "Tods", "stellst", "du", "der", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.7": {"text": "F\u00fchrst mich nach dem begl\u00fcckten Tarent, in's l\u00e4ndliche", "tokens": ["F\u00fchrst", "mich", "nach", "dem", "be\u00b7gl\u00fcck\u00b7ten", "Ta\u00b7rent", ",", "in's", "l\u00e4nd\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PRF", "APPR", "ART", "ADJA", "NN", "$,", "APPRART", "ADJA"], "meter": "+--+-+--+-+--", "measure": "iambic.penta.invert"}, "line.8": {"text": "Wo du die Wunder von Rom, ohne zu seufzen", "tokens": ["Wo", "du", "die", "Wun\u00b7der", "von", "Rom", ",", "oh\u00b7ne", "zu", "seuf\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "NE", "$,", "KOUI", "PTKZU", "VVINF"], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Oder ich lerne von dir, zum k\u00fchlen Pr\u00e4neste dir folgend,", "tokens": ["O\u00b7der", "ich", "ler\u00b7ne", "von", "dir", ",", "zum", "k\u00fch\u00b7len", "Pr\u00e4\u00b7nes\u00b7te", "dir", "fol\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPER", "$,", "APPRART", "ADJA", "NN", "PPER", "ADJD", "$,"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.10": {"text": "Wie man sinnigen Geists lese den Vater Homer.", "tokens": ["Wie", "man", "sin\u00b7ni\u00b7gen", "Geists", "le\u00b7se", "den", "Va\u00b7ter", "Ho\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJA", "NN", "VVFIN", "ART", "NN", "NE", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Wahres verk\u00fcndetest du, denn selbst in die W\u00e4lder des", "tokens": ["Wah\u00b7res", "ver\u00b7k\u00fcn\u00b7de\u00b7test", "du", ",", "denn", "selbst", "in", "die", "W\u00e4l\u00b7der", "des"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "VVFIN", "PPER", "$,", "KON", "ADV", "APPR", "ART", "NN", "ART"], "meter": "+--+-+--+--+-+", "measure": "iambic.hexa.invert"}, "line.12": {"text": "Drang des latinischen Lieds bl\u00fchende Stimme hin-", "tokens": ["Drang", "des", "la\u00b7ti\u00b7ni\u00b7schen", "Lieds", "bl\u00fc\u00b7hen\u00b7de", "Stim\u00b7me", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "ADJA", "NN", "TRUNC"], "meter": "+-+-+-++--+-+", "measure": "trochaic.septa.relaxed"}, "line.13": {"text": "Deines Augusts Alt\u00e4re zerbr\u00f6ckelten, deine Ges\u00e4nge", "tokens": ["Dei\u00b7nes", "Au\u00b7gusts", "Al\u00b7t\u00e4\u00b7re", "zer\u00b7br\u00f6\u00b7ckel\u00b7ten", ",", "dei\u00b7ne", "Ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$,", "PPOSAT", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Nicht, um's r\u00f6mische Haupt fliegen dir V\u00f6gel des", "tokens": ["Nicht", ",", "um's", "r\u00f6\u00b7mi\u00b7sche", "Haupt", "flie\u00b7gen", "dir", "V\u00f6\u00b7gel", "des"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "$,", "NE", "ADJA", "NN", "VVFIN", "PPER", "NN", "ART"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.15": {"text": "Strebt auch Mancher, wie du, stets hofft er die Krone", "tokens": ["Strebt", "auch", "Man\u00b7cher", ",", "wie", "du", ",", "stets", "hofft", "er", "die", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PIS", "$,", "PWAV", "PPER", "$,", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Und es bewahrt kein Baum k\u00f6stliche Zweige f\u00fcr ihn.", "tokens": ["Und", "es", "be\u00b7wahrt", "kein", "Baum", "k\u00f6st\u00b7li\u00b7che", "Zwei\u00b7ge", "f\u00fcr", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-++--+--+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Einst wohl trauert er noch um der Jahre verschwendetes", "tokens": ["Einst", "wohl", "trau\u00b7ert", "er", "noch", "um", "der", "Jah\u00b7re", "ver\u00b7schwen\u00b7de\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "ADJA"], "meter": "+-+--+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "Leicht zwar ist der Besitz, doch das Erringen, wie", "tokens": ["Leicht", "zwar", "ist", "der", "Be\u00b7sitz", ",", "doch", "das", "Er\u00b7rin\u00b7gen", ",", "wie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "ADV", "VAFIN", "ART", "NN", "$,", "ADV", "ART", "NN", "$,", "PWAV"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "So um den blendenden Nacken der F\u00fcrstin bilden die", "tokens": ["So", "um", "den", "blen\u00b7den\u00b7den", "Na\u00b7cken", "der", "F\u00fcrs\u00b7tin", "bil\u00b7den", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "ART"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Zierliche Ketten, sie tr\u00e4gt stolz ihr Geschmeide zur", "tokens": ["Zier\u00b7li\u00b7che", "Ket\u00b7ten", ",", "sie", "tr\u00e4gt", "stolz", "ihr", "Ge\u00b7schmei\u00b7de", "zur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "APPRART"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.21": {"text": "Aber bedenkt sie, wie oft in zerbrechlicher Glocke der", "tokens": ["A\u00b7ber", "be\u00b7denkt", "sie", ",", "wie", "oft", "in", "zer\u00b7brech\u00b7li\u00b7cher", "Glo\u00b7cke", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "ADV", "APPR", "ADJA", "NN", "ART"], "meter": "+--+--+--+--+-+", "measure": "diphilius"}, "line.22": {"text": "Um den entbehrlichen Schmuck fuhr in die Tiefe", "tokens": ["Um", "den", "ent\u00b7behr\u00b7li\u00b7chen", "Schmuck", "fuhr", "in", "die", "Tie\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}}}}}