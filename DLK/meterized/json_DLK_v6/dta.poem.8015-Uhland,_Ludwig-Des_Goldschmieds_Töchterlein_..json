{"dta.poem.8015": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Des Goldschmieds T\u00f6chterlein .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Goldschmied in der Bude stand", "tokens": ["Ein", "Gold\u00b7schmied", "in", "der", "Bu\u00b7de", "stand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei Perl\u2019 und Edelstein:", "tokens": ["Bei", "Perl'", "und", "E\u00b7del\u00b7stein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201edas beste Kleinod, das ich fand,", "tokens": ["\u201e", "das", "bes\u00b7te", "Klei\u00b7nod", ",", "das", "ich", "fand", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das bist doch du, Helene,", "tokens": ["Das", "bist", "doch", "du", ",", "He\u00b7le\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PPER", "$,", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein theures T\u00f6chterlein!\u201c", "tokens": ["Mein", "theu\u00b7res", "T\u00f6ch\u00b7ter\u00b7lein", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein schmucker Ritter trat herein:", "tokens": ["Ein", "schmu\u00b7cker", "Rit\u00b7ter", "trat", "her\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201ewillkommen, M\u00e4gdlein traut!", "tokens": ["\u201e", "will\u00b7kom\u00b7men", ",", "M\u00e4gd\u00b7lein", "traut", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Willkommen, lieber Goldschmied mein!", "tokens": ["Will\u00b7kom\u00b7men", ",", "lie\u00b7ber", "Gold\u00b7schmied", "mein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mach mir ein k\u00f6stlich Kr\u00e4nzchen", "tokens": ["Mach", "mir", "ein", "k\u00f6st\u00b7lich", "Kr\u00e4nz\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fcr meine s\u00fc\u00dfe Braut!\u201c", "tokens": ["F\u00fcr", "mei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Braut", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und als das Kr\u00e4nzlein war bereit", "tokens": ["Und", "als", "das", "Kr\u00e4nz\u00b7lein", "war", "be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und spielt\u2019 in reichem Glanz,", "tokens": ["Und", "spielt'", "in", "rei\u00b7chem", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da h\u00e4ngt\u2019 Helen\u2019 in Traurigkeit,", "tokens": ["Da", "h\u00e4ngt'", "He\u00b7len'", "in", "Trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl als sie war alleine,", "tokens": ["Wohl", "als", "sie", "war", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "An ihren Arm den Kranz.", "tokens": ["An", "ih\u00b7ren", "Arm", "den", "Kranz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u201each! wunderselig ist die Braut,", "tokens": ["\u201e", "ach", "!", "wun\u00b7der\u00b7se\u00b7lig", "ist", "die", "Braut", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$.", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die\u2019s Kr\u00f6nlein tragen soll.", "tokens": ["Die's", "Kr\u00f6n\u00b7lein", "tra\u00b7gen", "soll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach, schenkte mir der Ritter traut", "tokens": ["Ach", ",", "schenk\u00b7te", "mir", "der", "Rit\u00b7ter", "traut"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Kr\u00e4nzlein nur von Rosen,", "tokens": ["Ein", "Kr\u00e4nz\u00b7lein", "nur", "von", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie w\u00e4r\u2019 ich freudenvoll!\u201c", "tokens": ["Wie", "w\u00e4r'", "ich", "freu\u00b7den\u00b7voll", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nicht lang, der Ritter trat herein,", "tokens": ["Nicht", "lang", ",", "der", "Rit\u00b7ter", "trat", "her\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Kr\u00e4nzlein wohl beschaut\u2019:", "tokens": ["Das", "Kr\u00e4nz\u00b7lein", "wohl", "be\u00b7schaut'", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201eo fasse, lieber Goldschmied mein,", "tokens": ["\u201e", "o", "fas\u00b7se", ",", "lie\u00b7ber", "Gold\u00b7schmied", "mein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "$,", "ADV", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Ringlein mit Demanten", "tokens": ["Ein", "Rin\u00b7glein", "mit", "De\u00b7man\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fcr meine s\u00fc\u00dfe Braut!\u201c", "tokens": ["F\u00fcr", "mei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Braut", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und als das Ringlein war bereit", "tokens": ["Und", "als", "das", "Rin\u00b7glein", "war", "be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit theurem Demantstein,", "tokens": ["Mit", "theu\u00b7rem", "De\u00b7mant\u00b7stein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da steckt\u2019 Helen\u2019 in Traurigkeit,", "tokens": ["Da", "steckt'", "He\u00b7len'", "in", "Trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl als sie war alleine,", "tokens": ["Wohl", "als", "sie", "war", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es halb an\u2019s Fingerlein.", "tokens": ["Es", "halb", "an's", "Fin\u00b7ger\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u201each! wunderselig ist die Braut,", "tokens": ["\u201e", "ach", "!", "wun\u00b7der\u00b7se\u00b7lig", "ist", "die", "Braut", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$.", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die\u2019s Ringlein tragen soll.", "tokens": ["Die's", "Rin\u00b7glein", "tra\u00b7gen", "soll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach, schenkte mir der Ritter traut", "tokens": ["Ach", ",", "schenk\u00b7te", "mir", "der", "Rit\u00b7ter", "traut"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur seines Haars ein L\u00f6cklein,", "tokens": ["Nur", "sei\u00b7nes", "Haars", "ein", "L\u00f6c\u00b7klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie w\u00e4r\u2019 ich freudenvoll!\u201c", "tokens": ["Wie", "w\u00e4r'", "ich", "freu\u00b7den\u00b7voll", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Nicht lang, der Ritter trat herein,", "tokens": ["Nicht", "lang", ",", "der", "Rit\u00b7ter", "trat", "her\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Ringlein wohl beschaut\u2019:", "tokens": ["Das", "Rin\u00b7glein", "wohl", "be\u00b7schaut'", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201edu hast, o lieber Goldschmied mein!", "tokens": ["\u201e", "du", "hast", ",", "o", "lie\u00b7ber", "Gold\u00b7schmied", "mein", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "$,", "FM", "ADV", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gar fein gemacht die Gaben", "tokens": ["Gar", "fein", "ge\u00b7macht", "die", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fcr meine s\u00fc\u00dfe Braut.", "tokens": ["F\u00fcr", "mei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Braut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Doch da\u00df ich wisse, wie ihr\u2019s steh\u2019,", "tokens": ["Doch", "da\u00df", "ich", "wis\u00b7se", ",", "wie", "ih\u00b7r's", "steh'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Tritt, sch\u00f6ne Maid, herzu!", "tokens": ["Tritt", ",", "sch\u00f6\u00b7ne", "Maid", ",", "her\u00b7zu", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich an dir zur Probe seh\u2019", "tokens": ["Da\u00df", "ich", "an", "dir", "zur", "Pro\u00b7be", "seh'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Brautschmuck meiner Liebsten,", "tokens": ["Den", "Brautsc\u00b7hmuck", "mei\u00b7ner", "Liebs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie ist so sch\u00f6n, wie du.\u201c", "tokens": ["Sie", "ist", "so", "sch\u