{"textgrid.poem.34555": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: K\u00e4mmst du dir in Duft und Dunkel", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00e4mmst du dir in Duft und Dunkel", "tokens": ["K\u00e4mmst", "du", "dir", "in", "Duft", "und", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "deine krausen, reichen Haare,", "tokens": ["dei\u00b7ne", "krau\u00b7sen", ",", "rei\u00b7chen", "Haa\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "ist's, als ob ein blau Gefunkel", "tokens": ["ist's", ",", "als", "ob", "ein", "blau", "Ge\u00b7fun\u00b7kel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "$,", "KOKOM", "KOUS", "ART", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "knisternd dir vom Haupte fahre.", "tokens": ["knis\u00b7ternd", "dir", "vom", "Haup\u00b7te", "fah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und ich sp\u00fcrte scheue Wonne:", "tokens": ["Und", "ich", "sp\u00fcr\u00b7te", "scheu\u00b7e", "Won\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie uns Kr\u00e4fte gross umwanden,", "tokens": ["Wie", "uns", "Kr\u00e4f\u00b7te", "gross", "um\u00b7wan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "die an Tages tr\u00fcber Sonne", "tokens": ["die", "an", "Ta\u00b7ges", "tr\u00fc\u00b7ber", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welt und Leute nie verstanden \u2013", "tokens": ["Welt", "und", "Leu\u00b7te", "nie", "ver\u00b7stan\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "wie der Wunderkreis der M\u00e4chte", "tokens": ["wie", "der", "Wun\u00b7der\u00b7kreis", "der", "M\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "uns geworben zur Erwarmung", "tokens": ["uns", "ge\u00b7wor\u00b7ben", "zur", "Er\u00b7war\u00b7mung"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVPP", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und im Ahnungsfest der N\u00e4chte", "tokens": ["und", "im", "Ah\u00b7nungs\u00b7fest", "der", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "still vollendet zur Umarmung.", "tokens": ["still", "voll\u00b7en\u00b7det", "zur", "Um\u00b7ar\u00b7mung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}