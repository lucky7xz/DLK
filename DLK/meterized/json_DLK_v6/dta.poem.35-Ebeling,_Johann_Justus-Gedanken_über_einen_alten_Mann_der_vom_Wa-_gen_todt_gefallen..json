{"dta.poem.35": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Gedanken  \n \u00fcber einen alten Mann der vom Wa-  \n gen todt gefallen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hier liegt ein alter Greis, der pl\u00f6zlich\nzwar gestorben,", "tokens": ["Hier", "liegt", "ein", "al\u00b7ter", "Greis", ",", "der", "pl\u00f6z\u00b7lich", "zwar", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch wie ein Simeon das Himmelreich", "tokens": ["Doch", "wie", "ein", "Si\u00b7me\u00b7on", "das", "Him\u00b7mel\u00b7reich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NE", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Fall vom Wagen hat sein Lebens-", "tokens": ["Ein", "Fall", "vom", "Wa\u00b7gen", "hat", "sein", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "VAFIN", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch ward die Seel dadurch zu JEsu hinger\u00fckt:", "tokens": ["Doch", "ward", "die", "Seel", "da\u00b7durch", "zu", "Je\u00b7su", "hin\u00b7ge\u00b7r\u00fckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PAV", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Engel haben ihn, wie auf Eli\u00e4 Wagen,", "tokens": ["Die", "En\u00b7gel", "ha\u00b7ben", "ihn", ",", "wie", "auf", "E\u00b7li\u00e4", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "PWAV", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und den entbundnen Geist in Abrams Schoos ge-", "tokens": ["Und", "den", "ent\u00b7bund\u00b7nen", "Geist", "in", "Ab\u00b7rams", "Schoos", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "NE", "NE", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Jhr ", "tokens": ["Ihr"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "So wird eur Todesfall ein seelig Auferstehn.", "tokens": ["So", "wird", "eur", "To\u00b7des\u00b7fall", "ein", "see\u00b7lig", "Auf\u00b7er\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ART", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}