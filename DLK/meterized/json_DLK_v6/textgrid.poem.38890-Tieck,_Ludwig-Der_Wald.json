{"textgrid.poem.38890": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Wald", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wandl' im Gr\u00fcnen,", "tokens": ["Wandl'", "im", "Gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Willst du die Blumen verstehn,", "tokens": ["Willst", "du", "die", "Blu\u00b7men", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Mu\u00dft du erst den Wald durchgehn.", "tokens": ["Mu\u00dft", "du", "erst", "den", "Wald", "durch\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist dir erschienen", "tokens": ["Ist", "dir", "er\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Der Sinn des Gr\u00fcnen", "tokens": ["Der", "Sinn", "des", "Gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Dann magst du die Blumen verstehn.", "tokens": ["Dann", "magst", "du", "die", "Blu\u00b7men", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Gr\u00fcn ist das erste Geheimni\u00df,", "tokens": ["Gr\u00fcn", "ist", "das", "ers\u00b7te", "Ge\u00b7heim\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "In das die Natur dich weiht,", "tokens": ["In", "das", "die", "Na\u00b7tur", "dich", "weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gr\u00fcn schm\u00fcckt rings die Welt,", "tokens": ["Gr\u00fcn", "schm\u00fcckt", "rings", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Ein lebendiger Odem,", "tokens": ["Ein", "le\u00b7ben\u00b7di\u00b7ger", "O\u00b7dem", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Ein lieblich Element,", "tokens": ["Ein", "lieb\u00b7lich", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das alles froh umgie\u00dft.", "tokens": ["Das", "al\u00b7les", "froh", "um\u00b7gie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gr\u00fcne bedeutet Lebensmuth,", "tokens": ["Gr\u00fc\u00b7ne", "be\u00b7deu\u00b7tet", "Le\u00b7bens\u00b7muth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Den Muth der frohen Unschuld,", "tokens": ["Den", "Muth", "der", "fro\u00b7hen", "Un\u00b7schuld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Den Muth zur Poesie.", "tokens": ["Den", "Muth", "zur", "Poe\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Gr\u00fcn sind alle Blumenknospen", "tokens": ["Gr\u00fcn", "sind", "al\u00b7le", "Blu\u00b7men\u00b7knos\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und die Bl\u00e4tter um die Blumen,", "tokens": ["Und", "die", "Bl\u00e4t\u00b7ter", "um", "die", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Dann entspringt der Farbenglanz", "tokens": ["Dann", "ent\u00b7springt", "der", "Far\u00b7ben\u00b7glanz"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Aus dem m\u00fctterlichen Gr\u00fcn.", "tokens": ["Aus", "dem", "m\u00fct\u00b7ter\u00b7li\u00b7chen", "Gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}