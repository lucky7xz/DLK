{"textgrid.poem.51119": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Schweizerdegen", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hei\u00dft ein Haus zum Schweizerdegen,", "tokens": ["Hei\u00dft", "ein", "Haus", "zum", "Schwei\u00b7zer\u00b7de\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lustig mu\u00df die Herberg sein;", "tokens": ["Lus\u00b7tig", "mu\u00df", "die", "Her\u00b7berg", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn die Trommel spricht den Segen,", "tokens": ["Denn", "die", "Trom\u00b7mel", "spricht", "den", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Wirt schenkt Roten ein!", "tokens": ["Und", "der", "Wirt", "schenkt", "Ro\u00b7ten", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "NN", "PTKVZ", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Kommen die G\u00e4ste, sch\u00f6n Wirtin, sie lacht,", "tokens": ["Kom\u00b7men", "die", "G\u00e4s\u00b7te", ",", "sch\u00f6n", "Wir\u00b7tin", ",", "sie", "lacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJD", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Sie hat schon manchen zu Bette gebracht!", "tokens": ["Sie", "hat", "schon", "man\u00b7chen", "zu", "Bet\u00b7te", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "APPR", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ist kein Volk fast allerwegen,", "tokens": ["Ist", "kein", "Volk", "fast", "al\u00b7ler\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was da nicht schon eingekehrt,", "tokens": ["Was", "da", "nicht", "schon", "ein\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Wirt zum Schweizerdegen", "tokens": ["Und", "der", "Wirt", "zum", "Schwei\u00b7zer\u00b7de\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat den Eintritt nie verwehrt,", "tokens": ["Hat", "den", "Ein\u00b7tritt", "nie", "ver\u00b7wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hat dann die blutige Zeche gemacht,", "tokens": ["Hat", "dann", "die", "blu\u00b7ti\u00b7ge", "Ze\u00b7che", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Da\u00df die Frau Wirtin vor Freuden gelacht.", "tokens": ["Da\u00df", "die", "Frau", "Wir\u00b7tin", "vor", "Freu\u00b7den", "ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "APPR", "NN", "VVPP", "$."], "meter": "--+---+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Zweiundzwanzig Schilde blitzen", "tokens": ["Zwei\u00b7und\u00b7zwan\u00b7zig", "Schil\u00b7de", "blit\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["CARD", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von dem Giebel weit zu Tal:", "tokens": ["Von", "dem", "Gie\u00b7bel", "weit", "zu", "Tal", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zeug- und Bannerherren sitzen", "tokens": ["Zeug", "und", "Ban\u00b7ner\u00b7her\u00b7ren", "sit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["TRUNC", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Harrend in dem hohen Saal,", "tokens": ["Har\u00b7rend", "in", "dem", "ho\u00b7hen", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lauschend, bis jauchzend die Mutter sie ruft", "tokens": ["Lau\u00b7schend", ",", "bis", "jauch\u00b7zend", "die", "Mut\u00b7ter", "sie", "ruft"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "KOUS", "ADJD", "ART", "NN", "PPER", "VVFIN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Und von den Schilden erklinget die Luft.", "tokens": ["Und", "von", "den", "Schil\u00b7den", "er\u00b7klin\u00b7get", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Und auf allen Weg' und Stegen", "tokens": ["Und", "auf", "al\u00b7len", "Weg'", "und", "Ste\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht es auf zu Berg und Tal;", "tokens": ["Steht", "es", "auf", "zu", "Berg", "und", "Tal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6rt, es klingt der Schweizerdegen,", "tokens": ["H\u00f6rt", ",", "es", "klingt", "der", "Schwei\u00b7zer\u00b7de\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6rt, es singt der alte Stahl!", "tokens": ["H\u00f6rt", ",", "es", "singt", "der", "al\u00b7te", "Stahl", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tut ihm genug und erprobt ihn vereint!", "tokens": ["Tut", "ihm", "ge\u00b7nug", "und", "er\u00b7probt", "ihn", "ver\u00b7eint", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "KON", "VVFIN", "PPER", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Besser, das M\u00fctterchen lacht, als es weint!", "tokens": ["Bes\u00b7ser", ",", "das", "M\u00fct\u00b7ter\u00b7chen", "lacht", ",", "als", "es", "weint", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Wo in L\u00e4ndern, sch\u00f6n gelegen,", "tokens": ["Wo", "in", "L\u00e4n\u00b7dern", ",", "sch\u00f6n", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo in altget\u00fcrmter Stadt", "tokens": ["Wo", "in", "alt\u00b7ge\u00b7t\u00fcrm\u00b7ter", "Stadt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schweizerherz und Schweizerdegen", "tokens": ["Schwei\u00b7zer\u00b7herz", "und", "Schwei\u00b7zer\u00b7de\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die gemeine Herrschaft hat,", "tokens": ["Die", "ge\u00b7mei\u00b7ne", "Herr\u00b7schaft", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da ist die Mutter, so hold und so fein.", "tokens": ["Da", "ist", "die", "Mut\u00b7ter", ",", "so", "hold", "und", "so", "fein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Lacht sie? so wird's Frau Helvetia sein!", "tokens": ["Lacht", "sie", "?", "so", "wird's", "Frau", "Hel\u00b7ve\u00b7tia", "sein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "ADV", "VAFIN", "NN", "NE", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}