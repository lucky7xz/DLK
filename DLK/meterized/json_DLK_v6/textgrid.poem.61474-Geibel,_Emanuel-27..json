{"textgrid.poem.61474": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O la\u00dft mir meine stille Weise,", "tokens": ["O", "la\u00dft", "mir", "mei\u00b7ne", "stil\u00b7le", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O rei\u00dft mich nicht hervor ans Licht!", "tokens": ["O", "rei\u00dft", "mich", "nicht", "her\u00b7vor", "ans", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich d\u00fcrstet nicht nach eurem Preise,", "tokens": ["Mich", "d\u00fcrs\u00b7tet", "nicht", "nach", "eu\u00b7rem", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und eure Bahn ist meine nicht.", "tokens": ["Und", "eu\u00b7re", "Bahn", "ist", "mei\u00b7ne", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPOSAT", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dem S\u00e4nger sind genug der Schlingen", "tokens": ["Dem", "S\u00e4n\u00b7ger", "sind", "ge\u00b7nug", "der", "Schlin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom eignen hei\u00dfen Blut gelegt;", "tokens": ["Vom", "eig\u00b7nen", "hei\u00b7\u00dfen", "Blut", "ge\u00b7legt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es frommt das Ma\u00df in allen Dingen", "tokens": ["Es", "frommt", "das", "Ma\u00df", "in", "al\u00b7len", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und doppelt, wo man Geister w\u00e4gt.", "tokens": ["Und", "dop\u00b7pelt", ",", "wo", "man", "Geis\u00b7ter", "w\u00e4gt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "PIS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ist dieser Brust ein Ton beschieden,", "tokens": ["Ist", "die\u00b7ser", "Brust", "ein", "Ton", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der stimmt in eures Herzens Schlag:", "tokens": ["Der", "stimmt", "in", "eu\u00b7res", "Her\u00b7zens", "Schlag", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohlan, so g\u00f6nnt mir Rast und Frieden,", "tokens": ["Wo\u00b7hlan", ",", "so", "g\u00f6nnt", "mir", "Rast", "und", "Frie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich ihn voll verstr\u00f6men mag!", "tokens": ["Da\u00df", "ich", "ihn", "voll", "ver\u00b7str\u00f6\u00b7men", "mag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch nicht, wo bei der Kerzen Funkeln", "tokens": ["Doch", "nicht", ",", "wo", "bei", "der", "Ker\u00b7zen", "Fun\u00b7keln"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "$,", "PWAV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Reigen wilde Laune f\u00fchrt, \u2013", "tokens": ["Den", "Rei\u00b7gen", "wil\u00b7de", "Lau\u00b7ne", "f\u00fchrt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Gott hat immer nur im Dunkeln", "tokens": ["Der", "Gott", "hat", "im\u00b7mer", "nur", "im", "Dun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Seele t\u00f6nend mir ber\u00fchrt.", "tokens": ["Die", "See\u00b7le", "t\u00f6\u00b7nend", "mir", "be\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Er flieht die St\u00e4tten, wo die Menge", "tokens": ["Er", "flieht", "die", "St\u00e4t\u00b7ten", ",", "wo", "die", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich G\u00f6tzen formt und dann zerbricht;", "tokens": ["Sich", "G\u00f6t\u00b7zen", "formt", "und", "dann", "zer\u00b7bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NE", "VVPP", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum la\u00dft mich wert sein seiner Strenge", "tokens": ["Drum", "la\u00dft", "mich", "wert", "sein", "sei\u00b7ner", "Stren\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "VAINF", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und rei\u00dft mich nicht hervor ans Licht!", "tokens": ["Und", "rei\u00dft", "mich", "nicht", "her\u00b7vor", "ans", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}