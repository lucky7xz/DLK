{"textgrid.poem.58606": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "38.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr \u00dcbertreter, geht in euer Herz! Jes. 46, 8", "tokens": ["Ihr", "\u00dc\u00b7bert\u00b7re\u00b7ter", ",", "geht", "in", "eu\u00b7er", "Herz", "!", "Jes", ".", "46", ",", "8"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "number", "punct", "number"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "NE", "$.", "CARD", "$,", "CARD"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Ihr \u00dcbertreter, geht in euer Herz hinein,", "tokens": ["Ihr", "\u00dc\u00b7bert\u00b7re\u00b7ter", ",", "geht", "in", "eu\u00b7er", "Herz", "hin\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So will der fromme Gott den S\u00fcnder Bu\u00dfe lehren!", "tokens": ["So", "will", "der", "from\u00b7me", "Gott", "den", "S\u00fcn\u00b7der", "Bu\u00b7\u00dfe", "leh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er soll im Glauben sich von allem einw\u00e4rts kehren", "tokens": ["Er", "soll", "im", "Glau\u00b7ben", "sich", "von", "al\u00b7lem", "ein\u00b7w\u00e4rts", "keh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "PRF", "APPR", "PIS", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit Herze, Lust und Sinn und lieben Gott allein.", "tokens": ["Mit", "Her\u00b7ze", ",", "Lust", "und", "Sinn", "und", "lie\u00b7ben", "Gott", "al\u00b7lein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "$,", "NN", "KON", "NN", "KON", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Im Herzen wird uns Gott und alles offenbar,", "tokens": ["Im", "Her\u00b7zen", "wird", "uns", "Gott", "und", "al\u00b7les", "of\u00b7fen\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "NN", "KON", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich \u00dcbertreter bin noch so zerstreut und ferne;", "tokens": ["Ich", "\u00dc\u00b7bert\u00b7re\u00b7ter", "bin", "noch", "so", "zer\u00b7streut", "und", "fer\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "ADV", "ADV", "VVPP", "KON", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Zieh mich, mein Gott, hinein, ich wollte doch so gerne", "tokens": ["Zieh", "mich", ",", "mein", "Gott", ",", "hin\u00b7ein", ",", "ich", "woll\u00b7te", "doch", "so", "ger\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PTKVZ", "$,", "PPER", "VMFIN", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In mein Herz kehren ein und in dein Herze gar!", "tokens": ["In", "mein", "Herz", "keh\u00b7ren", "ein", "und", "in", "dein", "Her\u00b7ze", "gar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PTKVZ", "KON", "APPR", "PPOSAT", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}