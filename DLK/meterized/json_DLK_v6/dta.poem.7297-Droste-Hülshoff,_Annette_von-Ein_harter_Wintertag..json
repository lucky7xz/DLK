{"dta.poem.7297": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Ein harter Wintertag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df ich dich so verk\u00fcmmert seh',", "tokens": ["Da\u00df", "ich", "dich", "so", "ver\u00b7k\u00fcm\u00b7mert", "seh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein lieb' lebend'ges Wasserreich,", "tokens": ["Mein", "lieb'", "le\u00b7ben\u00b7d'\u00b7ges", "Was\u00b7ser\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da\u00df ganz versteckt in Eis und Schnee", "tokens": ["Da\u00df", "ganz", "ver\u00b7steckt", "in", "Eis", "und", "Schnee"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "VVPP", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du siehst der plumpen Erde gleich;", "tokens": ["Du", "siehst", "der", "plum\u00b7pen", "Er\u00b7de", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auch da\u00df voll Reif und Schollen h\u00e4ngt", "tokens": ["Auch", "da\u00df", "voll", "Reif", "und", "Schol\u00b7len", "h\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ADJD", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein \u00fcberglas'ter Fichtengang:", "tokens": ["Dein", "\u00fc\u00b7ber\u00b7glas'\u00b7ter", "Fich\u00b7ten\u00b7gang", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das ist es nicht, was mich beengt,", "tokens": ["Das", "ist", "es", "nicht", ",", "was", "mich", "be\u00b7engt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geh' ich an deinem Bord entlang.", "tokens": ["Geh'", "ich", "an", "dei\u00b7nem", "Bord", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zwar in der immer gr\u00fcnen Zier", "tokens": ["Zwar", "in", "der", "im\u00b7mer", "gr\u00fc\u00b7nen", "Zier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erschienst, o freundlich Element,", "tokens": ["Er\u00b7schienst", ",", "o", "freund\u00b7lich", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "FM", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du \u00e4hnlich den Oasen mir,", "tokens": ["Du", "\u00e4hn\u00b7lich", "den", "O\u00b7a\u00b7sen", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die des Arabers Sehnsucht kennt;", "tokens": ["Die", "des", "A\u00b7ra\u00b7bers", "Sehn\u00b7sucht", "kennt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}}, "stanza.4": {"line.1": {"text": "Wenn neben der verdorrten Flur", "tokens": ["Wenn", "ne\u00b7ben", "der", "ver\u00b7dorr\u00b7ten", "Flur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Erbl\u00fchten deine Moose noch,", "tokens": ["Er\u00b7bl\u00fch\u00b7ten", "dei\u00b7ne", "Moo\u00b7se", "noch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn durch die schweigende Natur", "tokens": ["Wenn", "durch", "die", "schwei\u00b7gen\u00b7de", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erklangen deine Wellen doch.", "tokens": ["Er\u00b7klan\u00b7gen", "dei\u00b7ne", "Wel\u00b7len", "doch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Allein auch heute wollt' ich gern", "tokens": ["Al\u00b7lein", "auch", "heu\u00b7te", "wollt'", "ich", "gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich des krystallnen Flimmers freun,", "tokens": ["Mich", "des", "krys\u00b7tall\u00b7nen", "Flim\u00b7mers", "freun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Belauschen jeden Farbenstern", "tokens": ["Be\u00b7lau\u00b7schen", "je\u00b7den", "Far\u00b7bens\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["NN", "PIAT", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Und keinen Sommertag bereun:", "tokens": ["Und", "kei\u00b7nen", "Som\u00b7mer\u00b7tag", "be\u00b7re\u00b7un", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "W\u00e4r' nicht dem Ufer l\u00e4ngs, so breit,", "tokens": ["W\u00e4r'", "nicht", "dem", "U\u00b7fer", "l\u00e4ngs", ",", "so", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "ADV", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die glatte Schlittenbahn gefegt,", "tokens": ["Die", "glat\u00b7te", "Schlit\u00b7ten\u00b7bahn", "ge\u00b7fegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Worauf sich wohl zur Mittagszeit", "tokens": ["Wo\u00b7rauf", "sich", "wohl", "zur", "Mit\u00b7tags\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "PRF", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gar manche r\u00fcst'ge Ferse regt.", "tokens": ["Gar", "man\u00b7che", "r\u00fcst'\u00b7ge", "Fer\u00b7se", "reg\u00b7t."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "PIAT", "ADJA", "NN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Bedenk' ich nun, wie manches Jahr", "tokens": ["Be\u00b7denk'", "ich", "nun", ",", "wie", "man\u00b7ches", "Jahr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "$,", "PWAV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich nimmer eine Eisbahn sah:", "tokens": ["Ich", "nim\u00b7mer", "ei\u00b7ne", "Eis\u00b7bahn", "sah", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl wird mir's tr\u00fcb' und wunderbar,", "tokens": ["Wohl", "wird", "mir's", "tr\u00fcb'", "und", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und tausend Bilder treten nah.", "tokens": ["Und", "tau\u00b7send", "Bil\u00b7der", "tre\u00b7ten", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Was blieb an W\u00fcnschen unerf\u00fcllt,", "tokens": ["Was", "blieb", "an", "W\u00fcn\u00b7schen", "un\u00b7er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das n\u00e4hm' ich noch gelassen mit:", "tokens": ["Das", "n\u00e4hm'", "ich", "noch", "ge\u00b7las\u00b7sen", "mit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "VVPP", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch ach, der Frost so manchen h\u00fcllt,", "tokens": ["Doch", "ach", ",", "der", "Frost", "so", "man\u00b7chen", "h\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "ART", "NN", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der einst so fr\u00f6hlich dr\u00fcber glitt!", "tokens": ["Der", "einst", "so", "fr\u00f6h\u00b7lich", "dr\u00fc\u00b7ber", "glitt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}