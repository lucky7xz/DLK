{"dta.poem.9781": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet  \n Auf die beerdigung Hn. D. D. Heimb\u00fcrgers/  \n Archidiaconi zu Berlin; nach anleitung des  \n Evangelii vom ungerechten  \n hau\u00dfhalter.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje redlich hastu doch dem Herren hau\u00dfgehalten/", "tokens": ["Wie", "red\u00b7lich", "has\u00b7tu", "doch", "dem", "Her\u00b7ren", "hau\u00df\u00b7ge\u00b7hal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VAFIN", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der die geheimni\u00df dir des himmels anvertraut:", "tokens": ["Der", "die", "ge\u00b7heim\u00b7ni\u00df", "dir", "des", "him\u00b7mels", "an\u00b7ver\u00b7traut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O theurer seelen-artzt! wer hat dich nicht geschaut", "tokens": ["O", "theu\u00b7rer", "see\u00b7len\u00b7artzt", "!", "wer", "hat", "dich", "nicht", "ge\u00b7schaut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "VVPP", "$.", "PWS", "VAFIN", "PPER", "PTKNEG", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit unerm\u00fcdtem flei\u00df dein schweres amt verwalten?", "tokens": ["Mit", "un\u00b7er\u00b7m\u00fcd\u00b7tem", "flei\u00df", "dein", "schwe\u00b7res", "amt", "ver\u00b7wal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wann so viel donner-wort\u2019 aus deinem mund erschallten/", "tokens": ["Wann", "so", "viel", "don\u00b7ner\u00b7wort'", "aus", "dei\u00b7nem", "mund", "er\u00b7schall\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Welch hartes felsen-hertz ward nicht dadurch erbaut?", "tokens": ["Welch", "har\u00b7tes", "fel\u00b7sen\u00b7hertz", "ward", "nicht", "da\u00b7durch", "er\u00b7baut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "PTKNEG", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jtzt aber da du schweigst/ ersch\u00fcttert uns die haut/", "tokens": ["Jtzt", "a\u00b7ber", "da", "du", "schweigst", "/", "er\u00b7sch\u00fct\u00b7tert", "uns", "die", "haut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "PPER", "VVFIN", "$(", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und unser hertz will schier samt deinem leid erkalten:", "tokens": ["Und", "un\u00b7ser", "hertz", "will", "schier", "samt", "dei\u00b7nem", "leid", "er\u00b7kal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "ADJD", "APPR", "PPOSAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du legst die rechnung ab vor GOttes lichtem thron/", "tokens": ["Du", "legst", "die", "rech\u00b7nung", "ab", "vor", "Got\u00b7tes", "lich\u00b7tem", "thron", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "APPR", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und \u00fcberkommst daselbst den theuren gnaden-lohn/", "tokens": ["Und", "\u00fc\u00b7ber\u00b7kommst", "da\u00b7selbst", "den", "theu\u00b7ren", "gna\u00b7den\u00b7lohn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die engel nehmen dich in ihren frommen orden:", "tokens": ["Die", "en\u00b7gel", "neh\u00b7men", "dich", "in", "ih\u00b7ren", "from\u00b7men", "or\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und da dein leib hier nicht mehr h\u00e4u\u00df auff erden h\u00e4lt/", "tokens": ["Und", "da", "dein", "leib", "hier", "nicht", "mehr", "h\u00e4u\u00df", "auff", "er\u00b7den", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ADV", "PTKNEG", "ADV", "ADJD", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verl\u00e4st als pilgrim du die h\u00fctten dieser welt/", "tokens": ["Ver\u00b7l\u00e4st", "als", "pil\u00b7grim", "du", "die", "h\u00fct\u00b7ten", "die\u00b7ser", "welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "NE", "PPER", "ART", "ADJA", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und bist nunmehr daheim ein himmels-b\u00fcrger worden.", "tokens": ["Und", "bist", "nun\u00b7mehr", "da\u00b7heim", "ein", "him\u00b7mels\u00b7b\u00fcr\u00b7ger", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ART", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}