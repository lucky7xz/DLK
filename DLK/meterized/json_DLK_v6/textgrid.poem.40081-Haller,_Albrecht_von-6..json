{"textgrid.poem.40081": {"metadata": {"author": {"name": "Haller, Albrecht von", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Steh still, Helvetier, hier liegt das k\u00fchne Heer,", "tokens": ["Steh", "still", ",", "Hel\u00b7ve\u00b7tier", ",", "hier", "liegt", "das", "k\u00fch\u00b7ne", "Heer", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "NN", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-++-+-+-+-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Vor welchem L\u00fcttich fiel und Frankreichs Thron erbebte;", "tokens": ["Vor", "wel\u00b7chem", "L\u00fct\u00b7tich", "fiel", "und", "Fran\u00b7kreichs", "Thron", "er\u00b7beb\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VVFIN", "KON", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nicht unsrer Ahnen Zahl, nicht k\u00fcnstlichers Gewehr,", "tokens": ["Nicht", "uns\u00b7rer", "Ah\u00b7nen", "Zahl", ",", "nicht", "k\u00fcnst\u00b7li\u00b7chers", "Ge\u00b7wehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "NN", "$,", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Eintracht schlug den Feind, die ihren Arm belebte.", "tokens": ["Die", "Ein\u00b7tracht", "schlug", "den", "Feind", ",", "die", "ih\u00b7ren", "Arm", "be\u00b7leb\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kennt, Br\u00fcder, eure Macht, sie liegt in unsrer Treu!", "tokens": ["Kennt", ",", "Br\u00fc\u00b7der", ",", "eu\u00b7re", "Macht", ",", "sie", "liegt", "in", "uns\u00b7rer", "Treu", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "O w\u00fcrde sie noch heut in jedem Leser neu!", "tokens": ["O", "w\u00fcr\u00b7de", "sie", "noch", "heut", "in", "je\u00b7dem", "Le\u00b7ser", "neu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "ADV", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}