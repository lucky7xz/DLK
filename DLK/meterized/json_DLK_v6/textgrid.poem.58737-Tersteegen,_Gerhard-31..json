{"textgrid.poem.58737": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Name ist eine ausgesch\u00fcttete Salbe. Zieh mich dir nach, so laufen wir! Hoheslied 1, 3. 4", "tokens": ["Dein", "Na\u00b7me", "ist", "ei\u00b7ne", "aus\u00b7ge\u00b7sch\u00fct\u00b7te\u00b7te", "Sal\u00b7be", ".", "Zieh", "mich", "dir", "nach", ",", "so", "lau\u00b7fen", "wir", "!", "Ho\u00b7hes\u00b7lied", "1", ",", "3.", "4"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "number", "punct", "ordinal", "number"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$.", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "$.", "NN", "CARD", "$,", "ADJA", "CARD"], "meter": "-+--+-+-+--+-+--+-+--+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Folgt man in Blindheit nur, o reine Liebe, dir,", "tokens": ["Folgt", "man", "in", "Blind\u00b7heit", "nur", ",", "o", "rei\u00b7ne", "Lie\u00b7be", ",", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "NN", "ADV", "$,", "FM", "ADJA", "NN", "$,", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie so h\u00f6chst sicher wandeln wir!", "tokens": ["Wie", "so", "h\u00f6chst", "si\u00b7cher", "wan\u00b7deln", "wir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ADJD", "VVFIN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Durchs Auge der Vernunft wir irren von der Stra\u00dfe,", "tokens": ["Durchs", "Au\u00b7ge", "der", "Ver\u00b7nunft", "wir", "ir\u00b7ren", "von", "der", "Stra\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In dunkler Glaubensnacht wir wollen gehn ohn' Licht,", "tokens": ["In", "dunk\u00b7ler", "Glau\u00b7bens\u00b7nacht", "wir", "wol\u00b7len", "gehn", "ohn'", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VMFIN", "VVINF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Obwohl es selten jetzt geschicht,", "tokens": ["Ob\u00b7wohl", "es", "sel\u00b7ten", "jetzt", "ge\u00b7schicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df in ", "tokens": ["Da\u00df", "in"], "token_info": ["word", "word"], "pos": ["KOUS", "APPR"], "meter": "++", "measure": "spondeus"}, "line.7": {"text": "Den Sinnen trauet man, man glaubt nicht nackt und rein,", "tokens": ["Den", "Sin\u00b7nen", "trau\u00b7et", "man", ",", "man", "glaubt", "nicht", "nackt", "und", "rein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "$,", "PIS", "VVFIN", "PTKNEG", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Sinne uns verf\u00fchr'n, die Lieb' f\u00fchrt recht allein;", "tokens": ["Die", "Sin\u00b7ne", "uns", "ver\u00b7f\u00fchr'n", ",", "die", "Lieb'", "f\u00fchrt", "recht", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$,", "ART", "NN", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Je mehr sie arm macht und entbl\u00f6\u00dfet,", "tokens": ["Je", "mehr", "sie", "arm", "macht", "und", "ent\u00b7bl\u00f6\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Je mehr sie alles nimmt und uns nichts \u00fcbrig l\u00e4sset,", "tokens": ["Je", "mehr", "sie", "al\u00b7les", "nimmt", "und", "uns", "nichts", "\u00fcb\u00b7rig", "l\u00e4s\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PIS", "VVFIN", "KON", "PPER", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Je mehr das Herz kriegt weiten Raum:", "tokens": ["Je", "mehr", "das", "Herz", "kriegt", "wei\u00b7ten", "Raum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Es geht gerad' zum Ziel, nicht stolpert, wanket, treibet,", "tokens": ["Es", "geht", "ge\u00b7rad'", "zum", "Ziel", ",", "nicht", "stol\u00b7pert", ",", "wan\u00b7ket", ",", "trei\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,", "PTKNEG", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sein Teil die reine Liebe bleibet,", "tokens": ["Sein", "Teil", "die", "rei\u00b7ne", "Lie\u00b7be", "blei\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Dies ist's dem Herzen gar; sonst liebt's nicht oder kaum.", "tokens": ["Dies", "ist's", "dem", "Her\u00b7zen", "gar", ";", "sonst", "liebt's", "nicht", "o\u00b7der", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "$.", "ADV", "VVFIN", "PTKNEG", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}