{"textgrid.poem.40955": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "N\u00e4he des Toten", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl m\u00fc\u00dft' ich herzlich weinen,", "tokens": ["Wohl", "m\u00fc\u00dft'", "ich", "herz\u00b7lich", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Herz! w\u00e4rst du wirklich tot,", "tokens": ["Herz", "!", "w\u00e4rst", "du", "wirk\u00b7lich", "tot", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PPER", "ADJD", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und k\u00f6nnt' mich nichts mehr einen", "tokens": ["Und", "k\u00f6nnt'", "mich", "nichts", "mehr", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PIS", "ADV", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit dir in Freud' und Not.", "tokens": ["Mit", "dir", "in", "Freud'", "und", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch, sieh, seit du gestorben", "tokens": ["Doch", ",", "sieh", ",", "seit", "du", "ge\u00b7stor\u00b7ben"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "VVFIN", "$,", "KOUS", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "(wei\u00df nicht, wie mir geschah),", "tokens": ["(", "wei\u00df", "nicht", ",", "wie", "mir", "ge\u00b7schah", ")", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$(", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hab' ich dich ", "tokens": ["Hab'", "ich", "dich"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "PRF"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Herz, bist du ", "tokens": ["Herz", ",", "bist", "du"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "VAFIN", "PPER"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Nicht Berg' und Tale trennen,", "tokens": ["Nicht", "Ber\u00b7g'", "und", "Ta\u00b7le", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "O Herz! mich mehr von dir,", "tokens": ["O", "Herz", "!", "mich", "mehr", "von", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Leis darf ich dich nur nennen,", "tokens": ["Leis", "darf", "ich", "dich", "nur", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Da bist du schon bei mir;", "tokens": ["Da", "bist", "du", "schon", "bei", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dann legt sich schnell die Welle", "tokens": ["Dann", "legt", "sich", "schnell", "die", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Herzen st\u00fcrmischtr\u00fcb,", "tokens": ["Im", "Her\u00b7zen", "st\u00fcr\u00b7mischtr\u00fcb", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und in mir wird es helle", "tokens": ["Und", "in", "mir", "wird", "es", "hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VAFIN", "PPER", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und um mich alles lieb.", "tokens": ["Und", "um", "mich", "al\u00b7les", "lieb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUI", "PRF", "PIS", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die andern nicht begreifen,", "tokens": ["Die", "an\u00b7dern", "nicht", "be\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was Sel'ges ich ersah!", "tokens": ["Was", "Sel'\u00b7ges", "ich", "er\u00b7sah", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Das ist f\u00fcr sie nicht da.", "tokens": ["Das", "ist", "f\u00fcr", "sie", "nicht", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nicht wie sich Geister lieben,", "tokens": ["Nicht", "wie", "sich", "Geis\u00b7ter", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PWAV", "PRF", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch, Herz! \u2013 das wissen wir!", "tokens": ["Doch", ",", "Herz", "!", "\u2013", "das", "wis\u00b7sen", "wir", "!"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$.", "$(", "PDS", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}