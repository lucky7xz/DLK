{"dta.poem.21051": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Das Erwachen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schau, der Morgen erwacht &#252;ber der Winterwelt.               ", "tokens": ["Schau", ",", "der", "Mor\u00b7gen", "er\u00b7wacht", "&#252;", "ber", "der", "Win\u00b7ter\u00b7welt", "."], "token_info": ["word", "punct", "word", "word", "word", "XML_entity", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVPP", "$(", "APPR", "ART", "NN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Lunens Silber erbleicht; Phosphoros Gold erlischt;", "tokens": ["Lu\u00b7nens", "Sil\u00b7ber", "er\u00b7bleicht", ";", "Phos\u00b7pho\u00b7ros", "Gold", "er\u00b7lischt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVPP", "$.", "NE", "NN", "VVPP", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "In den Locken der Eos", "tokens": ["In", "den", "Lo\u00b7cken", "der", "E\u00b7os"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Welkt der duftende Rosenkranz.", "tokens": ["Welkt", "der", "duf\u00b7ten\u00b7de", "Ro\u00b7sen\u00b7kranz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Luna, warum so blass? Eos und Phosphoros,", "tokens": ["Lu\u00b7na", ",", "wa\u00b7rum", "so", "blass", "?", "E\u00b7os", "und", "Phos\u00b7pho\u00b7ros", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADV", "ADJD", "$.", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie so eilig zur Flucht? S\u00e4umet, o s\u00e4umet noch!", "tokens": ["Wie", "so", "ei\u00b7lig", "zur", "Flucht", "?", "S\u00e4u\u00b7met", ",", "o", "s\u00e4u\u00b7met", "noch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "APPRART", "NN", "$.", "VVFIN", "$,", "FM", "VVFIN", "ADV", "$."], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Hellt Odaliens Fenster!", "tokens": ["Hellt", "O\u00b7da\u00b7li\u00b7ens", "Fens\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Winkt: Erwache! der Schlummernden!", "tokens": ["Winkt", ":", "Er\u00b7wa\u00b7che", "!", "der", "Schlum\u00b7mern\u00b7den", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$.", "NN", "$.", "ART", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Oda, Oda erwach! Siehe die Morgenwelt", "tokens": ["O\u00b7da", ",", "O\u00b7da", "er\u00b7wach", "!", "Sie\u00b7he", "die", "Mor\u00b7gen\u00b7welt"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NE", "VVFIN", "$.", "VVIMP", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Jauchzt im Schimmer des Tags. Luna und Phosphoros", "tokens": ["Jauchzt", "im", "Schim\u00b7mer", "des", "Tags", ".", "Lu\u00b7na", "und", "Phos\u00b7pho\u00b7ros"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$.", "NN", "KON", "NE"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und die rosige Eos", "tokens": ["Und", "die", "ro\u00b7si\u00b7ge", "E\u00b7os"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00e4umen, L\u00e4chelnde, dich zu schaun!", "tokens": ["S\u00e4u\u00b7men", ",", "L\u00e4\u00b7cheln\u00b7de", ",", "dich", "zu", "schaun", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Oda, Oda erwach! H\u00f6rt sie des S\u00e4ngers Ruf?", "tokens": ["O\u00b7da", ",", "O\u00b7da", "er\u00b7wach", "!", "H\u00f6rt", "sie", "des", "S\u00e4n\u00b7gers", "Ruf", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "VVFIN", "$.", "VVIMP", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Leise regt sich ihr Mund; leiser ihr Arm. Sie", "tokens": ["Lei\u00b7se", "regt", "sich", "ihr", "Mund", ";", "lei\u00b7ser", "ihr", "Arm", ".", "Sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "PRF", "PPOSAT", "NN", "$.", "ADJD", "PPOSAT", "NN", "$.", "PPER"], "meter": "+-+--++-+++", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "schl\u00e4gt,", "tokens": ["schl\u00e4gt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Schau, die leuchtenden Augen", "tokens": ["Schau", ",", "die", "leuch\u00b7ten\u00b7den", "Au\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Gleich zwei schwellenden Himmeln auf.", "tokens": ["Gleich", "zwei", "schwel\u00b7len\u00b7den", "Him\u00b7meln", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "H\u00f6her schwillt ihr die Brust; lauter erklopft ihr", "tokens": ["H\u00f6\u00b7her", "schwillt", "ihr", "die", "Brust", ";", "lau\u00b7ter", "er\u00b7klopft", "ihr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$.", "ADJD", "VVFIN", "PPER"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Herz.", "tokens": ["Herz", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Wem erklopft es? Es klopft, Vater der Leben,", "tokens": ["Wem", "er\u00b7klopft", "es", "?", "Es", "klopft", ",", "Va\u00b7ter", "der", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "dir,", "tokens": ["dir", ","], "token_info": ["word", "punct"], "pos": ["PPER", "$,"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Der den Morgen mit Rosen,", "tokens": ["Der", "den", "Mor\u00b7gen", "mit", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Der mit Strahlen den Mittag lockt.", "tokens": ["Der", "mit", "Strah\u00b7len", "den", "Mit\u00b7tag", "lockt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Leise Wehmuth bew\u00f6lkt ihren gesunknen Blick.", "tokens": ["Lei\u00b7se", "Weh\u00b7muth", "be\u00b7w\u00f6lkt", "ih\u00b7ren", "ge\u00b7sun\u00b7knen", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Sehnsuchtseufzer entwehn ihrer gehobnen Brust.", "tokens": ["Sehn\u00b7such\u00b7tseuf\u00b7zer", "ent\u00b7wehn", "ih\u00b7rer", "ge\u00b7hob\u00b7nen", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Ach, sie wehn dem Begl\u00fcckten,", "tokens": ["Ach", ",", "sie", "wehn", "dem", "Be\u00b7gl\u00fcck\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Den ihr schweigendes Lieben meint.", "tokens": ["Den", "ihr", "schwei\u00b7gen\u00b7des", "Lie\u00b7ben", "meint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Oda, Oda! und Ihm steiget' dein Busen nicht,", "tokens": ["O\u00b7da", ",", "O\u00b7da", "!", "und", "Ihm", "stei\u00b7get'", "dein", "Bu\u00b7sen", "nicht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$.", "KON", "PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Oder steigt ihm so sp\u00e4t, der von dem D\u00e4mmerstrahl", "tokens": ["O\u00b7der", "steigt", "ihm", "so", "sp\u00e4t", ",", "der", "von", "dem", "D\u00e4m\u00b7mer\u00b7strahl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Des ergrauenden Morgens", "tokens": ["Des", "er\u00b7grau\u00b7en\u00b7den", "Mor\u00b7gens"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Bis zur schattenden Nacht dein denkt?", "tokens": ["Bis", "zur", "schat\u00b7ten\u00b7den", "Nacht", "dein", "denkt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "PPOSAT", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "Dieses sp\u00e4tere Ach, w\u00e4hnst du, gen\u00fcge mir?", "tokens": ["Die\u00b7ses", "sp\u00e4\u00b7te\u00b7re", "Ach", ",", "w\u00e4hnst", "du", ",", "ge\u00b7n\u00fc\u00b7ge", "mir", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,", "VAFIN", "PPER", "$,", "VVFIN", "PPER", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Dieser fl\u00fcchtige Blick? Laue, ich hasse dich!", "tokens": ["Die\u00b7ser", "fl\u00fcch\u00b7ti\u00b7ge", "Blick", "?", "Lau\u00b7e", ",", "ich", "has\u00b7se", "dich", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$.", "NN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Ich verachte den Handdruck,", "tokens": ["Ich", "ver\u00b7ach\u00b7te", "den", "Hand\u00b7druck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Der der z\u00f6gernden Hand entf\u00e4hrt.", "tokens": ["Der", "der", "z\u00f6\u00b7gern\u00b7den", "Hand", "ent\u00b7f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Unmuth wandelt mich an, dumpfe Erbitterung,", "tokens": ["Un\u00b7muth", "wan\u00b7delt", "mich", "an", ",", "dump\u00b7fe", "Er\u00b7bit\u00b7te\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Menschenfeindlicher Grimm \u2014 Schau, ich zerreiss", "tokens": ["Men\u00b7schen\u00b7feind\u00b7li\u00b7cher", "Grimm", "Schau", ",", "ich", "zer\u00b7reiss"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$(", "NN", "$,", "PPER", "VVFIN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "den Kranz,", "tokens": ["den", "Kranz", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Der die Stirne mir schattet,", "tokens": ["Der", "die", "Stir\u00b7ne", "mir", "schat\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und zerschmettre mein Saitenspiel.", "tokens": ["Und", "zer\u00b7schmett\u00b7re", "mein", "Sai\u00b7ten\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "Dunkel w\u00f6lke den Tag, der mich geb\u00e4ren sah!", "tokens": ["Dun\u00b7kel", "w\u00f6l\u00b7ke", "den", "Tag", ",", "der", "mich", "ge\u00b7b\u00e4\u00b7ren", "sah", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "R\u00fcckzukehren zu mir, schwingt er den Fittig", "tokens": ["R\u00fcck\u00b7zu\u00b7keh\u00b7ren", "zu", "mir", ",", "schwingt", "er", "den", "Fit\u00b7tig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "schon.", "tokens": ["schon", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Aber Dunkel umrolle,", "tokens": ["A\u00b7ber", "Dun\u00b7kel", "um\u00b7rol\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Hagelwetter umrassle ihn! \u2014", "tokens": ["Ha\u00b7gel\u00b7wet\u00b7ter", "um\u00b7rass\u00b7le", "ihn", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "PPER", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.11": {"line.1": {"text": "Also sang ich und schwieg. Meine verstummende", "tokens": ["Al\u00b7so", "sang", "ich", "und", "schwieg", ".", "Mei\u00b7ne", "ver\u00b7stum\u00b7men\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "$.", "PPOSAT", "ADJA"], "meter": "+-+--++--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Harf' entbebte der Hand. Luna und Phosphoros", "tokens": ["Ha\u00b7rf'", "ent\u00b7beb\u00b7te", "der", "Hand", ".", "Lu\u00b7na", "und", "Phos\u00b7pho\u00b7ros"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "$.", "NN", "KON", "NE"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Dauchten Rachekometen,", "tokens": ["Dauch\u00b7ten", "Ra\u00b7che\u00b7ko\u00b7me\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und das Fr\u00fchroth mir Weltenbrand.", "tokens": ["Und", "das", "Fr\u00fch\u00b7roth", "mir", "Wel\u00b7ten\u00b7brand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.12": {"line.1": {"text": "Schau, da l\u00e4chelte mir durch der Melancholie", "tokens": ["Schau", ",", "da", "l\u00e4\u00b7chel\u00b7te", "mir", "durch", "der", "Me\u00b7lan\u00b7cho\u00b7lie"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Starres Dunkel dein Bild, meine Odalia.", "tokens": ["Star\u00b7res", "Dun\u00b7kel", "dein", "Bild", ",", "mei\u00b7ne", "O\u00b7da\u00b7lia", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$,", "PPOSAT", "NE", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wehmuth w\u00f6lkte dein Auge;", "tokens": ["Weh\u00b7muth", "w\u00f6lk\u00b7te", "dein", "Au\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Klag' entbebte dem Rosenmund.", "tokens": ["Klag'", "ent\u00b7beb\u00b7te", "dem", "Ro\u00b7sen\u