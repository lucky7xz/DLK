{"textgrid.poem.31672": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "44. Un bel morir, fa l' huemo contento", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welches Lebens-Schiff von Wellen vieles Vngl\u00fccks wird geplagt /", "tokens": ["Wel\u00b7ches", "Le\u00b7bens\u00b7Schiff", "von", "Wel\u00b7len", "vie\u00b7les", "Vn\u00b7gl\u00fccks", "wird", "ge\u00b7plagt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "APPR", "NN", "PIS", "NN", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "freut sich \u00fcber alle massen / wann der Wind im Port es jagt.", "tokens": ["freut", "sich", "\u00fc\u00b7ber", "al\u00b7le", "mas\u00b7sen", "/", "wann", "der", "Wind", "im", "Port", "es", "jagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PIAT", "NN", "$(", "PWAV", "ART", "NN", "APPRART", "NE", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Ach wer wolt doch froh nit seyn / wer von Donner / Blitz und Regen", "tokens": ["Ach", "wer", "wolt", "doch", "froh", "nit", "seyn", "/", "wer", "von", "Don\u00b7ner", "/", "Blitz", "und", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "PWS", "VMFIN", "ADV", "ADJD", "PTKNEG", "VAINF", "$(", "PWS", "APPR", "NN", "$(", "NN", "KON", "NN"], "meter": "-+--+-++-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "heim in sein Behausung k\u00e4m / k\u00f6nt daselbst zu Ruh sich legen.", "tokens": ["heim", "in", "sein", "Be\u00b7hau\u00b7sung", "k\u00e4m", "/", "k\u00f6nt", "da\u00b7selbst", "zu", "Ruh", "sich", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "APPR", "PPOSAT", "NN", "VVFIN", "$(", "VVFIN", "PAV", "APPR", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "voller Hagel / Strahl / und Wasser / Leid / und Feindsal / ist die Welt:", "tokens": ["vol\u00b7ler", "Ha\u00b7gel", "/", "Strahl", "/", "und", "Was\u00b7ser", "/", "Leid", "/", "und", "Fein\u00b7dsal", "/", "ist", "die", "Welt", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "NN", "$(", "KON", "NN", "$(", "NN", "$(", "KON", "NN", "$(", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "drum das allers\u00fcssest Sterben mich allein zu frieden stellt.", "tokens": ["drum", "das", "al\u00b7ler\u00b7s\u00fcs\u00b7sest", "Ster\u00b7ben", "mich", "al\u00b7lein", "zu", "frie\u00b7den", "stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}