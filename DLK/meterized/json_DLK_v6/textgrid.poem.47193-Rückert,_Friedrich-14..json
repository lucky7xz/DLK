{"textgrid.poem.47193": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nnte Trost mir etwas geben,", "tokens": ["K\u00f6nn\u00b7te", "Trost", "mir", "et\u00b7was", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00f6nnt' es der Gedanke,", "tokens": ["K\u00f6nnt'", "es", "der", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df du sahst vom dorn'gen Leben", "tokens": ["Da\u00df", "du", "sahst", "vom", "dorn'\u00b7gen", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur die bl\u00fch'nde Ranke.", "tokens": ["Nur", "die", "bl\u00fch'n\u00b7de", "Ran\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Da\u00df ich dir so viele Freuden,", "tokens": ["Da\u00df", "ich", "dir", "so", "vie\u00b7le", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als ich konnte, machte,", "tokens": ["Als", "ich", "konn\u00b7te", ",", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Alles an dich zu vergeuden", "tokens": ["Al\u00b7les", "an", "dich", "zu", "ver\u00b7geu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "APPR", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Niemals mich bedachte.", "tokens": ["Nie\u00b7mals", "mich", "be\u00b7dach\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wie die Sonne dich bescheinend", "tokens": ["Wie", "die", "Son\u00b7ne", "dich", "be\u00b7schei\u00b7nend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wie Tau betr\u00e4ufend,", "tokens": ["Und", "wie", "Tau", "be\u00b7tr\u00e4u\u00b7fend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Alle Lust um dich vereinend,", "tokens": ["Al\u00b7le", "Lust", "um", "dich", "ver\u00b7ei\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6nstes auf dich h\u00e4ufend.", "tokens": ["Sch\u00f6ns\u00b7tes", "auf", "dich", "h\u00e4u\u00b7fend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Dich mit allen Blumen schm\u00fcckend,", "tokens": ["Dich", "mit", "al\u00b7len", "Blu\u00b7men", "schm\u00fc\u00b7ckend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blume, die mich schm\u00fcckte;", "tokens": ["Blu\u00b7me", ",", "die", "mich", "schm\u00fcck\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Dich mit jedem Spiel begl\u00fcckend,", "tokens": ["Dich", "mit", "je\u00b7dem", "Spiel", "be\u00b7gl\u00fc\u00b7ckend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spiel, das mich begl\u00fcckte!", "tokens": ["Spiel", ",", "das", "mich", "be\u00b7gl\u00fcck\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Ahnend in dein kurzes Leben", "tokens": ["Ah\u00b7nend", "in", "dein", "kur\u00b7zes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Viel zusammendr\u00e4ngend,", "tokens": ["Viel", "zu\u00b7sam\u00b7men\u00b7dr\u00e4n\u00b7gend", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Es mit liebendem Bestreben", "tokens": ["Es", "mit", "lie\u00b7ben\u00b7dem", "Be\u00b7stre\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Innerlich verl\u00e4ngend.", "tokens": ["In\u00b7ner\u00b7lich", "ver\u00b7l\u00e4n\u00b7gend", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Da\u00df ich dich vor jedem Harme", "tokens": ["Da\u00df", "ich", "dich", "vor", "je\u00b7dem", "Har\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schirmend zu umstricken,", "tokens": ["Schir\u00b7mend", "zu", "um\u00b7stri\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nie dich lie\u00df aus meinem Arme,", "tokens": ["Nie", "dich", "lie\u00df", "aus", "mei\u00b7nem", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nie aus meinen Blicken.", "tokens": ["Nie", "aus", "mei\u00b7nen", "Bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Lie\u00df dich's f\u00fchlen allerwege,", "tokens": ["Lie\u00df", "dich's", "f\u00fch\u00b7len", "al\u00b7ler\u00b7we\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fchlen Nacht und Tage,", "tokens": ["F\u00fch\u00b7len", "Nacht", "und", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df dich Liebe heg' und pflege,", "tokens": ["Da\u00df", "dich", "Lie\u00b7be", "heg'", "und", "pfle\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebe heb' und trage.", "tokens": ["Lie\u00b7be", "heb'", "und", "tra\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Da\u00df ich dich gestrafet nimmer,", "tokens": ["Da\u00df", "ich", "dich", "ge\u00b7stra\u00b7fet", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Selten dich gescholten,", "tokens": ["Sel\u00b7ten", "dich", "ge\u00b7schol\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und mit Schmeicheleien immer", "tokens": ["Und", "mit", "Schmei\u00b7che\u00b7lei\u00b7en", "im\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ADV"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wieder es vergolten.", "tokens": ["Wie\u00b7der", "es", "ver\u00b7gol\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Da\u00df ich angethan dir habe,", "tokens": ["Da\u00df", "ich", "an\u00b7ge\u00b7than", "dir", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was ich Liebes wu\u00dfte,", "tokens": ["Was", "ich", "Lie\u00b7bes", "wu\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und zuletzt dich nur dem Grabe", "tokens": ["Und", "zu\u00b7letzt", "dich", "nur", "dem", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hingab, weil ich mu\u00dfte.", "tokens": ["Hin\u00b7gab", ",", "weil", "ich", "mu\u00df\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VMFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.10": {"line.1": {"text": "Heller w\u00fcrd' ich nun im Herzen", "tokens": ["Hel\u00b7ler", "w\u00fcrd'", "ich", "nun", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch dein Bild bewahren,", "tokens": ["Noch", "dein", "Bild", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "H\u00e4tt' ich nur des Todes Schmerzen", "tokens": ["H\u00e4tt'", "ich", "nur", "des", "To\u00b7des", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00f6nnen dir ersparen.", "tokens": ["K\u00f6n\u00b7nen", "dir", "er\u00b7spa\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}