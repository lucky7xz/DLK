{"textgrid.poem.32811": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "S\u00e4kulargesang beim Anfang des neunzehnten Jahrhunderts", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wirf, schrecklichstes von allen, die noch waren,", "tokens": ["Wirf", ",", "schreck\u00b7lichs\u00b7tes", "von", "al\u00b7len", ",", "die", "noch", "wa\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "APPR", "PIAT", "$,", "PRELS", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wirf, blutbeflecktestes von allen Jahren,", "tokens": ["Wirf", ",", "blut\u00b7be\u00b7fleck\u00b7tes\u00b7tes", "von", "al\u00b7len", "Jah\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wirf, o Jahrhundert, mir noch ", "tokens": ["Wirf", ",", "o", "Jahr\u00b7hun\u00b7dert", ",", "mir", "noch"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "FM", "NN", "$,", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eh' du entfleuchst, in unsre Welt zur\u00fcck.", "tokens": ["Eh'", "du", "ent\u00b7fleuchst", ",", "in", "uns\u00b7re", "Welt", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "H\u00f6r ", "tokens": ["H\u00f6r"], "token_info": ["word"], "pos": ["NE"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Blick nieder auf die Millionen Thr\u00e4nen,", "tokens": ["Blick", "nie\u00b7der", "auf", "die", "Mil\u00b7lion\u00b7en", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Deutschlands Flur, von Leichnamen ged\u00fcngt,", "tokens": ["Die", "Deutschlands", "Flur", ",", "von", "Leich\u00b7na\u00b7men", "ge\u00b7d\u00fcngt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Und Rhein und Donau blutger\u00f6tet trinkt.", "tokens": ["Und", "Rhein", "und", "Do\u00b7nau", "blut\u00b7ge\u00b7r\u00f6\u00b7tet", "trinkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "KON", "NE", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "H\u00f6r, an der H\u00fctt' und des Palastes Tr\u00fcmmern", "tokens": ["H\u00f6r", ",", "an", "der", "H\u00fctt'", "und", "des", "Pa\u00b7las\u00b7tes", "Tr\u00fcm\u00b7mern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "ART", "NN", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hinauf zu Gott die Halberstarrten wimmern!", "tokens": ["Hin\u00b7auf", "zu", "Gott", "die", "Hal\u00b7ber\u00b7starr\u00b7ten", "wim\u00b7mern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vernimm mit Schaudern, wie auf banger Flucht", "tokens": ["Ver\u00b7nimm", "mit", "Schau\u00b7dern", ",", "wie", "auf", "ban\u00b7ger", "Flucht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "NN", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der eine betet, dort der andre flucht!", "tokens": ["Der", "ei\u00b7ne", "be\u00b7tet", ",", "dort", "der", "and\u00b7re", "flucht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "ADV", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und nun erheb, hinauf vom Kriegsgewimmel,", "tokens": ["Und", "nun", "er\u00b7heb", ",", "hin\u00b7auf", "vom", "Kriegs\u00b7ge\u00b7wim\u00b7mel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dich wieder zu des Allerbarmers Himmel,", "tokens": ["Dich", "wie\u00b7der", "zu", "des", "Al\u00b7ler\u00b7bar\u00b7mers", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der, uns zu z\u00fccht'gen, dich herabgeschickt!", "tokens": ["Der", ",", "uns", "zu", "z\u00fccht'\u00b7gen", ",", "dich", "her\u00b7ab\u00b7ge\u00b7schickt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "PPER", "PTKZU", "VVINF", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf, und erz\u00e4hl ihm, was dein Aug' erblickt!", "tokens": ["Auf", ",", "und", "er\u00b7z\u00e4hl", "ihm", ",", "was", "dein", "Aug'", "er\u00b7blickt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "KON", "VVFIN", "PPER", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sag ihm, da\u00df unter all den Millionen", "tokens": ["Sag", "ihm", ",", "da\u00df", "un\u00b7ter", "all", "den", "Mil\u00b7lion\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KOUS", "APPR", "PIAT", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von B\u00f6sen auch noch gute Menschen wohnen,", "tokens": ["Von", "B\u00f6\u00b7sen", "auch", "noch", "gu\u00b7te", "Men\u00b7schen", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die, schlecht und recht, das ", "tokens": ["Die", ",", "schlecht", "und", "recht", ",", "das"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "$,", "ADJD", "KON", "ADJD", "$,", "PRELS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ringsum begl\u00fcckt die ganze Welt zu sehn.", "tokens": ["Ring\u00b7sum", "be\u00b7gl\u00fcckt", "die", "gan\u00b7ze", "Welt", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "Leg, an der Seite deiner \u00e4ltern Br\u00fcder,", "tokens": ["Leg", ",", "an", "der", "Sei\u00b7te", "dei\u00b7ner", "\u00e4l\u00b7tern", "Br\u00fc\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor seinem Thron die tausend Seufzer nieder,", "tokens": ["Vor", "sei\u00b7nem", "Thron", "die", "tau\u00b7send", "Seuf\u00b7zer", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die, Jahre schon vom Kriegessturm verweht,", "tokens": ["Die", ",", "Jah\u00b7re", "schon", "vom", "Krie\u00b7ges\u00b7sturm", "ver\u00b7weht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "NN", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Umsonst den Frieden uns herabgefleht.", "tokens": ["Um\u00b7sonst", "den", "Frie\u00b7den", "uns", "her\u00b7ab\u00b7ge\u00b7fleht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Dann senket sich im hellsten Morgenglanze,", "tokens": ["Dann", "sen\u00b7ket", "sich", "im", "hells\u00b7ten", "Mor\u00b7gen\u00b7glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Schl\u00e4f' umgr\u00fcnt vom frischen Myrtenkranze,", "tokens": ["Die", "Schl\u00e4f'", "um\u00b7gr\u00fcnt", "vom", "fri\u00b7schen", "Myr\u00b7ten\u00b7kran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Palmzweig in der hochgehobnen Hand,", "tokens": ["Den", "Palm\u00b7zweig", "in", "der", "hoch\u00b7ge\u00b7hob\u00b7nen", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Friedensengel aufs verheerte Land.", "tokens": ["Der", "Frie\u00b7den\u00b7sen\u00b7gel", "aufs", "ver\u00b7heer\u00b7te", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Die Menschheit jauchzt mit lauten Herzensschl\u00e4gen", "tokens": ["Die", "Menschheit", "jauchzt", "mit", "lau\u00b7ten", "Her\u00b7zens\u00b7schl\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-++-+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Dem Kommenden ihr Jubellied entgegen.", "tokens": ["Dem", "Kom\u00b7men\u00b7den", "ihr", "Ju\u00b7bel\u00b7lied", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Krieg durchbohrt wutknirschend sich die Brust,", "tokens": ["Der", "Krieg", "durch\u00b7bohrt", "wut\u00b7knir\u00b7schend", "sich", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und alles schwimmt in neuer Lebenslust.", "tokens": ["Und", "al\u00b7les", "schwimmt", "in", "neu\u00b7er", "Le\u00b7bens\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}