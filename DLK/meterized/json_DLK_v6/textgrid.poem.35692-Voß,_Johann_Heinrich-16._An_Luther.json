{"textgrid.poem.35692": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "16. An Luther", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Entschwebe, wie ein goldner Duft,", "tokens": ["Ent\u00b7schwe\u00b7be", ",", "wie", "ein", "gold\u00b7ner", "Duft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mann Gottes, deiner stillen Gruft,", "tokens": ["Mann", "Got\u00b7tes", ",", "dei\u00b7ner", "stil\u00b7len", "Gruft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schaure Graun durch ihr Gebein,", "tokens": ["Und", "schau\u00b7re", "Graun", "durch", "ihr", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die deine stille Gruft entweihn!", "tokens": ["Die", "dei\u00b7ne", "stil\u00b7le", "Gruft", "ent\u00b7weihn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ermattet von dem Drachenkampf", "tokens": ["Er\u00b7mat\u00b7tet", "von", "dem", "Dra\u00b7chen\u00b7kampf"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Priestern in der H\u00f6llen Dampf,", "tokens": ["Mit", "Pries\u00b7tern", "in", "der", "H\u00f6l\u00b7len", "Dampf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sogst du an Katharinens Brust", "tokens": ["Sogst", "du", "an", "Ka\u00b7tha\u00b7ri\u00b7nens", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "APPR", "NE", "NN"], "meter": "-------+", "measure": "unknown.measure.single"}, "line.4": {"text": "Dir junge Kraft und Heldenlust.", "tokens": ["Dir", "jun\u00b7ge", "Kraft", "und", "Hel\u00b7den\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie tr\u00e4nkte dich mit Rebentrank;", "tokens": ["Sie", "tr\u00e4nk\u00b7te", "dich", "mit", "Re\u00b7ben\u00b7trank", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und freudig t\u00f6nte dein Gesang:", "tokens": ["Und", "freu\u00b7dig", "t\u00f6n\u00b7te", "dein", "Ge\u00b7sang", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbdem Papst und allen Teufeln Spott!", "tokens": ["\u00bb", "dem", "Papst", "und", "al\u00b7len", "Teu\u00b7feln", "Spott", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "KON", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein' feste Burg ist unser Gott!\u00ab", "tokens": ["Ein'", "fes\u00b7te", "Burg", "ist", "un\u00b7ser", "Gott", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da zischelt nun die Afterbrut:", "tokens": ["Da", "zi\u00b7schelt", "nun", "die", "Af\u00b7ter\u00b7brut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbweh, Br\u00fcder, weh! wir sind sein Blut!", "tokens": ["\u00bb", "weh", ",", "Br\u00fc\u00b7der", ",", "weh", "!", "wir", "sind", "sein", "Blut", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "NN", "$,", "PTKVZ", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schleicht r\u00fccklings hin zu seiner Ruh,", "tokens": ["Schleicht", "r\u00fcck\u00b7lings", "hin", "zu", "sei\u00b7ner", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und deckt die Scham des Vaters zu!\u00ab", "tokens": ["Und", "deckt", "die", "Scham", "des", "Va\u00b7ters", "zu", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ihr M\u00e4nner Deutschlands, k\u00fchn und frei", "tokens": ["Ihr", "M\u00e4n\u00b7ner", "Deutschlands", ",", "k\u00fchn", "und", "frei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NE", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Durch ihn von Pfaffentyrannei!", "tokens": ["Durch", "ihn", "von", "Pfaf\u00b7fen\u00b7ty\u00b7ran\u00b7nei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ihr la\u00dft mit l\u00e4sterndem Gest\u00f6hn", "tokens": ["Ihr", "la\u00dft", "mit", "l\u00e4s\u00b7tern\u00b7dem", "Ge\u00b7st\u00f6hn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Heuchler Luthers Asche schm\u00e4hn?", "tokens": ["Die", "Heuch\u00b7ler", "Lu\u00b7thers", "A\u00b7sche", "schm\u00e4hn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wer ist, der nicht beim Kraftgesang", "tokens": ["Wer", "ist", ",", "der", "nicht", "beim", "Kraft\u00b7ge\u00b7sang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "PTKNEG", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Weisen auf zu Thaten sprang,", "tokens": ["Des", "Wei\u00b7sen", "auf", "zu", "Tha\u00b7ten", "sprang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem nicht die Seele sonnenhoch,", "tokens": ["Dem", "nicht", "die", "See\u00b7le", "son\u00b7nen\u00b7hoch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Adler mit dem Adler, flog?", "tokens": ["Ein", "Ad\u00b7ler", "mit", "dem", "Ad\u00b7ler", ",", "flog", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wem schafft nicht Gottes edler Wein", "tokens": ["Wem", "schafft", "nicht", "Got\u00b7tes", "ed\u00b7ler", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Donnerwolken Sonnenschein,", "tokens": ["Aus", "Don\u00b7ner\u00b7wol\u00b7ken", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und rei\u00dft der Lebensgeister Tanz", "tokens": ["Und", "rei\u00dft", "der", "Le\u00b7bens\u00b7geis\u00b7ter", "Tanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Tugendkampf und Siegeskranz?", "tokens": ["Zum", "Tu\u00b7gend\u00b7kampf", "und", "Sie\u00b7ges\u00b7kranz", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Was labt den Frommen in der Zeit", "tokens": ["Was", "labt", "den", "From\u00b7men", "in", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Ahndung h\u00f6hrer Seligkeit,", "tokens": ["Mit", "Ahn\u00b7dung", "h\u00f6h\u00b7rer", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als M\u00e4dchenblick und M\u00e4dchenku\u00df,", "tokens": ["Als", "M\u00e4d\u00b7chen\u00b7blick", "und", "M\u00e4d\u00b7chen\u00b7ku\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Weibes heiliger Genu\u00df?", "tokens": ["Des", "Wei\u00b7bes", "hei\u00b7li\u00b7ger", "Ge\u00b7nu\u00df", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Schweig, Glei\u00dfner, dich befrag' ich nicht!", "tokens": ["Schweig", ",", "Glei\u00df\u00b7ner", ",", "dich", "be\u00b7frag'", "ich", "nicht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir bleibt dies ewig ein Gedicht,", "tokens": ["Dir", "bleibt", "dies", "e\u00b7wig", "ein", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie dem, der Lastern Lieder zollt,", "tokens": ["Wie", "dem", ",", "der", "Las\u00b7tern", "Lie\u00b7der", "zollt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "$,", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Hurer, und dem Trunkenbold!", "tokens": ["Dem", "Hu\u00b7rer", ",", "und", "dem", "Trun\u00b7ken\u00b7bold", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Doch jeder Christ und gute Mann", "tokens": ["Doch", "je\u00b7der", "Christ", "und", "gu\u00b7te", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stimmt laut mit dir, o Vater, an:", "tokens": ["Stimmt", "laut", "mit", "dir", ",", "o", "Va\u00b7ter", ",", "an", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "$,", "FM", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbwer nicht liebt Weib, Wein und Gesang,", "tokens": ["\u00bb", "wer", "nicht", "liebt", "Weib", ",", "Wein", "und", "Ge\u00b7sang", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PTKNEG", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der bleibt ein Narr sein Lebelang!\u00ab", "tokens": ["Der", "bleibt", "ein", "Narr", "sein", "Le\u00b7be\u00b7lang", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}