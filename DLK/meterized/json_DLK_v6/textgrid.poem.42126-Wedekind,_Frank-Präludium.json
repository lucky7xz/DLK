{"textgrid.poem.42126": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Pr\u00e4ludium", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es graut der Morgen und die Sterne sinken,", "tokens": ["Es", "graut", "der", "Mor\u00b7gen", "und", "die", "Ster\u00b7ne", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bis alle in der kalten Flut ertrinken.", "tokens": ["Bis", "al\u00b7le", "in", "der", "kal\u00b7ten", "Flut", "er\u00b7trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die gro\u00dfe Sonne majest\u00e4tisch brennt", "tokens": ["Die", "gro\u00b7\u00dfe", "Son\u00b7ne", "ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "brennt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Schon feuerrot am fernen Firmament.", "tokens": ["Schon", "feu\u00b7er\u00b7rot", "am", "fer\u00b7nen", "Fir\u00b7ma\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Kalliope, die sch\u00f6nste der neun Musen,", "tokens": ["Kal\u00b7li\u00b7o\u00b7pe", ",", "die", "sch\u00f6ns\u00b7te", "der", "neun", "Mu\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "VVFIN", "ART", "CARD", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Erhebt sich in der goldnen Strahlen Schein", "tokens": ["Er\u00b7hebt", "sich", "in", "der", "gold\u00b7nen", "Strah\u00b7len", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Von ihrem Lager, und ihr stolzer Busen", "tokens": ["Von", "ih\u00b7rem", "La\u00b7ger", ",", "und", "ihr", "stol\u00b7zer", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Saugt lechzend frische Morgend\u00fcfte ein.", "tokens": ["Saugt", "lech\u00b7zend", "fri\u00b7sche", "Mor\u00b7gen\u00b7d\u00fcf\u00b7te", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Noch ganz entkleidet, ohne mit den Reizen", "tokens": ["Noch", "ganz", "ent\u00b7klei\u00b7det", ",", "oh\u00b7ne", "mit", "den", "Rei\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVPP", "$,", "KOUI", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der hohen g\u00f6ttlichen Gestalt zu geizen,", "tokens": ["Der", "ho\u00b7hen", "g\u00f6tt\u00b7li\u00b7chen", "Ge\u00b7stalt", "zu", "gei\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Tritt sie hinaus ins Freie der Natur.", "tokens": ["Tritt", "sie", "hin\u00b7aus", "ins", "Frei\u00b7e", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APZR", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Aus ihren gro\u00dfen, dunkelblauen Augen spr\u00fchen", "tokens": ["Aus", "ih\u00b7ren", "gro\u00b7\u00dfen", ",", "dun\u00b7kel\u00b7blau\u00b7en", "Au\u00b7gen", "spr\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Schon wieder neue, wunderbare Phantasien,", "tokens": ["Schon", "wie\u00b7der", "neu\u00b7e", ",", "wun\u00b7der\u00b7ba\u00b7re", "Phan\u00b7ta\u00b7si\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Und ihr Gedanke folgt der irren Spur", "tokens": ["Und", "ihr", "Ge\u00b7dan\u00b7ke", "folgt", "der", "ir\u00b7ren", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Der teuren Helden, die sie zu besingen", "tokens": ["Der", "teu\u00b7ren", "Hel\u00b7den", ",", "die", "sie", "zu", "be\u00b7sin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Die straffgespannten Saiten l\u00e4\u00dft erklingen.", "tokens": ["Die", "straff\u00b7ge\u00b7spann\u00b7ten", "Sai\u00b7ten", "l\u00e4\u00dft", "er\u00b7klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Des Waldes dunkle K\u00fchle nimmt sie auf,", "tokens": ["Des", "Wal\u00b7des", "dunk\u00b7le", "K\u00fch\u00b7le", "nimmt", "sie", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und folgend eines Baches klarem Lauf", "tokens": ["Und", "fol\u00b7gend", "ei\u00b7nes", "Ba\u00b7ches", "kla\u00b7rem", "Lauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Gelangt sie rasch mit zielbewu\u00dftem Schritte", "tokens": ["Ge\u00b7langt", "sie", "rasch", "mit", "ziel\u00b7be\u00b7wu\u00df\u00b7tem", "Schrit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "In ihres Reiches unwegsame Mitte.", "tokens": ["In", "ih\u00b7res", "Rei\u00b7ches", "un\u00b7weg\u00b7sa\u00b7me", "Mit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Hier l\u00e4\u00dft sie sich auf einen Baumstumpf nieder.", "tokens": ["Hier", "l\u00e4\u00dft", "sie", "sich", "auf", "ei\u00b7nen", "Baum\u00b7stumpf", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Im weiten Umkreis herrscht das tiefste Schweigen", "tokens": ["Im", "wei\u00b7ten", "Um\u00b7kreis", "herrscht", "das", "tiefs\u00b7te", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Bis auf ein Wispern in den h\u00f6chsten Zweigen,", "tokens": ["Bis", "auf", "ein", "Wis\u00b7pern", "in", "den", "h\u00f6chs\u00b7ten", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Bis auf ein Felsenecho ihrer Lieder.", "tokens": ["Bis", "auf", "ein", "Fel\u00b7sen\u00b7e\u00b7cho", "ih\u00b7rer", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Die Strahlen schie\u00dfen senkrecht nun herunter,", "tokens": ["Die", "Strah\u00b7len", "schie\u00b7\u00dfen", "sen\u00b7krecht", "nun", "her\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Die ganze Sch\u00f6pfung, eben noch so munter,", "tokens": ["Die", "gan\u00b7ze", "Sch\u00f6p\u00b7fung", ",", "e\u00b7ben", "noch", "so", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Erschlafft im Zittern ausgesto\u00dfner Gluten.", "tokens": ["Er\u00b7schlafft", "im", "Zit\u00b7tern", "aus\u00b7ge\u00b7sto\u00df\u00b7ner", "Glu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Kalliope tritt an des Baches Rand,", "tokens": ["Kal\u00b7li\u00b7o\u00b7pe", "tritt", "an", "des", "Ba\u00b7ches", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.29": {"text": "Sie legt die goldne Laute aus der Hand,", "tokens": ["Sie", "legt", "die", "gold\u00b7ne", "Lau\u00b7te", "aus", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Sie steigt hinab in die kristallnen Fluten.", "tokens": ["Sie", "steigt", "hin\u00b7ab", "in", "die", "kris\u00b7tall\u00b7nen", "Flu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Die Wasser kommen z\u00f6gernd angezogen,", "tokens": ["Die", "Was\u00b7ser", "kom\u00b7men", "z\u00f6\u00b7gernd", "an\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Sie l\u00e4\u00dft von ihnen sich das Haar zerw\u00fchlen,", "tokens": ["Sie", "l\u00e4\u00dft", "von", "ih\u00b7nen", "sich", "das", "Haar", "zer\u00b7w\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Die volle Brust, den wei\u00dfen Leib besp\u00fclen,", "tokens": ["Die", "vol\u00b7le", "Brust", ",", "den", "wei\u00b7\u00dfen", "Leib", "be\u00b7sp\u00fc\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Gl\u00fcckatmend treibt sie auf den kalten Wogen.", "tokens": ["Gl\u00fcc\u00b7kat\u00b7mend", "treibt", "sie", "auf", "den", "kal\u00b7ten", "Wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Sie dichtet summend eine Melodie,", "tokens": ["Sie", "dich\u00b7tet", "sum\u00b7mend", "ei\u00b7ne", "Me\u00b7lo\u00b7die", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Gedanken haben Fleisch und Blut erhalten,", "tokens": ["Ge\u00b7dan\u00b7ken", "ha\u00b7ben", "Fleisch", "und", "Blut", "er\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Als Menschenkinder wandeln die Gestalten", "tokens": ["Als", "Men\u00b7schen\u00b7kin\u00b7der", "wan\u00b7deln", "die", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Vorbei an ihrer klaren Phantasie.", "tokens": ["Vor\u00b7bei", "an", "ih\u00b7rer", "kla\u00b7ren", "Phan\u00b7ta\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Im sch\u00f6nen Land Italien weilt ihr Sinn,", "tokens": ["Im", "sch\u00f6\u00b7nen", "Land", "I\u00b7ta\u00b7li\u00b7en", "weilt", "ihr", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.40": {"text": "Ihr Herz verschwendet seine reichsten Gaben.", "tokens": ["Ihr", "Herz", "ver\u00b7schwen\u00b7det", "sei\u00b7ne", "reichs\u00b7ten", "Ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Sie singt von Felix, einem Hirtenknaben,", "tokens": ["Sie", "singt", "von", "Fe\u00b7lix", ",", "ei\u00b7nem", "Hir\u00b7ten\u00b7kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Von Galathea einer Sch\u00e4ferin.", "tokens": ["Von", "Ga\u00b7la\u00b7thea", "ei\u00b7ner", "Sch\u00e4\u00b7fe\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}