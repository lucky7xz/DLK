{"textgrid.poem.65685": {"metadata": {"author": {"name": "Drollinger, Carl Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Dichtkunst mahlt durch Kiel und Schrift.", "genre": "verse", "period": "N.A.", "pub_year": 1715, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Dichtkunst mahlt durch Kiel und Schrift.", "tokens": ["Die", "Dicht\u00b7kunst", "mahlt", "durch", "Kiel", "und", "Schrift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es zeigt ihr die Natur der Bilder \u00e4chte Z\u00fcge.", "tokens": ["Es", "zeigt", "ihr", "die", "Na\u00b7tur", "der", "Bil\u00b7der", "\u00e4ch\u00b7te", "Z\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr schw\u00e4rstes ist der Mensch. Wer ists, der solchen trifft,", "tokens": ["Ihr", "schw\u00e4rs\u00b7tes", "ist", "der", "Mensch", ".", "Wer", "ists", ",", "der", "sol\u00b7chen", "trifft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "ART", "NN", "$.", "PWS", "VAFIN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn ihn die Klugheit nicht entbl\u00f6\u00dft von Larv und L\u00fcge?", "tokens": ["Wenn", "ihn", "die", "Klug\u00b7heit", "nicht", "ent\u00b7bl\u00f6\u00dft", "von", "Larv", "und", "L\u00fc\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und da ein jedes Bild von einem Sch\u00f6pfer spricht,", "tokens": ["Und", "da", "ein", "je\u00b7des", "Bild", "von", "ei\u00b7nem", "Sch\u00f6p\u00b7fer", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "PIAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So ist auch dessen Ruhm der Dichtkunst erste Pflicht.", "tokens": ["So", "ist", "auch", "des\u00b7sen", "Ruhm", "der", "Dicht\u00b7kunst", "ers\u00b7te", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}