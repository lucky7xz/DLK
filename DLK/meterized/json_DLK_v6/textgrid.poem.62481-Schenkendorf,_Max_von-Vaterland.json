{"textgrid.poem.62481": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Vaterland", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Vaterland, das droben ist,", "tokens": ["O", "Va\u00b7ter\u00b7land", ",", "das", "dro\u00b7ben", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das uns der Heiland Jesus Christ", "tokens": ["Das", "uns", "der", "Hei\u00b7land", "Je\u00b7sus", "Christ"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "ART", "NN", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Ewigkeit bereitet!", "tokens": ["Von", "E\u00b7wig\u00b7keit", "be\u00b7rei\u00b7tet", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie herrlich wird es droben sein,", "tokens": ["Wie", "herr\u00b7lich", "wird", "es", "dro\u00b7ben", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn er aus allem Streit und Pein", "tokens": ["Wenn", "er", "aus", "al\u00b7lem", "Streit", "und", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PIS", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu deiner Lust uns leitet!", "tokens": ["Zu", "dei\u00b7ner", "Lust", "uns", "lei\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Auch hier, auch hier im Erdenthal", "tokens": ["Auch", "hier", ",", "auch", "hier", "im", "Er\u00b7dent\u00b7hal"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weht Gottes Hauch, scheint Gottes Strahl;", "tokens": ["Weht", "Got\u00b7tes", "Hauch", ",", "scheint", "Got\u00b7tes", "Strahl", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "NN", "$,", "VVFIN", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir haben auch empfangen", "tokens": ["Wir", "ha\u00b7ben", "auch", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den rechten Geist, den Geist vom Herrn;", "tokens": ["Den", "rech\u00b7ten", "Geist", ",", "den", "Geist", "vom", "Herrn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Allen ist ein heil'ger Stern", "tokens": ["Und", "Al\u00b7len", "ist", "ein", "heil'\u00b7ger", "Stern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Am Himmel aufgegangen.", "tokens": ["Am", "Him\u00b7mel", "auf\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das ist das ew'ge Gotteswort,", "tokens": ["Das", "ist", "das", "ew'\u00b7ge", "Got\u00b7tes\u00b7wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es kommt vom Himmel fort und fort", "tokens": ["Es", "kommt", "vom", "Him\u00b7mel", "fort", "und", "fort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zur Erde segnend nieder.", "tokens": ["Zur", "Er\u00b7de", "seg\u00b7nend", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das nehmen alle Menschen an,", "tokens": ["Das", "neh\u00b7men", "al\u00b7le", "Men\u00b7schen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und alle Menschen, Mann f\u00fcr Mann,", "tokens": ["Und", "al\u00b7le", "Men\u00b7schen", ",", "Mann", "f\u00fcr", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sind S\u00fcnder nur und Br\u00fcder.", "tokens": ["Sind", "S\u00fcn\u00b7der", "nur", "und", "Br\u00fc\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch jedem Volke ward ein Grund", "tokens": ["Doch", "je\u00b7dem", "Vol\u00b7ke", "ward", "ein", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Bau des Reiches Gottes kund,", "tokens": ["Zum", "Bau", "des", "Rei\u00b7ches", "Got\u00b7tes", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da soll sein Tempel stehen;", "tokens": ["Da", "soll", "sein", "Tem\u00b7pel", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus tiefem Grund, von unten aus", "tokens": ["Aus", "tie\u00b7fem", "Grund", ",", "von", "un\u00b7ten", "aus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADV", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Soll sich das ew'ge Gotteshaus", "tokens": ["Soll", "sich", "das", "ew'\u00b7ge", "Got\u00b7tes\u00b7haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erheben zu den H\u00f6hen.", "tokens": ["Er\u00b7he\u00b7ben", "zu", "den", "H\u00f6\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Im Vaterland, im Vaterland", "tokens": ["Im", "Va\u00b7ter\u00b7land", ",", "im", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat Jeder seinen rechten Stand", "tokens": ["Hat", "Je\u00b7der", "sei\u00b7nen", "rech\u00b7ten", "Stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und rechten Grund gefunden.", "tokens": ["Und", "rech\u00b7ten", "Grund", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da stehe fest und halte drauf!", "tokens": ["Da", "ste\u00b7he", "fest", "und", "hal\u00b7te", "drauf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und fl\u00f6hest du im schnellen Lauf,", "tokens": ["Und", "fl\u00f6\u00b7hest", "du", "im", "schnel\u00b7len", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es h\u00e4lt dich doch gebunden.", "tokens": ["Es", "h\u00e4lt", "dich", "doch", "ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich ziehe nimmer weit hinaus,", "tokens": ["Ich", "zie\u00b7he", "nim\u00b7mer", "weit", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bin daheim in meinem Haus,", "tokens": ["Ich", "bin", "da\u00b7heim", "in", "mei\u00b7nem", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im sch\u00f6nen deutschen Lande.", "tokens": ["Im", "sch\u00f6\u00b7nen", "deut\u00b7schen", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im ganzen weiten Vaterland", "tokens": ["Im", "gan\u00b7zen", "wei\u00b7ten", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ist alles traut mir und bekannt", "tokens": ["Ist", "al\u00b7les", "traut", "mir", "und", "be\u00b7kannt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "VVFIN", "PPER", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In jedem frommen Stande.", "tokens": ["In", "je\u00b7dem", "from\u00b7men", "Stan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die hohen Kunden alter Zeit,", "tokens": ["Die", "ho\u00b7hen", "Kun\u00b7den", "al\u00b7ter", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Tage, die uns j\u00fcngst erfreut,", "tokens": ["Die", "Ta\u00b7ge", ",", "die", "uns", "j\u00fcngst", "er\u00b7freut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das sch\u00f6ne, freie Leben;", "tokens": ["Das", "sch\u00f6\u00b7ne", ",", "frei\u00b7e", "Le\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch manches Schlo\u00df und manche Stadt,", "tokens": ["Auch", "man\u00b7ches", "Schlo\u00df", "und", "man\u00b7che", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die deutsche Kraft erbauet hat,", "tokens": ["Die", "deut\u00b7sche", "Kraft", "er\u00b7bau\u00b7et", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo V\u00e4tergeister schweben.", "tokens": ["Wo", "V\u00e4\u00b7ter\u00b7geis\u00b7ter", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ihr H\u00fcgel, wo die Trauben bl\u00fchn,", "tokens": ["Ihr", "H\u00fc\u00b7gel", ",", "wo", "die", "Trau\u00b7ben", "bl\u00fchn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Felder, wo sich Schnitter m\u00fchn,", "tokens": ["Ihr", "Fel\u00b7der", ",", "wo", "sich", "Schnit\u00b7ter", "m\u00fchn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "PRF", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sollt auf den Enkel kommen.", "tokens": ["Sollt", "auf", "den", "En\u00b7kel", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Kirchen, hoch und k\u00fchn und zart,", "tokens": ["Ihr", "Kir\u00b7chen", ",", "hoch", "und", "k\u00fchn", "und", "zart", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erdacht nach alter deutscher Art,", "tokens": ["Er\u00b7dacht", "nach", "al\u00b7ter", "deut\u00b7scher", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Euch lieben alle Frommen.", "tokens": ["Euch", "lie\u00b7ben", "al\u00b7le", "From\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Zum Eichenwald, zum Eichenwald,", "tokens": ["Zum", "Ei\u00b7chen\u00b7wald", ",", "zum", "Ei\u00b7chen\u00b7wald", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Gott in hohen Wipfeln wallt,", "tokens": ["Wo", "Gott", "in", "ho\u00b7hen", "Wip\u00b7feln", "wallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "M\u00f6cht' ich wol t\u00e4glich wandern.", "tokens": ["M\u00f6cht'", "ich", "wol", "t\u00e4g\u00b7lich", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du frommes, k\u00fchnes, deutsches Wort,", "tokens": ["Du", "from\u00b7mes", ",", "k\u00fch\u00b7nes", ",", "deut\u00b7sches", "Wort", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du bist der rechte Schild und Hort", "tokens": ["Du", "bist", "der", "rech\u00b7te", "Schild", "und", "Hort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zur Scheidung von den Andern.", "tokens": ["Zur", "Schei\u00b7dung", "von", "den", "An\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Das ist das deutsche Vaterland,", "tokens": ["Das", "ist", "das", "deut\u00b7sche", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da, J\u00fcngling, Jungfrau, sei dein Stand,", "tokens": ["Da", ",", "J\u00fcng\u00b7ling", ",", "Jung\u00b7frau", ",", "sei", "dein", "Stand", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da f\u00fchre du dein Leben!", "tokens": ["Da", "f\u00fch\u00b7re", "du", "dein", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da will ich stehn, ein gr\u00fcner Baum,", "tokens": ["Da", "will", "ich", "stehn", ",", "ein", "gr\u00fc\u00b7ner", "Baum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Will tr\u00e4umen manchen sel'gen Traum", "tokens": ["Will", "tr\u00e4u\u00b7men", "man\u00b7chen", "sel'\u00b7gen", "Traum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und nach dem Himmel streben.", "tokens": ["Und", "nach", "dem", "Him\u00b7mel", "stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}