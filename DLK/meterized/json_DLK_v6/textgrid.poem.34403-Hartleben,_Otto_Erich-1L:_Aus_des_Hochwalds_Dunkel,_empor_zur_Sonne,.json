{"textgrid.poem.34403": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus des Hochwalds Dunkel, empor zur Sonne,", "genre": "verse", "period": "N.A.", "pub_year": 1885, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus des Hochwalds Dunkel, empor zur Sonne,", "tokens": ["Aus", "des", "Hoch\u00b7walds", "Dun\u00b7kel", ",", "em\u00b7por", "zur", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "PTKVZ", "APPRART", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "die hindurchblitzt zwischen dem Laub der Kronen,", "tokens": ["die", "hin\u00b7durch\u00b7blitzt", "zwi\u00b7schen", "dem", "Laub", "der", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "ringt und w\u00e4chst und strebt in die H\u00f6h das junge", "tokens": ["ringt", "und", "w\u00e4chst", "und", "strebt", "in", "die", "H\u00f6h", "das", "jun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "schwankende St\u00e4mmchen.", "tokens": ["schwan\u00b7ken\u00b7de", "St\u00e4mm\u00b7chen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Nicht gedeihn kanns drunten im kalten Schatten,", "tokens": ["Nicht", "ge\u00b7deihn", "kanns", "drun\u00b7ten", "im", "kal\u00b7ten", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "aber droben l\u00e4chelt ihm Licht und W\u00e4rme,", "tokens": ["a\u00b7ber", "dro\u00b7ben", "l\u00e4\u00b7chelt", "ihm", "Licht", "und", "W\u00e4r\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "droben wirds im sonnigen Blau des Aethers", "tokens": ["dro\u00b7ben", "wirds", "im", "son\u00b7ni\u00b7gen", "Blau", "des", "A\u00b7e\u00b7thers"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+--+----", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "wiegen das Haupt einst. \u2013", "tokens": ["wie\u00b7gen", "das", "Haupt", "einst", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Auch du witterst und sp\u00fcrst, o meine Seele,", "tokens": ["Auch", "du", "wit\u00b7terst", "und", "sp\u00fcrst", ",", "o", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "KON", "VVFIN", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "hoch ob all der lastenden Nacht der Schmerzen", "tokens": ["hoch", "ob", "all", "der", "las\u00b7ten\u00b7den", "Nacht", "der", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KOUS", "PIAT", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "eines blauen, nimmer getr\u00fcbten Himmels", "tokens": ["ei\u00b7nes", "blau\u00b7en", ",", "nim\u00b7mer", "ge\u00b7tr\u00fcb\u00b7ten", "Him\u00b7mels"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADV", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "g\u00f6ttliche Reinheit.", "tokens": ["g\u00f6tt\u00b7li\u00b7che", "Rein\u00b7heit", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Auch du dr\u00e4nge zur H\u00f6h, o meine Seele,", "tokens": ["Auch", "du", "dr\u00e4n\u00b7ge", "zur", "H\u00f6h", ",", "o", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "APPRART", "NN", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "bis dich kr\u00f6nt das leuchtende Gold der Sonne,", "tokens": ["bis", "dich", "kr\u00f6nt", "das", "leuch\u00b7ten\u00b7de", "Gold", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "bis vergessen unter dir schweigt des Lebens", "tokens": ["bis", "ver\u00b7ges\u00b7sen", "un\u00b7ter", "dir", "schweigt", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "APPR", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "wuchernde Wildnis.", "tokens": ["wu\u00b7chern\u00b7de", "Wild\u00b7nis", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NE", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}