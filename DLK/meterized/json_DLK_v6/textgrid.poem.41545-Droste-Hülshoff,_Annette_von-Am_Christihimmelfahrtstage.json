{"textgrid.poem.41545": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am Christihimmelfahrtstage", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er war ihr eigen dreiunddrei\u00dfig Jahr',", "tokens": ["Er", "war", "ihr", "ei\u00b7gen", "drei\u00b7und\u00b7drei\u00b7\u00dfig", "Jahr'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Zeit ist hin, ist hin!", "tokens": ["Die", "Zeit", "ist", "hin", ",", "ist", "hin", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "VAFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie ist sie doch nun alles Glanzes bar", "tokens": ["Wie", "ist", "sie", "doch", "nun", "al\u00b7les", "Glan\u00b7zes", "bar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "PIAT", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die \u00f6de Erd', auf der ich atm' und bin!", "tokens": ["Die", "\u00f6\u00b7de", "Erd'", ",", "auf", "der", "ich", "atm'", "und", "bin", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "KON", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Warum durft' ich nicht leben, als sein Hauch", "tokens": ["Wa\u00b7rum", "durft'", "ich", "nicht", "le\u00b7ben", ",", "als", "sein", "Hauch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "KOUS", "PPOSAT", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Die Luft vers\u00fc\u00dfte, als sein reines Aug'", "tokens": ["Die", "Luft", "ver\u00b7s\u00fc\u00df\u00b7te", ",", "als", "sein", "rei\u00b7nes", "Aug'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Gesegnet jedes Kraut und jeden Stein?", "tokens": ["Ge\u00b7seg\u00b7net", "je\u00b7des", "Kraut", "und", "je\u00b7den", "Stein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Warum nicht mich? warum nicht mich allein?", "tokens": ["Wa\u00b7rum", "nicht", "mich", "?", "wa\u00b7rum", "nicht", "mich", "al\u00b7lein", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "PPER", "$.", "PWAV", "PTKNEG", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "O Herr! du h\u00e4ttest mich gesegnet auch!", "tokens": ["O", "Herr", "!", "du", "h\u00e4t\u00b7test", "mich", "ge\u00b7seg\u00b7net", "auch", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "VAFIN", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dir nachgeschlichen w\u00e4r' ich \u00fcberall,", "tokens": ["Dir", "nach\u00b7ge\u00b7schli\u00b7chen", "w\u00e4r'", "ich", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und h\u00e4tte ganz von fern,", "tokens": ["Und", "h\u00e4t\u00b7te", "ganz", "von", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verborgen von Geb\u00fcsches gr\u00fcnem Wall,", "tokens": ["Ver\u00b7bor\u00b7gen", "von", "Ge\u00b7b\u00fc\u00b7sches", "gr\u00fc\u00b7nem", "Wall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Geheim betrachtet meinen liebsten Herrn.", "tokens": ["Ge\u00b7heim", "be\u00b7trach\u00b7tet", "mei\u00b7nen", "liebs\u00b7ten", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zu Martha h\u00e4tt' ich bittend mich gewandt", "tokens": ["Zu", "Mar\u00b7tha", "h\u00e4tt'", "ich", "bit\u00b7tend", "mich", "ge\u00b7wandt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VAFIN", "PPER", "VVPP", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Um einen kleinen Dienst f\u00fcr meine Hand:", "tokens": ["Um", "ei\u00b7nen", "klei\u00b7nen", "Dienst", "f\u00fcr", "mei\u00b7ne", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Vielleicht den Herd zu sch\u00fcren dir zum Mahl,", "tokens": ["Viel\u00b7leicht", "den", "Herd", "zu", "sch\u00fc\u00b7ren", "dir", "zum", "Mahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKZU", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Zum Quell zu gehn, zu l\u00fcften dir den Saal \u2013", "tokens": ["Zum", "Quell", "zu", "gehn", ",", "zu", "l\u00fcf\u00b7ten", "dir", "den", "Saal", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$,", "PTKZU", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Du h\u00e4ttest meine Liebe wohl erkannt.", "tokens": ["Du", "h\u00e4t\u00b7test", "mei\u00b7ne", "Lie\u00b7be", "wohl", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und drau\u00dfen in des Volkes dichtem Schwarm", "tokens": ["Und", "drau\u00b7\u00dfen", "in", "des", "Vol\u00b7kes", "dich\u00b7tem", "Schwarm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "H\u00e4tt' ich versteckt gelauscht,", "tokens": ["H\u00e4tt'", "ich", "ver\u00b7steckt", "ge\u00b7lauscht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und deine Worte, lebensreich und warm,", "tokens": ["Und", "dei\u00b7ne", "Wor\u00b7te", ",", "le\u00b7bens\u00b7reich", "und", "warm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So gern um jede andre Lust getauscht;", "tokens": ["So", "gern", "um", "je\u00b7de", "and\u00b7re", "Lust", "ge\u00b7tauscht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit Magdalena h\u00e4tt' ich wollen knien,", "tokens": ["Mit", "Mag\u00b7da\u00b7le\u00b7na", "h\u00e4tt'", "ich", "wol\u00b7len", "kni\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "PPER", "VMFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Auch meine Tr\u00e4ne h\u00e4tte sollen gl\u00fchn", "tokens": ["Auch", "mei\u00b7ne", "Tr\u00e4\u00b7ne", "h\u00e4t\u00b7te", "sol\u00b7len", "gl\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Auf deinem Fu\u00df, vielleicht dann, ach vielleicht", "tokens": ["Auf", "dei\u00b7nem", "Fu\u00df", ",", "viel\u00b7leicht", "dann", ",", "ach", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADV", "ADV", "$,", "ITJ", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wohl h\u00e4tte mich dein selig Wort erreicht:", "tokens": ["Wohl", "h\u00e4t\u00b7te", "mich", "dein", "se\u00b7lig", "Wort", "er\u00b7reicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "\u00bbgeh hin, auch deine S\u00fcnden sind verziehn!\u00ab", "tokens": ["\u00bb", "geh", "hin", ",", "auch", "dei\u00b7ne", "S\u00fcn\u00b7den", "sind", "ver\u00b7ziehn", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "ADV", "PPOSAT", "NN", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Umsonst! und zwei Jahrtausende nun fast", "tokens": ["Um\u00b7sonst", "!", "und", "zwei", "Jahr\u00b7tau\u00b7sen\u00b7de", "nun", "fast"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "KON", "CARD", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sind ihrem Schlusse' nah,", "tokens": ["Sind", "ih\u00b7rem", "Schlus\u00b7se'", "nah", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Seitdem die Erde ihren s\u00fc\u00dfen Gast", "tokens": ["Seit\u00b7dem", "die", "Er\u00b7de", "ih\u00b7ren", "s\u00fc\u00b7\u00dfen", "Gast"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zuletzt getragen in Bethania.", "tokens": ["Zu\u00b7letzt", "ge\u00b7tra\u00b7gen", "in", "Be\u00b7tha\u00b7nia", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Schon l\u00e4ngst sind deine Martyrer erh\u00f6ht,", "tokens": ["Schon", "l\u00e4ngst", "sind", "dei\u00b7ne", "Mar\u00b7ty\u00b7rer", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und lange Unkraut hat der Feind ges\u00e4t,", "tokens": ["Und", "lan\u00b7ge", "Un\u00b7kraut", "hat", "der", "Feind", "ge\u00b7s\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Gespalten l\u00e4ngst ist deiner Kirche Reich", "tokens": ["Ge\u00b7spal\u00b7ten", "l\u00e4ngst", "ist", "dei\u00b7ner", "Kir\u00b7che", "Reich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VAFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und trauernd h\u00e4ngt der m\u00fchbeladne Zweig", "tokens": ["Und", "trau\u00b7ernd", "h\u00e4ngt", "der", "m\u00fch\u00b7be\u00b7lad\u00b7ne", "Zweig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "An deinem Baume, doch die Wurzel steht.", "tokens": ["An", "dei\u00b7nem", "Bau\u00b7me", ",", "doch", "die", "Wur\u00b7zel", "steht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Geboren bin ich in bedr\u00e4ngter Zeit;", "tokens": ["Ge\u00b7bo\u00b7ren", "bin", "ich", "in", "be\u00b7dr\u00e4ng\u00b7ter", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nach langer Glaubensrast", "tokens": ["Nach", "lan\u00b7ger", "Glau\u00b7bens\u00b7rast"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hat nun verschollner Frevel sich erneut;", "tokens": ["Hat", "nun", "ver\u00b7scholl\u00b7ner", "Fre\u00b7vel", "sich", "er\u00b7neut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJA", "NN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wir tragen eine fast verge\u00dfne Last,", "tokens": ["Wir", "tra\u00b7gen", "ei\u00b7ne", "fast", "ver\u00b7ge\u00df\u00b7ne", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und wieder deine Opfer stehn geweiht.", "tokens": ["Und", "wie\u00b7der", "dei\u00b7ne", "Op\u00b7fer", "stehn", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ach, ist nicht Lieben seliger im Leid?", "tokens": ["Ach", ",", "ist", "nicht", "Lie\u00b7ben", "se\u00b7li\u00b7ger", "im", "Leid", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PTKNEG", "ADJA", "ADJA", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Bist du nicht n\u00e4her, wenn die Trauer weint,", "tokens": ["Bist", "du", "nicht", "n\u00e4\u00b7her", ",", "wenn", "die", "Trau\u00b7er", "weint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wo drei in deinem Namen sind vereint,", "tokens": ["Wo", "drei", "in", "dei\u00b7nem", "Na\u00b7men", "sind", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "APPR", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Als Tausenden im Schmuck und Feierkleid?", "tokens": ["Als", "Tau\u00b7sen\u00b7den", "im", "Schmuck", "und", "Fei\u00b7er\u00b7kleid", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "'s ist sichtbar, wie die Glaubcnsflamme reich", "tokens": ["'s", "ist", "sicht\u00b7bar", ",", "wie", "die", "Glaubcns\u00b7flam\u00b7me", "reich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PWAV", "ART", "NN", "ADJD"], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Empor im Sturme schl\u00e4gt,", "tokens": ["Em\u00b7por", "im", "Stur\u00b7me", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie mancher, der zuvor Nachtwandlern gleich,", "tokens": ["Wie", "man\u00b7cher", ",", "der", "zu\u00b7vor", "Nacht\u00b7wand\u00b7lern", "gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "PRELS", "ADV", "NN", "ADV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Jetzt frisch und kr\u00e4ftig seine Glieder regt.", "tokens": ["Jetzt", "frisch", "und", "kr\u00e4f\u00b7tig", "sei\u00b7ne", "Glie\u00b7der", "reg\u00b7t."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gesundet sind die Kranken, wer da lag", "tokens": ["Ge\u00b7sun\u00b7det", "sind", "die", "Kran\u00b7ken", ",", "wer", "da", "lag"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,", "PW