"trochaic.tetra"}, "line.3": {"text": "Da\u00df die Blume dieser Flur", "tokens": ["Da\u00df", "die", "Blu\u00b7me", "die\u00b7ser", "Flur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weggegangen ohne Spur.", "tokens": ["Weg\u00b7ge\u00b7gan\u00b7gen", "oh\u00b7ne", "Spur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Weggegangen ohne Spur!", "tokens": ["Weg\u00b7ge\u00b7gan\u00b7gen", "oh\u00b7ne", "Spur", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach dir bleibt mein Seufzer nur,", "tokens": ["Nach", "dir", "bleibt", "mein", "Seuf\u00b7zer", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein Schauer der Natur,", "tokens": ["Und", "ein", "Schau\u00b7er", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Machend Herbst auf Sommerflur.", "tokens": ["Ma\u00b7chend", "Herbst", "auf", "Som\u00b7mer\u00b7flur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mach, o Herbst, auf Sommerflur,", "tokens": ["Mach", ",", "o", "Herbst", ",", "auf", "Som\u00b7mer\u00b7flur", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sichtbar jede Todesspur!", "tokens": ["Sicht\u00b7bar", "je\u00b7de", "To\u00b7des\u00b7spur", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ein Schmuck des Todes nur", "tokens": ["Denn", "ein", "Schmuck", "des", "To\u00b7des", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist die Bl\u00fcte der Natur.", "tokens": ["Ist", "die", "Bl\u00fc\u00b7te", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Liebesbl\u00fcte der Natur!", "tokens": ["Lie\u00b7bes\u00b7bl\u00fc\u00b7te", "der", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der Flur", "tokens": ["Auf", "der", "Flur"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Deine Spur sind Thr\u00e4nen nur.", "tokens": ["Dei\u00b7ne", "Spur", "sind", "Thr\u00e4\u00b7nen", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}