{"textgrid.poem.40635": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer dieses Eilands Herr? Kein Mal gibt Kunde,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer dieses Eilands Herr? Kein Mal gibt Kunde,", "tokens": ["Wer", "die\u00b7ses", "Ei\u00b7lands", "Herr", "?", "Kein", "Mal", "gibt", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "NN", "$.", "PIAT", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kein Pfahl in Landesfarben ist zu schauen,", "tokens": ["Kein", "Pfahl", "in", "Lan\u00b7des\u00b7far\u00b7ben", "ist", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kein Schilderhaus, kein Wappen steingehauen,", "tokens": ["Kein", "Schil\u00b7der\u00b7haus", ",", "kein", "Wap\u00b7pen", "stein\u00b7ge\u00b7hau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kein M\u00f6rser, der es spr\u00e4ch' aus eh'rnem Schlunde.", "tokens": ["Kein", "M\u00f6r\u00b7ser", ",", "der", "es", "spr\u00e4ch'", "aus", "eh'r\u00b7nem", "Schlun\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nur Sonntags, mit dem Glockenklang im Bunde,", "tokens": ["Nur", "Sonn\u00b7tags", ",", "mit", "dem", "Glo\u00b7cken\u00b7klang", "im", "Bun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aufsteigt die stolze Brittenflagg' im Blauen;", "tokens": ["Auf\u00b7steigt", "die", "stol\u00b7ze", "Brit\u00b7ten\u00b7flag\u00b7g'", "im", "Blau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Hier bin ich! mahnt sie landw\u00e4rts deutsche Gauen,", "tokens": ["Hier", "bin", "ich", "!", "mahnt", "sie", "land\u00b7w\u00e4rts", "deut\u00b7sche", "Gau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$.", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch Schmerz und Scham nur gr\u00fc\u00dft aus deutschem Munde.", "tokens": ["Doch", "Schmerz", "und", "Scham", "nur", "gr\u00fc\u00dft", "aus", "deut\u00b7schem", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mir soll's die kurze Sonntagslust nicht kr\u00e4nken,", "tokens": ["Mir", "soll's", "die", "kur\u00b7ze", "Sonn\u00b7tags\u00b7lust", "nicht", "kr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu freu'n mich solcher Macht und Kraft und Ehre", "tokens": ["Zu", "freu'n", "mich", "sol\u00b7cher", "Macht", "und", "Kraft", "und", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "PPER", "PIAT", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auch fremden Volks, als ob's das eigne w\u00e4re!", "tokens": ["Auch", "frem\u00b7den", "Volks", ",", "als", "ob's", "das", "eig\u00b7ne", "w\u00e4\u00b7re", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "KOUS", "NE", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Abend wird die Flagge wieder senken;", "tokens": ["Der", "A\u00b7bend", "wird", "die", "Flag\u00b7ge", "wie\u00b7der", "sen\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann gibt's sechs Tage, schmerzlich zu bedenken:", "tokens": ["Dann", "gibt's", "sechs", "Ta\u00b7ge", ",", "schmerz\u00b7lich", "zu", "be\u00b7den\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "$,", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Warum's so kam? und wie's zum Bessern kehre?", "tokens": ["War\u00b7um's", "so", "kam", "?", "und", "wie's", "zum", "Bes\u00b7sern", "keh\u00b7re", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "$.", "KON", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}