{"textgrid.poem.39798": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Zum 18. October 1814", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sollen wir ihn recht begehen", "tokens": ["Wie", "sol\u00b7len", "wir", "ihn", "recht", "be\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Tag der Freiheit? fragt nicht lang!", "tokens": ["Den", "Tag", "der", "Frei\u00b7heit", "?", "fragt", "nicht", "lang", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$.", "VVFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seht ihr die Tempel Gottes stehen,", "tokens": ["Seht", "ihr", "die", "Tem\u00b7pel", "Got\u00b7tes", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6rt ihr der Morgenglocke Klang?", "tokens": ["H\u00f6rt", "ihr", "der", "Mor\u00b7gen\u00b7glo\u00b7cke", "Klang", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Erst tretet in der Kirche Mitten,", "tokens": ["Erst", "tre\u00b7tet", "in", "der", "Kir\u00b7che", "Mit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und weiht ihn mit Gebeten ein;", "tokens": ["Und", "weiht", "ihn", "mit", "Ge\u00b7be\u00b7ten", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie haben ihn mit Gott erstritten,", "tokens": ["Sie", "ha\u00b7ben", "ihn", "mit", "Gott", "er\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Gott soll er gefeiert sein.", "tokens": ["Mit", "Gott", "soll", "er", "ge\u00b7fei\u00b7ert", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "F\u00e4ngt es dann lichter an zu tagen,", "tokens": ["F\u00e4ngt", "es", "dann", "lich\u00b7ter", "an", "zu", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So geht an deutschem Feld vorbei,", "tokens": ["So", "geht", "an", "deut\u00b7schem", "Feld", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und la\u00dft von jedem Thal euch sagen,", "tokens": ["Und", "la\u00dft", "von", "je\u00b7dem", "Thal", "euch", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von jedem H\u00fcgel: wir sind frei!", "tokens": ["Von", "je\u00b7dem", "H\u00fc\u00b7gel", ":", "wir", "sind", "frei", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$.", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Winkt gleich kein Segen von den Reben,", "tokens": ["Winkt", "gleich", "kein", "Se\u00b7gen", "von", "den", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Scheint gleich die Sonne nicht mehr warm:", "tokens": ["Scheint", "gleich", "die", "Son\u00b7ne", "nicht", "mehr", "warm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sieg, Freiheit, Friede, Lust und Leben,", "tokens": ["Sieg", ",", "Frei\u00b7heit", ",", "Frie\u00b7de", ",", "Lust", "und", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wem solches ward, der ist nicht arm.", "tokens": ["Wem", "sol\u00b7ches", "ward", ",", "der", "ist", "nicht", "arm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VAFIN", "$,", "PRELS", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und Manchem liegt vom edlen Weine", "tokens": ["Und", "Man\u00b7chem", "liegt", "vom", "ed\u00b7len", "Wei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch viel in tiefer Keller Schacht;", "tokens": ["Noch", "viel", "in", "tie\u00b7fer", "Kel\u00b7ler", "Schacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bei Sonnen- und Kometen-Scheine", "tokens": ["Bei", "Son\u00b7nen", "und", "Ko\u00b7me\u00b7ten\u00b7Schei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wuchs er gedeihlich Tag und Nacht.", "tokens": ["Wuchs", "er", "ge\u00b7deih\u00b7lich", "Tag", "und", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Noch in der Knechtschaft uns geboren", "tokens": ["Noch", "in", "der", "Knecht\u00b7schaft", "uns", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Lag tr\u00fcb im Fa\u00df der beste Wein,", "tokens": ["Lag", "tr\u00fcb", "im", "Fa\u00df", "der", "bes\u00b7te", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch hatt' er kaum erst ausgegoren,", "tokens": ["Doch", "hatt'", "er", "kaum", "erst", "aus\u00b7ge\u00b7go\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da gl\u00e4nzt' uns Moskau's Flammenschein.", "tokens": ["Da", "gl\u00e4nzt'", "uns", "Mos\u00b7kau's", "Flam\u00b7men\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Drauf zugenommen von der Stunde", "tokens": ["Drauf", "zu\u00b7ge\u00b7nom\u00b7men", "von", "der", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hat er an s\u00fc\u00dfer, deutscher Glut,", "tokens": ["Hat", "er", "an", "s\u00fc\u00b7\u00dfer", ",", "deut\u00b7scher", "Glut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und als von Leipzig kam die Kunde,", "tokens": ["Und", "als", "von", "Leip\u00b7zig", "kam", "die", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bot er begeisternd seine Flut.", "tokens": ["Bot", "er", "be\u00b7geis\u00b7ternd", "sei\u00b7ne", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.8": {"line.1": {"text": "Und nun, da ringsum die Gefilde", "tokens": ["Und", "nun", ",", "da", "ring\u00b7sum", "die", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Ruhe feiern, frei und frank,", "tokens": ["In", "Ru\u00b7he", "fei\u00b7ern", ",", "frei", "und", "frank", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat er zur Kraft gesellt die Milde,", "tokens": ["Hat", "er", "zur", "Kraft", "ge\u00b7sellt", "die", "Mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein rechter deutscher Friedenstrank!", "tokens": ["Ein", "rech\u00b7ter", "deut\u00b7scher", "Frie\u00b7dens\u00b7trank", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Heut' spendet eure reiche Habe,", "tokens": ["Heut'", "spen\u00b7det", "eu\u00b7re", "rei\u00b7che", "Ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vertheilet ihn mit rechtem Maas,", "tokens": ["Ver\u00b7thei\u00b7let", "ihn", "mit", "rech\u00b7tem", "Maas", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schenket von der Gottesgabe", "tokens": ["Und", "schen\u00b7ket", "von", "der", "Got\u00b7tes\u00b7ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem \u00e4rmsten Bruder auch ein Glas.", "tokens": ["Dem", "\u00e4rms\u00b7ten", "Bru\u00b7der", "auch", "ein", "Glas", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Verge\u00dft am heil'gen Tage Keinen!", "tokens": ["Ver\u00b7ge\u00dft", "am", "heil'\u00b7gen", "Ta\u00b7ge", "Kei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "PIAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Alle lie\u00df der Herr der Welt", "tokens": ["Auf", "Al\u00b7le", "lie\u00df", "der", "Herr", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Freiheit Sonne niederscheinen,", "tokens": ["Der", "Frei\u00b7heit", "Son\u00b7ne", "nie\u00b7der\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Fest sei Allen heut bestellt.", "tokens": ["Ein", "Fest", "sei", "Al\u00b7len", "heut", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NE", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Von Hohen t\u00f6ne dann und Niedern", "tokens": ["Von", "Ho\u00b7hen", "t\u00f6\u00b7ne", "dann", "und", "Nie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "ADV", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein fr\u00f6hlicher Zusammenklang,", "tokens": ["Ein", "fr\u00f6h\u00b7li\u00b7cher", "Zu\u00b7sam\u00b7men\u00b7klang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Lebewohl gefallnen Br\u00fcdern,", "tokens": ["Ein", "Le\u00b7be\u00b7wohl", "ge\u00b7fall\u00b7nen", "Br\u00fc\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem freien Volk ein Lebelang!", "tokens": ["Dem", "frei\u00b7en", "Volk", "ein", "Le\u00b7be\u00b7lang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}