{"textgrid.poem.46557": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[meine Gute kam zu mir im Traume]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Gute kam zu mir im Traume,", "tokens": ["Mei\u00b7ne", "Gu\u00b7te", "kam", "zu", "mir", "im", "Trau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aber gleichsam wie in fremdem Raume,", "tokens": ["A\u00b7ber", "gleich\u00b7sam", "wie", "in", "frem\u00b7dem", "Rau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "That sie bl\u00f6de, scheu und unbekannt,", "tokens": ["That", "sie", "bl\u00f6\u00b7de", ",", "scheu", "und", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Als ob hier mit ihr nichts sei verwandt.", "tokens": ["Als", "ob", "hier", "mit", "ihr", "nichts", "sei", "ver\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ADV", "APPR", "PPER", "PIS", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Auf das Tischchen stellt' ich Spielger\u00e4the,", "tokens": ["Auf", "das", "Tischchen", "stellt'", "ich", "Spiel\u00b7ge\u00b7r\u00e4\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Da\u00df ich eine Lieb' und Ehr' ihr th\u00e4te;", "tokens": ["Da\u00df", "ich", "ei\u00b7ne", "Lieb'", "und", "Ehr'", "ihr", "th\u00e4\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Doch sie r\u00fchrte nichts an und blieb still,", "tokens": ["Doch", "sie", "r\u00fchr\u00b7te", "nichts", "an", "und", "blieb", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIS", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Wie ein Kind, das heim zur Mutter will.", "tokens": ["Wie", "ein", "Kind", ",", "das", "heim", "zur", "Mut\u00b7ter", "will", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Und es dr\u00e4ngten sich zu ihr die Knaben,", "tokens": ["Und", "es", "dr\u00e4ng\u00b7ten", "sich", "zu", "ihr", "die", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die am Spielwerk wollten Antheil haben;", "tokens": ["Die", "am", "Spiel\u00b7werk", "woll\u00b7ten", "An\u00b7theil", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VMFIN", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Da verwies ich ihnen ihre Hast:", "tokens": ["Da", "ver\u00b7wies", "ich", "ih\u00b7nen", "ih\u00b7re", "Hast", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Thut ihr nichts! sie ist ja hier zu Gast.", "tokens": ["Thut", "ihr", "nichts", "!", "sie", "ist", "ja", "hier", "zu", "Gast", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "$.", "PPER", "VAFIN", "ADV", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Ach, nur Gast bist du in diesen R\u00e4umen,", "tokens": ["Ach", ",", "nur", "Gast", "bist", "du", "in", "die\u00b7sen", "R\u00e4u\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "NN", "VAFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Selbst ein Gast in deiner Mutter Tr\u00e4umen,", "tokens": ["Selbst", "ein", "Gast", "in", "dei\u00b7ner", "Mut\u00b7ter", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Fremd geworden in dem Mutterhaus,", "tokens": ["Fremd", "ge\u00b7wor\u00b7den", "in", "dem", "Mut\u00b7ter\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Seit der Tod dich hat entf\u00fchrt daraus.", "tokens": ["Seit", "der", "Tod", "dich", "hat", "ent\u00b7f\u00fchrt", "da\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VAFIN", "VVPP", "PAV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}