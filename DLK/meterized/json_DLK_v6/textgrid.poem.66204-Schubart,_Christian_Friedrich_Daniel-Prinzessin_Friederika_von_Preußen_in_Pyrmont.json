{"textgrid.poem.66204": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Prinzessin Friederika von Preu\u00dfen in Pyrmont", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geister, die ihr um die Quelle schwebet,", "tokens": ["Geis\u00b7ter", ",", "die", "ihr", "um", "die", "Quel\u00b7le", "schwe\u00b7bet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die des Menschen Kr\u00e4fte neu belebet,", "tokens": ["Die", "des", "Men\u00b7schen", "Kr\u00e4f\u00b7te", "neu", "be\u00b7le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Gie\u00dft in Friederikas Blut", "tokens": ["Gie\u00dft", "in", "Frie\u00b7de\u00b7ri\u00b7kas", "Blut"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NE", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Gesundheit neue Lebensfluth.", "tokens": ["Der", "Ge\u00b7sund\u00b7heit", "neu\u00b7e", "Le\u00b7bens\u00b7fluth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "F\u00e4chelt unsichtbar dem Engelbilde", "tokens": ["F\u00e4\u00b7chelt", "un\u00b7sicht\u00b7bar", "dem", "En\u00b7gel\u00b7bil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Paradiesesl\u00fcfte zu.", "tokens": ["Pa\u00b7ra\u00b7die\u00b7ses\u00b7l\u00fcf\u00b7te", "zu", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Himmlisch ist der K\u00f6nigstochter Milde!", "tokens": ["Himm\u00b7lisch", "ist", "der", "K\u00f6\u00b7nig\u00b7stoch\u00b7ter", "Mil\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Friederika, Gottes Strahl bist du.", "tokens": ["Frie\u00b7de\u00b7ri\u00b7ka", ",", "Got\u00b7tes", "Strahl", "bist", "du", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "NN", "VAFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Wandelst hier schon wie verkl\u00e4rt,", "tokens": ["Wan\u00b7delst", "hier", "schon", "wie", "ver\u00b7kl\u00e4rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "KOKOM", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Jedes gro\u00dfen Segens werth.", "tokens": ["Je\u00b7des", "gro\u00b7\u00dfen", "Se\u00b7gens", "werth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}