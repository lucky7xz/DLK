{"dta.poem.19883": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "An die  \n  Nymfe des Negenborns ,  \n einer Felsenquelle ohnweit dem Wohnort  \n des Dichters.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Neig aus deines Vaters Halle,               ", "tokens": ["Neig", "aus", "dei\u00b7nes", "Va\u00b7ters", "Hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Felsentochter, mir dein Ohr!", "tokens": ["Fel\u00b7sen\u00b7toch\u00b7ter", ",", "mir", "dein", "Ohr", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hell im Schimmer der Krystalle,", "tokens": ["Hell", "im", "Schim\u00b7mer", "der", "Krys\u00b7tal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hell im Silberschleier, walle,", "tokens": ["Hell", "im", "Sil\u00b7ber\u00b7schlei\u00b7er", ",", "wal\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Reine Nymfe, wall\u2019 hervor!", "tokens": ["Rei\u00b7ne", "Nym\u00b7fe", ",", "wall'", "her\u00b7vor", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Libern jauchzet die M\u00e4nade", "tokens": ["Li\u00b7bern", "jauch\u00b7zet", "die", "M\u00e4\u00b7na\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Huldigung bei Zymbelklang.", "tokens": ["Hul\u00b7di\u00b7gung", "bei", "Zym\u00b7bel\u00b7klang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dir nur, gl\u00e4nzende Najade,", "tokens": ["Dir", "nur", ",", "gl\u00e4n\u00b7zen\u00b7de", "Na\u00b7ja\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Deiner Urne, deinem Bade", "tokens": ["Dei\u00b7ner", "Ur\u00b7ne", ",", "dei\u00b7nem", "Ba\u00b7de"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weihte keiner Hochgesang? \u2014", "tokens": ["Weih\u00b7te", "kei\u00b7ner", "Hoch\u00b7ge\u00b7sang", "?"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wohl, ich weih\u2019 ihn! Wo der Zecher,", "tokens": ["Wohl", ",", "ich", "weih'", "ihn", "!", "Wo", "der", "Ze\u00b7cher", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "PPER", "$.", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der des Preises spotten sol?", "tokens": ["Der", "des", "Prei\u00b7ses", "spot\u00b7ten", "sol", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ha! Wo ist er? Ich bin R\u00e4cher!", "tokens": ["Ha", "!", "Wo", "ist", "er", "?", "Ich", "bin", "R\u00e4\u00b7cher", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VAFIN", "PPER", "$.", "PPER", "VAFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fleuch! Mein Bogen t\u00f6nt! Mein K\u00f6cher", "tokens": ["Fleuch", "!", "Mein", "Bo\u00b7gen", "t\u00f6nt", "!", "Mein", "K\u00f6\u00b7cher"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "PPOSAT", "NN", "VVFIN", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Rasselt goldner Pfeile vol!", "tokens": ["Ras\u00b7selt", "gold\u00b7ner", "Pfei\u00b7le", "vol", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Hier, wie aus der Traube, quillet", "tokens": ["Hier", ",", "wie", "aus", "der", "Trau\u00b7be", ",", "quil\u00b7let"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "$,", "PWAV", "APPR", "ART", "NN", "$,", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Geist und Leben, frisch und rein,", "tokens": ["Geist", "und", "Le\u00b7ben", ",", "frisch", "und", "rein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Leben, das den Hirten f\u00fcllet,", "tokens": ["Le\u00b7ben", ",", "das", "den", "Hir\u00b7ten", "f\u00fcl\u00b7let", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das den Durst der Heerde stillet,", "tokens": ["Das", "den", "Durst", "der", "Heer\u00b7de", "stil\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Welches Wiese tr\u00e4nkt und Hain.", "tokens": ["Wel\u00b7ches", "Wie\u00b7se", "tr\u00e4nkt", "und", "Hain", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VVFIN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Horch! Es ranscht im Felsenhaine,", "tokens": ["Horch", "!", "Es", "ranscht", "im", "Fel\u00b7sen\u00b7hai\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Woget auf der Wies\u2019 entlang,", "tokens": ["Wo\u00b7get", "auf", "der", "Wies'", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lekt im Widder auf dem Reine,", "tokens": ["Lekt", "im", "Wid\u00b7der", "auf", "dem", "Rei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schauert durch das Mark der Beine,", "tokens": ["Schau\u00b7ert", "durch", "das", "Mark", "der", "Bei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00fchlt des Wandrers heissen Gang.", "tokens": ["K\u00fchlt", "des", "Wand\u00b7rers", "heis\u00b7sen", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Saugt aus Wein der Klee sein Leben,", "tokens": ["Saugt", "aus", "Wein", "der", "Klee", "sein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wolgeruch und Honigsaft? \u2014", "tokens": ["Wol\u00b7ge\u00b7ruch", "und", "Ho\u00b7nigs\u00b7aft", "?"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kraut und Blumen, selbst die Reben", "tokens": ["Kraut", "und", "Blu\u00b7men", ",", "selbst", "die", "Re\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Danken dir, o Nais, Leben,", "tokens": ["Dan\u00b7ken", "dir", ",", "o", "Nais", ",", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$,", "FM", "NE", "$,", "NN", "$,"], "meter": "+-+-++-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "W\u00fcrze, S\u00fcssigkeit und Kraft.", "tokens": ["W\u00fcr\u00b7ze", ",", "S\u00fcs\u00b7sig\u00b7keit", "und", "Kraft", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Lebensf\u00fclle, Kraft und Streben", "tokens": ["Le\u00b7bens\u00b7f\u00fcl\u00b7le", ",", "Kraft", "und", "Stre\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trank auch ich schon oft bei dir.", "tokens": ["Trank", "auch", "ich", "schon", "oft", "bei", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drob sey auch von nun an Leben", "tokens": ["Drob", "sey", "auch", "von", "nun", "an", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und Unsterblichkeit gegeben", "tokens": ["Und", "U\u00b7nsterb\u00b7lich\u00b7keit", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVPP"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Deinem Namen f\u00fcr und f\u00fcr!", "tokens": ["Dei\u00b7nem", "Na\u00b7men", "f\u00fcr", "und", "f\u00fcr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "KON", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}