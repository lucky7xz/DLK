{"textgrid.poem.55847": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Der Nachbar", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fremde Geige, gehst du mir nach?", "tokens": ["Frem\u00b7de", "Gei\u00b7ge", ",", "gehst", "du", "mir", "nach", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "In wieviel fernen St\u00e4dten schon sprach", "tokens": ["In", "wie\u00b7viel", "fer\u00b7nen", "St\u00e4d\u00b7ten", "schon", "sprach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN", "ADV", "VVFIN"], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "deine einsame Nacht zu meiner?", "tokens": ["dei\u00b7ne", "ein\u00b7sa\u00b7me", "Nacht", "zu", "mei\u00b7ner", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Spielen dich hunderte? Spielt dich einer?", "tokens": ["Spie\u00b7len", "dich", "hun\u00b7der\u00b7te", "?", "Spielt", "dich", "ei\u00b7ner", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$.", "VVFIN", "PRF", "ART", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Giebt es in allen gro\u00dfen St\u00e4dten", "tokens": ["Giebt", "es", "in", "al\u00b7len", "gro\u00b7\u00dfen", "St\u00e4d\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "solche, die sich ohne dich", "tokens": ["sol\u00b7che", ",", "die", "sich", "oh\u00b7ne", "dich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "$,", "PRELS", "PRF", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "schon in den Fl\u00fcssen verloren h\u00e4tten?", "tokens": ["schon", "in", "den", "Fl\u00fcs\u00b7sen", "ver\u00b7lo\u00b7ren", "h\u00e4t\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und warum trifft es immer mich?", "tokens": ["Und", "wa\u00b7rum", "trifft", "es", "im\u00b7mer", "mich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Warum bin ich immer der Nachbar derer,", "tokens": ["Wa\u00b7rum", "bin", "ich", "im\u00b7mer", "der", "Nach\u00b7bar", "de\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ART", "NN", "PDS", "$,"], "meter": "+-+-+++-+--", "measure": "unknown.measure.hexa"}, "line.2": {"text": "die dich bange zwingen zu singen", "tokens": ["die", "dich", "ban\u00b7ge", "zwin\u00b7gen", "zu", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "VVINF", "PTKZU", "VVINF"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "und zu sagen: Das Leben ist schwerer", "tokens": ["und", "zu", "sa\u00b7gen", ":", "Das", "Le\u00b7ben", "ist", "schwe\u00b7rer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PTKZU", "VVINF", "$.", "ART", "NN", "VAFIN", "ADJD"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "als die Schwere von allen Dingen.", "tokens": ["als", "die", "Schwe\u00b7re", "von", "al\u00b7len", "Din\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}