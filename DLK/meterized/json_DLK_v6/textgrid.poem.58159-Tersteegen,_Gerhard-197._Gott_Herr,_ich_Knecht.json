{"textgrid.poem.58159": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "197. Gott Herr, ich Knecht", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott will als K\u00f6nig mich regieren", "tokens": ["Gott", "will", "als", "K\u00f6\u00b7nig", "mich", "re\u00b7gie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "KOUS", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und als ein Hirt mich immer f\u00fchren,", "tokens": ["Und", "als", "ein", "Hirt", "mich", "im\u00b7mer", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Will' und Wink mu\u00df gelten nur;", "tokens": ["Sein", "Will'", "und", "Wink", "mu\u00df", "gel\u00b7ten", "nur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VMFIN", "VVINF", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Herr, befiehl, ich will mich beugen,", "tokens": ["Mein", "Herr", ",", "be\u00b7fiehl", ",", "ich", "will", "mich", "beu\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "$,", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fchr, wie du willst, ich werde schweigen,", "tokens": ["F\u00fchr", ",", "wie", "du", "willst", ",", "ich", "wer\u00b7de", "schwei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VMFIN", "$,", "PPER", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du Sch\u00f6pfer bist, ich Kreatur!", "tokens": ["Du", "Sch\u00f6p\u00b7fer", "bist", ",", "ich", "Kre\u00b7a\u00b7tur", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}