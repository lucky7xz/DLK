{"textgrid.poem.26030": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Die Nacht weicht vor dir zur\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sommernacht ist eng voll Holunder und wildem Rosenholz;", "tokens": ["Die", "Som\u00b7mer\u00b7nacht", "ist", "eng", "voll", "Ho\u00b7lun\u00b7der", "und", "wil\u00b7dem", "Ro\u00b7sen\u00b7holz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADJD", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Sie dr\u00e4ngt unsere Lippen zusammen", "tokens": ["Sie", "dr\u00e4ngt", "un\u00b7se\u00b7re", "Lip\u00b7pen", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie die Gl\u00fchw\u00fcrmer, die sich im Dunkel paaren.", "tokens": ["Wie", "die", "Gl\u00fch\u00b7w\u00fcr\u00b7mer", ",", "die", "sich", "im", "Dun\u00b7kel", "paa\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Dein Herz macht deine Hand blendend,", "tokens": ["Dein", "Herz", "macht", "dei\u00b7ne", "Hand", "blen\u00b7dend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die Nacht weicht vor dir zur\u00fcck,", "tokens": ["Die", "Nacht", "weicht", "vor", "dir", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Du entschleierst meinen geheimen Wunsch.", "tokens": ["Du", "ent\u00b7schlei\u00b7erst", "mei\u00b7nen", "ge\u00b7hei\u00b7men", "Wunsch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}}}}