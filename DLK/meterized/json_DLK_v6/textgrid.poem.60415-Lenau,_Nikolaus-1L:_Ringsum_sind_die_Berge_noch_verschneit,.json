{"textgrid.poem.60415": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ringsum sind die Berge noch verschneit,", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ringsum sind die Berge noch verschneit,", "tokens": ["Ring\u00b7sum", "sind", "die", "Ber\u00b7ge", "noch", "ver\u00b7schneit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Aber Blumen seh ich hier, die fr\u00fchen!", "tokens": ["A\u00b7ber", "Blu\u00b7men", "seh", "ich", "hier", ",", "die", "fr\u00fc\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADV", "$,", "ART", "ADJA", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Blumen, sch\u00f6n, da\u00df ihr gekommen seid,", "tokens": ["Blu\u00b7men", ",", "sch\u00f6n", ",", "da\u00df", "ihr", "ge\u00b7kom\u00b7men", "seid", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Hier auf seinem fr\u00fchen Grab zu bl\u00fchen.", "tokens": ["Hier", "auf", "sei\u00b7nem", "fr\u00fc\u00b7hen", "Grab", "zu", "bl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Freudig stieg er manchen Berg hinan,", "tokens": ["Freu\u00b7dig", "stieg", "er", "man\u00b7chen", "Berg", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Um des Fr\u00fchlings Gr\u00fc\u00dfe zu empfangen;", "tokens": ["Um", "des", "Fr\u00fch\u00b7lings", "Gr\u00fc\u00b7\u00dfe", "zu", "emp\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Weil der Tote nicht mehr kommen kann,", "tokens": ["Weil", "der", "To\u00b7te", "nicht", "mehr", "kom\u00b7men", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ist nun ihm der Fr\u00fchling nachgegangen. \u2013", "tokens": ["Ist", "nun", "ihm", "der", "Fr\u00fch\u00b7ling", "nach\u00b7ge\u00b7gan\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ART", "NN", "VVIZU", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Blumen! ob ihr nicht die Freuden seid,", "tokens": ["Blu\u00b7men", "!", "ob", "ihr", "nicht", "die", "Freu\u00b7den", "seid", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KOUS", "PPER", "PTKNEG", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Die dem Toten h\u00e4tten kommen sollen?", "tokens": ["Die", "dem", "To\u00b7ten", "h\u00e4t\u00b7ten", "kom\u00b7men", "sol\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VAFIN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die, geh\u00fcllt in euer lichtes Kleid,", "tokens": ["Die", ",", "ge\u00b7h\u00fcllt", "in", "eu\u00b7er", "lich\u00b7tes", "Kleid", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Doch auf seinem Grabe bl\u00fchen wollen?", "tokens": ["Doch", "auf", "sei\u00b7nem", "Gra\u00b7be", "bl\u00fc\u00b7hen", "wol\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}