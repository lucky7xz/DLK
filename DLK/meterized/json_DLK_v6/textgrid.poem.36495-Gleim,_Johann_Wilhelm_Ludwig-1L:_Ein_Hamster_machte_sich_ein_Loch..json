{"textgrid.poem.36495": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Hamster machte sich ein Loch.", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Hamster machte sich ein Loch.", "tokens": ["Ein", "Hams\u00b7ter", "mach\u00b7te", "sich", "ein", "Loch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ei, sprach ein Dachs: was machst du doch?", "tokens": ["Ei", ",", "sprach", "ein", "Dachs", ":", "was", "machst", "du", "doch", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$.", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist ja viel zu klein!", "tokens": ["Es", "ist", "ja", "viel", "zu", "klein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "F\u00fcr dich, das k\u00f6nnte sein,", "tokens": ["F\u00fcr", "dich", ",", "das", "k\u00f6nn\u00b7te", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PDS", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Antwortete der Hamster \u2013 Gr\u00f6\u00dfer machen", "tokens": ["Ant\u00b7wor\u00b7te\u00b7te", "der", "Hams\u00b7ter", "\u2013", "Gr\u00f6\u00b7\u00dfer", "ma\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$(", "NN", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "K\u00f6nnt' ich's ja leicht, allein", "tokens": ["K\u00f6nnt'", "ich's", "ja", "leicht", ",", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VMFIN", "PIS", "ADV", "ADJD", "$,", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ihr G\u00e4ste w\u00fcrdet meiner lachen,", "tokens": ["Ihr", "G\u00e4s\u00b7te", "w\u00fcr\u00b7det", "mei\u00b7ner", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Fuchs, und du, ihr k\u00e4mt mir dann hinein.", "tokens": ["Der", "Fuchs", ",", "und", "du", ",", "ihr", "k\u00e4mt", "mir", "dann", "hin\u00b7ein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "KON", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}