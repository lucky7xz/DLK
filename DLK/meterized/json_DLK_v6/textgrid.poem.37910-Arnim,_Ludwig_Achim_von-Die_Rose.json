{"textgrid.poem.37910": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Die Rose", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Rose bl\u00fcht, ich bin die fromme Biene,", "tokens": ["Die", "Ro\u00b7se", "bl\u00fcht", ",", "ich", "bin", "die", "from\u00b7me", "Bie\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und r\u00fchre zwar die keuschen Bl\u00e4tter an,", "tokens": ["Und", "r\u00fch\u00b7re", "zwar", "die", "keu\u00b7schen", "Bl\u00e4t\u00b7ter", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Daher ich Thau und Honig sch\u00f6pfen kann,", "tokens": ["Da\u00b7her", "ich", "Thau", "und", "Ho\u00b7nig", "sch\u00f6p\u00b7fen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "NN", "KON", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch lebt ihr Glanz und bleibet immer gr\u00fcne,", "tokens": ["Doch", "lebt", "ihr", "Glanz", "und", "blei\u00b7bet", "im\u00b7mer", "gr\u00fc\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und also bin ich wohlgem\u00fcth,", "tokens": ["Und", "al\u00b7so", "bin", "ich", "wohl\u00b7ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil meine Rose bl\u00fcht.", "tokens": ["Weil", "mei\u00b7ne", "Ro\u00b7se", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Rose bl\u00fcht, Gott la\u00df den Schein verziehen,", "tokens": ["Die", "Ro\u00b7se", "bl\u00fcht", ",", "Gott", "la\u00df", "den", "Schein", "ver\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "NN", "VVIMP", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Damit die Zeit des Sommers langsam geht,", "tokens": ["Da\u00b7mit", "die", "Zeit", "des", "Som\u00b7mers", "lang\u00b7sam", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und weder Frost noch andere Noth entsteht,", "tokens": ["Und", "we\u00b7der", "Frost", "noch", "an\u00b7de\u00b7re", "Noth", "ent\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KON", "NN", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "So wird mein Gl\u00fcck in dieser Rose bl\u00fchen,", "tokens": ["So", "wird", "mein", "Gl\u00fcck", "in", "die\u00b7ser", "Ro\u00b7se", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So klingt mein s\u00fc\u00dfes Freuden-Lied:", "tokens": ["So", "klingt", "mein", "s\u00fc\u00b7\u00dfes", "Freu\u00b7den\u00b7Lied", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach, meine Rose bl\u00fcht!", "tokens": ["Ach", ",", "mei\u00b7ne", "Ro\u00b7se", "bl\u00fcht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Rose bl\u00fcht, und lacht vor andern Rosen", "tokens": ["Die", "Ro\u00b7se", "bl\u00fcht", ",", "und", "lacht", "vor", "an\u00b7dern", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit solcher Zier und Herzempfindlichkeit,", "tokens": ["Mit", "sol\u00b7cher", "Zier", "und", "Her\u00b7zemp\u00b7find\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df auch mein Sinn sich zu der Pflicht erbeut,", "tokens": ["Da\u00df", "auch", "mein", "Sinn", "sich", "zu", "der", "Pflicht", "er\u00b7beut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit keiner Blum im Garten liebzukosen,", "tokens": ["Mit", "kei\u00b7ner", "Blum", "im", "Gar\u00b7ten", "lieb\u00b7zu\u00b7ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Weil Alles, was man sonsten sieht,", "tokens": ["Weil", "Al\u00b7les", ",", "was", "man", "sons\u00b7ten", "sieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In dieser Rose bl\u00fcht.", "tokens": ["In", "die\u00b7ser", "Ro\u00b7se", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}