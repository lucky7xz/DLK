{"dta.poem.5470": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ueberzeugliche Vermahnung zur  \n N\u00e4chsten-Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein N\u00e4chster ist, so wol als du, vom Sch\u00f6pfer eine", "tokens": ["Dein", "N\u00e4chs\u00b7ter", "ist", ",", "so", "wol", "als", "du", ",", "vom", "Sch\u00f6p\u00b7fer", "ei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "ADV", "ADV", "KOUS", "PPER", "$,", "APPRART", "NN", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Creatur:", "tokens": ["Crea\u00b7tur", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Wie wir denn nun in allen Dingen, die GOtt gemacht,", "tokens": ["Wie", "wir", "denn", "nun", "in", "al\u00b7len", "Din\u00b7gen", ",", "die", "Gott", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "APPR", "PIAT", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "ihn selbst versp\u00fchren;", "tokens": ["ihn", "selbst", "ver\u00b7sp\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "So mu\u00df man, zu des N\u00e4chsten Besten, und unserm Nu-", "tokens": ["So", "mu\u00df", "man", ",", "zu", "des", "N\u00e4chs\u00b7ten", "Bes\u00b7ten", ",", "und", "un\u00b7serm", "Nu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "$,", "APPR", "ART", "ADJA", "NN", "$,", "KON", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "tzen, ihm nicht nur", "tokens": ["tzen", ",", "ihm", "nicht", "nur"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVINF", "$,", "PPER", "PTKNEG", "ADV"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Nicht schaden, sondern in ihm gleichsam den Sch\u00f6pfer", "tokens": ["Nicht", "scha\u00b7den", ",", "son\u00b7dern", "in", "ihm", "gleich\u00b7sam", "den", "Sch\u00f6p\u00b7fer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVINF", "$,", "KON", "APPR", "PPER", "ADJD", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "selber respectiren.", "tokens": ["sel\u00b7ber", "res\u00b7pec\u00b7ti\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}}}}}