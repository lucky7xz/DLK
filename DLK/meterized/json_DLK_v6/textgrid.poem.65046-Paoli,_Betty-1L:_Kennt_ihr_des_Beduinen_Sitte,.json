{"textgrid.poem.65046": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kennt ihr des Beduinen Sitte,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kennt ihr des Beduinen Sitte,", "tokens": ["Kennt", "ihr", "des", "Be\u00b7du\u00b7i\u00b7nen", "Sit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn er, bereit zu neuem Ritte,", "tokens": ["Wenn", "er", ",", "be\u00b7reit", "zu", "neu\u00b7em", "Rit\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Brust von frischem Drang geschwellt,", "tokens": ["Die", "Brust", "von", "fri\u00b7schem", "Drang", "ge\u00b7schwellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Blick l\u00e4\u00dft in die Ferne schweifen", "tokens": ["Den", "Blick", "l\u00e4\u00dft", "in", "die", "Fer\u00b7ne", "schwei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und, d\u00e4mmert kaum ein lichter Streifen", "tokens": ["Und", ",", "d\u00e4m\u00b7mert", "kaum", "ein", "lich\u00b7ter", "Strei\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im Osten, abbricht sein Gezelt?", "tokens": ["Im", "Os\u00b7ten", ",", "ab\u00b7bricht", "sein", "Ge\u00b7zelt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er ruhte s\u00fc\u00df; doch jetzt ist's Morgen!", "tokens": ["Er", "ruh\u00b7te", "s\u00fc\u00df", ";", "doch", "jetzt", "ist's", "Mor\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur Ein's bleibt ihm noch zu besorgen,", "tokens": ["Nur", "Ein's", "bleibt", "ihm", "noch", "zu", "be\u00b7sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Bevor er rasch von hinnen eilt:", "tokens": ["Be\u00b7vor", "er", "rasch", "von", "hin\u00b7nen", "eilt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Steinen f\u00fcgt er myst'sche Kreise", "tokens": ["Aus", "Stei\u00b7nen", "f\u00fcgt", "er", "myst'\u00b7sche", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zum Zeichen, da\u00df auf seiner Reise", "tokens": ["Zum", "Zei\u00b7chen", ",", "da\u00df", "auf", "sei\u00b7ner", "Rei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Wandrer ruhend hier geweilt. \u2013 \u2013", "tokens": ["Ein", "Wand\u00b7rer", "ru\u00b7hend", "hier", "ge\u00b7weilt", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVPP", "ADV", "VVPP", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O Menschengeist voll Sehnsuchtbangen!", "tokens": ["O", "Men\u00b7schen\u00b7geist", "voll", "Sehn\u00b7sucht\u00b7ban\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie gleichst im rastlosen Verlangen", "tokens": ["Wie", "gleichst", "im", "rast\u00b7lo\u00b7sen", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Dem fl\u00fccht'gen Sohn der W\u00fcste du!", "tokens": ["Dem", "fl\u00fccht'\u00b7gen", "Sohn", "der", "W\u00fcs\u00b7te", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach m\u00fchevollen Wanderfahrten", "tokens": ["Nach", "m\u00fc\u00b7he\u00b7vol\u00b7len", "Wan\u00b7der\u00b7fahr\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Winkt der Oasis gr\u00fcner Garten", "tokens": ["Winkt", "der", "O\u00b7a\u00b7sis", "gr\u00fc\u00b7ner", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "Dir stille Edenstr\u00e4ume zu.", "tokens": ["Dir", "stil\u00b7le", "E\u00b7dens\u00b7tr\u00e4u\u00b7me", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Als heilig gr\u00fc\u00dfest du die Stelle,", "tokens": ["Als", "hei\u00b7lig", "gr\u00fc\u00b7\u00dfest", "du", "die", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du labst dich an dem frischen Quelle,", "tokens": ["Du", "labst", "dich", "an", "dem", "fri\u00b7schen", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der aus dem Felsger\u00f6lle bricht;", "tokens": ["Der", "aus", "dem", "Fels\u00b7ge\u00b7r\u00f6l\u00b7le", "bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du f\u00fchlest mild'rer L\u00fcfte Wehen,", "tokens": ["Du", "f\u00fch\u00b7lest", "mild'\u00b7rer", "L\u00fcf\u00b7te", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du glaubst die Sprache zu verstehen", "tokens": ["Du", "glaubst", "die", "Spra\u00b7che", "zu", "ver\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Womit der Stern zur Blume spricht.", "tokens": ["Wo\u00b7mit", "der", "Stern", "zur", "Blu\u00b7me", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Erstarken f\u00fchlst du dich gesunden,", "tokens": ["Er\u00b7star\u00b7ken", "f\u00fchlst", "du", "dich", "ge\u00b7sun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Heimath scheint dir aufgefunden,", "tokens": ["Die", "Hei\u00b7math", "scheint", "dir", "auf\u00b7ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das letzte, h\u00f6chste Ziel erstrebt.", "tokens": ["Das", "letz\u00b7te", ",", "h\u00f6chs\u00b7te", "Ziel", "er\u00b7strebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du jubelst: Fort mit Schmerz und Grauen!", "tokens": ["Du", "ju\u00b7belst", ":", "Fort", "mit", "Schmerz", "und", "Grau\u00b7en", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hier will ich meine H\u00fctte bauen,", "tokens": ["Hier", "will", "ich", "mei\u00b7ne", "H\u00fct\u00b7te", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von Friedenshauchen sanft umschwebt.", "tokens": ["Von", "Frie\u00b7dens\u00b7hau\u00b7chen", "sanft", "um\u00b7schwebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dein innerst Glauben, Hoffen, Denken", "tokens": ["Dein", "in\u00b7nerst", "Glau\u00b7ben", ",", "Hof\u00b7fen", ",", "Den\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Willst du auf diesen Kreis beschr\u00e4nken,", "tokens": ["Willst", "du", "auf", "die\u00b7sen", "Kreis", "be\u00b7schr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In den du liebend dich gebannt.", "tokens": ["In", "den", "du", "lie\u00b7bend", "dich", "ge\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du meinst, es sei f\u00fcr alle Zeiten", "tokens": ["Du", "meinst", ",", "es", "sei", "f\u00fcr", "al\u00b7le", "Zei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "PPER", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vers\u00f6hnt der Kr\u00e4fte feindlich Streiten,", "tokens": ["Ver\u00b7s\u00f6hnt", "der", "Kr\u00e4f\u00b7te", "feind\u00b7lich", "Strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gel\u00f6scht des Herzens tiefer Brand. \u2013", "tokens": ["Ge\u00b7l\u00f6scht", "des", "Her\u00b7zens", "tie\u00b7fer", "Brand", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Umsonst! Bald wird sich dir's entfalten,", "tokens": ["Um\u00b7sonst", "!", "Bald", "wird", "sich", "dir's", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "VAFIN", "PRF", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df, was du f\u00fcr das Ziel gehalten,", "tokens": ["Da\u00df", ",", "was", "du", "f\u00fcr", "das", "Ziel", "ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWS", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nichts als ein Meilenzeiger nur.", "tokens": ["Nichts", "als", "ein", "Mei\u00b7len\u00b7zei\u00b7ger", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wieder treibt dich's in die Weite,", "tokens": ["Und", "wie\u00b7der", "treibt", "dich's", "in", "die", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Stern ist Gott und dein Geleite", "tokens": ["Dein", "Stern", "ist", "Gott", "und", "dein", "Ge\u00b7lei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die stille Ahnung seiner Spur!", "tokens": ["Die", "stil\u00b7le", "Ah\u00b7nung", "sei\u00b7ner", "Spur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und diese Tempel, diese Hallen,", "tokens": ["Und", "die\u00b7se", "Tem\u00b7pel", ",", "die\u00b7se", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Schutt und Tr\u00fcmmer l\u00e4ngst zerfallen", "tokens": ["In", "Schutt", "und", "Tr\u00fcm\u00b7mer", "l\u00e4ngst", "zer\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Umrauscht von ew'ger Meeresfluth,", "tokens": ["Um\u00b7rauscht", "von", "ew'\u00b7ger", "Mee\u00b7res\u00b7fluth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie mahnen feierlich und leise,", "tokens": ["Sie", "mah\u00b7nen", "fei\u00b7er\u00b7lich", "und", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df einst auf seiner Erdenreise", "tokens": ["Da\u00df", "einst", "auf", "sei\u00b7ner", "Er\u00b7den\u00b7rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Geist der Menschheit hier geruht.", "tokens": ["Der", "Geist", "der", "Menschheit", "hier", "ge\u00b7ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}