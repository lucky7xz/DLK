{"textgrid.poem.36559": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Schlachtgesang vor der Schlacht bei Prag", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was kannst du? Tolpatsch und Pandur,", "tokens": ["Was", "kannst", "du", "?", "Tol\u00b7patsch", "und", "Pan\u00b7dur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$.", "NN", "KON", "NN", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Soldat und Offizier!", "tokens": ["Sol\u00b7dat", "und", "Of\u00b7fi\u00b7zier", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was kannst du? fliehen kannst du nur;", "tokens": ["Was", "kannst", "du", "?", "flie\u00b7hen", "kannst", "du", "nur", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$.", "VVINF", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und siegen k\u00f6nnen wir.", "tokens": ["Und", "sie\u00b7gen", "k\u00f6n\u00b7nen", "wir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir kommen; zittre! Deinen Tod", "tokens": ["Wir", "kom\u00b7men", ";", "zitt\u00b7re", "!", "Dei\u00b7nen", "Tod"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verk\u00fcndigt Ro\u00df und Mann!", "tokens": ["Ver\u00b7k\u00fcn\u00b7digt", "Ro\u00df", "und", "Mann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir kommen, unser Kriegesgott,", "tokens": ["Wir", "kom\u00b7men", ",", "un\u00b7ser", "Krie\u00b7ges\u00b7gott", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Held Friedrich, ist voran!", "tokens": ["Held", "Fried\u00b7rich", ",", "ist", "vo\u00b7ran", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auch ist, mit seiner Heldenschar,", "tokens": ["Auch", "ist", ",", "mit", "sei\u00b7ner", "Hel\u00b7den\u00b7schar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Held ", "tokens": ["Der", "Held"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Wir sehen ihn; fein graues Haar", "tokens": ["Wir", "se\u00b7hen", "ihn", ";", "fein", "grau\u00b7es", "Haar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gl\u00e4nzt uns, als wie ein Stern!", "tokens": ["Gl\u00e4nzt", "uns", ",", "als", "wie", "ein", "Stern", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Was hilft es, Feind, da\u00df gro\u00df Gesch\u00fctz", "tokens": ["Was", "hilft", "es", ",", "Feind", ",", "da\u00df", "gro\u00df", "Ge\u00b7sch\u00fctz"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$,", "KOUS", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Steht um dich her gepflanzt?", "tokens": ["Steht", "um", "dich", "her", "ge\u00b7pflanzt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "APZR", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was hilft es, da\u00df mit Kunst und Witz", "tokens": ["Was", "hilft", "es", ",", "da\u00df", "mit", "Kunst", "und", "Witz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Lager steht umschanzt?", "tokens": ["Dein", "La\u00b7ger", "steht", "um\u00b7schanzt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Gehorsam feurigem Verstand", "tokens": ["Ge\u00b7hor\u00b7sam", "feu\u00b7ri\u00b7gem", "Ver\u00b7stand"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und alter Weisheit nun,", "tokens": ["Und", "al\u00b7ter", "Weis\u00b7heit", "nun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Stehn wir, die Waffen in der Hand,", "tokens": ["Stehn", "wir", ",", "die", "Waf\u00b7fen", "in", "der", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wollen Thaten thun.", "tokens": ["Und", "wol\u00b7len", "Tha\u00b7ten", "thun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und wollen trotzen deiner Macht,", "tokens": ["Und", "wol\u00b7len", "trot\u00b7zen", "dei\u00b7ner", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf hohem Felsensitz,", "tokens": ["Auf", "ho\u00b7hem", "Fel\u00b7sen\u00b7sitz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und deinem Streich, uns zugedacht,", "tokens": ["Und", "dei\u00b7nem", "Streich", ",", "uns", "zu\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und deinem Kriegeswitz.", "tokens": ["Und", "dei\u00b7nem", "Krie\u00b7ges\u00b7witz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und deinem Stolz und deinem Spott;", "tokens": ["Und", "dei\u00b7nem", "Stolz", "und", "dei\u00b7nem", "Spott", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Denn diesen b\u00f6sen Krieg", "tokens": ["Denn", "die\u00b7sen", "b\u00f6\u00b7sen", "Krieg"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hast du geboren: drum ist Gott", "tokens": ["Hast", "du", "ge\u00b7bo\u00b7ren", ":", "drum", "ist", "Gott"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "$.", "PAV", "VAFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit uns, und giebt uns Sieg!", "tokens": ["Mit", "uns", ",", "und", "giebt", "uns", "Sieg", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KON", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und l\u00e4\u00dft uns herrlichen Gesang", "tokens": ["Und", "l\u00e4\u00dft", "uns", "herr\u00b7li\u00b7chen", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Anstimmen nach der Schlacht.", "tokens": ["An\u00b7stim\u00b7men", "nach", "der", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schweig' Leier! H\u00f6rt Trompetenklang!", "tokens": ["Schweig'", "Lei\u00b7er", "!", "H\u00f6rt", "Trom\u00b7pe\u00b7ten\u00b7klang", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "VVIMP", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Still, Br\u00fcder! gebet Acht!", "tokens": ["Still", ",", "Br\u00fc\u00b7der", "!", "ge\u00b7bet", "Acht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "$.", "VVFIN", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}