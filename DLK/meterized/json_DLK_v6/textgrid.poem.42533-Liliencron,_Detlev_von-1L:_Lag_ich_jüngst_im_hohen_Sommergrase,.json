{"textgrid.poem.42533": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lag ich j\u00fcngst im hohen Sommergrase,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lag ich j\u00fcngst im hohen Sommergrase,", "tokens": ["Lag", "ich", "j\u00fcngst", "im", "ho\u00b7hen", "Som\u00b7mer\u00b7gra\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hatte gern das Menschenvolk gemieden.", "tokens": ["Hat\u00b7te", "gern", "das", "Men\u00b7schen\u00b7volk", "ge\u00b7mie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Grade, grade \u00fcber meiner Nase", "tokens": ["Gra\u00b7de", ",", "gra\u00b7de", "\u00fc\u00b7ber", "mei\u00b7ner", "Na\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Zog ein Sch\u00e4ferw\u00f6lkchen hin in Frieden;", "tokens": ["Zog", "ein", "Sch\u00e4\u00b7fer\u00b7w\u00f6lk\u00b7chen", "hin", "in", "Frie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Zog im Blauen seine stille Stra\u00dfe,", "tokens": ["Zog", "im", "Blau\u00b7en", "sei\u00b7ne", "stil\u00b7le", "Stra\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Zog den Weg ins Land der Pyramiden.", "tokens": ["Zog", "den", "Weg", "ins", "Land", "der", "Py\u00b7ra\u00b7mi\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Nickten Blumen, summten Himmelbrummer,", "tokens": ["Nick\u00b7ten", "Blu\u00b7men", ",", "summ\u00b7ten", "Him\u00b7mel\u00b7brum\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Summten langsam, langsam mich in Schlummer.", "tokens": ["Summ\u00b7ten", "lang\u00b7sam", ",", "lang\u00b7sam", "mich", "in", "Schlum\u00b7mer", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADJD", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}