{"dta.poem.15994": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unk\u00f6niglicher doch ist keine Eigenschaft", "tokens": ["Un\u00b7k\u00f6\u00b7nig\u00b7li\u00b7cher", "doch", "ist", "kei\u00b7ne", "Ei\u00b7gen\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VAFIN", "PIAT", "NN"], "meter": "+----+-+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Als Misgunst, durch sie wird ein K\u00f6nig bettelhaft.", "tokens": ["Als", "Mis\u00b7gunst", ",", "durch", "sie", "wird", "ein", "K\u00f6\u00b7nig", "bet\u00b7tel\u00b7haft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "APPR", "PPER", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein Bettler nur misg\u00f6nnt dem andern ein St\u00fcck Brot,", "tokens": ["Ein", "Bett\u00b7ler", "nur", "mis\u00b7g\u00f6nnt", "dem", "an\u00b7dern", "ein", "St\u00fcck", "Brot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "ADJA", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+++", "measure": "unknown.measure.septa"}, "line.2": {"text": "Weil seinem Sack entgeht, was jenem dar sich bot.", "tokens": ["Weil", "sei\u00b7nem", "Sack", "ent\u00b7geht", ",", "was", "je\u00b7nem", "dar", "sich", "bot", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$,", "PWS", "PDAT", "PTKVZ", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein K\u00f6nig aber braucht nichts einem zu misg\u00f6nnen,", "tokens": ["Ein", "K\u00f6\u00b7nig", "a\u00b7ber", "braucht", "nichts", "ei\u00b7nem", "zu", "mis\u00b7g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PIS", "ART", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil er nicht selber braucht, was andre brauchen k\u00f6nnen.", "tokens": ["Weil", "er", "nicht", "sel\u00b7ber", "braucht", ",", "was", "and\u00b7re", "brau\u00b7chen", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "$,", "PRELS", "PIS", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dem K\u00f6nig stehet an und ziemet Gunst vor allen,", "tokens": ["Dem", "K\u00f6\u00b7nig", "ste\u00b7het", "an", "und", "zie\u00b7met", "Gunst", "vor", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "VVFIN", "NN", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und seine Ungunst mu\u00dft du lassen dir gefallen.", "tokens": ["Und", "sei\u00b7ne", "Un\u00b7gunst", "mu\u00dft", "du", "las\u00b7sen", "dir", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PPER", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch seine Misgunst ist ein D\u00e4mon schadenfroh,", "tokens": ["Doch", "sei\u00b7ne", "Mis\u00b7gunst", "ist", "ein", "D\u00e4\u00b7mon", "scha\u00b7den\u00b7froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der selber ihm misg\u00f6nnt, zu werden gnadenfroh.", "tokens": ["Der", "sel\u00b7ber", "ihm", "mis\u00b7g\u00f6nnt", ",", "zu", "wer\u00b7den", "gna\u00b7den\u00b7froh", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "$,", "PTKZU", "VAINF", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}