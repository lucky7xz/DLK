{"dta.poem.158": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die erz\u00fcrnte   Victore.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Welch Laster ist von mir Victore denn begangen?", "tokens": ["Welch", "Las\u00b7ter", "ist", "von", "mir", "Vic\u00b7to\u00b7re", "denn", "be\u00b7gan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "PPER", "NE", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df kein beliebter Strahl aus deinen Augen zieht/", "tokens": ["Da\u00df", "kein", "be\u00b7lieb\u00b7ter", "Strahl", "aus", "dei\u00b7nen", "Au\u00b7gen", "zieht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und da\u00df nur Flamm und Blitz auf meine Scheitel gl\u00fcht/", "tokens": ["Und", "da\u00df", "nur", "Flamm", "und", "Blitz", "auf", "mei\u00b7ne", "Schei\u00b7tel", "gl\u00fcht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "NE", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Warum verstellet sich die Sch\u00f6nheit deiner Wangen?", "tokens": ["Wa\u00b7rum", "ver\u00b7stel\u00b7let", "sich", "die", "Sch\u00f6n\u00b7heit", "dei\u00b7ner", "Wan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Soll sie nicht wie zuvor mit holder Anmuth prangen?", "tokens": ["Soll", "sie", "nicht", "wie", "zu\u00b7vor", "mit", "hol\u00b7der", "An\u00b7muth", "pran\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "KOKOM", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und warum ist dein Grimm auf meinen Fall bem\u00fcht?", "tokens": ["Und", "wa\u00b7rum", "ist", "dein", "Grimm", "auf", "mei\u00b7nen", "Fall", "be\u00b7m\u00fcht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "PPOSAT", "NE", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da mir vordem bey dir das sch\u00f6nste Gl\u00fcck gebl\u00fcht/", "tokens": ["Da", "mir", "vor\u00b7dem", "bey", "dir", "das", "sch\u00f6ns\u00b7te", "Gl\u00fcck", "ge\u00b7bl\u00fcht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Und ich auf Erden schon mit ", "tokens": ["Und", "ich", "auf", "Er\u00b7den", "schon", "mit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "NN", "ADV", "APPR"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Erz\u00fcrnet dich der Ku\u00df/ den ich dir weggenommen/", "tokens": ["Er\u00b7z\u00fcr\u00b7net", "dich", "der", "Ku\u00df", "/", "den", "ich", "dir", "weg\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$(", "PRELS", "PPER", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ist dieses meine Schuld/ darauf du b\u00f6se bist?", "tokens": ["Ist", "die\u00b7ses", "mei\u00b7ne", "Schuld", "/", "da\u00b7rauf", "du", "b\u00f6\u00b7se", "bist", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "PPOSAT", "NN", "$(", "PAV", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So soll mein Mund davor satsahm in Straffe kommen/", "tokens": ["So", "soll", "mein", "Mund", "da\u00b7vor", "sat\u00b7sahm", "in", "Straf\u00b7fe", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "PAV", "VVFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und lieffern dir den Ku\u00df/ der dir geraubet ist.", "tokens": ["Und", "lief\u00b7fern", "dir", "den", "Ku\u00df", "/", "der", "dir", "ge\u00b7rau\u00b7bet", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$(", "PRELS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ist einer nicht genug/ und wilt du Zinse heben?", "tokens": ["Ist", "ei\u00b7ner", "nicht", "ge\u00b7nug", "/", "und", "wilt", "du", "Zin\u00b7se", "he\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PTKNEG", "ADV", "$(", "KON", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So soll er hundert dir/ vor einen/ wiedergeben.", "tokens": ["So", "soll", "er", "hun\u00b7dert", "dir", "/", "vor", "ei\u00b7nen", "/", "wie\u00b7der\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "PPER", "$(", "APPR", "ART", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}