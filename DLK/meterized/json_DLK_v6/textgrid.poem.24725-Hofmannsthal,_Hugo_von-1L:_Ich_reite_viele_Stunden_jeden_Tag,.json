{"textgrid.poem.24725": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich reite viele Stunden jeden Tag,", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich reite viele Stunden jeden Tag,", "tokens": ["Ich", "rei\u00b7te", "vie\u00b7le", "Stun\u00b7den", "je\u00b7den", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch tiefen toten Sand, durch hohes Gras,", "tokens": ["Durch", "tie\u00b7fen", "to\u00b7ten", "Sand", ",", "durch", "ho\u00b7hes", "Gras", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durch gutes helles Wasser und durch schwarzes", "tokens": ["Durch", "gu\u00b7tes", "hel\u00b7les", "Was\u00b7ser", "und", "durch", "schwar\u00b7zes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN", "KON", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Wald, das quillt und gurgelt unterm Huf.", "tokens": ["Im", "Wald", ",", "das", "quillt", "und", "gur\u00b7gelt", "un\u00b7term", "Huf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PDS", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zuweilen reit ich auf die Sonne zu,", "tokens": ["Zu\u00b7wei\u00b7len", "reit", "ich", "auf", "die", "Son\u00b7ne", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Kupferscheibe in den schwarzen B\u00fcschen,", "tokens": ["Die", "Kup\u00b7fer\u00b7schei\u00b7be", "in", "den", "schwar\u00b7zen", "B\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Zuweilen gegen feuchten Wind, manchmal", "tokens": ["Zu\u00b7wei\u00b7len", "ge\u00b7gen", "feuch\u00b7ten", "Wind", ",", "manch\u00b7mal"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Auf einem hei\u00dfen steilen Weg, manchmal", "tokens": ["Auf", "ei\u00b7nem", "hei\u00b7\u00dfen", "stei\u00b7len", "Weg", ",", "manch\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Auf einem Damm in heller stiller Luft,", "tokens": ["Auf", "ei\u00b7nem", "Damm", "in", "hel\u00b7ler", "stil\u00b7ler", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Da\u00df ich die krummen \u00c4ste z\u00e4hlen kann", "tokens": ["Da\u00df", "ich", "die", "krum\u00b7men", "\u00c4s\u00b7te", "z\u00e4h\u00b7len", "kann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Der Apfelb\u00e4ume auf der fernen Stra\u00dfe", "tokens": ["Der", "Ap\u00b7fel\u00b7b\u00e4u\u00b7me", "auf", "der", "fer\u00b7nen", "Stra\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und einen T\u00fcmpel leuchten seh, weit weit!", "tokens": ["Und", "ei\u00b7nen", "T\u00fcm\u00b7pel", "leuch\u00b7ten", "seh", ",", "weit", "weit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "VVFIN", "$,", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und meinen Fuchs und meine rote Kappe", "tokens": ["Und", "mei\u00b7nen", "Fuchs", "und", "mei\u00b7ne", "ro\u00b7te", "Kap\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NE", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und wei\u00dfe Handschuh sieht man auch weit weit", "tokens": ["Und", "wei\u00b7\u00dfe", "Hand\u00b7schuh", "sieht", "man", "auch", "weit", "weit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PIS", "ADV", "ADJD", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und meine dunklen H\u00fcften, Arm' und Schultern", "tokens": ["Und", "mei\u00b7ne", "dunk\u00b7len", "H\u00fcf\u00b7ten", ",", "Arm'", "und", "Schul\u00b7tern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Am gelben Damm bei dieser hellen Luft", "tokens": ["Am", "gel\u00b7ben", "Damm", "bei", "die\u00b7ser", "hel\u00b7len", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Wie fliegend Glas, das \u00fcberm Feuer flirrt.", "tokens": ["Wie", "flie\u00b7gend", "Glas", ",", "das", "\u00fc\u00b7berm", "Feu\u00b7er", "flirrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zuweilen reiten viele neben mir", "tokens": ["Zu\u00b7wei\u00b7len", "rei\u00b7ten", "vie\u00b7le", "ne\u00b7ben", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und viele vor mir, alles ist voll L\u00e4rm,", "tokens": ["Und", "vie\u00b7le", "vor", "mir", ",", "al\u00b7les", "ist", "voll", "L\u00e4rm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "PPER", "$,", "PIS", "VAFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die gr\u00fcnen Mulden dr\u00f6hnen, und die Luft", "tokens": ["Die", "gr\u00fc\u00b7nen", "Mul\u00b7den", "dr\u00f6h\u00b7nen", ",", "und", "die", "Luft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ist voller Klirren, und ich seh vor mir", "tokens": ["Ist", "vol\u00b7ler", "Klir\u00b7ren", ",", "und", "ich", "seh", "vor", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJA", "NN", "$,", "KON", "PPER", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "(mit feuchten Augen von dem starken Wind)", "tokens": ["(", "mit", "feuch\u00b7ten", "Au\u00b7gen", "von", "dem", "star\u00b7ken", "Wind", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die vordersten hinjagen auf dem Hang:", "tokens": ["Die", "vor\u00b7ders\u00b7ten", "hin\u00b7ja\u00b7gen", "auf", "dem", "Hang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.7": {"text": "Ein Kn\u00e4uel Braun' und Rappen, zwei, drei Schimmel,", "tokens": ["Ein", "Kn\u00e4u\u00b7el", "Braun'", "und", "Rap\u00b7pen", ",", "zwei", ",", "drei", "Schim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,", "CARD", "$,", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nur wei\u00dfe Flecken, und in dem Gedr\u00e4nge", "tokens": ["Nur", "wei\u00b7\u00dfe", "Fle\u00b7cken", ",", "und", "in", "dem", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der dunklen Reiter blinken gold die Helme", "tokens": ["Der", "dunk\u00b7len", "Rei\u00b7ter", "blin\u00b7ken", "gold", "die", "Hel\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und so die Klingen, wie ein Netz von Adern", "tokens": ["Und", "so", "die", "Klin\u00b7gen", ",", "wie", "ein", "Netz", "von", "A\u00b7dern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PWAV", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Lebendgen Wassers blinkt im st\u00e4rksten Mond", "tokens": ["Le\u00b7bend\u00b7gen", "Was\u00b7sers", "blinkt", "im", "st\u00e4rks\u00b7ten", "Mond"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.12": {"text": "(dar\u00fcber, wei\u00dft du? schwebt es milchig wei\u00df", "tokens": ["(", "da\u00b7r\u00fc\u00b7ber", ",", "wei\u00dft", "du", "?", "schwebt", "es", "mil\u00b7chig", "wei\u00df"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PAV", "$,", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und viele Unken schreien, wundervoll).", "tokens": ["Und", "vie\u00b7le", "Un\u00b7ken", "schrei\u00b7en", ",", "wun\u00b7der\u00b7voll", ")", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,", "ADJD", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Zuweilen aber reit ich ganz allein,", "tokens": ["Zu\u00b7wei\u00b7len", "a\u00b7ber", "reit", "ich", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "So still! ich h\u00f6re, wie die M\u00fccke schwirrt,", "tokens": ["So", "still", "!", "ich", "h\u00f6\u00b7re", ",", "wie", "die", "M\u00fc\u00b7cke", "schwirrt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Wenn sie dem Fuchs vom Hals zur Schulter fliegt;", "tokens": ["Wenn", "sie", "dem", "Fuchs", "vom", "Hals", "zur", "Schul\u00b7ter", "fliegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NE", "APPRART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Lang schau ich einer Nebelkr\u00e4he nach", "tokens": ["Lang", "schau", "ich", "ei\u00b7ner", "Ne\u00b7bel\u00b7kr\u00e4\u00b7he", "nach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und folg der schwarzen auf dem grauen Weg", "tokens": ["Und", "folg", "der", "schwar\u00b7zen", "auf", "dem", "grau\u00b7en", "Weg"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ART", "ADJA", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Durch d\u00fcrre Wipfel hin und her, und seh", "tokens": ["Durch", "d\u00fcr\u00b7re", "Wip\u00b7fel", "hin", "und", "her", ",", "und", "seh"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "KON", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Fasanenh\u00e4hnchen auf einander losgehn", "tokens": ["Fa\u00b7sa\u00b7nen\u00b7h\u00e4hn\u00b7chen", "auf", "ein\u00b7an\u00b7der", "los\u00b7gehn"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PRF", "VVINF"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.21": {"text": "Im niedern Gras, wo viele Anemonen,", "tokens": ["Im", "nie\u00b7dern", "Gras", ",", "wo", "vie\u00b7le", "A\u00b7ne\u00b7mo\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PWAV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Schneewei\u00dfe, stehn; sitz ab und la\u00df den Fuchs", "tokens": ["Schnee\u00b7wei\u00b7\u00dfe", ",", "stehn", ";", "sitz", "ab", "und", "la\u00df", "den", "Fuchs"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$.", "VVIMP", "PTKVZ", "KON", "VVIMP", "ART", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Mit nachgela\u00dfnen Gurten ruhig grasen", "tokens": ["Mit", "nach\u00b7ge\u00b7la\u00df\u00b7nen", "Gur\u00b7ten", "ru\u00b7hig", "gra\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Und riech dann noch, wenn ich zu Haus den Handschuh", "tokens": ["Und", "riech", "dann", "noch", ",", "wenn", "ich", "zu", "Haus", "den", "Hand\u00b7schuh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$,", "KOUS", "PPER", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Abstreif, gemengt mit dem Geruch vom Pferd", "tokens": ["Ab\u00b7streif", ",", "ge\u00b7mengt", "mit", "dem", "Ge\u00b7ruch", "vom", "Pferd"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVPP", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.26": {"text": "Den Duft von wildem k\u00fchlem Thymian ...", "tokens": ["Den", "Duft", "von", "wil\u00b7dem", "k\u00fch\u00b7lem", "Thy\u00b7mi\u00b7an", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "ADJA", "NE", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Und f\u00fchl in alledem so nichts vom Leben!", "tokens": ["Und", "f\u00fchl", "in", "al\u00b7le\u00b7dem", "so", "nichts", "vom", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIS", "ADV", "PIS", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie kann das nur geschehn, da\u00df man so lebt", "tokens": ["Wie", "kann", "das", "nur", "ge\u00b7schehn", ",", "da\u00df", "man", "so", "lebt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PDS", "ADV", "VVPP", "$,", "KOUS", "PIS", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und alles ist, als obs nicht wirklich w\u00e4re?", "tokens": ["Und", "al\u00b7les", "ist", ",", "als", "obs", "nicht", "wirk\u00b7lich", "w\u00e4\u00b7re", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "$,", "KOKOM", "KOUS", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nichts wirklich als das \u00f6de Zeitverrinnen", "tokens": ["Nichts", "wirk\u00b7lich", "als", "das", "\u00f6\u00b7de", "Zeit\u00b7ver\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "ADJD", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und alles andere wie nichts: das Wasser,", "tokens": ["Und", "al\u00b7les", "an\u00b7de\u00b7re", "wie", "nichts", ":", "das", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "