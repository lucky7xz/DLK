{"dta.poem.11728": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die heil'ge Brahmastadt, gleich einer Lotosbl\u00fcte,", "tokens": ["Die", "heil'\u00b7ge", "Brah\u00b7ma\u00b7stadt", ",", "gleich", "ei\u00b7ner", "Lo\u00b7tos\u00b7bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In welcher Brahma wohnt, o Mensch, ist dein Gem\u00fcte.", "tokens": ["In", "wel\u00b7cher", "Brah\u00b7ma", "wohnt", ",", "o", "Mensch", ",", "ist", "dein", "Ge\u00b7m\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VVFIN", "$,", "FM", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "F\u00fcnf Thore hat die Stadt an ihren Au\u00dfenwerken,", "tokens": ["F\u00fcnf", "Tho\u00b7re", "hat", "die", "Stadt", "an", "ih\u00b7ren", "Au\u00b7\u00dfen\u00b7wer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das sind die Sinne, die die Welt von au\u00dfen merken.", "tokens": ["Das", "sind", "die", "Sin\u00b7ne", ",", "die", "die", "Welt", "von", "au\u00b7\u00dfen", "mer\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die F\u00e4den des Geruchs, die Fasern der Empfindung", "tokens": ["Die", "F\u00e4\u00b7den", "des", "Ge\u00b7ruchs", ",", "die", "Fa\u00b7sern", "der", "Emp\u00b7fin\u00b7dung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erhalten mit der Welt den Lotos in Verbindung.", "tokens": ["Er\u00b7hal\u00b7ten", "mit", "der", "Welt", "den", "Lo\u00b7tos", "in", "Ver\u00b7bin\u00b7dung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Im Richtweg des Geschmacks, im Schneckengang des Ohres,", "tokens": ["Im", "Richt\u00b7weg", "des", "Ge\u00b7schmacks", ",", "im", "Schne\u00b7cken\u00b7gang", "des", "Oh\u00b7res", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Brahmamitte bleibt bewu\u00dft des offnen Thores.", "tokens": ["Die", "Brah\u00b7ma\u00b7mit\u00b7te", "bleibt", "be\u00b7wu\u00dft", "des", "off\u00b7nen", "Tho\u00b7res", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Am liebsten aber steigt auf seinem Lotosglanz", "tokens": ["Am", "liebs\u00b7ten", "a\u00b7ber", "steigt", "auf", "sei\u00b7nem", "Lo\u00b7tos\u00b7glanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Gott ins Aug' empor und schaut die Sch\u00f6pfung ganz.", "tokens": ["Der", "Gott", "ins", "Aug'", "em\u00b7por", "und", "schaut", "die", "Sch\u00f6p\u00b7fung", "ganz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKVZ", "KON", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Da wird die Sch\u00f6pfung hell, vom Lotosglanz bethaut,", "tokens": ["Da", "wird", "die", "Sch\u00f6p\u00b7fung", "hell", ",", "vom", "Lo\u00b7tos\u00b7glanz", "be\u00b7thaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": " und f\u00fchlet freudig, da\u00df ihr Sch\u00f6pfer sie beschaut.", "tokens": ["und", "f\u00fch\u00b7let", "freu\u00b7dig", ",", "da\u00df", "ihr", "Sch\u00f6p\u00b7fer", "sie", "be\u00b7schaut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Solang' er innen wacht, wacht au\u00dfen Welt in Wonne;", "tokens": ["So\u00b7lang'", "er", "in\u00b7nen", "wacht", ",", "wacht", "au\u00b7\u00dfen", "Welt", "in", "Won\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVFIN", "$,", "VVFIN", "ADV", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was hier die Sinnen macht, das machet dort die Sonne.", "tokens": ["Was", "hier", "die", "Sin\u00b7nen", "macht", ",", "das", "ma\u00b7chet", "dort", "die", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "$,", "PDS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Und hat durchs Aug' er sich die Welt beschaut mit Ruh,", "tokens": ["Und", "hat", "durchs", "Aug'", "er", "sich", "die", "Welt", "be\u00b7schaut", "mit", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "PPER", "PRF", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Steigt er ins Herz hinab, und macht die Fenster zu.", "tokens": ["Steigt", "er", "ins", "Herz", "hin\u00b7ab", ",", "und", "macht", "die", "Fens\u00b7ter", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Die Lotosbl\u00fcte schlie\u00dft sich dann als Schlummermohn,", "tokens": ["Die", "Lo\u00b7tos\u00b7bl\u00fc\u00b7te", "schlie\u00dft", "sich", "dann", "als", "Schlum\u00b7mer\u00b7mohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und drau\u00dfen tr\u00e4umt der Mond, und ist benannt davon.", "tokens": ["Und", "drau\u00b7\u00dfen", "tr\u00e4umt", "der", "Mond", ",", "und", "ist", "be\u00b7nannt", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,", "KON", "VAFIN", "ADJD", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Doch tief im Lotoskelch wird nun vom Schlummer frei,", "tokens": ["Doch", "tief", "im", "Lo\u00b7tos\u00b7kelch", "wird", "nun", "vom", "Schlum\u00b7mer", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VAFIN", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die m\u00fcd' am Tage schlief, die Biene Schw\u00e4rmerei.", "tokens": ["Die", "m\u00fcd'", "am", "Ta\u00b7ge", "schlief", ",", "die", "Bie\u00b7ne", "Schw\u00e4r\u00b7me\u00b7rei", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Die schw\u00e4rmt, den Nektarkelch des Lotos auszukosten,", "tokens": ["Die", "schw\u00e4rmt", ",", "den", "Nek\u00b7tar\u00b7kelch", "des", "Lo\u00b7tos", "aus\u00b7zu\u00b7kos\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und tr\u00e4nk' ihn leer, wenn nicht Besinnung tagt' im Osten.", "tokens": ["Und", "tr\u00e4nk'", "ihn", "leer", ",", "wenn", "nicht", "Be\u00b7sin\u00b7nung", "tagt'", "im", "Os\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "PTKNEG", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Und wieder wacht empor der Sinne St\u00e4dterchor,", "tokens": ["Und", "wie\u00b7der", "wacht", "em\u00b7por", "der", "Sin\u00b7ne", "St\u00e4d\u00b7ter\u00b7chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PTKVZ", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Lebensnahrung f\u00fchrt er ein durchs offne Thor.", "tokens": ["Und", "Le\u00b7bens\u00b7nah\u00b7rung", "f\u00fchrt", "er", "ein", "durchs", "off\u00b7ne", "Thor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ART", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Du schaust dem Treiben zu, und f\u00fchlst in stiller Lust", "tokens": ["Du", "schaust", "dem", "Trei\u00b7ben", "zu", ",", "und", "f\u00fchlst", "in", "stil\u00b7ler", "Lust"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den, der dies Alles lenkt, den Gott in deiner Brust.", "tokens": ["Den", ",", "der", "dies", "Al\u00b7les", "lenkt", ",", "den", "Gott", "in", "dei\u00b7ner", "Brust", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PDS", "PIS", "VVFIN", "$,", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Im Bilde zeigt er dir sein ew'ges Wohngefild,", "tokens": ["Im", "Bil\u00b7de", "zeigt", "er", "dir", "sein", "ew'\u00b7ges", "Wohn\u00b7ge\u00b7fild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil du ihn anders nicht kannst fassen als im Bild.", "tokens": ["Weil", "du", "ihn", "an\u00b7ders", "nicht", "kannst", "fas\u00b7sen", "als", "im", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "PTKNEG", "VMFIN", "VVINF", "KOKOM", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}