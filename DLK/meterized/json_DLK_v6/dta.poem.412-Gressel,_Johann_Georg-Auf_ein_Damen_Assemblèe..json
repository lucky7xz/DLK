{"dta.poem.412": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf ein  Damen Assembl\u00e8e.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.85", "af:0.14"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Was seht ihr Augen hier? wer kan hier wol bekennen/", "tokens": ["Was", "seht", "ihr", "Au\u00b7gen", "hier", "?", "wer", "kan", "hier", "wol", "be\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "ADV", "$.", "PWS", "VMFIN", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df es nur Menschen seyn? man mu\u00df sie G\u00f6tter nennen/", "tokens": ["Da\u00df", "es", "nur", "Men\u00b7schen", "seyn", "?", "man", "mu\u00df", "sie", "G\u00f6t\u00b7ter", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VAINF", "$.", "PIS", "VMFIN", "PPER", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der sch\u00f6nen Augen-Glantz", "tokens": ["Der", "sch\u00f6\u00b7nen", "Au\u00b7gen\u00b7Glantz"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Verdienet Kron' und Krantz/", "tokens": ["Ver\u00b7die\u00b7net", "Kron'", "und", "Krantz", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er raubet unsern Augen das Gesicht/", "tokens": ["Er", "rau\u00b7bet", "un\u00b7sern", "Au\u00b7gen", "das", "Ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Jetzt ist es Nacht/ uns scheinet doch ihr Licht/", "tokens": ["Jetzt", "ist", "es", "Nacht", "/", "uns", "schei\u00b7net", "doch", "ihr", "Licht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$(", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und macht/ da\u00df wir bey Nachte sehen/", "tokens": ["Und", "macht", "/", "da\u00df", "wir", "bey", "Nach\u00b7te", "se\u00b7hen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PPER", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hier Sternen/ Mond und Sonn\u2019 auffgehen", "tokens": ["Hier", "Ster\u00b7nen", "/", "Mond", "und", "Sonn'", "auff\u00b7ge\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "$(", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}