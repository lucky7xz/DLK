{"textgrid.poem.37755": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der wandernde Gesell", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie gl\u00e4nzt mir jede Stadt so hell,", "tokens": ["Wie", "gl\u00e4nzt", "mir", "je\u00b7de", "Stadt", "so", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PIAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo mir kein Haus gebauet,", "tokens": ["Wo", "mir", "kein", "Haus", "ge\u00b7bau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo ich als wandernder Gesell,", "tokens": ["Wo", "ich", "als", "wan\u00b7dern\u00b7der", "Ge\u00b7sell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich lustig umgeschauet;", "tokens": ["Mich", "lus\u00b7tig", "um\u00b7ge\u00b7schau\u00b7et", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wenn in der leichten Abendtracht,", "tokens": ["Wenn", "in", "der", "leich\u00b7ten", "A\u00b7bend\u00b7tracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die M\u00e4dchen in den Th\u00fcren,", "tokens": ["Die", "M\u00e4d\u00b7chen", "in", "den", "Th\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Weil sie vom hellen Mond bewacht,", "tokens": ["Weil", "sie", "vom", "hel\u00b7len", "Mond", "be\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So manchen Muthwill sp\u00fcren.", "tokens": ["So", "man\u00b7chen", "Mut\u00b7hwill", "sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbhilf Gott,\u00ab so spricht mich eine an,", "tokens": ["\u00bb", "hilf", "Gott", ",", "\u00ab", "so", "spricht", "mich", "ei\u00b7ne", "an", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "NN", "$,", "$(", "ADV", "VVFIN", "PPER", "ART", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbdas nenne ich noch g\u00e4hnen,", "tokens": ["\u00bb", "das", "nen\u00b7ne", "ich", "noch", "g\u00e4h\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bist du nicht auch ein Leiermann,", "tokens": ["Bist", "du", "nicht", "auch", "ein", "Lei\u00b7er\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sing mir von Lust und Thr\u00e4nen! \u2013", "tokens": ["Sing", "mir", "von", "Lust", "und", "Thr\u00e4\u00b7nen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sing langsam, da\u00df ichs von dir lern,", "tokens": ["Sing", "lang\u00b7sam", ",", "da\u00df", "ichs", "von", "dir", "lern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "KOUS", "PIS", "APPR", "PPER", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ich wills dem Liebsten singen,", "tokens": ["Ich", "wills", "dem", "Liebs\u00b7ten", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das Wetter leuchtet still von fern,", "tokens": ["Das", "Wet\u00b7ter", "leuch\u00b7tet", "still", "von", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Grillen St\u00e4ndchen bringen.\u00ab", "tokens": ["Die", "Gril\u00b7len", "St\u00e4nd\u00b7chen", "brin\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich sing von einem Ort im Rhein,", "tokens": ["Ich", "sing", "von", "ei\u00b7nem", "Ort", "im", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPRART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da liegen gro\u00dfe Glocken,", "tokens": ["Da", "lie\u00b7gen", "gro\u00b7\u00dfe", "Glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wird im Jahr ein edler Wein,", "tokens": ["Und", "wird", "im", "Jahr", "ein", "ed\u00b7ler", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da stehen sie ganz trocken,", "tokens": ["Da", "ste\u00b7hen", "sie", "ganz", "tro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und schlagen drauf die Schiffer an,", "tokens": ["Und", "schla\u00b7gen", "drauf", "die", "Schif\u00b7fer", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da rufen sie nach Weine;", "tokens": ["Da", "ru\u00b7fen", "sie", "nach", "Wei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich bin ein durst'ger Leiermann,", "tokens": ["Ich", "bin", "ein", "dur\u00b7st'\u00b7ger", "Lei\u00b7er\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und habe m\u00fcde Beine.", "tokens": ["Und", "ha\u00b7be", "m\u00fc\u00b7de", "Bei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbhier hast du eine Flasche Wein,", "tokens": ["\u00bb", "hier", "hast", "du", "ei\u00b7ne", "Fla\u00b7sche", "Wein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hier die Bank von Steinen,", "tokens": ["Und", "hier", "die", "Bank", "von", "Stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und denk, du s\u00e4\u00dfest hier am Rhein,", "tokens": ["Und", "denk", ",", "du", "s\u00e4\u00b7\u00dfest", "hier", "am", "Rhein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "APPRART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und tr\u00e4nkst von edlen Weinen;", "tokens": ["Und", "tr\u00e4nkst", "von", "ed\u00b7len", "Wei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und greif mir nicht nach meinem Arm,", "tokens": ["Und", "greif", "mir", "nicht", "nach", "mei\u00b7nem", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich w\u00e4rm ihn in der Sch\u00fcrze,", "tokens": ["Ich", "w\u00e4rm", "ihn", "in", "der", "Sch\u00fcr\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und singe nur, es ist nicht warm,", "tokens": ["Und", "sin\u00b7ge", "nur", ",", "es", "ist", "nicht", "warm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und mir die Zeit verk\u00fcrze.\u00ab", "tokens": ["Und", "mir", "die", "Zeit", "ver\u00b7k\u00fcr\u00b7ze", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Am Rheine war ein geiz'ger Abt,", "tokens": ["Am", "Rhei\u00b7ne", "war", "ein", "gei\u00b7z'\u00b7ger", "Abt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der g\u00f6nnt es nicht den Leuten,", "tokens": ["Der", "g\u00f6nnt", "es", "nicht", "den", "Leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df sie an Trauben sich erlabt,", "tokens": ["Da\u00df", "sie", "an", "Trau\u00b7ben", "sich", "er\u00b7labt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sie zur Lese schreiten;", "tokens": ["Wenn", "sie", "zur", "Le\u00b7se", "schrei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Darum erfand der listge Mann,", "tokens": ["Da\u00b7rum", "er\u00b7fand", "der", "list\u00b7ge", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie mu\u00dften immer singen:", "tokens": ["Sie", "mu\u00df\u00b7ten", "im\u00b7mer", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dieweil dann keiner essen kann,", "tokens": ["Die\u00b7weil", "dann", "kei\u00b7ner", "es\u00b7sen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und die in Butten springen.", "tokens": ["Und", "die", "in", "But\u00b7ten", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "So soll ich singen vor der Th\u00fcr,", "tokens": ["So", "soll", "ich", "sin\u00b7gen", "vor", "der", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und m\u00f6cht dich lieber k\u00fcssen,", "tokens": ["Und", "m\u00f6cht", "dich", "lie\u00b7ber", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O M\u00e4dchen nimm mich doch zu dir,", "tokens": ["O", "M\u00e4d\u00b7chen", "nimm", "mich", "doch", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVIMP", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und morgen will ich gr\u00fc\u00dfen,", "tokens": ["Und", "mor\u00b7gen", "will", "ich", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit allem s\u00fc\u00dfen Zaubersang,", "tokens": ["Mit", "al\u00b7lem", "s\u00fc\u00b7\u00dfen", "Zau\u00b7ber\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gesch\u00f6pft aus deinem Munde,", "tokens": ["Ge\u00b7sch\u00f6pft", "aus", "dei\u00b7nem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Jetzt schweigt mein Mund in Liebesdrang,", "tokens": ["Jetzt", "schweigt", "mein", "Mund", "in", "Lie\u00b7bes\u00b7drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der W\u00e4chter ruft die Stunde.", "tokens": ["Der", "W\u00e4ch\u00b7ter", "ruft", "die", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbder W\u00e4chter singt sein Verslein gut,", "tokens": ["\u00bb", "der", "W\u00e4ch\u00b7ter", "singt", "sein", "Ver\u00b7slein", "gut", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NE", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So gut magst du nicht singen;", "tokens": ["So", "gut", "magst", "du", "nicht", "sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er hat so einen tapfern Muth,", "tokens": ["Er", "hat", "so", "ei\u00b7nen", "tap\u00b7fern", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kann Gespenster zwingen.", "tokens": ["Und", "kann", "Ge\u00b7spens\u00b7ter", "zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er hat ein gar gewaltig Horn,", "tokens": ["Er", "hat", "ein", "gar", "ge\u00b7wal\u00b7tig", "Horn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und bl\u00e4\u00dft recht mir zum Spa\u00dfe,", "tokens": ["Und", "bl\u00e4\u00dft", "recht", "mir", "zum", "Spa\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sein Lieb zu mir hat grimmen Zorn,", "tokens": ["Sein", "Lieb", "zu", "mir", "hat", "grim\u00b7men", "Zorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Darum zieh deine Stra\u00dfe.\u00ab", "tokens": ["Da\u00b7rum", "zieh", "dei\u00b7ne", "Stra\u00b7\u00dfe", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Als ich die Warnung kaum vernehm',", "tokens": ["Als", "ich", "die", "War\u00b7nung", "kaum", "ver\u00b7nehm'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00f6r ich die Hunde heulen,", "tokens": ["H\u00f6r", "ich", "die", "Hun\u00b7de", "heu\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da ist's auch mir so unbequem,", "tokens": ["Da", "ist's", "auch", "mir", "so", "un\u00b7be\u00b7quem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich davon mu\u00df eilen:", "tokens": ["Da\u00df", "ich", "da\u00b7von", "mu\u00df", "ei\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "VMFIN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich seh den W\u00e4chter an der Th\u00fcr,", "tokens": ["Ich", "seh", "den", "W\u00e4ch\u00b7ter", "an", "der", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er thut mein M\u00e4dchen k\u00fcssen,", "tokens": ["Er", "thut", "mein", "M\u00e4d\u00b7chen", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Doch hat sie drauf, das glaubet mir,", "tokens": ["Doch", "hat", "sie", "drauf", ",", "das", "glau\u00b7bet", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKVZ", "$,", "PDS", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Die Th\u00fcr ihm zugeschmissen.", "tokens": ["Die", "Th\u00fcr", "ihm", "zu\u00b7ge\u00b7schmis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wie er nun in seinem Grimm", "tokens": ["Und", "wie", "er", "nun", "in", "sei\u00b7nem", "Grimm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "APPR", "PPOSAT", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ich in meinem Lachen,", "tokens": ["Und", "ich", "in", "mei\u00b7nem", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da ruft er mir mit starker Stimm:", "tokens": ["Da", "ruft", "er", "mir", "mit", "star\u00b7ker", "Stimm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was hast du Nachts zu machen? -", "tokens": ["Was", "hast", "du", "Nachts", "zu", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Lieb ist leer, die Flasch ist aus,", "tokens": ["Die", "Lieb", "ist", "leer", ",", "die", "Flasch", "ist", "aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf dir sei sie zerschmissen!", "tokens": ["Auf", "dir", "sei", "sie", "zer\u00b7schmis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+---+-", "measure": "dactylic.init"}, "line.8": {"text": "Das that ich und sie lacht im Haus;", "tokens": ["Das", "that", "ich", "und", "sie", "lacht", "im", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "KON", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dann bin ich ausgerissen.", "tokens": ["Dann", "bin", "ich", "aus\u00b7ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}