{"dta.poem.459": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sie ihn ihre Liebe endlich entdeckte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich dacht du w\u00e4rest stumm/ allein das Band der Zungen", "tokens": ["Ich", "dacht", "du", "w\u00e4\u00b7rest", "stumm", "/", "al\u00b7lein", "das", "Band", "der", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN", "ADJD", "$(", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird nunmehr auffgel\u00f6\u00dft. Wer zwingt dich doch dazu?", "tokens": ["Wird", "nun\u00b7mehr", "auff\u00b7ge\u00b7l\u00f6\u00dft", ".", "Wer", "zwingt", "dich", "doch", "da\u00b7zu", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "$.", "PWS", "VVFIN", "PPER", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein harr\u2019! ich wei\u00df es schon/ die Lieb hat dich bezwungen/", "tokens": ["Mein", "harr'", "!", "ich", "wei\u00df", "es", "schon", "/", "die", "Lieb", "hat", "dich", "be\u00b7zwun\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER", "ADV", "$(", "ART", "NN", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die lehret dich das Wort/ mein Liebster das bist du.", "tokens": ["Die", "leh\u00b7ret", "dich", "das", "Wort", "/", "mein", "Liebs\u00b7ter", "das", "bist", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ART", "NN", "$(", "PPOSAT", "NN", "PDS", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}