{"textgrid.poem.61135": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Fahr wohl", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den letzten Becher bring' ich dir,", "tokens": ["Den", "letz\u00b7ten", "Be\u00b7cher", "bring'", "ich", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du sch\u00f6ner, fremder Strand!", "tokens": ["Du", "sch\u00f6\u00b7ner", ",", "frem\u00b7der", "Strand", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach, bitter wird das Scheiden mir,", "tokens": ["Ach", ",", "bit\u00b7ter", "wird", "das", "Schei\u00b7den", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJD", "VAFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als w\u00e4r's mein Heimatland.", "tokens": ["Als", "w\u00e4r's", "mein", "Hei\u00b7mat\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Fahr wohl, fahr wohl! Im Segel ruht", "tokens": ["Fahr", "wohl", ",", "fahr", "wohl", "!", "Im", "Se\u00b7gel", "ruht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "VVFIN", "ADV", "$.", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Wind und treibt sein Spiel,", "tokens": ["Der", "Wind", "und", "treibt", "sein", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und rauschend furcht die gr\u00fcne Flut", "tokens": ["Und", "rau\u00b7schend", "furcht", "die", "gr\u00fc\u00b7ne", "Flut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Barke scharfer Kiel.", "tokens": ["Der", "Bar\u00b7ke", "schar\u00b7fer", "Kiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Sonne sinkt ins Inselmeer,", "tokens": ["Die", "Son\u00b7ne", "sinkt", "ins", "In\u00b7sel\u00b7meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Luft gl\u00fcht rosenrot -", "tokens": ["Die", "Luft", "gl\u00fcht", "ro\u00b7sen\u00b7rot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dort schimmert noch das Fenster her,", "tokens": ["Dort", "schim\u00b7mert", "noch", "das", "Fens\u00b7ter", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo sie mir Abschied bot.", "tokens": ["Wo", "sie", "mir", "Ab\u00b7schied", "bot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie gern, wie gern, du holdes Kind,", "tokens": ["Wie", "gern", ",", "wie", "gern", ",", "du", "hol\u00b7des", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "PWAV", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "H\u00e4tt' ich bei dir ges\u00e4umt!", "tokens": ["H\u00e4tt'", "ich", "bei", "dir", "ge\u00b7s\u00e4umt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Umsonst, auch dieser Traum zerrinnt", "tokens": ["Um\u00b7sonst", ",", "auch", "die\u00b7ser", "Traum", "zer\u00b7rinnt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und war so sch\u00f6n getr\u00e4umt.", "tokens": ["Und", "war", "so", "sch\u00f6n", "ge\u00b7tr\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das ist das Leben: Kommen, Gehn,", "tokens": ["Das", "ist", "das", "Le\u00b7ben", ":", "Kom\u00b7men", ",", "Gehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$.", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Treiben in Wind und Flut;", "tokens": ["Trei\u00b7ben", "in", "Wind", "und", "Flut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Fortziehn auf Nimmerwiedersehn,", "tokens": ["Fort\u00b7ziehn", "auf", "Nim\u00b7mer\u00b7wie\u00b7der\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn kaum wir sanft geruht;", "tokens": ["Wenn", "kaum", "wir", "sanft", "ge\u00b7ruht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Geliebt sein und vergessen sein,", "tokens": ["Ge\u00b7liebt", "sein", "und", "ver\u00b7ges\u00b7sen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "KON", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Selbst lieben - still! - Mir deucht,", "tokens": ["Selbst", "lie\u00b7ben", "still", "!", "Mir", "deucht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$(", "PTKVZ", "$.", "$(", "PPER", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Es blendet mich der Abendschein,", "tokens": ["Es", "blen\u00b7det", "mich", "der", "A\u00b7bend\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mir wird die Wimper feucht.", "tokens": ["Mir", "wird", "die", "Wim\u00b7per", "feucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Vorbei! vorbei! Die Tr\u00e4ne f\u00e4llt;", "tokens": ["Vor\u00b7bei", "!", "vor\u00b7bei", "!", "Die", "Tr\u00e4\u00b7ne", "f\u00e4llt", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PTKVZ", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vorbei so Lust als Schmerz!", "tokens": ["Vor\u00b7bei", "so", "Lust", "als", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "KOUS", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wieder einsam in der Welt", "tokens": ["Und", "wie\u00b7der", "ein\u00b7sam", "in", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schl\u00e4gt nun dies wilde Herz!", "tokens": ["Schl\u00e4gt", "nun", "dies", "wil\u00b7de", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PDS", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sei's drum! - Des Mondes erster Strahl", "tokens": ["Sei's", "drum", "!", "Des", "Mon\u00b7des", "ers\u00b7ter", "Strahl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PAV", "$.", "$(", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Begl\u00e4nzt das Meer in Pracht;", "tokens": ["Be\u00b7gl\u00e4nzt", "das", "Meer", "in", "Pracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die K\u00fcste flieht - zum letztenmal,", "tokens": ["Die", "K\u00fcs\u00b7te", "flieht", "zum", "letz\u00b7ten\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "APPRART", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Mein M\u00e4dchen, gute Nacht!", "tokens": ["Mein", "M\u00e4d\u00b7chen", ",", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}