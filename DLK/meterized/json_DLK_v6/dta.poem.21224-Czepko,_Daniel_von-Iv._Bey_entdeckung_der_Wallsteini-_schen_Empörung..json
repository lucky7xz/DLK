{"dta.poem.21224": {"metadata": {"author": {"name": "Czepko, Daniel von", "birth": "N.A.", "death": "N.A."}, "title": "Iv.  \n  Bey entdeckung der Wallsteini-  \n schen Emp\u00f6rung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20871-8", "language": ["de:0.99"], "booktitle": "Czepko, Daniel von: Triumph Bogen. Breslau, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Nichts ligt so tieff/ es sucht zu letzte Lufft vnd Licht/", "tokens": ["Nichts", "ligt", "so", "tieff", "/", "es", "sucht", "zu", "letz\u00b7te", "Lufft", "vnd", "Licht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADJD", "$(", "PPER", "VVFIN", "APPR", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da al\u00df vmb ", "tokens": ["Da", "al\u00df", "vmb"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "APPR"], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "Jm Nebel seinen Leib der ", "tokens": ["Jm", "Ne\u00b7bel", "sei\u00b7nen", "Leib", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Stund der von ", "tokens": ["Stund", "der", "von"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "APPR"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Also Jhr ", "tokens": ["Al\u00b7so", "Ihr"], "token_info": ["word", "word"], "pos": ["ADV", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Jhr m\u00fcst ja auff vnd ab im blancken Degen reiten:", "tokens": ["Ihr", "m\u00fcst", "ja", "auff", "vnd", "ab", "im", "blan\u00b7cken", "De\u00b7gen", "rei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "KON", "PTKVZ", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wolt Jhr jtzt wiedern ", "tokens": ["Wolt", "Ihr", "jtzt", "wie\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Nein/ bleibet trew vnd thut/ was ", "tokens": ["Nein", "/", "blei\u00b7bet", "trew", "vnd", "thut", "/", "was"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PTKANT", "$(", "VVFIN", "ADJD", "KON", "VVFIN", "$(", "PWS"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Vnd schawt die Sach ist kranck: Er rei\u00dft auff ", "tokens": ["Vnd", "schawt", "die", "Sach", "ist", "kranck", ":", "Er", "rei\u00dft", "auff"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "VAFIN", "ADJD", "$.", "PPER", "VVFIN", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd rufft den Feinden zu/ die Jhm zu langsam eilen.", "tokens": ["Vnd", "rufft", "den", "Fein\u00b7den", "zu", "/", "die", "Jhm", "zu", "lang\u00b7sam", "ei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKZU", "$(", "PRELS", "PPER", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er kan ja vnter Euch die ", "tokens": ["Er", "kan", "ja", "vn\u00b7ter", "Euch", "die"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPER", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie ist vnd bleibt bey ", "tokens": ["Sie", "ist", "vnd", "bleibt", "bey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KON", "VVFIN", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Sey wach/ ", "tokens": ["Sey", "wach", "/"], "token_info": ["word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$("], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Der di\u00dfmal falsch gespiele/ ist nicht so ", "tokens": ["Der", "di\u00df\u00b7mal", "falsch", "ge\u00b7spie\u00b7le", "/", "ist", "nicht", "so"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "$(", "VAFIN", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer wei\u00df wie viel der ", "tokens": ["Wer", "wei\u00df", "wie", "viel", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "KOKOM", "ADV", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Gieb Jhnen gleiche Macht/ Sie drewen gleiche Noth.", "tokens": ["Gieb", "Jh\u00b7nen", "glei\u00b7che", "Macht", "/", "Sie", "dre\u00b7wen", "glei\u00b7che", "Noth", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "$(", "PPER", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}