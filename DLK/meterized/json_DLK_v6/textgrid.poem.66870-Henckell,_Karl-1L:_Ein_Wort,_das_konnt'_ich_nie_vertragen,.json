{"textgrid.poem.66870": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Wort, das konnt' ich nie vertragen,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Wort, das konnt' ich nie vertragen,", "tokens": ["Ein", "Wort", ",", "das", "konnt'", "ich", "nie", "ver\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VMFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das lag mir immer schwer im Magen,", "tokens": ["Das", "lag", "mir", "im\u00b7mer", "schwer", "im", "Ma\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Schien mir auf Menschenwert ein Hohn,", "tokens": ["Schien", "mir", "auf", "Men\u00b7schen\u00b7wert", "ein", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das sch\u00f6ne W\u00f6rtchen: Protektion.", "tokens": ["Das", "sch\u00f6\u00b7ne", "W\u00f6rt\u00b7chen", ":", "Pro\u00b7tek\u00b7ti\u00b7on", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und gern mir red' ich's von der Leber:", "tokens": ["Und", "gern", "mir", "red'", "ich's", "von", "der", "Le\u00b7ber", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Steig f\u00fcr subalterne Streber,", "tokens": ["Ein", "Steig", "f\u00fcr", "sub\u00b7al\u00b7ter\u00b7ne", "Stre\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "F\u00fcr Diener ein geweihter Thron,", "tokens": ["F\u00fcr", "Die\u00b7ner", "ein", "ge\u00b7weih\u00b7ter", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die sch\u00f6ne Leiter: Protektion.", "tokens": ["Die", "sch\u00f6\u00b7ne", "Lei\u00b7ter", ":", "Pro\u00b7tek\u00b7ti\u00b7on", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Teilnahme will ich gern genie\u00dfen.", "tokens": ["Teil\u00b7nah\u00b7me", "will", "ich", "gern", "ge\u00b7nie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wen wird auch F\u00f6rderung verdrie\u00dfen?", "tokens": ["Wen", "wird", "auch", "F\u00f6r\u00b7de\u00b7rung", "ver\u00b7drie\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch weg den Armenschluckerlohn,", "tokens": ["Doch", "weg", "den", "Ar\u00b7mensc\u00b7hlu\u00b7cker\u00b7lohn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das sch\u00f6ne Scherflein: Protektion!", "tokens": ["Das", "sch\u00f6\u00b7ne", "Scher\u00b7flein", ":", "Pro\u00b7tek\u00b7ti\u00b7on", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Das schmeckt mir viel zu sehr nach Gnade,", "tokens": ["Das", "schmeckt", "mir", "viel", "zu", "sehr", "nach", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKA", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da wird der Dank gemein und fade,", "tokens": ["Da", "wird", "der", "Dank", "ge\u00b7mein", "und", "fa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn \u00bbhuldvoll\u00ab einheimst der \u00bbPatron\u00ab", "tokens": ["Wenn", "\u00bb", "huld\u00b7voll", "\u00ab", "ein\u00b7heimst", "der", "\u00bb", "Pat\u00b7ron", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "$(", "ADJD", "$(", "VVFIN", "ART", "$(", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Den sch\u00f6nen Schuldschein: Protektion.", "tokens": ["Den", "sch\u00f6\u00b7nen", "Schuld\u00b7schein", ":", "Pro\u00b7tek\u00b7ti\u00b7on", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Mein Leben m\u00f6ge mich beh\u00fcten", "tokens": ["Mein", "Le\u00b7ben", "m\u00f6\u00b7ge", "mich", "be\u00b7h\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor solchen ungeliebten G\u00fcten,", "tokens": ["Vor", "sol\u00b7chen", "un\u00b7ge\u00b7lieb\u00b7ten", "G\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vor ausstaffierter Bettlerfron", "tokens": ["Vor", "aus\u00b7staf\u00b7fier\u00b7ter", "Bett\u00b7ler\u00b7fron"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der sch\u00f6nen Huldin: Protektion!", "tokens": ["Der", "sch\u00f6\u00b7nen", "Hul\u00b7din", ":", "Pro\u00b7tek\u00b7ti\u00b7on", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}