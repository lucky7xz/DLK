{"dta.poem.7455": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XiI .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Ihr voll Mut zu schleudern euch nicht scheutet", "tokens": ["Die", "Ihr", "voll", "Mut", "zu", "schleu\u00b7dern", "euch", "nicht", "scheu\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "NN", "PTKZU", "VVFIN", "PPER", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein blitzend Wort in unsres Lebens Schw\u00fcle,", "tokens": ["Ein", "blit\u00b7zend", "Wort", "in", "uns\u00b7res", "Le\u00b7bens", "Schw\u00fc\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O Gl\u00fcck, wenn ihr euch auf dem Sterbepf\u00fchle", "tokens": ["O", "Gl\u00fcck", ",", "wenn", "ihr", "euch", "auf", "dem", "Ster\u00b7be\u00b7pf\u00fch\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "KOUS", "PPER", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom Neid zerst\u00fcckter Kr\u00e4nze noch erfreutet!", "tokens": ["Vom", "Neid", "zer\u00b7st\u00fcck\u00b7ter", "Kr\u00e4n\u00b7ze", "noch", "er\u00b7freu\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie haben Ruhm in Scheffeln sich erbeutet,", "tokens": ["Wie", "ha\u00b7ben", "Ruhm", "in", "Schef\u00b7feln", "sich", "er\u00b7beu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NN", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ruhig trabten ihren Weg zur M\u00fchle", "tokens": ["Die", "ru\u00b7hig", "trab\u00b7ten", "ih\u00b7ren", "Weg", "zur", "M\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und immer h\u00fcbsch die trunkensten Gef\u00fchle", "tokens": ["Und", "im\u00b7mer", "h\u00fcbsch", "die", "trun\u00b7kens\u00b7ten", "Ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Gleich tauben Bl\u00fcten aus dem Korn gereutet!", "tokens": ["Gleich", "tau\u00b7ben", "Bl\u00fc\u00b7ten", "aus", "dem", "Korn", "ge\u00b7reu\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Brauch' deine Hand, die ist der Welt genug,", "tokens": ["Brauch'", "dei\u00b7ne", "Hand", ",", "die", "ist", "der", "Welt", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PRELS", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Kopf und Herz sind beide \u00fcberfl\u00fcssig;", "tokens": ["Und", "Kopf", "und", "Herz", "sind", "bei\u00b7de", "\u00fc\u00b7berf\u00b7l\u00fcs\u00b7sig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Man will den Flaum vom Vogel, nicht den Flug.", "tokens": ["Man", "will", "den", "Flaum", "vom", "Vo\u00b7gel", ",", "nicht", "den", "Flug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "APPRART", "NE", "$,", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Kannst du nur dichten, gehe lieber m\u00fcssig;", "tokens": ["Kannst", "du", "nur", "dich\u00b7ten", ",", "ge\u00b7he", "lie\u00b7ber", "m\u00fcs\u00b7sig", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJA", "$,", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Welt, die stets das Ungereimte trug,", "tokens": ["Die", "Welt", ",", "die", "stets", "das", "Un\u00b7ge\u00b7reim\u00b7te", "trug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist des Gereimten schnell sehr \u00fcberdr\u00fcssig.", "tokens": ["Ist", "des", "Ge\u00b7reim\u00b7ten", "schnell", "sehr", "\u00fc\u00b7berd\u00b7r\u00fcs\u00b7sig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}