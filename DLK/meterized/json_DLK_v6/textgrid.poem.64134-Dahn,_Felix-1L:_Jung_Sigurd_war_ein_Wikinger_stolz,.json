{"textgrid.poem.64134": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Jung Sigurd war ein Wikinger stolz,", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jung Sigurd war ein Wikinger stolz,", "tokens": ["Jung", "Si\u00b7gurd", "war", "ein", "Wi\u00b7kin\u00b7ger", "stolz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der fuhr in den Sturm mit Lachen,", "tokens": ["Der", "fuhr", "in", "den", "Sturm", "mit", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und schwang er die Lanze von Eschenholz,", "tokens": ["Und", "schwang", "er", "die", "Lan\u00b7ze", "von", "E\u00b7schen\u00b7holz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Da mu\u00dften die Schilde zerkrachen:", "tokens": ["Da", "mu\u00df\u00b7ten", "die", "Schil\u00b7de", "zer\u00b7kra\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Die Traube von Chios, das Gold von Byzanz,", "tokens": ["Die", "Trau\u00b7be", "von", "Chios", ",", "das", "Gold", "von", "By\u00b7zanz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Begehrte sein Herz und sein Hammer gewann's.", "tokens": ["Be\u00b7gehr\u00b7te", "sein", "Herz", "und", "sein", "Ham\u00b7mer", "ge\u00b7wann'", "s."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["NN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Doch priesen die Freunde den bl\u00fchenden Leib", "tokens": ["Doch", "prie\u00b7sen", "die", "Freun\u00b7de", "den", "bl\u00fc\u00b7hen\u00b7den", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der R\u00f6merin, die sie gefangen,", "tokens": ["Der", "R\u00f6\u00b7me\u00b7rin", ",", "die", "sie", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und lobt ihm ein andrer sein ehelich Weib,", "tokens": ["Und", "lobt", "ihm", "ein", "an\u00b7drer", "sein", "e\u00b7he\u00b7lich", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Das daheim sein harre mit Bangen,", "tokens": ["Das", "da\u00b7heim", "sein", "har\u00b7re", "mit", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PPOSAT", "ADJA", "APPR", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und sprach ihm von Lieb' und von Liebesglut, \u2013", "tokens": ["Und", "sprach", "ihm", "von", "Lieb'", "und", "von", "Lie\u00b7bes\u00b7glut", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "KON", "APPR", "NN", "$,", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Laut lachte jung Sigurd wie brandende Flut.", "tokens": ["Laut", "lach\u00b7te", "jung", "Si\u00b7gurd", "wie", "bran\u00b7den\u00b7de", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ADJD", "NN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "\u2013 \u00bbMein schwellendes Segel hat wei\u00dfere Brust", "tokens": ["\u2013", "\u00bb", "Mein", "schwel\u00b7len\u00b7des", "Se\u00b7gel", "hat", "wei\u00b7\u00dfe\u00b7re", "Brust"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "$(", "PPOSAT", "ADJA", "NN", "VAFIN", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Als euere Buhlen, ihr Schelme,", "tokens": ["Als", "eu\u00b7e\u00b7re", "Buh\u00b7len", ",", "ihr", "Schel\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Mir ist kein Weiberauge bewu\u00dft", "tokens": ["Mir", "ist", "kein", "Wei\u00b7be\u00b7rau\u00b7ge", "be\u00b7wu\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADJD"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "So licht wie der Stein hier am Helme,", "tokens": ["So", "licht", "wie", "der", "Stein", "hier", "am", "Hel\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und l\u00fcstet nach lieblicher S\u00fc\u00dfe mein Mund,", "tokens": ["Und", "l\u00fcs\u00b7tet", "nach", "lieb\u00b7li\u00b7cher", "S\u00fc\u00b7\u00dfe", "mein", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "So schl\u00fcrf' ich den feurigen Wein von Burgund.", "tokens": ["So", "schl\u00fcr\u00b7f'", "ich", "den", "feu\u00b7ri\u00b7gen", "Wein", "von", "Bur\u00b7gund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "NE", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Ja, stieg', umflossen von Asgardhs Licht,", "tokens": ["Ja", ",", "stieg'", ",", "um\u00b7flos\u00b7sen", "von", "As\u00b7gardhs", "Licht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "$,", "VVFIN", "APPR", "NE", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mir Freya selber hernieder, \u2013", "tokens": ["Mir", "Frey\u00b7a", "sel\u00b7ber", "her\u00b7nie\u00b7der", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "ADV", "PTKVZ", "$,", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "F\u00fcrwahr, ich h\u00f6be die Wimper nicht,", "tokens": ["F\u00fcr\u00b7wahr", ",", "ich", "h\u00f6\u00b7be", "die", "Wim\u00b7per", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zu schau'n die unsterblichen Glieder:", "tokens": ["Zu", "schau'n", "die", "uns\u00b7terb\u00b7li\u00b7chen", "Glie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Wenn je mir ein Sehnen die Sch\u00f6nheit weckt,", "tokens": ["Wenn", "je", "mir", "ein", "Seh\u00b7nen", "die", "Sch\u00f6n\u00b7heit", "weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "So werde mit Nacht dies Auge bedeckt.\u00ab \u2013", "tokens": ["So", "wer\u00b7de", "mit", "Nacht", "dies", "Au\u00b7ge", "be\u00b7deckt", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "PDS", "NN", "VVPP", "$.", "$(", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und sie landen am \u00f6den Felsengestad", "tokens": ["Und", "sie", "lan\u00b7den", "am", "\u00f6\u00b7den", "Fel\u00b7sen\u00b7ge\u00b7stad"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJD", "APPRART", "ADJA", "NN"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Im Strahl mitt\u00e4glicher Sonnen: \u2013", "tokens": ["Im", "Strahl", "mit\u00b7t\u00e4g\u00b7li\u00b7cher", "Son\u00b7nen", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Jung Sigurd schweift auf verlassenem Pfad,", "tokens": ["Jung", "Si\u00b7gurd", "schweift", "auf", "ver\u00b7las\u00b7se\u00b7nem", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Da lockt ihn der rieselnde Bronnen", "tokens": ["Da", "lockt", "ihn", "der", "rie\u00b7seln\u00b7de", "Bron\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und als er schreitet zum Quellenrand,", "tokens": ["Und", "als", "er", "schrei\u00b7tet", "zum", "Quel\u00b7len\u00b7rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Da steht ein M\u00e4dchen im Bettlergewand;", "tokens": ["Da", "steht", "ein", "M\u00e4d\u00b7chen", "im", "Bett\u00b7ler\u00b7ge\u00b7wand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Wohl birgt sie der Schleier, wohl deckt sie der Rock,", "tokens": ["Wohl", "birgt", "sie", "der", "Schlei\u00b7er", ",", "wohl", "deckt", "sie", "der", "Rock", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Doch es schimmern so schneeig die F\u00fc\u00dfe,", "tokens": ["Doch", "es", "schim\u00b7mern", "so", "schne\u00b7e\u00b7ig", "die", "F\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "--+--+---+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und es gl\u00e4nzt durch die H\u00fclle wie golden Gelock", "tokens": ["Und", "es", "gl\u00e4nzt", "durch", "die", "H\u00fcl\u00b7le", "wie", "gol\u00b7den", "Ge\u00b7lock"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "KOKOM", "ADJA", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Und die Stimme, wie klingt sie so s\u00fc\u00dfe!", "tokens": ["Und", "die", "Stim\u00b7me", ",", "wie", "klingt", "sie", "so", "s\u00fc\u00b7\u00dfe", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "VVFIN", "PPER", "ADV", "ADJA", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Und als sie zum Trunke den Krug ihm bot, \u2013", "tokens": ["Und", "als", "sie", "zum", "Trun\u00b7ke", "den", "Krug", "ihm", "bot", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "KOUS", "PPER", "APPRART", "NN", "ART", "NN", "PPER", "VVFIN", "$,", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Da wurden die Wangen ihm bleich und rot:", "tokens": ["Da", "wur\u00b7den", "die", "Wan\u00b7gen", "ihm", "bleich", "und", "rot", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Und es wallte sein Blut und sein Herz schlug laut", "tokens": ["Und", "es", "wall\u00b7te", "sein", "Blut", "und", "sein", "Herz", "schlug", "laut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN", "ADJD"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Und er rief: \u00bbO lege geschwinde,", "tokens": ["Und", "er", "rief", ":", "\u00bb", "O", "le\u00b7ge", "ge\u00b7schwin\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "$(", "NE", "VVFIN", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf da\u00df mein verlangend Auge dich schaut,", "tokens": ["Auf", "da\u00df", "mein", "ver\u00b7lan\u00b7gend", "Au\u00b7ge", "dich", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPOSAT", "VVPP", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Vom Haupte die h\u00fcllende Binde:", "tokens": ["Vom", "Haup\u00b7te", "die", "h\u00fcl\u00b7len\u00b7de", "Bin\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Aus Mantel und Schleier wie strahlt es licht,", "tokens": ["Aus", "Man\u00b7tel", "und", "Schlei\u00b7er", "wie", "strahlt", "es", "licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KOKOM", "VVFIN", "PPER", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Wie hold mu\u00df strahlen dein Angesicht!\u00ab", "tokens": ["Wie", "hold", "mu\u00df", "strah\u00b7len", "dein", "An\u00b7ge\u00b7sicht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Und er greift nach den Falten und bittet und fleht: \u2013", "tokens": ["Und", "er", "greift", "nach", "den", "Fal\u00b7ten", "und", "bit\u00b7tet", "und", "fleht", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "KON", "VVFIN", "$.", "$("], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Da ruft sie: \u00bbDir werde dein Wille!\u00ab", "tokens": ["Da", "ruft", "sie", ":", "\u00bb", "Dir", "wer\u00b7de", "dein", "Wil\u00b7le", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "PPER", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und der Mantel f\u00e4llt und der Schleier verweht: \u2013", "tokens": ["Und", "der", "Man\u00b7tel", "f\u00e4llt", "und", "der", "Schlei\u00b7er", "ver\u00b7weht", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "$.", "$("], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da wurde jung Sigurd stille,", "tokens": ["Da", "wur\u00b7de", "jung", "Si\u00b7gurd", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "NE", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Denn hehr, von unsterblichem Glanz umwallt,", "tokens": ["Denn", "hehr", ",", "von", "uns\u00b7terb\u00b7li\u00b7chem", "Glanz", "um\u00b7wallt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach