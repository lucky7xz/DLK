{"dta.poem.10922": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Fisch-Ottern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer bewundert nicht die H\u00f6hle! wer ergetzet sein Gesicht", "tokens": ["Wer", "be\u00b7wun\u00b7dert", "nicht", "die", "H\u00f6h\u00b7le", "!", "wer", "er\u00b7get\u00b7zet", "sein", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "ART", "NN", "$.", "PWS", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "An der unterirdschen Landschaft Schrecken-reicher Sch\u00f6n-", "tokens": ["An", "der", "un\u00b7ter\u00b7ird\u00b7schen", "Land\u00b7schaft", "Schre\u00b7cken\u00b7rei\u00b7cher", "Sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Wer bewundert der Bewohner so des Landes, als der Fluth,", "tokens": ["Wer", "be\u00b7wun\u00b7dert", "der", "Be\u00b7woh\u00b7ner", "so", "des", "Lan\u00b7des", ",", "als", "der", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Der recht m\u00f6rderischen Ottern, nimmersatten Fra\u00df und Wuth", "tokens": ["Der", "recht", "m\u00f6r\u00b7de\u00b7ri\u00b7schen", "Ot\u00b7tern", ",", "nim\u00b7mer\u00b7sat\u00b7ten", "Fra\u00df", "und", "Wuth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Nicht, mit einem halben Grauen! Seht, wie lebhaft alles ist!", "tokens": ["Nicht", ",", "mit", "ei\u00b7nem", "hal\u00b7ben", "Grau\u00b7en", "!", "Seht", ",", "wie", "leb\u00b7haft", "al\u00b7les", "ist", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "APPR", "ART", "ADJA", "NN", "$.", "VVFIN", "$,", "PWAV", "ADJD", "PIS", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Seht doch, wie das Otter-Paar so nat\u00fcrlich Fische fri\u00dft!", "tokens": ["Seht", "doch", ",", "wie", "das", "Ot\u00b7ter\u00b7Paar", "so", "na\u00b7t\u00fcr\u00b7lich", "Fi\u00b7sche", "fri\u00dft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWAV", "ART", "NN", "ADV", "ADV", "NN", "VVFIN", "$."], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Schien des strengen Wasserfalles Rauschen es nicht zu ver-", "tokens": ["Schien", "des", "stren\u00b7gen", "Was\u00b7ser\u00b7fal\u00b7les", "Rau\u00b7schen", "es", "nicht", "zu", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "APPR", "TRUNC"], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Deucht mich, da\u00df man ihren\u2019 Bi\u00df deutlich w\u00fcrde knarschen h\u00f6ren.", "tokens": ["Deucht", "mich", ",", "da\u00df", "man", "ih\u00b7ren'", "Bi\u00df", "deut\u00b7lich", "w\u00fcr\u00b7de", "knar\u00b7schen", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PIS", "PPOSAT", "APPR", "ADJD", "VAFIN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.9": {"text": "Wie die\u00df Thier, zu seiner Nahrung, recht Bewunderns-werth", "tokens": ["Wie", "die\u00df", "Thier", ",", "zu", "sei\u00b7ner", "Nah\u00b7rung", ",", "recht", "Be\u00b7wun\u00b7derns\u00b7werth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PDS", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "ADJD", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.10": {"text": "Mu\u00df man halb erstaunt gestehen. Kopf und Schwanz und", "tokens": ["Mu\u00df", "man", "halb", "er\u00b7staunt", "ge\u00b7ste\u00b7hen", ".", "Kopf", "und", "Schwanz", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "ADJD", "ADJD", "VVPP", "$.", "NN", "KON", "NN", "KON"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.11": {"text": "Lassen eine weise Vorsorg, einen weisen Endzweck schauen.", "tokens": ["Las\u00b7sen", "ei\u00b7ne", "wei\u00b7se", "Vor\u00b7sorg", ",", "ei\u00b7nen", "wei\u00b7sen", "End\u00b7zweck", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Ob man nun gleich oft, durch sie, Schaden in den Teichen", "tokens": ["Ob", "man", "nun", "gleich", "oft", ",", "durch", "sie", ",", "Scha\u00b7den", "in", "den", "Tei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "ADV", "ADV", "$,", "APPR", "PPER", "$,", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wird doch, durch die sch\u00f6nen B\u00e4lg\u2019, oft auch der Verlust ersetzet,", "tokens": ["Wird", "doch", ",", "durch", "die", "sch\u00f6\u00b7nen", "B\u00e4l\u00b7g'", ",", "oft", "auch", "der", "Ver\u00b7lust", "er\u00b7set\u00b7zet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Da man ihr sch\u00f6n gl\u00e4nzend Haar besser, als der Biber, sch\u00e4tzet,", "tokens": ["Da", "man", "ihr", "sch\u00f6n", "gl\u00e4n\u00b7zend", "Haar", "bes\u00b7ser", ",", "als", "der", "Bi\u00b7ber", ",", "sch\u00e4t\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADJD", "ADJD", "NN", "ADJD", "$,", "KOUS", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Und zun feinsten H\u00fcten braucht. Ja es soll den Schwindel", "tokens": ["Und", "zun", "feins\u00b7ten", "H\u00fc\u00b7ten", "braucht", ".", "Ja", "es", "soll", "den", "Schwin\u00b7del"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN", "$.", "PTKANT", "PPER", "VMFIN", "ART", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Und ein Strumpf von Otterbalg podagr\u00e4msche Schmerzen", "tokens": ["Und", "ein", "Strumpf", "von", "Ot\u00b7ter\u00b7balg", "po\u00b7da\u00b7gr\u00e4m\u00b7sche", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NE", "ADJA", "NN"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Wird man also, wenn wir denken, wie wir sollen, auch so gar", "tokens": ["Wird", "man", "al\u00b7so", ",", "wenn", "wir", "den\u00b7ken", ",", "wie", "wir", "sol\u00b7len", ",", "auch", "so", "gar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PIS", "ADV", "$,", "KOUS", "PPER", "VVINF", "$,", "PWAV", "PPER", "VMFIN", "$,", "ADV", "ADV", "ADV"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Selbst an Ottern, nebst dem Nutzen, einer weisen Macht gewahr.", "tokens": ["Selbst", "an", "Ot\u00b7tern", ",", "nebst", "dem", "Nut\u00b7zen", ",", "ei\u00b7ner", "wei\u00b7sen", "Macht", "ge\u00b7wahr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}