{"textgrid.poem.61755": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Deutsche Wanderschaft", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Wald steht in Bl\u00fcte, die wilden Schw\u00e4ne ziehn,", "tokens": ["Der", "Wald", "steht", "in", "Bl\u00fc\u00b7te", ",", "die", "wil\u00b7den", "Schw\u00e4\u00b7ne", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mir klingt's im Gem\u00fcte wie Wandermelodien;", "tokens": ["Mir", "klingt's", "im", "Ge\u00b7m\u00fc\u00b7te", "wie", "Wan\u00b7der\u00b7me\u00b7lo\u00b7di\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KOKOM", "NN", "$."], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.3": {"text": "Zum Stab mu\u00df ich greifen, leb' wohl, altes Haus!", "tokens": ["Zum", "Stab", "mu\u00df", "ich", "grei\u00b7fen", ",", "leb'", "wohl", ",", "al\u00b7tes", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "VVINF", "$,", "VVFIN", "ADV", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und singend wieder schweifen ins deutsche Land hinaus.", "tokens": ["Und", "sin\u00b7gend", "wie\u00b7der", "schwei\u00b7fen", "ins", "deut\u00b7sche", "Land", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Ihr blauenden Gipfel, ihr T\u00e4ler, Gott gr\u00fc\u00df'!", "tokens": ["Ihr", "blau\u00b7en\u00b7den", "Gip\u00b7fel", ",", "ihr", "T\u00e4\u00b7ler", ",", "Gott", "gr\u00fc\u00df'", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ihr dunkeln Eichenwipfel, wie rauscht ihr so s\u00fc\u00df!", "tokens": ["Ihr", "dun\u00b7keln", "Ei\u00b7chen\u00b7wip\u00b7fel", ",", "wie", "rauscht", "ihr", "so", "s\u00fc\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PWAV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ihr wollt mir's erz\u00e4hlen, da\u00df endlich hoffnungsvoll", "tokens": ["Ihr", "wollt", "mir's", "er\u00b7z\u00e4h\u00b7len", ",", "da\u00df", "end\u00b7lich", "hoff\u00b7nungs\u00b7voll"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIS", "VVINF", "$,", "KOUS", "ADV", "ADJD"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Durch alle deutschen Seelen ein Lenzodem quoll.", "tokens": ["Durch", "al\u00b7le", "deut\u00b7schen", "See\u00b7len", "ein", "Len\u00b7zo\u00b7dem", "quoll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Durch Steingekl\u00fcft und Forsten zu klimmen, o Lust!", "tokens": ["Durch", "Stein\u00b7ge\u00b7kl\u00fcft", "und", "Fors\u00b7ten", "zu", "klim\u00b7men", ",", "o", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$,", "FM", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf schwindelnden Horsten zu l\u00fcften die Brust.", "tokens": ["Auf", "schwin\u00b7deln\u00b7den", "Hors\u00b7ten", "zu", "l\u00fcf\u00b7ten", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Tief unten verklingen die Glocken weit umher,", "tokens": ["Tief", "un\u00b7ten", "ver\u00b7klin\u00b7gen", "die", "Glo\u00b7cken", "weit", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ein Adler hebt die Schwingen vom Felsen zum Meer.", "tokens": ["Ein", "Ad\u00b7ler", "hebt", "die", "Schwin\u00b7gen", "vom", "Fel\u00b7sen", "zum", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Ins Brausen der Quellen wie pocht der H\u00e4mmer Schlag!", "tokens": ["Ins", "Brau\u00b7sen", "der", "Quel\u00b7len", "wie", "pocht", "der", "H\u00e4m\u00b7mer", "Schlag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "KOKOM", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da f\u00f6rdern die Gesellen das Eisen zutag',", "tokens": ["Da", "f\u00f6r\u00b7dern", "die", "Ge\u00b7sel\u00b7len", "das", "Ei\u00b7sen", "zu\u00b7tag'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da w\u00e4chst in roter Erde das Schwert f\u00fcr den Feind,", "tokens": ["Da", "w\u00e4chst", "in", "ro\u00b7ter", "Er\u00b7de", "das", "Schwert", "f\u00fcr", "den", "Feind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der uns am deutschen Herde noch dreinzureden meint.", "tokens": ["Der", "uns", "am", "deut\u00b7schen", "Her\u00b7de", "noch", "drein\u00b7zu\u00b7re\u00b7den", "meint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "ADJA", "NN", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Nun kommst auch du geschwommen im fr\u00fchroten Schein,", "tokens": ["Nun", "kommst", "auch", "du", "ge\u00b7schwom\u00b7men", "im", "fr\u00fch\u00b7ro\u00b7ten", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Willkommen, willkommen, du dunkelgr\u00fcner Rhein!", "tokens": ["Will\u00b7kom\u00b7men", ",", "will\u00b7kom\u00b7men", ",", "du", "dun\u00b7kel\u00b7gr\u00fc\u00b7ner", "Rhein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "PPER", "ADJA", "NE", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Du tr\u00e4nkst mit goldner Freude dein bl\u00fchend Gel\u00e4nd'", "tokens": ["Du", "tr\u00e4nkst", "mit", "gold\u00b7ner", "Freu\u00b7de", "dein", "bl\u00fc\u00b7hend", "Ge\u00b7l\u00e4nd'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und wei\u00dft von keiner Scheide, die seine St\u00e4mme trennt.", "tokens": ["Und", "wei\u00dft", "von", "kei\u00b7ner", "Schei\u00b7de", ",", "die", "sei\u00b7ne", "St\u00e4m\u00b7me", "trennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Wie lang wird es w\u00e4hren, Altvater, so pre\u00dft", "tokens": ["Wie", "lang", "wird", "es", "w\u00e4h\u00b7ren", ",", "Alt\u00b7va\u00b7ter", ",", "so", "pre\u00dft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "VVINF", "$,", "NN", "$,", "ADV", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Man wieder deine Beeren zum Kaiserkr\u00f6nungsfest,", "tokens": ["Man", "wie\u00b7der", "dei\u00b7ne", "Bee\u00b7ren", "zum", "Kai\u00b7ser\u00b7kr\u00f6\u00b7nungs\u00b7fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da kommt auf deinen Wogen im Purpurgewand", "tokens": ["Da", "kommt", "auf", "dei\u00b7nen", "Wo\u00b7gen", "im", "Pur\u00b7pur\u00b7ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der Hort des Reichs gezogen, das Banner in der Hand.", "tokens": ["Der", "Hort", "des", "Reichs", "ge\u00b7zo\u00b7gen", ",", "das", "Ban\u00b7ner", "in", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Dann ruhen alle Waffen, dann ist es vollbracht,", "tokens": ["Dann", "ru\u00b7hen", "al\u00b7le", "Waf\u00b7fen", ",", "dann", "ist", "es", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dran tausend Jahr' geschaffen, das Werk deutscher Macht,", "tokens": ["Dran", "tau\u00b7send", "Jahr'", "ge\u00b7schaf\u00b7fen", ",", "das", "Werk", "deut\u00b7scher", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "CARD", "NN", "VVPP", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In Norden und S\u00fcden der letzte Zwist ges\u00fchnt", "tokens": ["In", "Nor\u00b7den", "und", "S\u00fc\u00b7den", "der", "letz\u00b7te", "Zwist", "ge\u00b7s\u00fchnt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVPP"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und Freiheit und Frieden, so weit die Eiche gr\u00fcnt.", "tokens": ["Und", "Frei\u00b7heit", "und", "Frie\u00b7den", ",", "so", "weit", "die", "Ei\u00b7che", "gr\u00fcnt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,", "ADV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}}}}