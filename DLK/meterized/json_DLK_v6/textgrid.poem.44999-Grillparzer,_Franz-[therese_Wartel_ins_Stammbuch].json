{"textgrid.poem.44999": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[therese Wartel ins Stammbuch]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Wort, ein Mann;", "tokens": ["Ein", "Wort", ",", "ein", "Mann", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ein Blick, ein Weib!", "tokens": ["Ein", "Blick", ",", "ein", "Weib", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wer dich gesehn, er glaubt dich schon zu h\u00f6ren,", "tokens": ["Wer", "dich", "ge\u00b7sehn", ",", "er", "glaubt", "dich", "schon", "zu", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "PPER", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wer dich geh\u00f6rt, versteht erst, was er sah.", "tokens": ["Wer", "dich", "ge\u00b7h\u00f6rt", ",", "ver\u00b7steht", "erst", ",", "was", "er", "sah", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}