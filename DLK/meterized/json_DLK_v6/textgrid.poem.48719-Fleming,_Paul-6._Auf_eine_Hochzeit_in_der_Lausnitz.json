{"textgrid.poem.48719": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "6. Auf eine Hochzeit in der Lausnitz", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und ihr wollt auch an das, ihr wolgepaarten Herzen,", "tokens": ["Und", "ihr", "wollt", "auch", "an", "das", ",", "ihr", "wol\u00b7ge\u00b7paar\u00b7ten", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "APPR", "PDS", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "was uns den Mund voll Na\u00df, den Sinn voll Sehnen macht,", "tokens": ["was", "uns", "den", "Mund", "voll", "Na\u00df", ",", "den", "Sinn", "voll", "Seh\u00b7nen", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "ADJD", "NN", "$,", "ART", "NN", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "voll Seufzen manchen Tag, voll Tr\u00e4ume manche Nacht,", "tokens": ["voll", "Seuf\u00b7zen", "man\u00b7chen", "Tag", ",", "voll", "Tr\u00e4u\u00b7me", "man\u00b7che", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PIAT", "NN", "$,", "ADJD", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "was uns die Seele qu\u00e4lt mit angenehmen Schmerzen", "tokens": ["was", "uns", "die", "See\u00b7le", "qu\u00e4lt", "mit", "an\u00b7ge\u00b7neh\u00b7men", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "und s\u00fc\u00dfer Bitterkeit. Die liebbeflammten Kerzen", "tokens": ["und", "s\u00fc\u00b7\u00dfer", "Bit\u00b7ter\u00b7keit", ".", "Die", "lieb\u00b7be\u00b7flamm\u00b7ten", "Ker\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "tr\u00e4gt euch Kupido vor. Ihr habts dahin gebracht,", "tokens": ["tr\u00e4gt", "euch", "Ku\u00b7pi\u00b7do", "vor", ".", "Ihr", "habts", "da\u00b7hin", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PTKVZ", "$.", "PPER", "VAFIN", "PAV", "VVPP", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "da\u00df ihr in sanfter Lust beisammen schlaft und wacht", "tokens": ["da\u00df", "ihr", "in", "sanf\u00b7ter", "Lust", "bei\u00b7sam\u00b7men", "schlaft", "und", "wacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und euren Lebensrest verschlei\u00dft mit lieben Scherzen.", "tokens": ["und", "eu\u00b7ren", "Le\u00b7bens\u00b7rest", "ver\u00b7schlei\u00dft", "mit", "lie\u00b7ben", "Scher\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wir, die wir noch zur Zeit des Tuns entbehren m\u00fcssen", "tokens": ["Wir", ",", "die", "wir", "noch", "zur", "Zeit", "des", "Tuns", "ent\u00b7beh\u00b7ren", "m\u00fcs\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "VVINF", "VMINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und nicht so viel, wie ihr, von Lieb' und Lieben wissen,", "tokens": ["und", "nicht", "so", "viel", ",", "wie", "ihr", ",", "von", "Lieb'", "und", "Lie\u00b7ben", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ADV", "$,", "PWAV", "PPER", "$,", "APPR", "NN", "KON", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "bew\u00fcndschen euch mit Gl\u00fcck und himmelreicher Gunst.", "tokens": ["be\u00b7w\u00fcnd\u00b7schen", "euch", "mit", "Gl\u00fcck", "und", "him\u00b7mel\u00b7rei\u00b7cher", "Gunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und ihr, wenn ihr nun k\u00f6nnt den s\u00fc\u00dfen Schmack empfinden,", "tokens": ["Und", "ihr", ",", "wenn", "ihr", "nun", "k\u00f6nnt", "den", "s\u00fc\u00b7\u00dfen", "Schmack", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "KOUS", "PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "so w\u00fcndscht, wie wir euch, uns, die wir noch stehn dahinden,", "tokens": ["so", "w\u00fcnd\u00b7scht", ",", "wie", "wir", "euch", ",", "uns", ",", "die", "wir", "noch", "stehn", "da\u00b7hin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "PPER", "PPER", "$,", "PPER", "$,", "PRELS", "PPER", "ADV", "VVINF", "VVINF", "$,"], "meter": "--+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "wie ihr sie itzund habt, so eine gleiche Brunst.", "tokens": ["wie", "ihr", "sie", "it\u00b7zund", "habt", ",", "so", "ei\u00b7ne", "glei\u00b7che", "Brunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "VAFIN", "$,", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}