{"textgrid.poem.27561": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Vorsichtige", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch halt ich mein Urteil zur\u00fcck, das ist das bequemste;", "tokens": ["Noch", "halt", "ich", "mein", "Ur\u00b7teil", "zu\u00b7r\u00fcck", ",", "das", "ist", "das", "be\u00b7quems\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,", "PDS", "VAFIN", "ART", "ADJA", "$."], "meter": "-+--+--+-++-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "L\u00f6st sich das R\u00e4tsel einmal, bin ich wie alle gescheit.", "tokens": ["L\u00f6st", "sich", "das", "R\u00e4t\u00b7sel", "ein\u00b7mal", ",", "bin", "ich", "wie", "al\u00b7le", "ge\u00b7scheit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADV", "$,", "VAFIN", "PPER", "KOKOM", "PIAT", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}}}}