{"textgrid.poem.25344": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die kupferrote Sonne im Versinken", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die kupferrote Sonne im Versinken", "tokens": ["Die", "kup\u00b7fer\u00b7ro\u00b7te", "Son\u00b7ne", "im", "Ver\u00b7sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "H\u00e4ngt zwischen H\u00f6hlen scharf gezackter Zweige", "tokens": ["H\u00e4ngt", "zwi\u00b7schen", "H\u00f6h\u00b7len", "scharf", "ge\u00b7zack\u00b7ter", "Zwei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "ADJD", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "In harter Glut der strahlenlosen Neige,", "tokens": ["In", "har\u00b7ter", "Glut", "der", "strah\u00b7len\u00b7lo\u00b7sen", "Nei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die feuchte Luft scheint allen Glanz zu trinken.", "tokens": ["Die", "feuch\u00b7te", "Luft", "scheint", "al\u00b7len", "Glanz", "zu", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die grauen Wolken, aufgeschwellt von Regen,", "tokens": ["Die", "grau\u00b7en", "Wol\u00b7ken", ",", "auf\u00b7ge\u00b7schwellt", "von", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit langen Schleppen, die am Boden schleifen,", "tokens": ["Mit", "lan\u00b7gen", "Schlep\u00b7pen", ",", "die", "am", "Bo\u00b7den", "schlei\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und lau umstr\u00f6mt von schwachen Lilastreifen,", "tokens": ["Und", "lau", "um\u00b7str\u00f6mt", "von", "schwa\u00b7chen", "Li\u00b7last\u00b7rei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ergie\u00dfen d\u00fcnnes Licht auf allen Wegen.", "tokens": ["Er\u00b7gie\u00b7\u00dfen", "d\u00fcn\u00b7nes", "Licht", "auf", "al\u00b7len", "We\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nur in der B\u00e4ume enggedr\u00e4ngten Gruppen,", "tokens": ["Nur", "in", "der", "B\u00e4u\u00b7me", "eng\u00b7ge\u00b7dr\u00e4ng\u00b7ten", "Grup\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die steil wie Inseln aus den gr\u00fcnen Matten", "tokens": ["Die", "steil", "wie", "In\u00b7seln", "aus", "den", "gr\u00fc\u00b7nen", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOKOM", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Parkes steigen, lagern dichtre Schatten,", "tokens": ["Des", "Par\u00b7kes", "stei\u00b7gen", ",", "la\u00b7gern", "dicht\u00b7re", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hinsinkend von den braunen H\u00fcgelkuppen.", "tokens": ["Hin\u00b7sin\u00b7kend", "von", "den", "brau\u00b7nen", "H\u00fc\u00b7gel\u00b7kup\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}