{"dta.poem.16049": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "103.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auch dieses biet' ich dir, o Herr, zum Opfer an,", "tokens": ["Auch", "die\u00b7ses", "biet'", "ich", "dir", ",", "o", "Herr", ",", "zum", "Op\u00b7fer", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "PPER", "PPER", "$,", "FM", "NN", "$,", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was, wenn dus forderst, ich ja nicht verweigern kan.", "tokens": ["Was", ",", "wenn", "dus", "for\u00b7derst", ",", "ich", "ja", "nicht", "ver\u00b7wei\u00b7gern", "kan", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "KOUS", "PIS", "VVFIN", "$,", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Allein verschweigen kann ichs weder mir noch dir:", "tokens": ["Al\u00b7lein", "ver\u00b7schwei\u00b7gen", "kann", "ichs", "we\u00b7der", "mir", "noch", "dir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "PIS", "KON", "PPER", "ADV", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nimm die Willf\u00e4hrigkeit, und spar das Opfer mir!", "tokens": ["Nimm", "die", "Will\u00b7f\u00e4h\u00b7rig\u00b7keit", ",", "und", "spar", "das", "Op\u00b7fer", "mir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "