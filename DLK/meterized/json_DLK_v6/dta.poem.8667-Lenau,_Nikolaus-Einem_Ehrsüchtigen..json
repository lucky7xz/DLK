{"dta.poem.8667": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Einem Ehrs\u00fcchtigen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df das Ringen nach der Ehre;", "tokens": ["La\u00df", "das", "Rin\u00b7gen", "nach", "der", "Eh\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieber all' dein hei\u00dfes Streben", "tokens": ["Lie\u00b7ber", "all'", "dein", "hei\u00b7\u00dfes", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "PIAT", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In den eignen Busen kehre,", "tokens": ["In", "den", "eig\u00b7nen", "Bu\u00b7sen", "keh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und du lebst ein sch\u00f6n'res Leben.", "tokens": ["Und", "du", "lebst", "ein", "sch\u00f6n'\u00b7res", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}