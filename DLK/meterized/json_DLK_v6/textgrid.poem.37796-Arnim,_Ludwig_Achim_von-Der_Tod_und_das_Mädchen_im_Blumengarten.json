ord"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Warf sie sich auf der Erd,", "tokens": ["Warf", "sie", "sich", "auf", "der", "Erd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "All Wollust ihr vergangen war,", "tokens": ["All", "Wol\u00b7lust", "ihr", "ver\u00b7gan\u00b7gen", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kein Bl\u00fcmlein mehr wollt holen", "tokens": ["Kein", "Bl\u00fcm\u00b7lein", "mehr", "wollt", "ho\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wohl aus dem gr\u00fcnen Gra\u00df.", "tokens": ["Wohl", "aus", "dem", "gr\u00fc\u00b7nen", "Gra\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}