{"textgrid.poem.37796": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Tod und das M\u00e4dchen im Blumengarten", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ging ein M\u00e4gdlein zarte", "tokens": ["Es", "ging", "ein", "M\u00e4gd\u00b7lein", "zar\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fr\u00fch in der Morgenstund", "tokens": ["Fr\u00fch", "in", "der", "Mor\u00b7gen\u00b7stund"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "In einen Blumengarten,", "tokens": ["In", "ei\u00b7nen", "Blu\u00b7men\u00b7gar\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Frisch, fr\u00f6hlich und gesund,", "tokens": ["Frisch", ",", "fr\u00f6h\u00b7lich", "und", "ge\u00b7sund", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Bl\u00fcmlein es viel brechen wollt,", "tokens": ["Der", "Bl\u00fcm\u00b7lein", "es", "viel", "bre\u00b7chen", "wollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Daraus ein Kranz zu machen,", "tokens": ["Da\u00b7raus", "ein", "Kranz", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Von Silber und von Gold.", "tokens": ["Von", "Sil\u00b7ber", "und", "von", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da kam herzu geschlichen", "tokens": ["Da", "kam", "her\u00b7zu", "ge\u00b7schli\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein gar erschrecklich Mann,", "tokens": ["Ein", "gar", "er\u00b7schreck\u00b7lich", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Farb war ihm verblichen,", "tokens": ["Die", "Farb", "war", "ihm", "ver\u00b7bli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein' Kleider hatt' er an,", "tokens": ["Kein'", "Klei\u00b7der", "hatt'", "er", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er hatt' kein Fleisch, kein Blut, kein Haar,", "tokens": ["Er", "hatt'", "kein", "Fleisch", ",", "kein", "Blut", ",", "kein", "Haar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es war an ihm verdorret", "tokens": ["Es", "war", "an", "ihm", "ver\u00b7dor\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sein Haut, und Flechsen gar.", "tokens": ["Sein", "Haut", ",", "und", "Flech\u00b7sen", "gar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gar h\u00e4\u00dflich th\u00e4t er sehen,", "tokens": ["Gar", "h\u00e4\u00df\u00b7lich", "th\u00e4t", "er", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Scheu\u00dflich war sein Gesicht,", "tokens": ["Scheu\u00df\u00b7lich", "war", "sein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Er weiset seine Z\u00e4hne", "tokens": ["Er", "wei\u00b7set", "sei\u00b7ne", "Z\u00e4h\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und that noch einen Schritt,", "tokens": ["Und", "that", "noch", "ei\u00b7nen", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wohl zu dem M\u00e4gdlein zart,", "tokens": ["Wohl", "zu", "dem", "M\u00e4gd\u00b7lein", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das schier f\u00fcr gro\u00dfen Aengsten,", "tokens": ["Das", "schier", "f\u00fcr", "gro\u00b7\u00dfen", "A\u00b7engs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Des grimmen Todes ward.", "tokens": ["Des", "grim\u00b7men", "To\u00b7des", "ward", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbnun schick dich M\u00e4gdlein, schick dich,", "tokens": ["\u00bb", "nun", "schick", "dich", "M\u00e4gd\u00b7lein", ",", "schick", "dich", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "VVIMP", "PPER", "NN", "$,", "VVIMP", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du must mit mir an Tanz!", "tokens": ["Du", "must", "mit", "mir", "an", "Tanz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich will dir bald aufsetzen,", "tokens": ["Ich", "will", "dir", "bald", "auf\u00b7set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein wundersch\u00f6nen Kranz,", "tokens": ["Ein", "wun\u00b7der\u00b7sch\u00f6\u00b7nen", "Kranz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der wird dir nicht gebunden sein", "tokens": ["Der", "wird", "dir", "nicht", "ge\u00b7bun\u00b7den", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "PTKNEG", "VVPP", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von wohlriechenden Kr\u00e4utern,", "tokens": ["Von", "wohl\u00b7rie\u00b7chen\u00b7den", "Kr\u00e4u\u00b7tern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Und zarten Bl\u00fcmelein.", "tokens": ["Und", "zar\u00b7ten", "Bl\u00fc\u00b7me\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Kranz, den ich aufsetze,", "tokens": ["Der", "Kranz", ",", "den", "ich", "auf\u00b7set\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der hei\u00dft die Sterblichkeit;", "tokens": ["Der", "hei\u00dft", "die", "Sterb\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du wirst nicht seyn die letzte,", "tokens": ["Du", "wirst", "nicht", "seyn", "die", "letz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "VAINF", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die ihn tr\u00e4gt auf dem Haupt;", "tokens": ["Die", "ihn", "tr\u00e4gt", "auf", "dem", "Haupt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Wie viel allhie gebohren seyn,", "tokens": ["Wie", "viel", "all\u00b7hie", "ge\u00b7boh\u00b7ren", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die m\u00fcssen mit mir tanzen", "tokens": ["Die", "m\u00fcs\u00b7sen", "mit", "mir", "tan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wohl um das Kr\u00e4nzelein.", "tokens": ["Wohl", "um", "das", "Kr\u00e4n\u00b7zel\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der W\u00fcrmer in der Erde", "tokens": ["Der", "W\u00fcr\u00b7mer", "in", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist eine gro\u00dfe Zahl,", "tokens": ["Ist", "ei\u00b7ne", "gro\u00b7\u00dfe", "Zahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die werden dir verzehren", "tokens": ["Die", "wer\u00b7den", "dir", "ver\u00b7zeh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein Sch\u00f6nheit allzumahl,", "tokens": ["Dein", "Sch\u00f6n\u00b7heit", "all\u00b7zu\u00b7mahl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie werden deine Bl\u00fcmlein seyn,", "tokens": ["Sie", "wer\u00b7den", "dei\u00b7ne", "Bl\u00fcm\u00b7lein", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Gold, und auch die Perlen,", "tokens": ["Das", "Gold", ",", "und", "auch", "die", "Per\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Silber und Edelstein.", "tokens": ["Sil\u00b7ber", "und", "E\u00b7del\u00b7stein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "Willst du mich gerne kennen", "tokens": ["Willst", "du", "mich", "ger\u00b7ne", "ken\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wissen, wer ich sey?", "tokens": ["Und", "wis\u00b7sen", ",", "wer", "ich", "sey", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So h\u00f6r mein Nahmen nennen,", "tokens": ["So", "h\u00f6r", "mein", "Nah\u00b7men", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.4": {"text": "Will dir ihn sagen frey:", "tokens": ["Will", "dir", "ihn", "sa\u00b7gen", "frey", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der grimme Tod werd ich genannt,", "tokens": ["Der", "grim\u00b7me", "Tod", "werd", "ich", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und bin in allen Landen,", "tokens": ["Und", "bin", "in", "al\u00b7len", "Lan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Gar weit und breit bekannt.", "tokens": ["Gar", "weit", "und", "breit", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Sense ist mein Wappen,", "tokens": ["Die", "Sen\u00b7se", "ist", "mein", "Wap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das ich mit Rechte f\u00fchr,", "tokens": ["Das", "ich", "mit", "Rech\u00b7te", "f\u00fchr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Damit thu ich anklopfen", "tokens": ["Da\u00b7mit", "thu", "ich", "an\u00b7klop\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Jedem an seine Th\u00fcr,", "tokens": ["Je\u00b7dem", "an", "sei\u00b7ne", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Und wenn sein Zeit ist kommen schon,", "tokens": ["Und", "wenn", "sein", "Zeit", "ist", "kom\u00b7men", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "VAFIN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sp\u00e4t, fr\u00fch, und in der Mitten,", "tokens": ["Sp\u00e4t", ",", "fr\u00fch", ",", "und", "in", "der", "Mit\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "'s hilft nichts, er mu\u00df davon!\u00ab", "tokens": ["'s", "hilft", "nichts", ",", "er", "mu\u00df", "da\u00b7von", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "PPER", "VMFIN", "PAV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Das M\u00e4gdlein voller Schmerzen,", "tokens": ["Das", "M\u00e4gd\u00b7lein", "vol\u00b7ler", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Voll bittrer Angst und Noth,", "tokens": ["Voll", "bit\u00b7trer", "Angst", "und", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bek\u00fcmmert tief im Herzen,", "tokens": ["Be\u00b7k\u00fcm\u00b7mert", "tief", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bat: \u00bbAch du lieber Todt,", "tokens": ["Bat", ":", "\u00bb", "Ach", "du", "lie\u00b7ber", "Todt", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "$(", "ITJ", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wollst eilen nicht so sehr mit mir,", "tokens": ["Wollst", "ei\u00b7len", "nicht", "so", "sehr", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "ADV", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mich armes M\u00e4gdlein zarte", "tokens": ["Mich", "ar\u00b7mes", "M\u00e4gd\u00b7lein", "zar\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "La\u00df l\u00e4nger leben hier!", "tokens": ["La\u00df", "l\u00e4n\u00b7ger", "le\u00b7ben", "hier", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ich will dich reich begaben,", "tokens": ["Ich", "will", "dich", "reich", "be\u00b7ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Vater hat viel Gold,", "tokens": ["Mein", "Va\u00b7ter", "hat", "viel", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und was du nur willst haben", "tokens": ["Und", "was", "du", "nur", "willst", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "ADV", "VMFIN", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das all du nehmen sollt!", "tokens": ["Das", "all", "du", "neh\u00b7men", "sollt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nur lasse du, das Leben mir,", "tokens": ["Nur", "las\u00b7se", "du", ",", "das", "Le\u00b7ben", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein allerbeste Sch\u00e4tze,", "tokens": ["Mein", "al\u00b7ler\u00b7bes\u00b7te", "Sch\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die will ich geben dir!\u00ab", "tokens": ["Die", "will", "ich", "ge\u00b7ben", "dir", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "\u00bbkein Schatz sollt du mir geben,", "tokens": ["\u00bb", "kein", "Schatz", "sollt", "du", "mir", "ge\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kein Gold noch Edelstein!", "tokens": ["Kein", "Gold", "noch", "E\u00b7del\u00b7stein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich nehm dir nur das Leben,", "tokens": ["Ich", "nehm", "dir", "nur", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du zartes M\u00e4gdelein,", "tokens": ["Du", "zar\u00b7tes", "M\u00e4g\u00b7del\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du must mit mir an meinen Tanz,", "tokens": ["Du", "must", "mit", "mir", "an", "mei\u00b7nen", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Daran noch kommt manch Tausend,", "tokens": ["Da\u00b7ran", "noch", "kommt", "manch", "Tau\u00b7send", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bis da\u00df der Reihn wird ganz.\u00ab", "tokens": ["Bis", "da\u00df", "der", "Reihn", "wird", "ganz", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "VAFIN", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "\u00bbo Tod, la\u00df mich beim Leben,", "tokens": ["\u00bb", "o", "Tod", ",", "la\u00df", "mich", "beim", "Le\u00b7ben", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "VVIMP", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nimm all mein Hausgesind!", "tokens": ["Nimm", "all", "mein", "Haus\u00b7ge\u00b7sind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Vater wird dirs geben,", "tokens": ["Mein", "Va\u00b7ter", "wird", "dirs", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn er mich lebend findt,", "tokens": ["Wenn", "er", "mich", "le\u00b7bend", "findt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich bin sein einzigs T\u00f6chterlein,", "tokens": ["Ich", "bin", "sein", "ein\u00b7zigs", "T\u00f6ch\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er w\u00fcrde mich nicht geben", "tokens": ["Er", "w\u00fcr\u00b7de", "mich", "nicht", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Um tausend Gulden fein.\u00ab", "tokens": ["Um", "tau\u00b7send", "Gul\u00b7den", "fein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "CARD", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "\u00bbdein Vater will ich holen", "tokens": ["\u00bb", "dein", "Va\u00b7ter", "will", "ich", "ho\u00b7len"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und will ihn finden wohl,", "tokens": ["Und", "will", "ihn", "fin\u00b7den", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit seinem Hausgesinde,", "tokens": ["Mit", "sei\u00b7nem", "Haus\u00b7ge\u00b7sin\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wei\u00df, wenn ich kommen soll,", "tokens": ["Wei\u00df", ",", "wenn", "ich", "kom\u00b7men", "soll", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Jetzund nehm ich nur dich allein:", "tokens": ["Je\u00b7tzund", "nehm", "ich", "nur", "dich", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O zartes M\u00e4gdlein junge,", "tokens": ["O", "zar\u00b7tes", "M\u00e4gd\u00b7lein", "jun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Du must an meinen Reihen.\u00ab", "tokens": ["Du", "must", "an", "mei\u00b7nen", "Rei\u00b7hen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "\u00bberbarm dich meiner Jugend,\u00ab", "tokens": ["\u00bb", "er\u00b7barm", "dich", "mei\u00b7ner", "Ju\u00b7gend", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "PPER", "PPOSAT", "NN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sprach sie mit gro\u00dfer Klag,", "tokens": ["Sprach", "sie", "mit", "gro\u00b7\u00dfer", "Klag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbwill mich in aller Tugend,", "tokens": ["\u00bb", "will", "mich", "in", "al\u00b7ler", "Tu\u00b7gend", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ueben mein Lebetag.", "tokens": ["Ue\u00b7ben", "mein", "Le\u00b7be\u00b7tag", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Nimm mich nicht gleich dahin jetzund,", "tokens": ["Nimm", "mich", "nicht", "gleich", "da\u00b7hin", "je\u00b7tzund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ADV", "PAV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Spar mich noch eine Weile,", "tokens": ["Spar", "mich", "noch", "ei\u00b7ne", "Wei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Schon mich noch etlich' Stund!\u00ab", "tokens": ["Schon", "mich", "noch", "et\u00b7lich'", "Stund", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPER", "ADV", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Drauf sprach der Tod: \u00bbMit nichten,", "tokens": ["Drauf", "sprach", "der", "Tod", ":", "\u00bb", "Mit", "nich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$.", "$(", "APPR", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich kehr mich nicht daran,", "tokens": ["Ich", "kehr", "mich", "nicht", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es hilft allhier kein Bitten,", "tokens": ["Es", "hilft", "all\u00b7hier", "kein", "Bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich nehme Frau und Mann!", "tokens": ["Ich", "neh\u00b7me", "Frau", "und", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Kinderlein zieh ich herf\u00fcr,", "tokens": ["Die", "Kin\u00b7der\u00b7lein", "zieh", "ich", "her\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein jedes mu\u00df mir folgen,", "tokens": ["Ein", "je\u00b7des", "mu\u00df", "mir", "fol\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wenn ich klopf an die Th\u00fcr.\u00ab", "tokens": ["Wenn", "ich", "klopf", "an", "die", "Th\u00fcr.", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "abbreviation", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "--+--", "measure": "anapaest.init"}}, "stanza.16": {"line.1": {"text": "Er nahm sie in der Mitten,", "tokens": ["Er", "nahm", "sie", "in", "der", "Mit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da sie am schw\u00e4chsten was,", "tokens": ["Da", "sie", "am", "schw\u00e4chs\u00b7ten", "was", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "PWS", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es half bey ihm kein Bitten,", "tokens": ["Es", "half", "bey", "ihm", "kein", "Bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", 