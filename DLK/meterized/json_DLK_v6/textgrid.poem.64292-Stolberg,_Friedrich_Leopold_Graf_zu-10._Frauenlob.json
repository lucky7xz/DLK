{"textgrid.poem.64292": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "10. Frauenlob", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Traun, der Mann ist neideswert,", "tokens": ["Traun", ",", "der", "Mann", "ist", "nei\u00b7des\u00b7wert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VAFIN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Welchem Gott ein Weib beschert,", "tokens": ["Wel\u00b7chem", "Gott", "ein", "Weib", "be\u00b7schert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6n und klug und tugendreich,", "tokens": ["Sch\u00f6n", "und", "klug", "und", "tu\u00b7gen\u00b7dreich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sonder Falsch, den Tauben gleich!", "tokens": ["Son\u00b7der", "Falsch", ",", "den", "Tau\u00b7ben", "gleich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Seiner Wonne Ma\u00df ist gro\u00df,", "tokens": ["Sei\u00b7ner", "Won\u00b7ne", "Ma\u00df", "ist", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine Ruhe wechsellos;", "tokens": ["Sei\u00b7ne", "Ru\u00b7he", "wech\u00b7sel\u00b7los", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn kein Kummer nagt den Mann,", "tokens": ["Denn", "kein", "Kum\u00b7mer", "nagt", "den", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Den solch Weiblein tr\u00f6sten kann.", "tokens": ["Den", "solch", "Weib\u00b7lein", "tr\u00f6s\u00b7ten", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Gleich des Mondes Silberblick,", "tokens": ["Gleich", "des", "Mon\u00b7des", "Sil\u00b7berb\u00b7lick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4chelt sie den Gram zur\u00fcck,", "tokens": ["L\u00e4\u00b7chelt", "sie", "den", "Gram", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00fc\u00dft des Mannes Thr\u00e4nen auf,", "tokens": ["K\u00fc\u00dft", "des", "Man\u00b7nes", "Thr\u00e4\u00b7nen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deckt mit Blumen seinen Lauf!", "tokens": ["Deckt", "mit", "Blu\u00b7men", "sei\u00b7nen", "Lauf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn ihn j\u00e4her Mut emp\u00f6rt,", "tokens": ["Wenn", "ihn", "j\u00e4\u00b7her", "Mut", "em\u00b7p\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Er nicht mehr des Freundes h\u00f6rt,", "tokens": ["Er", "nicht", "mehr", "des", "Freun\u00b7des", "h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn von Zorn die Brust ihm gl\u00fcht,", "tokens": ["Wenn", "von", "Zorn", "die", "Brust", "ihm", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sein Auge Feuer spr\u00fcht;", "tokens": ["Und", "sein", "Au\u00b7ge", "Feu\u00b7er", "spr\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "O! dann schleicht sie weinend nach,", "tokens": ["O", "!", "dann", "schleicht", "sie", "wei\u00b7nend", "nach", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00e4nftigt ihn mit einem Ach!", "tokens": ["S\u00e4nf\u00b7tigt", "ihn", "mit", "ei\u00b7nem", "Ach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Also k\u00fchlt der Abendtau", "tokens": ["Al\u00b7so", "k\u00fchlt", "der", "A\u00b7bend\u00b7tau"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die versengte Blumenau.", "tokens": ["Die", "ver\u00b7seng\u00b7te", "Blu\u00b7me\u00b7nau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Keine M\u00fche wird ihm schwer,", "tokens": ["Kei\u00b7ne", "M\u00fc\u00b7he", "wird", "ihm", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Keine Stunde freudenleer;", "tokens": ["Kei\u00b7ne", "Stun\u00b7de", "freu\u00b7den\u00b7leer", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn nach jeder Arbeit Last", "tokens": ["Denn", "nach", "je\u00b7der", "Ar\u00b7beit", "Last"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Harret sein die s\u00fc\u00dfe Rast.", "tokens": ["Har\u00b7ret", "sein", "die", "s\u00fc\u00b7\u00dfe", "Rast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Engel f\u00f6rdern ihre Ruh,", "tokens": ["En\u00b7gel", "f\u00f6r\u00b7dern", "ih\u00b7re", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dr\u00fccken beider Augen zu;", "tokens": ["Dr\u00fc\u00b7cken", "bei\u00b7der", "Au\u00b7gen", "zu", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihrer keuschen Ehe Band", "tokens": ["Ih\u00b7rer", "keu\u00b7schen", "E\u00b7he", "Band"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kn\u00fcpfte Gottes Vaterhand!", "tokens": ["Kn\u00fcpf\u00b7te", "Got\u00b7tes", "Va\u00b7ter\u00b7hand", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Gott giebt ihren S\u00f6hnen Mut,", "tokens": ["Gott", "giebt", "ih\u00b7ren", "S\u00f6h\u00b7nen", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcr die Tugend reges Blut,", "tokens": ["F\u00fcr", "die", "Tu\u00b7gend", "re\u00b7ges", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "St\u00e4rket ihren jungen Arm", "tokens": ["St\u00e4r\u00b7ket", "ih\u00b7ren", "jun\u00b7gen", "Arm"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Macht ihr Herz f\u00fcr Freiheit warm.", "tokens": ["Macht", "ihr", "Herz", "f\u00fcr", "Frei\u00b7heit", "warm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Mit versch\u00e4mten Reizen bl\u00fchn", "tokens": ["Mit", "ver\u00b7sch\u00e4m\u00b7ten", "Rei\u00b7zen", "bl\u00fchn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihres Bettes T\u00f6chter, gl\u00fchn", "tokens": ["Ih\u00b7res", "Bet\u00b7tes", "T\u00f6ch\u00b7ter", ",", "gl\u00fchn"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit der Mutter Unschuld, rein", "tokens": ["Mit", "der", "Mut\u00b7ter", "Un\u00b7schuld", ",", "rein"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "NN", "$,", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein Quell im Sonnenschein.", "tokens": ["Wie", "ein", "Quell", "im", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Drob erfreut der Vater sich,", "tokens": ["Drob", "er\u00b7freut", "der", "Va\u00b7ter", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Drob die Mutter inniglich.", "tokens": ["Drob", "die", "Mut\u00b7ter", "in\u00b7nig\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihr vereintes Dankgebet", "tokens": ["Ihr", "ver\u00b7ein\u00b7tes", "Dank\u00b7ge\u00b7bet"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Preist den Geber fr\u00fch und sp\u00e4t!", "tokens": ["Preist", "den", "Ge\u00b7ber", "fr\u00fch", "und", "sp\u00e4t", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Gold hat keinen noch begl\u00fcckt;", "tokens": ["Gold", "hat", "kei\u00b7nen", "noch", "be\u00b7gl\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Falscher Ehre Lorbeer dr\u00fcckt;", "tokens": ["Fal\u00b7scher", "Eh\u00b7re", "Lor\u00b7beer", "dr\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer nach W\u00fcrden hascht greift Sand;", "tokens": ["Wer", "nach", "W\u00fcr\u00b7den", "hascht", "greift", "Sand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wissenschaft ist oft ein Tand.", "tokens": ["Wis\u00b7sen\u00b7schaft", "ist", "oft", "ein", "Tand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Aber Weiber giebt uns Gott!", "tokens": ["A\u00b7ber", "Wei\u00b7ber", "giebt", "uns", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne sie ist Leben Tod!", "tokens": ["Oh\u00b7ne", "sie", "ist", "Le\u00b7ben", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weiber leichten jedes Joch,", "tokens": ["Wei\u00b7ber", "leich\u00b7ten", "je\u00b7des", "Joch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieben uns im Himmel noch!", "tokens": ["Lie\u00b7ben", "uns", "im", "Him\u00b7mel", "noch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "PPER", "APPRART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}