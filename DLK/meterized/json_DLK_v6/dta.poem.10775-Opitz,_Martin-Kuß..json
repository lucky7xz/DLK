{"dta.poem.10775": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Ku\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Einig s\u00fc\u00dfes M\u00fcndelein/", "tokens": ["Ei\u00b7nig", "s\u00fc\u00b7\u00dfes", "M\u00fcn\u00b7del\u00b7ein", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "R\u00f6ter den ein R\u00f6selein/", "tokens": ["R\u00f6\u00b7ter", "den", "ein", "R\u00f6\u00b7se\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So ", "tokens": ["So"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Macht auffgehen.", "tokens": ["Macht", "auff\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVIZU", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Lefftzen vbertreffend weit", "tokens": ["Lefft\u00b7zen", "vbert\u00b7ref\u00b7fend", "weit"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVPP", "ADJD"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Den Taw/ so die Erden netzet/", "tokens": ["Den", "Taw", "/", "so", "die", "Er\u00b7den", "net\u00b7zet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Vnd mit Fruchtbarkeit ergetzet/", "tokens": ["Vnd", "mit", "Frucht\u00b7bar\u00b7keit", "er\u00b7get\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In der s\u00fcssen Fr\u00fclings zeit.", "tokens": ["In", "der", "s\u00fcs\u00b7sen", "Fr\u00fc\u00b7lings", "zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Holdseeliges Sch\u00e4tzelein/", "tokens": ["Hold\u00b7see\u00b7li\u00b7ges", "Sch\u00e4t\u00b7zel\u00b7ein", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gib mir so vil Schm\u00e4tzelein/", "tokens": ["Gib", "mir", "so", "vil", "Schm\u00e4t\u00b7zel\u00b7ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PIAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So vil du gibst meinem Hertzen", "tokens": ["So", "vil", "du", "gibst", "mei\u00b7nem", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pein vnd Schmertzen.", "tokens": ["Pein", "vnd", "Schmert\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Soviel Pfeil der fliegend Gott", "tokens": ["So\u00b7viel", "Pfeil", "der", "flie\u00b7gend", "Gott"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ART", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wider mein Hertz abgeschossen/", "tokens": ["Wi\u00b7der", "mein", "Hertz", "ab\u00b7ge\u00b7schos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "So vil ich leid vnverdrossen", "tokens": ["So", "vil", "ich", "leid", "vn\u00b7ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "ADJD", "VVINF"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.8": {"text": "Jammer/ Tr\u00fcbsal/ Angst vnd Noth:", "tokens": ["Jam\u00b7mer", "/", "Tr\u00fcb\u00b7sal", "/", "Angst", "vnd", "Noth", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "So vil man wohl k\u00f6rnlin Sands", "tokens": ["So", "vil", "man", "wohl", "k\u00f6rn\u00b7lin", "Sands"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIS", "ADV", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Am vsser de\u00df Morenlands", "tokens": ["Am", "vs\u00b7ser", "de\u00df", "Mo\u00b7ren\u00b7lands"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So vil Gra\u00df im Felde stehen", "tokens": ["So", "vil", "Gra\u00df", "im", "Fel\u00b7de", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "APPRART", "NN", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Man kan sehen:", "tokens": ["Man", "kan", "se\u00b7hen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "So vil tropffen in dem Meer/", "tokens": ["So", "vil", "tropf\u00b7fen", "in", "dem", "Meer", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "So vil Fisch die Fl\u00fcsse bringen/", "tokens": ["So", "vil", "Fisch", "die", "Fl\u00fcs\u00b7se", "brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "V\u00f6gel durch den Lufft sich schwingen/", "tokens": ["V\u00f6\u00b7gel", "durch", "den", "Lufft", "sich", "schwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PRF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Vnd so vil der Herbst Weinbeer:", "tokens": ["Vnd", "so", "vil", "der", "Herbst", "Wein\u00b7beer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-++-", "measure": "unknown.measure.tetra"}}, "stanza.4": {"line.1": {"text": "So vil sch\u00f6ne lieblichkeit/", "tokens": ["So", "vil", "sch\u00f6\u00b7ne", "lieb\u00b7lich\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schmollende Holdtseeligkeit/", "tokens": ["Schmol\u00b7len\u00b7de", "Holdt\u00b7see\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+--++-+", "measure": "dactylic.init"}, "line.3": {"text": "So vil H\u00f6fflichkeit vnd Lachen", "tokens": ["So", "vil", "H\u00f6ff\u00b7lich\u00b7keit", "vnd", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieblich machen", "tokens": ["Lieb\u00b7lich", "ma\u00b7chen"], "token_info": ["word", "word"], "pos": ["ADJD", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Deinen tewren Purpurmundt/", "tokens": ["Dei\u00b7nen", "tew\u00b7ren", "Pur\u00b7pur\u00b7mundt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie vil Rosen deine Wangen/", "tokens": ["Wie", "vil", "Ro\u00b7sen", "dei\u00b7ne", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie vil Gilgen machen prangen", "tokens": ["Wie", "vil", "Gil\u00b7gen", "ma\u00b7chen", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "VVINF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Deinen Busen steiff vnd rundt/", "tokens": ["Dei\u00b7nen", "Bu\u00b7sen", "steiff", "vnd", "rundt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "So offt k\u00fc\u00df mich Nimselein/", "tokens": ["So", "offt", "k\u00fc\u00df", "mich", "Nim\u00b7sel\u00b7ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "NE", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So osst schmatz mich Schimpfelein/", "tokens": ["So", "osst", "schmatz", "mich", "Schimp\u00b7fe\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPER", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "La\u00df vns mit einander schertzen/", "tokens": ["La\u00df", "vns", "mit", "ein\u00b7an\u00b7der", "schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PRF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vnd vns hertzen.", "tokens": ["Vnd", "vns", "hert\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Bi\u00df ich sag/ mein Fried/ mein Frewd", "tokens": ["Bi\u00df", "ich", "sag", "/", "mein", "Fried", "/", "mein", "Frewd"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$(", "PPOSAT", "NN", "$(", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich kan nicht mehr/ la\u00df mich gehen", "tokens": ["Ich", "kan", "nicht", "mehr", "/", "la\u00df", "mich", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "$(", "VVIMP", "PPER", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "So soltu ein weil abstehen/", "tokens": ["So", "sol\u00b7tu", "ein", "weil", "ab\u00b7ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "KOUS", "VVINF", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Da\u00df ich seusstzend halb verscheid.", "tokens": ["Da\u00df", "ich", "seusst\u00b7zend", "halb", "ver\u00b7scheid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Darnach k\u00fc\u00df mich widerumb/", "tokens": ["Dar\u00b7nach", "k\u00fc\u00df", "mich", "wi\u00b7de\u00b7rumb", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df noch gr\u00f6sser werd die Summ/", "tokens": ["Da\u00df", "noch", "gr\u00f6s\u00b7ser", "werd", "die", "Summ", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "VAFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "St\u00fcpff mich auch mit deiner Zungen/", "tokens": ["St\u00fcpff", "mich", "auch", "mit", "dei\u00b7ner", "Zun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vngezwungen;", "tokens": ["Vn\u00b7ge\u00b7zwun\u00b7gen", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Die s\u00fcsser dann Honig ist.", "tokens": ["Die", "s\u00fcs\u00b7ser", "dann", "Ho\u00b7nig", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "NN", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Also la\u00df vns Kurtzweil f\u00fchren/", "tokens": ["Al\u00b7so", "la\u00df", "vns", "Kurt\u00b7zweil", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Damit wir ja nit verlieren", "tokens": ["Da\u00b7mit", "wir", "ja", "nit", "ver\u00b7lie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Der Jugent einige frist.", "tokens": ["Der", "Ju\u00b7gent", "ei\u00b7ni\u00b7ge", "frist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "La\u00df vns nach ", "tokens": ["La\u00df", "vns", "nach"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Wandeln auff der Jugent spur/", "tokens": ["Wan\u00b7deln", "auff", "der", "Ju\u00b7gent", "spur", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bi\u00df das Alter krum gebogen", "tokens": ["Bi\u00df", "das", "Al\u00b7ter", "krum", "ge\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Komm gezogen/", "tokens": ["Komm", "ge\u00b7zo\u00b7gen", "/"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVPP", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Mit Zittern/ Kelt/ Forcht/ vnd Grau\u00df/", "tokens": ["Mit", "Zit\u00b7tern", "/", "Kelt", "/", "Forcht", "/", "vnd", "Grau\u00df", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "NN", "$(", "NN", "$(", "KON", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Welches mit sich auff dem Rucken", "tokens": ["Wel\u00b7ches", "mit", "sich", "auff", "dem", "Ru\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Viel leids bringet/ vns zudrucken/", "tokens": ["Viel", "leids", "brin\u00b7get", "/", "vns", "zu\u00b7dru\u00b7cken", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$(", "PPER", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Bi\u00df es vns macht den garau\u00df.", "tokens": ["Bi\u00df", "es", "vns", "macht", "den", "gar\u00b7au\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}