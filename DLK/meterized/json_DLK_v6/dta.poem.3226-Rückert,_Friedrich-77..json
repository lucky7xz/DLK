{"dta.poem.3226": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "77.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In bessern Zeiten war die Poesie im Frieden", "tokens": ["In", "bes\u00b7sern", "Zei\u00b7ten", "war", "die", "Poe\u00b7sie", "im", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit Prosa, weil Gebiet war von Gebiet geschieden.", "tokens": ["Mit", "Pro\u00b7sa", ",", "weil", "Ge\u00b7biet", "war", "von", "Ge\u00b7biet", "ge\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "NN", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mit Kunst und Weisheit wollt' in ihren eignen Grenzen", "tokens": ["Mit", "Kunst", "und", "Weis\u00b7heit", "wollt'", "in", "ih\u00b7ren", "eig\u00b7nen", "Gren\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VMFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich jede r\u00fcnden, und mit eigner Sch\u00f6nheit gl\u00e4nzen.", "tokens": ["Sich", "je\u00b7de", "r\u00fcn\u00b7den", ",", "und", "mit", "eig\u00b7ner", "Sch\u00f6n\u00b7heit", "gl\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "ADJA", "$,", "KON", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ohn' etwas von dem Gut der Nachbarin genommen", "tokens": ["Ohn'", "et\u00b7was", "von", "dem", "Gut", "der", "Nach\u00b7ba\u00b7rin", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "APPR", "ART", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu haben, jede hielt auf ihr's und war vollkommen.", "tokens": ["Zu", "ha\u00b7ben", ",", "je\u00b7de", "hielt", "auf", "ih\u00b7r's", "und", "war", "voll\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VAINF", "$,", "PIAT", "VVFIN", "APPR", "NE", "KON", "VAFIN", "ADJD", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Was hat sie nun beth\u00f6rt, den Haushalt so verst\u00f6rt,", "tokens": ["Was", "hat", "sie", "nun", "be\u00b7th\u00f6rt", ",", "den", "Haus\u00b7halt", "so", "ver\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVFIN", "$,", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df keine recht mehr wei\u00df, was recht ihr angeh\u00f6rt?", "tokens": ["Da\u00df", "kei\u00b7ne", "recht", "mehr", "wei\u00df", ",", "was", "recht", "ihr", "an\u00b7ge\u00b7h\u00f6rt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "VVFIN", "$,", "PRELS", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Anma\u00dfend haben sie begonnen auszuschweifen,", "tokens": ["An\u00b7ma\u00b7\u00dfend", "ha\u00b7ben", "sie", "be\u00b7gon\u00b7nen", "aus\u00b7zu\u00b7schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und jede will in's Reich der andern \u00fcbergreifen.", "tokens": ["Und", "je\u00b7de", "will", "in's", "Reich", "der", "an\u00b7dern", "\u00fc\u00b7berg\u00b7rei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "APPRART", "NN", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Daraus entstanden ist Grenzstreitigkeit und Irrung,", "tokens": ["Da\u00b7raus", "ent\u00b7stan\u00b7den", "ist", "Grenz\u00b7strei\u00b7tig\u00b7keit", "und", "Ir\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVPP", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Und drau\u00dfen \u00fcberhand und drinnen nimmt Verwirrung.", "tokens": ["Und", "drau\u00b7\u00dfen", "\u00fc\u00b7ber\u00b7hand", "und", "drin\u00b7nen", "nimmt", "Ver\u00b7wir\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "KON", "ADV", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Was eignes keine mehr will keiner mehr erlauben;", "tokens": ["Was", "eig\u00b7nes", "kei\u00b7ne", "mehr", "will", "kei\u00b7ner", "mehr", "er\u00b7lau\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "PIAT", "PIS", "VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie eine was erwarb, wird ihr's die andre rauben.", "tokens": ["Wie", "ei\u00b7ne", "was", "er\u00b7warb", ",", "wird", "ih\u00b7r's", "die", "and\u00b7re", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "PWS", "VVFIN", "$,", "VAFIN", "PIS", "ART", "PIS", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Daraus entbl\u00fchn nun hie trostlose Zwitter, wie", "tokens": ["Da\u00b7raus", "ent\u00b7bl\u00fchn", "nun", "hie", "trost\u00b7lo\u00b7se", "Zwit\u00b7ter", ",", "wie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PAV", "VVFIN", "ADV", "ADV", "ADJA", "NN", "$,", "PWAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Poetische Prosa und prosaische Poesie.", "tokens": ["Poe\u00b7ti\u00b7sche", "Pro\u00b7sa", "und", "pro\u00b7sa\u00b7i\u00b7sche", "Poe\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.9": {"line.1": {"text": "Und der sie r\u00fcgt, mein Ton, bist du nicht auch ein Zwitter?", "tokens": ["Und", "der", "sie", "r\u00fcgt", ",", "mein", "Ton", ",", "bist", "du", "nicht", "auch", "ein", "Zwit\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "$,", "VAFIN", "PPER", "PTKNEG", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus zweien nicht gemischt, einst du die zwei als dritter.", "tokens": ["Aus", "zwei\u00b7en", "nicht", "ge\u00b7mischt", ",", "einst", "du", "die", "zwei", "als", "drit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "PTKNEG", "VVPP", "$,", "ADV", "PPER", "ART", "CARD", "KOUS", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}