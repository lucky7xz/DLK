{"textgrid.poem.45314": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Carlo Alberto", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Schwert Italiens? Mag wohl sein!", "tokens": ["Das", "Schwert", "I\u00b7ta\u00b7li\u00b7ens", "?", "Mag", "wohl", "sein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$.", "VMFIN", "ADV", "VAINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zum wenigsten f\u00fcr solche.", "tokens": ["Zum", "we\u00b7nigs\u00b7ten", "f\u00fcr", "sol\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "PIS", "APPR", "PIAT", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Die Schwerter dort sind etwas klein,", "tokens": ["Die", "Schwer\u00b7ter", "dort", "sind", "et\u00b7was", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei uns nennt man sie Dolche.", "tokens": ["Bei", "uns", "nennt", "man", "sie", "Dol\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIS", "PPER", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}