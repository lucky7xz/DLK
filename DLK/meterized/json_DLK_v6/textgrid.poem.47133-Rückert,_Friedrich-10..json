{"textgrid.poem.47133": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Paradies, ein verlorenes,", "tokens": ["Ein", "Pa\u00b7ra\u00b7dies", ",", "ein", "ver\u00b7lo\u00b7re\u00b7nes", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+---+-+--", "measure": "dactylic.init"}, "line.2": {"text": "Liegt r\u00fcckw\u00e4rts in der Vergangenheit.", "tokens": ["Liegt", "r\u00fcck\u00b7w\u00e4rts", "in", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und ein wiedergeborenes", "tokens": ["Und", "ein", "wie\u00b7der\u00b7ge\u00b7bo\u00b7re\u00b7nes"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "ADJA"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.4": {"text": "Liegt vorw\u00e4rts in der Zukunft weit.", "tokens": ["Liegt", "vor\u00b7w\u00e4rts", "in", "der", "Zu\u00b7kunft", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Immer r\u00fcckw\u00e4rts nach jenem blickt", "tokens": ["Im\u00b7mer", "r\u00fcck\u00b7w\u00e4rts", "nach", "je\u00b7nem", "blickt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PDAT", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Und Blicke vorw\u00e4rts nach diesem schickt", "tokens": ["Und", "Bli\u00b7cke", "vor\u00b7w\u00e4rts", "nach", "die\u00b7sem", "schickt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "APPR", "PDAT", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Wehmut und Sehnsucht, dein Wegegeleit,", "tokens": ["Weh\u00b7mut", "und", "Sehn\u00b7sucht", ",", "dein", "We\u00b7ge\u00b7ge\u00b7leit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.8": {"text": "O Herz, durch die Spanne der \u00f6den Zeit.", "tokens": ["O", "Herz", ",", "durch", "die", "Span\u00b7ne", "der", "\u00f6\u00b7den", "Zeit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}