{"dta.poem.19533": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die gegei\u00dfelte Psyche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Wo von alter Sch\u00f6nheit Tr\u00fcmmern", "tokens": ["Wo", "von", "al\u00b7ter", "Sch\u00f6n\u00b7heit", "Tr\u00fcm\u00b7mern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Marmorhell die S\u00e4le schimmern,", "tokens": ["Mar\u00b7mo\u00b7rhell", "die", "S\u00e4\u00b7le", "schim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Windet bla\u00df und lieblich eine", "tokens": ["Win\u00b7det", "bla\u00df", "und", "lieb\u00b7lich", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Psyche sich im Marmelsteine.", "tokens": ["Psy\u00b7che", "sich", "im", "Mar\u00b7mel\u00b7stei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Unsichtbarem Gei\u00dfelhiebe", "tokens": ["Un\u00b7sicht\u00b7ba\u00b7rem", "Gei\u00b7\u00df\u00b7el\u00b7hie\u00b7be"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Beugt sie sich in Qual und Liebe,", "tokens": ["Beugt", "sie", "sich", "in", "Qual", "und", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf den zarten Knieen liegend,", "tokens": ["Auf", "den", "zar\u00b7ten", "Kni\u00b7e\u00b7en", "lie\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Enge sich zusammenschmiegend.", "tokens": ["En\u00b7ge", "sich", "zu\u00b7sam\u00b7men\u00b7schmie\u00b7gend", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PRF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Flehend halb und halb geduldig", "tokens": ["Fle\u00b7hend", "halb", "und", "halb", "ge\u00b7dul\u00b7dig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "ADJD", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4gt sie Schmach und wei\u00df sich schuldig,", "tokens": ["Tr\u00e4gt", "sie", "Schmach", "und", "wei\u00df", "sich", "schul\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "VVFIN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre Schmerzensblicke fragen:", "tokens": ["Ih\u00b7re", "Schmer\u00b7zens\u00b7bli\u00b7cke", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebst du mich? und kannst mich schlagen?", "tokens": ["Liebst", "du", "mich", "?", "und", "kannst", "mich", "schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$.", "KON", "VMFIN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Soll dich der Olymp begr\u00fc\u00dfen,", "tokens": ["Soll", "dich", "der", "O\u00b7lymp", "be\u00b7gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Arme Psyche, mu\u00dft du b\u00fc\u00dfen!", "tokens": ["Ar\u00b7me", "Psy\u00b7che", ",", "mu\u00dft", "du", "b\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eros, der dich sucht und peinigt,", "tokens": ["E\u00b7ros", ",", "der", "dich", "sucht", "und", "pei\u00b7nigt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Will dich selig und gereinigt.", "tokens": ["Will", "dich", "se\u00b7lig", "und", "ge\u00b7rei\u00b7nigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "KON", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}