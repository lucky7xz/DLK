{"dta.poem.21736": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Candore et Odore.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Siehst du der Lilie weisses Kleid", "tokens": ["Siehst", "du", "der", "Li\u00b7lie", "weis\u00b7ses", "Kleid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus dunkler Ferne winken?", "tokens": ["Aus", "dunk\u00b7ler", "Fer\u00b7ne", "win\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Licht besiegt die Dunkelheit,", "tokens": ["Ihr", "Licht", "be\u00b7siegt", "die", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Lunens Silberblinken.", "tokens": ["Wie", "Lu\u00b7nens", "Sil\u00b7ber\u00b7blin\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O Blume, die in Eden spross,", "tokens": ["O", "Blu\u00b7me", ",", "die", "in", "E\u00b7den", "spross", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Eden sich zuerst erschloss,", "tokens": ["In", "E\u00b7den", "sich", "zu\u00b7erst", "er\u00b7schloss", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dich tr\u00fcbet keine Makel.", "tokens": ["Dich", "tr\u00fc\u00b7bet", "kei\u00b7ne", "Ma\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die ihr der Giglio's Blume f\u00fchrt", "tokens": ["Die", "ihr", "der", "Giglio's", "Blu\u00b7me", "f\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NE", "NN", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In Wappen und Panieren;", "tokens": ["In", "Wap\u00b7pen", "und", "Pa\u00b7nie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Begn\u00fcgt euch nicht, was jene ziert,", "tokens": ["Be\u00b7gn\u00fcgt", "euch", "nicht", ",", "was", "je\u00b7ne", "ziert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PWS", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Schild und Ring zu f\u00fchren.", "tokens": ["In", "Schild", "und", "Ring", "zu", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Giglio's Sinn ist lilienklar", "tokens": ["Der", "Giglio's", "Sinn", "ist", "li\u00b7li\u00b7en\u00b7klar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und lilienrein! Seyd treu und wahr", "tokens": ["Und", "li\u00b7li\u00b7en\u00b7rein", "!", "Seyd", "treu", "und", "wahr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$.", "VAIMP", "ADJD", "KON", "ADJD"], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.7": {"text": "Auch ihr in Red' und Thaten.", "tokens": ["Auch", "ihr", "in", "Red'", "und", "Tha\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sp\u00fcrst du der Lilie Ambraduft", "tokens": ["Sp\u00fcrst", "du", "der", "Li\u00b7lie", "Am\u00b7bra\u00b7duft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von dort her\u00fcberschweben?", "tokens": ["Von", "dort", "her\u00b7\u00fc\u00b7bersc\u00b7hwe\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sp\u00fcrst rings um dich in lauer Luft", "tokens": ["Sp\u00fcrst", "rings", "um", "dich", "in", "lau\u00b7er", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wohlger\u00fcche weben?", "tokens": ["Die", "Wohl\u00b7ge\u00b7r\u00fc\u00b7che", "we\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O Blume, die das Aug' entz\u00fcckt,", "tokens": ["O", "Blu\u00b7me", ",", "die", "das", "Aug'", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O Blume, die das Herz erquickt,", "tokens": ["O", "Blu\u00b7me", ",", "die", "das", "Herz", "er\u00b7quickt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O Blume, sey gepriesen!", "tokens": ["O", "Blu\u00b7me", ",", "sey", "ge\u00b7prie\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Giglio's S\u00f6hne, offenbart", "tokens": ["Der", "Giglio's", "S\u00f6h\u00b7ne", ",", "of\u00b7fen\u00b7bart"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NE", "NN", "$,", "VVFIN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Der Giglio's Seelenadel!", "tokens": ["Der", "Giglio's", "See\u00b7le\u00b7na\u00b7del", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der Giglio's T\u00f6chter, ach bewahrt", "tokens": ["Der", "Giglio's", "T\u00f6ch\u00b7ter", ",", "ach", "be\u00b7wahrt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NE", "NN", "$,", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Giglio s Ruf vor Tadel!", "tokens": ["Der", "Gig\u00b7lio", "s", "Ruf", "vor", "Ta\u00b7del", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Lasst eurer Tugend s\u00fcssen Duft,", "tokens": ["Lasst", "eu\u00b7rer", "Tu\u00b7gend", "s\u00fcs\u00b7sen", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Vaterlands, des Auslands Luft", "tokens": ["Des", "Va\u00b7ter\u00b7lands", ",", "des", "Aus\u00b7lands", "Luft"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bis an die Sterne w\u00fcrzen!", "tokens": ["Bis", "an", "die", "Ster\u00b7ne", "w\u00fcr\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}