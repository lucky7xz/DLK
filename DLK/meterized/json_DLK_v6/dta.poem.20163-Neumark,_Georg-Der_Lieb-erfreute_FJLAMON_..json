{"dta.poem.20163": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Lieb-erfreute  \n  FJLAMON .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1666", "urn": "urn:nbn:de:kobv:b4-20424-1", "language": ["de:0.85", "et:0.14"], "booktitle": "Neumark, Georg: Poetisch-Historischer Lustgarten. Frankfurt (Main), 1666."}, "poem": {"stanza.1": {"line.1": {"text": "Viel tausend tausend mal hab\u2019 ich mit List betrogen", "tokens": ["Viel", "tau\u00b7send", "tau\u00b7send", "mal", "hab'", "ich", "mit", "List", "be\u00b7tro\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "CARD", "CARD", "ADV", "VAFIN", "PPER", "APPR", "NN", "VVPP"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So manche Dam\u2019 und Held durch diesen Pfeil und Bogen/", "tokens": ["So", "man\u00b7che", "Dam'", "und", "Held", "durch", "die\u00b7sen", "Pfeil", "und", "Bo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "KON", "NN", "APPR", "PDAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und du Belliflora/ solst mir entgegen sein?", "tokens": ["Und", "du", "Bell\u00b7if\u00b7lo\u00b7ra", "/", "solst", "mir", "ent\u00b7ge\u00b7gen", "sein", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NE", "$(", "VMFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "O weit gefehlt! halt/ halt! wer auch dein Hertz ein Stein/", "tokens": ["O", "weit", "ge\u00b7fehlt", "!", "halt", "/", "halt", "!", "wer", "auch", "dein", "Hertz", "ein", "Stein", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVPP", "$.", "VVFIN", "$(", "VVFIN", "$.", "PWS", "ADV", "PPOSAT", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So sol mein scharffer Pfcil dasselbe doch zerspalten.", "tokens": ["So", "sol", "mein", "scharf\u00b7fer", "Pf\u00b7cil", "das\u00b7sel\u00b7be", "doch", "zer\u00b7spal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "ADJA", "NN", "PDAT", "ADV", "VVFIN", "$."], "meter": "-+-+-++-+-+-+-", "measure": "unknown.measure.septa"}, "line.6": {"text": "Gl\u00e4ubstu es nicht? Sih da!", "tokens": ["Gl\u00e4ubs\u00b7tu", "es", "nicht", "?", "Sih", "da", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$.", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}