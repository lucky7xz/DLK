{"dta.poem.341": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Sie ist zu grausahm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Felsen brechen/ Steine springen/", "tokens": ["Fel\u00b7sen", "bre\u00b7chen", "/", "Stei\u00b7ne", "sprin\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$(", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber du bleibst immer hart/", "tokens": ["A\u00b7ber", "du", "bleibst", "im\u00b7mer", "hart", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Seuffzer/ meine Z\u00e4hren/", "tokens": ["Mei\u00b7ne", "Seuff\u00b7zer", "/", "mei\u00b7ne", "Z\u00e4h\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will dein Ohr niemahls erh\u00f6ren;", "tokens": ["Will", "dein", "Ohr", "nie\u00b7mahls", "er\u00b7h\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Du bist eine Tyger Art", "tokens": ["Du", "bist", "ei\u00b7ne", "Ty\u00b7ger", "Art"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sonsten w\u00fcrde man dich zwingen.", "tokens": ["Sons\u00b7ten", "w\u00fcr\u00b7de", "man", "dich", "zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIS", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Felsen brechen/ Steine springen", "tokens": ["Fel\u00b7sen", "bre\u00b7chen", "/", "Stei\u00b7ne", "sprin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVINF", "$(", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aber du bleibst immer hart.", "tokens": ["A\u00b7ber", "du", "bleibst", "im\u00b7mer", "hart", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Soll ich sterben?", "tokens": ["Soll", "ich", "ster\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Ey wohlan!", "tokens": ["Ey", "wo\u00b7hlan", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Ich will mich dazu bereiten/", "tokens": ["Ich", "will", "mich", "da\u00b7zu", "be\u00b7rei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PAV", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Weil ich doch nicht leben soll.", "tokens": ["Weil", "ich", "doch", "nicht", "le\u00b7ben", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mag ein Seuffzer mich begleiten", "tokens": ["Mag", "ein", "Seuff\u00b7zer", "mich", "be\u00b7glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist mir auch im Grabe wohl.", "tokens": ["Ist", "mir", "auch", "im", "Gra\u00b7be", "wohl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mein Verderben", "tokens": ["Mein", "Ver\u00b7der\u00b7ben"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Heisset jeder wohlgethan.", "tokens": ["Heis\u00b7set", "je\u00b7der", "wohl\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Plage/ qu\u00e4hle/ ich erdulde", "tokens": ["Pla\u00b7ge", "/", "qu\u00e4h\u00b7le", "/", "ich", "er\u00b7dul\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$(", "VVFIN", "$(", "PPER", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aller Marter Centner-Last/", "tokens": ["Al\u00b7ler", "Mar\u00b7ter", "Cent\u00b7ner\u00b7Last", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn wenn ich also gestorben", "tokens": ["Denn", "wenn", "ich", "al\u00b7so", "ge\u00b7stor\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hab ich diesen Trost erworben/", "tokens": ["Hab", "ich", "die\u00b7sen", "Trost", "er\u00b7wor\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PDAT", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ein M\u00e4rterer erbla\u00dft.", "tokens": ["Da\u00df", "ein", "M\u00e4r\u00b7te\u00b7rer", "er\u00b7bla\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Plage/ qu\u00e4hle ich erdulde", "tokens": ["Pla\u00b7ge", "/", "qu\u00e4h\u00b7le", "ich", "er\u00b7dul\u00b7de"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "VVFIN", "PPER", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Aller Marter Centner-Last.", "tokens": ["Al\u00b7ler", "Mar\u00b7ter", "Cent\u00b7ner\u00b7Last", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}