her", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Recht aufw\u00e4rts in die H\u00f6h', und, mit besond'rer M\u00fch',", "tokens": ["Recht", "auf\u00b7w\u00e4rts", "in", "die", "H\u00f6h'", ",", "und", ",", "mit", "be\u00b7son\u00b7d'\u00b7rer", "M\u00fch'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$,", "KON", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.42": {"text": "Nicht kriecht, nicht fliegt, nicht springt, an ihrem Drat sich hebet.", "tokens": ["Nicht", "kriecht", ",", "nicht", "fliegt", ",", "nicht", "springt", ",", "an", "ih\u00b7rem", "Drat", "sich", "he\u00b7bet", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "PTKNEG", "VVFIN", "$,", "PTKNEG", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Dort kriecht die faule Laus in steter Langsamkeit;", "tokens": ["Dort", "kriecht", "die", "fau\u00b7le", "Laus", "in", "ste\u00b7ter", "Lang\u00b7sam\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Dort h\u00fcpft ein schneller Floh viel tausend mahl so weit,", "tokens": ["Dort", "h\u00fcpft", "ein", "schnel\u00b7ler", "Floh", "viel", "tau\u00b7send", "mahl", "so", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"],