{"textgrid.poem.40015": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: J\u00fcngst sah ich einen Frosch, wie wir spatziren giengen,", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst sah ich einen Frosch, wie wir spatziren giengen,", "tokens": ["J\u00fcngst", "sah", "ich", "ei\u00b7nen", "Frosch", ",", "wie", "wir", "spat\u00b7zi\u00b7ren", "gien\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch das bethaute Gras, in gr\u00f6ster Eile, springen.", "tokens": ["Durch", "das", "be\u00b7thau\u00b7te", "Gras", ",", "in", "gr\u00f6s\u00b7ter", "Ei\u00b7le", ",", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Kinder waren gleich schnell hinter ihm darein,", "tokens": ["Die", "Kin\u00b7der", "wa\u00b7ren", "gleich", "schnell", "hin\u00b7ter", "ihm", "da\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "APPR", "PPER", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den feuchten Springer zu erhaschen;", "tokens": ["Den", "feuch\u00b7ten", "Sprin\u00b7ger", "zu", "er\u00b7ha\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er aber brauchte so die langen schlancken Bein',", "tokens": ["Er", "a\u00b7ber", "brauch\u00b7te", "so", "die", "lan\u00b7gen", "schlan\u00b7cken", "Bein'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es konnt ihn keiner \u00fcberraschen.", "tokens": ["Es", "konnt", "ihn", "kei\u00b7ner", "\u00fc\u00b7berr\u00b7a\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wor\u00fcber ich denn Anfangs lachte,", "tokens": ["Wo\u00b7r\u00fc\u00b7ber", "ich", "denn", "An\u00b7fangs", "lach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bis mich die\u00df Spiel zuletzt auf die Gedancken brachte:", "tokens": ["Bis", "mich", "die\u00df", "Spiel", "zu\u00b7letzt", "auf", "die", "Ge\u00b7dan\u00b7cken", "brach\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PDS", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hier seh' ich abermahl die Wunder-volle Spur", "tokens": ["Hier", "seh'", "ich", "a\u00b7ber\u00b7mahl", "die", "Wun\u00b7der\u00b7vol\u00b7le", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der wirckenden Natur,", "tokens": ["Der", "wir\u00b7cken\u00b7den", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die solchen Trieb und Kraft in jedes Thier gelegt;", "tokens": ["Die", "sol\u00b7chen", "Trieb", "und", "Kraft", "in", "je\u00b7des", "Thier", "ge\u00b7legt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "KON", "NN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df sich ein jedes fast verschiedentlich bewegt;", "tokens": ["Da\u00df", "sich", "ein", "je\u00b7des", "fast", "ver\u00b7schie\u00b7dent\u00b7lich", "be\u00b7wegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "PIAT", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da viel', um fortzugehn, sich wunderbarlich biegen,", "tokens": ["Da", "viel'", ",", "um", "fort\u00b7zu\u00b7gehn", ",", "sich", "wun\u00b7der\u00b7bar\u00b7lich", "bie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "KOUI", "VVIZU", "$,", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da ihrer viele mit, viel' ohne, Federn fliegen,", "tokens": ["Da", "ih\u00b7rer", "vie\u00b7le", "mit", ",", "viel'", "oh\u00b7ne", ",", "Fe\u00b7dern", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "PIS", "PTKVZ", "$,", "PIS", "APPR", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da ihrer viele mit, viel' ohne, F\u00fcsse gehn,", "tokens": ["Da", "ih\u00b7rer", "vie\u00b7le", "mit", ",", "viel'", "oh\u00b7ne", ",", "F\u00fcs\u00b7se", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "PIS", "PTKVZ", "$,", "PIS", "APPR", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der wir an vielen zwey, an vielen hundert sehn.", "tokens": ["Der", "wir", "an", "vie\u00b7len", "zwey", ",", "an", "vie\u00b7len", "hun\u00b7dert", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PIAT", "CARD", "$,", "APPR", "PIAT", "CARD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die V\u00f6gel heben sich, durch Fl\u00fcgel, in die L\u00fcfte,", "tokens": ["Die", "V\u00f6\u00b7gel", "he\u00b7ben", "sich", ",", "durch", "Fl\u00fc\u00b7gel", ",", "in", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "APPR", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und k\u00f6nnen doch zugleich gehn, h\u00fcpfen, sitzen, stehn.", "tokens": ["Und", "k\u00f6n\u00b7nen", "doch", "zu\u00b7gleich", "gehn", ",", "h\u00fcp\u00b7fen", ",", "sit\u00b7zen", ",", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Maulwurf gr\u00e4bt sich fort durch seine finstern Gr\u00fcfte,", "tokens": ["Der", "Maul\u00b7wurf", "gr\u00e4bt", "sich", "fort", "durch", "sei\u00b7ne", "fins\u00b7tern", "Gr\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKVZ", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Kann er darinnen gleich des Tages Licht nicht sehn.", "tokens": ["Kann", "er", "da\u00b7rin\u00b7nen", "gleich", "des", "Ta\u00b7ges", "Licht", "nicht", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ART", "NN", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Eyder siehet man, von einer Seit' zur andern,", "tokens": ["Die", "Ey\u00b7der", "sie\u00b7het", "man", ",", "von", "ei\u00b7ner", "Seit'", "zur", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "$,", "APPR", "ART", "NN", "APPRART", "ADJA", "$,"], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Auf recht besond're Weis', und doch geschwinde, wandern.", "tokens": ["Auf", "recht", "be\u00b7son\u00b7d'\u00b7re", "Weis'", ",", "und", "doch", "ge\u00b7schwin\u00b7de", ",", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,", "KON", "ADV", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Am gr\u00fcnen Laub-Frosch sind die F\u00fcsse so formirt,", "tokens": ["Am", "gr\u00fc\u00b7nen", "Laub\u00b7Frosch", "sind", "die", "F\u00fcs\u00b7se", "so", "for\u00b7mirt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df alles, was er, auch so gar im Sprung, ber\u00fchrt,", "tokens": ["Da\u00df", "al\u00b7les", ",", "was", "er", ",", "auch", "so", "gar", "im", "Sprung", ",", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PWS", "PPER", "$,", "ADV", "ADV", "ADV", "APPRART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ihm gleich zur Leiter dient, und w\u00e4r' es noch so glatt,", "tokens": ["Ihm", "gleich", "zur", "Lei\u00b7ter", "dient", ",", "und", "w\u00e4r'", "es", "noch", "so", "glatt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "VVFIN", "$,", "KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und w\u00e4r' es noch so steil:", "tokens": ["Und", "w\u00e4r'", "es", "noch", "so", "steil", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Wie ich ihn denn in Eil',", "tokens": ["Wie", "ich", "ihn", "denn", "in", "Eil'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Als er im Sprung ein Scheiben-Glas, so platt,", "tokens": ["Als", "er", "im", "Sprung", "ein", "Schei\u00b7ben\u00b7Glas", ",", "so", "platt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Ereilet hatt',", "tokens": ["Er\u00b7ei\u00b7let", "hatt'", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.22": {"text": "Ohn' alle M\u00fch' daran empor spatziren gehn,", "tokens": ["Ohn'", "al\u00b7le", "M\u00fch'", "da\u00b7ran", "em\u00b7por", "spat\u00b7zi\u00b7ren", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PAV", "PTKVZ", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Nicht ohn' Erstaunen einst gesehn.", "tokens": ["Nicht", "ohn'", "Er\u00b7stau\u00b7nen", "einst", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Verwunderlich weis sich die Schlange fortzubringen,", "tokens": ["Ver\u00b7wun\u00b7der\u00b7lich", "weis", "sich", "die", "Schlan\u00b7ge", "fort\u00b7zu\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.25": {"text": "Wie auch der Regen-Wurm; kein Fl\u00fcgel, Hand noch Fu\u00df", "tokens": ["Wie", "auch", "der", "Re\u00b7gen\u00b7Wurm", ";", "kein", "Fl\u00fc\u00b7gel", ",", "Hand", "noch", "Fu\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "$.", "PIAT", "NN", "$,", "NN", "ADV", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Hilft ihr, im Gehen, fort; ihr Leib mu\u00df in sich dringen;", "tokens": ["Hilft", "ihr", ",", "im", "Ge\u00b7hen", ",", "fort", ";", "ihr", "Leib", "mu\u00df", "in", "sich", "drin\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPRART", "NN", "$,", "PTKVZ", "$.", "PPOSAT", "NN", "VMFIN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Dann schiesset sie sich selbst, bald wie ein Bogen-Schu\u00df,", "tokens": ["Dann", "schies\u00b7set", "sie", "sich", "selbst", ",", "bald", "wie", "ein", "Bo\u00b7gen\u00b7Schu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "$,", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Bald schnell, bald langsam, fort. Auf andre Weise strecken", "tokens": ["Bald", "schnell", ",", "bald", "lang\u00b7sam", ",", "fort", ".", "Auf", "and\u00b7re", "Wei\u00b7se", "stre\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "PTKVZ", "$.", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die langsam wandernden, geh\u00f6rnten, glatten Schnecken", "tokens": ["Die", "lang\u00b7sam", "wan\u00b7dern\u00b7den", ",", "ge\u00b7h\u00f6rn\u00b7ten", ",", "glat\u00b7ten", "Schne\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "$,", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ihr schl\u00fcpfrig Fleisch voraus; sie \u00e4ndern Stand und Ort", "tokens": ["Ihr", "schl\u00fcpf\u00b7rig", "Fleisch", "vo\u00b7raus", ";", "sie", "\u00e4n\u00b7dern", "Stand", "und", "Ort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "NN", "PTKVZ", "$.", "PPER", "VVINF", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Unsichtbar, unvermerckt; sie fliessen gleichsam fort.", "tokens": ["Un\u00b7sicht\u00b7bar", ",", "un\u00b7ver\u00b7merckt", ";", "sie", "flies\u00b7sen", "gleich\u00b7sam", "fort", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$.", "PPER", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die Maden schliessen sich", "tokens": ["Die", "Ma\u00b7den", "schlies\u00b7sen", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Den Kopf an ihren Schwantz, recht wie gekr\u00fcmmte B\u00f6gen,", "tokens": ["Den", "Kopf", "an", "ih\u00b7ren", "Schwantz", ",", "recht", "wie", "ge\u00b7kr\u00fcmm\u00b7te", "B\u00f6\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,", "ADJD", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und werfen sich dahin so schnell, als wenn sie fl\u00f6gen,", "tokens": ["Und", "wer\u00b7fen", "sich", "da\u00b7hin", "so", "schnell", ",", "als", "wenn", "sie", "fl\u00f6\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PAV", "ADV", "ADJD", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Indem, was sonsten keine thut,", "tokens": ["In\