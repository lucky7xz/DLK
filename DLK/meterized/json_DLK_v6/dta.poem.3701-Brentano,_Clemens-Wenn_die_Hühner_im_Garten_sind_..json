{"dta.poem.3701": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Wenn die H\u00fchner im Garten sind .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Hinkelchen, mein Hinkelchen,               ", "tokens": ["Mein", "Hin\u00b7kel\u00b7chen", ",", "mein", "Hin\u00b7kel\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+---", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Was machst in unserm Garten,", "tokens": ["Was", "machst", "in", "un\u00b7serm", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Pfl\u00fckst uns all die Bl\u00fcmchen ab,", "tokens": ["Pfl\u00fckst", "uns", "all", "die", "Bl\u00fcm\u00b7chen", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIAT", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Machst es gar zu arg,", "tokens": ["Machst", "es", "gar", "zu", "arg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Mutter wird dich jagen,", "tokens": ["Mut\u00b7ter", "wird", "dich", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Vater wird dich schlagen,", "tokens": ["Va\u00b7ter", "wird", "dich", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PP