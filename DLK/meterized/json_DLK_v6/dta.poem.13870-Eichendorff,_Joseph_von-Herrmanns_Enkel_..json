{"dta.poem.13870": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Herrmanns Enkel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Statt des wind'gen Fracks Geflatter", "tokens": ["Statt", "des", "win\u00b7d'\u00b7gen", "Fracks", "Ge\u00b7flat\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Der Litefka Schurz aus Pohlen,", "tokens": ["Der", "Li\u00b7tef\u00b7ka", "Schurz", "aus", "Poh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NE", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Statt des Franzen knabenglatter", "tokens": ["Statt", "des", "Fran\u00b7zen", "kna\u00b7ben\u00b7glat\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schnautze: seinen ", "tokens": ["Schnaut\u00b7ze", ":", "sei\u00b7nen"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$.", "PPOSAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Bruder, ich sag's unverhohlen,", "tokens": ["Bru\u00b7der", ",", "ich", "sag's", "un\u00b7ver\u00b7hoh\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und auch Du wirst's nicht bestreiten:", "tokens": ["Und", "auch", "Du", "wir\u00b7st's", "nicht", "be\u00b7strei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VAFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Gro\u00dfe Zeichen gro\u00dfer Zeiten! \u2014", "tokens": ["Gro\u00b7\u00dfe", "Zei\u00b7chen", "gro\u00b7\u00dfer", "Zei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wahrlich, s\u00e4h' ich nicht den Kragen", "tokens": ["Wahr\u00b7lich", ",", "s\u00e4h'", "ich", "nicht", "den", "Kra\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "VVFIN", "PPER", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ueber'n schwarzen Rock geschlagen,", "tokens": ["Ue\u00b7ber'n", "schwar\u00b7zen", "Rock", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Schien' mir Alles Ironie.", "tokens": ["Schien'", "mir", "Al\u00b7les", "I\u00b7ro\u00b7nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Doch wie sprech' ich da? Ironisch \u2014", "tokens": ["Doch", "wie", "sprech'", "ich", "da", "?", "I\u00b7ron\u00b7isch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "ADV", "$.", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Dieses Wort ist nicht teutonisch.", "tokens": ["Die\u00b7ses", "Wort", "ist", "nicht", "teu\u00b7to\u00b7nisch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "+-+-++--", "measure": "unknown.measure.tetra"}, "line.13": {"text": "Undeutsch ist die falsche Freude:", "tokens": ["Un\u00b7deutsch", "ist", "die", "fal\u00b7sche", "Freu\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "K\u00fcnsteln am wahrhaften Wort!", "tokens": ["K\u00fcns\u00b7teln", "am", "wahr\u00b7haf\u00b7ten", "Wort", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Ob auch feige Poesie", "tokens": ["Ob", "auch", "fei\u00b7ge", "Poe\u00b7sie"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.16": {"text": "Sauere Gesichter schneide:", "tokens": ["Sau\u00b7e\u00b7re", "Ge\u00b7sich\u00b7ter", "schnei\u00b7de", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.17": {"text": "Durch den w\u00e4lschen L\u00fcgenwitz", "tokens": ["Durch", "den", "w\u00e4l\u00b7schen", "L\u00fc\u00b7gen\u00b7witz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Schreitet stramm der Teutsche fort", "tokens": ["Schrei\u00b7tet", "stramm", "der", "Teut\u00b7sche", "fort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Hinter seiner Nasenspitz',", "tokens": ["Hin\u00b7ter", "sei\u00b7ner", "Na\u00b7sen\u00b7spitz'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Aller Ehrlichkeiten Sitz,", "tokens": ["Al\u00b7ler", "Ehr\u00b7lich\u00b7kei\u00b7ten", "Sitz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Biderb immer gradeaus.", "tokens": ["Bi\u00b7derb", "im\u00b7mer", "gra\u00b7de\u00b7aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Alles W\u00e4lsche wird mir Graus,", "tokens": ["Al\u00b7les", "W\u00e4l\u00b7sche", "wird", "mir", "Graus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Seit ich steck' im teutschen Kleide:", "tokens": ["Seit", "ich", "steck'", "im", "teut\u00b7schen", "Klei\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Du auch Liebchen w\u00e4hle gleich", "tokens": ["Du", "auch", "Lieb\u00b7chen", "w\u00e4h\u00b7le", "gleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "NN", "VVIMP", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Deine Tracht Dir altteutsch aus!", "tokens": ["Dei\u00b7ne", "Tracht", "Dir", "alt\u00b7teutsch", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Wie's auf Bildern noch zu schauen:", "tokens": ["Wie's", "auf", "Bil\u00b7dern", "noch", "zu", "schau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Wedel von dem Schweif der Pfauen,", "tokens": ["We\u00b7del", "von", "dem", "Schweif", "der", "Pfau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Dann von Spitzen, blumenreich,", "tokens": ["Dann", "von", "Spit\u00b7zen", ",", "blu\u00b7men\u00b7reich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Wie 'ne mittelm\u00e4\u00df'ge Scheibe,", "tokens": ["Wie", "'ne", "mit\u00b7tel\u00b7m\u00e4\u00df'\u00b7ge", "Schei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Eine steife Hals-Rotunde!", "tokens": ["Ei\u00b7ne", "stei\u00b7fe", "Hals\u00b7Rotun\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}