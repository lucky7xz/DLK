{"dta.poem.7333": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Die Taxuswand.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Ich stehe gern vor dir,", "tokens": ["Ich", "ste\u00b7he", "gern", "vor", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Du Fl\u00e4che schwarz und rauh,", "tokens": ["Du", "Fl\u00e4\u00b7che", "schwarz", "und", "rauh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du schartiges Visier", "tokens": ["Du", "schar\u00b7ti\u00b7ges", "Vi\u00b7sier"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vor meines Liebsten Brau',", "tokens": ["Vor", "mei\u00b7nes", "Liebs\u00b7ten", "Brau'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gern mag ich vor dir stehen,", "tokens": ["Gern", "mag", "ich", "vor", "dir", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie vor grundirtem Tuch,", "tokens": ["Wie", "vor", "grun\u00b7dir\u00b7tem", "Tuch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Und dr\u00fcber gleiten sehen", "tokens": ["Und", "dr\u00fc\u00b7ber", "glei\u00b7ten", "se\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PAV", "VVINF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den bleichen Kr\u00f6nungszug;", "tokens": ["Den", "blei\u00b7chen", "Kr\u00f6\u00b7nungs\u00b7zug", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Als mein die Krone hier,", "tokens": ["Als", "mein", "die", "Kro\u00b7ne", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Von H\u00e4nden die nun kalt;", "tokens": ["Von", "H\u00e4n\u00b7den", "die", "nun", "kalt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als man gesungen mir", "tokens": ["Als", "man", "ge\u00b7sun\u00b7gen", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVPP", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In Weisen die nun alt;", "tokens": ["In", "Wei\u00b7sen", "die", "nun", "alt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vorhang am Heiligthume,", "tokens": ["Vor\u00b7hang", "am", "Hei\u00b7lig\u00b7thu\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Mein Paradiesesthor,", "tokens": ["Mein", "Pa\u00b7ra\u00b7die\u00b7sest\u00b7hor", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dahinter Alles Blume,", "tokens": ["Da\u00b7hin\u00b7ter", "Al\u00b7les", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und Alles Dorn davor.", "tokens": ["Und", "Al\u00b7les", "Dorn", "da\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Denn jenseits wei\u00df ich sie,", "tokens": ["Denn", "jen\u00b7seits", "wei\u00df", "ich", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die gr\u00fcne Gartenbank,", "tokens": ["Die", "gr\u00fc\u00b7ne", "Gar\u00b7ten\u00b7bank", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo ich das Leben fr\u00fch", "tokens": ["Wo", "ich", "das", "Le\u00b7ben", "fr\u00fch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit gl\u00fchen Lippen trank,", "tokens": ["Mit", "gl\u00fc\u00b7hen", "Lip\u00b7pen", "trank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Als mich mein Haar umwallte", "tokens": ["Als", "mich", "mein", "Haar", "um\u00b7wall\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Noch golden wie ein Stral,", "tokens": ["Noch", "gol\u00b7den", "wie", "ein", "Stral", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Als noch mein Ruf erschallte,", "tokens": ["Als", "noch", "mein", "Ruf", "er\u00b7schall\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein Hornsto\u00df, durch das Thal.", "tokens": ["Ein", "Horn\u00b7sto\u00df", ",", "durch", "das", "Thal", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das zarte Epheureis,", "tokens": ["Das", "zar\u00b7te", "E\u00b7pheu\u00b7reis", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So Liebe pflegte dort,", "tokens": ["So", "Lie\u00b7be", "pfleg\u00b7te", "dort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sechs Schritte, \u2014 und ich wei\u00df,", "tokens": ["Sechs", "Schrit\u00b7te", ",", "und", "ich", "wei\u00df", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "$(", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich wei\u00df dann, da\u00df es fort.", "tokens": ["Ich", "wei\u00df", "dann", ",", "da\u00df", "es", "fort", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So will ich immer schleichen", "tokens": ["So", "will", "ich", "im\u00b7mer", "schlei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nur an dein dunkles Tuch,", "tokens": ["Nur", "an", "dein", "dunk\u00b7les", "Tuch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und achtzehn Jahre streichen", "tokens": ["Und", "acht\u00b7zehn", "Jah\u00b7re", "strei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "CARD", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Aus meinem Lebensbuch.", "tokens": ["Aus", "mei\u00b7nem", "Le\u00b7bens\u00b7buch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du starrtest damals schon", "tokens": ["Du", "starr\u00b7test", "da\u00b7mals", "schon"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So d\u00fcster treu wie heut',", "tokens": ["So", "d\u00fcs\u00b7ter", "treu", "wie", "heut'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "KOKOM", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du, unsrer Liebe Thron", "tokens": ["Du", ",", "uns\u00b7rer", "Lie\u00b7be", "Thron"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und W\u00e4chter manche Zeit;", "tokens": ["Und", "W\u00e4ch\u00b7ter", "man\u00b7che", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Man sagt da\u00df Schlaf, ein schlimmer,", "tokens": ["Man", "sagt", "da\u00df", "Schlaf", ",", "ein", "schlim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KOUS", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dir aus den Nadeln raucht, \u2014", "tokens": ["Dir", "aus", "den", "Na\u00b7deln", "raucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$,", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Ach, wacher war ich nimmer,", "tokens": ["Ach", ",", "wa\u00b7cher", "war", "ich", "nim\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJD", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Als rings von dir umhaucht!", "tokens": ["Als", "rings", "von", "dir", "um\u00b7haucht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nun aber bin ich matt,", "tokens": ["Nun", "a\u00b7ber", "bin", "ich", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und m\u00f6cht an deinem Saum", "tokens": ["Und", "m\u00f6cht", "an", "dei\u00b7nem", "Saum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vergleiten, wie ein Blatt", "tokens": ["Ver\u00b7glei\u00b7ten", ",", "wie", "ein", "Blatt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Geweht vom n\u00e4chsten Baum;", "tokens": ["Ge\u00b7weht", "vom", "n\u00e4chs\u00b7ten", "Baum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du lockst mich wie ein Hafen,", "tokens": ["Du", "lockst", "mich", "wie", "ein", "Ha\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo alle St\u00fcrme stumm,", "tokens": ["Wo", "al\u00b7le", "St\u00fcr\u00b7me", "stumm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "O, schlafen m\u00f6cht ich, schlafen,", "tokens": ["O", ",", "schla\u00b7fen", "m\u00f6cht", "ich", ",", "schla\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVINF", "VMFIN", "PPER", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bis meine Zeit herum!", "tokens": ["Bis", "mei\u00b7ne", "Zeit", "he\u00b7rum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}