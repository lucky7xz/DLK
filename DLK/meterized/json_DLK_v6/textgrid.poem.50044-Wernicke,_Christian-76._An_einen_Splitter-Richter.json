{"textgrid.poem.50044": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "76. An einen Splitter-Richter", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Magst du was meine ", "tokens": ["Magst", "du", "was", "mei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PIS", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Von meinem ", "tokens": ["Von", "mei\u00b7nem"], "token_info": ["word", "word"], "pos": ["APPR", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "So lass' auch meinen ", "tokens": ["So", "lass'", "auch", "mei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Was man bei mir vor ", "tokens": ["Was", "man", "bei", "mir", "vor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PIS", "APPR", "PPER", "PTKVZ"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Spricht man dort ", "tokens": ["Spricht", "man", "dort"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Und legt man hier ", "tokens": ["Und", "legt", "man", "hier"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "So wehle du die ", "tokens": ["So", "weh\u00b7le", "du", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Und denk', ", "tokens": ["Und", "denk'", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}}}}}