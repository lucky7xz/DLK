{"textgrid.poem.31043": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Jungfern, seyd ihr blind an Augen, da\u00df ihr nicht am Fenster lieget,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jungfern, seyd ihr blind an Augen, da\u00df ihr nicht am Fenster lieget,", "tokens": ["Jung\u00b7fern", ",", "seyd", "ihr", "blind", "an", "Au\u00b7gen", ",", "da\u00df", "ihr", "nicht", "am", "Fens\u00b7ter", "lie\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "ADJD", "APPR", "NN", "$,", "KOUS", "PPER", "PTKNEG", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Jungfern, seyd ihr taub an Ohren, da\u00df ihr nicht f\u00fcr Kuppler t\u00fcget,", "tokens": ["Jung\u00b7fern", ",", "seyd", "ihr", "taub", "an", "Oh\u00b7ren", ",", "da\u00df", "ihr", "nicht", "f\u00fcr", "Kupp\u00b7ler", "t\u00fc\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "ADJD", "APPR", "NN", "$,", "KOUS", "PPER", "PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Jungfern, seyd ihr lahm an F\u00fcssen, da\u00df ihr nicht die Stadt durchstreichet,", "tokens": ["Jung\u00b7fern", ",", "seyd", "ihr", "lahm", "an", "F\u00fcs\u00b7sen", ",", "da\u00df", "ihr", "nicht", "die", "Stadt", "durch\u00b7strei\u00b7chet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "VVFIN", "APPR", "NN