{"textgrid.poem.62038": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[kannst Du zweifeln, kannst Du zagen?]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kannst Du zweifeln, kannst Du zagen?", "tokens": ["Kannst", "Du", "zwei\u00b7feln", ",", "kannst", "Du", "za\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blick nach jenem \u00bbgro\u00dfen Wagen,\u00ab", "tokens": ["Blick", "nach", "je\u00b7nem", "\u00bb", "gro\u00b7\u00dfen", "Wa\u00b7gen", ",", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "PDAT", "$(", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wovon jeder Himmelszoll", "tokens": ["Wo\u00b7von", "je\u00b7der", "Him\u00b7mels\u00b7zoll"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von Myriaden Welten voll.", "tokens": ["Von", "My\u00b7ria\u00b7den", "Wel\u00b7ten", "voll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "ADJD", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Myriaden Welten, Sonnen,", "tokens": ["My\u00b7ria\u00b7den", "Wel\u00b7ten", ",", "Son\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ewigkeiten, ew'ge Wonnen,", "tokens": ["E\u00b7wig\u00b7kei\u00b7ten", ",", "ew'\u00b7ge", "Won\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Heil'ge Gottheit, h\u00f6re mich,", "tokens": ["Heil'\u00b7ge", "Got\u00b7theit", ",", "h\u00f6\u00b7re", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tief im Staube preis ich Dich!", "tokens": ["Tief", "im", "Stau\u00b7be", "preis", "ich", "Dich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "PTKVZ", "PPER", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}