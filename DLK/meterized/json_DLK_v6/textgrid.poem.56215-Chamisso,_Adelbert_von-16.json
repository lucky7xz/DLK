{"textgrid.poem.56215": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "16", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du schlummerst, feiner Knabe,", "tokens": ["Du", "schlum\u00b7merst", ",", "fei\u00b7ner", "Kna\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du meiner Freuden Kind,", "tokens": ["Du", "mei\u00b7ner", "Freu\u00b7den", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So sanft in meinen Armen,", "tokens": ["So", "sanft", "in", "mei\u00b7nen", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die deine Welt noch sind.", "tokens": ["Die", "dei\u00b7ne", "Welt", "noch", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nun wachst du auf, du l\u00e4chelst,", "tokens": ["Nun", "wachst", "du", "auf", ",", "du", "l\u00e4\u00b7chelst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich blicke wonnereich", "tokens": ["Ich", "bli\u00b7cke", "won\u00b7ne\u00b7reich"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In deines Vaters Augen", "tokens": ["In", "dei\u00b7nes", "Va\u00b7ters", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und in mein Himmelreich.", "tokens": ["Und", "in", "mein", "Him\u00b7mel\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "La\u00df schwelgend mich genie\u00dfen", "tokens": ["La\u00df", "schwel\u00b7gend", "mich", "ge\u00b7nie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ADJD", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der s\u00fc\u00dfen kurzen Frist,", "tokens": ["Der", "s\u00fc\u00b7\u00dfen", "kur\u00b7zen", "Frist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo noch an meinem Herzen", "tokens": ["Wo", "noch", "an", "mei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du ganz der Meine bist.", "tokens": ["Du", "ganz", "der", "Mei\u00b7ne", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es will sich bald nicht passen,", "tokens": ["Es", "will", "sich", "bald", "nicht", "pas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es treibt und dehnt sich aus,", "tokens": ["Es", "treibt", "und", "dehnt", "sich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es wird dem lock'gen Knaben", "tokens": ["Es", "wird", "dem", "lock'\u00b7gen", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu klein das Mutterhaus.", "tokens": ["Zu", "klein", "das", "Mut\u00b7ter\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es st\u00fcrmt der Mann ins Leben,", "tokens": ["Es", "st\u00fcrmt", "der", "Mann", "ins", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er bricht sich seine Bahn;", "tokens": ["Er", "bricht", "sich", "sei\u00b7ne", "Bahn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Lieb und Ha\u00df ger\u00fcstet", "tokens": ["Mit", "Lieb", "und", "Ha\u00df", "ge\u00b7r\u00fcs\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Strebt k\u00e4mpfend er hinan.", "tokens": ["Strebt", "k\u00e4mp\u00b7fend", "er", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "PTKVZ", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Und der verarmten Mutter", "tokens": ["Und", "der", "ver\u00b7arm\u00b7ten", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist nun Entsagung Pflicht;", "tokens": ["Ist", "nun", "Ent\u00b7sa\u00b7gung", "Pflicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie folgt ihm mit dem Herzen,", "tokens": ["Sie", "folgt", "ihm", "mit", "dem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Aug erreicht ihn nicht.", "tokens": ["Ihr", "Aug", "er\u00b7reicht", "ihn", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O Liebling meines Herzens,", "tokens": ["O", "Lieb\u00b7ling", "mei\u00b7nes", "Her\u00b7zens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Segen \u00fcber dich!", "tokens": ["Mein", "Se\u00b7gen", "\u00fc\u00b7ber", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sei gleich nur deinem Vater,", "tokens": ["Sei", "gleich", "nur", "dei\u00b7nem", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das andre findet sich.", "tokens": ["Das", "and\u00b7re", "fin\u00b7det", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}