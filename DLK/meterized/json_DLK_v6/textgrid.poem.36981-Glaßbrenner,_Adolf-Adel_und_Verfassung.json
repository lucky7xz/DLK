{"textgrid.poem.36981": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "Adel und Verfassung", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbjetzt verzeihe\u00ab, sprach der Sultan, wenn ich schuldigst mich erkecke", "tokens": ["\u00bb", "jetzt", "ver\u00b7zei\u00b7he", "\u00ab", ",", "sprach", "der", "Sul\u00b7tan", ",", "wenn", "ich", "schul\u00b7digst", "mich", "er\u00b7ke\u00b7cke"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "$(", "$,", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Vorzustell'n Dir meine hohen, staatsministeriellen R\u00f6cke.", "tokens": ["Vor\u00b7zu\u00b7stell'n", "Dir", "mei\u00b7ne", "ho\u00b7hen", ",", "staats\u00b7mi\u00b7nis\u00b7te\u00b7ri\u00b7el\u00b7len", "R\u00f6\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbr\u00f6cke, ja! So n\u00e4mlich hei\u00dfen", "tokens": ["\u00bb", "r\u00f6\u00b7cke", ",", "ja", "!", "So", "n\u00e4m\u00b7lich", "hei\u00b7\u00dfen"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "$,", "ADV", "$.", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die, die an dem Schiff des Staates sich als Ruderer beflei\u00dfen.", "tokens": ["Die", ",", "die", "an", "dem", "Schiff", "des", "Staa\u00b7tes", "sich", "als", "Ru\u00b7de\u00b7rer", "be\u00b7flei\u00b7\u00dfen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "APPR", "ART", "NN", "ART", "NN", "PRF", "KOUS", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Dieser hier ist der Finanzrock, der Marquis von Rumpel-Rampel;", "tokens": ["Die\u00b7ser", "hier", "ist", "der", "Fi\u00b7nanz\u00b7rock", ",", "der", "Mar\u00b7quis", "von", "Rum\u00b7pel\u00b7Ram\u00b7pel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "ART", "NN", "$,", "ART", "NN", "APPR", "NE", "$."], "meter": "+--+---+-++-+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Dieser fette Kerl der Kriegsrock, General von Wumpel-Wampel;", "tokens": ["Die\u00b7ser", "fet\u00b7te", "Kerl", "der", "Kriegs\u00b7rock", ",", "Ge\u00b7ne\u00b7ral", "von", "Wum\u00b7pel\u00b7Wam\u00b7pel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "ART", "NN", "$,", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Jener ist der Rock des Cultus, Ober-Mufti Lumpel-Lampel,", "tokens": ["Je\u00b7ner", "ist", "der", "Rock", "des", "Cul\u00b7tus", ",", "O\u00b7ber\u00b7Muf\u00b7ti", "Lum\u00b7pel\u00b7Lam\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "VAFIN", "ART", "NN", "ART", "NN", "$,", "NE", "NE", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Neben ihm der Rock der Pr\u00fcgel, der Baron von Krumpel-Krampel;", "tokens": ["Ne\u00b7ben", "ihm", "der", "Rock", "der", "Pr\u00fc\u00b7gel", ",", "der", "Ba\u00b7ron", "von", "Krum\u00b7pel\u00b7Kram\u00b7pel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Dieser hier der Rock des Aeu\u00dfern, Excellenz von Schlumpel-Schlampel;", "tokens": ["Die\u00b7ser", "hier", "der", "Rock", "des", "A\u00b7e\u00b7u\u00b7\u00dfern", ",", "Ex\u00b7cel\u00b7lenz", "von", "Schlum\u00b7pel\u00b7Schlam\u00b7pel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "ART", "NN", "$,", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Endlich hier der Rock der Krone, Pr\u00e4sident von Humpel-Hampel:", "tokens": ["End\u00b7lich", "hier", "der", "Rock", "der", "Kro\u00b7ne", ",", "Pr\u00e4\u00b7si\u00b7dent", "von", "Hum\u00b7pel\u00b7Ham\u00b7pel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN", "$,", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.9": {"text": "S\u00e4mmtlich Diener ihres Herren, ihres Sultans Pumpel-Pampel.\u00ab", "tokens": ["S\u00e4mmt\u00b7lich", "Die\u00b7ner", "ih\u00b7res", "Her\u00b7ren", ",", "ih\u00b7res", "Sul\u00b7tans", "Pum\u00b7pel\u00b7Pam\u00b7pel", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "NN", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "\u00bbaber, wie ist's m\u00f6glich\u00ab, rief ich, \u00bbda\u00df die Namen der geheimen", "tokens": ["\u00bb", "a\u00b7ber", ",", "wie", "ist's", "m\u00f6g\u00b7lich", "\u00ab", ",", "rief", "ich", ",", "\u00bb", "da\u00df", "die", "Na\u00b7men", "der", "ge\u00b7hei\u00b7men"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "$,", "PWAV", "VAFIN", "ADJD", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "KOUS", "ART", "NN", "ART", "ADJA"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "R\u00e4the just sich, dummer Herrscher, alle auf den Deinen reimen?\u00ab", "tokens": ["R\u00e4\u00b7the", "just", "sich", ",", "dum\u00b7mer", "Herr\u00b7scher", ",", "al\u00b7le", "auf", "den", "Dei\u00b7nen", "rei\u00b7men", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PRF", "$,", "ADJA", "NN", "$,", "PIS", "APPR", "ART", "PPOSAT", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "\u00bbdiese Frage, nimm's nicht \u00fcbel,\u00ab sprach, indem er drei Mal schlug", "tokens": ["\u00bb", "die\u00b7se", "Fra\u00b7ge", ",", "nim\u00b7m's", "nicht", "\u00fc\u00b7bel", ",", "\u00ab", "sprach", ",", "in\u00b7dem", "er", "drei", "Mal", "schlug"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PDAT", "NN", "$,", "ADV", "PTKNEG", "ADJD", "$,", "$(", "VVFIN", "$,", "KOUS", "PPER", "CARD", "NN", "VVFIN"], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "\u00bbvor die Stirne mir, der Sultan lachend: ist ein wenig", "tokens": ["\u00bb", "vor", "die", "Stir\u00b7ne", "mir", ",", "der", "Sul\u00b7tan", "la\u00b7chend", ":", "ist", "ein", "we\u00b7nig"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "APPR", "ART", "NN", "PPER", "$,", "ART", "NN", "ADJD", "$.", "VAFIN", "ART", "PIS"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Wer in unsrer Welt gereimt ist auf den Namen des Tyrannen,", "tokens": ["Wer", "in", "uns\u00b7rer", "Welt", "ge\u00b7reimt", "ist", "auf", "den", "Na\u00b7men", "des", "Ty\u00b7ran\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "VVPP", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Der geh\u00f6rt zum hohen Adel, der geh\u00f6rt zu seinen Mannen,", "tokens": ["Der", "ge\u00b7h\u00f6rt", "zum", "ho\u00b7hen", "A\u00b7del", ",", "der", "ge\u00b7h\u00f6rt", "zu", "sei\u00b7nen", "Man\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "NN", "$,", "PRELS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Und die glattesten und besten unter diesen Reimen, die", "tokens": ["Und", "die", "glat\u00b7tes\u00b7ten", "und", "bes\u00b7ten", "un\u00b7ter", "die\u00b7sen", "Rei\u00b7men", ",", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "ADJA", "KON", "VVFIN", "APPR", "PDAT", "NN", "$,", "PRELS"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Kommen, abgesehn von Tugend, F\u00e4higkeiten und Genie,", "tokens": ["Kom\u00b7men", ",", "ab\u00b7ge\u00b7sehn", "von", "Tu\u00b7gend", ",", "F\u00e4\u00b7hig\u00b7kei\u00b7ten", "und", "Ge\u00b7nie", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Und wenn sie auch t\u00e4glich sch\u00f6ssen kluge und gescheidte B\u00f6cke,", "tokens": ["Und", "wenn", "sie", "auch", "t\u00e4g\u00b7lich", "sch\u00f6s\u00b7sen", "klu\u00b7ge", "und", "ge\u00b7scheid\u00b7te", "B\u00f6\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADJD", "VVFIN", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "--+-+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Dieser Census ist poetisch und verhindert abgefeimtes", "tokens": ["Die\u00b7ser", "Cen\u00b7sus", "ist", "po\u00b7e\u00b7tisch", "und", "ver\u00b7hin\u00b7dert", "ab\u00b7ge\u00b7feim\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VAFIN", "ADJD", "KON", "VVPP", "ADJA"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Und ehrgeiz'ges Stellenjagen, und da\u00df einmal Ungereimtes", "tokens": ["Und", "ehr\u00b7gei\u00b7z'\u00b7ges", "Stel\u00b7len\u00b7ja\u00b7gen", ",", "und", "da\u00df", "ein\u00b7mal", "Un\u00b7ge\u00b7reim\u00b7tes"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "KON", "KOUS", "ADV", "NN"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.10": {"text": "In der Administration sich k\u00f6nnte irgendwie ereignen,", "tokens": ["In", "der", "Ad\u00b7mi\u00b7nist\u00b7ra\u00b7ti\u00b7on", "sich", "k\u00f6nn\u00b7te", "ir\u00b7gend\u00b7wie", "er\u00b7eig\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VMFIN", "ADV", "VVINF", "$,"], "meter": "--+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "Da ja alle meine Ordern stets ein Rock mu\u00df unterzeichnen.", "tokens": ["Da", "ja", "al\u00b7le", "mei\u00b7ne", "Or\u00b7dern", "stets", "ein", "Rock", "mu\u00df", "un\u00b7ter\u00b7zeich\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "PPOSAT", "NN", "ADV", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Endlich ist dies Grundgesetz auch n\u00fctzlich bei Revolutionen,", "tokens": ["End\u00b7lich", "ist", "dies", "Grund\u00b7ge\u00b7setz", "auch", "n\u00fctz\u00b7lich", "bei", "Re\u00b7vo\u00b7lu\u00b7ti\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDS", "NN", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-+--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.13": {"text": "Lie\u00df das Volk, wie's auch emp\u00f6rt war, stets die Form des Staates schonen;", "tokens": ["Lie\u00df", "das", "Volk", ",", "wie's", "auch", "em\u00b7p\u00f6rt", "war", ",", "stets", "die", "Form", "des", "Staa\u00b7tes", "scho\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ADV", "ADJD", "VAFIN", "$,", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Alles blieb, so wie's gewesen, nur da\u00df mit dem andern ", "tokens": ["Al\u00b7les", "blieb", ",", "so", "wie's", "ge\u00b7we\u00b7sen", ",", "nur", "da\u00df", "mit", "dem", "an\u00b7dern"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "ADV", "ADJA", "VAPP", "$,", "ADV", "KOUS", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.15": {"text": "Den man auf den Thron schob, andre Reime an das Ruder kamen,", "tokens": ["Den", "man", "auf", "den", "Thron", "schob", ",", "and\u00b7re", "Rei\u00b7me", "an", "das", "Ru\u00b7der", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "ART", "NN", "VVFIN", "$,", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Ohne R\u00fccksicht auf Charakter, Dummheit, Tugend und Genie,", "tokens": ["Oh\u00b7ne", "R\u00fcck\u00b7sicht", "auf", "Cha\u00b7rak\u00b7ter", ",", "Dumm\u00b7heit", ",", "Tu\u00b7gend", "und", "Ge\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Ausgew\u00e4hlt nur nach der h\u00f6hern Eigennamen-Poesie.", "tokens": ["Aus\u00b7ge\u00b7w\u00e4hlt", "nur", "nach", "der", "h\u00f6\u00b7hern", "Ei\u00b7gen\u00b7na\u00b7men\u00b7Po\u00b7e\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Liest Du dieser Welt Geschichte, findst Du Knubbiacks, Knipse, Tapse,", "tokens": ["Liest", "Du", "die\u00b7ser", "Welt", "Ge\u00b7schich\u00b7te", ",", "findst", "Du", "Knub\u00b7biacks", ",", "Knip\u00b7se", ",", "Tap\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "NN", "$,", "VVFIN", "PPER", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.19": {"text": "Nichts von einem Volk', nur einzeln lauter Schubbiaks, Fips' und Flapse;", "tokens": ["Nichts", "von", "ei\u00b7nem", "Vol\u00b7k'", ",", "nur", "ein\u00b7zeln", "lau\u00b7ter", "Schub\u00b7bi\u00b7aks", ",", "Fips'", "und", "Flap\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "$,", "ADV", "ADJD", "PIAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+--+-+-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.20": {"text": "So regierte vor Zehn Jahren noch ein Herrscher Namens Kornox,", "tokens": ["So", "re\u00b7gier\u00b7te", "vor", "Zehn", "Jah\u00b7ren", "noch", "ein", "Herr\u00b7scher", "Na\u00b7mens", "Kor\u00b7nox", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "CARD", "NN", "ADV", "ART", "ADJA", "NN", "NE", "$,"], "meter": "+-+-+-+-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.21": {"text": "Dessen Oberrock reimmetrisch-selbstverst\u00e4ndlich war ein Hornochs.\u00ab", "tokens": ["Des\u00b7sen", "O\u00b7ber\u00b7rock", "reim\u00b7me\u00b7trischselbst\u00b7ver\u00b7st\u00e4nd\u00b7lich", "war", "ein", "Hor\u00b7nochs", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDAT", "NN", "ADJD", "VAFIN", "ART", "NN", "$.", "$("], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbhab' ich recht geh\u00f6rt, o Sultan, hocherhab'ner Dummheitsquell!", "tokens": ["\u00bb", "hab'", "ich", "recht", "ge\u00b7h\u00f6rt", ",", "o", "Sul\u00b7tan", ",", "hoch\u00b7er\u00b7hab'\u00b7ner", "Dumm\u00b7heits\u00b7quell", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "ADJD", "VVFIN", "$,", "FM", "FM", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measu