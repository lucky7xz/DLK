{"dta.poem.8562": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Herr, mein Herr, Du bist sehr herrlich!", "tokens": ["Herr", ",", "mein", "Herr", ",", "Du", "bist", "sehr", "herr\u00b7lich", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Alle G\u00f6tter der Welt sind G\u00f6tzen.", "tokens": ["Al\u00b7le", "G\u00f6t\u00b7ter", "der", "Welt", "sind", "G\u00f6t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VAFIN", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Nur Du nicht!", "tokens": ["Nur", "Du", "nicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKNEG", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Meine Lippen preisen Dich.", "tokens": ["Mei\u00b7ne", "Lip\u00b7pen", "prei\u00b7sen", "Dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Du bist gn\u00e4dig und gerecht, Du bist barmherzig!", "tokens": ["Du", "bist", "gn\u00e4\u00b7dig", "und", "ge\u00b7recht", ",", "Du", "bist", "barm\u00b7her\u00b7zig", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.6": {"line.1": {"text": "Ich f\u00fcrchte mich nicht vor Dir, dass mir die Haut schaudert,", "tokens": ["Ich", "f\u00fcrch\u00b7te", "mich", "nicht", "vor", "Dir", ",", "dass", "mir", "die", "Haut", "schau\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "und entsetze mich nicht vor Deinen Rechten.", "tokens": ["und", "ent\u00b7set\u00b7ze", "mich", "nicht", "vor", "Dei\u00b7nen", "Rech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Deine Gedanken sind so sehr tief!", "tokens": ["Dei\u00b7ne", "Ge\u00b7dan\u00b7ken", "sind", "so", "sehr", "tief", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.8": {"line.1": {"text": "Du drehst die Kurbel,", "tokens": ["Du", "drehst", "die", "Kur\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "die mich um diese S\u00e4ule quetscht,", "tokens": ["die", "mich", "um", "die\u00b7se", "S\u00e4u\u00b7le", "quetscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du hast ihre Scherben, Du hast ihre Rasirmesser gewetzt,", "tokens": ["Du", "hast", "ih\u00b7re", "Scher\u00b7ben", ",", "Du", "hast", "ih\u00b7re", "Ra\u00b7sir\u00b7mes\u00b7ser", "ge\u00b7wetzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-+-+---+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Du bist so allg\u00fctig . . .", "tokens": ["Du", "bist", "so", "all\u00b7g\u00fc\u00b7tig", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$.", "$.", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Sieh!", "tokens": ["Sieh", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}}, "stanza.10": {"line.1": {"text": "Auf Fl\u00fcgeln,", "tokens": ["Auf", "Fl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "die wie Silber und Gold schimmern,", "tokens": ["die", "wie", "Sil\u00b7ber", "und", "Gold", "schim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "in weissen Gew\u00e4ndern,", "tokens": ["in", "weis\u00b7sen", "Ge\u00b7w\u00e4n\u00b7dern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Rosen im Haar,", "tokens": ["Ro\u00b7sen", "im", "Haar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "schweben", "tokens": ["schwe\u00b7ben"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "um dieses gl\u00fchende, zuckende Fleisch", "tokens": ["um", "die\u00b7ses", "gl\u00fc\u00b7hen\u00b7de", ",", "zu\u00b7cken\u00b7de", "Fleisch"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "all Deine Engel.", "tokens": ["all", "Dei\u00b7ne", "En\u00b7gel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "Singend, jubelnd,", "tokens": ["Sin\u00b7gend", ",", "ju\u00b7belnd", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "in Millionen Schaalen,", "tokens": ["in", "Mil\u00b7lion\u00b7en", "Schaa\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.3": {"text": "sammeln sie meine Freudentr\u00e4nen!", "tokens": ["sam\u00b7meln", "sie", "mei\u00b7ne", "Freu\u00b7den\u00b7tr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Halleluja!!", "tokens": ["Hal\u00b7le\u00b7lu\u00b7ja", "!!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}