{"textgrid.poem.43780": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wurst wider Wurst", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sobald der erste Frost tritt ein,", "tokens": ["So\u00b7bald", "der", "ers\u00b7te", "Frost", "tritt", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wird geschlachtet unser Schwein:", "tokens": ["So", "wird", "ge\u00b7schlach\u00b7tet", "un\u00b7ser", "Schwein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Quiek quiek quiek quiek quiek quiek guiek,", "tokens": ["Qui\u00b7ek", "qui\u00b7ek", "qui\u00b7ek", "qui\u00b7ek", "qui\u00b7ek", "qui\u00b7ek", "gu\u00b7i\u00b7ek", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+-+-+-+--+", "measure": "iambic.septa.invert"}, "line.4": {"text": "So wird geschlachtet unser Schwein.", "tokens": ["So", "wird", "ge\u00b7schlach\u00b7tet", "un\u00b7ser", "Schwein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dann wird verschiedne Wurst gemacht,", "tokens": ["Dann", "wird", "ver\u00b7schied\u00b7ne", "Wurst", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df drob das Herz im Leibe lacht:", "tokens": ["Da\u00df", "drob", "das", "Herz", "im", "Lei\u00b7be", "lacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gr\u00fctz-, Knack-, Mett-, Schlack-, Leberwurst,", "tokens": ["Gr\u00fctz", ",", "Knack", ",", "Met\u00b7t", ",", "Schlack", ",", "Le\u00b7ber\u00b7wurst", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["TRUNC", "$,", "TRUNC", "$,", "TRUNC", "$,", "TRUNC", "$,", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Da\u00df drob das Herz im Leibe lacht.", "tokens": ["Da\u00df", "drob", "das", "Herz", "im", "Lei\u00b7be", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dann schickt die Mutter mich hinaus:", "tokens": ["Dann", "schickt", "die", "Mut\u00b7ter", "mich", "hin\u00b7aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geh' flink und bring dem Nachbar Claus", "tokens": ["Geh'", "flink", "und", "bring", "dem", "Nach\u00b7bar", "Claus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gr\u00fctz-, Knack-, Mett-, Schlack-, Leberwurst,", "tokens": ["Gr\u00fctz", ",", "Knack", ",", "Met\u00b7t", ",", "Schlack", ",", "Le\u00b7ber\u00b7wurst", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["TRUNC", "$,", "TRUNC", "$,", "TRUNC", "$,", "TRUNC", "$,", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und gr\u00fc\u00df' ihn und sein ganzes Haus!", "tokens": ["Und", "gr\u00fc\u00df'", "ihn", "und", "sein", "gan\u00b7zes", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und schlachtet dann der Nachbar ein,", "tokens": ["Und", "schlach\u00b7tet", "dann", "der", "Nach\u00b7bar", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So macht er W\u00fcrste gro\u00df und klein,", "tokens": ["So", "macht", "er", "W\u00fcrs\u00b7te", "gro\u00df", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gr\u00fctz-, Knack-, Mett-, Schlack-, Leberwurst,", "tokens": ["Gr\u00fctz", ",", "Knack", ",", "Met\u00b7t", ",", "Schlack", ",", "Le\u00b7ber\u00b7wurst", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["TRUNC", "$,", "TRUNC", "$,", "TRUNC", "$,", "TRUNC", "$,", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Vergleichen W\u00fcrste gro\u00df und klein.", "tokens": ["Ver\u00b7glei\u00b7chen", "W\u00fcrs\u00b7te", "gro\u00df", "und", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Nachbar Claus vergi\u00dft uns nicht,", "tokens": ["Der", "Nach\u00b7bar", "Claus", "ver\u00b7gi\u00dft", "uns", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er schickt uns, wie das Sprichwort spricht,", "tokens": ["Er", "schickt", "uns", ",", "wie", "das", "Sprich\u00b7wort", "spricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wurst, Wurst, Wurst, Wurst wider Wurst,", "tokens": ["Wurst", ",", "Wurst", ",", "Wurst", ",", "Wurst", "wi\u00b7der", "Wurst", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "VAFIN", "$,", "VAFIN", "$,", "VAFIN", "APPR", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und das ist auch so \u00fcbel nicht.", "tokens": ["Und", "das", "ist", "auch", "so", "\u00fc\u00b7bel", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "ADV", "ADJD", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}