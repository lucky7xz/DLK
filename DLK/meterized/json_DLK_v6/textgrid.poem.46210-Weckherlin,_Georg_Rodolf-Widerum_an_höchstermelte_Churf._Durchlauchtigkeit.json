{"textgrid.poem.46210": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Widerum an h\u00f6chstermelte Churf. Durchlauchtigkeit", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ohn m\u00fch und schwei\u00df kan man kein werk vollf\u00fchren,", "tokens": ["Ohn", "m\u00fch", "und", "schwei\u00df", "kan", "man", "kein", "werk", "voll\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "KON", "ADJD", "VMFIN", "PIS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "werk, welches uns mit lob und ehr kan zieren,", "tokens": ["werk", ",", "wel\u00b7ches", "uns", "mit", "lob", "und", "ehr", "kan", "zie\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PRELS", "PPER", "APPR", "NN", "KON", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "dan nur um m\u00fch der tugend preis und pracht", "tokens": ["dan", "nur", "um", "m\u00fch", "der", "tu\u00b7gend", "preis", "und", "pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ADJD", "ART", "NN", "PTKVZ", "KON", "NN"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.4": {"text": "und wahrer ruhm zu wegen wird gebracht,", "tokens": ["und", "wah\u00b7rer", "ruhm", "zu", "we\u00b7gen", "wird", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKZU", "VVINF", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "da\u00df ewiglich sie sich nicht mehr verlieren.", "tokens": ["da\u00df", "e\u00b7wig\u00b7lich", "sie", "sich", "nicht", "mehr", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "PRF", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Kein herr, kein held, zu der ehr zu passieren", "tokens": ["Kein", "herr", ",", "kein", "held", ",", "zu", "der", "ehr", "zu", "pas\u00b7sie\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "VVFIN", "$,", "APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "und \u00fcber not und tod zu jubilieren,", "tokens": ["und", "\u00fc\u00b7ber", "not", "und", "tod", "zu", "ju\u00b7bi\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "kan sigreich sein durch ein glorreiche schlacht", "tokens": ["kan", "sig\u00b7reich", "sein", "durch", "ein", "glor\u00b7rei\u00b7che", "schlacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "VAINF", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.9": {"text": "ohn m\u00fch und schwei\u00df.", "tokens": ["ohn", "m\u00fch", "und", "schwei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Wolan dan, prinz, wan schon die feind stolzieren", "tokens": ["Wo\u00b7lan", "dan", ",", "prinz", ",", "wan", "schon", "die", "feind", "stol\u00b7zie\u00b7ren"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "NE", "$,", "PWAV", "ADV", "ART", "NN", "VVINF"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "und ungerecht als torecht dominieren", "tokens": ["und", "un\u00b7ge\u00b7recht", "als", "to\u00b7recht", "do\u00b7mi\u00b7nie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "in euerm land, wird doch der tugend macht,", "tokens": ["in", "eu\u00b7erm", "land", ",", "wird", "doch", "der", "tu\u00b7gend", "macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VAFIN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "die euer liecht in euers ungl\u00fccks nacht,", "tokens": ["die", "eu\u00b7er", "liecht", "in", "eu\u00b7ers", "un\u00b7gl\u00fccks", "nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "endlich mit euch hie und dort triumfieren", "tokens": ["end\u00b7lich", "mit", "euch", "hie", "und", "dort", "tri\u00b7um\u00b7fie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "ADV", "KON", "ADV", "VVINF"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "ohn m\u00fch und schwei\u00df.", "tokens": ["ohn", "m\u00fch", "und", "schwei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}