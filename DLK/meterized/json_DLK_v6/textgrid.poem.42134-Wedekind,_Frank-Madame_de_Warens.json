{"textgrid.poem.42134": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Madame de Warens", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich soll ihn lassen", "tokens": ["Ich", "soll", "ihn", "las\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Und kann's nicht fassen;", "tokens": ["Und", "kann's", "nicht", "fas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und du, mein Herz,", "tokens": ["Und", "du", ",", "mein", "Herz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Du darfst es wagen,", "tokens": ["Du", "darfst", "es", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Noch fort zu schlagen", "tokens": ["Noch", "fort", "zu", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "PTKZU", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Bei solchem Schmerz?", "tokens": ["Bei", "sol\u00b7chem", "Schmerz", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}