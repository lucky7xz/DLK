{"dta.poem.11851": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "28.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Alswie ein Thor, der wohnt im Haus mit einem Weisen,", "tokens": ["Als\u00b7wie", "ein", "Thor", ",", "der", "wohnt", "im", "Haus", "mit", "ei\u00b7nem", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Weisheit Einflu\u00df f\u00fchlt in seiner Thorheit Kreisen,", "tokens": ["Der", "Weis\u00b7heit", "Ein\u00b7flu\u00df", "f\u00fchlt", "in", "sei\u00b7ner", "Thor\u00b7heit", "Krei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und sich vorm Nachbar sch\u00e4mt, was dem misf\u00e4llt, zu thun;", "tokens": ["Und", "sich", "vorm", "Nach\u00b7bar", "sch\u00e4mt", ",", "was", "dem", "mis\u00b7f\u00e4llt", ",", "zu", "thun", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PRF", "APPRART", "NN", "VVFIN", "$,", "PRELS", "ART", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wohnt auch ja dein Leib so nah dem Geiste nun.", "tokens": ["So", "wohnt", "auch", "ja", "dein", "Leib", "so", "nah", "dem", "Geis\u00b7te", "nun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Thor wird zwar ein Thor vorm Weisen immer bleiben,", "tokens": ["Der", "Thor", "wird", "zwar", "ein", "Thor", "vorm", "Wei\u00b7sen", "im\u00b7mer", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "APPRART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ihm zum Aergernis sein Thorenspiel nicht treiben.", "tokens": ["Doch", "ihm", "zum", "A\u00b7er\u00b7ger\u00b7nis", "sein", "Tho\u00b7ren\u00b7spiel", "nicht", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}