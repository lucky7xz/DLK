{"textgrid.poem.66854": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vor meinem Fenster schr\u00e4g empor", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor meinem Fenster schr\u00e4g empor", "tokens": ["Vor", "mei\u00b7nem", "Fens\u00b7ter", "schr\u00e4g", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "steigt eines grauen Hauses Wand,", "tokens": ["steigt", "ei\u00b7nes", "grau\u00b7en", "Hau\u00b7ses", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hart an ihr dr\u00fcckt den Blick ein Baum,", "tokens": ["Hart", "an", "ihr", "dr\u00fcckt", "den", "Blick", "ein", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "kohlschwarz und wie von Glut verbrannt.", "tokens": ["kohl\u00b7schwarz", "und", "wie", "von", "Glut", "ver\u00b7brannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "PWAV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich seh den Baum, ich seh die Wand,", "tokens": ["Ich", "seh", "den", "Baum", ",", "ich", "seh", "die", "Wand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "das ist so schwarz und grau und tot,", "tokens": ["das", "ist", "so", "schwarz", "und", "grau", "und", "tot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Auge z\u00fcrnt und flieht und sucht", "tokens": ["Mein", "Au\u00b7ge", "z\u00fcrnt", "und", "flieht", "und", "sucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "ein lebend Gr\u00fcn, ein leuchtend Rot.", "tokens": ["ein", "le\u00b7bend", "Gr\u00fcn", ",", "ein", "leuch\u00b7tend", "Rot", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ART", "ADJD", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Da sieh! Im Winkel fast versteckt", "tokens": ["Da", "sieh", "!", "Im", "Win\u00b7kel", "fast", "ver\u00b7steckt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "$.", "APPRART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "winkt mir ein sprossendes Geb\u00fcsch,", "tokens": ["winkt", "mir", "ein", "spros\u00b7sen\u00b7des", "Ge\u00b7b\u00fcsch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Mit welcher Lust hab ich's entdeckt!", "tokens": ["Mit", "wel\u00b7cher", "Lust", "hab", "ich's", "ent\u00b7deckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Wie wird mir frei und froh und frisch!", "tokens": ["Wie", "wird", "mir", "frei", "und", "froh", "und", "frisch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Aus jedem \u00c4stchen schl\u00fcpft hervor", "tokens": ["Aus", "je\u00b7dem", "\u00c4st\u00b7chen", "schl\u00fcpft", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "und trinkt den Tropfen, der ihm tr\u00e4uft,", "tokens": ["und", "trinkt", "den", "Trop\u00b7fen", ",", "der", "ihm", "tr\u00e4uft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ein Bl\u00e4ttchenvolk, ein gr\u00fcner Quirl,", "tokens": ["Ein", "Bl\u00e4tt\u00b7chen\u00b7volk", ",", "ein", "gr\u00fc\u00b7ner", "Quirl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "dem Wonne durch die Adern l\u00e4uft", "tokens": ["dem", "Won\u00b7ne", "durch", "die", "A\u00b7dern", "l\u00e4uft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Das ist der sch\u00f6nste Fr\u00fchling ja,", "tokens": ["Das", "ist", "der", "sch\u00f6ns\u00b7te", "Fr\u00fch\u00b7ling", "ja", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "PTKANT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "der mir vom Hofe Meldung bringt,", "tokens": ["der", "mir", "vom", "Ho\u00b7fe", "Mel\u00b7dung", "bringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und morgen ist der Mai schon da,", "tokens": ["Und", "mor\u00b7gen", "ist", "der", "Mai", "schon", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "der hold an meine Seele klingt.", "tokens": ["der", "hold", "an", "mei\u00b7ne", "See\u00b7le", "klingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Der erste Mai, der erste Mai,", "tokens": ["Der", "ers\u00b7te", "Mai", ",", "der", "ers\u00b7te", "Mai", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "nun lebt es gr\u00fcn, nun leuchtet's rot,", "tokens": ["nun", "lebt", "es", "gr\u00fcn", ",", "nun", "leuch\u00b7tet's", "rot", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Nun seh ich kein Kasernengrau,", "tokens": ["Nun", "seh", "ich", "kein", "Ka\u00b7ser\u00b7nen\u00b7grau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "kein kahler Baum ragt schwarz und tot.", "tokens": ["kein", "kah\u00b7ler", "Baum", "ragt", "schwarz", "und", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Der erste Mai, der gr\u00fcne Mai,", "tokens": ["Der", "ers\u00b7te", "Mai", ",", "der", "gr\u00fc\u00b7ne", "Mai", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "von roten Wimpeln flutet's drein,", "tokens": ["von", "ro\u00b7ten", "Wim\u00b7peln", "flu\u00b7tet's", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Nun bin ich frisch und froh und frei", "tokens": ["Nun", "bin", "ich", "frisch", "und", "froh", "und", "frei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "und will mein Lied dem Fr\u00fchling weihn.", "tokens": ["und", "will", "mein", "Lied", "dem", "Fr\u00fch\u00b7ling", "weihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dem Menschheitsfr\u00fchling will ich's weihn,", "tokens": ["Dem", "Menschheits\u00b7fr\u00fch\u00b7ling", "will", "ich's", "weihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "der jubelnd durch die Lande zieht,", "tokens": ["der", "ju\u00b7belnd", "durch", "die", "Lan\u00b7de", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Hoffnung in die Herzen gie\u00dft,", "tokens": ["Der", "Hoff\u00b7nung", "in", "die", "Her\u00b7zen", "gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "da\u00df V\u00f6lkerfrost und Elend flieht.", "tokens": ["da\u00df", "V\u00f6l\u00b7ker\u00b7frost", "und", "E\u00b7lend", "flieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Sonne in die Seelen streut,", "tokens": ["Der", "Son\u00b7ne", "in", "die", "See\u00b7len", "streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "da\u00df sich verkriecht die Nacht der Not,", "tokens": ["da\u00df", "sich", "ver\u00b7kriecht", "die", "Nacht", "der", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Bl\u00fcten lockt am Lebensbaum", "tokens": ["Der", "Bl\u00fc\u00b7ten", "lockt", "am", "Le\u00b7bens\u00b7baum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "mit siegesl\u00e4utendem Gebot.", "tokens": ["mit", "sie\u00b7ges\u00b7l\u00e4u\u00b7ten\u00b7dem", "Ge\u00b7bot", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Nun kommt zuhauf und feiert all,", "tokens": ["Nun", "kommt", "zu\u00b7hauf", "und", "fei\u00b7ert", "all", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "KON", "VVFIN", "PIAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "die ihr mit Hirn und Muskel schafft,", "tokens": ["die", "ihr", "mit", "Hirn", "und", "Mus\u00b7kel", "schafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Nun lobt mit lautem Jubelschall", "tokens": ["Nun", "lobt", "mit", "lau\u00b7tem", "Ju\u00b7bel\u00b7schall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "den Siegeszug der Arbeitskraft!", "tokens": ["den", "Sie\u00b7ges\u00b7zug", "der", "Ar\u00b7beits\u00b7kraft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Das ist ein herrlich Hochzeitsfest", "tokens": ["Das", "ist", "ein", "herr\u00b7lich", "Hoch\u00b7zeits\u00b7fest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "der triumphierenden Idee,", "tokens": ["der", "tri\u00b7um\u00b7phie\u00b7ren\u00b7den", "I\u00b7dee", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Wie bin ich fr\u00f6hlich bis ins Mark,", "tokens": ["Wie", "bin", "ich", "fr\u00f6h\u00b7lich", "bis", "ins", "Mark", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "da\u00df ich den Flug der Freiheit seh!", "tokens": ["da\u00df", "ich", "den", "Flug", "der", "Frei\u00b7heit", "seh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Einst m\u00fchsam nur, mit schwerem Schlag,", "tokens": ["Einst", "m\u00fch\u00b7sam", "nur", ",", "mit", "schwe\u00b7rem", "Schlag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "langsam von Haupt zu Haupt sie zog,", "tokens": ["lang\u00b7sam", "von", "Haupt", "zu", "Haupt", "sie", "zog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.19": {"text": "Heut mit gewaltigem Fittich schwebt", "tokens": ["Heut", "mit", "ge\u00b7wal\u00b7ti\u00b7gem", "Fit\u00b7tich", "schwebt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "sie durch der Massen Weltgewog.", "tokens": ["sie", "durch", "der", "Mas\u00b7sen", "Welt\u00b7ge\u00b7wog", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Und was der klaren Denker Mut", "tokens": ["Und", "was", "der", "kla\u00b7ren", "Den\u00b7ker", "Mut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "als waltendes Gesetz erkannt,", "tokens": ["als", "wal\u00b7ten\u00b7des", "Ge\u00b7setz", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Nun wird es Fleisch, nun wird es Blut,", "tokens": ["Nun", "wird", "es", "Fleisch", ",", "nun", "wird", "es", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "ADV", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "nun wird es Mensch von Land zu Land.", "tokens": ["nun", "wird", "es", "Mensch", "von", "Land", "zu", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Un\u00fcberwindlich gro\u00df und stark,", "tokens": ["Un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7lich", "gro\u00df", "und", "stark", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "so w\u00e4chst der Wahrheit Wort zur Tat,", "tokens": ["so", "w\u00e4chst", "der", "Wahr\u00b7heit", "Wort", "zur", "Tat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Wie bin ich fr\u00f6hlich bis ins Mark!", "tokens": 