{"textgrid.poem.58383": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "421. Frei und doch gebunden", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von allen Dingen frei", "tokens": ["Von", "al\u00b7len", "Din\u00b7gen", "frei"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "An Gott gebunden sei", "tokens": ["An", "Gott", "ge\u00b7bun\u00b7den", "sei"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit allen Liebeskr\u00e4ften!", "tokens": ["Mit", "al\u00b7len", "Lie\u00b7bes\u00b7kr\u00e4f\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Stehst du in Wahrheit so,", "tokens": ["Stehst", "du", "in", "Wahr\u00b7heit", "so", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "So bleibst du still und froh", "tokens": ["So", "bleibst", "du", "still", "und", "froh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Allein und in Gesch\u00e4ften.", "tokens": ["Al\u00b7lein", "und", "in", "Ge\u00b7sch\u00e4f\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}