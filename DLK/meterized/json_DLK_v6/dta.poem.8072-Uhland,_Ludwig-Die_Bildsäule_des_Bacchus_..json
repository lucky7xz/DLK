{"dta.poem.8072": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Bilds\u00e4ule des Bacchus .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Kallisthenes, ein J\u00fcngling zu Athen,", "tokens": ["Kal\u00b7list\u00b7he\u00b7nes", ",", "ein", "J\u00fcng\u00b7ling", "zu", "A\u00b7then", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "APPR", "NE", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Kam einst, nach einer durchgeschw\u00e4rmten Nacht,", "tokens": ["Kam", "einst", ",", "nach", "ei\u00b7ner", "durch\u00b7ge\u00b7schw\u00e4rm\u00b7ten", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den welken Epheukranz um\u2019s wilde Haar,", "tokens": ["Den", "wel\u00b7ken", "E\u00b7pheu\u00b7kranz", "um's", "wil\u00b7de", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hintaumelnd in der D\u00e4mmerung, nach Haus,", "tokens": ["Hin\u00b7tau\u00b7melnd", "in", "der", "D\u00e4m\u00b7me\u00b7rung", ",", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er selber, wie die D\u00e4mmrung, w\u00fcst und bleich.", "tokens": ["Er", "sel\u00b7ber", ",", "wie", "die", "D\u00e4mm\u00b7rung", ",", "w\u00fcst", "und", "bleich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PWAV", "ART", "NN", "$,", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Als nun der Diener nach dem Schlafgemach", "tokens": ["Als", "nun", "der", "Die\u00b7ner", "nach", "dem", "Schlaf\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ihm leuchtet durch den hohen S\u00e4ulengang,", "tokens": ["Ihm", "leuch\u00b7tet", "durch", "den", "ho\u00b7hen", "S\u00e4u\u00b7len\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Da tritt mit Eins im vollen Fackelschein", "tokens": ["Da", "tritt", "mit", "Eins", "im", "vol\u00b7len", "Fa\u00b7ckel\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Des Bacchus g\u00f6ttlich Marmorbild hervor,", "tokens": ["Des", "Bac\u00b7chus", "g\u00f6tt\u00b7lich", "Mar\u00b7mor\u00b7bild", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Von sch\u00f6pferischer Meisterhand geformt.", "tokens": ["Von", "sch\u00f6p\u00b7fe\u00b7ri\u00b7scher", "Meis\u00b7ter\u00b7hand", "ge\u00b7formt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "In Jugendf\u00fclle hebt sich die Gestalt,", "tokens": ["In", "Ju\u00b7gend\u00b7f\u00fcl\u00b7le", "hebt", "sich", "die", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Aus reichem, lang hinwallendem Gelock", "tokens": ["Aus", "rei\u00b7chem", ",", "lang", "hin\u00b7wal\u00b7len\u00b7dem", "Ge\u00b7lock"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ergl\u00e4nzt das feingew\u00f6lbte Schulternpaar,", "tokens": ["Er\u00b7gl\u00e4nzt", "das", "fein\u00b7ge\u00b7w\u00f6lb\u00b7te", "Schul\u00b7tern\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und unter\u2019m Schatten \u00fcppigen Geflechts", "tokens": ["Und", "un\u00b7ter'm", "Schat\u00b7ten", "\u00fcp\u00b7pi\u00b7gen", "Ge\u00b7flechts"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Von Rebenlaub und schwellender Traubenfrucht", "tokens": ["Von", "Re\u00b7ben\u00b7laub", "und", "schwel\u00b7len\u00b7der", "Trau\u00b7ben\u00b7frucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Erscheint das runde, bl\u00fchende Gesicht.", "tokens": ["Er\u00b7scheint", "das", "run\u00b7de", ",", "bl\u00fc\u00b7hen\u00b7de", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Erschrocken f\u00e4hrt Kallisthenes zur\u00fcck", "tokens": ["Er\u00b7schro\u00b7cken", "f\u00e4hrt", "Kal\u00b7list\u00b7he\u00b7nes", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "NE", "PTKVZ"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Vor der Erscheinung Herrlichkeit und Glanz,", "tokens": ["Vor", "der", "Er\u00b7schei\u00b7nung", "Herr\u00b7lich\u00b7keit", "und", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Ihm ist, als h\u00e4tte mit dem Thyrsusstab", "tokens": ["Ihm", "ist", ",", "als", "h\u00e4t\u00b7te", "mit", "dem", "Thyr\u00b7sus\u00b7stab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Der Gott die Stirne strafend ihm ber\u00fchrt,", "tokens": ["Der", "Gott", "die", "Stir\u00b7ne", "stra\u00b7fend", "ihm", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Als spr\u00e4che z\u00fcrnend der belebte Mund:", "tokens": ["Als", "spr\u00e4\u00b7che", "z\u00fcr\u00b7nend", "der", "be\u00b7leb\u00b7te", "Mund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "\u201ewas spuckst du hier, du wankendes Gespenst?", "tokens": ["\u201e", "was", "spuckst", "du", "hier", ",", "du", "wan\u00b7ken\u00b7des", "Ge\u00b7spenst", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Ereb\u2019scher Schatten, kraftlos, sinnbet\u00e4ubt!", "tokens": ["E\u00b7re\u00b7b'\u00b7scher", "Schat\u00b7ten", ",", "kraft\u00b7los", ",", "sinn\u00b7be\u00b7t\u00e4ubt", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.24": {"text": "Du hast den heil\u2019gen Epheu mir entweiht,", "tokens": ["Du", "hast", "den", "heil'\u00b7gen", "E\u00b7pheu", "mir", "ent\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Du nennest frevelnd meinen Priester dich;", "tokens": ["Du", "nen\u00b7nest", "fre\u00b7velnd", "mei\u00b7nen", "Pries\u00b7ter", "dich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Hinweg von mir! ich kenne deiner nicht.", "tokens": ["Hin\u00b7weg", "von", "mir", "!", "ich", "ken\u00b7ne", "dei\u00b7ner", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$.", "PPER", "VVFIN", "PPOSAT", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Ich bin die F\u00fclle schaffender Natur,", "tokens": ["Ich", "bin", "die", "F\u00fcl\u00b7le", "schaf\u00b7fen\u00b7der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Die sich besonders in dem edeln Blut", "tokens": ["Die", "sich", "be\u00b7son\u00b7ders", "in", "dem", "e\u00b7deln", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Der Rebe reich und g\u00f6ttlich offenbart.", "tokens": ["Der", "Re\u00b7be", "reich", "und", "g\u00f6tt\u00b7lich", "of\u00b7fen\u00b7bart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Will euer w\u00fcstes Treiben einen Gott,", "tokens": ["Will", "eu\u00b7er", "w\u00fcs\u00b7tes", "Trei\u00b7ben", "ei\u00b7nen", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "So sucht ihn nicht auf sonnigem Weingebirg,", "tokens": ["So", "sucht", "ihn", "nicht", "auf", "son\u00b7ni\u00b7gem", "Wein\u00b7ge\u00b7birg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Nein! sucht ihn drunten in des Hades Nacht!\u201c", "tokens": ["Nein", "!", "sucht", "ihn", "drun\u00b7ten", "in", "des", "Ha\u00b7des", "Nacht", "!", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "$.", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Der Gott verstummt, der Fackel Licht erlischt,", "tokens": ["Der", "Gott", "ver\u00b7stummt", ",", "der", "Fa\u00b7ckel", "Licht", "er\u00b7lischt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Der J\u00fcngling schleicht besch\u00e4mt in sein Gemach,", "tokens": ["Der", "J\u00fcng\u00b7ling", "schleicht", "be\u00b7sch\u00e4mt", "in", "sein", "Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Er nimmt vom Haupt den welken Epheukranz", "tokens": ["Er", "nimmt", "vom", "Haupt", "den", "wel\u00b7ken", "E\u00b7pheu\u00b7kranz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Und still in des Gem\u00fcthes Innerstem", "tokens": ["Und", "still", "in", "des", "Ge\u00b7m\u00fc\u00b7thes", "In\u00b7ners\u00b7tem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.37": {"text": "Beschw\u00f6ret er ein heiliges Gel\u00fcbd.", "tokens": ["Be\u00b7schw\u00f6\u00b7ret", "er", "ein", "hei\u00b7li\u00b7ges", "Ge\u00b7l\u00fcbd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}