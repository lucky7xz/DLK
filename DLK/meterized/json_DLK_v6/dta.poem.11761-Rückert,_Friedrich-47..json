{"dta.poem.11761": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "47.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vernimm die Fabeln, die ich nicht gefabelt habe;", "tokens": ["Ver\u00b7nimm", "die", "Fa\u00b7beln", ",", "die", "ich", "nicht", "ge\u00b7fa\u00b7belt", "ha\u00b7be", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als Mann erz\u00e4hl' ich dir, was ich geh\u00f6rt als Knabe.", "tokens": ["Als", "Mann", "er\u00b7z\u00e4hl'", "ich", "dir", ",", "was", "ich", "ge\u00b7h\u00f6rt", "als", "Kna\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "PPER", "$,", "PWS", "PPER", "VVFIN", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die zahme Ente schwamm auf ihrem Pfuhl zufrieden,", "tokens": ["Die", "zah\u00b7me", "En\u00b7te", "schwamm", "auf", "ih\u00b7rem", "Pfuhl", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo von dem Hausherrn ihr das Futter war beschieden.", "tokens": ["Wo", "von", "dem", "Haus\u00b7herrn", "ihr", "das", "Fut\u00b7ter", "war", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PPER", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die wilde Ente flog vorbei mit Lustgeschrei;", "tokens": ["Die", "wil\u00b7de", "En\u00b7te", "flog", "vor\u00b7bei", "mit", "Lust\u00b7ge\u00b7schrei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die zahme blickt hinauf, verwundert, was es sei?", "tokens": ["Die", "zah\u00b7me", "blickt", "hin\u00b7auf", ",", "ver\u00b7wun\u00b7dert", ",", "was", "es", "sei", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PTKVZ", "$,", "VVFIN", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "\u201emein wilder Vetter, ei, wohin?\u201c \u2014 Zur Quellenflut", "tokens": ["\u201e", "mein", "wil\u00b7der", "Vet\u00b7ter", ",", "ei", ",", "wo\u00b7hin", "?", "\u201c", "Zur", "Quel\u00b7len\u00b7flut"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "punct", "punct", "word", "word"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$,", "ITJ", "$,", "PWAV", "$.", "$(", "$(", "APPRART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf Bergen, weil das Land versengt hat Sommerglut.", "tokens": ["Auf", "Ber\u00b7gen", ",", "weil", "das", "Land", "ver\u00b7sengt", "hat", "Som\u00b7mer\u00b7glut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "ART", "NN", "VVPP", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "\u201ezu Quellen? ei! kennst du die Quellen, warst du dort?\u201c", "tokens": ["\u201e", "zu", "Quel\u00b7len", "?", "ei", "!", "kennst", "du", "die", "Quel\u00b7len", ",", "warst", "du", "dort", "?", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "NN", "$.", "NE", "$.", "VVFIN", "PPER", "ART", "NN", "$,", "VAFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich nicht, die Mutter wars, und nach ihr zieht michs fort.", "tokens": ["Ich", "nicht", ",", "die", "Mut\u00b7ter", "wars", ",", "und", "nach", "ihr", "zieht", "michs", "fort", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "$,", "ART", "NN", "VAFIN", "$,", "KON", "APPR", "PPER", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "\u201eund wei\u00dft du denn den Weg?\u201c Ich wei\u00df ihn nicht, ich f\u00fchle", "tokens": ["\u201e", "und", "wei\u00dft", "du", "denn", "den", "Weg", "?", "\u201c", "Ich", "wei\u00df", "ihn", "nicht", ",", "ich", "f\u00fch\u00b7le"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$.", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Trieb nur und den Zug entgegen jener K\u00fchle.", "tokens": ["Den", "Trieb", "nur", "und", "den", "Zug", "ent\u00b7ge\u00b7gen", "je\u00b7ner", "K\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "ART", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die zahme spricht: Bin ich nicht auch von deinem Stamm,", "tokens": ["Die", "zah\u00b7me", "spricht", ":", "Bin", "ich", "nicht", "auch", "von", "dei\u00b7nem", "Stamm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$.", "VAFIN", "PPER", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und f\u00fchle keinen Trieb und Zug aus meinem Schlamm.", "tokens": ["Und", "f\u00fch\u00b7le", "kei\u00b7nen", "Trieb", "und", "Zug", "aus", "mei\u00b7nem", "Schlamm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Die wilde spricht: du hast, von der Natur entfernt,", "tokens": ["Die", "wil\u00b7de", "spricht", ":", "du", "hast", ",", "von", "der", "Na\u00b7tur", "ent\u00b7fernt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$.", "PPER", "VAFIN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den angestammten Trieb der Freiheit nur verlernt.", "tokens": ["Den", "an\u00b7ge\u00b7stamm\u00b7ten", "Trieb", "der", "Frei\u00b7heit", "nur", "ver\u00b7lernt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Ich aber f\u00fchle michs durchzittern und durchwittern;", "tokens": ["Ich", "a\u00b7ber", "f\u00fch\u00b7le", "michs", "durch\u00b7zit\u00b7tern", "und", "durch\u00b7wit\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PIS", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Leb wol! dort reicht man dir dein Futter aus den Gittern.", "tokens": ["Leb", "wol", "!", "dort", "reicht", "man", "dir", "dein", "Fut\u00b7ter", "aus", "den", "Git\u00b7tern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$.", "ADV", "VVFIN", "PIS", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}