{"textgrid.poem.60084": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier, du liebe kleine Wilde!", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier, du liebe kleine Wilde!", "tokens": ["Hier", ",", "du", "lie\u00b7be", "klei\u00b7ne", "Wil\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruf' ich dir bey deinem Bilde.", "tokens": ["Ruf'", "ich", "dir", "bey", "dei\u00b7nem", "Bil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gingst von hinnen: Ach! warum?", "tokens": ["Gingst", "von", "hin\u00b7nen", ":", "Ach", "!", "wa\u00b7rum", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "ADV", "$.", "ITJ", "$.", "PWAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieses K\u00e4mmerchen, wie stumm!", "tokens": ["Die\u00b7ses", "K\u00e4m\u00b7mer\u00b7chen", ",", "wie", "stumm", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PWAV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles \u00f6de rings herum!", "tokens": ["Al\u00b7les", "\u00f6\u00b7de", "rings", "he\u00b7rum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mag ich sp\u00e4hen, mag ich lauschen;", "tokens": ["Mag", "ich", "sp\u00e4\u00b7hen", ",", "mag", "ich", "lau\u00b7schen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nirgend h\u00f6r' ich, Rehen gleich,", "tokens": ["Nir\u00b7gend", "h\u00f6r'", "ich", ",", "Re\u00b7hen", "gleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Durch das bebende Gestr\u00e4uch", "tokens": ["Durch", "das", "be\u00b7ben\u00b7de", "Ge\u00b7str\u00e4uch"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.9": {"text": "Unsrer D\u00fc\u00dfel froh dich rauschen;", "tokens": ["Uns\u00b7rer", "D\u00fc\u00b7\u00dfel", "froh", "dich", "rau\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Kann, wenn aus dem Buchengang", "tokens": ["Kann", ",", "wenn", "aus", "dem", "Bu\u00b7chen\u00b7gang"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Fr\u00fch schon deiner Stimme Klang,", "tokens": ["Fr\u00fch", "schon", "dei\u00b7ner", "Stim\u00b7me", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Heller als der Schlag des Finken,", "tokens": ["Hel\u00b7ler", "als", "der", "Schlag", "des", "Fin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Und an Jubel nur gew\u00f6hnt,", "tokens": ["Und", "an", "Ju\u00b7bel", "nur", "ge\u00b7w\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Auf zu meinem Fenster t\u00f6nt,", "tokens": ["Auf", "zu", "mei\u00b7nem", "Fens\u00b7ter", "t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Keine Gr\u00fc\u00dfe mehr dir winken. \u2013", "tokens": ["Kei\u00b7ne", "Gr\u00fc\u00b7\u00dfe", "mehr", "dir", "win\u00b7ken", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "ADV", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Ach! zur\u00fcck, weil diese Flur", "tokens": ["Ach", "!", "zu\u00b7r\u00fcck", ",", "weil", "die\u00b7se", "Flur"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "PTKVZ", "$,", "KOUS", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Um dich trauert, kleine Wilde!", "tokens": ["Um", "dich", "trau\u00b7ert", ",", "klei\u00b7ne", "Wil\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Komm, und dr\u00fccke deine Spur", "tokens": ["Komm", ",", "und", "dr\u00fc\u00b7cke", "dei\u00b7ne", "Spur"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Tanzend in die Herbstgefilde.", "tokens": ["Tan\u00b7zend", "in", "die", "Herbst\u00b7ge\u00b7fil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Komm ans Ufer, gutes Kind,", "tokens": ["Komm", "ans", "U\u00b7fer", ",", "gu\u00b7tes", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Wo im rauhen Morgenwind", "tokens": ["Wo", "im", "rau\u00b7hen", "Mor\u00b7gen\u00b7wind"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Unsre Weidenb\u00fcsche wallen,", "tokens": ["Uns\u00b7re", "Wei\u00b7den\u00b7b\u00fc\u00b7sche", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Und die welken Bl\u00e4tter fallen;", "tokens": ["Und", "die", "wel\u00b7ken", "Bl\u00e4t\u00b7ter", "fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Da\u00df vom Ufer, gutes Kind,", "tokens": ["Da\u00df", "vom", "U\u00b7fer", ",", "gu\u00b7tes", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Mir der rauhe Morgenwind", "tokens": ["Mir", "der", "rau\u00b7he", "Mor\u00b7gen\u00b7wind"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Deinen Sang her\u00fcber wehe,", "tokens": ["Dei\u00b7nen", "Sang", "her\u00b7\u00fc\u00b7ber", "we\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Bis ich pl\u00f6tzlich aus dem Flor", "tokens": ["Bis", "ich", "pl\u00f6tz\u00b7lich", "aus", "dem", "Flor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Grauer Nebel dich hervor", "tokens": ["Grau\u00b7er", "Ne\u00b7bel", "dich", "her\u00b7vor"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "PPER", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Mir entgegen schimmern sehe.", "tokens": ["Mir", "ent\u00b7ge\u00b7gen", "schim\u00b7mern", "se\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "VVFIN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Bringe mit dein frisches Blut,", "tokens": ["Brin\u00b7ge", "mit", "dein", "fri\u00b7sches", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Deiner Augen rasches Feuer,", "tokens": ["Dei\u00b7ner", "Au\u00b7gen", "ra\u00b7sches", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Deines Herzens volle Glut,", "tokens": ["Dei\u00b7nes", "Her\u00b7zens", "vol\u00b7le", "Glut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Und den sorgenlosen Muth", "tokens": ["Und", "den", "sor\u00b7gen\u00b7lo\u00b7sen", "Muth"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.34": {"text": "Wider manches Abentheuer,", "tokens": ["Wi\u00b7der", "man\u00b7ches", "A\u00b7bent\u00b7heu\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Das in unsern Weg sich stellt,", "tokens": ["Das", "in", "un\u00b7sern", "Weg", "sich", "stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Wider manches Ungeheuer,", "tokens": ["Wi\u00b7der", "man\u00b7ches", "Un\u00b7ge\u00b7heu\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Das am Rosen-Pfade bellt,", "tokens": ["Das", "am", "Ro\u00b7sen\u00b7Pfa\u00b7de", "bellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Und aufs erste L\u00e4cheln f\u00e4llt.", "tokens": ["Und", "aufs", "ers\u00b7te", "L\u00e4\u00b7cheln", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Eile, frohes M\u00e4dchen, eile!", "tokens": ["Ei\u00b7le", ",", "fro\u00b7hes", "M\u00e4d\u00b7chen", ",", "ei\u00b7le", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Misch' ein wenig Schelmerey", "tokens": ["Misch'", "ein", "we\u00b7nig", "Schel\u00b7me\u00b7rey"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "In des Lebens Einerley;", "tokens": ["In", "des", "Le\u00b7bens", "Ei\u00b7ner\u00b7ley", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.42": {"text": "Scherz' hinweg die lange Weile,", "tokens": ["Scher\u00b7z'", "hin\u00b7weg", "die", "lan\u00b7ge", "Wei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APZR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Die des Geistes Mark verzehrt,", "tokens": ["Die", "des", "Geis\u00b7tes", "Mark", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.44": {"text": "Und den leichten Flug ihm wehrt.", "tokens": ["Und", "den", "leich\u00b7ten", "Flug", "ihm", "wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.45": {"text": "Fern von Mode-Ziererey,", "tokens": ["Fern", "von", "Mo\u00b7de\u00b7Zie\u00b7re\u00b7rey", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.46": {"text": "Komm, und red' und lache frey", "tokens": ["Komm", ",", "und", "red'", "und", "la\u00b7che", "frey"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "KON", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Hier am Ufer; M\u00e4dchen, eile!", "tokens": ["Hier", "am", "U\u00b7fer", ";", "M\u00e4d\u00b7chen", ",", "ei\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$.", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Uns sind nicht die B\u00fcsche todt,", "tokens": ["Uns", "sind", "nicht", "die", "B\u00fc\u00b7sche", "todt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.49": {"text": "Die uns, gr\u00fcnend, einst gefielen;", "tokens": ["Die", "uns", ",", "gr\u00fc\u00b7nend", ",", "einst", "ge\u00b7fie\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "VVPP", "$,", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Sieh die Bl\u00e4tter, gelb und roth,", "tokens": ["Sieh", "die", "Bl\u00e4t\u00b7ter", ",", "gelb", "und", "roth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Wie sie durch einander spielen,", "tokens": ["Wie", "sie", "durch", "ein\u00b7an\u00b7der", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Und das B\u00e4chlein hier, so klar", "tokens": ["Und", "das", "B\u00e4ch\u00b7lein", "hier", ",", "so", "klar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "$,", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "Als zur Bl\u00fcthenzeit es war!", "tokens": ["Als", "zur", "Bl\u00fc\u00b7then\u00b7zeit", "es", "war", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.54": {"text": "Deinen Jubel will ich h\u00f6ren,", "tokens": ["Dei\u00b7nen", "Ju\u00b7bel", "will", "ich", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Deinen Jubel, wenn das Haar", "tokens": ["Dei\u00b7nen", "Ju\u00b7bel", ",", "wenn", "das", "Haar"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.56": {"text": "Wind und Nebel dir zerst\u00f6ren,", "tokens": ["Wind", "und", "Ne\u00b7bel", "dir", "zer\u00b7st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.57": {"text": "Und wenn deine Hand, bereift,", "tokens": ["Und", "wenn", "dei\u00b7ne", "Hand", ",", "be\u00b7reift", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.58": {"text": "Nach der letzten Blume greift.", "tokens": ["Nach", "der", "letz\u00b7ten", "Blu\u00b7me", "greift", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}