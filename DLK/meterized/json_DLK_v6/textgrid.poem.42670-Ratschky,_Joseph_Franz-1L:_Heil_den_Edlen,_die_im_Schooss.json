{"textgrid.poem.42670": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Heil den Edlen, die im Schooss", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heil den Edlen, die im Schooss", "tokens": ["Heil", "den", "Ed\u00b7len", ",", "die", "im", "Scho\u00b7oss"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weiser Freyheit uns regieren,", "tokens": ["Wei\u00b7ser", "Frey\u00b7heit", "uns", "re\u00b7gie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit Ruhm den Hammer f\u00fchren!", "tokens": ["Und", "mit", "Ruhm", "den", "Ham\u00b7mer", "f\u00fch\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dank und Liebe sey ihr Loos!", "tokens": ["Dank", "und", "Lie\u00b7be", "sey", "ihr", "Loos", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Joch und Knechtschaft hassen sie,", "tokens": ["Joch", "und", "Knecht\u00b7schaft", "has\u00b7sen", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "So wie wir Despoten hassen;", "tokens": ["So", "wie", "wir", "Des\u00b7po\u00b7ten", "has\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Denn selbst br\u00fcderlichen Bassen", "tokens": ["Denn", "selbst", "br\u00fc\u00b7der\u00b7li\u00b7chen", "Bas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Beugt sich keines Maurers Knie.", "tokens": ["Beugt", "sich", "kei\u00b7nes", "Mau\u00b7rers", "Knie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Freyheit nur giebt uns Gedeihn:", "tokens": ["Frey\u00b7heit", "nur", "giebt", "uns", "Ge\u00b7deihn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Sie beherrsche diese St\u00e4tte!", "tokens": ["Sie", "be\u00b7herr\u00b7sche", "die\u00b7se", "St\u00e4t\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nie soll unsre Br\u00fcderkette", "tokens": ["Nie", "soll", "uns\u00b7re", "Br\u00fc\u00b7der\u00b7ket\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Eine Sklavenkette seyn.", "tokens": ["Ei\u00b7ne", "Skla\u00b7ven\u00b7ket\u00b7te", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}