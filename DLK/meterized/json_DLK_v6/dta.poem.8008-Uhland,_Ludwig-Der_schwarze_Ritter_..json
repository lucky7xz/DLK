{"dta.poem.8008": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der schwarze Ritter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Pfingsten war, das Fest der Freude,", "tokens": ["Pfings\u00b7ten", "war", ",", "das", "Fest", "der", "Freu\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das da feiern Wald und Haide.", "tokens": ["Das", "da", "fei\u00b7ern", "Wald", "und", "Hai\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJA", "NN", "KON", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hub der K\u00f6nig an zu sprechen:", "tokens": ["Hub", "der", "K\u00f6\u00b7nig", "an", "zu", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APZR", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201eauch aus den Hallen", "tokens": ["\u201e", "auch", "aus", "den", "Hal\u00b7len"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Der alten Hofburg allen", "tokens": ["Der", "al\u00b7ten", "Hof\u00b7burg", "al\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Soll ein reicher Fr\u00fchling brechen!\u201c", "tokens": ["Soll", "ein", "rei\u00b7cher", "Fr\u00fch\u00b7ling", "bre\u00b7chen", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ART", "ADJD", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Trommeln und Trommeten schallen,", "tokens": ["Trom\u00b7meln", "und", "Trom\u00b7me\u00b7ten", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Rothe Fahnen festlich wallen.", "tokens": ["Ro\u00b7the", "Fah\u00b7nen", "fest\u00b7lich", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sah der K\u00f6nig vom Balkone;", "tokens": ["Sah", "der", "K\u00f6\u00b7nig", "vom", "Bal\u00b7ko\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In Lanzenspielen", "tokens": ["In", "Lan\u00b7zen\u00b7spie\u00b7len"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Die Ritter alle fielen", "tokens": ["Die", "Rit\u00b7ter", "al\u00b7le", "fie\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PIS", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vor des K\u00f6nigs starkem Sohne.", "tokens": ["Vor", "des", "K\u00f6\u00b7nigs", "star\u00b7kem", "Soh\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber vor des Kampfes Gitter", "tokens": ["A\u00b7ber", "vor", "des", "Kamp\u00b7fes", "Git\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ritt zuletzt ein schwarzer Ritter.", "tokens": ["Ritt", "zu\u00b7letzt", "ein", "schwar\u00b7zer", "Rit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201eherr! wie ist Eur Nam\u2019 und Zeichen?\u201c", "tokens": ["\u201e", "herr", "!", "wie", "ist", "Eur", "Nam'", "und", "Zei\u00b7chen", "?", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$.", "PWAV", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201ew\u00fcrd\u2019 ich es sagen,", "tokens": ["\u201e", "w\u00fcrd'", "ich", "es", "sa\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ihr m\u00f6chtet zittern und zagen,", "tokens": ["Ihr", "m\u00f6ch\u00b7tet", "zit\u00b7tern", "und", "za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "KON", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Bin ein F\u00fcrst von gro\u00dfen Reichen.\u201c", "tokens": ["Bin", "ein", "F\u00fcrst", "von", "gro\u00b7\u00dfen", "Rei\u00b7chen", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Als er in die Bahn gezogen,", "tokens": ["Als", "er", "in", "die", "Bahn", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dunkel ward des Himmels Bogen", "tokens": ["Dun\u00b7kel", "ward", "des", "Him\u00b7mels", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das Schlo\u00df begann zu beben.", "tokens": ["Und", "das", "Schlo\u00df", "be\u00b7gann", "zu", "be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Beim ersten Sto\u00dfe", "tokens": ["Beim", "ers\u00b7ten", "Sto\u00b7\u00dfe"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Der J\u00fcngling sank vom Rosse,", "tokens": ["Der", "J\u00fcng\u00b7ling", "sank", "vom", "Ros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Konnte kaum sich wieder heben.", "tokens": ["Konn\u00b7te", "kaum", "sich", "wie\u00b7der", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PRF", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Pfeif\u2019 und Geige ruft zu T\u00e4nzen,", "tokens": ["Pfeif'", "und", "Gei\u00b7ge", "ruft", "zu", "T\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fackeln durch die S\u00e4le gl\u00e4nzen;", "tokens": ["Fa\u00b7ckeln", "durch", "die", "S\u00e4\u00b7le", "gl\u00e4n\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wankt ein gro\u00dfer Schatten drinnen.", "tokens": ["Wankt", "ein", "gro\u00b7\u00dfer", "Schat\u00b7ten", "drin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Er th\u00e4t mit Sitten", "tokens": ["Er", "th\u00e4t", "mit", "Sit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Des K\u00f6nigs Tochter bitten,", "tokens": ["Des", "K\u00f6\u00b7nigs", "Toch\u00b7ter", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Th\u00e4t den Tanz mit ihr beginnen.", "tokens": ["Th\u00e4t", "den", "Tanz", "mit", "ihr", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Tanzt im schwarzen Kleid von Eisen,", "tokens": ["Tanzt", "im", "schwar\u00b7zen", "Kleid", "von", "Ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tanzet schauerliche Weisen,", "tokens": ["Tan\u00b7zet", "schau\u00b7er\u00b7li\u00b7che", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlingt sich kalt um ihre Glieder.", "tokens": ["Schlingt", "sich", "kalt", "um", "ih\u00b7re", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von Brust und Haaren", "tokens": ["Von", "Brust", "und", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Entfallen ihr die klaren", "tokens": ["Ent\u00b7fal\u00b7len", "ihr", "die", "kla\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bl\u00fcmlein welk zur Erde nieder.", "tokens": ["Bl\u00fcm\u00b7lein", "welk", "zur", "Er\u00b7de", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und zur reichen Tafel kamen", "tokens": ["Und", "zur", "rei\u00b7chen", "Ta\u00b7fel", "ka\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle Ritter, alle Damen.", "tokens": ["Al\u00b7le", "Rit\u00b7ter", ",", "al\u00b7le", "Da\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zwischen Sohn und Tochter innen", "tokens": ["Zwi\u00b7schen", "Sohn", "und", "Toch\u00b7ter", "in\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit bangem Muthe", "tokens": ["Mit", "ban\u00b7gem", "Mu\u00b7the"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Der alte K\u00f6nig ruhte,", "tokens": ["Der", "al\u00b7te", "K\u00f6\u00b7nig", "ruh\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sah sie an mit stillem Sinnen.", "tokens": ["Sah", "sie", "an", "mit", "stil\u00b7lem", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Bleich die Kinder beide schienen,", "tokens": ["Bleich", "die", "Kin\u00b7der", "bei\u00b7de", "schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bot der Gast den Becher ihnen:", "tokens": ["Bot", "der", "Gast", "den", "Be\u00b7cher", "ih\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201egoldner Wein macht euch genesen.\u201c", "tokens": ["\u201e", "gold\u00b7ner", "Wein", "macht", "euch", "ge\u00b7ne\u00b7sen", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "VVFIN", "PPER", "VVPP", "$.", "$("], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Die Kinder tranken,", "tokens": ["Die", "Kin\u00b7der", "tran\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Sie th\u00e4ten h\u00f6flich danken:", "tokens": ["Sie", "th\u00e4\u00b7ten", "h\u00f6f\u00b7lich", "dan\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u201ek\u00fchl ist dieser Trunk gewesen.\u201c", "tokens": ["\u201e", "k\u00fchl", "ist", "die\u00b7ser", "Trunk", "ge\u00b7we\u00b7sen", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PDAT", "NN", "VAPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "An des Vaters Brust sich schlangen", "tokens": ["An", "des", "Va\u00b7ters", "Brust", "sich", "schlan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sohn und Tochter; ihre Wangen", "tokens": ["Sohn", "und", "Toch\u00b7ter", ";", "ih\u00b7re", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Th\u00e4ten v\u00f6llig sich entf\u00e4rben.", "tokens": ["Th\u00e4\u00b7ten", "v\u00f6l\u00b7lig", "sich", "ent\u00b7f\u00e4r\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wohin der graue,", "tokens": ["Wo\u00b7hin", "der", "grau\u00b7e", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Erschrockne Vater schaue,", "tokens": ["Er\u00b7schrock\u00b7ne", "Va\u00b7ter", "schau\u00b7e", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sieht er eins der Kinder sterben.", "tokens": ["Sieht", "er", "eins", "der", "Kin\u00b7der", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "\u201eweh! die holden Kinder beide", "tokens": ["\u201e", "weh", "!", "die", "hol\u00b7den", "Kin\u00b7der", "bei\u00b7de"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PTKVZ", "$.", "ART", "ADJA", "NN", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nahmst du hin in Ingendfreude:", "tokens": ["Nahmst", "du", "hin", "in", "In\u00b7gend\u00b7freu\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimm auch mich, den Freudelosen!\u201c", "tokens": ["Nimm", "auch", "mich", ",", "den", "Freu\u00b7de\u00b7lo\u00b7sen", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "ADV", "PPER", "$,", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da sprach der Grimme", "tokens": ["Da", "sprach", "der", "Grim\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Mit hohler, dumpfer Stimme:", "tokens": ["Mit", "hoh\u00b7ler", ",", "dum\u00b7pfer", "Stim\u00b7me", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u201egreis! im Fr\u00fchling brech\u2019 ich Rosen.\u201c", "tokens": ["\u201e", "greis", "!", "im", "Fr\u00fch\u00b7ling", "brech'", "ich", "Ro\u00b7sen", ".", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$.", "APPRART", "NN", "VVFIN", "PPER", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}