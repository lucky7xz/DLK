{"textgrid.poem.58063": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "101. Herr, wie du willst!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr, wie du willst, nicht dies noch das ich w\u00e4hle;", "tokens": ["Herr", ",", "wie", "du", "willst", ",", "nicht", "dies", "noch", "das", "ich", "w\u00e4h\u00b7le", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VMFIN", "$,", "PTKNEG", "PDS", "ADV", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Herr, wie du willst, so la\u00df es nur geschehn!", "tokens": ["Herr", ",", "wie", "du", "willst", ",", "so", "la\u00df", "es", "nur", "ge\u00b7schehn", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VMFIN", "$,", "ADV", "VVIMP", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich bin dein Sklav', mein K\u00f6nig, nur befehle!", "tokens": ["Ich", "bin", "dein", "Skla\u00b7v'", ",", "mein", "K\u00f6\u00b7nig", ",", "nur", "be\u00b7feh\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Ich will als Kind dir nach den Augen sehn.", "tokens": ["Ich", "will", "als", "Kind", "dir", "nach", "den", "Au\u00b7gen", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KOUS", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}