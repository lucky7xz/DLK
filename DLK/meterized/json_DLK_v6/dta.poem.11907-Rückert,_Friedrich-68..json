{"dta.poem.11907": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "68.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu lernen halte nur dich nie zu alt, und lerne", "tokens": ["Zu", "ler\u00b7nen", "hal\u00b7te", "nur", "dich", "nie", "zu", "alt", ",", "und", "ler\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKZU", "VVINF", "VVFIN", "ADV", "PPER", "ADV", "PTKA", "ADJD", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von denen, die von dir gelernt, nun wieder gerne.", "tokens": ["Von", "de\u00b7nen", ",", "die", "von", "dir", "ge\u00b7lernt", ",", "nun", "wie\u00b7der", "ger\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "$,", "PRELS", "APPR", "PPER", "VVPP", "$,", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie haben manches wol, was dir aus schlaffern Falten", "tokens": ["Sie", "ha\u00b7ben", "man\u00b7ches", "wol", ",", "was", "dir", "aus", "schlaf\u00b7fern", "Fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIS", "ADV", "$,", "PWS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Indes entfallen, fest in strafferen gehalten;", "tokens": ["In\u00b7des", "ent\u00b7fal\u00b7len", ",", "fest", "in", "straf\u00b7fe\u00b7ren", "ge\u00b7hal\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "ADJD", "APPR", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gebildet manches aus, was du nur angelegt,", "tokens": ["Ge\u00b7bil\u00b7det", "man\u00b7ches", "aus", ",", "was", "du", "nur", "an\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKVZ", "$,", "PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu Bl\u00fct' und Frucht gebracht, was du nur angeregt.", "tokens": ["Zu", "Bl\u00fct'", "und", "Frucht", "ge\u00b7bracht", ",", "was", "du", "nur", "an\u00b7ge\u00b7regt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,", "PWS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nimmst du von ihnen nun, was sie von dir genommen,", "tokens": ["Nimmst", "du", "von", "ih\u00b7nen", "nun", ",", "was", "sie", "von", "dir", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "ADV", "$,", "PRELS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So hast du sch\u00f6ner dich verj\u00fcngt zur\u00fcck bekommen.", "tokens": ["So", "hast", "du", "sch\u00f6\u00b7ner", "dich", "ver\u00b7j\u00fcngt", "zu\u00b7r\u00fcck", "be\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "PPER", "VVFIN", "PTKVZ", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}