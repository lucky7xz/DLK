{"dta.poem.15730": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "63.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein fester Standpunkt sei in deinem Kreis dir eigen,", "tokens": ["Ein", "fes\u00b7ter", "Stand\u00b7punkt", "sei", "in", "dei\u00b7nem", "Kreis", "dir", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo dir die Dinge sich in rechter Weite zeigen.", "tokens": ["Wo", "dir", "die", "Din\u00b7ge", "sich", "in", "rech\u00b7ter", "Wei\u00b7te", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nur da erblickst du sie vom wahren Licht erhellt,", "tokens": ["Nur", "da", "er\u00b7blickst", "du", "sie", "vom", "wah\u00b7ren", "Licht", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo um die Mitte sie im Kreise sind gestellt.", "tokens": ["Wo", "um", "die", "Mit\u00b7te", "sie", "im", "Krei\u00b7se", "sind", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PPER", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Den andern mu\u00dft du auch ihren Gesichtskreis g\u00f6nnen,", "tokens": ["Den", "an\u00b7dern", "mu\u00dft", "du", "auch", "ih\u00b7ren", "Ge\u00b7sichts\u00b7kreis", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In jeden fremden dich zugleich versetzen k\u00f6nnen.", "tokens": ["In", "je\u00b7den", "frem\u00b7den", "dich", "zu\u00b7gleich", "ver\u00b7set\u00b7zen", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "PPER", "ADV", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Statt deiner Augen mu\u00dft du k\u00f6nnen sehn mit ihren,", "tokens": ["Statt", "dei\u00b7ner", "Au\u00b7gen", "mu\u00dft", "du", "k\u00f6n\u00b7nen", "sehn", "mit", "ih\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VMFIN", "PPER", "VMFIN", "VVINF", "APPR", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein eignes Urtheil nur deswegen nicht verlieren.", "tokens": ["Dein", "eig\u00b7nes", "Ur\u00b7theil", "nur", "des\u00b7we\u00b7gen", "nicht", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "PAV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Einseitigkeit ist Noth, die's t\u00fcchtig meint und ehrlich,", "tokens": ["Ein\u00b7sei\u00b7tig\u00b7keit", "ist", "Noth", ",", "die's", "t\u00fcch\u00b7tig", "meint", "und", "ehr\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,", "PRELS", "ADJD", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch von Allseitigkeit ein St\u00fcck auch unentbehrlich.", "tokens": ["Doch", "von", "All\u00b7sei\u00b7tig\u00b7keit", "ein", "St\u00fcck", "auch", "un\u00b7ent\u00b7behr\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}