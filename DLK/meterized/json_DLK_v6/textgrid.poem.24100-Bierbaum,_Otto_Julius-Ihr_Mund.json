{"textgrid.poem.24100": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Ihr Mund", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Mund ist sch\u00f6n. Nicht vieles auf der Welt", "tokens": ["Ihr", "Mund", "ist", "sch\u00f6n", ".", "Nicht", "vie\u00b7les", "auf", "der", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$.", "PTKNEG", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist sch\u00f6n wie dieser Mund, so v\u00f6llig sch\u00f6n,", "tokens": ["Ist", "sch\u00f6n", "wie", "die\u00b7ser", "Mund", ",", "so", "v\u00f6l\u00b7lig", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOKOM", "PDAT", "NN", "$,", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ich ergriffen bin, denk ich daran.", "tokens": ["Da\u00df", "ich", "er\u00b7grif\u00b7fen", "bin", ",", "denk", "ich", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,", "VVFIN", "PPER", "PAV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ihr Mund ist sch\u00f6n. Aus diesem Munde kann", "tokens": ["Ihr", "Mund", "ist", "sch\u00f6n", ".", "Aus", "die\u00b7sem", "Mun\u00b7de", "kann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$.", "APPR", "PDAT", "NN", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kein schlechtes Wort, kein b\u00f6ses Lachen wehn;", "tokens": ["Kein", "schlech\u00b7tes", "Wort", ",", "kein", "b\u00f6\u00b7ses", "La\u00b7chen", "wehn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von diesem Mund zu tr\u00e4umen ist schon Gl\u00fcck.", "tokens": ["Von", "die\u00b7sem", "Mund", "zu", "tr\u00e4u\u00b7men", "ist", "schon", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKZU", "VVINF", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich werd ihn wiedersehn. Dann bin ich froh,", "tokens": ["Ich", "werd", "ihn", "wie\u00b7der\u00b7sehn", ".", "Dann", "bin", "ich", "froh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "$.", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie nach dem Winter, wenn es Fr\u00fchling ist:", "tokens": ["Wie", "nach", "dem", "Win\u00b7ter", ",", "wenn", "es", "Fr\u00fch\u00b7ling", "ist", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Oh Leben, allerseligstes Geschenk!", "tokens": ["Oh", "Le\u00b7ben", ",", "al\u00b7ler\u00b7se\u00b7ligs\u00b7tes", "Ge\u00b7schenk", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["XY", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Reinheit und G\u00fcte sind auf ihm gepaart,", "tokens": ["Rein\u00b7heit", "und", "G\u00fc\u00b7te", "sind", "auf", "ihm", "ge\u00b7paart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Dort hat die kleinste L\u00fcge keine Statt;", "tokens": ["Dort", "hat", "die", "kleins\u00b7te", "L\u00fc\u00b7ge", "kei\u00b7ne", "Statt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein h\u00f6chster Eid ist Schwur bei ihrem Mund.", "tokens": ["Mein", "h\u00f6chs\u00b7ter", "Eid", "ist", "Schwur", "bei", "ih\u00b7rem", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wie gl\u00fccklich bin ich! St\u00f6\u00dft mich Gram und Leid,", "tokens": ["Wie", "gl\u00fcck\u00b7lich", "bin", "ich", "!", "St\u00f6\u00dft", "mich", "Gram", "und", "Leid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "$.", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So denk ich, wie sie sch\u00f6n ist, wie ihr Mund", "tokens": ["So", "denk", "ich", ",", "wie", "sie", "sch\u00f6n", "ist", ",", "wie", "ihr", "Mund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "ADJD", "VAFIN", "$,", "PWAV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Klar l\u00e4cheln kann, und alles ist verscheucht.", "tokens": ["Klar", "l\u00e4\u00b7cheln", "kann", ",", "und", "al\u00b7les", "ist", "ver\u00b7scheucht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "VMFIN", "$,", "KON", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}