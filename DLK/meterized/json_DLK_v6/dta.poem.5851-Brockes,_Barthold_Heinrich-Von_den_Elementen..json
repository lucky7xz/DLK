{"dta.poem.5851": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn sich mein Geist den Anfang unsrer Welt", "tokens": ["Wenn", "sich", "mein", "Geist", "den", "An\u00b7fang", "uns\u00b7rer", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vor Augen stellt;", "tokens": ["Vor", "Au\u00b7gen", "stellt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "So glaub ich, da\u00df sie GOTT durch seine Macht,", "tokens": ["So", "glaub", "ich", ",", "da\u00df", "sie", "GoTT", "durch", "sei\u00b7ne", "Macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In ihrer Vollenkommenheit,", "tokens": ["In", "ih\u00b7rer", "Vol\u00b7len\u00b7kom\u00b7men\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf einmahl hab hervorgebracht,", "tokens": ["Auf", "ein\u00b7mahl", "hab", "her\u00b7vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ohn\u2019 einige der Zeiten Langsamkeit.", "tokens": ["Ohn'", "ei\u00b7ni\u00b7ge", "der", "Zei\u00b7ten", "Lang\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Allein dies grosse Werck, so allbereit", "tokens": ["Al\u00b7lein", "dies", "gros\u00b7se", "Werck", ",", "so", "all\u00b7be\u00b7reit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PDS", "ADJA", "NN", "$,", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Vollkommen in der Wieg\u2019; empfing ein Ziel der Zeit.", "tokens": ["Voll\u00b7kom\u00b7men", "in", "der", "Wieg'", ";", "emp\u00b7fing", "ein", "Ziel", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dieselbe Macht, die von der grossen Welt", "tokens": ["Die\u00b7sel\u00b7be", "Macht", ",", "die", "von", "der", "gros\u00b7sen", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Den wundersch\u00f6nen Bau erh\u00e4lt:", "tokens": ["Den", "wun\u00b7der\u00b7sch\u00f6\u00b7nen", "Bau", "er\u00b7h\u00e4lt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Derselbige Befehl, der sie beweget,", "tokens": ["Der\u00b7sel\u00b7bi\u00b7ge", "Be\u00b7fehl", ",", "der", "sie", "be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Der sie so sch\u00fctzt als tr\u00e4get,", "tokens": ["Der", "sie", "so", "sch\u00fctzt", "als", "tr\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "KOKOM", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Vermag ein\u2019, ihr in allen Sachen", "tokens": ["Ver\u00b7mag", "ein'", ",", "ihr", "in", "al\u00b7len", "Sa\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Gantz gleiche, Welt, all\u2019 Augenblick zu machen.", "tokens": ["Gantz", "glei\u00b7che", ",", "Welt", ",", "all'", "Au\u00b7gen\u00b7blick", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "$,", "NN", "$,", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Es scheint, ob schaffte GOTT dies herrliche Geb\u00e4u", "tokens": ["Es", "scheint", ",", "ob", "schaff\u00b7te", "GoTT", "dies", "herr\u00b7li\u00b7che", "Ge\u00b7b\u00e4u"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADJA", "NN", "PDS", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "An einem jeden Tage neu.", "tokens": ["An", "ei\u00b7nem", "je\u00b7den", "Ta\u00b7ge", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}