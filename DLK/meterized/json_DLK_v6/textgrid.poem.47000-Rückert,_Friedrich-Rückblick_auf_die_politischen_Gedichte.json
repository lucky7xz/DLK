{"textgrid.poem.47000": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "R\u00fcckblick auf die politischen Gedichte", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Politik ein Herz zu r\u00fchren,", "tokens": ["Die", "Po\u00b7li\u00b7tik", "ein", "Herz", "zu", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den sanften Lieberobrungskrieg,", "tokens": ["Den", "sanf\u00b7ten", "Lie\u00b7be\u00b7ro\u00b7brungs\u00b7krieg", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie hab' ich lassen mich verf\u00fchren,", "tokens": ["Wie", "hab'", "ich", "las\u00b7sen", "mich", "ver\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gering zu achten diesen Sieg!", "tokens": ["Ge\u00b7ring", "zu", "ach\u00b7ten", "die\u00b7sen", "Sieg", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich wollte stolz mich \u00fcberheben,", "tokens": ["Ich", "woll\u00b7te", "stolz", "mich", "\u00fc\u00b7ber\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In hochbegeistertem Gesang", "tokens": ["In", "hoch\u00b7be\u00b7geis\u00b7ter\u00b7tem", "Ge\u00b7sang"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hinfort nur blut'ge Lorbeern weben", "tokens": ["Hin\u00b7fort", "nur", "blut'\u00b7ge", "Lor\u00b7beern", "we\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Um vaterl\u00e4nd'scher Waffen Klang.", "tokens": ["Um", "va\u00b7ter\u00b7l\u00e4n\u00b7d'\u00b7scher", "Waf\u00b7fen", "Klang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch wie der Krieger aus dem Schalle", "tokens": ["Doch", "wie", "der", "Krie\u00b7ger", "aus", "dem", "Schal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des eh'rnen Feldes still zur\u00fcck", "tokens": ["Des", "eh'r\u00b7nen", "Fel\u00b7des", "still", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich sehnt nach seines Hauses Halle,", "tokens": ["Sich", "sehnt", "nach", "sei\u00b7nes", "Hau\u00b7ses", "Hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Lebens heimgebliebnem Gl\u00fcck;", "tokens": ["Des", "Le\u00b7bens", "heim\u00b7ge\u00b7blieb\u00b7nem", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So sehnt nach fr\u00fchen Liebeskl\u00e4ngen", "tokens": ["So", "sehnt", "nach", "fr\u00fc\u00b7hen", "Lie\u00b7bes\u00b7kl\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Lied sich heimw\u00e4rts, lang' entfernt,", "tokens": ["Mein", "Lied", "sich", "heim\u00b7w\u00e4rts", ",", "lang'", "ent\u00b7fernt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "ADV", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und freut sich, da\u00df im wilden Dr\u00e4ngen", "tokens": ["Und", "freut", "sich", ",", "da\u00df", "im", "wil\u00b7den", "Dr\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "$,", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Es nicht den Wohllaut ganz verlernt.", "tokens": ["Es", "nicht", "den", "Wohl\u00b7laut", "ganz", "ver\u00b7lernt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf paradiesischem Gefilde", "tokens": ["Auf", "pa\u00b7ra\u00b7die\u00b7si\u00b7schem", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "War Liebe bei dem ersten Paar", "tokens": ["War", "Lie\u00b7be", "bei", "dem", "ers\u00b7ten", "Paar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Viel fr\u00fcher, als mit Helm und Schilde", "tokens": ["Viel", "fr\u00fc\u00b7her", ",", "als", "mit", "Helm", "und", "Schil\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Kampfe zog die erste Schar.", "tokens": ["Zum", "Kamp\u00b7fe", "zog", "die", "ers\u00b7te", "Schar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und in der eignen Jugend Stille,", "tokens": ["Und", "in", "der", "eig\u00b7nen", "Ju\u00b7gend", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von Adlers Kreischen ungemahnt,", "tokens": ["Von", "Ad\u00b7lers", "Krei\u00b7schen", "un\u00b7ge\u00b7mahnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hab' ich beim Sommerlied der Grille", "tokens": ["Hab'", "ich", "beim", "Som\u00b7mer\u00b7lied", "der", "Gril\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Viel eher Lieb' als Krieg geahnt.", "tokens": ["Viel", "e\u00b7her", "Lieb'", "als", "Krieg", "ge\u00b7ahnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nach dem verlornen Doppel-Eden", "tokens": ["Nach", "dem", "ver\u00b7lor\u00b7nen", "Dop\u00b7pel\u00b7E\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Der Kindheit und der ersten Welt,", "tokens": ["Der", "Kind\u00b7heit", "und", "der", "ers\u00b7ten", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kehr' ich, entsagend allen Fehden,", "tokens": ["Kehr'", "ich", ",", "ent\u00b7sa\u00b7gend", "al\u00b7len", "Feh\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "VVPP", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ich der Lieb' anheimgestellt;", "tokens": ["Die", "ich", "der", "Lieb'", "an\u00b7heim\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und nur von Liebe will ich singen,", "tokens": ["Und", "nur", "von", "Lie\u00b7be", "will", "ich", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die dieser Erden \u00f6dem Raum,", "tokens": ["Die", "die\u00b7ser", "Er\u00b7den", "\u00f6\u00b7dem", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo nicht ein Paradies kann bringen,", "tokens": ["Wo", "nicht", "ein", "Pa\u00b7ra\u00b7dies", "kann", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Doch eines Paradieses Traum.", "tokens": ["Doch", "ei\u00b7nes", "Pa\u00b7ra\u00b7die\u00b7ses", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}