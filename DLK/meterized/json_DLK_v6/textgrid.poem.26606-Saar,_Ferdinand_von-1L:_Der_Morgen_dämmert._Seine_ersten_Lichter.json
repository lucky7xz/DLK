{"textgrid.poem.26606": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Morgen d\u00e4mmert. Seine ersten Lichter", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Morgen d\u00e4mmert. Seine ersten Lichter", "tokens": ["Der", "Mor\u00b7gen", "d\u00e4m\u00b7mert", ".", "Sei\u00b7ne", "ers\u00b7ten", "Lich\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erhellen matt und k\u00fchl des Parkes Gr\u00fcn.", "tokens": ["Er\u00b7hel\u00b7len", "matt", "und", "k\u00fchl", "des", "Par\u00b7kes", "Gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Rings tiefe Stille; leise zwitschernd nur", "tokens": ["Rings", "tie\u00b7fe", "Stil\u00b7le", ";", "lei\u00b7se", "zwit\u00b7schernd", "nur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "$.", "ADJD", "ADJD", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Regt's in den Wipfeln sich, und aus dem Spiegel", "tokens": ["Regt's", "in", "den", "Wip\u00b7feln", "sich", ",", "und", "aus", "dem", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "PRF", "$,", "KON", "APPR", "ART", "NN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.5": {"text": "Des Teiches schnellt ein Silberfisch empor.", "tokens": ["Des", "Tei\u00b7ches", "schnellt", "ein", "Sil\u00b7ber\u00b7fisch", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mit dicht verh\u00fcllten Fenstern lautlos liegt", "tokens": ["Mit", "dicht", "ver\u00b7h\u00fcll\u00b7ten", "Fens\u00b7tern", "laut\u00b7los", "liegt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Schlo\u00df, und in den dunkelnden Gem\u00e4chern,", "tokens": ["Das", "Schlo\u00df", ",", "und", "in", "den", "dun\u00b7keln\u00b7den", "Ge\u00b7m\u00e4\u00b7chern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vom Schlaf umfangen, liegen die Bewohner.", "tokens": ["Vom", "Schlaf", "um\u00b7fan\u00b7gen", ",", "lie\u00b7gen", "die", "Be\u00b7woh\u00b7ner", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Selbst jene, die der kurzen Sommernacht", "tokens": ["Selbst", "je\u00b7ne", ",", "die", "der", "kur\u00b7zen", "Som\u00b7mer\u00b7nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PDS", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Langsame Stunden schlummerlos gez\u00e4hlt,", "tokens": ["Lang\u00b7sa\u00b7me", "Stun\u00b7den", "schlum\u00b7mer\u00b7los", "ge\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Im Seelenaufruhr hin und her sich werfend \u2013", "tokens": ["Im", "See\u00b7len\u00b7auf\u00b7ruhr", "hin", "und", "her", "sich", "wer\u00b7fend", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "ADV", "PRF", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Selbst jene hat des Morgens Schauer jetzt", "tokens": ["Selbst", "je\u00b7ne", "hat", "des", "Mor\u00b7gens", "Schau\u00b7er", "jetzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PDS", "VAFIN", "ART", "ADV", "NE", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Zur Ruh' gebracht ...", "tokens": ["Zur", "Ruh'", "ge\u00b7bracht", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Noch eine Stunde. Dann ein erster Ruck \u2013", "tokens": ["Noch", "ei\u00b7ne", "Stun\u00b7de", ".", "Dann", "ein", "ers\u00b7ter", "Ruck", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$.", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und nach und nach belebt sich dieses Schweigen.", "tokens": ["Und", "nach", "und", "nach", "be\u00b7lebt", "sich", "die\u00b7ses", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "KON", "APPR", "VVFIN", "PRF", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Emporger\u00fcttelt aus dem kurzen Schlaf", "tokens": ["Em\u00b7por\u00b7ge\u00b7r\u00fct\u00b7telt", "aus", "dem", "kur\u00b7zen", "Schlaf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Arbeit hat die Pflicht den Dienertro\u00df.", "tokens": ["Der", "Ar\u00b7beit", "hat", "die", "Pflicht", "den", "Die\u00b7ner\u00b7tro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit unvergn\u00fcgter Hast geht er an's Tagwerk,", "tokens": ["Mit", "un\u00b7ver\u00b7gn\u00fcg\u00b7ter", "Hast", "geht", "er", "an's", "Tag\u00b7werk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Inde\u00df verschlaf'ne Bonnen, leisen Fu\u00dfes,", "tokens": ["In\u00b7de\u00df", "ver\u00b7schlaf'\u00b7ne", "Bon\u00b7nen", ",", "lei\u00b7sen", "Fu\u00b7\u00dfes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Vorsorglich seid'nen Kinderbetten nah'n,", "tokens": ["Vor\u00b7sorg\u00b7lich", "sei\u00b7d'\u00b7nen", "Kin\u00b7der\u00b7bet\u00b7ten", "nah'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Und g\u00e4hnend ihre Brust die tr\u00e4ge Amme", "tokens": ["Und", "g\u00e4h\u00b7nend", "ih\u00b7re", "Brust", "die", "tr\u00e4\u00b7ge", "Am\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Dem S\u00e4ugling reicht, der schon nach ihr gewimmert.", "tokens": ["Dem", "S\u00e4ug\u00b7ling", "reicht", ",", "der", "schon", "nach", "ihr", "ge\u00b7wim\u00b7mert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Und sp\u00e4ter dann, von einsam \u00f6den Lagern,", "tokens": ["Und", "sp\u00e4\u00b7ter", "dann", ",", "von", "ein\u00b7sam", "\u00f6\u00b7den", "La\u00b7gern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus \u00f6den Tr\u00e4umen, heben seufzend sich", "tokens": ["Aus", "\u00f6\u00b7den", "Tr\u00e4u\u00b7men", ",", "he\u00b7ben", "seuf\u00b7zend", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "ADJD", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Empor die Lehrer und die Gouvernanten,", "tokens": ["Em\u00b7por", "die", "Leh\u00b7rer", "und", "die", "Gou\u00b7ver\u00b7nan\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die mit ergrau'nden H\u00e4uptern immer noch", "tokens": ["Die", "mit", "er\u00b7grau'n\u00b7den", "H\u00e4up\u00b7tern", "im\u00b7mer", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Als lebende Vocabelntrichter wandeln.", "tokens": ["Als", "le\u00b7ben\u00b7de", "Vo\u00b7ca\u00b7beln\u00b7trich\u00b7ter", "wan\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Sie schl\u00fcpfen rasch in abgen\u00fctzte Tracht", "tokens": ["Sie", "schl\u00fcp\u00b7fen", "rasch", "in", "ab\u00b7ge\u00b7n\u00fctz\u00b7te", "Tracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und blicken in den Hof stumpfsinnig nieder,", "tokens": ["Und", "bli\u00b7cken", "in", "den", "Hof", "stumpf\u00b7sin\u00b7nig", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wo wiehernd schon die stolzen Rosse stampfen", "tokens": ["Wo", "wie\u00b7hernd", "schon", "die", "stol\u00b7zen", "Ros\u00b7se", "stamp\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der stolzen Herren, die mit Sporngeklirr", "tokens": ["Der", "stol\u00b7zen", "Her\u00b7ren", ",", "die", "mit", "Sporn\u00b7gek\u00b7lirr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Zum Morgenritt hinab die Treppen eilen.", "tokens": ["Zum", "Mor\u00b7gen\u00b7ritt", "hin\u00b7ab", "die", "Trep\u00b7pen", "ei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "So Jung, wie Alt. Mit leerer Stirn die Einen", "tokens": ["So", "Jung", ",", "wie", "Alt", ".", "Mit", "lee\u00b7rer", "Stirn", "die", "Ei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "$,", "PWAV", "ADJD", "$.", "APPR", "ADJA", "NN", "ART", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und leerem Herzen; And're k\u00fchnen Geistes,", "tokens": ["Und", "lee\u00b7rem", "Her\u00b7zen", ";", "An\u00b7d'\u00b7re", "k\u00fch\u00b7nen", "Geis\u00b7tes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$.", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Die Brust zerw\u00fchlt vom Drang der Leidenschaften,", "tokens": ["Die", "Brust", "zer\u00b7w\u00fchlt", "vom", "Drang", "der", "Lei\u00b7den\u00b7schaf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Von Herrschsucht, Ehrgeiz, Eifersucht und Ha\u00df,", "tokens": ["Von", "Herrschsucht", ",", "Ehr\u00b7geiz", ",", "Ei\u00b7fer\u00b7sucht", "und", "Ha\u00df", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Die Brau'n gefaltet und durchfurcht das Antlitz", "tokens": ["Die", "Brau'n", "ge\u00b7fal\u00b7tet", "und", "durch\u00b7furcht", "das", "Ant\u00b7litz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Von Sorgen des Besitzes und der Macht,", "tokens": ["Von", "Sor\u00b7gen", "des", "Be\u00b7sit\u00b7zes", "und", "der", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Von Sorgen, die schon fr\u00fch die Haare bleichen,", "tokens": ["Von", "Sor\u00b7gen", ",", "die", "schon", "fr\u00fch", "die", "Haa\u00b7re", "blei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Doch auch zum Widerstand die Glieder st\u00e4hlen ...", "tokens": ["Doch", "auch", "zum", "Wi\u00b7der\u00b7stand", "die", "Glie\u00b7der", "st\u00e4h\u00b7len", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Schon blitzt es gold'ger um das Laub des Parks;", "tokens": ["Schon", "blitzt", "es", "gold'\u00b7ger", "um", "das", "Laub", "des", "Parks", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Thaufrischer Rosen Duft dringt s\u00fc\u00df durch Fenster,", "tokens": ["Thauf\u00b7ri\u00b7scher", "Ro\u00b7sen", "Duft", "dringt", "s\u00fc\u00df", "durch", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So man ge\u00f6ffnet leise zur Erquickung", "tokens": ["So", "man", "ge\u00b7\u00f6ff\u00b7net", "lei\u00b7se", "zur", "Er\u00b7quic\u00b7kung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr hei\u00dfe Stirnen, die auf Spitzenkissen", "tokens": ["F\u00fcr", "hei\u00b7\u00dfe", "Stir\u00b7nen", ",", "die", "auf", "Spit\u00b7zen\u00b7kis\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Im Wachen noch forttr\u00e4umen jene Tr\u00e4ume,", "tokens": ["Im", "Wa\u00b7chen", "noch", "fort\u00b7tr\u00e4u\u00b7men", "je\u00b7ne", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie sie die Frauen tr\u00e4umen ...", "tokens": ["Wie", "sie", "die", "Frau\u00b7en", "tr\u00e4u\u00b7men", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Allgemach", "tokens": ["All\u00b7ge\u00b7mach"], "token_info": ["word"], "pos": ["XY"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Bewegen wei\u00dfe Arme sich und Schultern,", "tokens": ["Be\u00b7we\u00b7gen", "wei\u00b7\u00dfe", "Ar\u00b7me", "sich", "und", "Schul\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PRF", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und von dem Schnee der Linnen richtet sich", "tokens": ["Und", "von", "dem", "Schnee", "der", "Lin\u00b7nen", "rich\u00b7tet", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "P