{"dta.poem.366": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er verlanget ihren Fu\u00df zu k\u00fcssen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Hertze sehnt sich nach den Marmor F\u00fcssen/", "tokens": ["Mein", "Hert\u00b7ze", "sehnt", "sich", "nach", "den", "Mar\u00b7mor", "F\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und will die heisse Lust", "tokens": ["Und", "will", "die", "heis\u00b7se", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit einem Kusse b\u00fcssen", "tokens": ["Mit", "ei\u00b7nem", "Kus\u00b7se", "b\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Lippen ihr Rubin", "tokens": ["Der", "Lip\u00b7pen", "ihr", "Ru\u00b7bin"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mag mich nicht zu sich ziehn/", "tokens": ["Mag", "mich", "nicht", "zu", "sich", "ziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "APPR", "PRF", "VVINF", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Noch weniger die zarte Brust.", "tokens": ["Noch", "we\u00b7ni\u00b7ger", "die", "zar\u00b7te", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ist schon der Br\u00fcste Pracht annehmlicher", "tokens": ["Ist", "schon", "der", "Br\u00fcs\u00b7te", "Pracht", "an\u00b7nehm\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Als Wein/", "tokens": ["Als", "Wein", "/"], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "NN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Als Balsam der vom Morgen k\u00f6mmt.", "tokens": ["Als", "Bal\u00b7sam", "der", "vom", "Mor\u00b7gen", "k\u00f6mmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So mag es doch drum seyn!", "tokens": ["So", "mag", "es", "doch", "drum", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PAV", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Blandella reiche her", "tokens": ["Blan\u00b7del\u00b7la", "rei\u00b7che", "her"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "PTKVZ"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.12": {"text": "Nicht deinen Mund/ noch Brust/ noch Schoo\u00df", "tokens": ["Nicht", "dei\u00b7nen", "Mund", "/", "noch", "Brust", "/", "noch", "Schoo\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "PPOSAT", "NN", "$(", "ADV", "NN", "$(", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Nur blo\u00df", "tokens": ["Nur", "blo\u00df"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "++", "measure": "spondeus"}, "line.14": {"text": "Den Alabaster Fu\u00df/", "tokens": ["Den", "A\u00b7la\u00b7bas\u00b7ter", "Fu\u00df", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Ich mu\u00df", "tokens": ["Ich", "mu\u00df"], "token_info": ["word", "word"], "pos": ["PPER", "VMFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Mich an dem ", "tokens": ["Mich", "an", "dem"], "token_info": ["word", "word", "word"], "pos": ["PPER", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Soll ich ein frohes Hertze haben.", "tokens": ["Soll", "ich", "ein", "fro\u00b7hes", "Hert\u00b7ze", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}