{"dta.poem.132": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als er ihre Gunst erhalten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Nun gehet bey mir auf die Sonne meiner Seelen/", "tokens": ["Nun", "ge\u00b7het", "bey", "mir", "auf", "die", "Son\u00b7ne", "mei\u00b7ner", "See\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "APPR", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Charlotte zeiget sich als wie ein Engels-Kind/", "tokens": ["Char\u00b7lot\u00b7te", "zei\u00b7get", "sich", "als", "wie", "ein", "En\u00b7gels\u00b7Kind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "KOUS", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Jammer-Stern ist fort/ er kan mich nicht mehr qu\u00e4hlen/", "tokens": ["Mein", "Jam\u00b7mer\u00b7S\u00b7tern", "ist", "fort", "/", "er", "kan", "mich", "nicht", "mehr", "qu\u00e4h\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKVZ", "$(", "PPER", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mir wird ein s\u00fcsser Blick und holde Gunst geg\u00fcnnt.", "tokens": ["Mir", "wird", "ein", "s\u00fcs\u00b7ser", "Blick", "und", "hol\u00b7de", "Gunst", "ge\u00b7g\u00fcnnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das Hoffnungs-Schiff darff nun die frohen Seegel spannen/", "tokens": ["Das", "Hoff\u00b7nungs\u00b7Schiff", "darff", "nun", "die", "fro\u00b7hen", "See\u00b7gel", "span\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Liebe leitet es zum sichern Haven hin/", "tokens": ["Die", "Lie\u00b7be", "lei\u00b7tet", "es", "zum", "si\u00b7chern", "Ha\u00b7ven", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Kein Ungl\u00fccks-Sturm wird es in seiner Fahrt bemannen/", "tokens": ["Kein", "Un\u00b7gl\u00fccks\u00b7Sturm", "wird", "es", "in", "sei\u00b7ner", "Fahrt", "be\u00b7man\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mein sch\u00f6ner Nord-stern lenckt sich recht nach meinem Sinn.", "tokens": ["Mein", "sch\u00f6\u00b7ner", "Nord\u00b7stern", "lenckt", "sich", "recht", "nach", "mei\u00b7nem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es wird von diesem Port zu keinen Zeiten weichen/", "tokens": ["Es", "wird", "von", "die\u00b7sem", "Port", "zu", "kei\u00b7nen", "Zei\u00b7ten", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein angenehmer West in seine Segel spielt.", "tokens": ["Ein", "an\u00b7ge\u00b7neh\u00b7mer", "West", "in", "sei\u00b7ne", "Se\u00b7gel", "spielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So wird die lange Fahrt die Wohlfahrt nun erreichen/", "tokens": ["So", "wird", "die", "lan\u00b7ge", "Fahrt", "die", "Wohl\u00b7fahrt", "nun", "er\u00b7rei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wohin mein seuffzend Hertz so lange Zeit gezielt.", "tokens": ["Wo\u00b7hin", "mein", "seuff\u00b7zend", "Hertz", "so", "lan\u00b7ge", "Zeit", "ge\u00b7zielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJD", "NN", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Hoffnungs-Ancker ruht in solchen festen Gr\u00fcnden/", "tokens": ["Der", "Hoff\u00b7nungs\u00b7An\u00b7cker", "ruht", "in", "sol\u00b7chen", "fes\u00b7ten", "Gr\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df ihn kein starcker Sturm aus seinem Lager reist/", "tokens": ["Da\u00df", "ihn", "kein", "star\u00b7cker", "Sturm", "aus", "sei\u00b7nem", "La\u00b7ger", "reist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Nun kan ich meine Ruh nach langen Suchen finden/", "tokens": ["Nun", "kan", "ich", "mei\u00b7ne", "Ruh", "nach", "lan\u00b7gen", "Su\u00b7chen", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der angenehme Port sich in der N\u00e4he weist.", "tokens": ["Der", "an\u00b7ge\u00b7neh\u00b7me", "Port", "sich", "in", "der", "N\u00e4\u00b7he", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Des Zornes strenge Fluht/ die mich sonst auffgehalten/", "tokens": ["Des", "Zor\u00b7nes", "stren\u00b7ge", "Fluht", "/", "die", "mich", "sonst", "auff\u00b7ge\u00b7hal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "PRELS", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Verkennet ihren Grimm/ nun kan ich fr\u00f6lich seyn", "tokens": ["Ver\u00b7ken\u00b7net", "ih\u00b7ren", "Grimm", "/", "nun", "kan", "ich", "fr\u00f6\u00b7lich", "seyn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NE", "$(", "ADV", "VMFIN", "PPER", "ADJD", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Mein hartes Schicksahl wil nun gantz und gar veralten/", "tokens": ["Mein", "har\u00b7tes", "Schick\u00b7sahl", "wil", "nun", "gantz", "und", "gar", "ver\u00b7al\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "ADV", "ADV", "KON", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Es giebt den sch\u00f6nsten Blick und angenehmsten Schein.", "tokens": ["Es", "giebt", "den", "sch\u00f6ns\u00b7ten", "Blick", "und", "an\u00b7ge\u00b7nehms\u00b7ten", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Charlotte l\u00e4st den Mund und ihre Wangen k\u00fcssen/", "tokens": ["Char\u00b7lot\u00b7te", "l\u00e4st", "den", "Mund", "und", "ih\u00b7re", "Wan\u00b7gen", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "KON", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Mein Lippen-Opffer brennt auf ihrer Marmor-Brust", "tokens": ["Mein", "Lip\u00b7pen\u00b7Opf\u00b7fer", "brennt", "auf", "ih\u00b7rer", "Mar\u00b7mor\u00b7Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Mein froher Geist wei\u00df nichts von Furcht und Hindernissen", "tokens": ["Mein", "fro\u00b7her", "Geist", "wei\u00df", "nichts", "von", "Furcht", "und", "Hin\u00b7der\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PIS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Er kennet keine Angst/ er sp\u00fchret lauter Lust.", "tokens": ["Er", "ken\u00b7net", "kei\u00b7ne", "Angst", "/", "er", "sp\u00fch\u00b7ret", "lau\u00b7ter", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$(", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Mir steht das Paradie\u00df der s\u00fcssen Wollust offen/", "tokens": ["Mir", "steht", "das", "Pa\u00b7ra\u00b7die\u00df", "der", "s\u00fcs\u00b7sen", "Wol\u00b7lust", "of\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Das Thor ist auffgethan so sonst verschlossen war/", "tokens": ["Das", "Thor", "ist", "auff\u00b7ge\u00b7than", "so", "sonst", "ver\u00b7schlos\u00b7sen", "war", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "ADV", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Mein Sehnen ist erf\u00fcllt/ es trifft mir zu mein Hoffen/", "tokens": ["Mein", "Seh\u00b7nen", "ist", "er\u00b7f\u00fcllt", "/", "es", "trifft", "mir", "zu", "mein", "Hof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$(", "PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ich halte gantz vergn\u00fcgt ein frohes Jubel-Jahr.", "tokens": ["Ich", "hal\u00b7te", "gantz", "ver\u00b7gn\u00fcgt", "ein", "fro\u00b7hes", "Ju\u00b7bel\u00b7Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Mein Geist kan seine Lust an s\u00fcssen Blicken haben", "tokens": ["Mein", "Geist", "kan", "sei\u00b7ne", "Lust", "an", "s\u00fcs\u00b7sen", "Bli\u00b7cken", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Kein strenger Zorn verstellt dein sch\u00f6nes Angesicht;", "tokens": ["Kein", "stren\u00b7ger", "Zorn", "ver\u00b7stellt", "dein", "sch\u00f6\u00b7nes", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Dein sch\u00f6nes Wesen wil die matten Sinnen laben/", "tokens": ["Dein", "sch\u00f6\u00b7nes", "We\u00b7sen", "wil", "die", "mat\u00b7ten", "Sin\u00b7nen", "la\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Du bist mein Schatz der Welt/ mein Leben/ und mein Licht.", "tokens": ["Du", "bist", "mein", "Schatz", "der", "Welt", "/", "mein", "Le\u00b7ben", "/", "und", "mein", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ART", "NN", "$(", "PPOSAT", "NN", "$(", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "So hab ich was ich wil/ ich k\u00fcsse mein Verlangen/", "tokens": ["So", "hab", "ich", "was", "ich", "wil", "/", "ich", "k\u00fcs\u00b7se", "mein", "Ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PWS", "PPER", "VMFIN", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Mein Klagen h\u00f6ret auf/ mein W\u00fcnschen ist erh\u00f6rt/", "tokens": ["Mein", "Kla\u00b7gen", "h\u00f6\u00b7ret", "auf", "/", "mein", "W\u00fcn\u00b7schen", "ist", "er\u00b7h\u00f6rt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "$(", "PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ich kan den Rosen-Mund und deine Brust umfangen/", "tokens": ["Ich", "kan", "den", "Ro\u00b7sen\u00b7Mund", "und", "dei\u00b7ne", "Brust", "um\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "KON", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der sanfften K\u00fcsse-Thau die matte Seele nehret.", "tokens": ["Der", "sanff\u00b7ten", "K\u00fcs\u00b7se\u00b7\u00b7T\u00b7hau", "die", "mat\u00b7te", "See\u00b7le", "neh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.37": {"text": "Ach bleibe so gesinnt mein wehrter Schatz im Lieben/", "tokens": ["Ach", "blei\u00b7be", "so", "ge\u00b7sinnt", "mein", "wehr\u00b7ter", "Schatz", "im", "Lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "ADJD", "PPOSAT", "ADJA", "NN", "APPRART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Kein Neidhart treffe uns mit seinem Schlangen-Gifft/", "tokens": ["Kein", "Neid\u00b7hart", "tref\u00b7fe", "uns", "mit", "sei\u00b7nem", "Schlan\u00b7gen\u00b7Gifft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Kein Schein der Eyversucht mu\u00df unser Brust betr\u00fcben/", "tokens": ["Kein", "Schein", "der", "Ey\u00b7ver\u00b7sucht", "mu\u00df", "un\u00b7ser", "Brust", "be\u00b7tr\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So wei\u00df ich/ da\u00df mein Gl\u00fcck mein Ungl\u00fcck \u00fcbertrifft.", "tokens": ["So", "wei\u00df", "ich", "/", "da\u00df", "mein", "Gl\u00fcck", "mein", "Un\u00b7gl\u00fcck", "\u00fc\u00b7bert\u00b7rifft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PPOSAT", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}