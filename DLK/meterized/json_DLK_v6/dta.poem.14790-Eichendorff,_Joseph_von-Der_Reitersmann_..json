{"dta.poem.14790": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Reitersmann .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ach Gott! wann wird sich doch r\u00f6then,", "tokens": ["Ach", "Gott", "!", "wann", "wird", "sich", "doch", "r\u00f6\u00b7then", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "PWAV", "VAFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die fr\u00f6hliche Morgenstund'!", "tokens": ["Die", "fr\u00f6h\u00b7li\u00b7che", "Mor\u00b7gen\u00b7stund'", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich m\u00f6chte hinausgeh'n und beten,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "hin\u00b7aus\u00b7geh'n", "und", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Und beten aus Herzensgrund!", "tokens": ["Und", "be\u00b7ten", "aus", "Her\u00b7zens\u00b7grund", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}