{"textgrid.poem.47688": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht mehr zu dir zu gehen,", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht mehr zu dir zu gehen,", "tokens": ["Nicht", "mehr", "zu", "dir", "zu", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Beschlo\u00df ich und beschwor ich,", "tokens": ["Be\u00b7schlo\u00df", "ich", "und", "be\u00b7schwor", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und gehe jeden Abend,", "tokens": ["Und", "ge\u00b7he", "je\u00b7den", "A\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denn jede Kraft und jeden Halt verlor ich.", "tokens": ["Denn", "je\u00b7de", "Kraft", "und", "je\u00b7den", "Halt", "ver\u00b7lor", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KON", "PIAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich m\u00f6chte nicht mehr leben,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "nicht", "mehr", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "M\u00f6cht' Augenblicks verderben,", "tokens": ["M\u00f6cht'", "Au\u00b7gen\u00b7blicks", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und m\u00f6chte doch auch leben", "tokens": ["Und", "m\u00f6ch\u00b7te", "doch", "auch", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr dich, mit dir, und nimmer, nimmer sterben.", "tokens": ["F\u00fcr", "dich", ",", "mit", "dir", ",", "und", "nim\u00b7mer", ",", "nim\u00b7mer", "ster\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "$,", "KON", "ADV", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ach rede, sprich ein Wort nur,", "tokens": ["Ach", "re\u00b7de", ",", "sprich", "ein", "Wort", "nur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$,", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein einziges, ein klares;", "tokens": ["Ein", "ein\u00b7zi\u00b7ges", ",", "ein", "kla\u00b7res", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Gieb Leben oder Tod mir,", "tokens": ["Gieb", "Le\u00b7ben", "o\u00b7der", "Tod", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur dein Gef\u00fchl enth\u00fclle mir, dein wahres!", "tokens": ["Nur", "dein", "Ge\u00b7f\u00fchl", "ent\u00b7h\u00fcl\u00b7le", "mir", ",", "dein", "wah\u00b7res", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}