{"textgrid.poem.46763": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ach von meinem lieben Schw\u00e4rmchen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach von meinem lieben Schw\u00e4rmchen", "tokens": ["Ach", "von", "mei\u00b7nem", "lie\u00b7ben", "Schw\u00e4rm\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die zwei kleinsten, die zwei feinsten,", "tokens": ["Die", "zwei", "kleins\u00b7ten", ",", "die", "zwei", "feins\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "ADJA", "$,", "PRELS", "CARD", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer unter sich am einsten,", "tokens": ["Im\u00b7mer", "un\u00b7ter", "sich", "am", "eins\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "APPRART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die sich hatten lieb am reinsten,", "tokens": ["Die", "sich", "hat\u00b7ten", "lieb", "am", "reins\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VAFIN", "ADJD", "APPRART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie sie mit geschlungnen Aermchen", "tokens": ["Wie", "sie", "mit", "ge\u00b7schlung\u00b7nen", "A\u00b7erm\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Eines um des andern N\u00e4ckchen,", "tokens": ["Ei\u00b7nes", "um", "des", "an\u00b7dern", "N\u00e4ck\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Eines an des andern B\u00e4ckchen,", "tokens": ["Ei\u00b7nes", "an", "des", "an\u00b7dern", "B\u00e4ck\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sa\u00dfen zwei auf einem St\u00fchlchen,", "tokens": ["Sa\u00b7\u00dfen", "zwei", "auf", "ei\u00b7nem", "St\u00fchl\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "CARD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Lehnten zwei an einem Pf\u00fchlchen,", "tokens": ["Lehn\u00b7ten", "zwei", "an", "ei\u00b7nem", "Pf\u00fchl\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Spielten zwei auf einem Tischchen;", "tokens": ["Spiel\u00b7ten", "zwei", "auf", "ei\u00b7nem", "Tischchen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "CARD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "So im golden sch\u00f6nsten Nischchen", "tokens": ["So", "im", "gol\u00b7den", "sch\u00f6ns\u00b7ten", "Nischchen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Paradiesesweiter Hallen,", "tokens": ["Pa\u00b7ra\u00b7die\u00b7ses\u00b7wei\u00b7ter", "Hal\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Wie einst hier im engen St\u00fcbchen,", "tokens": ["Wie", "einst", "hier", "im", "en\u00b7gen", "St\u00fcb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Soll mein M\u00e4dchen und mein B\u00fcbchen,", "tokens": ["Soll", "mein", "M\u00e4d\u00b7chen", "und", "mein", "B\u00fcb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Sitzen, allen", "tokens": ["Sit\u00b7zen", ",", "al\u00b7len"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "PIAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.16": {"text": "Engeln itzt ein Wohlgefallen,", "tokens": ["En\u00b7geln", "itzt", "ein", "Wohl\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Mit den leisen Wangengr\u00fcbchen,", "tokens": ["Mit", "den", "lei\u00b7sen", "Wan\u00b7gen\u00b7gr\u00fcb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Und ihr Unschuldslallen,", "tokens": ["Und", "ihr", "Un\u00b7schulds\u00b7lal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "Ihres Lachens Schallen,", "tokens": ["Ih\u00b7res", "La\u00b7chens", "Schal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.20": {"text": "Mache wie mein Herz den Himmel wallen!", "tokens": ["Ma\u00b7che", "wie", "mein", "Herz", "den", "Him\u00b7mel", "wal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}