{"textgrid.poem.55129": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Seefahrt", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lange Tag' und N\u00e4chte stand mein Schiff befrachtet;", "tokens": ["Lan\u00b7ge", "Tag'", "und", "N\u00e4ch\u00b7te", "stand", "mein", "Schiff", "be\u00b7frach\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "G\u00fcnst'ger Winde harrend, sa\u00df mit treuen Freunden,", "tokens": ["G\u00fcnst'\u00b7ger", "Win\u00b7de", "har\u00b7rend", ",", "sa\u00df", "mit", "treu\u00b7en", "Freun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Mir Geduld und guten Mut erzechend,", "tokens": ["Mir", "Ge\u00b7duld", "und", "gu\u00b7ten", "Mut", "er\u00b7ze\u00b7chend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ich im Hafen.", "tokens": ["Ich", "im", "Ha\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Und sie waren doppelt ungeduldig:", "tokens": ["Und", "sie", "wa\u00b7ren", "dop\u00b7pelt", "un\u00b7ge\u00b7dul\u00b7dig", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "\u00bbgerne g\u00f6nnen wir die schnellste Reise,", "tokens": ["\u00bb", "ger\u00b7ne", "g\u00f6n\u00b7nen", "wir", "die", "schnells\u00b7te", "Rei\u00b7se", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVINF", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Gern die hohe Fahrt dir; G\u00fcterf\u00fclle", "tokens": ["Gern", "die", "ho\u00b7he", "Fahrt", "dir", ";", "G\u00fc\u00b7ter\u00b7f\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "PPER", "$.", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wartet dr\u00fcben in den Welten deiner,", "tokens": ["War\u00b7tet", "dr\u00fc\u00b7ben", "in", "den", "Wel\u00b7ten", "dei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "PPOSAT", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wird R\u00fcckkehrendem in unsern Armen", "tokens": ["Wird", "R\u00fcck\u00b7keh\u00b7ren\u00b7dem", "in", "un\u00b7sern", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Lieb und Preis dir.\u00ab", "tokens": ["Lieb", "und", "Preis", "dir", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "NN", "PPER", "$.", "$("], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Und am fr\u00fchen Morgen ward's Get\u00fcmmel,", "tokens": ["Und", "am", "fr\u00fc\u00b7hen", "Mor\u00b7gen", "ward's", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und dem Schlaf entjauchzt uns der Matrose,", "tokens": ["Und", "dem", "Schlaf", "ent\u00b7jauchzt", "uns", "der", "Mat\u00b7ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Alles wimmelt, alles lebet, webet,", "tokens": ["Al\u00b7les", "wim\u00b7melt", ",", "al\u00b7les", "le\u00b7bet", ",", "we\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mit dem ersten Segenshauch zu schiffen.", "tokens": ["Mit", "dem", "ers\u00b7ten", "Se\u00b7gens\u00b7hauch", "zu", "schif\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Und die Segel bl\u00fchen in dem Hauche,", "tokens": ["Und", "die", "Se\u00b7gel", "bl\u00fc\u00b7hen", "in", "dem", "Hau\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und die Sonne lockt mit Feuerliebe;", "tokens": ["Und", "die", "Son\u00b7ne", "lockt", "mit", "Feu\u00b7er\u00b7lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ziehn die Segel, ziehn die hohen Wolken,", "tokens": ["Ziehn", "die", "Se\u00b7gel", ",", "ziehn", "die", "ho\u00b7hen", "Wol\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Jauchzen an dem Ufer alle Freunde", "tokens": ["Jauch\u00b7zen", "an", "dem", "U\u00b7fer", "al\u00b7le", "Freun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Hoffnungslieder nach, im Freudetaumel", "tokens": ["Hoff\u00b7nungs\u00b7lie\u00b7der", "nach", ",", "im", "Freu\u00b7de\u00b7tau\u00b7mel"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Reisefreuden w\u00e4hnend wie des Einschiffmorgens,", "tokens": ["Rei\u00b7se\u00b7freu\u00b7den", "w\u00e4h\u00b7nend", "wie", "des", "Ein\u00b7schiff\u00b7mor\u00b7gens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Wie der ersten hohen Sternenn\u00e4chte.", "tokens": ["Wie", "der", "ers\u00b7ten", "ho\u00b7hen", "Ster\u00b7nen\u00b7n\u00e4ch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Aber gottgesandte Wechselwinde treiben", "tokens": ["A\u00b7ber", "gott\u00b7ge\u00b7sand\u00b7te", "Wech\u00b7sel\u00b7win\u00b7de", "trei\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Seitw\u00e4rts ihn der vorgesteckten Fahrt ab,", "tokens": ["Seit\u00b7w\u00e4rts", "ihn", "der", "vor\u00b7ge\u00b7steck\u00b7ten", "Fahrt", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Und er scheint sich ihnen hinzugeben,", "tokens": ["Und", "er", "scheint", "sich", "ih\u00b7nen", "hin\u00b7zu\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "PPER", "VVIZU", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Strebet leise sie zu \u00fcberlisten,", "tokens": ["Stre\u00b7bet", "lei\u00b7se", "sie", "zu", "\u00fc\u00b7berl\u00b7is\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Treu dem Zweck auch auf dem schiefen Wege.", "tokens": ["Treu", "dem", "Zweck", "auch", "auf", "dem", "schie\u00b7fen", "We\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Aber aus der dumpfen, grauen Ferne", "tokens": ["A\u00b7ber", "aus", "der", "dum\u00b7pfen", ",", "grau\u00b7en", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "K\u00fcndet leisewandelnd sich der Sturm an,", "tokens": ["K\u00fcn\u00b7det", "lei\u00b7se\u00b7wan\u00b7delnd", "sich", "der", "Sturm", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Dr\u00fcckt die V\u00f6gel nieder aufs Gew\u00e4sser,", "tokens": ["Dr\u00fcckt", "die", "V\u00f6\u00b7gel", "nie\u00b7der", "aufs", "Ge\u00b7w\u00e4s\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Dr\u00fcckt der Menschen schwellend Herz darnieder,", "tokens": ["Dr\u00fcckt", "der", "Men\u00b7schen", "schwel\u00b7lend", "Herz", "dar\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "NN", "PAV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und er kommt. Vor seinem starren W\u00fcten", "tokens": ["Und", "er", "kommt", ".", "Vor", "sei\u00b7nem", "star\u00b7ren", "W\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Streckt der Schiffer klug die Segel nieder,", "tokens": ["Streckt", "der", "Schif\u00b7fer", "klug", "die", "Se\u00b7gel", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mit dem angsterf\u00fcllten Balle spielen", "tokens": ["Mit", "dem", "angs\u00b7ter\u00b7f\u00fcll\u00b7ten", "Bal\u00b7le", "spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wind und Wellen.", "tokens": ["Wind", "und", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.8": {"line.1": {"text": "Und an jenem Ufer dr\u00fcben stehen", "tokens": ["Und", "an", "je\u00b7nem", "U\u00b7fer", "dr\u00fc\u00b7ben", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Freund' und Lieben, beben auf dem Festen:", "tokens": ["Freund'", "und", "Lie\u00b7ben", ",", "be\u00b7ben", "auf", "dem", "Fes\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "\u00bbach, warum ist er nicht hier geblieben!", "tokens": ["\u00bb", "ach", ",", "wa\u00b7rum", "ist", "er", "nicht", "hier", "ge\u00b7blie\u00b7ben", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "PWAV", "VAFIN", "PPER", "PTKNEG", "ADV", "VVPP", "$."], "meter": "+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Ach, der Sturm! Verschlagen weg vom Gl\u00fccke!", "tokens": ["Ach", ",", "der", "Sturm", "!", "Ver\u00b7schla\u00b7gen", "weg", "vom", "Gl\u00fc\u00b7cke", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "$.", "NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Soll der Gute so zugrunde gehen?", "tokens": ["Soll", "der", "Gu\u00b7te", "so", "zu\u00b7grun\u00b7de", "ge\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ach, er sollte, ach, er k\u00f6nnte! G\u00f6tter!\u00ab", "tokens": ["Ach", ",", "er", "soll\u00b7te", ",", "ach", ",", "er", "k\u00f6nn\u00b7te", "!", "G\u00f6t\u00b7ter", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ITJ", "$,", "PPER", "VMFIN", "$,", "ITJ", "$,", "PPER", "VMFIN", "$.", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Doch er stehet m\u00e4nnlich an dem Steuer;", "tokens": ["Doch", "er", "ste\u00b7het", "m\u00e4nn\u00b7lich", "an", "dem", "Steu\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mit dem Schiffe spielen Wind und Wellen;", "tokens": ["Mit", "dem", "Schif\u00b7fe", "spie\u00b7len", "Wind", "und", "Wel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wind und Wellen nicht mit seinem Herzen:", "tokens": ["Wind", "und", "Wel\u00b7len", "nicht", "mit", "sei\u00b7nem", "Her\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Herrschend blickt er auf die grimme Tiefe", "tokens": ["Herr\u00b7schend", "blickt", "er", "auf", "die", "grim\u00b7me", "Tie\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und vertrauet, scheiternd oder landend,", "tokens": ["Und", "ver\u00b7trau\u00b7et", ",", "schei\u00b7ternd", "o\u00b7der", "lan\u00b7dend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Seinen G\u00f6ttern.", "tokens": ["Sei\u00b7nen", "G\u00f6t\u00b7tern", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}