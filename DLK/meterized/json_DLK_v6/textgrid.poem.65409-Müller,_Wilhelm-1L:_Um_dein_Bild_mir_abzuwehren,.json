{"textgrid.poem.65409": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Um dein Bild mir abzuwehren,", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um dein Bild mir abzuwehren,", "tokens": ["Um", "dein", "Bild", "mir", "ab\u00b7zu\u00b7weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PPER", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bin ich auf das Feld gegangen:", "tokens": ["Bin", "ich", "auf", "das", "Feld", "ge\u00b7gan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, da sah ich goldne \u00c4hren", "tokens": ["Ach", ",", "da", "sah", "ich", "gold\u00b7ne", "\u00c4h\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf den Pfad her\u00fcberhangen!", "tokens": ["Auf", "den", "Pfad", "her\u00b7\u00fc\u00b7berh\u00b7an\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, da sah ich goldne Ranken", "tokens": ["Ach", ",", "da", "sah", "ich", "gold\u00b7ne", "Ran\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich um wei\u00dfe St\u00e4mme schlingen!", "tokens": ["Sich", "um", "wei\u00b7\u00dfe", "St\u00e4m\u00b7me", "schlin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, da flogen die Gedanken", "tokens": ["Ach", ",", "da", "flo\u00b7gen", "die", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Heim zu deinen Lockenringen!", "tokens": ["Heim", "zu", "dei\u00b7nen", "Lo\u00b7cken\u00b7rin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}