{"dta.poem.17264": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "114.  Ein Lamb bezwingt den Drachen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Vertraue GOtt/ der Drach wird leichtlich \u00fcberwunden/", "tokens": ["Ver\u00b7trau\u00b7e", "Gott", "/", "der", "Drach", "wird", "leicht\u00b7lich", "\u00fc\u00b7berw\u00b7un\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ART", "NN", "VAFIN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat ihn doch nur ein Lamm gef\u00e4llet und gebunden!", "tokens": ["Hat", "ihn", "doch", "nur", "ein", "Lamm", "ge\u00b7f\u00e4l\u00b7let", "und", "ge\u00b7bun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}