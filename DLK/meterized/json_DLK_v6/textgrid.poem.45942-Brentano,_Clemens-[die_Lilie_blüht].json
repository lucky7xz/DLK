{"textgrid.poem.45942": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[die Lilie bl\u00fcht]", "genre": "verse", "period": "N.A.", "pub_year": 1811, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Lilie bl\u00fcht, ich bin die fromme Biene,", "tokens": ["Die", "Li\u00b7lie", "bl\u00fcht", ",", "ich", "bin", "die", "from\u00b7me", "Bie\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die in der Bl\u00e4tter keuschen Busen sinkt,", "tokens": ["Die", "in", "der", "Bl\u00e4t\u00b7ter", "keu\u00b7schen", "Bu\u00b7sen", "sinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und s\u00fc\u00dfen Tau und milden Honig trinkt,", "tokens": ["Und", "s\u00fc\u00b7\u00dfen", "Tau", "und", "mil\u00b7den", "Ho\u00b7nig", "trinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch lebt ihr Glanz, und bleibet ewig gr\u00fcne", "tokens": ["Doch", "lebt", "ihr", "Glanz", ",", "und", "blei\u00b7bet", "e\u00b7wig", "gr\u00fc\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So ist dann selig mein Gem\u00fct", "tokens": ["So", "ist", "dann", "se\u00b7lig", "mein", "Ge\u00b7m\u00fct"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil meine Lilie bl\u00fcht!", "tokens": ["Weil", "mei\u00b7ne", "Li\u00b7lie", "bl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Lilie bl\u00fcht, Gott, la\u00df den Schein verziehn,", "tokens": ["Die", "Li\u00b7lie", "bl\u00fcht", ",", "Gott", ",", "la\u00df", "den", "Schein", "ver\u00b7ziehn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "NN", "$,", "VVIMP", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Damit die Zeit des Sommers langsam geht,", "tokens": ["Da\u00b7mit", "die", "Zeit", "des", "Som\u00b7mers", "lang\u00b7sam", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und weder Frost noch andre Not entsteht,", "tokens": ["Und", "we\u00b7der", "Frost", "noch", "and\u00b7re", "Not", "ent\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KON", "NN", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So wird mein Gl\u00fcck in dieser Lilie bl\u00fchn,", "tokens": ["So", "wird", "mein", "Gl\u00fcck", "in", "die\u00b7ser", "Li\u00b7lie", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So klingt mein s\u00fc\u00dfes Freudenlied:", "tokens": ["So", "klingt", "mein", "s\u00fc\u00b7\u00dfes", "Freu\u00b7den\u00b7lied", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach, meine Lilie bl\u00fcht!", "tokens": ["Ach", ",", "mei\u00b7ne", "Li\u00b7lie", "bl\u00fcht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}