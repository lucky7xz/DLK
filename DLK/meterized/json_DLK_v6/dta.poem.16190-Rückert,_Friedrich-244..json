{"dta.poem.16190": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "244.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Kind, o k\u00f6nnt' ich dich, da du nun auf die Schwellen", "tokens": ["Mein", "Kind", ",", "o", "k\u00f6nnt'", "ich", "dich", ",", "da", "du", "nun", "auf", "die", "Schwel\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "FM", "VMFIN", "PPER", "PRF", "$,", "KOUS", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Lebens eintritst, gleich ans Ziel im Geiste stellen;", "tokens": ["Des", "Le\u00b7bens", "ein\u00b7tritst", ",", "gleich", "ans", "Ziel", "im", "Geis\u00b7te", "stel\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "APPRART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "Damit du, was gethan am Schlu\u00df einst deiner Bahn", "tokens": ["Da\u00b7mit", "du", ",", "was", "ge\u00b7than", "am", "Schlu\u00df", "einst", "dei\u00b7ner", "Bahn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "PRELS", "VVPP", "APPRART", "NN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du m\u00f6chtest, th\u00e4test jetzt, indem du sie tritst an.", "tokens": ["Du", "m\u00f6ch\u00b7test", ",", "th\u00e4\u00b7test", "jetzt", ",", "in\u00b7dem", "du", "sie", "tritst", "an", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mein Kind, auf diesem Weg bin ich vor dir gegangen;", "tokens": ["Mein", "Kind", ",", "auf", "die\u00b7sem", "Weg", "bin", "ich", "vor", "dir", "ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PDAT", "NN", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was hilfts, vor Dornen dich zu warnen und vor Schlangen?", "tokens": ["Was", "hilfts", ",", "vor", "Dor\u00b7nen", "dich", "zu", "war\u00b7nen", "und", "vor", "Schlan\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "APPR", "NN", "PPER", "PTKZU", "VVINF", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Mein Kind, mit deinem Gang heb' ich neu meine Schwingen;", "tokens": ["Mein", "Kind", ",", "mit", "dei\u00b7nem", "Gang", "heb'", "ich", "neu", "mei\u00b7ne", "Schwin\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Was selbst mir nicht gelang, das m\u00f6ge dir gelingen.", "tokens": ["Was", "selbst", "mir", "nicht", "ge\u00b7lang", ",", "das", "m\u00f6\u00b7ge", "dir", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "PTKNEG", "VVFIN", "$,", "PDS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Was selbst ich nicht errang, das m\u00f6gest du erringen;", "tokens": ["Was", "selbst", "ich", "nicht", "er\u00b7rang", ",", "das", "m\u00f6\u00b7gest", "du", "er\u00b7rin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "PTKNEG", "VVFIN", "$,", "PDS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was unvollbracht ich lie\u00df, Gott lass' es dich vollbringen.", "tokens": ["Was", "un\u00b7voll\u00b7bracht", "ich", "lie\u00df", ",", "Gott", "lass'", "es", "dich", "voll\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPER", "VVFIN", "$,", "NN", "VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Mein Kind, ich zittre beim Gedanken schon, da\u00df fallen", "tokens": ["Mein", "Kind", ",", "ich", "zitt\u00b7re", "beim", "Ge\u00b7dan\u00b7ken", "schon", ",", "da\u00df", "fal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN", "ADV", "$,", "KOUS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du k\u00f6nnest, und allein mu\u00df ich dich lassen wallen;", "tokens": ["Du", "k\u00f6n\u00b7nest", ",", "und", "al\u00b7lein", "mu\u00df", "ich", "dich", "las\u00b7sen", "wal\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KON", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Allein, in Gottes Hut, allein mit deinem Muth;", "tokens": ["Al\u00b7lein", ",", "in", "Got\u00b7tes", "Hut", ",", "al\u00b7lein", "mit", "dei\u00b7nem", "Muth", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "NN", "NN", "$,", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schreit und bedenk, da\u00df man zur\u00fcck den Schritt nie thut.", "tokens": ["Schreit", "und", "be\u00b7denk", ",", "da\u00df", "man", "zu\u00b7r\u00fcck", "den", "Schritt", "nie", "thut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,", "KOUS", "PIS", "ADV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}