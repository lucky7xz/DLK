{"textgrid.poem.64118": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Lied der Walk\u00fcre", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Froh sah' ich dich aufbl\u00fchn,", "tokens": ["Froh", "sah'", "ich", "dich", "auf\u00b7bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Du freudiger Held,", "tokens": ["Du", "freu\u00b7di\u00b7ger", "Held", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Lang folg' ich dir schwebend", "tokens": ["Lang", "fol\u00b7g'", "ich", "dir", "schwe\u00b7bend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PPER", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und schweigend gesellt.", "tokens": ["Und", "schwei\u00b7gend", "ge\u00b7sellt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Oft k\u00fc\u00dft' ich des Schlummernden", "tokens": ["Oft", "k\u00fc\u00dft'", "ich", "des", "Schlum\u00b7mern\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Schl\u00e4fe gelind", "tokens": ["Schl\u00e4\u00b7fe", "ge\u00b7lind"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und leise die Locken,", "tokens": ["Und", "lei\u00b7se", "die", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die dir wehen im Wind.", "tokens": ["Die", "dir", "we\u00b7hen", "im", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Hoch flog ich zu H\u00e4upten,", "tokens": ["Hoch", "flog", "ich", "zu", "H\u00e4up\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "\u2013 Du kanntest mich kaum \u2013", "tokens": ["\u2013", "Du", "kann\u00b7test", "mich", "kaum", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Durch die Wipfel der W\u00e4lder,", "tokens": ["Durch", "die", "Wip\u00b7fel", "der", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Dein Trost und dein Traum.", "tokens": ["Dein", "Trost", "und", "dein", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Ich brach vor dem Bugspriet", "tokens": ["Ich", "brach", "vor", "dem", "Bug\u00b7spriet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Durch Brandung dir Bahn,", "tokens": ["Durch", "Bran\u00b7dung", "dir", "Bahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Vor dem Schiffe dir schwamm ich,", "tokens": ["Vor", "dem", "Schif\u00b7fe", "dir", "schwamm", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "PPER", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wei\u00dfschwingig, ein Schwan.", "tokens": ["Wei\u00df\u00b7schwin\u00b7gig", ",", "ein", "Schwan", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Ich zog dir zum Ziele", "tokens": ["Ich", "zog", "dir", "zum", "Zie\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Den zischenden Pfeil,", "tokens": ["Den", "zi\u00b7schen\u00b7den", "Pfeil", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Aufri\u00df ich das Ro\u00df dir,", "tokens": ["Auf\u00b7ri\u00df", "ich", "das", "Ro\u00df", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PPER", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Das gestrauchelt am Steil.", "tokens": ["Das", "ge\u00b7strau\u00b7chelt", "am", "Steil", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVPP", "APPRART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Oft fing ich des Feindes", "tokens": ["Oft", "fing", "ich", "des", "Fein\u00b7des"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Geschwungenes Schwert,", "tokens": ["Ge\u00b7schwun\u00b7ge\u00b7nes", "Schwert", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Lang hab' ich die Lanzen", "tokens": ["Lang", "hab'", "ich", "die", "Lan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Vom Leib dir gewehrt.", "tokens": ["Vom", "Leib", "dir", "ge\u00b7wehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Und nun, da die Norne", "tokens": ["Und", "nun", ",", "da", "die", "Nor\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "KOUS", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Den Tod dir verh\u00e4ngt,", "tokens": ["Den", "Tod", "dir", "ver\u00b7h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Hab' ich dir den schnellsten,", "tokens": ["Hab'", "ich", "dir", "den", "schnells\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ART", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Den sch\u00f6nsten geschenkt.", "tokens": ["Den", "sch\u00f6ns\u00b7ten", "ge\u00b7schenkt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "\u00bbsieg!\u00ab riefest du selig,", "tokens": ["\u00bb", "sieg", "!", "\u00ab", "rie\u00b7fest", "du", "se\u00b7lig", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$.", "$(", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "\u00bbsieg, Sieg allerw\u00e4rts!\u00ab", "tokens": ["\u00bb", "sieg", ",", "Sieg", "al\u00b7ler\u00b7w\u00e4rts", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "ADV", "$.", "$("], "meter": "-++-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Da lenkt' ich die Lanze", "tokens": ["Da", "lenkt'", "ich", "die", "Lan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Dir ins herrliche Herz.", "tokens": ["Dir", "ins", "herr\u00b7li\u00b7che", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Du l\u00e4cheltest lieblich, \u2013", "tokens": ["Du", "l\u00e4\u00b7chel\u00b7test", "lieb\u00b7lich", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ich umfing dich im Fall \u2013", "tokens": ["Ich", "um\u00b7fing", "dich", "im", "Fall", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$("], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ich k\u00fcsse die Wunde \u2013", "tokens": ["Ich", "k\u00fcs\u00b7se", "die", "Wun\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und nun auf: \u2013 nach Walhall!", "tokens": ["Und", "nun", "auf", ":", "\u2013", "nach", "Wal\u00b7hall", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKVZ", "$.", "$(", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}