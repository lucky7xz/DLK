{"textgrid.poem.56560": {"metadata": {"author": {"name": "Groth, Klaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Grotmoder n\u00fclt inn L\u0153hnstohl", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Grotmoder n\u00fclt inn L\u0153hnstohl", "tokens": ["Grot\u00b7mo\u00b7der", "n\u00fclt", "inn", "L\u0153hn\u00b7stohl"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Un hollt de Huspostill.", "tokens": ["Un", "hollt", "de", "Hus\u00b7pos\u00b7till", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ik weet ni, wat de Olsche", "tokens": ["Ik", "weet", "ni", ",", "wat", "de", "Ol\u00b7sche"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "PRELS", "NE", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nu j\u00fcmmer l\u0119sen will!", "tokens": ["Nu", "j\u00fcm\u00b7mer", "l\u0119\u00b7sen", "will", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Se kikt sik d\u0153r \u0119r Brillglas", "tokens": ["Se", "kikt", "sik", "d\u0153r", "\u0119r", "Brill\u00b7glas"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "De Ogn noch redi blind.", "tokens": ["De", "Ogn", "noch", "re\u00b7di", "blind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "NE", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Se is noch orri str\u0119wi,", "tokens": ["Se", "is", "noch", "or\u00b7ri", "str\u0119wi", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Doch lang ni mehr keen Kind.", "tokens": ["Doch", "lang", "ni", "mehr", "ke\u00b7en", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ADV", "PIAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Vunmorgens is se g\u00e4nzli", "tokens": ["Vun\u00b7mor\u00b7gens", "is", "se", "g\u00e4nz\u00b7li"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "FM", "FM", "FM"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verbistert un verbas't,", "tokens": ["Ver\u00b7bis\u00b7tert", "un", "ver\u00b7bas't", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "FM", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Se s\u00fcht ni, dat de M\u00fcppe", "tokens": ["Se", "s\u00fcht", "ni", ",", "dat", "de", "M\u00fcp\u00b7pe"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["FM.fr", "FM.fr", "FM.fr", "$,", "PRELS", "NE", "NE"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "\u0118r anne Rocken tas't.", "tokens": ["\u0118r", "an\u00b7ne", "Ro\u00b7cken", "tas'", "t."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Se markt ni, dat de Kater", "tokens": ["Se", "markt", "ni", ",", "dat", "de", "Ka\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["FM.fr", "FM.fr", "FM.fr", "$,", "PRELS", "NE", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u0118r inne Nachm\u00fctz sl\u00f6ppt", "tokens": ["\u0118r", "in\u00b7ne", "Nach\u00b7m\u00fctz", "sl\u00f6ppt"], "token_info": ["word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Un de Kanarjenvagel", "tokens": ["Un", "de", "Ka\u00b7nar\u00b7jen\u00b7va\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["NE", "NE", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "\u0118r oppe Fingern l\u00f6ppt.", "tokens": ["\u0118r", "op\u00b7pe", "Fin\u00b7gern", "l\u00f6ppt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "De S\u00fcnn schint doch so fr\u00fcndli", "tokens": ["De", "S\u00fcnn", "schint", "doch", "so", "fr\u00fcnd\u00b7li"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Un makt \u0119r Backen roth:", "tokens": ["Un", "makt", "\u0119r", "Ba\u00b7cken", "roth", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJA", "NN", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Du lewe Gott in Himmel \u2013", "tokens": ["Du", "le\u00b7we", "Gott", "in", "Him\u00b7mel", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "De Olsche ... de is dot!", "tokens": ["De", "Ol\u00b7sche", "...", "de", "is", "dot", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "FM", "FM", "FM", "$."], "meter": "-+---+", "measure": "dactylic.init"}}}}}