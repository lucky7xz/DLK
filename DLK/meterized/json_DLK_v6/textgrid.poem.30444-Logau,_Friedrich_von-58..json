{"textgrid.poem.30444": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "58.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Welt ist voller jungen; die Welt ist voller Welten;", "tokens": ["Die", "Welt", "ist", "vol\u00b7ler", "jun\u00b7gen", ";", "die", "Welt", "ist", "vol\u00b7ler", "Wel\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "ADJA", "$.", "ART", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die Mutter mit den jungen mag keines f\u00fcr Gott gelten,", "tokens": ["Die", "Mut\u00b7ter", "mit", "den", "jun\u00b7gen", "mag", "kei\u00b7nes", "f\u00fcr", "Gott", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "VMFIN", "PIS", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+--+--+--", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sind t\u00fcchtig nicht, sind nichtig, sind arg und falsch zu schelten.", "tokens": ["Sind", "t\u00fcch\u00b7tig", "nicht", ",", "sind", "nich\u00b7tig", ",", "sind", "arg", "und", "falsch", "zu", "schel\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PTKNEG", "$,", "VAFIN", "ADJD", "$,", "VAFIN", "ADJD", "KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}