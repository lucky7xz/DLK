{"dta.poem.254": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er entdecket ihr seine Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sclaven \u00e4chtzen in den Banden/", "tokens": ["Die", "Scla\u00b7ven", "\u00e4cht\u00b7zen", "in", "den", "Ban\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ihr Gem\u00fchte ist betr\u00fcbt/", "tokens": ["Und", "ihr", "Ge\u00b7m\u00fch\u00b7te", "ist", "be\u00b7tr\u00fcbt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Warum? es ist niemand verhanden", "tokens": ["Wa\u00b7rum", "?", "es", "ist", "nie\u00b7mand", "ver\u00b7han\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "PPER", "VAFIN", "PIS", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der ihnen gute Worte giebt.", "tokens": ["Der", "ih\u00b7nen", "gu\u00b7te", "Wor\u00b7te", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie d\u00fcrffen ihre Noht nicht klagen/", "tokens": ["Sie", "d\u00fcrf\u00b7fen", "ih\u00b7re", "Noht", "nicht", "kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Damit sie ihre Henckers plagen.", "tokens": ["Da\u00b7mit", "sie", "ih\u00b7re", "Hen\u00b7ckers", "pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mit recht verfluchen sie die Ketten/", "tokens": ["Mit", "recht", "ver\u00b7flu\u00b7chen", "sie", "die", "Ket\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die sie so \u00fcberm\u00e4ssig dr\u00fcckt/", "tokens": ["Die", "sie", "so", "\u00fc\u00b7berm\u00b7\u00e4s\u00b7sig", "dr\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie w\u00fcnschen/ da\u00df sie niemahls h\u00e4tten", "tokens": ["Sie", "w\u00fcn\u00b7schen", "/", "da\u00df", "sie", "nie\u00b7mahls", "h\u00e4t\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "ADV", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dis Rund der Erden angeblickt.", "tokens": ["Dis", "Rund", "der", "Er\u00b7den", "an\u00b7ge\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jhr Jammer zwinget sie zum Sterben/", "tokens": ["Ihr", "Jam\u00b7mer", "zwin\u00b7get", "sie", "zum", "Ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und wil sie dennoch nicht verderben.", "tokens": ["Und", "wil", "sie", "den\u00b7noch", "nicht", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein Kluger hasset dieses Leben/", "tokens": ["Ein", "Klu\u00b7ger", "has\u00b7set", "die\u00b7ses", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wil gern ohne Banden gehn/", "tokens": ["Und", "wil", "gern", "oh\u00b7ne", "Ban\u00b7den", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch viele sind/ die dahin streben", "tokens": ["Doch", "vie\u00b7le", "sind", "/", "die", "da\u00b7hin", "stre\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "VAFIN", "$(", "ART", "PAV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich als Gefangene zu sehn;", "tokens": ["Sich", "als", "Ge\u00b7fan\u00b7ge\u00b7ne", "zu", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KOUS", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich selber bin in diesem Orden", "tokens": ["Ich", "sel\u00b7ber", "bin", "in", "die\u00b7sem", "Or\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Freywillig eingeschrieben worden.", "tokens": ["Frey\u00b7wil\u00b7lig", "ein\u00b7ge\u00b7schrie\u00b7ben", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Allein! die Banden/ die ich f\u00fchre/", "tokens": ["Al\u00b7lein", "!", "die", "Ban\u00b7den", "/", "die", "ich", "f\u00fch\u00b7re", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ART", "NN", "$(", "PRELS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sind angenehm/ und auch sehr leicht.", "tokens": ["Sind", "an\u00b7ge\u00b7nehm", "/", "und", "auch", "sehr", "leicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$(", "KON", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Freyheit/ die ich jetzt verliehre", "tokens": ["Die", "Frey\u00b7heit", "/", "die", "ich", "jetzt", "ver\u00b7lieh\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor einer solchen G\u00f6ttinn weicht/", "tokens": ["Vor", "ei\u00b7ner", "sol\u00b7chen", "G\u00f6t\u00b7tinn", "weicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der alle Welt begehrt zu dienen/", "tokens": ["Der", "al\u00b7le", "Welt", "be\u00b7gehrt", "zu", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn sie sich dessen darf erk\u00fchnen.", "tokens": ["Wenn", "sie", "sich", "des\u00b7sen", "darf", "er\u00b7k\u00fch\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PDS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Sch\u00f6nheit/ so eur Antlitz heget/", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "/", "so", "eur", "Ant\u00b7litz", "he\u00b7get", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der blauen Augen Wunder-Macht/", "tokens": ["Der", "blau\u00b7en", "Au\u00b7gen", "Wun\u00b7der\u00b7Macht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und was sich in dem Busen reget/", "tokens": ["Und", "was", "sich", "in", "dem", "Bu\u00b7sen", "re\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die haben mich dazu gebracht/", "tokens": ["Die", "ha\u00b7ben", "mich", "da\u00b7zu", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PRF", "PAV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die sch\u00f6nen Fesseln zu verehren/", "tokens": ["Die", "sch\u00f6\u00b7nen", "Fes\u00b7seln", "zu", "ver\u00b7eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die meine Hand und Brust beschwehren.", "tokens": ["Die", "mei\u00b7ne", "Hand", "und", "Brust", "be\u00b7schweh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Seyd nun so g\u00fctig als ihr sch\u00f6ne/", "tokens": ["Seyd", "nun", "so", "g\u00fc\u00b7tig", "als", "ihr", "sch\u00f6\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "ADV", "ADJD", "KOKOM", "PPOSAT", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Blickt euren Sclaven gn\u00e4dig an/", "tokens": ["Blickt", "eu\u00b7ren", "Scla\u00b7ven", "gn\u00e4\u00b7dig", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "G\u00f6nnt/ da\u00df ich mich nach diesen sehne/", "tokens": ["G\u00f6nnt", "/", "da\u00df", "ich", "mich", "nach", "die\u00b7sen", "seh\u00b7ne", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KOUS", "PPER", "PRF", "APPR", "PDAT", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was meine Banden mildern kan.", "tokens": ["Was", "mei\u00b7ne", "Ban\u00b7den", "mil\u00b7dern", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wird die Treue nicht gebrochen/", "tokens": ["Und", "wird", "die", "Treu\u00b7e", "nicht", "ge\u00b7bro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So werd mir Gegen-Gunst versprochen.", "tokens": ["So", "werd", "mir", "Ge\u00b7gen\u00b7Gunst", "ver\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}