{"textgrid.poem.54078": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Die Karte f\u00fcr den Pfirsich-Melba", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.85", "no:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "und so fort. Den Vogel aber hat Jakopp abgeschossen.", "tokens": ["und", "so", "fort", ".", "Den", "Vo\u00b7gel", "a\u00b7ber", "hat", "Ja\u00b7kopp", "ab\u00b7ge\u00b7schos\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKVZ", "$.", "ART", "NE", "ADV", "VAFIN", "NE", "VVPP", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Jetzt erhebt sich die Frage:", "tokens": ["Jetzt", "er\u00b7hebt", "sich", "die", "Fra\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": " wie w\u00e4re es, und wir lie\u00dfen dem Arthur f\u00fcr sein Doktorexamen kleine Antwortzettel drucken \u2013?", "tokens": ["wie", "w\u00e4\u00b7re", "es", ",", "und", "wir", "lie\u00b7\u00dfen", "dem", "Ar\u00b7thur", "f\u00fcr", "sein", "Dok\u00b7to\u00b7re\u00b7xa\u00b7men", "klei\u00b7ne", "Ant\u00b7wort\u00b7zet\u00b7tel", "dru\u00b7cken", "\u2013", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "ADJA", "NN", "VVINF", "$(", "$."], "meter": "-+--+-+-+-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}}}}