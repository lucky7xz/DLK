{"textgrid.poem.25359": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Klangen Frauenschritte hinter H\u00e4userbogen,", "tokens": ["Klan\u00b7gen", "Frau\u00b7en\u00b7schrit\u00b7te", "hin\u00b7ter", "H\u00e4u\u00b7ser\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Folgtest du durch Gassen hingezogen", "tokens": ["Folg\u00b7test", "du", "durch", "Gas\u00b7sen", "hin\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Feilen Blicken und geschminkten Wangen nach,", "tokens": ["Fei\u00b7len", "Bli\u00b7cken", "und", "ge\u00b7schmink\u00b7ten", "Wan\u00b7gen", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "H\u00f6rtest in den L\u00fcften Engelsch\u00f6re musizieren,", "tokens": ["H\u00f6r\u00b7test", "in", "den", "L\u00fcf\u00b7ten", "En\u00b7gel\u00b7sch\u00f6\u00b7re", "mu\u00b7si\u00b7zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NE", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "Sp\u00fcrtest Gl\u00fcck, dich zu zerst\u00f6ren, zu verlieren,", "tokens": ["Sp\u00fcr\u00b7test", "Gl\u00fcck", ",", "dich", "zu", "zer\u00b7st\u00f6\u00b7ren", ",", "zu", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Branntest dunkel nach Erniedrigung und Schmach.", "tokens": ["Brann\u00b7test", "dun\u00b7kel", "nach", "Er\u00b7nied\u00b7ri\u00b7gung", "und", "Schmach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Bis du dich an Eklem vollgetrunken,", "tokens": ["Bis", "du", "dich", "an", "Ek\u00b7lem", "voll\u00b7ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Vor dem ausgebrannten K\u00f6rper hingesunken,", "tokens": ["Vor", "dem", "aus\u00b7ge\u00b7brann\u00b7ten", "K\u00f6r\u00b7per", "hin\u00b7ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Dein Gesicht dem eingeschrumpften Scho\u00df verw\u00fchlt \u2013", "tokens": ["Dein", "Ge\u00b7sicht", "dem", "ein\u00b7ge\u00b7schrumpf\u00b7ten", "Scho\u00df", "ver\u00b7w\u00fchlt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "F\u00fchltest, wie aus Schmach dir Gl\u00fcck gesch\u00e4he,", "tokens": ["F\u00fchl\u00b7test", ",", "wie", "aus", "Schmach", "dir", "Gl\u00fcck", "ge\u00b7sch\u00e4\u00b7he", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "APPR", "NN", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und des Gottes tausendfache N\u00e4he", "tokens": ["Und", "des", "Got\u00b7tes", "tau\u00b7send\u00b7fa\u00b7che", "N\u00e4\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Dich in Himmelsreinheit h\u00f6be, niegef\u00fchlt.", "tokens": ["Dich", "in", "Him\u00b7mels\u00b7rein\u00b7heit", "h\u00f6\u00b7be", ",", "nie\u00b7ge\u00b7f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}