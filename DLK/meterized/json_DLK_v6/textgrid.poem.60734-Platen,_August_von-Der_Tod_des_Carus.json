{"textgrid.poem.60734": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Der Tod des Carus", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mutig stand an Persiens Grenzen Roms erprobtes Heer im Feld,", "tokens": ["Mu\u00b7tig", "stand", "an", "Per\u00b7siens", "Gren\u00b7zen", "Roms", "er\u00b7prob\u00b7tes", "Heer", "im", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NE", "NE", "NE", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Carus sa\u00df in seinem Zelte, der den Purpur trug, ein Held.", "tokens": ["Ca\u00b7rus", "sa\u00df", "in", "sei\u00b7nem", "Zel\u00b7te", ",", "der", "den", "Pur\u00b7pur", "trug", ",", "ein", "Held", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Persiens Abgesandte beugten sich vor Roms erneuter Macht,", "tokens": ["Per\u00b7siens", "Ab\u00b7ge\u00b7sand\u00b7te", "beug\u00b7ten", "sich", "vor", "Roms", "er\u00b7neu\u00b7ter", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PRF", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Flehn um Frieden an den Kaiser; doch der Kaiser w\u00e4hlt die Schlacht.", "tokens": ["Flehn", "um", "Frie\u00b7den", "an", "den", "Kai\u00b7ser", ";", "doch", "der", "Kai\u00b7ser", "w\u00e4hlt", "die", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "APPR", "ART", "NN", "$.", "ADV", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Kampfbegierig sind die Scharen, die er fern und nah beschied,", "tokens": ["Kampf\u00b7be\u00b7gie\u00b7rig", "sind", "die", "Scha\u00b7ren", ",", "die", "er", "fern", "und", "nah", "be\u00b7schied", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Durch das Heer, aus tausend Kehlen, ging das hohe Siegeslied:", "tokens": ["Durch", "das", "Heer", ",", "aus", "tau\u00b7send", "Keh\u00b7len", ",", "ging", "das", "ho\u00b7he", "Sie\u00b7ges\u00b7lied", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "CARD", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Weh den Persern, R\u00f6mer kommen, R\u00f6mer ziehn im Flug heran,", "tokens": ["Weh", "den", "Per\u00b7sern", ",", "R\u00f6\u00b7mer", "kom\u00b7men", ",", "R\u00f6\u00b7mer", "ziehn", "im", "Flug", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "VVINF", "$,", "NE", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "R\u00e4chen ihren Imperator, r\u00e4chen dich, Valerian!", "tokens": ["R\u00e4\u00b7chen", "ih\u00b7ren", "Im\u00b7pe\u00b7ra\u00b7tor", ",", "r\u00e4\u00b7chen", "dich", ",", "Va\u00b7le\u00b7ri\u00b7an", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "$,", "NE", "$."], "meter": "+-+--+--+-+++-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.5": {"line.1": {"text": "Durch Verrat und Mi\u00dfgeschick nur trugst du ein barbarisch Joch;", "tokens": ["Durch", "Ver\u00b7rat", "und", "Mi\u00df\u00b7ge\u00b7schick", "nur", "trugst", "du", "ein", "bar\u00b7ba\u00b7risch", "Joch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "VVFIN", "PPER", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Aber, starbst du auch im Kerker, deine R\u00e4cher leben noch!", "tokens": ["A\u00b7ber", ",", "starbst", "du", "auch", "im", "Ker\u00b7ker", ",", "dei\u00b7ne", "R\u00e4\u00b7cher", "le\u00b7ben", "noch", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,", "PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Wenn zu Pferd stieg Artaxerxes, ungez\u00e4hmten Stolz im Blick,", "tokens": ["Wenn", "zu", "Pferd", "stieg", "Ar\u00b7ta\u00b7xe\u00b7rxes", ",", "un\u00b7ge\u00b7z\u00e4hm\u00b7ten", "Stolz", "im", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "VVFIN", "NE", "$,", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Setzte seinen Fu\u00df der K\u00f6nig auf Valerians Genick.", "tokens": ["Setz\u00b7te", "sei\u00b7nen", "Fu\u00df", "der", "K\u00f6\u00b7nig", "auf", "Va\u00b7le\u00b7ri\u00b7ans", "Ge\u00b7nick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "APPR", "NE", "NN", "$."], "meter": "+-+-+-+--+-+--+", "measure": "trochaic.septa.relaxed"}}, "stanza.7": {"line.1": {"text": "Ach, und Rom in seiner Schande, das vordem die Welt gewann,", "tokens": ["Ach", ",", "und", "Rom", "in", "sei\u00b7ner", "Schan\u00b7de", ",", "das", "vor\u00b7dem", "die", "Welt", "ge\u00b7wann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "NE", "APPR", "PPOSAT", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+--+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Flehte zum Olymp um einen, flehte nur um Einen Mann!", "tokens": ["Fleh\u00b7te", "zum", "O\u00b7lymp", "um", "ei\u00b7nen", ",", "fleh\u00b7te", "nur", "um", "Ei\u00b7nen", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "APPR", "ART", "$,", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.8": {"line.1": {"text": "Aber M\u00e4nner sind erstanden, M\u00e4nner f\u00fchren uns zur Schlacht:", "tokens": ["A\u00b7ber", "M\u00e4n\u00b7ner", "sind", "er\u00b7stan\u00b7den", ",", "M\u00e4n\u00b7ner", "f\u00fch\u00b7ren", "uns", "zur", "Schlacht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "VVPP", "$,", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Scipio, Marius und Pompejus sind aus ihrem Grab erwacht!", "tokens": ["Sci\u00b7pio", ",", "Ma\u00b7rius", "und", "Pom\u00b7pe\u00b7jus", "sind", "aus", "ih\u00b7rem", "Grab", "er\u00b7wacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "KON", "NE", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+-+--+-+-+-+", "measure": "iambic.septa.invert"}}, "stanza.9": {"line.1": {"text": "Unser Kaiser Aurelianus hat die Goten \u00fcbermannt,", "tokens": ["Un\u00b7ser", "Kai\u00b7ser", "Au\u00b7re\u00b7li\u00b7a\u00b7nus", "hat", "die", "Go\u00b7ten", "\u00fc\u00b7berm\u00b7annt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+--+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Welche deinen Wundertempel, Ephesus, zu Staub verbrannt.", "tokens": ["Wel\u00b7che", "dei\u00b7nen", "Wun\u00b7der\u00b7tem\u00b7pel", ",", "E\u00b7phe\u00b7sus", ",", "zu", "Staub", "ver\u00b7brannt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAT", "PPOSAT", "NN", "$,", "NE", "$,", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.10": {"line.1": {"text": "Unser Kaiser Aurelianus hat die stolze Frau besiegt,", "tokens": ["Un\u00b7ser", "Kai\u00b7ser", "Au\u00b7re\u00b7li\u00b7a\u00b7nus", "hat", "die", "stol\u00b7ze", "Frau", "be\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+--+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Welche nun im stillen Tibur ihre Schmach in Tr\u00e4ume wiegt.", "tokens": ["Wel\u00b7che", "nun", "im", "stil\u00b7len", "Ti\u00b7bur", "ih\u00b7re", "Schmach", "in", "Tr\u00e4u\u00b7me", "wiegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPRART", "ADJA", "NN", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.11": {"line.1": {"text": "Probus f\u00fchrte seine Mauer durch des Nordens halbe Welt,", "tokens": ["Pro\u00b7bus", "f\u00fchr\u00b7te", "sei\u00b7ne", "Mau\u00b7er", "durch", "des", "Nor\u00b7dens", "hal\u00b7be", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Neun Germanenf\u00fcrsten knieten vor dem r\u00f6mischen Kaiserzelt.", "tokens": ["Neun", "Ger\u00b7ma\u00b7nen\u00b7f\u00fcrs\u00b7ten", "knie\u00b7ten", "vor", "dem", "r\u00f6\u00b7mi\u00b7schen", "Kai\u00b7ser\u00b7zelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.12": {"line.1": {"text": "Carus, unser Imperator, s\u00fchnt nun auch die letzte Schmach,", "tokens": ["Ca\u00b7rus", ",", "un\u00b7ser", "Im\u00b7pe\u00b7ra\u00b7tor", ",", "s\u00fchnt", "nun", "auch", "die", "letz\u00b7te", "Schmach", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Geht mit Heldenschritt voran uns, Heldenschritte folgen nach.", "tokens": ["Geht", "mit", "Hel\u00b7den\u00b7schritt", "vo\u00b7ran", "uns", ",", "Hel\u00b7den\u00b7schrit\u00b7te", "fol\u00b7gen", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PTKVZ", "PPER", "$,", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.13": {"line.1": {"text": "So der Weihgesang. Und siehe, pl\u00f6tzlich steigt Gew\u00f6lk empor,", "tokens": ["So", "der", "Weih\u00b7ge\u00b7sang", ".", "Und", "sie\u00b7he", ",", "pl\u00f6tz\u00b7lich", "steigt", "Ge\u00b7w\u00f6lk", "em\u00b7por", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$.", "KON", "VVIMP", "$,", "ADJD", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Finsternis bedeckt den Himmel, wie ein schwarzer Trauerflor.", "tokens": ["Fins\u00b7ter\u00b7nis", "be\u00b7deckt", "den", "Him\u00b7mel", ",", "wie", "ein", "schwar\u00b7zer", "Trau\u00b7er\u00b7flor", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.14": {"line.1": {"text": "Regen st\u00fcrzt in wilden G\u00fcssen, grausenhafter Donner br\u00fcllt,", "tokens": ["Re\u00b7gen", "st\u00fcrzt", "in", "wil\u00b7den", "G\u00fcs\u00b7sen", ",", "grau\u00b7sen\u00b7haf\u00b7ter", "Don\u00b7ner", "br\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Keiner mehr erkennt den Andern, Alles ist in Nacht verh\u00fcllt.", "tokens": ["Kei\u00b7ner", "mehr", "er\u00b7kennt", "den", "An\u00b7dern", ",", "Al\u00b7les", "ist", "in", "Nacht", "ver\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVFIN", "ART", "ADJA", "$,", "PIS", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.15": {"line.1": {"text": "Pl\u00f6tzlich zuckt ein Blitz vom Himmel. Viele st\u00fcrzen bang herbei,", "tokens": ["Pl\u00f6tz\u00b7lich", "zuckt", "ein", "Blitz", "vom", "Him\u00b7mel", ".", "Vie\u00b7le", "st\u00fcr\u00b7zen", "bang", "her\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$.", "PIS", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Denn im Zelt des Imperators h\u00f6rt man einen lauten Schrei.", "tokens": ["Denn", "im", "Zelt", "des", "Im\u00b7pe\u00b7ra\u00b7tors", "h\u00f6rt", "man", "ei\u00b7nen", "lau\u00b7ten", "Schrei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VVFIN", "PIS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.16": {"line.1": {"text": "Carus ist erschlagen! Jeder tut auf Kampf und Wehr Verzicht,", "tokens": ["Ca\u00b7rus", "ist", "er\u00b7schla\u00b7gen", "!", "Je\u00b7der", "tut", "auf", "Kampf", "und", "Wehr", "Ver\u00b7zicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "VVPP", "$.", "PIS", "VVFIN", "APPR", "NN", "KON", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und es folgt des Heers Verzweiflung auf die sch\u00f6ne Zuversicht.", "tokens": ["Und", "es", "folgt", "des", "Heers", "Ver\u00b7zwei\u00b7flung", "auf", "die", "sch\u00f6\u00b7ne", "Zu\u00b7ver\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.17": {"line.1": {"text": "Alle fliehn, das Lager feiert, wie ein unbewohntes Haus,", "tokens": ["Al\u00b7le", "fliehn", ",", "das", "La\u00b7ger", "fei\u00b7ert", ",", "wie", "ein", "un\u00b7be\u00b7wohn\u00b7tes", "Haus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "$,", "ART", "NN", "VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und der Schmerz der Legionen bricht in laute Klagen aus:", "tokens": ["Und", "der", "Schmerz", "der", "Le\u00b7gi\u00b7o\u00b7nen", "bricht", "in", "lau\u00b7te", "Kla\u00b7gen", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.18": {"line.1": {"text": "G\u00f6tter haben uns gerichtet, Untergang ist unser Teil;", "tokens": ["G\u00f6t\u00b7ter", "ha\u00b7ben", "uns", "ge\u00b7rich\u00b7tet", ",", "Un\u00b7ter\u00b7gang", "ist", "un\u00b7ser", "Teil", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "$,", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Denn des Kapitols Gebieter sandte seinen Donnerkeil!", "tokens": ["Denn", "des", "Ka\u00b7pi\u00b7tols", "Ge\u00b7bie\u00b7ter", "sand\u00b7te", "sei\u00b7nen", "Don\u00b7ner\u00b7keil", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN