{"textgrid.poem.29882": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "96.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcrsten sind de\u00df Vaterlandes V\u00e4ter; drum wer ihre Scham", "tokens": ["F\u00fcrs\u00b7ten", "sind", "de\u00df", "Va\u00b7ter\u00b7lan\u00b7des", "V\u00e4\u00b7ter", ";", "drum", "wer", "ih\u00b7re", "Scham"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$.", "PAV", "PWS", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wo entbl\u00f6st und sie verschimpfft, hat den Fluch, wie weiland Cham.", "tokens": ["Wo", "ent\u00b7bl\u00f6st", "und", "sie", "ver\u00b7schimpfft", ",", "hat", "den", "Fluch", ",", "wie", "wei\u00b7land", "Cham", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "KON", "PPER", "VVFIN", "$,", "VAFIN", "ART", "NN", "$,", "PWAV", "VVFIN", "NE", "$."], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}