{"textgrid.poem.57450": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "1L: Victoria! du hast gesieget,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Victoria! du hast gesieget,", "tokens": ["Vic\u00b7to\u00b7ria", "!", "du", "hast", "ge\u00b7sie\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich bin dein Knecht, Victoria!", "tokens": ["Ich", "bin", "dein", "Knecht", ",", "Vic\u00b7to\u00b7ria", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "NE", "$."], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Den seine Dienstbarkeit vergn\u00fcget,", "tokens": ["Den", "sei\u00b7ne", "Dienst\u00b7bar\u00b7keit", "ver\u00b7gn\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So bald er deine Sch\u00f6nheit sah.", "tokens": ["So", "bald", "er", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So la\u00df mich denn die Fessel k\u00fcssen,", "tokens": ["So", "la\u00df", "mich", "denn", "die", "Fes\u00b7sel", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die deine Macht mir angelegt;", "tokens": ["Die", "dei\u00b7ne", "Macht", "mir", "an\u00b7ge\u00b7legt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wenn dein Stral mich niederschl\u00e4gt,", "tokens": ["Und", "wenn", "dein", "Stral", "mich", "nie\u00b7der\u00b7schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht meiner Schwachheit Fehler b\u00fc\u00dfen;", "tokens": ["Nicht", "mei\u00b7ner", "Schwach\u00b7heit", "Feh\u00b7ler", "b\u00fc\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die leichter Feinde, Schwert und Mann,", "tokens": ["Die", "leich\u00b7ter", "Fein\u00b7de", ",", "Schwert", "und", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Als deinen Angriff, hemmen kann.", "tokens": ["Als", "dei\u00b7nen", "An\u00b7griff", ",", "hem\u00b7men", "kann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Des edlen Geistes Fr\u00fchlingsfr\u00fcchte,", "tokens": ["Des", "ed\u00b7len", "Geis\u00b7tes", "Fr\u00fch\u00b7lings\u00b7fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Werke deiner klugen Hand,", "tokens": ["Die", "Wer\u00b7ke", "dei\u00b7ner", "klu\u00b7gen", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sind durch das preisende Ger\u00fcchte", "tokens": ["Sind", "durch", "das", "prei\u00b7sen\u00b7de", "Ge\u00b7r\u00fcch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir schon seit langer Zeit bekannt.", "tokens": ["Mir", "schon", "seit", "lan\u00b7ger", "Zeit", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dort, wo in Mei\u00dfens fetten Auen", "tokens": ["Dort", ",", "wo", "in", "Mei\u00b7\u00dfens", "fet\u00b7ten", "Au\u00b7en"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die schlanke Plei\u00dfe rauschend flie\u00dft;", "tokens": ["Die", "schlan\u00b7ke", "Plei\u00b7\u00dfe", "rau\u00b7schend", "flie\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dort, wo der Musenh\u00fcgel ist,", "tokens": ["Dort", ",", "wo", "der", "Mu\u00b7sen\u00b7h\u00fc\u00b7gel", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Darauf ganz Deutschland pflegt zu schauen;", "tokens": ["Da\u00b7rauf", "ganz", "Deutschland", "pflegt", "zu", "schau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "NE", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Da hat es mir zuerst gegl\u00fcckt,", "tokens": ["Da", "hat", "es", "mir", "zu\u00b7erst", "ge\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Da\u00df ich ein Lied von dir erblickt.", "tokens": ["Da\u00df", "ich", "ein", "Lied", "von", "dir", "er\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Im weit entlegnen Sachsenlande", "tokens": ["Im", "weit", "ent\u00b7leg\u00b7nen", "Sach\u00b7sen\u00b7lan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ert\u00f6nte deiner Seyten Klang;", "tokens": ["Er\u00b7t\u00f6n\u00b7te", "dei\u00b7ner", "Sey\u00b7ten", "Klang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von dem entfernten Weichselstrande,", "tokens": ["Von", "dem", "ent\u00b7fern\u00b7ten", "Weich\u00b7sel\u00b7stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Entz\u00fcckte mich dein Lustgesang.", "tokens": ["Ent\u00b7z\u00fcck\u00b7te", "mich", "dein", "Lust\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Nymphen jener Philurenen,", "tokens": ["Die", "Nym\u00b7phen", "je\u00b7ner", "Phi\u00b7lu\u00b7re\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sammt jeder muntern Huldg\u00f6ttinn,", "tokens": ["Sammt", "je\u00b7der", "mun\u00b7tern", "Huld\u00b7g\u00f6t\u00b7tinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Entsetzten sich in ihrem Sinn", "tokens": ["Ent\u00b7setz\u00b7ten", "sich", "in", "ih\u00b7rem", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vor solchen Proben einer Sch\u00f6nen;", "tokens": ["Vor", "sol\u00b7chen", "Pro\u00b7ben", "ei\u00b7ner", "Sch\u00f6\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und zweifelten wohl gar dabey:", "tokens": ["Und", "zwei\u00b7fel\u00b7ten", "wohl", "gar", "da\u00b7bey", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PAV", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.10": {"text": "Ob Famens Nachricht glaublich sey?", "tokens": ["Ob", "Fa\u00b7mens", "Nach\u00b7richt", "glaub\u00b7lich", "sey", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich selber sprach in meinem Herzen:", "tokens": ["Ich", "sel\u00b7ber", "sprach", "in", "mei\u00b7nem", "Her\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer weis, ob mich der Ruf nicht tr\u00fcgt?", "tokens": ["Wer", "weis", ",", "ob", "mich", "der", "Ruf", "nicht", "tr\u00fcgt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vieleicht will jener Freund nur scherzen,", "tokens": ["Vie\u00b7leicht", "will", "je\u00b7ner", "Freund", "nur", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PDAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Indem er merkt, da\u00df michs vergn\u00fcgt.", "tokens": ["In\u00b7dem", "er", "merkt", ",", "da\u00df", "michs", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich wagte mich, an dich zu schreiben,", "tokens": ["Ich", "wag\u00b7te", "mich", ",", "an", "dich", "zu", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da sah ich bald ein neues Blatt,", "tokens": ["Da", "sah", "ich", "bald", "ein", "neu\u00b7es", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und an des alten Zweifels statt,", "tokens": ["Und", "an", "des", "al\u00b7ten", "Zwei\u00b7fels", "statt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nichts, als Erstaunung, \u00fcbrig bleiben:", "tokens": ["Nichts", ",", "als", "Er\u00b7stau\u00b7nung", ",", "\u00fcb\u00b7rig", "blei\u00b7ben", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "NN", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Weil jede Zeile deiner Schrift", "tokens": ["Weil", "je\u00b7de", "Zei\u00b7le", "dei\u00b7ner", "Schrift"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Fast Wunsch und Hoffnung \u00fcbertrifft.", "tokens": ["Fast", "Wunsch", "und", "Hoff\u00b7nung", "\u00fc\u00b7bert\u00b7rifft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Erw\u00fcnschtes Schicksal! sey gepriesen,", "tokens": ["Er\u00b7w\u00fcnschtes", "Schick\u00b7sal", "!", "sey", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da\u00df deiner F\u00fchrung Wunderzug", "tokens": ["Da\u00df", "dei\u00b7ner", "F\u00fch\u00b7rung", "Wun\u00b7der\u00b7zug"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir That und Wahrheit selbst gewiesen,", "tokens": ["Mir", "That", "und", "Wahr\u00b7heit", "selbst", "ge\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als mich dein Wink nach Danzig trug.", "tokens": ["Als", "mich", "dein", "Wink", "nach", "Dan\u00b7zig", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der edlen ", "tokens": ["Der", "ed\u00b7len"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Erh\u00f6ht der frischen Jugend Pracht,", "tokens": ["Er\u00b7h\u00f6ht", "der", "fri\u00b7schen", "Ju\u00b7gend", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In welcher so viel Anmuth lacht,", "tokens": ["In", "wel\u00b7cher", "so", "viel", "An\u00b7muth", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als hundert andre Sch\u00f6nen haben,", "tokens": ["Als", "hun\u00b7dert", "and\u00b7re", "Sch\u00f6\u00b7nen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die doch, denn ihr Verstand ist blind!", "tokens": ["Die", "doch", ",", "denn", "ihr", "Ver\u00b7stand", "ist", "blind", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Nur todte Marmorbilder sind.", "tokens": ["Nur", "tod\u00b7te", "Mar\u00b7mor\u00b7bil\u00b7der", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O w\u00e4ren meine Lobges\u00e4nge", "tokens": ["O", "w\u00e4\u00b7ren", "mei\u00b7ne", "Lob\u00b7ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sch\u00f6nheit deiner Bildung gleich,", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "dei\u00b7ner", "Bil\u00b7dung", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und so, wie deiner Glieder L\u00e4nge,", "tokens": ["Und", "so", ",", "wie", "dei\u00b7ner", "Glie\u00b7der", "L\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An reizerf\u00fclltem Wesen reich!", "tokens": ["An", "rei\u00b7zer\u00b7f\u00fcll\u00b7tem", "We\u00b7sen", "reich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach, unvergleichliche Louise!", "tokens": ["Ach", ",", "un\u00b7ver\u00b7gleich\u00b7li\u00b7che", "Lou\u00b7i\u00b7se", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So w\u00fcrde bald ein Blatt erf\u00fcllt,", "tokens": ["