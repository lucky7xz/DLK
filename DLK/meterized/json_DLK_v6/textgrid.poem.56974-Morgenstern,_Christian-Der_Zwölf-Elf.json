{"textgrid.poem.56974": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Der Zw\u00f6lf-Elf", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Zw\u00f6lf-Elf hebt die linke Hand:", "tokens": ["Der", "Zw\u00f6l\u00b7fElf", "hebt", "die", "lin\u00b7ke", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da schl\u00e4gt es Mitternacht im Land.", "tokens": ["Da", "schl\u00e4gt", "es", "Mit\u00b7ter\u00b7nacht", "im", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es lauscht der Teich mit offnem Mund.", "tokens": ["Es", "lauscht", "der", "Teich", "mit", "off\u00b7nem", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ganz leise heult der Schluchtenhund.", "tokens": ["Ganz", "lei\u00b7se", "heult", "der", "Schluch\u00b7ten\u00b7hund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Dommel reckt sich auf im Rohr", "tokens": ["Die", "Dom\u00b7mel", "reckt", "sich", "auf", "im", "Rohr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Moosfrosch lugt aus seinem Moor.", "tokens": ["Der", "Moos\u00b7frosch", "lugt", "aus", "sei\u00b7nem", "Moor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Schneck horcht auf in seinem Haus.", "tokens": ["Der", "Schneck", "horcht", "auf", "in", "sei\u00b7nem", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Desgleichen die Kartoffelmaus.", "tokens": ["Des\u00b7glei\u00b7chen", "die", "Kar\u00b7tof\u00b7fel\u00b7maus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Das Irrlicht selbst macht Halt und Rast", "tokens": ["Das", "Irr\u00b7licht", "selbst", "macht", "Halt", "und", "Rast"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "auf einem windgebrochnen Ast.", "tokens": ["auf", "ei\u00b7nem", "wind\u00b7ge\u00b7broch\u00b7nen", "Ast", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sophie, die Maid, hat ein Gesicht:", "tokens": ["So\u00b7phie", ",", "die", "Maid", ",", "hat", "ein", "Ge\u00b7sicht", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Mondschaf geht zum Hochgericht.", "tokens": ["Das", "Mond\u00b7schaf", "geht", "zum", "Hoch\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die Galgenbr\u00fcder wehn im Wind.", "tokens": ["Die", "Gal\u00b7gen\u00b7br\u00fc\u00b7der", "wehn", "im", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im fernen Dorfe schreit ein Kind.", "tokens": ["Im", "fer\u00b7nen", "Dor\u00b7fe", "schreit", "ein", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Zwei Maulw\u00fcrf k\u00fcssen sich zur Stund", "tokens": ["Zwei", "Maul\u00b7w\u00fcrf", "k\u00fcs\u00b7sen", "sich", "zur", "Stund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "als Neuverm\u00e4hlte auf den Mund.", "tokens": ["als", "Neu\u00b7ver\u00b7m\u00e4hl\u00b7te", "auf", "den", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Hingegen tief im finstern Wald", "tokens": ["Hin\u00b7ge\u00b7gen", "tief", "im", "fins\u00b7tern", "Wald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPRART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "ein Nachtmahr seine F\u00e4uste ballt:", "tokens": ["ein", "Nacht\u00b7mahr", "sei\u00b7ne", "F\u00e4us\u00b7te", "ballt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Dieweil ein sp\u00e4ter Wanderstrumpf", "tokens": ["Die\u00b7weil", "ein", "sp\u00e4\u00b7ter", "Wan\u00b7der\u00b7strumpf"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "sich nicht verlief in Teich und Sumpf.", "tokens": ["sich", "nicht", "ver\u00b7lief", "in", "Teich", "und", "Sumpf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Der Rabe Ralf ruft schaurig: \u203aKra!", "tokens": ["Der", "Ra\u00b7be", "Ralf", "ruft", "schau\u00b7rig", ":", "\u203a", "Kra", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "ADJD", "$.", "$(", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das End ist da! Das End ist da!\u2039", "tokens": ["Das", "End", "ist", "da", "!", "Das", "End", "ist", "da", "!", "\u2039"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$.", "ART", "NN", "VAFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Der Zw\u00f6lf-Elf senkt die linke Hand:", "tokens": ["Der", "Zw\u00f6l\u00b7fElf", "senkt", "die", "lin\u00b7ke", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wieder schl\u00e4ft das ganze Land.", "tokens": ["Und", "wie\u00b7der", "schl\u00e4ft", "das", "gan\u00b7ze", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}