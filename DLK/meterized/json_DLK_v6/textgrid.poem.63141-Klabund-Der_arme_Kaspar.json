{"textgrid.poem.63141": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Der arme Kaspar", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich geh \u2013 wohin?", "tokens": ["Ich", "geh", "\u2013", "wo\u00b7hin", "?"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWAV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ich kam \u2013 woher?", "tokens": ["Ich", "kam", "\u2013", "wo\u00b7her", "?"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWAV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Bin aussen und inn,", "tokens": ["Bin", "aus\u00b7sen", "und", "inn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "KON", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Bin voll und leer.", "tokens": ["Bin", "voll", "und", "leer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Geboren \u2013 wo?", "tokens": ["Ge\u00b7bo\u00b7ren", "\u2013", "wo", "?"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$(", "PWAV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Erkoren \u2013 wann?", "tokens": ["Er\u00b7ko\u00b7ren", "\u2013", "wann", "?"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$(", "PWAV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Ich schlief im Stroh", "tokens": ["Ich", "schlief", "im", "Stroh"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Bei Weib und Mann.", "tokens": ["Bei", "Weib", "und", "Mann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Ich liebe dich,", "tokens": ["Ich", "lie\u00b7be", "dich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Und liebst du mich?", "tokens": ["Und", "liebst", "du", "mich", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Ich tr\u00fcbe dich,", "tokens": ["Ich", "tr\u00fc\u00b7be", "dich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Betr\u00fcbst du mich?", "tokens": ["Be\u00b7tr\u00fcbst", "du", "mich", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Ich steh und fall,", "tokens": ["Ich", "steh", "und", "fall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Ich werde sein.", "tokens": ["Ich", "wer\u00b7de", "sein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Ich bin ein All", "tokens": ["Ich", "bin", "ein", "All"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Und bin allein.", "tokens": ["Und", "bin", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Ich war. Ich bin.", "tokens": ["Ich", "war", ".", "Ich", "bin", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$.", "PPER", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "Viel leicht. Viel schwer.", "tokens": ["Viel", "leicht", ".", "Viel", "schwer", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Ich geh \u2013 wohin?", "tokens": ["Ich", "geh", "\u2013", "wo\u00b7hin", "?"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWAV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "Ich kam \u2013 woher?", "tokens": ["Ich", "kam", "\u2013", "wo\u00b7her", "?"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWAV", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}