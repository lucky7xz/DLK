{"dta.poem.12763": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Bey dem W. und T. hochzeit-festin  \n stellte die ausgaben und eink\u00fcnffte  \n der verliebten vor.  \n  M.  F. E. G.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das ist ein segens-voller stand,", "tokens": ["Das", "ist", "ein", "se\u00b7gens\u00b7vol\u00b7ler", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo der verlobten sinn nach liebes-renten trachtet,", "tokens": ["Wo", "der", "ver\u00b7lob\u00b7ten", "sinn", "nach", "lie\u00b7bes\u00b7ren\u00b7ten", "trach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und alles, was nur mund und hand,", "tokens": ["Und", "al\u00b7les", ",", "was", "nur", "mund", "und", "hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den keuschen hertzen schmeckt, vor lauter reichthum achtet.", "tokens": ["Den", "keu\u00b7schen", "hert\u00b7zen", "schmeckt", ",", "vor", "lau\u00b7ter", "reicht\u00b7hum", "ach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hier giebt man aus. Hier nimmt man wieder ein;", "tokens": ["Hier", "giebt", "man", "aus", ".", "Hier", "nimmt", "man", "wie\u00b7der", "ein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKVZ", "$.", "ADV", "VVFIN", "PIS", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Doch ohne den verlust und schaden,", "tokens": ["Doch", "oh\u00b7ne", "den", "ver\u00b7lust", "und", "scha\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Weil s\u00fc\u00dfer zeit-vertreib und marmeladen,", "tokens": ["Weil", "s\u00fc\u00b7\u00dfer", "zeit\u00b7ver\u00b7treib", "und", "mar\u00b7me\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "An statt des gelds zur rechnung g\u00fcltig seyn.", "tokens": ["An", "statt", "des", "gelds", "zur", "rech\u00b7nung", "g\u00fcl\u00b7tig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "APPRART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die liebe treibet hier zum calculiren an.", "tokens": ["Die", "lie\u00b7be", "trei\u00b7bet", "hier", "zum", "cal\u00b7cu\u00b7li\u00b7ren", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sie lehret uns die blicke sch\u00e4tzen,", "tokens": ["Sie", "leh\u00b7ret", "uns", "die", "bli\u00b7cke", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ja ku\u00df um ku\u00df zusammen setzen.", "tokens": ["Ja", "ku\u00df", "um", "ku\u00df", "zu\u00b7sam\u00b7men", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Sie weiset, wie ein paar die welt vermehren kan.", "tokens": ["Sie", "wei\u00b7set", ",", "wie", "ein", "paar", "die", "welt", "ver\u00b7meh\u00b7ren", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "PIAT", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}