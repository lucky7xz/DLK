{"dta.poem.19951": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Der Wassermann .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es war in des Mayen lindem Glanz,", "tokens": ["Es", "war", "in", "des", "Ma\u00b7yen", "lin\u00b7dem", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da hielten die Jungfern von T\u00fcbingen Tanz,", "tokens": ["Da", "hiel\u00b7ten", "die", "Jung\u00b7fern", "von", "T\u00fc\u00b7bin\u00b7gen", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Sie tanzten und tanzten wohl allzumal", "tokens": ["Sie", "tanz\u00b7ten", "und", "tanz\u00b7ten", "wohl", "all\u00b7zu\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Um eine Linde im gr\u00fcnen Thal.", "tokens": ["Um", "ei\u00b7ne", "Lin\u00b7de", "im", "gr\u00fc\u00b7nen", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NE", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ein fremder J\u00fcngling in stolzem Kleid", "tokens": ["Ein", "frem\u00b7der", "J\u00fcng\u00b7ling", "in", "stol\u00b7zem", "Kleid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sich wandte bald zu der sch\u00f6nsten Maid,", "tokens": ["Sich", "wand\u00b7te", "bald", "zu", "der", "sch\u00f6ns\u00b7ten", "Maid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Er reicht ihr dar die H\u00e4nde zum Tanz,", "tokens": ["Er", "reicht", "ihr", "dar", "die", "H\u00e4n\u00b7de", "zum", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Er setzt ihr auf's Haar einen meergr\u00fcnen Kranz.", "tokens": ["Er", "setzt", "ihr", "auf's", "Haar", "ei\u00b7nen", "meer\u00b7gr\u00fc\u00b7nen", "Kranz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+---+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "O J\u00fcngling! warum ist so kalt dein Arm?", "tokens": ["O", "J\u00fcng\u00b7ling", "!", "wa\u00b7rum", "ist", "so", "kalt", "dein", "Arm", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAV", "VAFIN", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In Neckars Tiefen da ist's nicht warm.", "tokens": ["In", "Ne\u00b7ckars", "Tie\u00b7fen", "da", "ist's", "nicht", "warm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "ADV", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "O J\u00fcngling! warum ist so bleich deine Hand?", "tokens": ["O", "J\u00fcng\u00b7ling", "!", "wa\u00b7rum", "ist", "so", "bleich", "dei\u00b7ne", "Hand", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAV", "VAFIN", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "In's Wasser dringt nicht der Sonne Brand!", "tokens": ["In's", "Was\u00b7ser", "dringt", "nicht", "der", "Son\u00b7ne", "Brand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Er tanzt mit ihr von der Linde weit.", "tokens": ["Er", "tanzt", "mit", "ihr", "von", "der", "Lin\u00b7de", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "La\u00df J\u00fcngling! horch, die Mutter mir schreit!", "tokens": ["La\u00df", "J\u00fcng\u00b7ling", "!", "horch", ",", "die", "Mut\u00b7ter", "mir", "schreit", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$.", "VVIMP", "$,", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.8": {"line.1": {"text": "Er tanzt mit ihr den Neckar entlang:", "tokens": ["Er", "tanzt", "mit", "ihr", "den", "Ne\u00b7ckar", "ent\u00b7lang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "La\u00df J\u00fcngling! weh! mir wird so bang!", "tokens": ["La\u00df", "J\u00fcng\u00b7ling", "!", "weh", "!", "mir", "wird", "so", "bang", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$.", "PTKVZ", "$.", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Er fa\u00dft sie fest um den schlanken Leib.", "tokens": ["Er", "fa\u00dft", "sie", "fest", "um", "den", "schlan\u00b7ken", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sch\u00f6n Maid! du bist des Wassermanns Weib!", "tokens": ["Sch\u00f6n", "Maid", "!", "du", "bist", "des", "Was\u00b7ser\u00b7manns", "Weib", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.10": {"line.1": {"text": "Er tanzt mit ihr in die Wellen hinein:", "tokens": ["Er", "tanzt", "mit", "ihr", "in", "die", "Wel\u00b7len", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "O Vater und o du Mutter mein!", "tokens": ["O", "Va\u00b7ter", "und", "o", "du", "Mut\u00b7ter", "mein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "FM", "PPER", "NN", "PPOSAT", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Er f\u00fchrt sie in einen krystallenen Saal:", "tokens": ["Er", "f\u00fchrt", "sie", "in", "ei\u00b7nen", "krys\u00b7tal\u00b7le\u00b7nen", "Saal", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ade, ihr Schwestern im gr\u00fcnen Thal!", "tokens": ["A\u00b7de", ",", "ihr", "Schwes\u00b7tern", "im", "gr\u00fc\u00b7nen", "Thal", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}