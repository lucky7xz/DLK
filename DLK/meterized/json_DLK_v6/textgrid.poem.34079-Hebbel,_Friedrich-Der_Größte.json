{"textgrid.poem.34079": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Gr\u00f6\u00dfte", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was der Gr\u00f6\u00dfte sich denkt? Die\u00df denkt er: Hole der Teufel", "tokens": ["Was", "der", "Gr\u00f6\u00df\u00b7te", "sich", "denkt", "?", "Die\u00df", "denkt", "er", ":", "Ho\u00b7le", "der", "Teu\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "PRF", "VVFIN", "$.", "PDS", "VVFIN", "PPER", "$.", "NN", "ART", "NN"], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Euer ganzes Geschlecht, wenn ich das bin, was ihr glaubt!", "tokens": ["Eu\u00b7er", "gan\u00b7zes", "Ge\u00b7schlecht", ",", "wenn", "ich", "das", "bin", ",", "was", "ihr", "glaubt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KOUS", "PPER", "PDS", "VAFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}}}}}