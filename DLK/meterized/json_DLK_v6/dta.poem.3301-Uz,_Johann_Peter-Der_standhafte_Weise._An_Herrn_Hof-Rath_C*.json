{"dta.poem.3301": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Der standhafte Weise.  \n An Herrn Hof-Rath C*", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Hat nun dein Saitenspiel den s\u00fcssen Scherz verges-", "tokens": ["Hat", "nun", "dein", "Sai\u00b7ten\u00b7spiel", "den", "s\u00fcs\u00b7sen", "Scherz", "ver\u00b7ge\u00b7s"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "sen,", "tokens": ["sen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Und schweigt, stets ungestimmt, an traurigen Cy-", "tokens": ["Und", "schweigt", ",", "stets", "un\u00b7ge\u00b7stimmt", ",", "an", "trau\u00b7ri\u00b7gen", "Cy"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ADV", "ADJD", "$,", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "pressen,", "tokens": ["pres\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Um deiner holden Gattinn Grab?", "tokens": ["Um", "dei\u00b7ner", "hol\u00b7den", "Gat\u00b7tinn", "Grab", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wer kann, o weiser C* den wilden Schmerz besiegen,", "tokens": ["Wer", "kann", ",", "o", "wei\u00b7ser", "C", "*", "den", "wil\u00b7den", "Schmerz", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$,", "FM", "ADJA", "XY", "XY", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn Seelen, deren Muth erhabne Proben gab,", "tokens": ["Wenn", "See\u00b7len", ",", "de\u00b7ren", "Muth", "er\u00b7hab\u00b7ne", "Pro\u00b7ben", "gab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELAT", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wenn starke Seelen unterliegen?", "tokens": ["Wenn", "star\u00b7ke", "See\u00b7len", "un\u00b7ter\u00b7lie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie? soll die Traurigkeit unwidersetzlich w\u00fcthen,", "tokens": ["Wie", "?", "soll", "die", "Trau\u00b7rig\u00b7keit", "un\u00b7wi\u00b7der\u00b7setz\u00b7lich", "w\u00fct\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VMFIN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wo sie einmal herrscht, stets f\u00fcrchterlich gebiethen,", "tokens": ["Und", "wo", "sie", "ein\u00b7mal", "herrscht", ",", "stets", "f\u00fcrch\u00b7ter\u00b7lich", "ge\u00b7bie\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVFIN", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In ewig unerhellter Nacht?", "tokens": ["In", "e\u00b7wig", "un\u00b7er\u00b7hell\u00b7ter", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nein! von dem Weisen mu\u00df die Welt und Nachwelt lesen,", "tokens": ["Nein", "!", "von", "dem", "Wei\u00b7sen", "mu\u00df", "die", "Welt", "und", "Nach\u00b7welt", "le\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "APPR", "ART", "NN", "VMFIN", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.5": {"text": "Er sey gem\u00e4ssigt froh, wenn ihm das Gl\u00fcck gelacht,", "tokens": ["Er", "sey", "ge\u00b7m\u00e4s\u00b7sigt", "froh", ",", "wenn", "ihm", "das", "Gl\u00fcck", "ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ADJD", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und auch in Leiden gro\u00df gewesen.", "tokens": ["Und", "auch", "in", "Lei\u00b7den", "gro\u00df", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Jhm darf die tr\u00e4ge Zeit auf mitleidvollen Schwin-", "tokens": ["Jhm", "darf", "die", "tr\u00e4\u00b7ge", "Zeit", "auf", "mit\u00b7leid\u00b7vol\u00b7len", "Schwin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "gen", "tokens": ["gen"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Nicht ihren sp\u00e4ten Trost, nicht ihre Lindrung bringen:", "tokens": ["Nicht", "ih\u00b7ren", "sp\u00e4\u00b7ten", "Trost", ",", "nicht", "ih\u00b7re", "Lind\u00b7rung", "brin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "ADJA", "NN", "$,", "PTKNEG", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie sey des P\u00f6bels Tr\u00f6sterinn!", "tokens": ["Sie", "sey", "des", "P\u00f6\u00b7bels", "Tr\u00f6s\u00b7te\u00b7rinn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Weise braucht sie nicht, er tr\u00f6stet sich aus Gr\u00fcnden:", "tokens": ["Der", "Wei\u00b7se", "braucht", "sie", "nicht", ",", "er", "tr\u00f6s\u00b7tet", "sich", "aus", "Gr\u00fcn\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Wahrheit schimmert ihm durch tr\u00fcbe Nebel hin;", "tokens": ["Die", "Wahr\u00b7heit", "schim\u00b7mert", "ihm", "durch", "tr\u00fc\u00b7be", "Ne\u00b7bel", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er kann sie sehen und empfinden.", "tokens": ["Er", "kann", "sie", "se\u00b7hen", "und", "emp\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sein lehrend Beyspiel strahlt auch auf entfernte", "tokens": ["Sein", "leh\u00b7rend", "Bey\u00b7spiel", "strahlt", "auch", "auf", "ent\u00b7fern\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Tage:", "tokens": ["Ta\u00b7ge", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Der Schwache, der es h\u00f6rt, sch\u00e4mt sich der feigen Kla-", "tokens": ["Der", "Schwa\u00b7che", ",", "der", "es", "h\u00f6rt", ",", "sch\u00e4mt", "sich", "der", "fei\u00b7gen", "Kla"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ge,", "tokens": ["ge", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Und f\u00fchlet ungewohnten Muth.", "tokens": ["Und", "f\u00fch\u00b7let", "un\u00b7ge\u00b7wohn\u00b7ten", "Muth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Um seine Helden-Stirn m\u00fcss\u2019 ewig Lorbeer gr\u00fcnen!", "tokens": ["Um", "sei\u00b7ne", "Hel\u00b7den\u00b7Stirn", "m\u00fcss'", "e\u00b7wig", "Lor\u00b7beer", "gr\u00fc\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VMFIN", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "O Lorbeer bessrer Art, als den durch fremdes Blut", "tokens": ["O", "Lor\u00b7beer", "bess\u00b7rer", "Art", ",", "als", "den", "durch", "frem\u00b7des", "Blut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "ADJA", "NN", "$,", "KOUS", "ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Weltverw\u00fcster sich verdienen!", "tokens": ["Die", "Welt\u00b7ver\u00b7w\u00fcs\u00b7ter", "sich", "ver\u00b7die\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Kein stoischer Gesang ert\u00f6nt von meinen Saiten;", "tokens": ["Kein", "sto\u00b7i\u00b7scher", "Ge\u00b7sang", "er\u00b7t\u00f6nt", "von", "mei\u00b7nen", "Sai\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich waffne nicht den Stolz, die Thr\u00e4nen zu bestreiten;", "tokens": ["Ich", "waff\u00b7ne", "nicht", "den", "Stolz", ",", "die", "Thr\u00e4\u00b7nen", "zu", "be\u00b7strei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhm widersteht ein z\u00e4rtlich Herz.", "tokens": ["Jhm", "wi\u00b7der\u00b7steht", "ein", "z\u00e4rt\u00b7lich", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Stimme der Natur gebeut in allen Seelen,", "tokens": ["Die", "Stim\u00b7me", "der", "Na\u00b7tur", "ge\u00b7beut", "in", "al\u00b7len", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und falscher Gro\u00dfmuth Zwang kann einen wahren", "tokens": ["Und", "fal\u00b7scher", "Gro\u00df\u00b7muth", "Zwang", "kann", "ei\u00b7nen", "wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "NN", "VMFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Schmerz", "tokens": ["Schmerz"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Nicht \u00fcberwinden, nur verhehlen.", "tokens": ["Nicht", "\u00fc\u00b7berw\u00b7in\u00b7den", ",", "nur", "ver\u00b7heh\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch was kein Stolz vermag, kann Weisheit m\u00f6g-", "tokens": ["Doch", "was", "kein", "Stolz", "ver\u00b7mag", ",", "kann", "Weis\u00b7heit", "m\u00f6g"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWS", "PIAT", "NN", "VVFIN", "$,", "VMFIN", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "lich machen:", "tokens": ["lich", "ma\u00b7chen", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Auch Triebe der Natur, die herrschbegierig wachen,", "tokens": ["Auch", "Trie\u00b7be", "der", "Na\u00b7tur", ",", "die", "herrschbe\u00b7gie\u00b7rig", "wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "NN", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "-+---+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gew\u00f6hnt sie zum Gehorsam an.", "tokens": ["Ge\u00b7w\u00f6hnt", "sie", "zum", "Ge\u00b7hor\u00b7sam", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie m\u00fcssen sich vor ihr, so wild sie brausen, schmiegen,", "tokens": ["Sie", "m\u00fcs\u00b7sen", "sich", "vor", "ihr", ",", "so", "wild", "sie", "brau\u00b7sen", ",", "schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "PPER", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie in verschlossner Gruft, dem Aeol unterthan,", "tokens": ["Wie", "in", "ver\u00b7schloss\u00b7ner", "Gruft", ",", "dem", "A\u00b7e\u00b7ol", "un\u00b7ter\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Die lauten Winde knirschend liegen.", "tokens": ["Die", "lau\u00b7ten", "Win\u00b7de", "knir\u00b7schend", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Sieh auf den starken Trieb, der uns zur Wollust", "tokens": ["Sieh", "auf", "den", "star\u00b7ken", "Trieb", ",", "der", "uns", "zur", "Wol\u00b7lust"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "reisset,", "tokens": ["reis\u00b7set", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Jm freyen Wilde Brunst, in Menschen Liebe heisset,", "tokens": ["Jm", "frey\u00b7en", "Wil\u00b7de", "Brunst", ",", "in", "Men\u00b7schen", "Lie\u00b7be", "heis\u00b7set", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "NN", "$,", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und, unbeherrscht, sich leicht verirrt.", "tokens": ["Und", ",", "un\u00b7be\u00b7herrscht", ",", "sich", "leicht", "ver\u00b7irrt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "$,", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er wird Gesetz und Recht und Menschlichkeit verletzen,", "tokens": ["Er", "wird", "Ge\u00b7setz", "und", "Recht", "und", "Menschlich\u00b7keit", "ver\u00b7let\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Wenn ihn kein Z\u00fcgel h\u00e4lt, und ihm erlaubet wird,", "tokens": ["Wenn", "ihn", "kein", "Z\u00fc\u00b7gel", "h\u00e4lt", ",", "und", "ihm", "er\u00b7lau\u00b7bet", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,", "KON", "PPER", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sich h\u00f6hern Pflichten vorzusetzen.", "tokens": ["Sich", "h\u00f6\u00b7hern", "Pflich\u00b7ten", "vor\u00b7zu\u00b7set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Aus ihren Schranken darf auch die Natur nicht", "tokens": ["Aus", "ih\u00b7ren", "Schran\u00b7ken", "darf", "auch", "die", "Na\u00b7tur", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "ADV", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "schreiten:", "tokens": ["schrei\u00b7ten", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Soll nicht ein gleicher Zaum die weiche Wehmuth lei-", "tokens": ["Soll", "nicht", "ein", "glei\u00b7cher", "Zaum", "die", "wei\u00b7che", "Weh\u00b7muth", "lei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ten,", "tokens": ["ten", ","], "token_info": ["word", "punct"], "pos": ["FM", "$,"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Die ein verlohrnes Gut bedaurt?", "tokens": ["Die", "ein", "ver\u00b7lohr\u00b7nes", "Gut", "be\u00b7daurt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kein allzulanger Schmerz mu\u00df unsre Ruhe st\u00f6ren;", "tokens": ["Kein", "all\u00b7zu\u00b7lan\u00b7ger", "Schmerz", "mu\u00df", "uns\u00b7re", "Ru\u00b7he", "st\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und wenn es Menschheit ist, da\u00df unsre Seele traurt,", "tokens": ["Und", "wenn", "es", "Menschheit", "ist", ",", "da\u00df", "uns\u00b7re", "See\u00b7le", "traurt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "VAFIN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "So ist es Weisheit, aufzuh\u00f6ren.", "tokens": ["So", "ist", "es", "Weis\u00b7heit", ",", "auf\u00b7zu\u00b7h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Was kann den Sterblichen das wilde Gl\u00fcck entzie-", "tokens": ["Was", "kann", "den", "Sterb\u00b7li\u00b7chen", "das", "wil\u00b7de", "Gl\u00fcck", "ent\u00b7zie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ART", "NN", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "hen,", "tokens": ["hen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Das ewig Leid verdient? Ist alles nicht geliehen?", "tokens": ["Das", "e\u00b7wig", "Leid", "ver\u00b7dient", "?", "Ist", "al\u00b7les", "nicht", "ge\u00b7lie\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "NN", "VVPP", "$.", "VAFIN", "PIS", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Geb\u00fchrt nicht alles ihm zur\u00fcck?", "tokens": ["Ge\u00b7b\u00fchrt", "nicht", "al\u00b7les", "ihm", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PIS", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die G\u00fcter, die es giebt, verschenkt es nicht auf immer:", "tokens": ["Die", "G\u00fc\u00b7ter", ",", "die", "es", "giebt", ",", "ver\u00b7schenkt", "es", "nicht", "auf", "im\u00b7mer", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PTKNEG", "APPR", "ADV", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Sein schmeichlend L\u00e4cheln ist ein kurzer Sonnenblick,", "tokens": ["Sein", "schmeich\u00b7lend", "L\u00e4\u00b7cheln", "ist", "ein", "kur\u00b7zer", "Son\u00b7nen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein kaum genossner Fr\u00fchlings-Schimmer.", "tokens": ["Ein", "kaum", "ge\u00b7noss\u00b7ner", "Fr\u00fch\u00b7lings\u00b7Schim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Wann sich die dunkle Luft mit Winter-Wolken", "tokens": ["Wann", "sich", "die", "dunk\u00b7le", "Luft", "mit", "Win\u00b7ter\u00b7Wol\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "schw\u00e4rzet;", "tokens": ["schw\u00e4r\u00b7zet", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Wann Philomele schweigt, kein lauer Zephyr scherzet,", "tokens": ["Wann", "Phi\u00b7lo\u00b7me\u00b7le", "schweigt", ",", "kein", "lau\u00b7er", "Ze\u00b7phyr", "scher\u00b7zet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "$,", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kein Zephyr Morgen-Rosen k\u00fcsst:", "tokens": ["Kein", "Ze\u00b7phyr", "Mor\u00b7gen\u00b7Ro\u00b7sen", "k\u00fcsst", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was hilfts, mit finstrer Stirn den Unbestand beklagen?", "tokens": ["Was", "hilfts", ",", "mit", "finst\u00b7rer", "Stirn", "den", "Un\u00b7be\u00b7stand", "be\u00b7kla\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es kommt nicht mehr zur\u00fcck, was einst entflohen ist;", "tokens": ["Es", "kommt", "nicht", "mehr", "zu\u00b7r\u00fcck", ",", "was", "einst", "ent\u00b7flo\u00b7hen", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$,", "PRELS", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch leicht wird, was wir freudig tragen.", "tokens": ["Doch", "leicht", "wird", ",", "was", "wir", "freu\u00b7dig", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "PRELS", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Der Weise bleibt sich gleich im Schoos erw\u00fcnschter", "tokens": ["Der", "Wei\u00b7se", "bleibt", "sich", "gleich", "im", "Schoos", "er\u00b7w\u00fcnschter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "APPRART", "NN", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Freuden,", "tokens": ["Freu\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und sieht, noch ehe sie, bald oder sp\u00e4te, scheiden,", "tokens": ["Und", "sieht", ",", "noch", "e\u00b7he", "sie", ",", "bald", "o\u00b7der", "sp\u00e4\u00b7te", ",", "schei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "KOUS", "PPER", "$,", "ADV", "KON", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die leichten Fl\u00fcgel ieder Lust.", "tokens": ["Die", "leich\u00b7ten", "Fl\u00fc\u00b7gel", "ie\u00b7der", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn ihr Gefieder sich in schneller Flucht verspreitet,", "tokens": ["Wenn", "ihr", "Ge\u00b7fie\u00b7der", "sich", "in", "schnel\u00b7ler", "Flucht", "ver\u00b7sprei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So sieht ers unbet\u00e4ubt: er hatte sei