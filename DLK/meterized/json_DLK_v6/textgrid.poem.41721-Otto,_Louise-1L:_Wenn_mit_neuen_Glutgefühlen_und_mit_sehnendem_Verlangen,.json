{"textgrid.poem.41721": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn mit neuen Glutgef\u00fchlen und mit sehnendem Verlangen,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn mit neuen Glutgef\u00fchlen und mit sehnendem Verlangen,", "tokens": ["Wenn", "mit", "neu\u00b7en", "Glut\u00b7ge\u00b7f\u00fch\u00b7len", "und", "mit", "seh\u00b7nen\u00b7dem", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Einem Herzen ist die Liebe gleich der Sonne aufgegangen,", "tokens": ["Ei\u00b7nem", "Her\u00b7zen", "ist", "die", "Lie\u00b7be", "gleich", "der", "Son\u00b7ne", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Da erwachen neue Lieder so die neue Sch\u00f6pfung preisen,", "tokens": ["Da", "er\u00b7wa\u00b7chen", "neu\u00b7e", "Lie\u00b7der", "so", "die", "neu\u00b7e", "Sch\u00f6p\u00b7fung", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Aus den langverstummten Innern klingts in wunderreichen Weisen.", "tokens": ["Aus", "den", "lang\u00b7ver\u00b7stumm\u00b7ten", "In\u00b7nern", "klingts", "in", "wun\u00b7der\u00b7rei\u00b7chen", "Wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Aber eine Liebe wei\u00df ich, eine, die uns mehr ersch\u00fcttert", "tokens": ["A\u00b7ber", "ei\u00b7ne", "Lie\u00b7be", "wei\u00df", "ich", ",", "ei\u00b7ne", ",", "die", "uns", "mehr", "er\u00b7sch\u00fct\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "$,", "ART", "$,", "PRELS", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und ihr Aufgang gleicht der Sonne, wenn die Wetterwolke wittert,", "tokens": ["Und", "ihr", "Auf\u00b7gang", "gleicht", "der", "Son\u00b7ne", ",", "wenn", "die", "Wet\u00b7ter\u00b7wol\u00b7ke", "wit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wenn die Fluren tauig gl\u00e4nzen und die V\u00f6gel wonnig schmettern", "tokens": ["Wenn", "die", "Flu\u00b7ren", "tau\u00b7ig", "gl\u00e4n\u00b7zen", "und", "die", "V\u00f6\u00b7gel", "won\u00b7nig", "schmet\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVINF", "KON", "ART", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und in rabenschwarzen Wolken Blitze machtlos niederklettern.", "tokens": ["Und", "in", "ra\u00b7ben\u00b7schwar\u00b7zen", "Wol\u00b7ken", "Blit\u00b7ze", "macht\u00b7los", "nie\u00b7der\u00b7klet\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Wollet nimmer Euch verwundern, da\u00df, die fr\u00fche stumm verblieben,", "tokens": ["Wol\u00b7let", "nim\u00b7mer", "Euch", "ver\u00b7wun\u00b7dern", ",", "da\u00df", ",", "die", "fr\u00fc\u00b7he", "stumm", "ver\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "VVINF", "$,", "KOUS", "$,", "ART", "ADJA", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Von den deutschen S\u00e4ngern allen, jetzt auch Schlachtenlieder singen \u2013", "tokens": ["Von", "den", "deut\u00b7schen", "S\u00e4n\u00b7gern", "al\u00b7len", ",", "jetzt", "auch", "Schlach\u00b7ten\u00b7lie\u00b7der", "sin\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PIAT", "$,", "ADV", "ADV", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Freiheit, des Jahrhunderts G\u00f6ttin heischt solch waffenhelles Klingen.", "tokens": ["Frei\u00b7heit", ",", "des", "Jahr\u00b7hun\u00b7derts", "G\u00f6t\u00b7tin", "heischt", "solch", "waf\u00b7fen\u00b7hel\u00b7les", "Klin\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+--++-+-+-+-+-+-", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Drohet nur ihr dunklen Wolken, so die Sonne noch umzogen", "tokens": ["Dro\u00b7het", "nur", "ihr", "dunk\u00b7len", "Wol\u00b7ken", ",", "so", "die", "Son\u00b7ne", "noch", "um\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,", "ADV", "ART", "NN", "ADV", "VVIZU"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Eure Blitze falln wie Pfeile von den schlaffgewordnen Bogen,", "tokens": ["Eu\u00b7re", "Blit\u00b7ze", "falln", "wie", "Pfei\u00b7le", "von", "den", "schlaff\u00b7ge\u00b7word\u00b7nen", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "KOKOM", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Fallen machtlos, z\u00fcndlos nieder, schrecken nur der Feigen Wege,", "tokens": ["Fal\u00b7len", "macht\u00b7los", ",", "z\u00fcnd\u00b7los", "nie\u00b7der", ",", "schre\u00b7cken", "nur", "der", "Fei\u00b7gen", "We\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADJD", "PTKVZ", "$,", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Doch wir Andern sprechen tr\u00f6stend: seht! es sind nur kalte Schl\u00e4ge.", "tokens": ["Doch", "wir", "An\u00b7dern", "spre\u00b7chen", "tr\u00f6s\u00b7tend", ":", "seht", "!", "es", "sind", "nur", "kal\u00b7te", "Schl\u00e4\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "VVFIN", "ADJD", "$.", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Laut besingen die Geliebte heischt des Liebenden Begehren", "tokens": ["Laut", "be\u00b7sin\u00b7gen", "die", "Ge\u00b7lieb\u00b7te", "heischt", "des", "Lie\u00b7ben\u00b7den", "Be\u00b7geh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "ART", "NN", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ihre Huld in Liedern preisen und vor aller Welt sie ehren.", "tokens": ["Ih\u00b7re", "Huld", "in", "Lie\u00b7dern", "prei\u00b7sen", "und", "vor", "al\u00b7ler", "Welt", "sie", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "KON", "APPR", "PIAT", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "K\u00e4mpfen auch sie zu besitzen, sterben, f\u00fcr die Liebste sterben \u2013", "tokens": ["K\u00e4mp\u00b7fen", "auch", "sie", "zu", "be\u00b7sit\u00b7zen", ",", "ster\u00b7ben", ",", "f\u00fcr", "die", "Liebs\u00b7te", "ster\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "PTKZU", "VVINF", "$,", "VVFIN", "$,", "APPR", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Dazu sind wir all begeistert, die um's Liebchen \u00bbFreiheit\u00ab werben.", "tokens": ["Da\u00b7zu", "sind", "wir", "all", "be\u00b7geis\u00b7tert", ",", "die", "um's", "Lieb\u00b7chen", "\u00bb", "Frei\u00b7heit", "\u00ab", "wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PIAT", "VVPP", "$,", "ART", "ADJA", "NN", "$(", "NN", "$(", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}