{"textgrid.poem.38468": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Abz\u00e4hlen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ahne, Krahne, wickele, wahne,", "tokens": ["Ah\u00b7ne", ",", "Krah\u00b7ne", ",", "wi\u00b7cke\u00b7le", ",", "wah\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wollen wir nit nach England fahren,", "tokens": ["Wol\u00b7len", "wir", "nit", "nach", "En\u00b7gland", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "NE", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "England ist verschlossen,", "tokens": ["En\u00b7gland", "ist", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Schl\u00f6sser sind verrostet,", "tokens": ["Schl\u00f6s\u00b7ser", "sind", "ver\u00b7ros\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Schl\u00fcssel ist verloren,", "tokens": ["Schl\u00fcs\u00b7sel", "ist", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "M\u00fcssen wir ein Loch nein bohren,", "tokens": ["M\u00fcs\u00b7sen", "wir", "ein", "Loch", "nein", "boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKANT", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sind wir nein gekrochen,", "tokens": ["Sind", "wir", "nein", "ge\u00b7kro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKANT", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Haben die T\u00f6pf verbrochen,", "tokens": ["Ha\u00b7ben", "die", "T\u00f6pf", "ver\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.9": {"text": "Wenn der Kessel tief ist,", "tokens": ["Wenn", "der", "Kes\u00b7sel", "tief", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Wenn die Milch s\u00fc\u00df ist,", "tokens": ["Wenn", "die", "Milch", "s\u00fc\u00df", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Wenn die Puppen tanzen,", "tokens": ["Wenn", "die", "Pup\u00b7pen", "tan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Wollen wir Lanzen pflanzen.", "tokens": ["Wol\u00b7len", "wir", "Lan\u00b7zen", "pflan\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}