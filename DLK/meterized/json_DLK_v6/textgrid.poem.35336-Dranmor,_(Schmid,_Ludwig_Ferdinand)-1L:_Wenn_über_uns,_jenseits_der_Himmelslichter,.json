{"textgrid.poem.35336": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn \u00fcber uns, jenseits der Himmelslichter,", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn \u00fcber uns, jenseits der Himmelslichter,", "tokens": ["Wenn", "\u00fc\u00b7ber", "uns", ",", "jen\u00b7seits", "der", "Him\u00b7mels\u00b7lich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Sch\u00f6pfer thront, der Zeit und Raum ersonnen,", "tokens": ["Ein", "Sch\u00f6p\u00b7fer", "thront", ",", "der", "Zeit", "und", "Raum", "er\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit unl\u00f6sbaren R\u00e4tseln uns umsponnen,", "tokens": ["Mit", "un\u00b7l\u00f6s\u00b7ba\u00b7ren", "R\u00e4t\u00b7seln", "uns", "um\u00b7spon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der ist Jehovah, der ist unser Richter!", "tokens": ["Der", "ist", "Je\u00b7ho\u00b7vah", ",", "der", "ist", "un\u00b7ser", "Rich\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NE", "$,", "PRELS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nicht du mit all den irdischen Gebrechen.", "tokens": ["Nicht", "du", "mit", "all", "den", "ir\u00b7di\u00b7schen", "Ge\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "APPR", "PIAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was ist dein priesterlicher Strahlenkranz?", "tokens": ["Was", "ist", "dein", "pries\u00b7ter\u00b7li\u00b7cher", "Strah\u00b7len\u00b7kranz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Theaterschmuck; \u2013 dein Pomp? \u2013 ein Mummenschanz;", "tokens": ["The\u00b7a\u00b7ter\u00b7schmuck", ";", "\u2013", "dein", "Pomp", "?", "\u2013", "ein", "Mum\u00b7men\u00b7schanz", ";"], "token_info": ["word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "$(", "PPOSAT", "NN", "$.", "$(", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dein Gotteswort? \u2013 ein nichtiges Versprechen.", "tokens": ["Dein", "Got\u00b7tes\u00b7wort", "?", "\u2013", "ein", "nich\u00b7ti\u00b7ges", "Ver\u00b7spre\u00b7chen", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}