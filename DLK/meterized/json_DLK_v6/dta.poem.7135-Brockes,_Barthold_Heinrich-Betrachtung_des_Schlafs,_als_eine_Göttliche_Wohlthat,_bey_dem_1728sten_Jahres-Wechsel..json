{"dta.poem.7135": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie geht es aber zu, da\u00df T\u00f6ne, sonder Klang,", "tokens": ["Wie", "geht", "es", "a\u00b7ber", "zu", ",", "da\u00df", "T\u00f6\u00b7ne", ",", "son\u00b7der", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "KOUS", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df Farben, sonder Farb und Strahlen, sonder Licht,", "tokens": ["Da\u00df", "Far\u00b7ben", ",", "son\u00b7der", "Farb", "und", "Strah\u00b7len", ",", "son\u00b7der", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KON", "NN", "KON", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df C\u00f6rper, die nicht breit, nicht lang,", "tokens": ["Da\u00df", "C\u00f6r\u00b7per", ",", "die", "nicht", "breit", ",", "nicht", "lang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "PRELS", "PTKNEG", "ADJD", "$,", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Man sonder Ohren und Gesicht,", "tokens": ["Man", "son\u00b7der", "Oh\u00b7ren", "und", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jm Schlaff und Traum erblickt,", "tokens": ["Jm", "Schlaff", "und", "Traum", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Aufs wenigste zu sehn vermeinet?", "tokens": ["Aufs", "we\u00b7nigs\u00b7te", "zu", "sehn", "ver\u00b7mei\u00b7net", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "PIS", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ja, was mir noch am Traum gantz unbegreifflich scheinet,", "tokens": ["Ja", ",", "was", "mir", "noch", "am", "Traum", "gantz", "un\u00b7be\u00b7greif\u00b7flich", "schei\u00b7net", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWS", "PPER", "ADV", "APPRART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ist, da\u00df uns offt gar tr\u00e4umt, uns tr\u00e4ume.", "tokens": ["Ist", ",", "da\u00df", "uns", "offt", "gar", "tr\u00e4umt", ",", "uns", "tr\u00e4u\u00b7me", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}