{"textgrid.poem.24862": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "In den Alpen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ein Sohn, der, lange versto\u00dfen,", "tokens": ["Wie", "ein", "Sohn", ",", "der", ",", "lan\u00b7ge", "ver\u00b7sto\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "$,", "ADV", "VVINF", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Reuig zum Elternhause kehrt,", "tokens": ["Reu\u00b7ig", "zum", "El\u00b7tern\u00b7hau\u00b7se", "kehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Fl\u00fccht' ich wieder an den gro\u00dfen,", "tokens": ["Fl\u00fccht'", "ich", "wie\u00b7der", "an", "den", "gro\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Allen heimatlichen Herd,", "tokens": ["Al\u00b7len", "hei\u00b7mat\u00b7li\u00b7chen", "Herd", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo, wenn die Sonne tauende Strahlen", "tokens": ["Wo", ",", "wenn", "die", "Son\u00b7ne", "tau\u00b7en\u00b7de", "Strah\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "KOUS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ueber die Eisgebirge senkt,", "tokens": ["Ue\u00b7ber", "die", "Eis\u00b7ge\u00b7bir\u00b7ge", "senkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Mutter Natur aus krystallenen Schalen", "tokens": ["Mut\u00b7ter", "Na\u00b7tur", "aus", "krys\u00b7tal\u00b7le\u00b7nen", "Scha\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Ihre d\u00fcrstenden Kinder tr\u00e4nkt.", "tokens": ["Ih\u00b7re", "d\u00fcrs\u00b7ten\u00b7den", "Kin\u00b7der", "tr\u00e4nkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Die an den wei\u00dfen Gletscherbr\u00fcsten", "tokens": ["Die", "an", "den", "wei\u00b7\u00dfen", "Glet\u00b7scher\u00b7br\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr den Tag, den jungen, s\u00e4ugt,", "tokens": ["Ihr", "den", "Tag", ",", "den", "jun\u00b7gen", ",", "s\u00e4ugt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denen die morgenlicht-gek\u00fc\u00dften", "tokens": ["De\u00b7nen", "die", "mor\u00b7gen\u00b7licht\u00b7ge\u00b7k\u00fc\u00df\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PDS", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Scheitel nimmer das Alter beugt,", "tokens": ["Schei\u00b7tel", "nim\u00b7mer", "das", "Al\u00b7ter", "beugt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Ewige Alpen, zu eurer Firne", "tokens": ["E\u00b7wi\u00b7ge", "Al\u00b7pen", ",", "zu", "eu\u00b7rer", "Fir\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "Hebt mich empor in das reinere Blau!", "tokens": ["Hebt", "mich", "em\u00b7por", "in", "das", "rei\u00b7ne\u00b7re", "Blau", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Sprengt mir auf die gl\u00fchende Stirne", "tokens": ["Sprengt", "mir", "auf", "die", "gl\u00fc\u00b7hen\u00b7de", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Eurer B\u00e4che silbernen Tau!", "tokens": ["Eu\u00b7rer", "B\u00e4\u00b7che", "sil\u00b7ber\u00b7nen", "Tau", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "In der unendlichen S\u00e4ulenhalle,", "tokens": ["In", "der", "un\u00b7end\u00b7li\u00b7chen", "S\u00e4u\u00b7len\u00b7hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Drunter die ersten Menschen gekniet,", "tokens": ["Drun\u00b7ter", "die", "ers\u00b7ten", "Men\u00b7schen", "ge\u00b7kniet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Juble um mich im donnernden Falle", "tokens": ["Jub\u00b7le", "um", "mich", "im", "don\u00b7nern\u00b7den", "Fal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PRF", "APPRART", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die Lawine ihr Morgenlied,", "tokens": ["Die", "La\u00b7wi\u00b7ne", "ihr", "Mor\u00b7gen\u00b7lied", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Und zum Himmel, der \u00fcber den hehren", "tokens": ["Und", "zum", "Him\u00b7mel", ",", "der", "\u00fc\u00b7ber", "den", "heh\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "$,", "PRELS", "APPR", "ART", "ADJA"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Bergespfeilern sich weithin spannt,", "tokens": ["Ber\u00b7ge\u00b7spfei\u00b7lern", "sich", "weit\u00b7hin", "spannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Von den schimmernden Eisalt\u00e4ren", "tokens": ["Von", "den", "schim\u00b7mern\u00b7den", "Ei\u00b7sal\u00b7t\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Hochauf schlage der Opferbrand!", "tokens": ["Hoch\u00b7auf", "schla\u00b7ge", "der", "Op\u00b7fer\u00b7brand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Wo seit der Zeiten fr\u00fchstem Beginnen", "tokens": ["Wo", "seit", "der", "Zei\u00b7ten", "fr\u00fchs\u00b7tem", "Be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "An die Sterne die Jungfrau ragt,", "tokens": ["An", "die", "Ster\u00b7ne", "die", "Jung\u00b7frau", "ragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Will ich ruhen auf strahlenden Zinnen,", "tokens": ["Will", "ich", "ru\u00b7hen", "auf", "strah\u00b7len\u00b7den", "Zin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Die zu erfliegen kein Adler wagt;", "tokens": ["Die", "zu", "er\u00b7flie\u00b7gen", "kein", "Ad\u00b7ler", "wagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKZU", "VVINF", "PIAT", "NN", "VVFIN", "$."], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Oder am Strom, der mit Wirbeln und Kochen", "tokens": ["O\u00b7der", "am", "Strom", ",", "der", "mit", "Wir\u00b7beln", "und", "Ko\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "$,", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Selbst die Windsbraut \u00fcbert\u00e4ubt,", "tokens": ["Selbst", "die", "Winds\u00b7braut", "\u00fc\u00b7bert\u00b7\u00e4ubt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schaun, wie in wei\u00dfe Nebel gebrochen,", "tokens": ["Schaun", ",", "wie", "in", "wei\u00b7\u00dfe", "Ne\u00b7bel", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Wieder die Flut aus dem Abgrund st\u00e4ubt!", "tokens": ["Wie\u00b7der", "die", "Flut", "aus", "dem", "Ab\u00b7grund", "st\u00e4ubt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Dort in den Armen sollst du mich halten,", "tokens": ["Dort", "in", "den", "Ar\u00b7men", "sollst", "du", "mich", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "++-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Mutter, die einzig du treu uns bist,", "tokens": ["Mut\u00b7ter", ",", "die", "ein\u00b7zig", "du", "treu", "uns", "bist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "PPER", "ADJD", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Mir vor deinen gro\u00dfen Gestalten", "tokens": ["Mir", "vor", "dei\u00b7nen", "gro\u00b7\u00dfen", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Zeigen, wie klein das Leben ist,", "tokens": ["Zei\u00b7gen", ",", "wie", "klein", "das", "Le\u00b7ben", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADJD", "ART", "NN", "VAFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Und die Hand auf die Schl\u00e4fe mir legen,", "tokens": ["Und", "die", "Hand", "auf", "die", "Schl\u00e4\u00b7fe", "mir", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Die von den M\u00fchen der Erde tropft,", "tokens": ["Die", "von", "den", "M\u00fc\u00b7hen", "der", "Er\u00b7de", "tropft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Bis der Puls mir in leiseren Schl\u00e4gen", "tokens": ["Bis", "der", "Puls", "mir", "in", "lei\u00b7se\u00b7ren", "Schl\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "APPR", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Mit dem deinen in Einklang klopft.", "tokens": ["Mit", "dem", "dei\u00b7nen", "in", "Ein\u00b7klang", "klopft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "APPR", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}