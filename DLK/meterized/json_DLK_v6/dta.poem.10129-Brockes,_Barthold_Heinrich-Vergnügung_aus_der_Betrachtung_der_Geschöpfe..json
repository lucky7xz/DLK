{"dta.poem.10129": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vergn\u00fcgung aus der Betrachtung  \n der Gesch\u00f6pfe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Sch\u00f6pfer, dessen Allmachts-Spuren", "tokens": ["Mein", "Sch\u00f6p\u00b7fer", ",", "des\u00b7sen", "All\u00b7machts\u00b7Spu\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich einzig in den Creaturen,", "tokens": ["Ich", "ein\u00b7zig", "in", "den", "Crea\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mit fr\u00f6hlichem Erstaunen, seh\u2019,", "tokens": ["Mit", "fr\u00f6h\u00b7li\u00b7chem", "Er\u00b7stau\u00b7nen", ",", "seh'", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Dessen Weisheit, sammt den Trieben", "tokens": ["Und", "Des\u00b7sen", "Weis\u00b7heit", ",", "sammt", "den", "Trie\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Von Seinem v\u00e4terlichen lieben", "tokens": ["Von", "Sei\u00b7nem", "v\u00e4\u00b7ter\u00b7li\u00b7chen", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich, wenn ich sie beschau\u2019, erh\u00f6h.", "tokens": ["Ich", ",", "wenn", "ich", "sie", "be\u00b7schau'", ",", "er\u00b7h\u00f6h", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "PPER", "PPER", "PTKVZ", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In allem, was sie mir entdecken,", "tokens": ["In", "al\u00b7lem", ",", "was", "sie", "mir", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nimmt mir ein holdes heilges Schrecken,", "tokens": ["Nimmt", "mir", "ein", "hol\u00b7des", "heil\u00b7ges", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Vermischt mit s\u00fcssen Freuden, ein.", "tokens": ["Ver\u00b7mischt", "mit", "s\u00fcs\u00b7sen", "Freu\u00b7den", ",", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "HeRR, Deine Wercke sind so sch\u00f6ne,", "tokens": ["HeRR", ",", "Dei\u00b7ne", "Wer\u00b7cke", "sind", "so", "sch\u00f6\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "ADJA", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Da\u00df sie ein seligs Lob-Geth\u00f6ne", "tokens": ["Da\u00df", "sie", "ein", "se\u00b7ligs", "Lob\u00b7Get\u00b7h\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Von Mensch- und Engeln w\u00fcrdig seyn.", "tokens": ["Von", "Men\u00b7sch", "und", "En\u00b7geln", "w\u00fcr\u00b7dig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "ADJD", "VAINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ach! la\u00df uns, wenn wir sie ergr\u00fcnden,", "tokens": ["Ach", "!", "la\u00df", "uns", ",", "wenn", "wir", "sie", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVIMP", "PPER", "$,", "KOUS", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dich stets allgegenw\u00e4rtig finden,", "tokens": ["Dich", "stets", "all\u00b7ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Du allgegenw\u00e4rtig bist.", "tokens": ["Wie", "Du", "all\u00b7ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "bist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So wird, was uns auch wiederf\u00e4hret,", "tokens": ["So", "wird", ",", "was", "uns", "auch", "wie\u00b7der\u00b7f\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Fr\u00fch oder sp\u00e4t in Lust verkehret,", "tokens": ["Fr\u00fch", "o\u00b7der", "sp\u00e4t", "in", "Lust", "ver\u00b7keh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Weil, wo Du bist, kein Elend ist.", "tokens": ["Weil", ",", "wo", "Du", "bist", ",", "kein", "E\u00b7lend", "ist", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "PPER", "VAFIN", "$,", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}