{"textgrid.poem.58941": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "160. Richte nicht!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siehst du, da\u00df andre Menschen fehlen,", "tokens": ["Siehst", "du", ",", "da\u00df", "and\u00b7re", "Men\u00b7schen", "feh\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So schlie\u00df die Augen davor zu;", "tokens": ["So", "schlie\u00df", "die", "Au\u00b7gen", "da\u00b7vor", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PAV", "PTKVZ", "$."], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Gedenk, wie steht's mit deiner Seelen!", "tokens": ["Ge\u00b7denk", ",", "wie", "steht's", "mit", "dei\u00b7ner", "See\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Viel mehr und schlimmer fehlest du.", "tokens": ["Viel", "mehr", "und", "schlim\u00b7mer", "feh\u00b7lest", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADJD", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}