{"textgrid.poem.67690": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Welt ist Gottes Kleid;", "tokens": ["Die", "Welt", "ist", "Got\u00b7tes", "Kleid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Lobt sich ein edler Mann vom Kleide?", "tokens": ["Lobt", "sich", "ein", "ed\u00b7ler", "Mann", "vom", "Klei\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu Seel' und Herz und Freundlichkeit,", "tokens": ["Zu", "Seel'", "und", "Herz", "und", "Freund\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ist's noch weit, noch weit", "tokens": ["Wie", "ist's", "noch", "weit", ",", "noch", "weit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vom Kleide!", "tokens": ["Vom", "Klei\u00b7de", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Er zog sein Kleid, die Himmel, aus,", "tokens": ["Er", "zog", "sein", "Kleid", ",", "die", "Him\u00b7mel", ",", "aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kam arm wie wir auf Erden,", "tokens": ["Kam", "arm", "wie", "wir", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KOKOM", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kam Unsersgleichen in sein Haus,", "tokens": ["Kam", "Un\u00b7sers\u00b7glei\u00b7chen", "in", "sein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur unser Freund zu werden,", "tokens": ["Nur", "un\u00b7ser", "Freund", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nur uns sein Herz anzuvertraun,", "tokens": ["Nur", "uns", "sein", "Herz", "an\u00b7zu\u00b7ver\u00b7traun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPOSAT", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O Gottesherz zu lernen!", "tokens": ["O", "Got\u00b7tes\u00b7herz", "zu", "ler\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sein Blick, ", "tokens": ["Sein", "Blick", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Als Laut von allen Sternen.", "tokens": ["Als", "Laut", "von", "al\u00b7len", "Ster\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}