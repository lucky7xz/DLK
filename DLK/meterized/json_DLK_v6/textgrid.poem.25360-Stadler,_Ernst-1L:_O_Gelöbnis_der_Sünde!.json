{"textgrid.poem.25360": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Gel\u00f6bnis der S\u00fcnde!", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Gel\u00f6bnis der S\u00fcnde!", "tokens": ["O", "Ge\u00b7l\u00f6b\u00b7nis", "der", "S\u00fcn\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "All' ihr auferlegten Pilgerfahrten in entehrte Betten!", "tokens": ["All'", "ihr", "auf\u00b7er\u00b7leg\u00b7ten", "Pil\u00b7ger\u00b7fahr\u00b7ten", "in", "ent\u00b7ehr\u00b7te", "Bet\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Stationen der Erniedrigung und der Begierde", "tokens": ["Sta\u00b7ti\u00b7o\u00b7nen", "der", "Er\u00b7nied\u00b7ri\u00b7gung", "und", "der", "Be\u00b7gier\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "an verdammten St\u00e4tten!", "tokens": ["an", "ver\u00b7damm\u00b7ten", "St\u00e4t\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Obdach beschmutzter Kammern, Herd in der Stube,", "tokens": ["Ob\u00b7dach", "be\u00b7schmutz\u00b7ter", "Kam\u00b7mern", ",", "Herd", "in", "der", "Stu\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "wo die Speisereste verderben,", "tokens": ["wo", "die", "Spei\u00b7se\u00b7res\u00b7te", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Und die qualmende \u00d6llampe, und \u00fcber der", "tokens": ["Und", "die", "qual\u00b7men\u00b7de", "\u00d6l\u00b7lam\u00b7pe", ",", "und", "\u00fc\u00b7ber", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "KON", "APPR", "ART"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "wackligen Kommode der Spiegel in Scherben!", "tokens": ["wack\u00b7li\u00b7gen", "Kom\u00b7mo\u00b7de", "der", "Spie\u00b7gel", "in", "Scher\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Ihr zertretnen Leiber! du L\u00e4cheln,", "tokens": ["Ihr", "zer\u00b7tret\u00b7nen", "Lei\u00b7ber", "!", "du", "L\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "PPER", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "krampfhaft in gemalte Lippen eingeschnitten!", "tokens": ["krampf\u00b7haft", "in", "ge\u00b7mal\u00b7te", "Lip\u00b7pen", "ein\u00b7ge\u00b7schnit\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.11": {"text": "Armes, ungepflegtes Haar!", "tokens": ["Ar\u00b7mes", ",", "un\u00b7ge\u00b7pfleg\u00b7tes", "Haar", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "ihr Worte, denen Leben l\u00e4ngst entglitten \u2013", "tokens": ["ihr", "Wor\u00b7te", ",", "de\u00b7nen", "Le\u00b7ben", "l\u00e4ngst", "ent\u00b7glit\u00b7ten", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Seid ihr wieder um mich,", "tokens": ["Seid", "ihr", "wie\u00b7der", "um", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.14": {"text": "h\u00f6r' ich euch meinen Namen nennen?", "tokens": ["h\u00f6r'", "ich", "euch", "mei\u00b7nen", "Na\u00b7men", "nen\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+--+--+--", "measure": "dactylic.tri"}, "line.15": {"text": "F\u00fchl' ich aus Scham und Angst wieder den einen Drang", "tokens": ["F\u00fchl'", "ich", "aus", "Scham", "und", "Angst", "wie\u00b7der", "den", "ei\u00b7nen", "Drang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "NN", "KON", "NN", "ADV", "ART", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "nur mich zerbrennen:", "tokens": ["nur", "mich", "zer\u00b7bren\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "Sicherheit der Frommen,", "tokens": ["Si\u00b7cher\u00b7heit", "der", "From\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.18": {"text": "W\u00fcrde der Gerechten anzuspeien,", "tokens": ["W\u00fcr\u00b7de", "der", "Ge\u00b7rech\u00b7ten", "an\u00b7zu\u00b7spei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Tr\u00fcbem, Ungewissem, schon Verlornem", "tokens": ["Tr\u00fc\u00b7bem", ",", "Un\u00b7ge\u00b7wis\u00b7sem", ",", "schon", "Ver\u00b7lor\u00b7nem"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "ADV", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "mich zu schenken, mich zu weihen,", "tokens": ["mich", "zu", "schen\u00b7ken", ",", "mich", "zu", "wei\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Selig singend", "tokens": ["Se\u00b7lig", "sin\u00b7gend"], "token_info": ["word", "word"], "pos": ["ADJD", "VVPP"], "meter": "+-+-", "measure": "trochaic.di"}, "line.22": {"text": "Schmach und Dumpfheit der Geschlagenen zu f\u00fchlen,", "tokens": ["Schmach", "und", "Dum\u00b7pf\u00b7heit", "der", "Ge\u00b7schla\u00b7ge\u00b7nen", "zu", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "Mich ins Mark des Lebens", "tokens": ["Mich", "ins", "Mark", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.24": {"text": "wie in Gruben Erde einzuw\u00fchlen.", "tokens": ["wie", "in", "Gru\u00b7ben", "Er\u00b7de", "ein\u00b7zu\u00b7w\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "NN", "VVIZU", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}