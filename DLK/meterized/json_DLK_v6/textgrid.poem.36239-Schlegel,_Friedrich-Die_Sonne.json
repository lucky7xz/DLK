{"textgrid.poem.36239": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Sonne", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit lieblichem Bedauern", "tokens": ["Mit", "lieb\u00b7li\u00b7chem", "Be\u00b7dau\u00b7ern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sehnt sich der Mutter Auge, und mu\u00df trauern.", "tokens": ["Sehnt", "sich", "der", "Mut\u00b7ter", "Au\u00b7ge", ",", "und", "mu\u00df", "trau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "$,", "KON", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Noch einmal sie umfangend,", "tokens": ["Noch", "ein\u00b7mal", "sie", "um\u00b7fan\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vergehn die Kleinen, an den Blicken hangend;", "tokens": ["Ver\u00b7gehn", "die", "Klei\u00b7nen", ",", "an", "den", "Bli\u00b7cken", "han\u00b7gend", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie soll und mu\u00df sich trennen,", "tokens": ["Sie", "soll", "und", "mu\u00df", "sich", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nur eine Mutter kann solch Leid erkennen.", "tokens": ["Nur", "ei\u00b7ne", "Mut\u00b7ter", "kann", "solch", "Leid", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So str\u00f6m' ich volle Farben,", "tokens": ["So", "str\u00f6m'", "ich", "vol\u00b7le", "Far\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df meine Lieben in der Nacht nicht darben;", "tokens": ["Da\u00df", "mei\u00b7ne", "Lie\u00b7ben", "in", "der", "Nacht", "nicht", "dar\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "APPR", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und fort vom ird'schen Bande", "tokens": ["Und", "fort", "vom", "ird'\u00b7schen", "Ban\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Will alles hin zu mir in sanftem Brande.", "tokens": ["Will", "al\u00b7les", "hin", "zu", "mir", "in", "sanf\u00b7tem", "Bran\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "APPR", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ach d\u00fcrft' ich mich erniedern,", "tokens": ["Ach", "d\u00fcrft'", "ich", "mich", "er\u00b7nie\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Ihr kindlich Feuer dankbar zu erwidern!", "tokens": ["Ihr", "kind\u00b7lich", "Feu\u00b7er", "dank\u00b7bar", "zu", "er\u00b7wi\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Noch str\u00f6men bunte Fluten,", "tokens": ["Noch", "str\u00f6\u00b7men", "bun\u00b7te", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Und heller lodern nur die Lebensgluten;", "tokens": ["Und", "hel\u00b7ler", "lo\u00b7dern", "nur", "die", "Le\u00b7bens\u00b7glu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Die Erde scheint zu rauschen,", "tokens": ["Die", "Er\u00b7de", "scheint", "zu", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Als strebte sie den Wohnsitz zu vertauschen. \u2013", "tokens": ["Als", "streb\u00b7te", "sie", "den", "Wohn\u00b7sitz", "zu", "ver\u00b7tau\u00b7schen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Nun mu\u00df ich dennoch scheiden,", "tokens": ["Nun", "mu\u00df", "ich", "den\u00b7noch", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Und euer T\u00e4ndeln bis auf Morgen meiden!", "tokens": ["Und", "eu\u00b7er", "T\u00e4n\u00b7deln", "bis", "auf", "Mor\u00b7gen", "mei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "So sauge, Mensch, denn trunken", "tokens": ["So", "sau\u00b7ge", ",", "Mensch", ",", "denn", "trun\u00b7ken"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "NN", "$,", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Der gro\u00dfen Mutter letzte Liebesfunken!", "tokens": ["Der", "gro\u00b7\u00dfen", "Mut\u00b7ter", "letz\u00b7te", "Lie\u00b7be\u00b7sfun\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Noch einmal will ich strahlen,", "tokens": ["Noch", "ein\u00b7mal", "will", "ich", "strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Und dann versinken in der Trennung Qualen.", "tokens": ["Und", "dann", "ver\u00b7sin\u00b7ken", "in", "der", "Tren\u00b7nung", "Qua\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}