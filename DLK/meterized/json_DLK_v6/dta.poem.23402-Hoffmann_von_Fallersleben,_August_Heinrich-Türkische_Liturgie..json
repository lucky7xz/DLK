{"dta.poem.23402": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "T\u00fcrkische Liturgie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir m\u00fcssen beten f\u00fcr den Einen,", "tokens": ["Wir", "m\u00fcs\u00b7sen", "be\u00b7ten", "f\u00fcr", "den", "Ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und nur f\u00fcr Ihn und f\u00fcr die Seinen.", "tokens": ["Und", "nur", "f\u00fcr", "Ihn", "und", "f\u00fcr", "die", "Sei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "KON", "APPR", "ART", "PPOSS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wir thaten's gern und thun es gern", "tokens": ["Wir", "tha\u00b7ten's", "gern", "und", "thun", "es", "gern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und siehn f\u00fcr Ihn zu Gott dem Herrn.", "tokens": ["Und", "siehn", "f\u00fcr", "Ihn", "zu", "Gott", "dem", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPER", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es steht ja in der Schrift geschrieben:", "tokens": ["Es", "steht", "ja", "in", "der", "Schrift", "ge\u00b7schrie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir sollen unsre Feinde lieben.", "tokens": ["Wir", "sol\u00b7len", "uns\u00b7re", "Fein\u00b7de", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Drum la\u00dft uns beten das Gebet", "tokens": ["Drum", "la\u00dft", "uns", "be\u00b7ten", "das", "Ge\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "F\u00fcr unsers Sultans Majest\u00e4t!", "tokens": ["F\u00fcr", "un\u00b7sers", "Sul\u00b7tans", "Ma\u00b7jes\u00b7t\u00e4t", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}