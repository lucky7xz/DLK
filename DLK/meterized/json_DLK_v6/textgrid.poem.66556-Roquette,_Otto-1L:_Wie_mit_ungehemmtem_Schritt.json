{"textgrid.poem.66556": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie mit ungehemmtem Schritt", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie mit ungehemmtem Schritt", "tokens": ["Wie", "mit", "un\u00b7ge\u00b7hemm\u00b7tem", "Schritt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wechseln Tag und Leben,", "tokens": ["Wech\u00b7seln", "Tag", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nimmt der Wechsel dich auch mit,", "tokens": ["Nimmt", "der", "Wech\u00b7sel", "dich", "auch", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wandelt sich dein Streben.", "tokens": ["Wan\u00b7delt", "sich", "dein", "Stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Holde Z\u00fcge, Melodie'n", "tokens": ["Hol\u00b7de", "Z\u00fc\u00b7ge", ",", "Me\u00b7lo\u00b7die'n"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zaubrisch einst ergreifend,", "tokens": ["Zaub\u00b7risch", "einst", "er\u00b7grei\u00b7fend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "L\u00e4\u00dft du k\u00fchl vor\u00fcber ziehn,", "tokens": ["L\u00e4\u00dft", "du", "k\u00fchl", "vor\u00b7\u00fc\u00b7ber", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kaum die Seele streifend.", "tokens": ["Kaum", "die", "See\u00b7le", "strei\u00b7fend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Was dein Wesen einst ber\u00fcckt,", "tokens": ["Was", "dein", "We\u00b7sen", "einst", "be\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was dein Herz bereute,", "tokens": ["Was", "dein", "Herz", "be\u00b7reu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Bl\u00fcthen sind's, im Lenz gepfl\u00fcckt,", "tokens": ["Bl\u00fc\u00b7then", "sin\u00b7d's", ",", "im", "Lenz", "ge\u00b7pfl\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Die der Wind zerstreute.", "tokens": ["Die", "der", "Wind", "zer\u00b7streu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Wenn zu l\u00e4cheln dir gelang", "tokens": ["Wenn", "zu", "l\u00e4\u00b7cheln", "dir", "ge\u00b7lang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKZU", "VVFIN", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dem, was du verloren,", "tokens": ["Dem", ",", "was", "du", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wei\u00dft du, welchem Wandelgang", "tokens": ["Wei\u00dft", "du", ",", "wel\u00b7chem", "Wan\u00b7del\u00b7gang"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich die Zeit erkoren?", "tokens": ["Dich", "die", "Zeit", "er\u00b7ko\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}