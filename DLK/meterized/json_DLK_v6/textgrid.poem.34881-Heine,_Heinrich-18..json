{"textgrid.poem.34881": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ritter Paulus, edler R\u00e4uber,", "tokens": ["Rit\u00b7ter", "Pau\u00b7lus", ",", "ed\u00b7ler", "R\u00e4u\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit gerunzelt d\u00fcstren Stirnen", "tokens": ["Mit", "ge\u00b7run\u00b7zelt", "d\u00fcst\u00b7ren", "Stir\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaun die G\u00f6tter auf dich nieder,", "tokens": ["Schaun", "die", "G\u00f6t\u00b7ter", "auf", "dich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich bedroht das h\u00f6chste Z\u00fcrnen,", "tokens": ["Dich", "be\u00b7droht", "das", "h\u00f6chs\u00b7te", "Z\u00fcr\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ob dem Raube, ob dem Diebstahl,", "tokens": ["Ob", "dem", "Rau\u00b7be", ",", "ob", "dem", "Dieb\u00b7stahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den du im Olymp begangen \u2013", "tokens": ["Den", "du", "im", "O\u00b7lymp", "be\u00b7gan\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcrchte des Prometheus Schicksal,", "tokens": ["F\u00fcrch\u00b7te", "des", "Pro\u00b7me\u00b7theus", "Schick\u00b7sal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn dich Jovis H\u00e4scher fangen!", "tokens": ["Wenn", "dich", "Jo\u00b7vis", "H\u00e4\u00b7scher", "fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Freilich, jener stahl noch Schlimmres,", "tokens": ["Frei\u00b7lich", ",", "je\u00b7ner", "stahl", "noch", "Schlimm\u00b7res", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PDS", "VVFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stahl das Licht, die Flammenkr\u00e4fte,", "tokens": ["Stahl", "das", "Licht", ",", "die", "Flam\u00b7men\u00b7kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Um die Menschheit zu erleuchten \u2013", "tokens": ["Um", "die", "Menschheit", "zu", "er\u00b7leuch\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Du, du stahlest Schellings Hefte,", "tokens": ["Du", ",", "du", "stah\u00b7lest", "Schel\u00b7lings", "Hef\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "VVFIN", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Just das Gegenteil des Lichtes,", "tokens": ["Just", "das", "Ge\u00b7gen\u00b7teil", "des", "Lich\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Finsternis, die man betastet,", "tokens": ["Fins\u00b7ter\u00b7nis", ",", "die", "man", "be\u00b7tas\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die man greifen kann wie jene,", "tokens": ["Die", "man", "grei\u00b7fen", "kann", "wie", "je\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVINF", "VMFIN", "KOKOM", "PDS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die \u00c4gypten einst belastet.", "tokens": ["Die", "\u00c4\u00b7gyp\u00b7ten", "einst", "be\u00b7las\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}}}}}