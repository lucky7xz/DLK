{"textgrid.poem.66605": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Alter Berliner Weihnachtsmarkt", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df die Glocke l\u00e4uten vom Dome,", "tokens": ["La\u00df", "die", "Glo\u00b7cke", "l\u00e4u\u00b7ten", "vom", "Do\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+--++", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Keiner h\u00f6rt es im Menschenstrome.", "tokens": ["Kei\u00b7ner", "h\u00f6rt", "es", "im", "Men\u00b7schen\u00b7stro\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Christmarkt, Waldteufel, Trompeten, juchhei!", "tokens": ["Christ\u00b7markt", ",", "Wald\u00b7teu\u00b7fel", ",", "Trom\u00b7pe\u00b7ten", ",", "juch\u00b7hei", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,", "ITJ", "$."], "meter": "+-+--++--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wenig Wolle und viel Geschrei.", "tokens": ["We\u00b7nig", "Wol\u00b7le", "und", "viel", "Ge\u00b7schrei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Alles besehen, wenig erstehen,", "tokens": ["Al\u00b7les", "be\u00b7se\u00b7hen", ",", "we\u00b7nig", "er\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVINF", "$,", "PIS", "VVINF", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Nur zum Pl\u00e4sier mal dar\u00fcber gehen,", "tokens": ["Nur", "zum", "Pl\u00e4\u00b7sier", "mal", "da\u00b7r\u00fc\u00b7ber", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADV", "PAV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Eine Ba\u00dfgeige sich kaufen wollen,", "tokens": ["Ei\u00b7ne", "Ba\u00df\u00b7gei\u00b7ge", "sich", "kau\u00b7fen", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "VMINF", "$,"], "meter": "+-++--+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Mit einer Knarre nach Hause sich trollen, \u2013", "tokens": ["Mit", "ei\u00b7ner", "Knar\u00b7re", "nach", "Hau\u00b7se", "sich", "trol\u00b7len", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "PRF", "VVINF", "$,", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "F\u00fcr einen Sechser Schmalzkuchen schmecken,", "tokens": ["F\u00fcr", "ei\u00b7nen", "Sech\u00b7ser", "Schmalz\u00b7ku\u00b7chen", "schme\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Mumpitz machen an allen Ecken \u2013", "tokens": ["Mum\u00b7pitz", "ma\u00b7chen", "an", "al\u00b7len", "E\u00b7cken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Mag auch der Regen vom Himmel flie\u00dfen,", "tokens": ["Mag", "auch", "der", "Re\u00b7gen", "vom", "Him\u00b7mel", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Das mu\u00df der wahre Berliner genie\u00dfen,", "tokens": ["Das", "mu\u00df", "der", "wah\u00b7re", "Ber\u00b7li\u00b7ner", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.13": {"text": "Schuster und Schneider, J\u00fcngling und Mann,", "tokens": ["Schus\u00b7ter", "und", "Schnei\u00b7der", ",", "J\u00fcng\u00b7ling", "und", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.14": {"text": "Jeder, der es sich leisten kann.", "tokens": ["Je\u00b7der", ",", "der", "es", "sich", "leis\u00b7ten", "kann", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}