{"dta.poem.6783": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Des Miroirs et des  \n Lunettes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Uml\u00e4ufft wol unser Aug den Creis, der allgemein?", "tokens": ["Um\u00b7l\u00e4ufft", "wol", "un\u00b7ser", "Aug", "den", "Creis", ",", "der", "all\u00b7ge\u00b7mein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "ART", "NN", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn man den grossen Bau des Firmamentes misst;", "tokens": ["Wenn", "man", "den", "gros\u00b7sen", "Bau", "des", "Fir\u00b7ma\u00b7men\u00b7tes", "misst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "L\u00e4sst dann die Seele, die in sich erleuchtet ist,", "tokens": ["L\u00e4sst", "dann", "die", "See\u00b7le", ",", "die", "in", "sich", "er\u00b7leuch\u00b7tet", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "APPR", "PRF", "VVPP", "VAFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Jhr Instrument, das leiblich, nicht allein?", "tokens": ["Ihr", "Inst\u00b7ru\u00b7ment", ",", "das", "leib\u00b7lich", ",", "nicht", "al\u00b7lein", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADJD", "$,", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie \u00f6ffnet sich so gar den Eingang zu den Sternen,", "tokens": ["Sie", "\u00f6ff\u00b7net", "sich", "so", "gar", "den", "Ein\u00b7gang", "zu", "den", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sieht ihre Symmetrie und ew\u2019gen Wunder-Schein,", "tokens": ["Sieht", "ih\u00b7re", "Sym\u00b7me\u00b7trie", "und", "ew'\u00b7gen", "Wun\u00b7der\u00b7Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und mu\u00df in ihrem Schwung betrachtet seyn,", "tokens": ["Und", "mu\u00df", "in", "ih\u00b7rem", "Schwung", "be\u00b7trach\u00b7tet", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PPOSAT", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df sie vom irdschen Joch geschickt sich zu entfernen.", "tokens": ["Da\u00df", "sie", "vom", "ird\u00b7schen", "Joch", "ge\u00b7schickt", "sich", "zu", "ent\u00b7fer\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "VVPP", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}