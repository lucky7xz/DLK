{"textgrid.poem.65203": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Teuern, die sich hie", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Teuern, die sich hie", "tokens": ["Die", "Teu\u00b7ern", ",", "die", "sich", "hie"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein Heim gegr\u00fcndet hatten,", "tokens": ["Ein", "Heim", "ge\u00b7gr\u00fcn\u00b7det", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nicht l\u00e4nger wandeln sie", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "wan\u00b7deln", "sie"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In dieser B\u00e4ume Schatten.", "tokens": ["In", "die\u00b7ser", "B\u00e4u\u00b7me", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie schauen l\u00e4nger nicht", "tokens": ["Sie", "schau\u00b7en", "l\u00e4n\u00b7ger", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den Segen dieser Fluren.", "tokens": ["Den", "Se\u00b7gen", "die\u00b7ser", "Flu\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Aug' schlo\u00df sich dem Licht,", "tokens": ["Ihr", "Aug'", "schlo\u00df", "sich", "dem", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+---+", "measure": "dactylic.init"}, "line.4": {"text": "Verweht sind ihre Spuren.", "tokens": ["Ver\u00b7weht", "sind", "ih\u00b7re", "Spu\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch, ob geschieden auch,", "tokens": ["Doch", ",", "ob", "ge\u00b7schie\u00b7den", "auch", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "VVPP", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ich h\u00f6r' auf Geisterschwingen,", "tokens": ["Ich", "h\u00f6r'", "auf", "Geis\u00b7ter\u00b7schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im sanften Windeshauch", "tokens": ["Im", "sanf\u00b7ten", "Win\u00b7des\u00b7hauch"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Wort noch zu mir dringen.", "tokens": ["Ihr", "Wort", "noch", "zu", "mir", "drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und, ob die dunkle Schicht", "tokens": ["Und", ",", "ob", "die", "dunk\u00b7le", "Schicht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Erde sie umgebe,", "tokens": ["Der", "Er\u00b7de", "sie", "um\u00b7ge\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gestorben sind sie nicht,", "tokens": ["Ge\u00b7stor\u00b7ben", "sind", "sie", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So lange ich noch lebe.", "tokens": ["So", "lan\u00b7ge", "ich", "noch", "le\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Denn Liebe, stark und frei,", "tokens": ["Denn", "Lie\u00b7be", ",", "stark", "und", "frei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Kennt kein Vergeh'n und Schwinden,", "tokens": ["Kennt", "kein", "Ver\u00b7geh'n", "und", "Schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie wei\u00df der Welten zwei", "tokens": ["Sie", "wei\u00df", "der", "Wel\u00b7ten", "zwei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "CARD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Untrennbar zu verbinden!", "tokens": ["Un\u00b7trenn\u00b7bar", "zu", "ver\u00b7bin\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}