{"dta.poem.12787": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als  \n  TIT.  Herr A. S. von Hohberg  \n  an.  1687 zu Franckf. an einer t\u00f6dli-  \n chen wunde blieb.  \n W. A. von Stosch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df nicht, Seeligster! ob ich zu tadeln bin,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "See\u00b7ligs\u00b7ter", "!", "ob", "ich", "zu", "ta\u00b7deln", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "NN", "$.", "KOUS", "PPER", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich mehr thr\u00e4nen itzt, als dinte mu\u00df vergie\u00dfen;", "tokens": ["Da\u00df", "ich", "mehr", "thr\u00e4\u00b7nen", "itzt", ",", "als", "din\u00b7te", "mu\u00df", "ver\u00b7gie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "ADV", "$,", "KOUS", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn dein verscharrter leib rei\u00dft meine geister hin,", "tokens": ["Denn", "dein", "ver\u00b7scharr\u00b7ter", "leib", "rei\u00dft", "mei\u00b7ne", "geis\u00b7ter", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und l\u00e4st mir nichts als blut in meine feder flie\u00dfen.", "tokens": ["Und", "l\u00e4st", "mir", "nichts", "als", "blut", "in", "mei\u00b7ne", "fe\u00b7der", "flie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "KOKOM", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein muth, dein ernster flei\u00df, und deine freundligkeit", "tokens": ["Dein", "muth", ",", "dein", "erns\u00b7ter", "flei\u00df", ",", "und", "dei\u00b7ne", "freund\u00b7lig\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Stehn als drey Gratien noch stets vor meinen augen,", "tokens": ["Stehn", "als", "drey", "Gra\u00b7ti\u00b7en", "noch", "stets", "vor", "mei\u00b7nen", "au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "CARD", "NN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und lassen, ob man dich mit sande gleich bestreut,", "tokens": ["Und", "las\u00b7sen", ",", "ob", "man", "dich", "mit", "san\u00b7de", "gleich", "be\u00b7streut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "KOUS", "PIS", "PRF", "APPR", "ADJA", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mich doch die liebe noch aus deinem grabe saugen.", "tokens": ["Mich", "doch", "die", "lie\u00b7be", "noch", "aus", "dei\u00b7nem", "gra\u00b7be", "sau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach da\u00df doch wind und sturm auch hohe berge trifft!", "tokens": ["Ach", "da\u00df", "doch", "wind", "und", "sturm", "auch", "ho\u00b7he", "ber\u00b7ge", "trifft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "ADV", "ADJD", "KON", "ADJD", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich baut\u2019 auf deinen grund schon tausend freuden-s\u00e4ulen;", "tokens": ["Ich", "baut'", "auf", "dei\u00b7nen", "grund", "schon", "tau\u00b7send", "freu\u00b7den\u00b7s\u00e4u\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nun seh\u2019 ich, wie der tod uns die gesetze stifft,", "tokens": ["Nun", "seh'", "ich", ",", "wie", "der", "tod", "uns", "die", "ge\u00b7set\u00b7ze", "stifft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "PPER", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und wie die gipffel selbst zu dem verderben eilen.", "tokens": ["Und", "wie", "die", "gipf\u00b7fel", "selbst", "zu", "dem", "ver\u00b7der\u00b7ben", "ei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "APPR", "ART", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch dis wird, Seeligster! dir nur zuwider seyn,", "tokens": ["Doch", "dis", "wird", ",", "See\u00b7ligs\u00b7ter", "!", "dir", "nur", "zu\u00b7wi\u00b7der", "seyn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "$,", "NN", "$.", "PPER", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und du must selbst vielleicht uns dieses trost-mahl reichen:", "tokens": ["Und", "du", "must", "selbst", "viel\u00b7leicht", "uns", "die\u00b7ses", "trost\u00b7mahl", "rei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "ADV", "PPER", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Schenckt uns des himmels zorn gleich gall und wermuth ein;", "tokens": ["Schenckt", "uns", "des", "him\u00b7mels", "zorn", "gleich", "gall", "und", "wer\u00b7muth", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "ADV", "NE", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So soll sie die vernunfft mit zucker doch bestreichen.", "tokens": ["So", "soll", "sie", "die", "ver\u00b7nunfft", "mit", "zu\u00b7cker", "doch", "be\u00b7strei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wohlan! Ich g\u00f6nne dir der seelen s\u00fc\u00dfe ruh:", "tokens": ["Wo\u00b7hlan", "!", "Ich", "g\u00f6n\u00b7ne", "dir", "der", "see\u00b7len", "s\u00fc\u00b7\u00dfe", "ruh", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VVFIN", "PPER", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ich lasse deinen geist im hohen himmel schweben,", "tokens": ["Ich", "las\u00b7se", "dei\u00b7nen", "geist", "im", "ho\u00b7hen", "him\u00b7mel", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und ruffe dir zuletzt nur diese worte zu:", "tokens": ["Und", "ruf\u00b7fe", "dir", "zu\u00b7letzt", "nur", "die\u00b7se", "wor\u00b7te", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Kanst du nicht irdisch seyn, so will ich himmlisch leben.", "tokens": ["Kanst", "du", "nicht", "ir\u00b7disch", "seyn", ",", "so", "will", "ich", "himm\u00b7lisch", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$,", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}