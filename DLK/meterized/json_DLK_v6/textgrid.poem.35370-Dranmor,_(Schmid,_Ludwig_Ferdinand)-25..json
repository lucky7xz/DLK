{"textgrid.poem.35370": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Begrenzt ist alles Hoffen, alles Streben,", "tokens": ["Be\u00b7grenzt", "ist", "al\u00b7les", "Hof\u00b7fen", ",", "al\u00b7les", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Wahn der Ewigkeit \u2013 ein Selbstverkennen,", "tokens": ["Der", "Wahn", "der", "E\u00b7wig\u00b7keit", "\u2013", "ein", "Selbst\u00b7ver\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kein Leben ohne Tod und alles Leben", "tokens": ["Kein", "Le\u00b7ben", "oh\u00b7ne", "Tod", "und", "al\u00b7les", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur Selbstverzehrung, hastiges Verbrennen.", "tokens": ["Nur", "Selbst\u00b7ver\u00b7zeh\u00b7rung", ",", "has\u00b7ti\u00b7ges", "Ver\u00b7bren\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Des Menschen Dasein, ach, wie bald zerronnen!", "tokens": ["Des", "Men\u00b7schen", "Da\u00b7sein", ",", "ach", ",", "wie", "bald", "zer\u00b7ron\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ITJ", "$,", "PWAV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Beschr\u00e4nkt ist alles Leiden und Genie\u00dfen;", "tokens": ["Be\u00b7schr\u00e4nkt", "ist", "al\u00b7les", "Lei\u00b7den", "und", "Ge\u00b7nie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wir sch\u00f6pfen Leben aus des Schlafes Wonnen,", "tokens": ["Wir", "sch\u00f6p\u00b7fen", "Le\u00b7ben", "aus", "des", "Schla\u00b7fes", "Won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Bis wir auf immer unsre Augen schlie\u00dfen.", "tokens": ["Bis", "wir", "auf", "im\u00b7mer", "uns\u00b7re", "Au\u00b7gen", "schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ein Himmelstau f\u00fcr das erhitzte Blut,", "tokens": ["Ein", "Him\u00b7mel\u00b7stau", "f\u00fcr", "das", "er\u00b7hitz\u00b7te", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ein leiser Tod, vers\u00f6hnend und belehrend,", "tokens": ["Ein", "lei\u00b7ser", "Tod", ",", "ver\u00b7s\u00f6h\u00b7nend", "und", "be\u00b7leh\u00b7rend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Bist du, mit jedem Abend wiederkehrend,", "tokens": ["Bist", "du", ",", "mit", "je\u00b7dem", "A\u00b7bend", "wie\u00b7der\u00b7keh\u00b7rend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "O s\u00fc\u00dfer Schlummer, unser h\u00f6chstes Gut.", "tokens": ["O", "s\u00fc\u00b7\u00dfer", "Schlum\u00b7mer", ",", "un\u00b7ser", "h\u00f6chs\u00b7tes", "Gut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ja, bis auf immer wir die Augen schlie\u00dfen,", "tokens": ["Ja", ",", "bis", "auf", "im\u00b7mer", "wir", "die", "Au\u00b7gen", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "APPR", "ADV", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Kommst du, geliebter Schlaf, als eine Mahnung", "tokens": ["Kommst", "du", ",", "ge\u00b7lieb\u00b7ter", "Schlaf", ",", "als", "ei\u00b7ne", "Mah\u00b7nung"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "In Freud' und Leid \u2013 als eine Todesahnung,", "tokens": ["In", "Freud'", "und", "Leid", "\u2013", "als", "ei\u00b7ne", "To\u00b7de\u00b7sah\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$(", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Bis Sein und Nichtsein ineinander flie\u00dfen.", "tokens": ["Bis", "Sein", "und", "Nich\u00b7tsein", "in\u00b7ein\u00b7an\u00b7der", "flie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "KON", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}