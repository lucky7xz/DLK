{"dta.poem.8565": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich liege auf dem alten Kr\u00e4uterboden und \u201esimmiliere\u201c.", "tokens": ["Ich", "lie\u00b7ge", "auf", "dem", "al\u00b7ten", "Kr\u00e4u\u00b7ter\u00b7bo\u00b7den", "und", "\u201e", "sim\u00b7mi\u00b7lie\u00b7re", "\u201c", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "KON", "$(", "ADJA", "$(", "$."], "meter": "-+-+-+-+-+--+-+-", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Der liebe Gott ist der Konditor Knorr.", "tokens": ["Der", "lie\u00b7be", "Gott", "ist", "der", "Kon\u00b7di\u00b7tor", "Knorr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "NE", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.2": {"text": "Er hat eine weisse M\u00fctze", "tokens": ["Er", "hat", "ei\u00b7ne", "weis\u00b7se", "M\u00fct\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und in seinem Fenster stehn lauter Lik\u00f6rflaschen.", "tokens": ["und", "in", "sei\u00b7nem", "Fens\u00b7ter", "stehn", "lau\u00b7ter", "Li\u00b7k\u00f6r\u00b7fla\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Wenn die Sonne scheint, kann man mitten durch sie durchsehn.", "tokens": ["Wenn", "die", "Son\u00b7ne", "scheint", ",", "kann", "man", "mit\u00b7ten", "durch", "sie", "durch\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,", "VMFIN", "PIS", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "--+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Dann sind die Kuchen dahinter manchmal gelb, manchmal rot und manchmal sogar", "tokens": ["Dann", "sind", "die", "Ku\u00b7chen", "da\u00b7hin\u00b7ter", "manch\u00b7mal", "gelb", ",", "manch\u00b7mal", "rot", "und", "manch\u00b7mal", "so\u00b7gar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PAV", "ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "ADV", "ADV"], "meter": "-+-+--+-+-+--+-+--+", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "blau", "tokens": ["blau"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Der Teufel ist der Schornsteinfeger Killkant.", "tokens": ["Der", "Teu\u00b7fel", "ist", "der", "Schorn\u00b7stein\u00b7fe\u00b7ger", "Kill\u00b7kant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er hat einen Cylinderhut und keine Str\u00fcmpfe. Seine F\u00fcsse sind zum Sch\u00e4men.", "tokens": ["Er", "hat", "ei\u00b7nen", "Cy\u00b7lin\u00b7der\u00b7hut", "und", "kei\u00b7ne", "Str\u00fcmp\u00b7fe", ".", "Sei\u00b7ne", "F\u00fcs\u00b7se", "sind", "zum", "Sch\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "PIAT", "NN", "$.", "PPOSAT", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "-+---+-+-+-+-+-+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Wenn der am lieben Gott seine Lik\u00f6rflaschen vorbeigeht,", "tokens": ["Wenn", "der", "am", "lie\u00b7ben", "Gott", "sei\u00b7ne", "Li\u00b7k\u00f6r\u00b7fla\u00b7schen", "vor\u00b7bei\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPRART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "verdrehn sich seine Augen.", "tokens": ["ver\u00b7drehn", "sich", "sei\u00b7ne", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie sehn dann weiss aus!", "tokens": ["Sie", "sehn", "dann", "weiss", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Wenn man tot ist,", "tokens": ["Wenn", "man", "tot", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "wird man in die Erde gebuddelt und kriegt einen Kranz auf den Bauch.", "tokens": ["wird", "man", "in", "die", "Er\u00b7de", "ge\u00b7bud\u00b7delt", "und", "kriegt", "ei\u00b7nen", "Kranz", "auf", "den", "Bauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "ART", "NN", "VVPP", "KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+--+--+--+", "measure": "trochaic.septa.relaxed"}}, "stanza.6": {"line.1": {"text": "Ja.", "tokens": ["Ja", "."], "token_info": ["word", "punct"], "pos": ["PTKANT", "$."], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Und wenn dann bald wieder Weihnachten ist,", "tokens": ["Und", "wenn", "dann", "bald", "wie\u00b7der", "Weih\u00b7nach\u00b7ten", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ADV", "ADV", "NN", "VAFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "backt die Mutter Judenkringel.", "tokens": ["backt", "die", "Mut\u00b7ter", "Ju\u00b7den\u00b7krin\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ach, Judenkringel!", "tokens": ["Ach", ",", "Ju\u00b7den\u00b7krin\u00b7gel", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Die kann man immerzu essen. Die sind das Sch\u00f6nste, was es giebt.", "tokens": ["Die", "kann", "man", "im\u00b7mer\u00b7zu", "es\u00b7sen", ".", "Die", "sind", "das", "Sch\u00f6ns\u00b7te", ",", "was", "es", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIS", "ADV", "VVINF", "$.", "ART", "VAFIN", "ART", "NN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}}}}