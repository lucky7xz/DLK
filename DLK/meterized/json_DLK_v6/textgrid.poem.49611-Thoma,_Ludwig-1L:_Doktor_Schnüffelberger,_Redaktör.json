{"textgrid.poem.49611": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Doktor Schn\u00fcffelberger, Redakt\u00f6r", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Doktor Schn\u00fcffelberger, Redakt\u00f6r", "tokens": ["Dok\u00b7tor", "Schn\u00fcf\u00b7fel\u00b7ber\u00b7ger", ",", "Re\u00b7dak\u00b7t\u00f6r"], "token_info": ["word", "word", "punct", "word"], "pos": ["NN", "NN", "$,", "NN"], "meter": "+-+---+-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Einer durchaus gut kathol'schen", "tokens": ["Ei\u00b7ner", "durc\u00b7haus", "gut", "ka\u00b7thol'\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zeitung, kam aufs Land von M\u00fcnchen her,", "tokens": ["Zei\u00b7tung", ",", "kam", "aufs", "Land", "von", "M\u00fcn\u00b7chen", "her", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPRART", "NN", "APPR", "NE", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Um sich von der dort'gen alkohol'schen", "tokens": ["Um", "sich", "von", "der", "dort'\u00b7gen", "al\u00b7ko\u00b7hol'\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Stimmung und was sonst die Nerven reizt,", "tokens": ["Stim\u00b7mung", "und", "was", "sonst", "die", "Ner\u00b7ven", "reizt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PWS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Zu erholen. Denn das Waldozon", "tokens": ["Zu", "er\u00b7ho\u00b7len", ".", "Denn", "das", "Wal\u00b7do\u00b7zon"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$.", "KON", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "In Verbindung etwa auch mit Baden", "tokens": ["In", "Ver\u00b7bin\u00b7dung", "et\u00b7wa", "auch", "mit", "Ba\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "ADV", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Kann dem abgehetzten Gro\u00dfstadtsohn", "tokens": ["Kann", "dem", "ab\u00b7ge\u00b7hetz\u00b7ten", "Gro\u00df\u00b7stadt\u00b7sohn"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "An und f\u00fcr sich \u00fcberhaupt nicht schaden.", "tokens": ["An", "und", "f\u00fcr", "sich", "\u00fc\u00b7ber\u00b7haupt", "nicht", "scha\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "APPR", "PRF", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Seine Arbeit nahm er auch mit sich", "tokens": ["Sei\u00b7ne", "Ar\u00b7beit", "nahm", "er", "auch", "mit", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "APPR", "PRF"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Auf das Pornographische bez\u00fcglich,", "tokens": ["Auf", "das", "Por\u00b7no\u00b7gra\u00b7phi\u00b7sche", "be\u00b7z\u00fcg\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Nackicht, ganz und halb, und l\u00fcderlich,", "tokens": ["Na\u00b7ckicht", ",", "ganz", "und", "halb", ",", "und", "l\u00fc\u00b7der\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "KON", "ADJD", "$,", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Nur f\u00fcr die Verdorbensten vergn\u00fcglich.", "tokens": ["Nur", "f\u00fcr", "die", "Ver\u00b7dor\u00b7bens\u00b7ten", "ver\u00b7gn\u00fcg\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "$."], "meter": "--+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Mit der Lupe sah er das Detail,", "tokens": ["Mit", "der", "Lu\u00b7pe", "sah", "er", "das", "De\u00b7tail", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Wenn auch schon mit Abscheu, aber gr\u00fcndlich.", "tokens": ["Wenn", "auch", "schon", "mit", "Ab\u00b7scheu", ",", "a\u00b7ber", "gr\u00fcnd\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "APPR", "NN", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Ohne Schaden blieb sein Seelenheil;", "tokens": ["Oh\u00b7ne", "Scha\u00b7den", "blieb", "sein", "See\u00b7len\u00b7heil", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "Scheinbar war er wirklich nicht entz\u00fcndlich.", "tokens": ["Schein\u00b7bar", "war", "er", "wirk\u00b7lich", "nicht", "ent\u00b7z\u00fcnd\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADJD", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Also Schn\u00fcffelberger mit Gep\u00e4ck", "tokens": ["Al\u00b7so", "Schn\u00fcf\u00b7fel\u00b7ber\u00b7ger", "mit", "Ge\u00b7p\u00e4ck"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "APPR", "NN"], "meter": "+-+---+-+", "measure": "unknown.measure.tetra"}, "line.19": {"text": "Kam in diese unber\u00fchrte, stille", "tokens": ["Kam", "in", "die\u00b7se", "un\u00b7be\u00b7r\u00fchr\u00b7te", ",", "stil\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NE", "APPR", "PDAT", "ADJA", "$,", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "\u2013 Zur Erholung und zum Arbeitszweck \u2013", "tokens": ["\u2013", "Zur", "Er\u00b7ho\u00b7lung", "und", "zum", "Ar\u00b7beits\u00b7zweck", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "KON", "APPRART", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.21": {"text": "In die unber\u00fchrte Landidylle.", "tokens": ["In", "die", "un\u00b7be\u00b7r\u00fchr\u00b7te", "Lan\u00b7di\u00b7dyl\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Wohnt' bei einem gut kathol'schen Mann", "tokens": ["Wohnt'", "bei", "ei\u00b7nem", "gut", "ka\u00b7thol'\u00b7schen", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.23": {"text": "In der N\u00e4he auch des Seegestades,", "tokens": ["In", "der", "N\u00e4\u00b7he", "auch", "des", "See\u00b7ge\u00b7sta\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Wo er allenfalles baden kann.", "tokens": ["Wo", "er", "al\u00b7len\u00b7fal\u00b7les", "ba\u00b7den", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.25": {"text": "Mancher glaubt 's mir nicht, und doch, er tat es.", "tokens": ["Man\u00b7cher", "glaubt", "'s", "mir", "nicht", ",", "und", "doch", ",", "er", "tat", "es", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PPER", "PTKNEG", "$,", "KON", "ADV", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.26": {"text": "Ja, er nahm ein Bad. Nur bis zum Knie,", "tokens": ["Ja", ",", "er", "nahm", "ein", "Bad", ".", "Nur", "bis", "zum", "Knie", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ART", "NE", "$.", "ADV", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.27": {"text": "Weiter konnte er sich nicht verstehen.", "tokens": ["Wei\u00b7ter", "konn\u00b7te", "er", "sich", "nicht", "ver\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Erstens einmal tat er 's vorher nie,", "tokens": ["Ers\u00b7tens", "ein\u00b7mal", "tat", "er", "'s", "vor\u00b7her", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.29": {"text": "Zweitens h\u00e4tte er sich selbst gesehen.", "tokens": ["Zwei\u00b7tens", "h\u00e4t\u00b7te", "er", "sich", "selbst", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.30": {"text": "Drittens hat er 's gern am Nabel warm,", "tokens": ["Drit\u00b7tens", "hat", "er", "'s", "gern", "am", "Na\u00b7bel", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "APPRART", "NN", "ADJD", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.31": {"text": "Und von jung auf hat er f\u00fcr das Wasser", "tokens": ["Und", "von", "jung", "auf", "hat", "er", "f\u00fcr", "das", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJD", "APPR", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.32": {"text": "Keine Lieb' und ausgesproch'nen Schwarm,", "tokens": ["Kei\u00b7ne", "Lieb'", "und", "aus\u00b7ge\u00b7spro\u00b7ch'\u00b7nen", "Schwarm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.33": {"text": "Als ein guter Zentrumsschriftverfasser.", "tokens": ["Als", "ein", "gu\u00b7ter", "Zent\u00b7rums\u00b7schrift\u00b7ver\u00b7fas\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.34": {"text": "Ergo badet er blo\u00df bis zum Knie.", "tokens": ["Er\u00b7go", "ba\u00b7det", "er", "blo\u00df", "bis", "zum", "Knie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.35": {"text": "Pl\u00f6tzlich doch vernahm er ein Ger\u00e4usche,", "tokens": ["Pl\u00f6tz\u00b7lich", "doch", "ver\u00b7nahm", "er", "ein", "Ge\u00b7r\u00e4u\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.36": {"text": "Neben ihm da badet eine \u00bbsie\u00ab,", "tokens": ["Ne\u00b7ben", "ihm", "da", "ba\u00b7det", "ei\u00b7ne", "\u00bb", "sie", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "ART", "$(", "PPER", "$(", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.37": {"text": "von beachtenswertem Weiberfleische.", "tokens": ["von", "be\u00b7ach\u00b7tens\u00b7wer\u00b7tem", "Wei\u00b7ber\u00b7flei\u00b7sche", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.38": {"text": "Was er \u00fcbrigens nicht gleich erfuhr,", "tokens": ["Was", "er", "\u00fcb\u00b7ri\u00b7gens", "nicht", "gleich", "er\u00b7fuhr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.39": {"text": "Sondern nur gewisserma\u00dfen ahnte,", "tokens": ["Son\u00b7dern", "nur", "ge\u00b7wis\u00b7ser\u00b7ma\u00b7\u00dfen", "ahn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.40": {"text": "Durch die alte Stimme der Natur,", "tokens": ["Durch", "die", "al\u00b7te", "Stim\u00b7me", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.41": {"text": "Die bewirkte, da\u00df ihm etwas schwante.", "tokens": ["Die", "be\u00b7wirk\u00b7te", ",", "da\u00df", "ihm", "et\u00b7was", "schwan\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.42": {"text": "Schn\u00fcffelberger war ja k\u00fchl, und doch", "tokens": ["Schn\u00fcf\u00b7fel\u00b7ber\u00b7ger", "war", "ja", "k\u00fchl", ",", "und", "doch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "ADJD", "$,", "KON", "ADV"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.43": {"text": "Suchte er sogleich an allen Brettern", "tokens": ["Such\u00b7te", "er", "sog\u00b7leich", "an", "al\u00b7len", "Bret\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.44": {"text": "Mit dem sch\u00e4rfsten Blick nach einem Loch.", "tokens": ["Mit", "dem", "sch\u00e4rfs\u00b7ten", "Blick", "nach", "ei\u00b7nem", "Loch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.45": {"text": "Er begann auf eine Bank zu klettern.", "tokens": ["Er", "be\u00b7gann", "auf", "ei\u00b7ne", "Bank", "zu", "klet\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.46": {"text": "Endlich hat er eines! Ei, man sieht", "tokens": ["End\u00b7lich", "hat", "er", "ei\u00b7nes", "!", "Ei", ",", "man", "sieht"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "$.", "NN", "$,", "PIS", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.47": {"text": "Durch das Loch die ganze Badkabine,", "tokens": ["Durch", "das", "Loch", "die", "gan\u00b7ze", "Bad\u00b7ka\u00b7bi\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.48": {"text": "Jede Ecke, kurzum das Gebiet", "tokens": ["Je\u00b7de", "E\u00b7cke", ",", "kur\u00b7zum", "das", "Ge\u00b7biet"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "ADV", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.49": {"text": "Dieser badenwollenden Blondine.", "tokens": ["Die\u00b7ser", "ba\u00b7den\u00b7wol\u00b7len\u00b7den", "Blon\u00b7di\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.50": {"text": "Husch! Sie zog sich mal die Bluse aus,", "tokens": ["Husch", "!", "Sie", "zog", "sich", "mal", "die", "Blu\u00b7se", "aus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "PRF", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.51": {"text": "Und der starke Busen wogt' und wallte,", "tokens": ["Und", "der", "star\u00b7ke", "Bu\u00b7sen", "wogt'", "und", "wall\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.52": {"text": "Theoretisch war ihm das ein Graus,", "tokens": ["The\u00