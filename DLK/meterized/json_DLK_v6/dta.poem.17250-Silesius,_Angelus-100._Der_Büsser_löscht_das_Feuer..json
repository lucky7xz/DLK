{"dta.poem.17250": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "100.  Der B\u00fcsser l\u00f6scht das Feuer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Du sprichst das H\u00f6llsche Feur wird nie gel\u00f6scht gesehn:", "tokens": ["Du", "sprichst", "das", "H\u00f6ll\u00b7sche", "Feur", "wird", "nie", "ge\u00b7l\u00f6scht", "ge\u00b7sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sih der B\u00fcsser l\u00f6schts mit einem Augenthr\u00e4n!", "tokens": ["Und", "sih", "der", "B\u00fcs\u00b7ser", "l\u00f6schts", "mit", "ei\u00b7nem", "Au\u00b7gent\u00b7hr\u00e4n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}