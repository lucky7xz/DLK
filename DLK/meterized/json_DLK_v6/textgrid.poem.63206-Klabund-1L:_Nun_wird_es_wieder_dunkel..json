{"textgrid.poem.63206": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun wird es wieder dunkel.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun wird es wieder dunkel.", "tokens": ["Nun", "wird", "es", "wie\u00b7der", "dun\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kein Stern tritt mit Gefunkel", "tokens": ["Kein", "Stern", "tritt", "mit", "Ge\u00b7fun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In meine Zelle ein.", "tokens": ["In", "mei\u00b7ne", "Zel\u00b7le", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die W\u00e4nde schier erblassen,", "tokens": ["Die", "W\u00e4n\u00b7de", "schier", "er\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und gr\u00fcne H\u00e4nde fassen", "tokens": ["Und", "gr\u00fc\u00b7ne", "H\u00e4n\u00b7de", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nach mir wie zum Gespensterreihn.", "tokens": ["Nach", "mir", "wie", "zum", "Ge\u00b7spens\u00b7ter\u00b7reihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KOKOM", "APPRART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Wie wird es morgen werden?", "tokens": ["Wie", "wird", "es", "mor\u00b7gen", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kein Himmel hier auf Erden.", "tokens": ["Kein", "Him\u00b7mel", "hier", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Nacht so sanfte Wellen schl\u00e4gt.", "tokens": ["Die", "Nacht", "so", "sanf\u00b7te", "Wel\u00b7len", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich sinke wie verloren,", "tokens": ["Ich", "sin\u00b7ke", "wie", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Umh\u00fcllt von schwarzen Floren,", "tokens": ["Um\u00b7h\u00fcllt", "von", "schwar\u00b7zen", "Flo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In einen Fluss, der mich von dannen tr\u00e4gt.", "tokens": ["In", "ei\u00b7nen", "Fluss", ",", "der", "mich", "von", "dan\u00b7nen", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PRF", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}