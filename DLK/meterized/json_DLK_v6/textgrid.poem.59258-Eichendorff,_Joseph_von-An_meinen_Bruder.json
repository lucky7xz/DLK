{"textgrid.poem.59258": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An meinen Bruder", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Steig aufw\u00e4rts, Morgenstunde!", "tokens": ["Steig", "auf\u00b7w\u00e4rts", ",", "Mor\u00b7gen\u00b7stun\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "ADV", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zerrei\u00df die Nacht, da\u00df ich in meinem Wehe", "tokens": ["Zer\u00b7rei\u00df", "die", "Nacht", ",", "da\u00df", "ich", "in", "mei\u00b7nem", "We\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Himmel wiedersehe,", "tokens": ["Den", "Him\u00b7mel", "wie\u00b7der\u00b7se\u00b7he", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo ew'ger Frieden in dem blauen Grunde!", "tokens": ["Wo", "ew'\u00b7ger", "Frie\u00b7den", "in", "dem", "blau\u00b7en", "Grun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Will Licht die Welt erneuen,", "tokens": ["Will", "Licht", "die", "Welt", "er\u00b7neu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mag auch der Schmerz in Tr\u00e4nen sich befreien.", "tokens": ["Mag", "auch", "der", "Schmerz", "in", "Tr\u00e4\u00b7nen", "sich", "be\u00b7frei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "APPR", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein lieber Herzensbruder!", "tokens": ["Mein", "lie\u00b7ber", "Her\u00b7zens\u00b7bru\u00b7der", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Still war der Morgen \u2013 Ein Schiff trug uns beide,", "tokens": ["Still", "war", "der", "Mor\u00b7gen", "\u2013", "Ein", "Schiff", "trug", "uns", "bei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$(", "ART", "NN", "VVFIN", "PPER", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie war die Welt voll Freude!", "tokens": ["Wie", "war", "die", "Welt", "voll", "Freu\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du fa\u00dftest ritterlich das schwanke Ruder,", "tokens": ["Du", "fa\u00df\u00b7test", "rit\u00b7ter\u00b7lich", "das", "schwan\u00b7ke", "Ru\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Uns beide treulich lenkend,", "tokens": ["Uns", "bei\u00b7de", "treu\u00b7lich", "len\u00b7kend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auf froher Fahrt nur ", "tokens": ["Auf", "fro\u00b7her", "Fahrt", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Mich irrte manches Sch\u00f6ne,", "tokens": ["Mich", "irr\u00b7te", "man\u00b7ches", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Viel reizte mich und viel mu\u00dft ich vermissen.", "tokens": ["Viel", "reiz\u00b7te", "mich", "und", "viel", "mu\u00dft", "ich", "ver\u00b7mis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von Lust und Schmerz zerrissen,", "tokens": ["Von", "Lust", "und", "Schmerz", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was so mein Herz hinausgestr\u00f6mt in T\u00f6ne:", "tokens": ["Was", "so", "mein", "Herz", "hin\u00b7aus\u00b7ge\u00b7str\u00f6mt", "in", "T\u00f6\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es waren Widerspiele", "tokens": ["Es", "wa\u00b7ren", "Wi\u00b7der\u00b7spie\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von deines Busens ewigem Gef\u00fchle.", "tokens": ["Von", "dei\u00b7nes", "Bu\u00b7sens", "e\u00b7wi\u00b7gem", "Ge\u00b7f\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da ward die Welt so tr\u00fcbe,", "tokens": ["Da", "ward", "die", "Welt", "so", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Rings stiegen Wetter von der Berge Spitzen,", "tokens": ["Rings", "stie\u00b7gen", "Wet\u00b7ter", "von", "der", "Ber\u00b7ge", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Himmel borst in Blitzen,", "tokens": ["Der", "Him\u00b7mel", "borst", "in", "Blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df neugest\u00e4rkt sich Deutschland draus erh\u00fcbe. \u2013", "tokens": ["Da\u00df", "neu\u00b7ge\u00b7st\u00e4rkt", "sich", "Deutschland", "draus", "er\u00b7h\u00fc\u00b7be", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "VVFIN", "PRF", "NE", "PAV", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Nun ist das Schiff zerschlagen,", "tokens": ["Nun", "ist", "das", "Schiff", "zer\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie soll ich ohne dich die Flut ertragen! \u2013", "tokens": ["Wie", "soll", "ich", "oh\u00b7ne", "dich", "die", "Flut", "er\u00b7tra\u00b7gen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "PRF", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Auf einem Fels geboren,", "tokens": ["Auf", "ei\u00b7nem", "Fels", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verteilen k\u00fchler rauschend sich zwei Quellen,", "tokens": ["Ver\u00b7tei\u00b7len", "k\u00fch\u00b7ler", "rau\u00b7schend", "sich", "zwei", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "PRF", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die eigne Bahn zu schwellen.", "tokens": ["Die", "eig\u00b7ne", "Bahn", "zu", "schwel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch wie sie fern einander auch verloren:", "tokens": ["Doch", "wie", "sie", "fern", "ein\u00b7an\u00b7der", "auch", "ver\u00b7lo\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es treffen echte Br\u00fcder", "tokens": ["Es", "tref\u00b7fen", "ech\u00b7te", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im ew'gen Meere doch zusammen wieder.", "tokens": ["Im", "ew'\u00b7gen", "Mee\u00b7re", "doch", "zu\u00b7sam\u00b7men", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So wolle Gott du flehen,", "tokens": ["So", "wol\u00b7le", "Gott", "du", "fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df er mit meinem Blut und Leben schalte,", "tokens": ["Da\u00df", "er", "mit", "mei\u00b7nem", "Blut", "und", "Le\u00b7ben", "schal\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Seele nur erhalte,", "tokens": ["Die", "See\u00b7le", "nur", "er\u00b7hal\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf da\u00df wir freudig einst uns wiedersehen,", "tokens": ["Auf", "da\u00df", "wir", "freu\u00b7dig", "einst", "uns", "wie\u00b7der\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADJD", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wenn nimmermehr hienieden:", "tokens": ["Wenn", "nim\u00b7mer\u00b7mehr", "hien\u00b7ie\u00b7den", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So dort, wo Heimat, Licht und ew'ger Frieden!", "tokens": ["So", "dort", ",", "wo", "Hei\u00b7mat", ",", "Licht", "und", "ew'\u00b7ger", "Frie\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "NN", "$,", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}