{"textgrid.poem.35291": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unweit von Sorocaba hatte", "tokens": ["Un\u00b7weit", "von", "So\u00b7ro\u00b7ca\u00b7ba", "hat\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NE", "VAFIN"], "meter": "----+--+-", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Garcia sein beh\u00e4big Haus;", "tokens": ["Gar\u00b7cia", "sein", "be\u00b7h\u00e4\u00b7big", "Haus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Er wohnte dort jahrein jahraus,", "tokens": ["Er", "wohn\u00b7te", "dort", "ja\u00b7hrein", "ja\u00b7hraus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein ernster Mann, ein treuer Gatte.", "tokens": ["Ein", "erns\u00b7ter", "Mann", ",", "ein", "treu\u00b7er", "Gat\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihm war das Gl\u00fcck nicht zugeflogen,", "tokens": ["Ihm", "war", "das", "Gl\u00fcck", "nicht", "zu\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es kam als wohlverdienter Lohn:", "tokens": ["Es", "kam", "als", "wohl\u00b7ver\u00b7dien\u00b7ter", "Lohn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zwei Kinder, beide gut erzogen,", "tokens": ["Zwei", "Kin\u00b7der", ",", "bei\u00b7de", "gut", "er\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PIS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein M\u00e4dchen und ein muntrer Sohn", "tokens": ["Ein", "M\u00e4d\u00b7chen", "und", "ein", "mun\u00b7trer", "Sohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Verj\u00fcngten seine Lebensgeister,", "tokens": ["Ver\u00b7j\u00fcng\u00b7ten", "sei\u00b7ne", "Le\u00b7bens\u00b7geis\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und sonder Gram, in stillem Gl\u00fccke,", "tokens": ["Und", "son\u00b7der", "Gram", ",", "in", "stil\u00b7lem", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Verga\u00df er, da\u00df der Nachbarn T\u00fccke", "tokens": ["Ver\u00b7ga\u00df", "er", ",", "da\u00df", "der", "Nach\u00b7barn", "T\u00fc\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Ihm mit Prozessen, immer dreister,", "tokens": ["Ihm", "mit", "Pro\u00b7zes\u00b7sen", ",", "im\u00b7mer", "dreis\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Ein St\u00fcck bestritt von einem Felde,", "tokens": ["Ein", "St\u00fcck", "be\u00b7stritt", "von", "ei\u00b7nem", "Fel\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Das er mit selbsterworbnem Gelde", "tokens": ["Das", "er", "mit", "selbs\u00b7ter\u00b7worb\u00b7nem", "Gel\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Gekauft, nicht um es zu verschenken.", "tokens": ["Ge\u00b7kauft", ",", "nicht", "um", "es", "zu", "ver\u00b7schen\u00b7ken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PTKNEG", "KOUI", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "An kleine Plagen nicht zu denken", "tokens": ["An", "klei\u00b7ne", "Pla\u00b7gen", "nicht", "zu", "den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PTKNEG", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Ist f\u00fcr den Weisen schon Gewinn;", "tokens": ["Ist", "f\u00fcr", "den", "Wei\u00b7sen", "schon", "Ge\u00b7winn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Doch ein Ereignis, grausenhaft,", "tokens": ["Doch", "ein", "Er\u00b7eig\u00b7nis", ",", "grau\u00b7sen\u00b7haft", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Vermochte dieses Mannes Sinn", "tokens": ["Ver\u00b7moch\u00b7te", "die\u00b7ses", "Man\u00b7nes", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PDAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und seines Zornes Riesenkraft", "tokens": ["Und", "sei\u00b7nes", "Zor\u00b7nes", "Rie\u00b7sen\u00b7kraft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Auf blut'ge Thaten hinzulenken.", "tokens": ["Auf", "blut'\u00b7ge", "Tha\u00b7ten", "hin\u00b7zu\u00b7len\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In fr\u00fcher Morgenstunde war", "tokens": ["In", "fr\u00fc\u00b7her", "Mor\u00b7gen\u00b7stun\u00b7de", "war"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Sohn zum Pirschen ausgegangen;", "tokens": ["Sein", "Sohn", "zum", "Pir\u00b7schen", "aus\u00b7ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er wollte, als ein junger Aar,", "tokens": ["Er", "woll\u00b7te", ",", "als", "ein", "jun\u00b7ger", "Aar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht an der Mutter Sch\u00fcrze hangen.", "tokens": ["Nicht", "an", "der", "Mut\u00b7ter", "Sch\u00fcr\u00b7ze", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Vater, ohne Furcht zu n\u00e4hren,", "tokens": ["Der", "Va\u00b7ter", ",", "oh\u00b7ne", "Furcht", "zu", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Lie\u00df seinen Spr\u00f6\u00dfling gern gew\u00e4hren,", "tokens": ["Lie\u00df", "sei\u00b7nen", "Spr\u00f6\u00df\u00b7ling", "gern", "ge\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Doch bleibt er heute \u2013 schon erschrocken,", "tokens": ["Doch", "bleibt", "er", "heu\u00b7te", "\u2013", "schon", "er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$(", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als er beim Klang der Vesperglocken", "tokens": ["Als", "er", "beim", "Klang", "der", "Ves\u00b7per\u00b7glo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Den J\u00e4ger, wie es sonst geschah,", "tokens": ["Den", "J\u00e4\u00b7ger", ",", "wie", "es", "sonst", "ge\u00b7schah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Nicht an der Abendtafel sah \u2013", "tokens": ["Nicht", "an", "der", "A\u00b7bend\u00b7ta\u00b7fel", "sah", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Nicht l\u00e4nger unter seinem Dache.", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "un\u00b7ter", "sei\u00b7nem", "Da\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Gedanken steigen in ihm auf", "tokens": ["Ge\u00b7dan\u00b7ken", "stei\u00b7gen", "in", "ihm", "auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPER", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "An Hinterlist und Feindesrache,", "tokens": ["An", "Hin\u00b7ter\u00b7list", "und", "Fein\u00b7des\u00b7ra\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und Angst befl\u00fcgelt seinen Lauf.", "tokens": ["Und", "Angst", "be\u00b7fl\u00fc\u00b7gelt", "sei\u00b7nen", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Er hat sie nicht umsonst empfunden.", "tokens": ["Er", "hat", "sie", "nicht", "um\u00b7sonst", "emp\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Zwar ist der Knabe bald gefunden,", "tokens": ["Zwar", "ist", "der", "Kna\u00b7be", "bald", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Doch traut er seinen Augen kaum \u2013", "tokens": ["Doch", "traut", "er", "sei\u00b7nen", "Au\u00b7gen", "kaum", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Wie? \u2013 Das sein Kind? \u2013 An einen Baum", "tokens": ["Wie", "?", "\u2013", "Das", "sein", "Kind", "?", "\u2013", "An", "ei\u00b7nen", "Baum"], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PWAV", "$.", "$(", "PDS", "PPOSAT", "NN", "$.", "$(", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Gelehnt, die H\u00e4nde festgebunden,", "tokens": ["Ge\u00b7lehnt", ",", "die", "H\u00e4n\u00b7de", "fest\u00b7ge\u00b7bun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Und auf den Lippen roter Schaum?", "tokens": ["Und", "auf", "den", "Lip\u00b7pen", "ro\u00b7ter", "Schaum", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Ach! \u2013 und der Busen klafft von Wunden,", "tokens": ["Ach", "!", "\u2013", "und", "der", "Bu\u00b7sen", "klafft", "von", "Wun\u00b7den", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "$(", "KON", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Von sieben, sieben Messerstichen!", "tokens": ["Von", "sie\u00b7ben", ",", "sie\u00b7ben", "Mes\u00b7ser\u00b7sti\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "CARD", "$,", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Garcia ist zur\u00fcckgewichen,", "tokens": ["Gar\u00b7cia", "ist", "zu\u00b7r\u00fcck\u00b7ge\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "VVPP", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.24": {"text": "Entsetzten Blicks, das Haar gestr\u00e4ubt \u2013", "tokens": ["Ent\u00b7setz\u00b7ten", "Blicks", ",", "das", "Haar", "ge\u00b7str\u00e4ubt", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Von ungeheurem Schmerz bet\u00e4ubt,", "tokens": ["Von", "un\u00b7ge\u00b7heu\u00b7rem", "Schmerz", "be\u00b7t\u00e4ubt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Entfesselt er die teure Leiche", "tokens": ["Ent\u00b7fes\u00b7selt", "er", "die", "teu\u00b7re", "Lei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Und h\u00e4lt sie bebend in den Armen", "tokens": ["Und", "h\u00e4lt", "sie", "be\u00b7bend", "in", "den", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Und k\u00fc\u00dft die Stirn, die marmorbleiche;", "tokens": ["Und", "k\u00fc\u00dft", "die", "Stirn", ",", "die", "mar\u00b7mor\u00b7blei\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Dann schluchzt er: \u00bbJungfrau, gnadenreiche,", "tokens": ["Dann", "schluchzt", "er", ":", "\u00bb", "Jung\u00b7frau", ",", "gna\u00b7den\u00b7rei\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "NN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "O f\u00fcr den M\u00f6rder kein Erbarmen!\u00ab", "tokens": ["O", "f\u00fcr", "den", "M\u00f6r\u00b7der", "kein", "Er\u00b7bar\u00b7men", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PIAT", "NN", "$.", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Und seiner Seele, die verwirrt", "tokens": ["Und", "sei\u00b7ner", "See\u00b7le", ",", "die", "ver\u00b7wirrt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bald den, bald jenen Feind verd\u00e4chtigt", "tokens": ["Bald", "den", ",", "bald", "je\u00b7nen", "Feind", "ver\u00b7d\u00e4ch\u00b7tigt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "$,", "ADV", "PDAT", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wieder sucht und wieder irrt,", "tokens": ["Und", "wie\u00b7der", "sucht", "und", "wie\u00b7der", "irrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat bald die Wahrheit sich bem\u00e4chtigt.", "tokens": ["Hat", "bald", "die", "Wahr\u00b7heit", "si