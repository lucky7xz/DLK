ach", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPOSAT", "NN", "$(", "FM", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Gepackt \u2013 gefoltert \u2013 hingerichtet. \u2013 \u2013\u00ab", "tokens": ["Ge\u00b7packt", "\u2013", "ge\u00b7fol\u00b7tert", "\u2013", "hin\u00b7ge\u00b7rich\u00b7tet", ".", "\u2013", "\u2013", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct", "punct", "punct"], "pos": ["VVPP", "$(", "VVPP", "$(", "VVPP", "$.", "$(", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und als er nun, in wilder Hast,", "tokens": ["Und", "als", "er", "nun", ",", "in", "wil\u00b7der", "Hast", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Mantel um sein Kind geschlagen,", "tokens": ["Den", "Man\u00b7tel", "um", "sein", "Kind", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hat keuchend unter solcher Last", "tokens": ["Hat", "keu\u00b7chend", "un\u00b7ter", "sol\u00b7cher", "Last"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Leichnam heimw\u00e4rts er getragen.", "tokens": ["Den", "Leich\u00b7nam", "heim\u00b7w\u00e4rts", "er", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nacht