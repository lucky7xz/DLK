{"dta.poem.13227": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje? was verwirret mich? was fehlt dir meine Leyr?", "tokens": ["Wie", "?", "was", "ver\u00b7wir\u00b7ret", "mich", "?", "was", "fehlt", "dir", "mei\u00b7ne", "Leyr", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWS", "VVFIN", "PPER", "$.", "PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "10Dein Thon verstimmet sich? mein Auge deckt ein Schleyr?", "tokens": ["Thon", "ver\u00b7stim\u00b7met", "sich", "?", "mein", "Au\u00b7ge", "deckt", "ein", "Schleyr", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "$.", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Thalia! was geschicht? mein Geist f\u00e4ngt an zu zittern!", "tokens": ["Tha\u00b7lia", "!", "was", "ge\u00b7schicht", "?", "mein", "Geist", "f\u00e4ngt", "an", "zu", "zit\u00b7tern", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWS", "VVPP", "$.", "PPOSAT", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Eil Freundinn! wann du nicht mir willst die Lust verbittern.", "tokens": ["Eil", "Freun\u00b7dinn", "!", "wann", "du", "nicht", "mir", "willst", "die", "Lust", "ver\u00b7bit\u00b7tern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "PWAV", "PPER", "PTKNEG", "PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Allein es f\u00e4llt mir bey: seit dem ich von der Nacht", "tokens": ["Al\u00b7lein", "es", "f\u00e4llt", "mir", "bey", ":", "seit", "dem", "ich", "von", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "PTKVZ", "$.", "APPR", "PRELS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und von dem Tugend-Streit das Lied hab ausgedacht,", "tokens": ["Und", "von", "dem", "Tu\u00b7gen\u00b7dStreit", "das", "Lied", "hab", "aus\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "15Hat dieser grosse F\u00fcrst den Kayser-Thron bestiegen;", "tokens": ["die\u00b7ser", "gros\u00b7se", "F\u00fcrst", "den", "Kay\u00b7ser\u00b7Thron", "be\u00b7stie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "So soll die Cither nun vor seinen F\u00fcssen ligen.", "tokens": ["So", "soll", "die", "Cit\u00b7her", "nun", "vor", "sei\u00b7nen", "F\u00fcs\u00b7sen", "li\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der h\u00f6chst-erw\u00fcnschte Fall! die Demuth und die Lieb", "tokens": ["Der", "h\u00f6chst\u00b7er\u00b7w\u00fcnschte", "Fall", "!", "die", "De\u00b7muth", "und", "die", "Lieb"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "NN", "KON", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Sch\u00e4rft, hemmt und \u00fcberwiegt derselben Klang und Trieb;", "tokens": ["Sch\u00e4rft", ",", "hemmt", "und", "\u00fc\u00b7berw\u00b7iegt", "der\u00b7sel\u00b7ben", "Klang", "und", "Trieb", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "KON", "VVFIN", "PDAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das, was man in dem Saal gew\u00fcnscht und vorgesehen,", "tokens": ["Das", ",", "was", "man", "in", "dem", "Saal", "ge\u00b7w\u00fcnscht", "und", "vor\u00b7ge\u00b7se\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PIS", "APPR", "ART", "NN", "VVPP", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "20Ist, eh ich noch das End' erreiche, schon geschehen.", "tokens": [",", "eh", "ich", "noch", "das", "End'", "er\u00b7rei\u00b7che", ",", "schon", "ge\u00b7sche\u00b7hen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$,", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.13": {"text": "Der frohe Ruff der Welt ist mein Beweis. Allein", "tokens": ["Der", "fro\u00b7he", "Ruff", "der", "Welt", "ist", "mein", "Be\u00b7weis", ".", "Al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$.", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wie trifft nun mein Gesang mit dieser Nachricht ein?", "tokens": ["Wie", "trifft", "nun", "mein", "Ge\u00b7sang", "mit", "die\u00b7ser", "Nach\u00b7richt", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Kein Wunder ists da\u00df ich mich fast entz\u00fcckt befinde;", "tokens": ["Kein", "Wun\u00b7der", "ists", "da\u00df", "ich", "mich", "fast", "ent\u00b7z\u00fcckt", "be\u00b7fin\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "KOUS", "PPER", "PRF", "ADV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Was mach ich, da\u00df das Lied sich mit dem Fall verbinde?", "tokens": ["Was", "mach", "ich", ",", "da\u00df", "das", "Lied", "sich", "mit", "dem", "Fall", "ver\u00b7bin\u00b7de", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "25Dem von der halben Welt verfolgten K\u00f6nigs-Haupt", "tokens": ["von", "der", "hal\u00b7ben", "Welt", "ver\u00b7folg\u00b7ten", "K\u00f6\u00b7nigs\u00b7Haupt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.18": {"text": "War erst, nach einem Thron zu streben, nicht erlaubt;", "tokens": ["War", "erst", ",", "nach", "ei\u00b7nem", "Thron", "zu", "stre\u00b7ben", ",", "nicht", "er\u00b7laubt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Jezt herrscht es schon begl\u00fcckt als Haupt der Majest\u00e4ten;", "tokens": ["Jezt", "herrscht", "es", "schon", "be\u00b7gl\u00fcckt", "als", "Haupt", "der", "Ma\u00b7jes\u00b7t\u00e4\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVPP", "KOKOM", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So mu\u00df sich meine Leyr verbergen, ich err\u00f6then.", "tokens": ["So", "mu\u00df", "sich", "mei\u00b7ne", "Leyr", "ver\u00b7ber\u00b7gen", ",", "ich", "er\u00b7r\u00f6\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "PPOSAT", "NN", "VVINF", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die\u00df geschahe den 13. Septemb.\n1745. Nachmittag gegen halb drey Uhr.", "tokens": ["Die\u00df", "ge\u00b7scha\u00b7he", "den", "13.", "Sep\u00b7temb", ".", "1745", ".", "Nach\u00b7mit\u00b7tag", "ge\u00b7gen", "halb", "drey", "Uhr", "."], "token_info": ["word", "word", "word", "ordinal", "word", "punct", "number", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$.", "CARD", "$.", "NN", "APPR", "ADJD", "CARD", "NN", "$."], "meter": "--+--+-+-+--+-+", "measure": "anapaest.di.plus"}, "line.22": {"text": "Die Staats-Schriften erkl\u00e4ren/\nwas den 20. Decembr. 1740. von Jhro\nMaj. der K\u00f6niginn auf diejenigen Ein-\n\nw\u00fcrffe/ welche Jhro nunmehro glor-\nreich-regierende Kays Maj. Allerh\u00f6chst\nDero Ehgemahl von dem Kayser-Thron\ndamahls noch entfernt hielten/ sey ge-\nantwortet worden.", "tokens": ["Die", "Staats\u00b7Schrif\u00b7ten", "er\u00b7kl\u00e4\u00b7ren", "/", "was", "den", "20.", "De\u00b7cembr", ".", "1740", ".", "von", "Ih\u00b7ro", "Maj", ".", "der", "K\u00f6\u00b7ni\u00b7ginn", "auf", "die\u00b7je\u00b7ni\u00b7gen", "Ein", "w\u00fcrf\u00b7fe", "/", "wel\u00b7che", "Ih\u00b7ro", "nun\u00b7meh\u00b7ro", "glor", "reich\u00b7re\u00b7gie\u00b7ren\u00b7de", "Kays", "Maj", ".", "Al\u00b7ler\u00b7h\u00f6chst", "De\u00b7ro", "Eh\u00b7ge\u00b7mahl", "von", "dem", "Kay\u00b7ser\u00b7Thron", "da\u00b7mahls", "noch", "ent\u00b7fernt", "hiel\u00b7ten", "/", "sey", "ge", "ant\u00b7wor\u00b7tet", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "ordinal", "word", "punct", "number", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$(", "PWS", "ART", "ADJA", "NN", "$.", "CARD", "$.", "APPR", "PPOSAT", "NN", "$.", "ART", "NN", "APPR", "PDS", "TRUNC", "VVFIN", "$(", "PRELS", "PPER", "ADV", "TRUNC", "ADJA", "NE", "NE", "$.", "ADV", "PDS", "NN", "APPR", "ART", "NN", "ADV", "ADV", "ADJD", "VVFIN", "$(", "VAFIN", "TRUNC", "VVFIN", "VAPP", "$."], "meter": "--+--+-+--+-+-+-+-+--+--+--+-+--+-+--+--+---+-+--+--+-+-+--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.23": {"text": "Was kan ein Sayten-Spiel bey dem Trompeten Schall?", "tokens": ["Was", "kan", "ein", "Say\u00b7ten\u00b7Spiel", "bey", "dem", "Trom\u00b7pe\u00b7ten", "Schall", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "30Ein schwaches Leyr-Geth\u00f6n bey dem Carthaunen-Knall?", "tokens": ["schwa\u00b7ches", "Ley\u00b7rGet\u00b7h\u00f6n", "bey", "dem", "Cart\u00b7hau\u00b7nen\u00b7Knall", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+--++-+", "measure": "trochaic.hexa.relaxed"}}}}}