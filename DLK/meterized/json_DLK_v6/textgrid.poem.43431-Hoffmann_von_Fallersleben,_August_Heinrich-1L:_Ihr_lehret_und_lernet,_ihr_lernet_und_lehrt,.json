{"textgrid.poem.43431": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr lehret und lernet, ihr lernet und lehrt,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr lehret und lernet, ihr lernet und lehrt,", "tokens": ["Ihr", "leh\u00b7ret", "und", "ler\u00b7net", ",", "ihr", "ler\u00b7net", "und", "lehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "So wird denn das Wissen", "tokens": ["So", "wird", "denn", "das", "Wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Von Tage zu Tage gewaltig gemehrt.", "tokens": ["Von", "Ta\u00b7ge", "zu", "Ta\u00b7ge", "ge\u00b7wal\u00b7tig", "ge\u00b7mehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Was n\u00fctzet uns eure Gelehrsamkeit?", "tokens": ["Was", "n\u00fct\u00b7zet", "uns", "eu\u00b7re", "Ge\u00b7lehr\u00b7sam\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wol wisset ihr Vieles \u2013", "tokens": ["Wol", "wis\u00b7set", "ihr", "Vie\u00b7les", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Was wi\u00dft ihr f\u00fcr uns und f\u00fcr unsere Zeit?", "tokens": ["Was", "wi\u00dft", "ihr", "f\u00fcr", "uns", "und", "f\u00fcr", "un\u00b7se\u00b7re", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "PPER", "KON", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Ihr habt euch zu Knechten der Vorwelt gemacht;", "tokens": ["Ihr", "habt", "euch", "zu", "Knech\u00b7ten", "der", "Vor\u00b7welt", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Studieret und gr\u00fcbelt,", "tokens": ["Stu\u00b7die\u00b7ret", "und", "gr\u00fc\u00b7belt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Was Plato, was Cato sich haben gedacht.", "tokens": ["Was", "Pla\u00b7to", ",", "was", "Ca\u00b7to", "sich", "ha\u00b7ben", "ge\u00b7dacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "$,", "PRELS", "NE", "PRF", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.4": {"line.1": {"text": "Ihr seid an Gedanken und Kenntnissen reich,", "tokens": ["Ihr", "seid", "an", "Ge\u00b7dan\u00b7ken", "und", "Kennt\u00b7nis\u00b7sen", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Doch fehlt euch die Liebe", "tokens": ["Doch", "fehlt", "euch", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "In euerem Dichten und Trachten zugleich.", "tokens": ["In", "eu\u00b7e\u00b7rem", "Dich\u00b7ten", "und", "Trach\u00b7ten", "zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Die Liebe hat immer ein Vaterland:", "tokens": ["Die", "Lie\u00b7be", "hat", "im\u00b7mer", "ein", "Va\u00b7ter\u00b7land", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Germania ist euch", "tokens": ["Ger\u00b7ma\u00b7nia", "ist", "euch"], "token_info": ["word", "word", "word"], "pos": ["NE", "VAFIN", "PPER"], "meter": "-+-++", "measure": "unknown.measure.tri"}, "line.3": {"text": "Allein aus dem Tacitus etwas bekannt.", "tokens": ["Al\u00b7lein", "aus", "dem", "Ta\u00b7ci\u00b7tus", "et\u00b7was", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NE", "ADV", "ADJD", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}}}}}