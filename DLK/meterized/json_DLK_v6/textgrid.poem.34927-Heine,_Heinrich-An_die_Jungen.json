{"textgrid.poem.34927": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An die Jungen", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df dich nicht kirren, la\u00df dich nicht wirren", "tokens": ["La\u00df", "dich", "nicht", "kir\u00b7ren", ",", "la\u00df", "dich", "nicht", "wir\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKNEG", "VVINF", "$,", "VVIMP", "PPER", "PTKNEG", "VVINF"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Durch goldne \u00c4pfel in deinem Lauf!", "tokens": ["Durch", "gold\u00b7ne", "\u00c4p\u00b7fel", "in", "dei\u00b7nem", "Lauf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Schwerter klirren, die Pfeile schwirren,", "tokens": ["Die", "Schwer\u00b7ter", "klir\u00b7ren", ",", "die", "Pfei\u00b7le", "schwir\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch halten sie nicht den Helden auf.", "tokens": ["Doch", "hal\u00b7ten", "sie", "nicht", "den", "Hel\u00b7den", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein k\u00fchnes Beginnen ist halbes Gewinnen,", "tokens": ["Ein", "k\u00fch\u00b7nes", "Be\u00b7gin\u00b7nen", "ist", "hal\u00b7bes", "Ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ein Alexander erbeutet die Welt!", "tokens": ["Ein", "A\u00b7lex\u00b7an\u00b7der", "er\u00b7beu\u00b7tet", "die", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Kein langes Besinnen! Die K\u00f6niginnen", "tokens": ["Kein", "lan\u00b7ges", "Be\u00b7sin\u00b7nen", "!", "Die", "K\u00f6\u00b7ni\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$.", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Erwarten schon kniend den Sieger im Zelt.", "tokens": ["Er\u00b7war\u00b7ten", "schon", "kni\u00b7end", "den", "Sie\u00b7ger", "im", "Zelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Wir wagen, wir werben! besteigen als Erben", "tokens": ["Wir", "wa\u00b7gen", ",", "wir", "wer\u00b7ben", "!", "be\u00b7stei\u00b7gen", "als", "Er\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVINF", "$.", "VVFIN", "KOUS", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Des alten Darius Bett und Thron.", "tokens": ["Des", "al\u00b7ten", "Da\u00b7ri\u00b7us", "Bett", "und", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "O s\u00fc\u00dfes Verderben! o bl\u00fchendes Sterben!", "tokens": ["O", "s\u00fc\u00b7\u00dfes", "Ver\u00b7der\u00b7ben", "!", "o", "bl\u00fc\u00b7hen\u00b7des", "Ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "FM", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Berauschter Triumphtod zu Babylon!", "tokens": ["Be\u00b7rauschter", "Tri\u00b7umph\u00b7tod", "zu", "Ba\u00b7by\u00b7lon", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}