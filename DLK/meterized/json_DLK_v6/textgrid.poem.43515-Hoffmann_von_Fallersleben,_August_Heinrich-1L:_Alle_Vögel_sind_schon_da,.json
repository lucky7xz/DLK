{"textgrid.poem.43515": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Alle V\u00f6gel sind schon da,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.85", "it:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alle V\u00f6gel sind schon da,", "tokens": ["Al\u00b7le", "V\u00f6\u00b7gel", "sind", "schon", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle V\u00f6gel, alle!", "tokens": ["Al\u00b7le", "V\u00f6\u00b7gel", ",", "al\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIS", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Welch ein Singen, Musicir'n,", "tokens": ["Welch", "ein", "Sin\u00b7gen", ",", "Mu\u00b7si\u00b7cir'n", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "ART", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Pfeifen, Zwitschern, Tirelir'n,", "tokens": ["Pfei\u00b7fen", ",", "Zwit\u00b7schern", ",", "Ti\u00b7re\u00b7lir'n", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fr\u00fchling will nun einmarschir'n,", "tokens": ["Fr\u00fch\u00b7ling", "will", "nun", "ein\u00b7mar\u00b7schir'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Kommt mit Sang und Schalle.", "tokens": ["Kommt", "mit", "Sang", "und", "Schal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wie sie alle lustig sind,", "tokens": ["Wie", "sie", "al\u00b7le", "lus\u00b7tig", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Flink und froh sich regen!", "tokens": ["Flink", "und", "froh", "sich", "re\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "PRF", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Amsel, Dro\u00dfel, Fink und Staar,", "tokens": ["Am\u00b7sel", ",", "Dro\u00b7\u00dfel", ",", "Fink", "und", "Staar", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die ganze Vogelschaar", "tokens": ["Und", "die", "gan\u00b7ze", "Vo\u00b7gel\u00b7schaar"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "W\u00fcnschet uns ein frohes Jahr,", "tokens": ["W\u00fcn\u00b7schet", "uns", "ein", "fro\u00b7hes", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Lauter Heil und Segen.", "tokens": ["Lau\u00b7ter", "Heil", "und", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Was sie uns verk\u00fcndet nun,", "tokens": ["Was", "sie", "uns", "ver\u00b7k\u00fcn\u00b7det", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nehmen wir zu Herzen:", "tokens": ["Neh\u00b7men", "wir", "zu", "Her\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wir auch wollen lustig sein,", "tokens": ["Wir", "auch", "wol\u00b7len", "lus\u00b7tig", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lustig wie die V\u00f6gelein,", "tokens": ["Lus\u00b7tig", "wie", "die", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hier und dort, feldaus, feldein", "tokens": ["Hier", "und", "dort", ",", "fel\u00b7daus", ",", "feld\u00b7ein"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "KON", "ADV", "$,", "PTKVZ", "$,", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Singen, springen, scherzen!", "tokens": ["Sin\u00b7gen", ",", "sprin\u00b7gen", ",", "scher\u00b7zen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}