{"textgrid.poem.45204": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[studenten, die nicht studieren] [1]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Studenten, die nicht studieren,", "tokens": ["Stu\u00b7den\u00b7ten", ",", "die", "nicht", "stu\u00b7die\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sind eben keine Studenten,", "tokens": ["Sind", "e\u00b7ben", "kei\u00b7ne", "Stu\u00b7den\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und wenn sie denn auch studieren,", "tokens": ["Und", "wenn", "sie", "denn", "auch", "stu\u00b7die\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So sind sie doch nur Studenten.", "tokens": ["So", "sind", "sie", "doch", "nur", "Stu\u00b7den\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Uns aber nicht nach Studenten,", "tokens": ["Uns", "a\u00b7ber", "nicht", "nach", "Stu\u00b7den\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Uns t\u00e4t es not nach Studierten,", "tokens": ["Uns", "t\u00e4t", "es", "not", "nach", "Stu\u00b7dier\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Was freilich, wenn sie studierten,", "tokens": ["Was", "frei\u00b7lich", ",", "wenn", "sie", "stu\u00b7dier\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Wohl w\u00fcrden dereinst die Studenten.", "tokens": ["Wohl", "w\u00fcr\u00b7den", "de\u00b7reinst", "die", "Stu\u00b7den\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}}}}}