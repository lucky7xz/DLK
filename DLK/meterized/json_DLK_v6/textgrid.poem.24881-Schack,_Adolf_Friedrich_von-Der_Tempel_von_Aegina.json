{"textgrid.poem.24881": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Tempel von Aegina", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Halbauf noch ragt mit seinem Ruhm", "tokens": ["Hal\u00b7bauf", "noch", "ragt", "mit", "sei\u00b7nem", "Ruhm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Der Wunderbau der Aegineten,", "tokens": ["Der", "Wun\u00b7der\u00b7bau", "der", "A\u00b7e\u00b7gi\u00b7ne\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch \u00f6de steht sein Heiligtum,", "tokens": ["Doch", "\u00f6\u00b7de", "steht", "sein", "Hei\u00b7lig\u00b7tum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verwaist von Opfern und Gebeten;", "tokens": ["Ver\u00b7waist", "von", "Op\u00b7fern", "und", "Ge\u00b7be\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zerbr\u00f6ckelnd in den Archipel", "tokens": ["Zer\u00b7br\u00f6\u00b7ckelnd", "in", "den", "Ar\u00b7chi\u00b7pel"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sinkt das Gestein vom Felsenhange,", "tokens": ["Sinkt", "das", "Ge\u00b7stein", "vom", "Fel\u00b7sen\u00b7han\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Um S\u00e4ulensturz und Kapit\u00e4l", "tokens": ["Um", "S\u00e4u\u00b7len\u00b7sturz", "und", "Ka\u00b7pi\u00b7t\u00e4l"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In Ringeln windet sich die Schlange.", "tokens": ["In", "Rin\u00b7geln", "win\u00b7det", "sich", "die", "Schlan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nur wenn beim Sternenschein der Nacht", "tokens": ["Nur", "wenn", "beim", "Ster\u00b7nen\u00b7schein", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Fels zu Fels die Schatten wallen,", "tokens": ["Von", "Fels", "zu", "Fels", "die", "Schat\u00b7ten", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NE", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erhebt in alter Dorerpracht", "tokens": ["Er\u00b7hebt", "in", "al\u00b7ter", "Do\u00b7rer\u00b7pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der hehre Tempel seine Hallen,", "tokens": ["Der", "heh\u00b7re", "Tem\u00b7pel", "sei\u00b7ne", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und durch die S\u00e4uleng\u00e4nge hin,", "tokens": ["Und", "durch", "die", "S\u00e4u\u00b7len\u00b7g\u00e4n\u00b7ge", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den goldnen Kranz im Lockenhaare,", "tokens": ["Den", "gold\u00b7nen", "Kranz", "im", "Lo\u00b7cken\u00b7haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Tritt feierlich die Priesterin", "tokens": ["Tritt", "fei\u00b7er\u00b7lich", "die", "Pries\u00b7te\u00b7rin"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Im wei\u00dfen Lichtkleid zum Altare", "tokens": ["Im", "wei\u00b7\u00dfen", "Licht\u00b7kleid", "zum", "Al\u00b7ta\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da ist's, als ob am Himmelssaum", "tokens": ["Da", "ist's", ",", "als", "ob", "am", "Him\u00b7mels\u00b7saum"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "KOKOM", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des G\u00f6ttervaters Donner rolle", "tokens": ["Des", "G\u00f6t\u00b7ter\u00b7va\u00b7ters", "Don\u00b7ner", "rol\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und aus jahrtausendlangem Traum", "tokens": ["Und", "aus", "jahr\u00b7tau\u00b7send\u00b7lan\u00b7gem", "Traum"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die alte Welt erwachen wolle;", "tokens": ["Die", "al\u00b7te", "Welt", "er\u00b7wa\u00b7chen", "wol\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Als ob die Mutter Cybele", "tokens": ["Als", "ob", "die", "Mut\u00b7ter", "Cy\u00b7be\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "NN", "NE"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "All ihre Kinder wieder wecke", "tokens": ["All", "ih\u00b7re", "Kin\u00b7der", "wie\u00b7der", "we\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und sehnsuchtsvoll in s\u00fc\u00dfem Weh", "tokens": ["Und", "sehn\u00b7suchts\u00b7voll", "in", "s\u00fc\u00b7\u00dfem", "Weh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Arme nach der Erde strecke.", "tokens": ["Die", "Ar\u00b7me", "nach", "der", "Er\u00b7de", "stre\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und horch! Ein Regen auf der Flur,", "tokens": ["Und", "horch", "!", "Ein", "Re\u00b7gen", "auf", "der", "Flur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Rauschen um die Uferklippen!", "tokens": ["Ein", "Rau\u00b7schen", "um", "die", "Uf\u00b7er\u00b7klip\u00b7pen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Noch einmal \u00f6ffnet die Natur", "tokens": ["Noch", "ein\u00b7mal", "\u00f6ff\u00b7net", "die", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aufjubelnd ihre bleichen Lippen;", "tokens": ["Auf\u00b7ju\u00b7belnd", "ih\u00b7re", "blei\u00b7chen", "Lip\u00b7pen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In k\u00fchler Grotten D\u00e4mmerglanz", "tokens": ["In", "k\u00fch\u00b7ler", "Grot\u00b7ten", "D\u00e4m\u00b7mer\u00b7glanz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und an den hallenden Gestaden", "tokens": ["Und", "an", "den", "hal\u00b7len\u00b7den", "Ge\u00b7sta\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Schlingt sich der Nymphen Reigentanz;", "tokens": ["Schlingt", "sich", "der", "Nym\u00b7phen", "Rei\u00b7gen\u00b7tanz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Im Walde fl\u00fcstern die Dryaden.", "tokens": ["Im", "Wal\u00b7de", "fl\u00fcs\u00b7tern", "die", "Dry\u00b7a\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wie Ges\u00e4nge des Homer", "tokens": ["Und", "wie", "Ge\u00b7s\u00e4n\u00b7ge", "des", "Ho\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "NN", "ART", "NE"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "T\u00f6nt es durch das Geroll der Wogen;", "tokens": ["T\u00f6nt", "es", "durch", "das", "Ge\u00b7roll", "der", "Wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Auf silbernem Gew\u00f6lk daher", "tokens": ["Auf", "sil\u00b7ber\u00b7nem", "Ge\u00b7w\u00f6lk", "da\u00b7her"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kommt leuchtend Artemis gezogen;", "tokens": ["Kommt", "leuch\u00b7tend", "Ar\u00b7te\u00b7mis", "ge\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NE", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Anbetend gie\u00dft die Priesterin", "tokens": ["An\u00b7be\u00b7tend", "gie\u00dft", "die", "Pries\u00b7te\u00b7rin"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Das Opfer aus der Weiheschale \u2013", "tokens": ["Das", "Op\u00b7fer", "aus", "der", "Wei\u00b7he\u00b7scha\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Doch neu in Schweigen und Ruin", "tokens": ["Doch", "neu", "in", "Schwei\u00b7gen", "und", "Ru\u00b7in"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Sinkt alles hin beim Morgenstrahle.", "tokens": ["Sinkt", "al\u00b7les", "hin", "beim", "Mor\u00b7gen\u00b7strah\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}