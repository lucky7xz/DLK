{"textgrid.poem.46222": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Von Ernsten Grafen von Mansfeld", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil der drach, adler, l\u00f6w mit schweinen, w\u00f6lfen, b\u00e4ren", "tokens": ["Weil", "der", "drach", ",", "ad\u00b7ler", ",", "l\u00f6w", "mit", "schwei\u00b7nen", ",", "w\u00f6l\u00b7fen", ",", "b\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KOUS", "ART", "PAV", "$,", "NE", "$,", "VVFIN", "APPR", "ADJA", "$,", "VVFIN", "$,", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "nach ihrem rat und lust schier gar ohn widerstand", "tokens": ["nach", "ih\u00b7rem", "rat", "und", "lust", "schier", "gar", "ohn", "wi\u00b7der\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "VVFIN", "KON", "VVFIN", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "(so gro\u00df ist gottes zorn) das teutsche reich verst\u00f6ren", "tokens": ["(", "so", "gro\u00df", "ist", "got\u00b7tes", "zorn", ")", "das", "teut\u00b7sche", "reich", "ver\u00b7st\u00f6\u00b7ren"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "ADJD", "VAFIN", "ADJA", "NN", "$(", "ART", "ADJA", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und es begraben ganz aus gro\u00dfem grim in schand:", "tokens": ["und", "es", "be\u00b7gra\u00b7ben", "ganz", "aus", "gro\u00b7\u00dfem", "grim", "in", "schand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "ADV", "APPR", "ADJA", "NN", "APPR", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein Man schier gar allein des reichs freiheit zu ehren", "tokens": ["Ein", "Man", "schier", "gar", "al\u00b7lein", "des", "reichs", "frei\u00b7heit", "zu", "eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "ADJD", "ADV", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "(o ruhm f\u00fcr solchen man! o schand f\u00fcr solches land!)", "tokens": ["(", "o", "ruhm", "f\u00fcr", "sol\u00b7chen", "man", "!", "o", "schand", "f\u00fcr", "sol\u00b7ches", "land", "!", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "NN", "APPR", "PIAT", "PIS", "$.", "FM", "VVFIN", "APPR", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "verhindert, da\u00df sie nicht erf\u00fcllen ihr begehren,", "tokens": ["ver\u00b7hin\u00b7dert", ",", "da\u00df", "sie", "nicht", "er\u00b7f\u00fcl\u00b7len", "ihr", "be\u00b7geh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "mit stets forchtlosem mut und stets sigreicher hand;", "tokens": ["mit", "stets", "forcht\u00b7lo\u00b7sem", "mut", "und", "stets", "sig\u00b7rei\u00b7cher", "hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "KON", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Darum weil theils hilflos und theils sorglos anschauen", "tokens": ["Da\u00b7rum", "weil", "theils", "hil\u00b7flos", "und", "theils", "sorg\u00b7los", "an\u00b7schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "KOUS", "ADV", "ADJD", "KON", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "wie solche wilde thier zu fressen alle welt", "tokens": ["wie", "sol\u00b7che", "wil\u00b7de", "thier", "zu", "fres\u00b7sen", "al\u00b7le", "welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "PTKZU", "VVINF", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "nun wetzen w\u00fctiglich die h\u00f6rner, z\u00e4hn und klauen;", "tokens": ["nun", "wet\u00b7zen", "w\u00fc\u00b7tig\u00b7lich", "die", "h\u00f6r\u00b7ner", ",", "z\u00e4hn", "und", "klau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "ADJA", "$,", "VVFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und wie sie dan mit macht, dan mit list, gift und geld", "tokens": ["Und", "wie", "sie", "dan", "mit", "macht", ",", "dan", "mit", "list", ",", "gift", "und", "geld"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "APPR", "VVFIN", "$,", "ADV", "APPR", "NN", "$,", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "des lands b\u00e4um, blumen, gras verderben und abhauen,", "tokens": ["des", "lands", "b\u00e4um", ",", "blu\u00b7men", ",", "gras", "ver\u00b7der\u00b7ben", "und", "ab\u00b7hau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "VVFIN", "$,", "NN", "VVFIN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "so bl\u00fchet doch noch stets des dapfersten Mans feld.", "tokens": ["so", "bl\u00fc\u00b7het", "doch", "noch", "stets", "des", "dap\u00b7fers\u00b7ten", "Mans", "feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-++-+-", "measure": "unknown.measure.hexa"}}}}}