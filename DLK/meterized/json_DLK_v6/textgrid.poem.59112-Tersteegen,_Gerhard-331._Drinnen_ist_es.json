{"textgrid.poem.59112": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "331. Drinnen ist es", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die ganze Sch\u00f6pfung ist zu klein;", "tokens": ["Die", "gan\u00b7ze", "Sch\u00f6p\u00b7fung", "ist", "zu", "klein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kehr ein und la\u00df dich nirgends binden!", "tokens": ["Kehr", "ein", "und", "la\u00df", "dich", "nir\u00b7gends", "bin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VVIMP", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Im Grund der Seele ist allein", "tokens": ["Im", "Grund", "der", "See\u00b7le", "ist", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Recht Seligkeit in Gott zu finden.", "tokens": ["Recht", "Se\u00b7lig\u00b7keit", "in", "Gott", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}