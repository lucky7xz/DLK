{"textgrid.poem.42222": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und nun ist es doch gekommen,", "tokens": ["Und", "nun", "ist", "es", "doch", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trotz des stolzen Sinns im K\u00f6pfchen;", "tokens": ["Trotz", "des", "stol\u00b7zen", "Sinns", "im", "K\u00f6pf\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wir haben von dem T\u00f6pfchen", "tokens": ["Und", "wir", "ha\u00b7ben", "von", "dem", "T\u00f6pf\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fchn den Deckel abgenommen.", "tokens": ["K\u00fchn", "den", "De\u00b7ckel", "ab\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schw\u00fcler Paradieses-Brodem", "tokens": ["Schw\u00fc\u00b7ler", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7Bro\u00b7dem"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stieg mir schmeichelnd in die Nase,", "tokens": ["Stieg", "mir", "schmei\u00b7chelnd", "in", "die", "Na\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dennoch bangt ich wie ein Hase", "tokens": ["Den\u00b7noch", "bangt", "ich", "wie", "ein", "Ha\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor dem Pechgeruch von Sodom.", "tokens": ["Vor", "dem", "Pech\u00b7ge\u00b7ruch", "von", "So\u00b7dom", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Zwei von hei\u00dfer Glut erf\u00fcllte,", "tokens": ["Zwei", "von", "hei\u00b7\u00dfer", "Glut", "er\u00b7f\u00fcll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mittern\u00e4chtlich helle Sterne", "tokens": ["Mit\u00b7ter\u00b7n\u00e4cht\u00b7lich", "hel\u00b7le", "Ster\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Blinken tr\u00e4umend in die Ferne,", "tokens": ["Blin\u00b7ken", "tr\u00e4u\u00b7mend", "in", "die", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die sich scheu in Nebel h\u00fcllte.", "tokens": ["Die", "sich", "scheu", "in", "Ne\u00b7bel", "h\u00fcll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}