{"dta.poem.7569": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "68.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Ueberliefrung ist ein umgekehrter Flu\u00df,", "tokens": ["Die", "Ue\u00b7berl\u00b7ief\u00b7rung", "ist", "ein", "um\u00b7ge\u00b7kehr\u00b7ter", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der, wie er weiter flie\u00dft, sich weiter theilen mu\u00df.", "tokens": ["Der", ",", "wie", "er", "wei\u00b7ter", "flie\u00dft", ",", "sich", "wei\u00b7ter", "thei\u00b7len", "mu\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,", "PRF", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht Fl\u00fcsse rinnen hier in einen Strom zusammen,", "tokens": ["Nicht", "Fl\u00fcs\u00b7se", "rin\u00b7nen", "hier", "in", "ei\u00b7nen", "Strom", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die aus viel B\u00e4chen und aus noch mehr Quellen stammen;", "tokens": ["Die", "aus", "viel", "B\u00e4\u00b7chen", "und", "aus", "noch", "mehr", "Quel\u00b7len", "stam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "KON", "APPR", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das Ganze war Ein Quell, der ward ein Doppelbach,", "tokens": ["Das", "Gan\u00b7ze", "war", "Ein", "Quell", ",", "der", "ward", "ein", "Dop\u00b7pel\u00b7bach", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus welchem Fl\u00fcss' und Str\u00f6m' entstanden hundertfach.", "tokens": ["Aus", "wel\u00b7chem", "Fl\u00fcss'", "und", "Str\u00f6m'", "ent\u00b7stan\u00b7den", "hun\u00b7dert\u00b7fach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "KON", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Meister jeder Schul' und seiner Sch\u00fcler Schwarm", "tokens": ["Der", "Meis\u00b7ter", "je\u00b7der", "Schul'", "und", "sei\u00b7ner", "Sch\u00fc\u00b7ler", "Schwarm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "NN", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Rei\u00dft von dem Mittelleib sich los als Seitenarm.", "tokens": ["Rei\u00dft", "von", "dem", "Mit\u00b7tel\u00b7leib", "sich", "los", "als", "Sei\u00b7ten\u00b7arm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PRF", "PTKVZ", "KOKOM", "NN", "$."], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}}, "stanza.5": {"line.1": {"text": "Der Arm wird selbst ein Leib, der sich in Glieder spaltet,", "tokens": ["Der", "Arm", "wird", "selbst", "ein", "Leib", ",", "der", "sich", "in", "Glie\u00b7der", "spal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Stamm, der sich in Ast und Zweig und Blatt entfaltet.", "tokens": ["Ein", "Stamm", ",", "der", "sich", "in", "Ast", "und", "Zweig", "und", "Blatt", "ent\u00b7fal\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "APPR", "NN", "KON", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ein Stromge\u00e4der, das, wie es sich kraus verschlingt,", "tokens": ["Ein", "Strom\u00b7ge\u00e4\u00b7der", ",", "das", ",", "wie", "es", "sich", "kraus", "ver\u00b7schlingt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "$,", "PWAV", "PPER", "PRF", "APZR", "VVFIN", "$,"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nicht kennt den Mittelpunkt, aus dem, zu dem es dringt.", "tokens": ["Nicht", "kennt", "den", "Mit\u00b7tel\u00b7punkt", ",", "aus", "dem", ",", "zu", "dem", "es", "dringt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "NN", "$,", "APPR", "ART", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Das wirre Stromgeflecht, wer schlingt es ein als Meer,", "tokens": ["Das", "wir\u00b7re", "Strom\u00b7ge\u00b7flecht", ",", "wer", "schlingt", "es", "ein", "als", "Meer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWS", "VVFIN", "PPER", "ART", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und stellt im tiefen Sinn des Urquells Einheit her?", "tokens": ["Und", "stellt", "im", "tie\u00b7fen", "Sinn", "des", "Ur\u00b7quells", "Ein\u00b7heit", "her", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Denn wie's vom Herzen kommt, zum Herzen ist sein Streben;", "tokens": ["Denn", "wie's", "vom", "Her\u00b7zen", "kommt", ",", "zum", "Her\u00b7zen", "ist", "sein", "Stre\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "VVFIN", "$,", "APPRART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und da\u00df der Blutlauf kreist, das ist des Leibes Leben.", "tokens": ["Und", "da\u00df", "der", "Blut\u00b7lauf", "kreist", ",", "das", "ist", "des", "Lei\u00b7bes", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}