{"dta.poem.16121": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "175.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hier auf der Tafel, Sohn, liegt manche Pomeranze,", "tokens": ["Hier", "auf", "der", "Ta\u00b7fel", ",", "Sohn", ",", "liegt", "man\u00b7che", "Po\u00b7me\u00b7ran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "NN", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und eine gleicht davon der anderen an Glanze.", "tokens": ["Und", "ei\u00b7ne", "gleicht", "da\u00b7von", "der", "an\u00b7de\u00b7ren", "an", "Glan\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PAV", "ART", "ADJA", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht t\u00e4usche dich der Glanz! es hat des Himmels Gunst", "tokens": ["Nicht", "t\u00e4u\u00b7sche", "dich", "der", "Glanz", "!", "es", "hat", "des", "Him\u00b7mels", "Gunst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PPER", "ART", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erschaffen einige, doch andere die Kunst.", "tokens": ["Er\u00b7schaf\u00b7fen", "ei\u00b7ni\u00b7ge", ",", "doch", "an\u00b7de\u00b7re", "die", "Kunst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "ADV", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gewachsen, wenn du willst, magst du sie alle nennen,", "tokens": ["Ge\u00b7wach\u00b7sen", ",", "wenn", "du", "willst", ",", "magst", "du", "sie", "al\u00b7le", "nen\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPER", "VMFIN", "$,", "VMFIN", "PPER", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Doch ein'ge sind von Wachs, woran wirst du's erkennen?", "tokens": ["Doch", "ein'\u00b7ge", "sind", "von", "Wachs", ",", "wo\u00b7ran", "wirst", "du's", "er\u00b7ken\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VAFIN", "APPR", "NN", "$,", "PWAV", "VAFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sie haben nebst Gestalt und Farb' auch den Geruch,", "tokens": ["Sie", "ha\u00b7ben", "nebst", "Ge\u00b7stalt", "und", "Fa\u00b7rb'", "auch", "den", "Ge\u00b7ruch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Nur der Geschmack allein fehlt ihnen beim Versuch.", "tokens": ["Nur", "der", "Ge\u00b7schmack", "al\u00b7lein", "fehlt", "ih\u00b7nen", "beim", "Ver\u00b7such", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch auch von denen, die am Baum gewachsen sind,", "tokens": ["Doch", "auch", "von", "de\u00b7nen", ",", "die", "am", "Baum", "ge\u00b7wach\u00b7sen", "sind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PRELS", "$,", "PRELS", "APPRART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind s\u00fc\u00df die wenigsten, die meisten herb, o Kind,", "tokens": ["Sind", "s\u00fc\u00df", "die", "we\u00b7nigs\u00b7ten", ",", "die", "meis\u00b7ten", "herb", ",", "o", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "$,", "FM", "NN", "$,"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}}, "stanza.6": {"line.1": {"text": "Und bitter einige; doch la\u00df dich nicht verdrie\u00dfen", "tokens": ["Und", "bit\u00b7ter", "ei\u00b7ni\u00b7ge", ";", "doch", "la\u00df", "dich", "nicht", "ver\u00b7drie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PIS", "$.", "KON", "VVIMP", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Bi\u00dfchen Bitterkeit, auch sie sind zu genie\u00dfen.", "tokens": ["Das", "Bi\u00df\u00b7chen", "Bit\u00b7ter\u00b7keit", ",", "auch", "sie", "sind", "zu", "ge\u00b7nie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "PPER", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Und halt in Ehren auch die w\u00e4chsernen Gestalten!", "tokens": ["Und", "halt", "in", "Eh\u00b7ren", "auch", "die", "w\u00e4ch\u00b7ser\u00b7nen", "Ge\u00b7stal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie werden, ohne Saft, sich desto l\u00e4nger halten.", "tokens": ["Sie", "wer\u00b7den", ",", "oh\u00b7ne", "Saft", ",", "sich", "des\u00b7to", "l\u00e4n\u00b7ger", "hal\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUI", "NN", "$,", "PRF", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}