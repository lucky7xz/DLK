{"dta.poem.23766": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Das Geschenk.  \n Der Bruder an die Schwester.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Sieh! kann ein Apfel sch\u00f6ner seyn?", "tokens": ["Sieh", "!", "kann", "ein", "Ap\u00b7fel", "sch\u00f6\u00b7ner", "seyn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VMFIN", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ja Schwester, eine Augenweide!", "tokens": ["Ja", "Schwes\u00b7ter", ",", "ei\u00b7ne", "Au\u00b7gen\u00b7wei\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie mu\u00df nicht sein Geschmack erfreun,", "tokens": ["Wie", "mu\u00df", "nicht", "sein", "Ge\u00b7schmack", "er\u00b7freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PTKNEG", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Macht schon sein Anblick solche Freude.", "tokens": ["Macht", "schon", "sein", "An\u00b7blick", "sol\u00b7che", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "PIAT", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Sein lieblicher Geruch, wie hold!", "tokens": ["Sein", "lieb\u00b7li\u00b7cher", "Ge\u00b7ruch", ",", "wie", "hold", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PWAV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In gelben roth durchstreiften Schaalen,", "tokens": ["In", "gel\u00b7ben", "roth", "durch\u00b7streif\u00b7ten", "Schaa\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gl\u00e4nzt ein Rubin gefa\u00dft in Gold:", "tokens": ["Gl\u00e4nzt", "ein", "Ru\u00b7bin", "ge\u00b7fa\u00dft", "in", "Gold", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "VVPP", "APPR", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Kein Maler kann den Apfel malen.", "tokens": ["Kein", "Ma\u00b7ler", "kann", "den", "Ap\u00b7fel", "ma\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du m\u00f6chtest ihn? ich geb ihn dir,", "tokens": ["Du", "m\u00f6ch\u00b7test", "ihn", "?", "ich", "geb", "ihn", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$.", "PPER", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ja, h\u00e4tt ich auch noch sch\u00f6nre Sachen!", "tokens": ["Ja", ",", "h\u00e4tt", "ich", "auch", "noch", "sch\u00f6n\u00b7re", "Sa\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VAFIN", "PPER", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sch\u00f6n ist es, gl\u00fccklich seyn: Doch mir", "tokens": ["Sch\u00f6n", "ist", "es", ",", "gl\u00fcck\u00b7lich", "seyn", ":", "Doch", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "$,", "ADJD", "VAINF", "$.", "KON", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist es weit sch\u00f6ner, gl\u00fccklich machen.", "tokens": ["Ist", "es", "weit", "sch\u00f6\u00b7ner", ",", "gl\u00fcck\u00b7lich", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADJA", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}