{"textgrid.poem.35232": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun heben an zu klagen die H\u00fcgel, That und Feld,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun heben an zu klagen die H\u00fcgel, That und Feld,", "tokens": ["Nun", "he\u00b7ben", "an", "zu", "kla\u00b7gen", "die", "H\u00fc\u00b7gel", ",", "That", "und", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es bringt viel Mi\u00dfbehagen des rauhen Windes K\u00e4lt',", "tokens": ["Es", "bringt", "viel", "Mi\u00df\u00b7be\u00b7ha\u00b7gen", "des", "rau\u00b7hen", "Win\u00b7des", "K\u00e4lt'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Es fallen falbe Bl\u00e4tter", "tokens": ["Es", "fal\u00b7len", "fal\u00b7be", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und schweben in der Luft;", "tokens": ["Und", "schwe\u00b7ben", "in", "der", "Luft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn Schnee und Winterwetter", "tokens": ["Denn", "Schnee", "und", "Win\u00b7ter\u00b7wet\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Nordenst\u00fcrmer ruft.", "tokens": ["Der", "Nor\u00b7den\u00b7st\u00fcr\u00b7mer", "ruft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die reifen Fr\u00fcchte fallen, wenn man sie nicht nimmt ab,", "tokens": ["Die", "rei\u00b7fen", "Fr\u00fcch\u00b7te", "fal\u00b7len", ",", "wenn", "man", "sie", "nicht", "nimmt", "ab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,", "KOUS", "PIS", "PPER", "PTKNEG", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die alten Menschen wallen hin zu dem alten Grab.", "tokens": ["Die", "al\u00b7ten", "Men\u00b7schen", "wal\u00b7len", "hin", "zu", "dem", "al\u00b7ten", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Das, was hat zugenommen", "tokens": ["Das", ",", "was", "hat", "zu\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PDS", "$,", "PWS", "VAFIN", "VVPP"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Bis auf gewisse Zeit,", "tokens": ["Bis", "auf", "ge\u00b7wis\u00b7se", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mu\u00df zu dem Ende kommen", "tokens": ["Mu\u00df", "zu", "dem", "En\u00b7de", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In dieser Eitelkeit.", "tokens": ["In", "die\u00b7ser", "Ei\u00b7tel\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wann wir die Aexte sehen den B\u00e4umen angesetzt,", "tokens": ["Wann", "wir", "die", "A\u00b7ex\u00b7te", "se\u00b7hen", "den", "B\u00e4u\u00b7men", "an\u00b7ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "So ist es bald geschehen, da\u00df er, dadurch verletzt,", "tokens": ["So", "ist", "es", "bald", "ge\u00b7sche\u00b7hen", ",", "da\u00df", "er", ",", "da\u00b7durch", "ver\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,", "KOUS", "PPER", "$,", "PAV", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Zu der entf\u00e4rbten Erden", "tokens": ["Zu", "der", "ent\u00b7f\u00e4rb\u00b7ten", "Er\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich neigend bricht und kracht,", "tokens": ["Sich", "nei\u00b7gend", "bricht", "und", "kracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und mu\u00df er endlich werden", "tokens": ["Und", "mu\u00df", "er", "end\u00b7lich", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dem Feuer zugebracht.", "tokens": ["Dem", "Feu\u00b7er", "zu\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So m\u00fcssen auch die alle, so sind ohn' gute Frucht,", "tokens": ["So", "m\u00fcs\u00b7sen", "auch", "die", "al\u00b7le", ",", "so", "sind", "ohn'", "gu\u00b7te", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ART", "PIS", "$,", "ADV", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sich f\u00fcrchten vor dem Falle, das ist die Menschensucht.", "tokens": ["Sich", "f\u00fcrch\u00b7ten", "vor", "dem", "Fal\u00b7le", ",", "das", "ist", "die", "Men\u00b7schen\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN", "$,", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und wie der Baum gef\u00e4llet,", "tokens": ["Und", "wie", "der", "Baum", "ge\u00b7f\u00e4l\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So liegt er fort und fort;", "tokens": ["So", "liegt", "er", "fort", "und", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der B\u00f6se wird gestellet", "tokens": ["Der", "B\u00f6\u00b7se", "wird", "ge\u00b7stel\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dort in den Jammerort.", "tokens": ["Dort", "in", "den", "Jam\u00b7mer\u00b7ort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So lasset uns bedenken bei dieser Herbsteszeit,", "tokens": ["So", "las\u00b7set", "uns", "be\u00b7den\u00b7ken", "bei", "die\u00b7ser", "Herbs\u00b7tes\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVINF", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wie alle Ding' erkranken und zu dem Tod bereit.", "tokens": ["Wie", "al\u00b7le", "Ding'", "er\u00b7kran\u00b7ken", "und", "zu", "dem", "Tod", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVINF", "KON", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da\u00df wir noch l\u00e4nger leben,", "tokens": ["Da\u00df", "wir", "noch", "l\u00e4n\u00b7ger", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df Alles nicht ist aus,", "tokens": ["Da\u00df", "Al\u00b7les", "nicht", "ist", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hat Gottes Gnad' gegeben", "tokens": ["Hat", "Got\u00b7tes", "Gnad'", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "NN", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hier in dem Erdenhaus.", "tokens": ["Hier", "in", "dem", "Er\u00b7den\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}