{"textgrid.poem.30351": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "65.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Longus ist der andre Bias; was er bey und an sich tr\u00e4get,", "tokens": ["Lon\u00b7gus", "ist", "der", "and\u00b7re", "Bias", ";", "was", "er", "bey", "und", "an", "sich", "tr\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$.", "PWS", "PPER", "APPR", "KON", "APPR", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Dieses ists, das ihn ern\u00e4hret und in weiche Bette leget.", "tokens": ["Die\u00b7ses", "ists", ",", "das", "ihn", "er\u00b7n\u00e4h\u00b7ret", "und", "in", "wei\u00b7che", "Bet\u00b7te", "le\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "KON", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}