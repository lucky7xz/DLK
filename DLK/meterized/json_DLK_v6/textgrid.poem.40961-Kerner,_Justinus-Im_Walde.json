{"textgrid.poem.40961": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Im Walde", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es t\u00f6nt der Bach wie klagend", "tokens": ["Es", "t\u00f6nt", "der", "Bach", "wie", "kla\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOKOM", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem Wandersmanne sagend:", "tokens": ["Dem", "Wan\u00b7ders\u00b7man\u00b7ne", "sa\u00b7gend", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In mir auch wohnt ein Leid.", "tokens": ["In", "mir", "auch", "wohnt", "ein", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es rauschen drein die B\u00e4ume,", "tokens": ["Es", "rau\u00b7schen", "drein", "die", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Erz\u00e4hlen ihre Tr\u00e4ume", "tokens": ["Er\u00b7z\u00e4h\u00b7len", "ih\u00b7re", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der gr\u00fcnen Einsamkeit.", "tokens": ["Der", "gr\u00fc\u00b7nen", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Vogel singt in L\u00fcften", "tokens": ["Der", "Vo\u00b7gel", "singt", "in", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sein Leid aus, \u2013 aus in D\u00fcften", "tokens": ["Sein", "Leid", "aus", ",", "\u2013", "aus", "in", "D\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKVZ", "$,", "$(", "APPR", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Str\u00f6mt es die Blum' der Flur.", "tokens": ["Str\u00f6mt", "es", "die", "Blum'", "der", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Und oft ert\u00f6nt's in N\u00e4chten,", "tokens": ["Und", "oft", "er\u00b7t\u00f6nt's", "in", "N\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Als ob uns L\u00fcfte br\u00e4chten", "tokens": ["Als", "ob", "uns", "L\u00fcf\u00b7te", "br\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wehlaute der Natur.", "tokens": ["We\u00b7hlau\u00b7te", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und schweigen sollt' alleine,", "tokens": ["Und", "schwei\u00b7gen", "sollt'", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf da\u00df es fr\u00f6hlich scheine,", "tokens": ["Auf", "da\u00df", "es", "fr\u00f6h\u00b7lich", "schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein volles Menschenherz?", "tokens": ["Ein", "vol\u00b7les", "Men\u00b7schen\u00b7herz", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nicht singen sollt's, nichts sagen,", "tokens": ["Nicht", "sin\u00b7gen", "sollt's", ",", "nichts", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "$,", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Stumm dulden, niemand klagen,", "tokens": ["Stumm", "dul\u00b7den", ",", "nie\u00b7mand", "kla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie es zerrei\u00dft der Schmerz?", "tokens": ["Wie", "es", "zer\u00b7rei\u00dft", "der", "Schmerz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}