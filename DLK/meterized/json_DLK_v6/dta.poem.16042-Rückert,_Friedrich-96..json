{"dta.poem.16042": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "96.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sie sagen dir, nichts sei wie Eigenlob zu hassen:", "tokens": ["Sie", "sa\u00b7gen", "dir", ",", "nichts", "sei", "wie", "Ei\u00b7gen\u00b7lob", "zu", "has\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PIS", "VAFIN", "KOKOM", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Uns sollst du loben, und von uns dich loben lassen!", "tokens": ["Uns", "sollst", "du", "lo\u00b7ben", ",", "und", "von", "uns", "dich", "lo\u00b7ben", "las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,", "KON", "APPR", "PPER", "PRF", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch wenn du sie nun lobst, da\u00df sie dich wieder loben,", "tokens": ["Doch", "wenn", "du", "sie", "nun", "lobst", ",", "da\u00df", "sie", "dich", "wie\u00b7der", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,", "KOUS", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sie dich preisen, um von dir zu seyn erhoben;", "tokens": ["Und", "sie", "dich", "prei\u00b7sen", ",", "um", "von", "dir", "zu", "seyn", "er\u00b7ho\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "VVFIN", "$,", "KOUI", "APPR", "PPER", "PTKZU", "VAINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ist dieser Eigenruhm, weil er umst\u00e4ndlicher", "tokens": ["Ist", "die\u00b7ser", "Ei\u00b7gen\u00b7ruhm", ",", "weil", "er", "um\u00b7st\u00e4nd\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PDAT", "NN", "$,", "KOUS", "PPER", "ADJA"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Geworden ist, darum ein minder sch\u00e4ndlicher?", "tokens": ["Ge\u00b7wor\u00b7den", "ist", ",", "da\u00b7rum", "ein", "min\u00b7der", "sch\u00e4nd\u00b7li\u00b7cher", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PAV", "ART", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}, "stanza.4": {"line.1": {"text": "Ihr habet nur das Amt einander zugeschoben,", "tokens": ["Ihr", "ha\u00b7bet", "nur", "das", "Amt", "ein\u00b7an\u00b7der", "zu\u00b7ge\u00b7scho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Einer den andern, statt jeder sich selbst, zu loben.", "tokens": ["Ei\u00b7ner", "den", "an\u00b7dern", ",", "statt", "je\u00b7der", "sich", "selbst", ",", "zu", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "$,", "KOUI", "PIAT", "PRF", "ADV", "$,", "PTKZU", "VVINF", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}}}}}