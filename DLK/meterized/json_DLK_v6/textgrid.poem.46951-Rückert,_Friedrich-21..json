{"textgrid.poem.46951": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft, Himmel, t\u00f6nen eure Morgensterne,", "tokens": ["La\u00dft", ",", "Him\u00b7mel", ",", "t\u00f6\u00b7nen", "eu\u00b7re", "Mor\u00b7gens\u00b7ter\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Thu deinen Mund auf, Erd', und juble Lieder,", "tokens": ["Thu", "dei\u00b7nen", "Mund", "auf", ",", "Erd'", ",", "und", "jub\u00b7le", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "NN", "$,", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df es erschalle bis zum Abgrund nieder", "tokens": ["Da\u00df", "es", "er\u00b7schal\u00b7le", "bis", "zum", "Ab\u00b7grund", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ihn erzittern mach' in seinem Kerne;", "tokens": ["Und", "ihn", "er\u00b7zit\u00b7tern", "mach'", "in", "sei\u00b7nem", "Ker\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df er des gro\u00dfen Siegs Bedeutung lerne,", "tokens": ["Da\u00df", "er", "des", "gro\u00b7\u00dfen", "Siegs", "Be\u00b7deu\u00b7tung", "ler\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie Gottes Kraft der nachtentstammten Hyder", "tokens": ["Wie", "Got\u00b7tes", "Kraft", "der", "nach\u00b7tent\u00b7stamm\u00b7ten", "Hy\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Durch diesen Schlag zerschmettert hat die Glieder,", "tokens": ["Durch", "die\u00b7sen", "Schlag", "zer\u00b7schmet\u00b7tert", "hat", "die", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und f\u00fcr ihr Haupt ist auch der Schlag nicht ferne.", "tokens": ["Und", "f\u00fcr", "ihr", "Haupt", "ist", "auch", "der", "Schlag", "nicht", "fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VAFIN", "ADV", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ihr Engel singt's, da\u00df es der Himmel wisse!", "tokens": ["Ihr", "En\u00b7gel", "singt's", ",", "da\u00df", "es", "der", "Him\u00b7mel", "wis\u00b7se", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wie Nacht und Tag im Anfang einst gerungen,", "tokens": ["Wie", "Nacht", "und", "Tag", "im", "An\u00b7fang", "einst", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "So rangen heute Licht und Finsternisse.", "tokens": ["So", "ran\u00b7gen", "heu\u00b7te", "Licht", "und", "Fins\u00b7ter\u00b7nis\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "H\u00f6r's, Himmel, da\u00df den Sieg das Licht errungen", "tokens": ["H\u00f6r's", ",", "Him\u00b7mel", ",", "da\u00df", "den", "Sieg", "das", "Licht", "er\u00b7run\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "KOUS", "ART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und da\u00df die Erde nicht die Kunde misse,", "tokens": ["Und", "da\u00df", "die", "Er\u00b7de", "nicht", "die", "Kun\u00b7de", "mis\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Sag's ein Tedeum ihr in tausend Zungen.", "tokens": ["Sag's", "ein", "Te\u00b7de\u00b7um", "ihr", "in", "tau\u00b7send", "Zun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PPER", "APPR", "CARD", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}}}}