{"textgrid.poem.58388": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "426. Der Schatz in irdenen Gef\u00e4\u00dfen", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verachtung, Armut, Schwachheit, Leiden", "tokens": ["Ver\u00b7ach\u00b7tung", ",", "Ar\u00b7mut", ",", "Schwach\u00b7heit", ",", "Lei\u00b7den"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bedecken hier den Glanz von Jesu Braut;", "tokens": ["Be\u00b7de\u00b7cken", "hier", "den", "Glanz", "von", "Je\u00b7su", "Braut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch wird sich bald die Schal' vom Kern abscheiden,", "tokens": ["Doch", "wird", "sich", "bald", "die", "Schal'", "vom", "Kern", "ab\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "ADV", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So wird sie dann in Herrlichkeit geschaut.", "tokens": ["So", "wird", "sie", "dann", "in", "Herr\u00b7lich\u00b7keit", "ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Des Glaubens Aug' schaut in die Ewigkeit", "tokens": ["Des", "Glau\u00b7bens", "Aug'", "schaut", "in", "die", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und liebt das Kreuz, das nur die Schlacken scheid't.", "tokens": ["Und", "liebt", "das", "Kreuz", ",", "das", "nur", "die", "Schla\u00b7cken", "scheid'", "t."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}