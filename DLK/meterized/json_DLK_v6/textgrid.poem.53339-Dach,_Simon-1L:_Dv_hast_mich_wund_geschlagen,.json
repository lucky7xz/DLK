{"textgrid.poem.53339": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dv hast mich wund geschlagen,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dv hast mich wund geschlagen,", "tokens": ["Dv", "hast", "mich", "wund", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich, Herr, f\u00fcr Feind erkandt,", "tokens": ["Mich", ",", "Herr", ",", "f\u00fcr", "Feind", "er\u00b7kandt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was soll ich weiter sagen,", "tokens": ["Was", "soll", "ich", "wei\u00b7ter", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich f\u00fchle deine Hand", "tokens": ["Ich", "f\u00fch\u00b7le", "dei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd deines Eyfers Brandt.", "tokens": ["Vnd", "dei\u00b7nes", "Ey\u00b7fers", "Brandt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du k\u00f6mmst auff mich gedrungen,", "tokens": ["Du", "k\u00f6mmst", "auff", "mich", "ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gleich wie ein Schiff zur See", "tokens": ["Gleich", "wie", "ein", "Schiff", "zur", "See"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wird durch den Sturm besprungen,", "tokens": ["Wird", "durch", "den", "Sturm", "be\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie man ein schwaches Reh", "tokens": ["Wie", "man", "ein", "schwa\u00b7ches", "Reh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "F\u00e4ht auff der Berge H\u00f6h.", "tokens": ["F\u00e4ht", "auff", "der", "Ber\u00b7ge", "H\u00f6h", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Seit da\u00df der Witwen Orden", "tokens": ["Seit", "da\u00df", "der", "Wit\u00b7wen", "Or\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich unter sich gebracht,", "tokens": ["Mich", "un\u00b7ter", "sich", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bin ich mir gantz entworden,", "tokens": ["Bin", "ich", "mir", "gantz", "ent\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir wild und frembd gemacht,", "tokens": ["Mir", "wild", "und", "frembd", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich weine Tag und Nacht.", "tokens": ["Ich", "wei\u00b7ne", "Tag", "und", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mein Hau\u00df, darin ich wohne,", "tokens": ["Mein", "Hau\u00df", ",", "da\u00b7rin", "ich", "woh\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist eine W\u00fcsteney,", "tokens": ["Ist", "ei\u00b7ne", "W\u00fcs\u00b7te\u00b7ney", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es misset seine Krohne", "tokens": ["Es", "mis\u00b7set", "sei\u00b7ne", "Kroh\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd f\u00fchret darumb Rew", "tokens": ["Vnd", "f\u00fch\u00b7ret", "da\u00b7rumb", "Rew"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PAV", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd grosses Angst-geschrey.", "tokens": ["Vnd", "gros\u00b7ses", "Angst\u00b7ge\u00b7schrey", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was hilfft es, da\u00df ich lebe?", "tokens": ["Was", "hilfft", "es", ",", "da\u00df", "ich", "le\u00b7be", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich komm umb Mann und Kind,", "tokens": ["Ich", "komm", "umb", "Mann", "und", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil meines Alters St\u00e4be", "tokens": ["Weil", "mei\u00b7nes", "Al\u00b7ters", "St\u00e4\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd Stecken so geschwind", "tokens": ["Vnd", "Ste\u00b7cken", "so", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gleich mit zerbrochen sind.", "tokens": ["Gleich", "mit", "zer\u00b7bro\u00b7chen", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wo sind nun meine Frewden?", "tokens": ["Wo", "sind", "nun", "mei\u00b7ne", "Frew\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo ist mein' Hoffnung hin?", "tokens": ["Wo", "ist", "mein'", "Hoff\u00b7nung", "hin", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An ihre stat ist Leiden", "tokens": ["An", "ih\u00b7re", "stat", "ist", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd hochbekr\u00e4nckter Sinn", "tokens": ["Vnd", "hoch\u00b7be\u00b7kr\u00e4nck\u00b7ter", "Sinn"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Geworden mein Gewinn.", "tokens": ["Ge\u00b7wor\u00b7den", "mein", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ergiesst euch, heisse Zehren,", "tokens": ["Er\u00b7giesst", "euch", ",", "heis\u00b7se", "Zeh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch meiner Augen Strah\u00df,", "tokens": ["Durch", "mei\u00b7ner", "Au\u00b7gen", "Strah\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd wil euch jemand wehren,", "tokens": ["Vnd", "wil", "euch", "je\u00b7mand", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gebeut euch jemand Ma\u00df,", "tokens": ["Ge\u00b7beut", "euch", "je\u00b7mand", "Ma\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIS", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den hasst ohn unterla\u00df,", "tokens": ["Den", "hasst", "ohn", "un\u00b7ter\u00b7la\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "In dem ich mich beraube", "tokens": ["In", "dem", "ich", "mich", "be\u00b7rau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Frewden aller Welt,", "tokens": ["Der", "Frew\u00b7den", "al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie eine Turtel-Taube,", "tokens": ["Wie", "ei\u00b7ne", "Tur\u00b7tel\u00b7Tau\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn ihr der Gatt' entf\u00e4llt,", "tokens": ["Wenn", "ihr", "der", "Gatt'", "ent\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sich \u00f6ed und einsam h\u00e4lt.", "tokens": ["Sich", "\u00f6\u00b7ed", "und", "ein\u00b7sam", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}}, "stanza.9": {"line.1": {"text": "Nur Du, mein Wunden-schl\u00e4ger,", "tokens": ["Nur", "Du", ",", "mein", "Wun\u00b7den\u00b7schl\u00e4\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie hart greiffst du mich an!", "tokens": ["Wie", "hart", "greiffst", "du", "mich", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Komm, sey nun auch mein Pfleger,", "tokens": ["Komm", ",", "sey", "nun", "auch", "mein", "Pfle\u00b7ger", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Vater, Schutz und Mann,", "tokens": ["Mein", "Va\u00b7ter", ",", "Schutz", "und", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd trag was ich nicht kan!", "tokens": ["Vnd", "trag", "was", "ich", "nicht", "kan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "PPER", "PTKNEG", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Du weist umb meine St\u00e4rcke", "tokens": ["Du", "weist", "umb", "mei\u00b7ne", "St\u00e4r\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bey dieser schweren Last,", "tokens": ["Bey", "die\u00b7ser", "schwe\u00b7ren", "Last", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wofern ich, Gott, nicht mercke,", "tokens": ["Wo\u00b7fern", "ich", ",", "Gott", ",", "nicht", "mer\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NN", "$,", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Du sie angefasst,", "tokens": ["Das", "Du", "sie", "an\u00b7ge\u00b7fasst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So wei\u00df ich keine Rast.", "tokens": ["So", "wei\u00df", "ich", "kei\u00b7ne", "Rast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "La\u00df mich in meinem Leiden", "tokens": ["La\u00df", "mich", "in", "mei\u00b7nem", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O liebster Vater, seyn", "tokens": ["O", "liebs\u00b7ter", "Va\u00b7ter", ",", "seyn"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gedultig und bescheiden,", "tokens": ["Ge\u00b7dul\u00b7tig", "und", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nimm mich nach solcher Pein", "tokens": ["Nimm", "mich", "nach", "sol\u00b7cher", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PRF", "APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In deinen Himmel ein!", "tokens": ["In", "dei\u00b7nen", "Him\u00b7mel", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}