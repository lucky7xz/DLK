{"textgrid.poem.67097": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An Diotima", "genre": "verse", "period": "N.A.", "pub_year": 1797, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm und siehe die Freude um uns; in k\u00fchlenden L\u00fcften", "tokens": ["Komm", "und", "sie\u00b7he", "die", "Freu\u00b7de", "um", "uns", ";", "in", "k\u00fch\u00b7len\u00b7den", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVIMP", "ART", "NN", "APPR", "PPER", "$.", "APPR", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Fliegen die Zweige des Hains,", "tokens": ["Flie\u00b7gen", "die", "Zwei\u00b7ge", "des", "Hains", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Wie die Locken im Tanz; und wie auf t\u00f6nender Leier", "tokens": ["Wie", "die", "Lo\u00b7cken", "im", "Tanz", ";", "und", "wie", "auf", "t\u00f6\u00b7nen\u00b7der", "Lei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$.", "KON", "PWAV", "APPR", "ADJA", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ein erfreulicher Geist,", "tokens": ["Ein", "er\u00b7freu\u00b7li\u00b7cher", "Geist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Spielt mit Regen und Sonnenschein auf der Erde der Himmel;", "tokens": ["Spielt", "mit", "Re\u00b7gen", "und", "Son\u00b7nen\u00b7schein", "auf", "der", "Er\u00b7de", "der", "Him\u00b7mel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Wie in liebendem Streit", "tokens": ["Wie", "in", "lie\u00b7ben\u00b7dem", "Streit"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "\u00dcber dem Saitenspiel ein tausendf\u00e4ltig Gewimmel", "tokens": ["\u00dc\u00b7ber", "dem", "Sai\u00b7ten\u00b7spiel", "ein", "tau\u00b7send\u00b7f\u00e4l\u00b7tig", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJD", "NN"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Fl\u00fcchtiger T\u00f6ne sich regt,", "tokens": ["Fl\u00fcch\u00b7ti\u00b7ger", "T\u00f6\u00b7ne", "sich", "regt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.9": {"text": "Wandelt Schatten und Licht in s\u00fc\u00dfmelodischem Wechsel", "tokens": ["Wan\u00b7delt", "Schat\u00b7ten", "und", "Licht", "in", "s\u00fc\u00df\u00b7me\u00b7lo\u00b7di\u00b7schem", "Wech\u00b7sel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN", "APPR", "ADJA", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "\u00dcber die Berge dahin.", "tokens": ["\u00dc\u00b7ber", "die", "Ber\u00b7ge", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PAV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.11": {"text": "Leise ber\u00fchrte der Himmel zuvor mit der silbernen Tropfe", "tokens": ["Lei\u00b7se", "be\u00b7r\u00fchr\u00b7te", "der", "Him\u00b7mel", "zu\u00b7vor", "mit", "der", "sil\u00b7ber\u00b7nen", "Trop\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Seinen Bruder, den Strom,", "tokens": ["Sei\u00b7nen", "Bru\u00b7der", ",", "den", "Strom", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Nah ist er nun, nun sch\u00fcttet er ganz die k\u00f6stliche F\u00fclle,", "tokens": ["Nah", "ist", "er", "nun", ",", "nun", "sch\u00fct\u00b7tet", "er", "ganz", "die", "k\u00f6st\u00b7li\u00b7che", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Die er am Herzen trug,", "tokens": ["Die", "er", "am", "Her\u00b7zen", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "\u00dcber den Hain und den Strom, und ...", "tokens": ["\u00dc\u00b7ber", "den", "Hain", "und", "den", "Strom", ",", "und", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "$,", "KON", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.16": {"text": ". . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$."]}, "line.17": {"text": "Und das Gr\u00fcnen des Hains, und des Himmels Bild in dem Strome", "tokens": ["Und", "das", "Gr\u00fc\u00b7nen", "des", "Hains", ",", "und", "des", "Him\u00b7mels", "Bild", "in", "dem", "Stro\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,", "KON", "ART", "NN", "NN", "APPR", "ART", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.18": {"text": "D\u00e4mmert und schwindet vor uns", "tokens": ["D\u00e4m\u00b7mert", "und", "schwin\u00b7det", "vor", "uns"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "VVFIN", "APPR", "PPER"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.19": {"text": "Und des einsamen Berges Haupt mit den H\u00fctten und Felsen,", "tokens": ["Und", "des", "ein\u00b7sa\u00b7men", "Ber\u00b7ges", "Haupt", "mit", "den", "H\u00fct\u00b7ten", "und", "Fel\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NN", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "---+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Die er im Scho\u00dfe verbirgt,", "tokens": ["Die", "er", "im", "Scho\u00b7\u00dfe", "ver\u00b7birgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.21": {"text": "Und die H\u00fcgel, die um ihn her, wie L\u00e4mmer, gelagert", "tokens": ["Und", "die", "H\u00fc\u00b7gel", ",", "die", "um", "ihn", "her", ",", "wie", "L\u00e4m\u00b7mer", ",", "ge\u00b7la\u00b7gert"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "PTKVZ", "$,", "PWAV", "NN", "$,", "VVPP"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Und in bl\u00fchend Gestr\u00e4uch", "tokens": ["Und", "in", "bl\u00fc\u00b7hend", "Ge\u00b7str\u00e4uch"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJD", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.23": {"text": "Wie in zarte Wolle geh\u00fcllt, sich n\u00e4hren von klaren", "tokens": ["Wie", "in", "zar\u00b7te", "Wol\u00b7le", "ge\u00b7h\u00fcllt", ",", "sich", "n\u00e4h\u00b7ren", "von", "kla\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "VVPP", "$,", "PRF", "VVFIN", "APPR", "ADJA"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "K\u00fchlenden Quellen des Bergs,", "tokens": ["K\u00fch\u00b7len\u00b7den", "Quel\u00b7len", "des", "Bergs", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.25": {"text": "Und das dampfende Tal mit seinen Saaten und Blumen,", "tokens": ["Und", "das", "damp\u00b7fen\u00b7de", "Tal", "mit", "sei\u00b7nen", "Saa\u00b7ten", "und", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.26": {"text": "Und der Garten vor uns,", "tokens": ["Und", "der", "Gar\u00b7ten", "vor", "uns", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPER", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.27": {"text": "Nah und Fernes entweicht, verliert sich in froher Verwirrung", "tokens": ["Nah", "und", "Fer\u00b7nes", "ent\u00b7weicht", ",", "ver\u00b7liert", "sich", "in", "fro\u00b7her", "Ver\u00b7wir\u00b7rung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NE", "VVFIN", "$,", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.28": {"text": "Und die Sonne verlischt.", "tokens": ["Und", "die", "Son\u00b7ne", "ver\u00b7lischt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.29": {"text": "Aber vor\u00fcbergerauscht sind nun die Fluten des Himmels", "tokens": ["A\u00b7ber", "vor\u00b7\u00fc\u00b7ber\u00b7ge\u00b7rauscht", "sind", "nun", "die", "Flu\u00b7ten", "des", "Him\u00b7mels"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "VAFIN", "ADV", "ART", "NN", "ART", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.30": {"text": "Und gel\u00e4utert, verj\u00fcngt", "tokens": ["Und", "ge\u00b7l\u00e4u\u00b7tert", ",", "ver\u00b7j\u00fcngt"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "VVPP", "$,", "VVFIN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.31": {"text": "Geht mit den seligen Kindern hervor die Erd aus dem Bade.", "tokens": ["Geht", "mit", "den", "se\u00b7li\u00b7gen", "Kin\u00b7dern", "her\u00b7vor", "die", "Erd", "aus", "dem", "Ba\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.32": {"text": "Froher lebendiger", "tokens": ["Fro\u00b7her", "le\u00b7ben\u00b7di\u00b7ger"], "token_info": ["word", "word"], "pos": ["ADJA", "ADJA"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.33": {"text": "Gl\u00e4nzt im Haine das Gr\u00fcn, und goldner funkeln die Blumen,", "tokens": ["Gl\u00e4nzt", "im", "Hai\u00b7ne", "das", "Gr\u00fcn", ",", "und", "gold\u00b7ner", "fun\u00b7keln", "die", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$,", "KON", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.34": {"text": ". . . . . . . . . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$."]}, "line.35": {"text": "Wei\u00df, wie die Herde, die in den Strom der Sch\u00e4fer geworfen,", "tokens": ["Wei\u00df", ",", "wie", "die", "Her\u00b7de", ",", "die", "in", "den", "Strom", "der", "Sch\u00e4\u00b7fer", "ge\u00b7wor\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "ART", "N