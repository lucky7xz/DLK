{"textgrid.poem.60381": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Schlaflose Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1835, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlaflose Nacht, du bist allein die Zeit", "tokens": ["Schlaf\u00b7lo\u00b7se", "Nacht", ",", "du", "bist", "al\u00b7lein", "die", "Zeit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "ADV", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Der ungest\u00f6rten Einsamkeit!", "tokens": ["Der", "un\u00b7ge\u00b7st\u00f6r\u00b7ten", "Ein\u00b7sam\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn seine Herde treibt der laute Tag", "tokens": ["Denn", "sei\u00b7ne", "Her\u00b7de", "treibt", "der", "lau\u00b7te", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In unsern gr\u00fcnenden Gedankenhag,", "tokens": ["In", "un\u00b7sern", "gr\u00fc\u00b7nen\u00b7den", "Ge\u00b7dan\u00b7ken\u00b7hag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die sch\u00f6nsten Bl\u00fcten werden abgefressen,", "tokens": ["Die", "sch\u00f6ns\u00b7ten", "Bl\u00fc\u00b7ten", "wer\u00b7den", "ab\u00b7ge\u00b7fres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zertreten oft im Keime und vergessen.", "tokens": ["Zer\u00b7tre\u00b7ten", "oft", "im", "Kei\u00b7me", "und", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Tr\u00e4gt aber uns der Schlaf mit weicher Hand", "tokens": ["Tr\u00e4gt", "a\u00b7ber", "uns", "der", "Schlaf", "mit", "wei\u00b7cher", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPER", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ins Zauberboot, das heimlich sto\u00dft vom Strand,", "tokens": ["Ins", "Zau\u00b7ber\u00b7boot", ",", "das", "heim\u00b7lich", "sto\u00dft", "vom", "Strand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und lenkt das Boot im weiten Ozean", "tokens": ["Und", "lenkt", "das", "Boot", "im", "wei\u00b7ten", "O\u00b7ze\u00b7an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der Traum herum, ein trunkner Steuermann,", "tokens": ["Der", "Traum", "he\u00b7rum", ",", "ein", "trunk\u00b7ner", "Steu\u00b7er\u00b7mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "So sind wir nicht allein, denn bald gesellen", "tokens": ["So", "sind", "wir", "nicht", "al\u00b7lein", ",", "denn", "bald", "ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "$,", "KON", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Die Launen uns der unbeherrschten Wellen", "tokens": ["Die", "Lau\u00b7nen", "uns", "der", "un\u00b7be\u00b7herrschten", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Mit Menschen mancherlei, vielleicht mit solchen,", "tokens": ["Mit", "Men\u00b7schen", "man\u00b7cher\u00b7lei", ",", "viel\u00b7leicht", "mit", "sol\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIS", "$,", "ADV", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Die feindlich unser Innres tief verletzt,", "tokens": ["Die", "feind\u00b7lich", "un\u00b7ser", "Inn\u00b7res", "tief", "ver\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Bei deren Anblick sich das Herz entsetzt,", "tokens": ["Bei", "de\u00b7ren", "An\u00b7blick", "sich", "das", "Herz", "ent\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Getroffen von des Hasses kalten Dolchen;", "tokens": ["Ge\u00b7trof\u00b7fen", "von", "des", "Has\u00b7ses", "kal\u00b7ten", "Dol\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "An denen gerne wir vor\u00fcberdenken,", "tokens": ["An", "de\u00b7nen", "ger\u00b7ne", "wir", "vor\u00b7\u00fc\u00b7ber\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Um tiefer nicht den Dolch ins Herz zu senken. \u2013", "tokens": ["Um", "tie\u00b7fer", "nicht", "den", "Dolch", "ins", "Herz", "zu", "sen\u00b7ken", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUI", "ADJD", "PTKNEG", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Dann wieder bringen uns die Wellenfluchten,", "tokens": ["Dann", "wie\u00b7der", "brin\u00b7gen", "uns", "die", "Wel\u00b7len\u00b7fluch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Wohin wir wachend nimmermehr gelangen,", "tokens": ["Wo\u00b7hin", "wir", "wa\u00b7chend", "nim\u00b7mer\u00b7mehr", "ge\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "In der Vergangenheit geheimste Buchten,", "tokens": ["In", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", "ge\u00b7heims\u00b7te", "Buch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Wo uns der Jugend Hoffnungen empfangen.", "tokens": ["Wo", "uns", "der", "Ju\u00b7gend", "Hoff\u00b7nun\u00b7gen", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Was aber hilfts? wir wachen auf \u2013 entschwunden", "tokens": ["Was", "a\u00b7ber", "hilfts", "?", "wir", "wa\u00b7chen", "auf", "\u2013", "ent\u00b7schwun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PWS", "ADV", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "$(", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Ist all das Gl\u00fcck, es schmerzen alte Wunden.", "tokens": ["Ist", "all", "das", "Gl\u00fcck", ",", "es", "schmer\u00b7zen", "al\u00b7te", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ART", "NN", "$,", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Schlaflose Nacht, du bist allein die Zeit", "tokens": ["Schlaf\u00b7lo\u00b7se", "Nacht", ",", "du", "bist", "al\u00b7lein", "die", "Zeit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "ADV", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.26": {"text": "Der ungest\u00f6rten Einsamkeit!", "tokens": ["Der", "un\u00b7ge\u00b7st\u00f6r\u00b7ten", "Ein\u00b7sam\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}