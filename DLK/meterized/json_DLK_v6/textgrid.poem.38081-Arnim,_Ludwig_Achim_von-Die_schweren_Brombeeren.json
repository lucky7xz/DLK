{"textgrid.poem.38081": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Die schweren Brombeeren", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wollt ein M\u00e4gdlein fr\u00fch aufstehn,", "tokens": ["Es", "wollt", "ein", "M\u00e4gd\u00b7lein", "fr\u00fch", "auf\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drey St\u00fcndelein vor dem Tag,", "tokens": ["Drey", "St\u00fcn\u00b7del\u00b7ein", "vor", "dem", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wollt in den gr\u00fcnen Wald n'aus gehn,", "tokens": ["Wollt", "in", "den", "gr\u00fc\u00b7nen", "Wald", "n'\u00b7aus", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Brombeerlein brechen ab.", "tokens": ["Brom\u00b7beer\u00b7lein", "bre\u00b7chen", "ab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Und als sie in den Wald nein kam,", "tokens": ["Und", "als", "sie", "in", "den", "Wald", "nein", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "PTKANT", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Begegnet ihr J\u00e4gers Knecht.", "tokens": ["Be\u00b7geg\u00b7net", "ihr", "J\u00e4\u00b7gers", "Knecht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ey M\u00e4dchen scher dich weg nach Haus,", "tokens": ["Ey", "M\u00e4d\u00b7chen", "scher", "dich", "weg", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Herren ist das nicht recht.", "tokens": ["Dem", "Her\u00b7ren", "ist", "das", "nicht", "recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PDS", "PTKNEG", "ADJD", "$."], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "Und als das M\u00e4dchen r\u00fcckw\u00e4rts kam,", "tokens": ["Und", "als", "das", "M\u00e4d\u00b7chen", "r\u00fcck\u00b7w\u00e4rts", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Begegnet ihr J\u00e4gers Sohn:", "tokens": ["Be\u00b7geg\u00b7net", "ihr", "J\u00e4\u00b7gers", "Sohn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbey M\u00e4dchen brech dir ohne Scham,", "tokens": ["\u00bb", "ey", "M\u00e4d\u00b7chen", "brech", "dir", "oh\u00b7ne", "Scham", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Schoo\u00df voll g\u00f6nn ich dir schon.\u00ab", "tokens": ["Ein", "Schoo\u00df", "voll", "g\u00f6nn", "ich", "dir", "schon", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "PPER", "PPER", "ADV", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "\u00bbein Schoo\u00df voll den begehr ich nicht,", "tokens": ["\u00bb", "ein", "Schoo\u00df", "voll", "den", "be\u00b7gehr", "ich", "nicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ADJD", "ART", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Handvoll hab ich genug.\u00ab", "tokens": ["Ein", "Hand\u00b7voll", "hab", "ich", "ge\u00b7nug", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die Brombeeren standen da so dicht,", "tokens": ["Die", "Brom\u00b7bee\u00b7ren", "stan\u00b7den", "da", "so", "dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Sie suchten da immerzu.", "tokens": ["Sie", "such\u00b7ten", "da", "im\u00b7mer\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Und als ein halbes Jahr um war,", "tokens": ["Und", "als", "ein", "hal\u00b7bes", "Jahr", "um", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "APPR", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Brombeerlein wurden gro\u00df,", "tokens": ["Brom\u00b7beer\u00b7lein", "wur\u00b7den", "gro\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und als ein drey Vierteljahr um waren,", "tokens": ["Und", "als", "ein", "drey", "Vier\u00b7tel\u00b7jahr", "um", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "CARD", "NN", "APPR", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ein Kindlein auf dem Schoo\u00df.", "tokens": ["Ein", "Kin\u00b7dlein", "auf", "dem", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ach Gott sind das die Brombeerlein,", "tokens": ["Ach", "Gott", "sind", "das", "die", "Brom\u00b7beer\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VAFIN", "PDS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ich mir gebrochen hab,", "tokens": ["Die", "ich", "mir", "ge\u00b7bro\u00b7chen", "hab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Komm her du falsches J\u00e4gerlein,", "tokens": ["Komm", "her", "du", "fal\u00b7sches", "J\u00e4\u00b7ger\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hilf tragen mich ins Grab.", "tokens": ["Hilf", "tra\u00b7gen", "mich", "ins", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}