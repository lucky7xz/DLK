{"textgrid.poem.53547": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Kriegsgefangen", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.71", "nl:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer hat in Belgiens Etappen regiert?", "tokens": ["Wer", "hat", "in", "Bel\u00b7giens", "E\u00b7tap\u00b7pen", "re\u00b7giert", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Offiziere! Offiziere!", "tokens": ["Of\u00b7fi\u00b7zie\u00b7re", "!", "Of\u00b7fi\u00b7zie\u00b7re", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer hat da im preu\u00dfischen Ton kommandiert?", "tokens": ["Wer", "hat", "da", "im", "preu\u00b7\u00dfi\u00b7schen", "Ton", "kom\u00b7man\u00b7diert", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Offiziere! Offiziere!", "tokens": ["Of\u00b7fi\u00b7zie\u00b7re", "!", "Of\u00b7fi\u00b7zie\u00b7re", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sollen die Belgier die Schuhe putzen:", "tokens": ["Sol\u00b7len", "die", "Bel\u00b7gier", "die", "Schu\u00b7he", "put\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "wir haben den Spa\u00df, wir haben den Nutzen!", "tokens": ["wir", "ha\u00b7ben", "den", "Spa\u00df", ",", "wir", "ha\u00b7ben", "den", "Nut\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Aktiver Leutnant \u2013 Rechnungsrat \u2013", "tokens": ["Ak\u00b7ti\u00b7ver", "Leut\u00b7nant", "\u2013", "Rech\u00b7nungs\u00b7rat", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "$(", "NN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "einmal: C\u00e4sar! Wie wohl das tat!", "tokens": ["ein\u00b7mal", ":", "C\u00e4\u00b7sar", "!", "Wie", "wohl", "das", "tat", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "NE", "$.", "PWAV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "\u00bbwer nicht pariert, den stellt an die Wand!", "tokens": ["\u00bb", "wer", "nicht", "pa\u00b7riert", ",", "den", "stellt", "an", "die", "Wand", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PTKNEG", "VVPP", "$,", "ART", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "(gez.) Lehmann, Ortskommandant.\u00ab", "tokens": ["(", "ge\u00b7z.", ")", "Leh\u00b7mann", ",", "Orts\u00b7kom\u00b7man\u00b7dant", ".", "\u00ab"], "token_info": ["punct", "abbreviation", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "NE", "$(", "NE", "$,", "NN", "$.", "$("], "meter": "+----+", "measure": "dactylic.init"}, "line.11": {"text": "Und die Belgier waren Menschen wie wir,", "tokens": ["Und", "die", "Bel\u00b7gier", "wa\u00b7ren", "Men\u00b7schen", "wie", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "NN", "KOKOM", "PPER", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "warteten ruhig der Jahre vier,", "tokens": ["war\u00b7te\u00b7ten", "ru\u00b7hig", "der", "Jah\u00b7re", "vier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "CARD", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.13": {"text": "bis sich der fremde Spuk entfernt.", "tokens": ["bis", "sich", "der", "frem\u00b7de", "Spuk", "ent\u00b7fernt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wen haben sie gr\u00fcndlich kennen gelernt?", "tokens": ["Wen", "ha\u00b7ben", "sie", "gr\u00fcnd\u00b7lich", "ken\u00b7nen", "ge\u00b7lernt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADJD", "VVINF", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Offiziere! Offiziere!", "tokens": ["Of\u00b7fi\u00b7zie\u00b7re", "!", "Of\u00b7fi\u00b7zie\u00b7re", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kein Stroh auf dem Boden, kein Wasser, kein Bett,", "tokens": ["Kein", "Stroh", "auf", "dem", "Bo\u00b7den", ",", "kein", "Was\u00b7ser", ",", "kein", "Bett", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "es schlottern die d\u00fcnnen Jacken.", "tokens": ["es", "schlot\u00b7tern", "die", "d\u00fcn\u00b7nen", "Ja\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbmutter!\u00ab Wer jetzt einen Heimatgru\u00df h\u00e4tt!", "tokens": ["\u00bb", "mut\u00b7ter", "!", "\u00ab", "Wer", "jetzt", "ei\u00b7nen", "Hei\u00b7mat\u00b7gru\u00df", "h\u00e4tt", "!"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$.", "$(", "PWS", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Will der Tod uns noch nicht packen?", "tokens": ["Will", "der", "Tod", "uns", "noch", "nicht", "pa\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbtravaillez! En avant, les boches! Vite! vite!\u00ab", "tokens": ["\u00bb", "tra\u00b7vail\u00b7lez", "!", "En", "a\u00b7vant", ",", "les", "bo\u00b7ches", "!", "Vi\u00b7te", "!", "vi\u00b7te", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "$.", "FM.fr", "FM.fr", "$,", "FM", "FM", "$.", "NN", "$.", "NN", "$.", "$("], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Ein Kolbensto\u00df in den R\u00fccken.", "tokens": ["Ein", "Kol\u00b7ben\u00b7sto\u00df", "in", "den", "R\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Ein Mann, der vorbeif\u00e4hrt und das sieht,", "tokens": ["Ein", "Mann", ",", "der", "vor\u00b7bei\u00b7f\u00e4hrt", "und", "das", "sieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "KON", "PDS", "VVFIN", "$,"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.8": {"text": "mu\u00df die Tr\u00e4nen unterdr\u00fccken.", "tokens": ["mu\u00df", "die", "Tr\u00e4\u00b7nen", "un\u00b7ter\u00b7dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wer fri\u00dft es aus, was f\u00fcr uns vergangen? \u2013", "tokens": ["Wer", "fri\u00dft", "es", "aus", ",", "was", "f\u00fcr", "uns", "ver\u00b7gan\u00b7gen", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKVZ", "$,", "PRELS", "APPR", "PPER", "VVPP", "$.", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Kriegsgefangen. Kriegsgefangen.", "tokens": ["Kriegs\u00b7ge\u00b7fan\u00b7gen", ".", "Kriegs\u00b7ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wer fri\u00dft es aus, was scheinbar vorbei?", "tokens": ["Wer", "fri\u00dft", "es", "aus", ",", "was", "schein\u00b7bar", "vor\u00b7bei", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKVZ", "$,", "PRELS", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die eigenen, unschuldigen Leute!", "tokens": ["Die", "ei\u00b7ge\u00b7nen", ",", "un\u00b7schul\u00b7di\u00b7gen", "Leu\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--++--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Deutschland, h\u00f6rst du den Marterschrei?", "tokens": ["Deutschland", ",", "h\u00f6rst", "du", "den", "Mar\u00b7ter\u00b7schrei", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deutschland, tu dies noch heute:", "tokens": ["Deutschland", ",", "tu", "dies", "noch", "heu\u00b7te", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PDS", "ADV", "ADV", "$."], "meter": "+---+-", "measure": "dactylic.init"}, "line.5": {"text": "Stell die Burschen von damals vor ein Gericht!", "tokens": ["Stell", "die", "Bur\u00b7schen", "von", "da\u00b7mals", "vor", "ein", "Ge\u00b7richt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Sie sind noch frei. Sie b\u00fc\u00dfen ja nicht!", "tokens": ["Sie", "sind", "noch", "frei", ".", "Sie", "b\u00fc\u00b7\u00dfen", "ja", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$.", "PPER", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Sieh, wie sie wohlgeborgen sitzen!", "tokens": ["Sieh", ",", "wie", "sie", "wohl\u00b7ge\u00b7bor\u00b7gen", "sit\u00b7zen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit ersparten Geh\u00e4ltern, mit Br\u00fcssler Spitzen \u2013", "tokens": ["Mit", "er\u00b7spar\u00b7ten", "Ge\u00b7h\u00e4l\u00b7tern", ",", "mit", "Br\u00fcss\u00b7ler", "Spit\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$("], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Auge um Auge! Zahn um Zahn!", "tokens": ["Au\u00b7ge", "um", "Au\u00b7ge", "!", "Zahn", "um", "Zahn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$.", "NN", "APPR", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "In die H\u00f6lle mit ihrem C\u00e4sarenwahn!", "tokens": ["In", "die", "H\u00f6l\u00b7le", "mit", "ih\u00b7rem", "C\u00e4\u00b7sa\u00b7ren\u00b7wahn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.11": {"text": "Deutschland, wo ist der Tag des Gerichts?", "tokens": ["Deutschland", ",", "wo", "ist", "der", "Tag", "des", "Ge\u00b7richts", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Deutschland, was tust du?", "tokens": ["Deutschland", ",", "was", "tust", "du", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VVFIN", "PPER", "$."], "meter": "+---", "measure": "dactylic.init"}, "line.13": {"text": "Nichts. Nichts. Nichts.", "tokens": ["Nichts", ".", "Nichts", ".", "Nichts", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["PIS", "$.", "PIS", "$.", "PIS", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}