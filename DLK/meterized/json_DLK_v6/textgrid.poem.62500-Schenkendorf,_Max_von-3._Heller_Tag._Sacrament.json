{"textgrid.poem.62500": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "3. Heller Tag. Sacrament", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drum hei\u00dft er ja der Heiland", "tokens": ["Drum", "hei\u00dft", "er", "ja", "der", "Hei\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und ew'ger Trost und Rath,", "tokens": ["Und", "ew'\u00b7ger", "Trost", "und", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil er noch stets, wie weiland,", "tokens": ["Weil", "er", "noch", "stets", ",", "wie", "wei\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "$,", "PWAV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich allen S\u00fcndern naht.", "tokens": ["Sich", "al\u00b7len", "S\u00fcn\u00b7dern", "naht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mit seinem Geist und Gaben", "tokens": ["Mit", "sei\u00b7nem", "Geist", "und", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist er noch immer hier,", "tokens": ["Ist", "er", "noch", "im\u00b7mer", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man kann ihn immer haben,", "tokens": ["Man", "kann", "ihn", "im\u00b7mer", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er steht vor jeder Th\u00fcr.", "tokens": ["Er", "steht", "vor", "je\u00b7der", "Th\u00fcr."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Den Gottes Magd geboren,", "tokens": ["Den", "Got\u00b7tes", "Magd", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Gottes Magd ges\u00e4ugt,", "tokens": ["Den", "Got\u00b7tes", "Magd", "ge\u00b7s\u00e4ugt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er wird noch heut geboren", "tokens": ["Er", "wird", "noch", "heut", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und immerfort erzeugt.", "tokens": ["Und", "im\u00b7mer\u00b7fort", "er\u00b7zeugt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "In frommen Menschenherzen", "tokens": ["In", "from\u00b7men", "Men\u00b7schen\u00b7her\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gewinnt er die Gestalt,", "tokens": ["Ge\u00b7winnt", "er", "die", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu Lust und auch zu Schmerzen", "tokens": ["Zu", "Lust", "und", "auch", "zu", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit himmlischer Gewalt.", "tokens": ["Mit", "himm\u00b7li\u00b7scher", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das ist das Pfand der Gnade,", "tokens": ["Das", "ist", "das", "Pfand", "der", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die uns der Herr geschenkt,", "tokens": ["Die", "uns", "der", "Herr", "ge\u00b7schenkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die ew'ge Bundeslade,", "tokens": ["Die", "ew'\u00b7ge", "Bun\u00b7des\u00b7la\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In die er sich gesenkt.", "tokens": ["In", "die", "er", "sich", "ge\u00b7senkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Das ist das ew'ge Leben,", "tokens": ["Das", "ist", "das", "ew'\u00b7ge", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das jeder haben kann,", "tokens": ["Das", "je\u00b7der", "ha\u00b7ben", "kann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das liebend sich ergeben", "tokens": ["Das", "lie\u00b7bend", "sich", "er\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ADJD", "PRF", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zur Speise Jedermann.", "tokens": ["Zur", "Spei\u00b7se", "Je\u00b7der\u00b7mann", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Das ist die dritte Weihnacht,", "tokens": ["Das", "ist", "die", "drit\u00b7te", "Weih\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nach der sein J\u00fcnger weint,", "tokens": ["Nach", "der", "sein", "J\u00fcn\u00b7ger", "weint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das ist die sch\u00f6nste Weihnacht,", "tokens": ["Das", "ist", "die", "sch\u00f6ns\u00b7te", "Weih\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn er in uns erscheint!", "tokens": ["Wenn", "er", "in", "uns", "er\u00b7scheint", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}