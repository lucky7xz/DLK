{"textgrid.poem.53147": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Aus dem Frantz\u00f6sischen: Printemps sans ma belle", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lentz ohn meine Sonne,", "tokens": ["Lentz", "ohn", "mei\u00b7ne", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Bist du wieder hie?", "tokens": ["Bist", "du", "wie\u00b7der", "hie", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Meinstu, da\u00df mir Wonne", "tokens": ["Meins\u00b7tu", ",", "da\u00df", "mir", "Won\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ein Tag bring' ohn Sie?", "tokens": ["Ein", "Tag", "bring'", "ohn", "Sie", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Nein, Ohn Cloris kan der Pein", "tokens": ["Nein", ",", "Ohn", "Clo\u00b7ris", "kan", "der", "Pein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUI", "NE", "VMFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Tirsis nie entladen seyn.", "tokens": ["Tir\u00b7sis", "nie", "ent\u00b7la\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Deiner Blumen Menge,", "tokens": ["Dei\u00b7ner", "Blu\u00b7men", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Flora, n\u00fctzt mir nicht,", "tokens": ["Flo\u00b7ra", ",", "n\u00fctzt", "mir", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ist gleich jhr Gepr\u00e4nge", "tokens": ["Ist", "gleich", "jhr", "Ge\u00b7pr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Tausentf\u00e4rbigs Liecht,", "tokens": ["Tau\u00b7sent\u00b7f\u00e4r\u00b7bigs", "Liecht", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Tyrsis Blumen m\u00fcssen seyn", "tokens": ["Tyr\u00b7sis", "Blu\u00b7men", "m\u00fcs\u00b7sen", "seyn"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "VMFIN", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Leid-Gedancken, Sorg' vnd Pein.", "tokens": ["Lei\u00b7dGe\u00b7dan\u00b7cken", ",", "Sor\u00b7g'", "vnd", "Pein", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Sol dein Wind mir dienen,", "tokens": ["Sol", "dein", "Wind", "mir", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Angenehmer West,", "tokens": ["An\u00b7ge\u00b7neh\u00b7mer", "West", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Der sich hier im Gr\u00fcnen", "tokens": ["Der", "sich", "hier", "im", "Gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "ADV", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Lieblich h\u00f6ren l\u00e4sst?", "tokens": ["Lieb\u00b7lich", "h\u00f6\u00b7ren", "l\u00e4sst", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Tirsis Wind vnd Blumen seyn", "tokens": ["Tir\u00b7sis", "Wind", "vnd", "Blu\u00b7men", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NN", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Tieffe Seufftzer, Sorg vnd Pein.", "tokens": ["Tief\u00b7fe", "Seufft\u00b7zer", ",", "Sorg", "vnd", "Pein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dein Gesang daneben,", "tokens": ["Dein", "Ge\u00b7sang", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PAV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Nachtigal, den man", "tokens": ["Nach\u00b7ti\u00b7gal", ",", "den", "man"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$,", "PRELS", "PIS"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sonst nur mu\u00df erheben,", "tokens": ["Sonst", "nur", "mu\u00df", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Geht mich nicht mehr an,", "tokens": ["Geht", "mich", "nicht", "mehr", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Tirsis Klang vnd Blumen seyn", "tokens": ["Tir\u00b7sis", "Klang", "vnd", "Blu\u00b7men", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NN", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Klag' vnd Seufftzer, Sorg' vnd Pein.", "tokens": ["Klag'", "vnd", "Seufft\u00b7zer", ",", "Sor\u00b7g'", "vnd", "Pein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Ja, ich wil auch meiden", "tokens": ["Ja", ",", "ich", "wil", "auch", "mei\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Euch, jhr Brunnen, wol,", "tokens": ["Euch", ",", "jhr", "Brun\u00b7nen", ",", "wol", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Seht, von meinem Leiden", "tokens": ["Seht", ",", "von", "mei\u00b7nem", "Lei\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Sind die B\u00e4ch' hie voll!", "tokens": ["Sind", "die", "B\u00e4ch'", "hie", "voll", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Tirsis Fluth vnd Blumen seyn", "tokens": ["Tir\u00b7sis", "Fluth", "vnd", "Blu\u00b7men", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NN", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Thr\u00e4nen-Wasser, Sorg' vnd Pein.", "tokens": ["Thr\u00e4\u00b7nen\u00b7Was\u00b7ser", ",", "Sor\u00b7g'", "vnd", "Pein", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+----+", "measure": "unknown.measure.tri"}}, "stanza.6": {"line.1": {"text": "Cloris ist von hinnen!", "tokens": ["Clo\u00b7ris", "ist", "von", "hin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Seh' ich Sie nicht hier,", "tokens": ["Seh'", "ich", "Sie", "nicht", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Nichts wird mich gewinnen,", "tokens": ["Nichts", "wird", "mich", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Nichts von ewrer Zier,", "tokens": ["Nichts", "von", "ew\u00b7rer", "Zier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Denn ohn Cloris kan der Pein", "tokens": ["Denn", "ohn", "Clo\u00b7ris", "kan", "der", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "VMFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Tirsis nie entladen seyn.", "tokens": ["Tir\u00b7sis", "nie", "ent\u00b7la\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}