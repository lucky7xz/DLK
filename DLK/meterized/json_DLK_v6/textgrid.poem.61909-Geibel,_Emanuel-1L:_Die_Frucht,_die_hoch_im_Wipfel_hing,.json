{"textgrid.poem.61909": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Frucht, die hoch im Wipfel hing,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Frucht, die hoch im Wipfel hing,", "tokens": ["Die", "Frucht", ",", "die", "hoch", "im", "Wip\u00b7fel", "hing", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df sie des G\u00e4rtners Blick entging,", "tokens": ["Da\u00df", "sie", "des", "G\u00e4rt\u00b7ners", "Blick", "ent\u00b7ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verkehrte lautrer nur in Saft", "tokens": ["Ver\u00b7kehr\u00b7te", "laut\u00b7rer", "nur", "in", "Saft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die eingesogne Sonnenkraft", "tokens": ["Die", "ein\u00b7ge\u00b7sog\u00b7ne", "Son\u00b7nen\u00b7kraft"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und ward, wie sie zu oberst schwoll,", "tokens": ["Und", "ward", ",", "wie", "sie", "zu", "o\u00b7berst", "schwoll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "PWAV", "PPER", "APPR", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zwief\u00e4ltig edler S\u00fc\u00dfe voll,", "tokens": ["Zwie\u00b7f\u00e4l\u00b7tig", "ed\u00b7ler", "S\u00fc\u00b7\u00dfe", "voll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Goldball, von des Herbstes Luft", "tokens": ["Ein", "Gold\u00b7ball", ",", "von", "des", "Herbs\u00b7tes", "Luft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Noch \u00fcberhaucht mit Purpurduft.", "tokens": ["Noch", "\u00fc\u00b7ber\u00b7haucht", "mit", "Pur\u00b7pur\u00b7duft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zuletzt im leisen Windeswallen", "tokens": ["Zu\u00b7letzt", "im", "lei\u00b7sen", "Win\u00b7des\u00b7wal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Macht sie die eigne Schwere fallen.", "tokens": ["Macht", "sie", "die", "eig\u00b7ne", "Schwe\u00b7re", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Der G\u00e4rtner hebt sie auf und spricht:", "tokens": ["Der", "G\u00e4rt\u00b7ner", "hebt", "sie", "auf", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "\u00bbdie hatt' ich auch und wu\u00dft' es nicht\u00ab,", "tokens": ["\u00bb", "die", "hatt'", "ich", "auch", "und", "wu\u00dft'", "es", "nicht", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VAFIN", "PPER", "ADV", "KON", "VVFIN", "PPER", "PTKNEG", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und legt sie obenauf beim Feste", "tokens": ["Und", "legt", "sie", "o\u00b7ben\u00b7auf", "beim", "Fes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Als Zier des Mahls f\u00fcr edle G\u00e4ste.", "tokens": ["Als", "Zier", "des", "Mahls", "f\u00fcr", "ed\u00b7le", "G\u00e4s\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}