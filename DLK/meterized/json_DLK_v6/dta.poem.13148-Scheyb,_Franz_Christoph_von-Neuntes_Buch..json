{"dta.poem.13148": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ethalia komm hervor! ich wei\u00df du hast gesehn,", "tokens": ["\u201e", "tha\u00b7lia", "komm", "her\u00b7vor", "!", "ich", "wei\u00df", "du", "hast", "ge\u00b7sehn", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "\u201ewie, wo, mit was vor Pomp desselben Theile stehn.", "tokens": ["\u201e", "wie", ",", "wo", ",", "mit", "was", "vor", "Pomp", "des\u00b7sel\u00b7ben", "Thei\u00b7le", "stehn", "."], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$,", "PWAV", "$,", "APPR", "PRELS", "APPR", "NN", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ebeschreib uns das Geb\u00e4u mit allen Kostbarkeiten;", "tokens": ["\u201e", "be\u00b7schreib", "uns", "das", "Ge\u00b7b\u00e4u", "mit", "al\u00b7len", "Kost\u00b7bar\u00b7kei\u00b7ten", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ewie Kunst, Natur und Pracht dort um den Vorzug streiten.", "tokens": ["\u201e", "wie", "Kunst", ",", "Na\u00b7tur", "und", "Pracht", "dort", "um", "den", "Vor\u00b7zug", "strei\u00b7ten", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "NN", "$,", "NN", "KON", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201ekomm! wiederhohl uns jezt, was du mir vorgebracht;", "tokens": ["\u201e", "komm", "!", "wie\u00b7der\u00b7hohl", "uns", "jezt", ",", "was", "du", "mir", "vor\u00b7ge\u00b7bracht", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "KOUS", "PPER", "ADV", "$,", "PWS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "230\u201dIch finde, da\u00df der Bau von dir wohl ausgedacht.", "tokens": ["\"", "Ich", "fin\u00b7de", ",", "da\u00df", "der", "Bau", "von", "dir", "wohl", "aus\u00b7ge\u00b7dacht", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201efreundinnen! gebt es zu! die Me\u00df-Kunst ihrer Augen", "tokens": ["\u201e", "freun\u00b7din\u00b7nen", "!", "gebt", "es", "zu", "!", "die", "Me\u00df\u00b7Kunst", "ih\u00b7rer", "Au\u00b7gen"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVINF", "$.", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "NN", "PPOSAT", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.8": {"text": "\u201ewird uns zu diesem Werck am allerbesten taugen.", "tokens": ["\u201e", "wird", "uns", "zu", "die\u00b7sem", "Werck", "am", "al\u00b7ler\u00b7bes\u00b7ten", "tau\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "APPR", "PDAT", "NN", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}