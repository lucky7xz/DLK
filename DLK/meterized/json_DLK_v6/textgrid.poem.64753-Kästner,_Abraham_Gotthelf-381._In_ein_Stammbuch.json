{"textgrid.poem.64753": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "381. In ein Stammbuch", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So wie wir aus der Kinder Thaten", "tokens": ["So", "wie", "wir", "aus", "der", "Kin\u00b7der", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der reifern Jahre Trieb errathen,", "tokens": ["Der", "rei\u00b7fern", "Jah\u00b7re", "Trieb", "er\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So pr\u00fcft uns Gott in dieser Welt:", "tokens": ["So", "pr\u00fcft", "uns", "Gott", "in", "die\u00b7ser", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hier l\u00e4\u00dft er uns noch Spiele w\u00e4hlen,", "tokens": ["Hier", "l\u00e4\u00dft", "er", "uns", "noch", "Spie\u00b7le", "w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bis einstens den erwachsnen Seelen", "tokens": ["Bis", "eins\u00b7tens", "den", "er\u00b7wachs\u00b7nen", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Puppe selbst nicht mehr gef\u00e4llt.", "tokens": ["Die", "Pup\u00b7pe", "selbst", "nicht", "mehr", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}