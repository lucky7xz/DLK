{"textgrid.poem.53067": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Albert Linemann und Anna Gericke", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frewd ohn einiges entsetzen", "tokens": ["Frewd", "ohn", "ei\u00b7ni\u00b7ges", "ent\u00b7set\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Helt jetzt vnser Vaterland,", "tokens": ["Helt", "jetzt", "vn\u00b7ser", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mars vergist sein Schwerd zu wetzen,", "tokens": ["Mars", "ver\u00b7gist", "sein", "Schwerd", "zu", "wet\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zeucht von hie mit Mord vnd Brand,", "tokens": ["Zeucht", "von", "hie", "mit", "Mord", "vnd", "Brand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gott du Schutz-Herr, giebst vns ja", "tokens": ["Gott", "du", "Schutz\u00b7Herr", ",", "giebst", "vns", "ja"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "NN", "$,", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Friede vor das wilde Kriegen,", "tokens": ["Frie\u00b7de", "vor", "das", "wil\u00b7de", "Krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Vnser K\u00f6nig Wladisla", "tokens": ["Vn\u00b7ser", "K\u00f6\u00b7nig", "Wla\u00b7dis\u00b7la"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Kan durch Macht vnd G\u00fcte siegen.", "tokens": ["Kan", "durch", "Macht", "vnd", "G\u00fc\u00b7te", "sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Vormals hab ich h\u00f6ren fragen,", "tokens": ["Vor\u00b7mals", "hab", "ich", "h\u00f6\u00b7ren", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob man sol so furchtsam sein", "tokens": ["Ob", "man", "sol", "so", "furcht\u00b7sam", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VMFIN", "ADV", "ADJD", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd dem Freyen gantz entsagen,", "tokens": ["Vnd", "dem", "Frey\u00b7en", "gantz", "ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fragt man jetzt noch also? Nein,", "tokens": ["Fragt", "man", "jetzt", "noch", "al\u00b7so", "?", "Nein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "ADV", "$.", "PTKANT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Den nachdem der starcke Gott", "tokens": ["Den", "nach\u00b7dem", "der", "star\u00b7cke", "Gott"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Vor dem Krieg' vns wil beh\u00fcten,", "tokens": ["Vor", "dem", "Krieg'", "vns", "wil", "be\u00b7h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Was fragt Heyraht nach der Noth", "tokens": ["Was", "fragt", "Hey\u00b7raht", "nach", "der", "Noth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "NN", "APPR", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Vnd der Krieges-Gurgeln w\u00fcten?", "tokens": ["Vnd", "der", "Krie\u00b7ge\u00b7sGur\u00b7geln", "w\u00fc\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Auch die Ruh (die Gott nie wende)", "tokens": ["Auch", "die", "Ruh", "(", "die", "Gott", "nie", "wen\u00b7de", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$(", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fordert ja des Mannes Schutz,", "tokens": ["For\u00b7dert", "ja", "des", "Man\u00b7nes", "Schutz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Er des Weibes H\u00fclff vnd H\u00e4nde", "tokens": ["Er", "des", "Wei\u00b7bes", "H\u00fclff", "vnd", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seinem Haus vnd Herdt zu nutz.", "tokens": ["Sei\u00b7nem", "Haus", "vnd", "Herdt", "zu", "nutz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wol nun (das Gott noch bescher')", "tokens": ["Wol", "nun", "(", "das", "Gott", "noch", "be\u00b7scher'", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "ART", "NN", "ADV", "ADJD", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Euch, die Ihr zuvor genommen", "tokens": ["Euch", ",", "die", "Ihr", "zu\u00b7vor", "ge\u00b7nom\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Eine, die Euch jetzund wer'", "tokens": ["Ei\u00b7ne", ",", "die", "Euch", "je\u00b7tzund", "wer'"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "PRELS", "PPER", "ADV", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "An die Seite nimmer kommen.", "tokens": ["An", "die", "Sei\u00b7te", "nim\u00b7mer", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wehe dem, der stets alleine", "tokens": ["We\u00b7he", "dem", ",", "der", "stets", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "$,", "PRELS", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vnd noch ferner Einsam lebt,", "tokens": ["Vnd", "noch", "fer\u00b7ner", "Ein\u00b7sam", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fellet Er, so ist je keine,", "tokens": ["Fel\u00b7let", "Er", ",", "so", "ist", "je", "kei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VAFIN", "ADV", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Ihn tr\u00f6stet vnd auffhebt,", "tokens": ["Die", "Ihn", "tr\u00f6s\u00b7tet", "vnd", "auff\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auch wen eins zum andern kreucht", "tokens": ["Auch", "wen", "eins", "zum", "an\u00b7dern", "kreucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWS", "PIS", "APPRART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Vmbgeschrenckt mit Hertz vnd Armen,", "tokens": ["Vmb\u00b7ge\u00b7schrenckt", "mit", "Hertz", "vnd", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wermen sie sich, wer dies fleucht,", "tokens": ["Wer\u00b7men", "sie", "sich", ",", "wer", "dies", "fleucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "$,", "PWS", "PDS", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Wird so leichte nicht erwarmen.", "tokens": ["Wird", "so", "leich\u00b7te", "nicht", "er\u00b7war\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJA", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Kompt, Herr Breutgam, bey zu pflichten", "tokens": ["Kompt", ",", "Herr", "Breut\u00b7gam", ",", "bey", "zu", "pflich\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "NE", "$,", "APPR", "PTKZU", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Meiner Meinung, niemand lacht,", "tokens": ["Mei\u00b7ner", "Mei\u00b7nung", ",", "nie\u00b7mand", "lacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Niemand wird in dem Euch richten,", "tokens": ["Nie\u00b7mand", "wird", "in", "dem", "Euch", "rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wessen Ihr euch habt bedacht,", "tokens": ["Wes\u00b7sen", "Ihr", "euch", "habt", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gott, der solchen Handel f\u00fchrt,", "tokens": ["Gott", ",", "der", "sol\u00b7chen", "Han\u00b7del", "f\u00fchrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird Ihm dies dazu verleihen,", "tokens": ["Wird", "Ihm", "dies", "da\u00b7zu", "ver\u00b7lei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PDS", "PAV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Das Ein jeder k\u00fcnfftig sp\u00fcrt,", "tokens": ["Das", "Ein", "je\u00b7der", "k\u00fcnff\u00b7tig", "sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "PIS", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie sein niemand sol gerewen.", "tokens": ["Wie", "sein", "nie\u00b7mand", "sol", "ge\u00b7re\u00b7wen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAINF", "PIS", "VMFIN", "VVPP", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Fahrt nur fort, liebt die bestendig,", "tokens": ["Fahrt", "nur", "fort", ",", "liebt", "die", "bes\u00b7ten\u00b7dig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$,", "VVFIN", "ART", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Die wol liebens w\u00fcrdig ist,", "tokens": ["Die", "wol", "lie\u00b7bens", "w\u00fcr\u00b7dig", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Werdet nicht von Gott abwendig,", "tokens": ["Wer\u00b7det", "nicht", "von", "Gott", "ab\u00b7wen\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lebet wie Ihr leben m\u00fc\u00dft,", "tokens": ["Le\u00b7bet", "wie", "Ihr", "le\u00b7ben", "m\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tragt das Vngl\u00fcck, wenn es beist,", "tokens": ["Tragt", "das", "Vn\u00b7gl\u00fcck", ",", "wenn", "es", "beist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur mit m\u00fchtigen geberden,", "tokens": ["Nur", "mit", "m\u00fch\u00b7ti\u00b7gen", "ge\u00b7ber\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Was die Eh' an leidt beschleust,", "tokens": ["Was", "die", "Eh'", "an", "leidt", "be\u00b7schleust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPR", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wird zu lauter Frewd Euch werden.", "tokens": ["Wird", "zu", "lau\u00b7ter", "Frewd", "Euch", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIAT", "NN", "PPER", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}