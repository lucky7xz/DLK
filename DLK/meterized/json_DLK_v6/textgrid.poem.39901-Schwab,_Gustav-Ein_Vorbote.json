{"textgrid.poem.39901": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Ein Vorbote", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im ", "tokens": ["Im"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Zu Rom die K\u00fcnstler plaudernd.", "tokens": ["Zu", "Rom", "die", "K\u00fcnst\u00b7ler", "plau\u00b7dernd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Th\u00fcr sich in der Angel dreht,", "tokens": ["Die", "Th\u00fcr", "sich", "in", "der", "An\u00b7gel", "dreht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Diener naht sich schaudernd.", "tokens": ["Ein", "Die\u00b7ner", "naht", "sich", "schau\u00b7dernd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbwoher noch, Mensch, so bleich und stumm?", "tokens": ["\u00bb", "wo\u00b7her", "noch", ",", "Mensch", ",", "so", "bleich", "und", "stumm", "?"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADV", "$,", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist Mord los, oder Feuer?\u00ab \u2013", "tokens": ["Ist", "Mord", "los", ",", "o\u00b7der", "Feu\u00b7er", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["VAFIN", "NN", "PTKVZ", "$,", "KON", "NN", "$.", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbherr! in ", "tokens": ["\u00bb", "herr", "!", "in"], "token_info": ["punct", "word", "punct", "word"], "pos": ["$(", "PTKVZ", "$.", "APPR"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Dort ist es nicht geheuer!\u00ab", "tokens": ["Dort", "ist", "es", "nicht", "ge\u00b7heu\u00b7er", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADJD", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und bei dem Namen \u2013 wei\u00df nicht wie \u2013", "tokens": ["Und", "bei", "dem", "Na\u00b7men", "\u2013", "wei\u00df", "nicht", "wie", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$(", "VVFIN", "PTKNEG", "KOKOM", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Herzen ernster schlagen;", "tokens": ["Die", "Her\u00b7zen", "erns\u00b7ter", "schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des greisen Meisters denken sie", "tokens": ["Des", "grei\u00b7sen", "Meis\u00b7ters", "den\u00b7ken", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im fernen Kopenhagen.", "tokens": ["Im", "fer\u00b7nen", "Ko\u00b7pen\u00b7ha\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbwas ist's?\u00ab \u2013 \u00bbMich f\u00fchrte sp\u00e4t am Tag", "tokens": ["\u00bb", "was", "ist's", "?", "\u00ab", "\u2013", "\u00bb", "Mich", "f\u00fchr\u00b7te", "sp\u00e4t", "am", "Tag"], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PWS", "VAFIN", "$.", "$(", "$(", "$(", "PPER", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Auftrag, Herr, zur Stelle;", "tokens": ["Ein", "Auf\u00b7trag", ",", "Herr", ",", "zur", "Stel\u00b7le", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da h\u00f6rt' ich drinnen Mei\u00dfelschlag", "tokens": ["Da", "h\u00f6rt'", "ich", "drin\u00b7nen", "Mei\u00b7\u00dfel\u00b7schlag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und rief: mach' auf, Geselle!", "tokens": ["Und", "rief", ":", "mach'", "auf", ",", "Ge\u00b7sel\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "VVFIN", "PTKVZ", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Kein Wort. Mein Schl\u00fcssel thut mir auf:", "tokens": ["Kein", "Wort", ".", "Mein", "Schl\u00fcs\u00b7sel", "thut", "mir", "auf", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Vorplatz nichts zu schauen,", "tokens": ["Im", "Vor\u00b7platz", "nichts", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch hinterm Umhang, drauf und drauf,", "tokens": ["Doch", "hin\u00b7term", "Um\u00b7hang", ",", "drauf", "und", "drauf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "$,", "PAV", "KON", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da mei\u00dfelt's, mir zum Grauen.", "tokens": ["Da", "mei\u00b7\u00dfelt's", ",", "mir", "zum", "Grau\u00b7en", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich schl\u00fcpf' hinein; der Saal ist leer,", "tokens": ["Ich", "schl\u00fcpf'", "hin\u00b7ein", ";", "der", "Saal", "ist", "leer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ganz \u00f6de, Mondenschimmer.", "tokens": ["Ganz", "\u00f6\u00b7de", ",", "Mon\u00b7den\u00b7schim\u00b7mer", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vom zweiten Vorhang schallt es her,", "tokens": ["Vom", "zwei\u00b7ten", "Vor\u00b7hang", "schallt", "es", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Heiligtum im Zimmer.", "tokens": ["Vom", "Hei\u00b7lig\u00b7tum", "im", "Zim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Dort, wo ich oft den alten Herrn", "tokens": ["Dort", ",", "wo", "ich", "oft", "den", "al\u00b7ten", "Herrn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So mutig h\u00e4mmern h\u00f6rte,", "tokens": ["So", "mu\u00b7tig", "h\u00e4m\u00b7mern", "h\u00f6r\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit Frag' und Sendung gar nicht gern", "tokens": ["Mit", "Frag'", "und", "Sen\u00b7dung", "gar", "nicht", "gern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In tiefer Arbeit st\u00f6rte.", "tokens": ["In", "tie\u00b7fer", "Ar\u00b7beit", "st\u00f6r\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich mu\u00dft' hinein \u2013 da schwieg der Laut;", "tokens": ["Ich", "mu\u00dft'", "hin\u00b7ein", "\u2013", "da", "schwieg", "der", "Laut", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$(", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch sah ich jetzt Gesichte:", "tokens": ["Doch", "sah", "ich", "jetzt", "Ge\u00b7sich\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn Bild an Bild herunterschaut", "tokens": ["Denn", "Bild", "an", "Bild", "her\u00b7un\u00b7ter\u00b7schaut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beseelt im Mondenlichte.", "tokens": ["Be\u00b7seelt", "im", "Mon\u00b7den\u00b7lich\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und Lippen r\u00fchrten hier und dort", "tokens": ["Und", "Lip\u00b7pen", "r\u00fchr\u00b7ten", "hier", "und", "dort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich, marmorne, zum Klagen,", "tokens": ["Sich", ",", "mar\u00b7mor\u00b7ne", ",", "zum", "Kla\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "$,", "VVFIN", "$,", "APPRART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Als wollten sie ein schrecklich Wort,", "tokens": ["Als", "woll\u00b7ten", "sie", "ein", "schreck\u00b7lich", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein schrecklich Wort mir sagen!", "tokens": ["Ein", "schreck\u00b7lich", "Wort", "mir", "sa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wenn Totes, Herr, lebendig wird,", "tokens": ["Wenn", "To\u00b7tes", ",", "Herr", ",", "le\u00b7ben\u00b7dig", "wird", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "$,", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So will der Tod an's Leben!", "tokens": ["So", "will", "der", "Tod", "an's", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Lufthauch zieht, ein K\u00e4uzchen schwirrt;", "tokens": ["Ein", "Luft\u00b7hauch", "zieht", ",", "ein", "K\u00e4uz\u00b7chen", "schwirrt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich eilte weg mit Beben.\u00ab", "tokens": ["Ich", "eil\u00b7te", "weg", "mit", "Be\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Nachdenklich h\u00f6rt's der K\u00fcnstlerkreis,", "tokens": ["Nach\u00b7denk\u00b7lich", "h\u00f6rt's", "der", "K\u00fcnst\u00b7ler\u00b7kreis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch zwinget Scherz das Grausen:", "tokens": ["Doch", "zwin\u00b7get", "Scherz", "das", "Grau\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbnicht mach' uns da Gespenster wei\u00df,", "tokens": ["\u00bb", "nicht", "mach'", "uns", "da", "Ge\u00b7spens\u00b7ter", "wei\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "VVFIN", "PPER", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo nur die Genien hausen!", "tokens": ["Wo", "nur", "die", "Ge\u00b7ni\u00b7en", "hau\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Hebt hoch den Kelch! sto\u00dft an mit Macht!", "tokens": ["Hebt", "hoch", "den", "Kelch", "!", "sto\u00dft", "an", "mit", "Macht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$.", "VVFIN", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Thorwaldsen lebe, lebe!", "tokens": ["Thor\u00b7wald\u00b7sen", "le\u00b7be", ",", "le\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zerrei\u00dft der abergl\u00e4ub'schen Nacht", "tokens": ["Zer\u00b7rei\u00dft", "der", "a\u00b7ber\u00b7gl\u00e4ub'\u00b7schen", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr n\u00e4rrisch Traumgewebe!\u00ab", "tokens": ["Ihr", "n\u00e4r\u00b7risch", "Traum\u00b7ge\u00b7we\u00b7be", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Des Meisters treuster Sch\u00fcler sa\u00df", "tokens": ["Des", "Meis\u00b7ters", "treus\u00b7ter", "Sch\u00fc\u00b7ler", "sa\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Allein verstummt im Bunde;", "tokens": ["Al\u00b7lein", "ver\u00b7stummt", "im", "Bun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Beiseite lie\u00df er stehn das Glas,", "tokens": ["Bei\u00b7sei\u00b7te", "lie\u00df", "er", "stehn", "das", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und merkt sich Tag und Stunde. \u2013", "tokens": ["Und", "merkt", "sich", "Tag", "und", "Stun\u00b7de", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PRF", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Und wieder \u2013 ohne Sang und Klang \u2013", "tokens": ["Und", "wie\u00b7der", "\u2013", "oh\u00b7ne", "Sang", "und", "Klang", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die K\u00fcnstler sind beisammen;", "tokens": ["Die", "K\u00fcnst\u00b7ler", "sind", "bei\u00b7sam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Fl\u00fcstern geht den Reihn entlang,", "tokens": ["Ein", "Fl\u00fcs\u00b7tern", "geht", "den", "Reihn", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Totenkerzen flammen.", "tokens": ["Und", "To\u00b7ten\u00b7ker\u00b7zen", "flam\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", 