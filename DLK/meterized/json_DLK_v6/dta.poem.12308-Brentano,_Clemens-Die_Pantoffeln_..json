{"dta.poem.12308": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Die Pantoffeln .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein M\u00e4gdlein zu dem Brunnen ging,               ", "tokens": ["Ein", "M\u00e4gd\u00b7lein", "zu", "dem", "Brun\u00b7nen", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und das war s\u00e4uberlichen", "tokens": ["Und", "das", "war", "s\u00e4u\u00b7ber\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das M\u00e4gdlein in Pantoffeln ging,", "tokens": ["Das", "M\u00e4gd\u00b7lein", "in", "Pan\u00b7tof\u00b7feln", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ganz sacht kam sie geschlichen.", "tokens": ["Ganz", "sacht", "kam", "sie", "ge\u00b7schli\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Begegnet ihr ein stolzer Knab,", "tokens": ["Be\u00b7geg\u00b7net", "ihr", "ein", "stol\u00b7zer", "Knab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der gr\u00fc\u00dft sie herziglichen,", "tokens": ["Der", "gr\u00fc\u00dft", "sie", "her\u00b7zig\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie setzt das Kr\u00fcglein neben sich,", "tokens": ["Sie", "setzt", "das", "Kr\u00fcg\u00b7lein", "ne\u00b7ben", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und fraget, wer ich w\u00e4re?", "tokens": ["Und", "fra\u00b7get", ",", "wer", "ich", "w\u00e4\u00b7re", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Weil ich ihr nicht recht schwatzen kann,", "tokens": ["Weil", "ich", "ihr", "nicht", "recht", "schwat\u00b7zen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie schneidt mir bald ein Kappen,", "tokens": ["Sie", "schneidt", "mir", "bald", "ein", "Kap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kein Tuch daran ward nicht gespart,", "tokens": ["Kein", "Tuch", "da\u00b7ran", "ward", "nicht", "ge\u00b7spart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PAV", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kann einen h\u00f6flich zwacken.", "tokens": ["Kann", "ei\u00b7nen", "h\u00f6f\u00b7lich", "zwa\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das M\u00e4gdlein von dem Brunnen geht,", "tokens": ["Das", "M\u00e4gd\u00b7lein", "von", "dem", "Brun\u00b7nen", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df traben die, la\u00df traben,", "tokens": ["La\u00df", "tra\u00b7ben", "die", ",", "la\u00df", "tra\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "ART", "$,", "VVIMP", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die vorne in Pantoffeln gehn,", "tokens": ["Die", "vor\u00b7ne", "in", "Pan\u00b7tof\u00b7feln", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+++-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Die ihnen hinten schlappen.", "tokens": ["Die", "ih\u00b7nen", "hin\u00b7ten", "schlap\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}