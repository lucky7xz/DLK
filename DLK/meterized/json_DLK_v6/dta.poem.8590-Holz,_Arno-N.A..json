{"dta.poem.8590": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unter dunklen, treibenden Novemberwolken", "tokens": ["Un\u00b7ter", "dunk\u00b7len", ",", "trei\u00b7ben\u00b7den", "No\u00b7vem\u00b7ber\u00b7wol\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "verd\u00e4mmert die Haide.", "tokens": ["ver\u00b7d\u00e4m\u00b7mert", "die", "Hai\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Geb\u00fcckt,", "tokens": ["Ge\u00b7b\u00fcckt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "am Wegrand,", "tokens": ["am", "Weg\u00b7rand", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "sitzst du und starrst", "tokens": ["sitz\u00b7st", "du", "und", "starrst"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "auf deine welken H\u00e4nde.", "tokens": ["auf", "dei\u00b7ne", "wel\u00b7ken", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Lebst du noch?", "tokens": ["Lebst", "du", "noch", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Gemartert,", "tokens": ["Ge\u00b7mar\u00b7tert", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "im Dornenstrauch,", "tokens": ["im", "Dor\u00b7nen\u00b7strauch", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "zittert ein letztes Bl\u00e4ttchen!", "tokens": ["zit\u00b7tert", "ein", "letz\u00b7tes", "Bl\u00e4tt\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}