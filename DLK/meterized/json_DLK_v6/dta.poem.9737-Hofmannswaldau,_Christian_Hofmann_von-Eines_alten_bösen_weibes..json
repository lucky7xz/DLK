{"dta.poem.9737": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines alten b\u00f6sen weibes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein sch\u00e4dlich basilisck/ ein grimmig tieger-thier/", "tokens": ["Ein", "sch\u00e4d\u00b7lich", "ba\u00b7si\u00b7lisck", "/", "ein", "grim\u00b7mig", "tie\u00b7ger\u00b7thier", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$(", "ART", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein weib/ das wie ein hund zum beissen trug begier/", "tokens": ["Ein", "weib", "/", "das", "wie", "ein", "hund", "zum", "beis\u00b7sen", "trug", "be\u00b7gier", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PDS", "KOKOM", "ART", "NN", "APPRART", "PIS", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und in dem leben hat gleich einer sau gerochen/", "tokens": ["Und", "in", "dem", "le\u00b7ben", "hat", "gleich", "ei\u00b7ner", "sau", "ge\u00b7ro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "VVINF", "VAFIN", "ADV", "ART", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die ist in dieses l\u00f6ch nur allzu sp\u00e4t gekrochen.", "tokens": ["Die", "ist", "in", "die\u00b7ses", "l\u00f6ch", "nur", "all\u00b7zu", "sp\u00e4t", "ge\u00b7kro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PDAT", "NN",