{"dta.poem.18418": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die Schweitzer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer hat von eurer faust nicht sch&#228;ndlich eingeb\u00fcst/", "tokens": ["Wer", "hat", "von", "eu\u00b7rer", "faust", "nicht", "sch", "&#228;", "nd\u00b7lich", "ein\u00b7ge\u00b7b\u00fcst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "ADJD", "$(", "ADJD", "VVPP", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Als eure mannheit sich die einfalt lie\u00df belieben?", "tokens": ["Als", "eu\u00b7re", "mann\u00b7heit", "sich", "die", "ein\u00b7falt", "lie\u00df", "be\u00b7lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Seit aber ihr erkannt/ was zinn und silber ist/", "tokens": ["Seit", "a\u00b7ber", "ihr", "er\u00b7kannt", "/", "was", "zinn", "und", "sil\u00b7ber", "ist", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPER", "VVPP", "$(", "PWS", "NN", "KON", "ADV", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So ist kein k\u00fchner Carl vor eurer faust geblieben.", "tokens": ["So", "ist", "kein", "k\u00fch\u00b7ner", "Carl", "vor", "eu\u00b7rer", "faust", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFI