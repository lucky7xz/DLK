{"dta.poem.21041": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchgesang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Aufgang flammt. Der Abend steht d&#252;sterblau.               ", "tokens": ["Der", "Auf\u00b7gang", "flammt", ".", "Der", "A\u00b7bend", "steht", "d", "&#252;", "ster\u00b7blau", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "XY", "$(", "ADJD", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Im klaren \u00c4ther schwimmet erblasst der Mond;", "tokens": ["Im", "kla\u00b7ren", "\u00c4\u00b7ther", "schwim\u00b7met", "er\u00b7blasst", "der", "Mond", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Denn schon erbl\u00fchn Aurorens Wangen,", "tokens": ["Denn", "schon", "er\u00b7bl\u00fchn", "Au\u00b7ro\u00b7rens", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Thauend und frisch, wie die deinen, Rosa!", "tokens": ["Thau\u00b7end", "und", "frisch", ",", "wie", "die", "dei\u00b7nen", ",", "Ro\u00b7sa", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "PWAV", "ART", "PPOSAT", "$,", "NE", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Erwacht, ihr Leben alle! Lobsingt dem Herrn!", "tokens": ["Er\u00b7wacht", ",", "ihr", "Le\u00b7ben", "al\u00b7le", "!", "Lob\u00b7singt", "dem", "Herrn", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "PIAT", "$.", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Lobsing' ihm, Lerche, Drossel und Nachtigall!", "tokens": ["Lob\u00b7sing'", "ihm", ",", "Ler\u00b7che", ",", "Dros\u00b7sel", "und", "Nach\u00b7ti\u00b7gall", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Br\u00fcllt euern Dank ihm, satte Heerden!", "tokens": ["Br\u00fcllt", "eu\u00b7ern", "Dank", "ihm", ",", "sat\u00b7te", "Heer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "$,", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wiehert ihm Rosse! frohlockt ihm, Adler!", "tokens": ["Wie\u00b7hert", "ihm", "Ros\u00b7se", "!", "froh\u00b7lockt", "ihm", ",", "Ad\u00b7ler", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$.", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Neig' ihm die H\u00e4upter, wallender Aehrenwald!", "tokens": ["Neig'", "ihm", "die", "H\u00e4up\u00b7ter", ",", "wal\u00b7len\u00b7der", "A\u00b7eh\u00b7ren\u00b7wald", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hauch' ihm Ger\u00fcche, thymianreiche Flur!", "tokens": ["Hauch'", "ihm", "Ge\u00b7r\u00fc\u00b7che", ",", "thy\u00b7mi\u00b7an\u00b7rei\u00b7che", "Flur", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Erduftet, Wiesen! flistert, Hayne!", "tokens": ["Er\u00b7duf\u00b7tet", ",", "Wie\u00b7sen", "!", "flis\u00b7tert", ",", "Hay\u00b7ne", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "NN", "$.", "VVFIN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rieselt, ihr Quellen, und donnre, Weltmeer!", "tokens": ["Rie\u00b7selt", ",", "ihr", "Quel\u00b7len", ",", "und", "donn\u00b7re", ",", "Welt\u00b7meer", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "$,", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Erwach' in deiner Sch\u00f6ne, Holdselige!", "tokens": ["Er\u00b7wach'", "in", "dei\u00b7ner", "Sch\u00f6\u00b7ne", ",", "Hold\u00b7se\u00b7li\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In deiner Strahlenf\u00fclle, Goldlockige!", "tokens": ["In", "dei\u00b7ner", "Strah\u00b7len\u00b7f\u00fcl\u00b7le", ",", "Gold\u00b7lo\u00b7cki\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erwach' und sch\u00fcttle, Sonne Gottes,", "tokens": ["Er\u00b7wach'", "und", "sch\u00fctt\u00b7le", ",", "Son\u00b7ne", "Got\u00b7tes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Segnend den Thau aus den goldenen Locken!", "tokens": ["Seg\u00b7nend", "den", "Thau", "aus", "den", "gol\u00b7de\u00b7nen", "Lo\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Schau, sie erwacht! Wie l\u00e4chelt die Freundliche!", "tokens": ["Schau", ",", "sie", "er\u00b7wacht", "!", "Wie", "l\u00e4\u00b7chelt", "die", "Freund\u00b7li\u00b7che", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVPP", "$.", "PWAV", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+--+--", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wie gl\u00fcht ihr Mutterantlitz! Wie liegt die Erd'", "tokens": ["Wie", "gl\u00fcht", "ihr", "Mut\u00b7ter\u00b7ant\u00b7litz", "!", "Wie", "liegt", "die", "Erd'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$.", "PWAV", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Anbetend vor ihr, schauert D\u00fcfte,", "tokens": ["An\u00b7be\u00b7tend", "vor", "ihr", ",", "schau\u00b7ert", "D\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPER", "$,", "VVFIN", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wimmert Entz\u00fccken, und jauchzet Freude!", "tokens": ["Wim\u00b7mert", "Ent\u00b7z\u00fc\u00b7cken", ",", "und", "jauch\u00b7zet", "Freu\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KON", "VVFIN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Kraft meines Geistes, schwinge dich, Ewige!", "tokens": ["Kraft", "mei\u00b7nes", "Geis\u00b7tes", ",", "schwin\u00b7ge", "dich", ",", "E\u00b7wi\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "$,", "ADJA", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Schlag meines Herzens, schlage gewaltiger!", "tokens": ["Schlag", "mei\u00b7nes", "Her\u00b7zens", ",", "schla\u00b7ge", "ge\u00b7wal\u00b7ti\u00b7ger", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "VVFIN", "ADJA", "$."], "meter": "+--+-+--+--", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Rauscht fey'rlich, meine frommen Saiten!", "tokens": ["Rauscht", "fey'\u00b7rlich", ",", "mei\u00b7ne", "from\u00b7men", "Sai\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mehret den Jubel der Morgensch\u00f6pfung!", "tokens": ["Meh\u00b7ret", "den", "Ju\u00b7bel", "der", "Mor\u00b7gen\u00b7sch\u00f6p\u00b7fung", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Wem t\u00f6nt mein unentheiligter Fr\u00fchgesang?", "tokens": ["Wem", "t\u00f6nt", "mein", "un\u00b7en\u00b7thei\u00b7lig\u00b7ter", "Fr\u00fch\u00b7ge\u00b7sang", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wem flammt die Flamme meiner entbrannten Brust?", "tokens": ["Wem", "flammt", "die", "Flam\u00b7me", "mei\u00b7ner", "ent\u00b7brann\u00b7ten", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ursch\u00f6ner dir, und deiner Sch\u00f6nheit", "tokens": ["Ur\u00b7sch\u00f6\u00b7ner", "dir", ",", "und", "dei\u00b7ner", "Sch\u00f6n\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lichtestem, holdestem, hellstem Abglanz!", "tokens": ["Lich\u00b7tes\u00b7tem", ",", "hol\u00b7des\u00b7tem", ",", "hells\u00b7tem", "Ab\u00b7glanz", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Wem weih' ich diese trauten Vergissmeinnicht?", "tokens": ["Wem", "weih'", "ich", "die\u00b7se", "trau\u00b7ten", "Ver\u00b7giss\u00b7mein\u00b7nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die erste, Unvergesslicher, weih' ich dir!", "tokens": ["Die", "ers\u00b7te", ",", "Un\u00b7ver\u00b7gess\u00b7li\u00b7cher", ",", "weih'", "ich", "dir", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJD", "$,", "KOUS", "PPER", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die zweyte deines reinen Odems", "tokens": ["Die", "zwey\u00b7te", "dei\u00b7nes", "rei\u00b7nen", "O\u00b7dems"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Reinster, unstr\u00e4flichster, sch\u00f6nster Tochter!", "tokens": ["Reins\u00b7ter", ",", "uns\u00b7tr\u00e4f\u00b7lichs\u00b7ter", ",", "sch\u00f6ns\u00b7ter", "Toch\u00b7ter", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}