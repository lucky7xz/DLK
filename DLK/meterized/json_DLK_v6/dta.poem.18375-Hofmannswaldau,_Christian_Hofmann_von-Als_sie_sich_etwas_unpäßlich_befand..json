{"dta.poem.18375": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie sich etwas unp\u00e4\u00dflich  \n befand.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Arminde/ bistu kranck? hastu dein haupt verbunden?", "tokens": ["Ar\u00b7min\u00b7de", "/", "bis\u00b7tu", "kranck", "?", "has\u00b7tu", "dein", "haupt", "ver\u00b7bun\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ADV", "ADJD", "$.", "VVFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So mu\u00df ich warlich auch mit dir im leide gehn/", "tokens": ["So", "mu\u00df", "ich", "war\u00b7lich", "auch", "mit", "dir", "im", "lei\u00b7de", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein schmertzen schl\u00e4get mir die tieffsten seelen-wunden/", "tokens": ["Dein", "schmert\u00b7zen", "schl\u00e4\u00b7get", "mir", "die", "tieffs\u00b7ten", "see\u00b7len\u00b7wun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und heist mein blut erstarrt in allen adern stehn.", "tokens": ["Und", "heist", "mein", "blut", "er\u00b7starrt", "in", "al\u00b7len", "a\u00b7dern", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "VVFIN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach! glaube deine pein/ die zwinget mich zum sterben/", "tokens": ["Ach", "!", "glau\u00b7be", "dei\u00b7ne", "pein", "/", "die", "zwin\u00b7get", "mich", "zum", "ster\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPOSAT", "NN", "$(", "ART", "VVFIN", "PPER", "APPRART", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was dir verdru\u00df erweckt/ greifft mich nicht minder an.", "tokens": ["Was", "dir", "ver\u00b7dru\u00df", "er\u00b7weckt", "/", "greifft", "mich", "nicht", "min\u00b7der", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "VVPP", "$(", "VVFIN", "PPER", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Au\u00df deinem ungemach komt endlich mein verderben;", "tokens": ["Au\u00df", "dei\u00b7nem", "un\u00b7ge\u00b7mach", "komt", "end\u00b7lich", "mein", "ver\u00b7der\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Schau wie der himmel mich so grausam straffen kan!", "tokens": ["Schau", "wie", "der", "him\u00b7mel", "mich", "so", "grau\u00b7sam", "straf\u00b7fen", "kan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "PPER", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du klagst: Dein sch\u00f6nstes haupt empfinde harte schmertzen;", "tokens": ["Du", "klagst", ":", "Dein", "sch\u00f6ns\u00b7tes", "haupt", "emp\u00b7fin\u00b7de", "har\u00b7te", "schmert\u00b7zen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "ADJA", "NN", "ADJA", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Gewi\u00df/ mein wohlergehn vernimt ietzt seinen tod/", "tokens": ["Ge\u00b7wi\u00df", "/", "mein", "woh\u00b7ler\u00b7gehn", "ver\u00b7nimt", "ietzt", "sei\u00b7nen", "tod", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PPOSAT", "VVINF", "VVFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es steigt die traurigkeit mir gar zu sehr zu hertzen/", "tokens": ["Es", "steigt", "die", "trau\u00b7rig\u00b7keit", "mir", "gar", "zu", "sehr", "zu", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "ADV", "PTKA", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und kurtz: Mein gantzes gl\u00fcck liegt in der letzten noth.", "tokens": ["Und", "kurtz", ":", "Mein", "gant\u00b7zes", "gl\u00fcck", "liegt", "in", "der", "letz\u00b7ten", "noth", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie kan es anders seyn/ in dir beruht mein leben/", "tokens": ["Wie", "kan", "es", "an\u00b7ders", "seyn", "/", "in", "dir", "be\u00b7ruht", "mein", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "VAINF", "$(", "APPR", "PPER", "VVFIN", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Trifft dich ein ungemach/ so s\u00fchl ich deine pein/", "tokens": ["Trifft", "dich", "ein", "un\u00b7ge\u00b7mach", "/", "so", "s\u00fchl", "ich", "dei\u00b7ne", "pein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJD", "$(", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Dein schmertzen kan mir nichts als bleichen kummer geben/", "tokens": ["Dein", "schmert\u00b7zen", "kan", "mir", "nichts", "als", "blei\u00b7chen", "kum\u00b7mer", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "VMFIN", "PPER", "PIS", "KOKOM", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und deine angst mischt sich mit meinem jammer ein.", "tokens": ["Und", "dei\u00b7ne", "angst", "mischt", "sich", "mit", "mei\u00b7nem", "jam\u00b7mer", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Thustu mir aber kund; dein haupt-weh sey vergangen/", "tokens": ["Thu\u00b7stu", "mir", "a\u00b7ber", "kund", ";", "dein", "haup\u00b7tweh", "sey", "ver\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "$.", "PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So f\u00fchl ich wiederum/ was lust erwecken kan/", "tokens": ["So", "f\u00fchl", "ich", "wie\u00b7de\u00b7rum", "/", "was", "lust", "er\u00b7we\u00b7cken", "kan", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "PWS", "VVFIN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So kan ich abermals mein freuden-licht empfangen", "tokens": ["So", "kan", "ich", "a\u00b7ber\u00b7mals", "mein", "freu\u00b7den\u00b7licht", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und meine seele wird mit wollust angethan.", "tokens": ["Und", "mei\u00b7ne", "see\u00b7le", "wird", "mit", "wol\u00b7lust", "an\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Es lieget nun bey dir/ du kanst mich gl\u00fccklich machen/", "tokens": ["Es", "lie\u00b7get", "nun", "bey", "dir", "/", "du", "kanst", "mich", "gl\u00fcck\u00b7lich", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "$(", "PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ein wort von deiner hand labt meinen matten geist;", "tokens": ["Ein", "wort", "von", "dei\u00b7ner", "hand", "labt", "mei\u00b7nen", "mat\u00b7ten", "geist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Geschichts/ so kan ich auch der gr\u00f6sten schmertzen lachen/", "tokens": ["Ge\u00b7schichts", "/", "so", "kan", "ich", "auch", "der", "gr\u00f6s\u00b7ten", "schmert\u00b7zen", "la\u00b7chen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADV", "VMFIN", "PPER", "ADV", "ART", "ADJA", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die mir der himmel ietzt bey deiner kranckheit weist.", "tokens": ["Die", "mir", "der", "him\u00b7mel", "ietzt", "bey", "dei\u00b7ner", "kran\u00b7ck\u00b7heit", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}}}}