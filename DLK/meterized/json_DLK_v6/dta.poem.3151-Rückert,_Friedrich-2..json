{"dta.poem.3151": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erbauen l\u00e4\u00dft sich nicht, so da\u00df sie steht und h\u00e4lt,", "tokens": ["Er\u00b7bau\u00b7en", "l\u00e4\u00dft", "sich", "nicht", ",", "so", "da\u00df", "sie", "steht", "und", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "PTKNEG", "$,", "ADV", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus epikurischen Atomen eine Welt.", "tokens": ["Aus", "e\u00b7pi\u00b7ku\u00b7ri\u00b7schen", "A\u00b7to\u00b7men", "ei\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Aus Sonnenst\u00e4ubchen ist die Sonne nicht entstanden;", "tokens": ["Aus", "Son\u00b7nen\u00b7st\u00e4ub\u00b7chen", "ist", "die", "Son\u00b7ne", "nicht", "ent\u00b7stan\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die St\u00e4ubchen sind nur, weil die Sonne scheint, vorhanden.", "tokens": ["Die", "St\u00e4ub\u00b7chen", "sind", "nur", ",", "weil", "die", "Son\u00b7ne", "scheint", ",", "vor\u00b7han\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "KOUS", "ART", "NN", "VVFIN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Viel ehr gefallen mir Leibnitzische Monaden,", "tokens": ["Viel", "ehr", "ge\u00b7fal\u00b7len", "mir", "Leib\u00b7nit\u00b7zi\u00b7sche", "Mo\u00b7na\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die eine Urmonad' unsichtbar h\u00e4lt am Faden;", "tokens": ["Die", "ei\u00b7ne", "Ur\u00b7mo\u00b7nad'", "un\u00b7sicht\u00b7bar", "h\u00e4lt", "am", "Fa\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ein Sph\u00e4renwirbel von beseelten Einzelheiten,", "tokens": ["Ein", "Sph\u00e4\u00b7ren\u00b7wir\u00b7bel", "von", "be\u00b7seel\u00b7ten", "Ein\u00b7zel\u00b7hei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie aus Bruchst\u00fccken hier sich will ein Lied bereiten.", "tokens": ["Wie", "aus", "Bruchs\u00b7t\u00fc\u00b7cken", "hier", "sich", "will", "ein", "Lied", "be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ADV", "PRF", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}