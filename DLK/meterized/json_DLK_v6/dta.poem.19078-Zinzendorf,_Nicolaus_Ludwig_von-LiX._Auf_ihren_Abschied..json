{"dta.poem.19078": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "LiX.   Auf ihren Abschied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Ejle, hie\u00df es einst bey mir, gr\u00fcsse zwey erlauchte Helden.", "tokens": ["E\u00b7jle", ",", "hie\u00df", "es", "einst", "bey", "mir", ",", "gr\u00fcs\u00b7se", "zwey", "er\u00b7lauch\u00b7te", "Hel\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,", "VVFIN", "CARD", "ADJA", "NN", "$."], "meter": "-++--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Sage diesen ", "tokens": ["Sa\u00b7ge", "die\u00b7sen"], "token_info": ["word", "word"], "pos": ["NN", "PDAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Aber, was den Seligen nimmer gnugsam anzumelden:", "tokens": ["A\u00b7ber", ",", "was", "den", "Se\u00b7li\u00b7gen", "nim\u00b7mer", "gnug\u00b7sam", "an\u00b7zu\u00b7mel\u00b7den", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "ART", "NN", "ADV", "ADJD", "VVIZU", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Dem Gecreutzigten zu dienen, sey die gr\u00f6ste F\u00fcrsten-Lust.", "tokens": ["Dem", "Ge\u00b7cr\u00b7eut\u00b7zig\u00b7ten", "zu", "die\u00b7nen", ",", "sey", "die", "gr\u00f6s\u00b7te", "F\u00fcrs\u00b7ten\u00b7Lust", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.5": {"text": "Also geht die Reise fort, ob sie gleich beschwerlich schiene,", "tokens": ["Al\u00b7so", "geht", "die", "Rei\u00b7se", "fort", ",", "ob", "sie", "gleich", "be\u00b7schwer\u00b7lich", "schie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "ADJD", "ADJA", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Eine unerkannte F\u00fchrung leitet mich nach Rudelstadt,", "tokens": ["Ei\u00b7ne", "un\u00b7er\u00b7kann\u00b7te", "F\u00fch\u00b7rung", "lei\u00b7tet", "mich", "nach", "Ru\u00b7del\u00b7stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "NE", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Da ich schleunig weiter will, werthe ", "tokens": ["Da", "ich", "schleu\u00b7nig", "wei\u00b7ter", "will", ",", "wert\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "VMFIN", "$,", "VVFIN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Hertzog Ernsts Durch laucht'ge Tochter, die daselbst den F\u00fcr-\nsten hat.", "tokens": ["Hert\u00b7zog", "Ernsts", "Durch", "laucht'\u00b7ge", "Toch\u00b7ter", ",", "die", "da\u00b7selbst", "den", "F\u00fcr", "sten", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ADJA", "NN", "$,", "PRELS", "PAV", "ART", "NN", "VVFIN", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Suchet ihr, so redet sie, Seelen, die den Heyland lieben,", "tokens": ["Su\u00b7chet", "ihr", ",", "so", "re\u00b7det", "sie", ",", "See\u00b7len", ",", "die", "den", "Hey\u00b7land", "lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "$,", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Ich bin Christian Ernstens Schwester auf die ein und an-\ndre Art,", "tokens": ["Ich", "bin", "Chris\u00b7ti\u00b7an", "Erns\u00b7tens", "Schwes\u00b7ter", "auf", "die", "ein", "und", "an", "dre", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "NE", "NN", "APPR", "PRELS", "ART", "KON", "TRUNC", "ADJA", "NN", "$,"], "meter": "-++--+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.11": {"text": "Jesus hat sein Gnaden-Werck lange schon bey mir ge-\ntrieben,", "tokens": ["Je\u00b7sus", "hat", "sein", "Gna\u00b7den\u00b7\u00b7Werck", "lan\u00b7ge", "schon", "bey", "mir", "ge", "trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "ADV", "ADV", "APPR", "PPER", "TRUNC", "VVFIN", "$,"], "meter": "--+-+-+---+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Und sich meinem armen Hertzen, als ein Freund geoffen-\nbahrt.", "tokens": ["Und", "sich", "mei\u00b7nem", "ar\u00b7men", "Hert\u00b7zen", ",", "als", "ein", "Freund", "geof\u00b7fen", "bahrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PPOSAT", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "TRUNC", "VVFIN", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.13": {"text": "Hertzogin! versetzt' ich drauf: Man hat allemal gesaget,", "tokens": ["Hert\u00b7zo\u00b7gin", "!", "ver\u00b7setzt'", "ich", "drauf", ":", "Man", "hat", "al\u00b7le\u00b7mal", "ge\u00b7sa\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "PTKVZ", "$.", "PIS", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Da\u00df die F\u00fcrstin dieses Landes eine fromme F\u00fcrstin sey;", "tokens": ["Da\u00df", "die", "F\u00fcrs\u00b7tin", "die\u00b7ses", "Lan\u00b7des", "ei\u00b7ne", "from\u00b7me", "F\u00fcrs\u00b7tin", "sey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PDAT", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Aber ich gestehe gern, da\u00df ich wenig drum gefraget,", "tokens": ["A\u00b7ber", "ich", "ge\u00b7ste\u00b7he", "gern", ",", "da\u00df", "ich", "we\u00b7nig", "drum", "ge\u00b7fra\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PIS", "PAV", "VVPP", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.16": {"text": "Wegen der bey vielen Grossen eingeri\u00dfnen Heucheley.", "tokens": ["We\u00b7gen", "der", "bey", "vie\u00b7len", "Gros\u00b7sen", "ein\u00b7ge\u00b7ri\u00df\u00b7nen", "Heu\u00b7che\u00b7ley", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "PIAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Jesus, unser Landes-HErr, forderte von seinen Strei-\ntern", "tokens": ["Je\u00b7sus", ",", "un\u00b7ser", "Lan\u00b7des\u00b7HErr", ",", "for\u00b7der\u00b7te", "von", "sei\u00b7nen", "Strei", "tern"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "TRUNC", "NN"], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Allem, allem abzusagen. Wen er anders finden wird,", "tokens": ["Al\u00b7lem", ",", "al\u00b7lem", "ab\u00b7zu\u00b7sa\u00b7gen", ".", "Wen", "er", "an\u00b7ders", "fin\u00b7den", "wird", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "VVIZU", "$.", "PWS", "PPER", "ADV", "VVINF", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.19": {"text": "Den wird zur Vergeltungs-Zeit seine Majest\u00e4t zuscheitern,", "tokens": ["Den", "wird", "zur", "Ver\u00b7gel\u00b7tungs\u00b7Zeit", "sei\u00b7ne", "Ma\u00b7jes\u00b7t\u00e4t", "zu\u00b7schei\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPRART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Ob er hier ein F\u00fcrst gewesen; oder aber nur ein Hirt.", "tokens": ["Ob", "er", "hier", "ein", "F\u00fcrst", "ge\u00b7we\u00b7sen", ";", "o\u00b7der", "a\u00b7ber", "nur", "ein", "Hirt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VAPP", "$.", "KON", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.21": {"text": "Das ist wahr! erwiederte die Durchlauchtige Sophie,", "tokens": ["Das", "ist", "wahr", "!", "er\u00b7wie\u00b7der\u00b7te", "die", "Durch\u00b7lauch\u00b7ti\u00b7ge", "So\u00b7phie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "$.", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Aber meynet ihr, da\u00df JEsus, der Geringen ihr Patron,", "tokens": ["A\u00b7ber", "mey\u00b7net", "ihr", ",", "da\u00df", "Je\u00b7sus", ",", "der", "Ge\u00b7rin\u00b7gen", "ihr", "Pat\u00b7ron", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "NE", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.23": {"text": "Nicht zuweilen auch ein Hertz aus der Zahl der Edlen ziehe,", "tokens": ["Nicht", "zu\u00b7wei\u00b7len", "auch", "ein", "Hertz", "aus", "der", "Zahl", "der", "Ed\u00b7len", "zie\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.24": {"text": "Und aus Gnaden w\u00fcrdig mache der geehrten Dornen-\nCron", "tokens": ["Und", "aus", "Gna\u00b7den", "w\u00fcr\u00b7dig", "ma\u00b7che", "der", "ge\u00b7ehr\u00b7ten", "Dor\u00b7nen", "Cron"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ADJD", "VVFIN", "ART", "ADJA", "TRUNC", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.25": {"text": "So und so erzehlte sie, hat mich seine Treu gebunden,", "tokens": ["So", "und", "so", "er\u00b7zehl\u00b7te", "sie", ",", "hat", "mich", "sei\u00b7ne", "Treu", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "VVFIN", "PPER", "$,", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.26": {"text": "Di\u00df und jenes hat mein Hertze vor und nach der Angst ge-\nf\u00fchlt,", "tokens": ["Di\u00df", "und", "je\u00b7nes", "hat", "mein", "Hert\u00b7ze", "vor", "und", "nach", "der", "Angst", "ge", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KON", "PDS", "VAFIN", "PPOSAT", "VVFIN", "PTKVZ", "KON", "APPR", "ART", "NN", "TRUNC", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.27": {"text": "Und nun ruht mein Innerstes in des theuren Heylands\nWunden,", "tokens": ["Und", "nun", "ruht", "mein", "In\u00b7ners\u00b7tes", "in", "des", "theu\u00b7ren", "Hey\u00b7lands", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.28": {"text": "Da inde\u00df die Welt nur immer nach der \u00e4ussern Ruhe zielt.", "tokens": ["Da", "in\u00b7de\u00df", "die", "Welt", "nur", "im\u00b7mer", "nach", "der", "\u00e4us\u00b7sern", "Ru\u00b7he", "zielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.29": {"text": "Drauf begab ich mich dem Hof in der Ehrenburg zu gr\u00fcssen,", "tokens": ["Drauf", "be\u00b7gab", "ich", "mich", "dem", "Hof", "in", "der", "Eh\u00b7ren\u00b7burg", "zu", "gr\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.30": {"text": "Christian Ernst, den lieben Printzen, auch von Angesicht zu\nsehn,", "tokens": ["Chris\u00b7ti\u00b7an", "Ernst", ",", "den", "lie\u00b7ben", "Print\u00b7zen", ",", "auch", "von", "An\u00b7ge\u00b7sicht", "zu", "sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "ADJA", "NN", "$,", "ADV", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.31": {"text": "Kaum, da\u00df wir uns angeblickt, lagen wir zu JEsu F\u00fcssen,", "tokens": ["Kaum", ",", "da\u00df", "wir", "uns", "an\u00b7ge\u00b7blickt", ",", "la\u00b7gen", "wir", "zu", "Je\u00b7su", "F\u00fcs\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PPER", "VVPP", "$,", "VVFIN", "PPER", "APPR", "NE", "NN", "$,"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Und verbanden uns von neuen unter seinem Creutz zu\nstehn", "tokens": ["Und", "ver\u00b7ban\u00b7den", "uns", "von", "neu\u00b7en", "un\u00b7ter", "sei\u00b7nem", "Creutz", "zu", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.33": {"text": "Ich begleitete den Printz bi\u00df in Saalfelds werthe Mauren,", "tokens": ["Ich", "be\u00b7glei\u00b7te\u00b7te", "den", "Printz", "bi\u00df", "in", "Saal\u00b7felds", "wert\u00b7he", "Mau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "APPR", "NE", "VVFIN", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.34": {"text": "Da bezeugete die F\u00fcrstin, ", "tokens": ["Da", "be\u00b7zeu\u00b7ge\u00b7te", "die", "F\u00fcrs\u00b7tin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Aber unser Aufenthalt konte hier nicht lange dauren,", "tokens": ["A\u00b7ber", "un\u00b7ser", "Auf\u00b7ent\u00b7halt", "kon\u00b7te", "hier", "nicht", "lan\u00b7ge", "dau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "ADV", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.36": {"text": "Christian Ernsts Durchlaucht'ge Schwester ruft uns unge-\ns\u00e4umt herbey.", "tokens": ["Chris\u00b7ti\u00b7an", "Ernsts", "Durch\u00b7laucht'\u00b7ge", "Schwes\u00b7ter", "ruft", "uns", "un\u00b7ge", "s\u00e4umt", "her\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.37": {"text": "Kurtz: Nach einer theuren Schrift, ", "tokens": ["Kurtz", ":", "Nach", "ei\u00b7ner", "theu\u00b7ren", "Schrift", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Eilte GOtt mit dieser Seele zu der frohen Ewigkeit.", "tokens": ["Eil\u00b7te", "Gott", "mit", "die\u00b7ser", "See\u00b7le", "zu", "der", "fro\u00b7hen", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "PDAT", "NN", "APPR", "ART", "ADJA", "NN", "