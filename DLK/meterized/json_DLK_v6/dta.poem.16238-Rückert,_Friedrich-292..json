{"dta.poem.16238": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "292.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein heller Morgen bringt dir einen guten Tag;", "tokens": ["Ein", "hel\u00b7ler", "Mor\u00b7gen", "bringt", "dir", "ei\u00b7nen", "gu\u00b7ten", "Tag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ist nun, das dir hell den Morgen machen mag?", "tokens": ["Was", "ist", "nun", ",", "das", "dir", "hell", "den", "Mor\u00b7gen", "ma\u00b7chen", "mag", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "$,", "PRELS", "PPER", "ADJD", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein froher Abend wirkt wie Zauber durch die Nacht;", "tokens": ["Ein", "fro\u00b7her", "A\u00b7bend", "wirkt", "wie", "Zau\u00b7ber", "durch", "die", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KOKOM", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sei der Morgen tr\u00fcb, doch bist du hell erwacht.", "tokens": ["Und", "sei", "der", "Mor\u00b7gen", "tr\u00fcb", ",", "doch", "bist", "du", "hell", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADJD", "$,", "ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was aber konnte dir den frohen Abend bringen?", "tokens": ["Was", "a\u00b7ber", "konn\u00b7te", "dir", "den", "fro\u00b7hen", "A\u00b7bend", "brin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df du am Tage sahst dein Treiben dir gelingen.", "tokens": ["Da\u00df", "du", "am", "Ta\u00b7ge", "sahst", "dein", "Trei\u00b7ben", "dir", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Auf hellen Morgen weist das wiederum zur\u00fcck;", "tokens": ["Auf", "hel\u00b7len", "Mor\u00b7gen", "weist", "das", "wie\u00b7de\u00b7rum", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PDS", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So aus sich selbst im Kreis entfaltet sich das Gl\u00fcck.", "tokens": ["So", "aus", "sich", "selbst", "im", "Kreis", "ent\u00b7fal\u00b7tet", "sich", "das", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "ADV", "APPRART", "NN", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "La\u00df es, einmal im Schwung, in Stocken nicht gerathen!", "tokens": ["La\u00df", "es", ",", "ein\u00b7mal", "im", "Schwung", ",", "in", "Sto\u00b7cken", "nicht", "ge\u00b7ra\u00b7then", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ADV", "APPRART", "NN", "$,", "APPR", "NN", "PTKNEG", "VVPP", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Stets Saamen tr\u00e4gt die Saat, und stets der Saame Saaten.", "tokens": ["Stets", "Saa\u00b7men", "tr\u00e4gt", "die", "Saat", ",", "und", "stets", "der", "Saa\u00b7me", "Saa\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ART", "NN", "$,", "KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}