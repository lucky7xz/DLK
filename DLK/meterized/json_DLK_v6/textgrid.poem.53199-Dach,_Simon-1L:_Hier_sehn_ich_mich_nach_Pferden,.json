{"textgrid.poem.53199": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier sehn ich mich nach Pferden,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier sehn ich mich nach Pferden,", "tokens": ["Hier", "sehn", "ich", "mich", "nach", "Pfer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wa\u00df geh ich immer an,", "tokens": ["Wa\u00df", "geh", "ich", "im\u00b7mer", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df, eh es Nacht wil werden,", "tokens": ["Da\u00df", ",", "eh", "es", "Nacht", "wil", "wer\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "NN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich heim gelangen kan?", "tokens": ["Ich", "heim", "ge\u00b7lan\u00b7gen", "kan", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ist Niemand hie zugegen,", "tokens": ["Ist", "Nie\u00b7mand", "hie", "zu\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dem die\u00df zu Hertzen f\u00e4hrt", "tokens": ["Dem", "die\u00df", "zu", "Hert\u00b7zen", "f\u00e4hrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PDS", "APPR", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vnd mich auff zweyen W\u00e4gen", "tokens": ["Vnd", "mich", "auff", "zwe\u00b7yen", "W\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nach K\u00f6nigsberg gewehrt?", "tokens": ["Nach", "K\u00f6\u00b7nigs\u00b7berg", "ge\u00b7wehrt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wirdt mich von Oppen lassen,", "tokens": ["Wirdt", "mich", "von", "Op\u00b7pen", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des wahren Adels Ruhm,", "tokens": ["Des", "wah\u00b7ren", "A\u00b7dels", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Kunst in Gunst zu fassen", "tokens": ["Der", "Kunst", "in", "Gunst", "zu", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "H\u00e4lt f\u00fcr sein Eigenthum?", "tokens": ["H\u00e4lt", "f\u00fcr", "sein", "Ei\u00b7gen\u00b7thum", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "La\u00df die\u00df anietzt, Herr, schawen", "tokens": ["La\u00df", "die\u00df", "an\u00b7ietzt", ",", "Herr", ",", "scha\u00b7wen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["VVIMP", "PDS", "VVFIN", "$,", "NN", "$,", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Vnd hilff mir eilends fort,", "tokens": ["Vnd", "hilff", "mir", "ei\u00b7lends", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "F\u00fchr, o du Pracht der Frawen,", "tokens": ["F\u00fchr", ",", "o", "du", "Pracht", "der", "Fra\u00b7wen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bey ihm f\u00fcr mich das Wort.", "tokens": ["Bey", "ihm", "f\u00fcr", "mich", "das", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich komm aus reinen Enden,", "tokens": ["Ich", "komm", "aus", "rei\u00b7nen", "En\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erschrecket nicht f\u00fcr mir,", "tokens": ["Er\u00b7schre\u00b7cket", "nicht", "f\u00fcr", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "De\u00df wil ich euch verpf\u00e4nden", "tokens": ["De\u00df", "wil", "ich", "euch", "ver\u00b7pf\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Glimpf, Ehr vnd alle Zier,", "tokens": ["Glimpf", ",", "Ehr", "vnd", "al\u00b7le", "Zier", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich wil nicht vor Euch kommen,", "tokens": ["Ich", "wil", "nicht", "vor", "Euch", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Lasst meine Bitte nur", "tokens": ["Lasst", "mei\u00b7ne", "Bit\u00b7te", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nicht seyn vnangenommen,", "tokens": ["Nicht", "seyn", "vnan\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Vnd helfft mir mit der Fuhr.", "tokens": ["Vnd", "helfft", "mir", "mit", "der", "Fuhr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ihr k\u00f6nnt ja nicht entbehren", "tokens": ["Ihr", "k\u00f6nnt", "ja", "nicht", "ent\u00b7beh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der angesteckten Stadt,", "tokens": ["Der", "an\u00b7ge\u00b7steck\u00b7ten", "Stadt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie mu\u00df euch offt gewehren", "tokens": ["Sie", "mu\u00df", "euch", "offt", "ge\u00b7weh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wa\u00df Waldaw gar nicht hat,", "tokens": ["Wa\u00df", "Wal\u00b7daw", "gar", "nicht", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "PTKNEG", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr m\u00fcsset euch nicht schewen,", "tokens": ["Ihr", "m\u00fcs\u00b7set", "euch", "nicht", "sche\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Giebt euch der Artzt wa\u00df ein,", "tokens": ["Giebt", "euch", "der", "Artzt", "wa\u00df", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wer wei\u00df, ob die Artzneyen", "tokens": ["Wer", "wei\u00df", ",", "ob", "die", "Artz\u00b7ne\u00b7yen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Gantz vnverd\u00e4chtig seyn.", "tokens": ["Gantz", "vn\u00b7ver\u00b7d\u00e4ch\u00b7tig", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sonst habt ihr mir erwiesen", "tokens": ["Sonst", "habt", "ihr", "mir", "er\u00b7wie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Gutthat also viel,", "tokens": ["Der", "Gut\u00b7that", "al\u00b7so", "viel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich sie vngepriesen", "tokens": ["Da\u00df", "ich", "sie", "vn\u00b7ge\u00b7prie\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht lassen kan noch wil.", "tokens": ["Nicht", "las\u00b7sen", "kan", "noch", "wil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "ADV", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So helfft mir ietzt auch stillen", "tokens": ["So", "helfft", "mir", "ietzt", "auch", "stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Vrsach dieser Pein,", "tokens": ["Die", "Vr\u00b7sach", "die\u00b7ser", "Pein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und wo nicht meinet Willen,", "tokens": ["Und", "wo", "nicht", "mei\u00b7net", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PTKNEG", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Doch meiner Reim allein.", "tokens": ["Doch", "mei\u00b7ner", "Reim", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}