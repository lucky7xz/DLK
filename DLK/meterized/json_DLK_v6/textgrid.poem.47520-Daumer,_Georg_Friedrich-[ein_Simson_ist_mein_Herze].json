{"textgrid.poem.47520": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ein Simson ist mein Herze]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Simson ist mein Herze;", "tokens": ["Ein", "Sim\u00b7son", "ist", "mein", "Her\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Jedwede Fessel sprengt es,", "tokens": ["Jed\u00b7we\u00b7de", "Fes\u00b7sel", "sprengt", "es", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und w\u00e4re sie von Eisen,", "tokens": ["Und", "w\u00e4\u00b7re", "sie", "von", "Ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur eine nicht \u2013 nur deine Locke nicht.", "tokens": ["Nur", "ei\u00b7ne", "nicht", "\u2013", "nur", "dei\u00b7ne", "Lo\u00b7cke", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PTKNEG", "$(", "ADV", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Darin verhaftet h\u00e4ngt es,", "tokens": ["Da\u00b7rin", "ver\u00b7haf\u00b7tet", "h\u00e4ngt", "es", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVPP", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und nicht die Kr\u00e4fte strengt es,", "tokens": ["Und", "nicht", "die", "Kr\u00e4f\u00b7te", "strengt", "es", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Schlinge zu zerrei\u00dfen,", "tokens": ["Die", "Schlin\u00b7ge", "zu", "zer\u00b7rei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die es so s\u00fc\u00df, so wunders\u00fc\u00df umflicht.", "tokens": ["Die", "es", "so", "s\u00fc\u00df", ",", "so", "wun\u00b7der\u00b7s\u00fc\u00df", "um\u00b7flicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}