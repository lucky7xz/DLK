{"textgrid.poem.66721": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Schmerz und Verkl\u00e4rung", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ging hinaus, wo sich unendlich spannte", "tokens": ["Ich", "ging", "hin\u00b7aus", ",", "wo", "sich", "un\u00b7end\u00b7lich", "spann\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PWAV", "PRF", "ADJD", "VVFIN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des Sommertages leuchtendes Gefild.", "tokens": ["Des", "Som\u00b7mer\u00b7ta\u00b7ges", "leuch\u00b7ten\u00b7des", "Ge\u00b7fild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der herbe Schmerz, der durch die Seele brannte,", "tokens": ["Der", "her\u00b7be", "Schmerz", ",", "der", "durch", "die", "See\u00b7le", "brann\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Versank in Tiefen, und mein Sinn ward mild.", "tokens": ["Ver\u00b7sank", "in", "Tie\u00b7fen", ",", "und", "mein", "Sinn", "ward", "mild", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Segler nun auf goldigheller Reise,", "tokens": ["Ein", "Seg\u00b7ler", "nun", "auf", "gol\u00b7dig\u00b7hel\u00b7ler", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Durchfahr' ich Lande, traumesklar gesehn,", "tokens": ["Durch\u00b7fahr'", "ich", "Lan\u00b7de", ",", "trau\u00b7mes\u00b7klar", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Indes vom seidenblauen Himmel leise", "tokens": ["In\u00b7des", "vom", "sei\u00b7den\u00b7blau\u00b7en", "Him\u00b7mel", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Durch die Platanen sanfte Winde wehn.", "tokens": ["Durch", "die", "Pla\u00b7ta\u00b7nen", "sanf\u00b7te", "Win\u00b7de", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.9": {"text": "Der gro\u00dfen Bl\u00e4tter flache Schatten schwanken", "tokens": ["Der", "gro\u00b7\u00dfen", "Bl\u00e4t\u00b7ter", "fla\u00b7che", "Schat\u00b7ten", "schwan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und zeichnen jeden Atemzug der Luft,", "tokens": ["Und", "zeich\u00b7nen", "je\u00b7den", "A\u00b7tem\u00b7zug", "der", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Der Esche dichtverschlungne Zweige ranken", "tokens": ["Der", "E\u00b7sche", "dicht\u00b7ver\u00b7schlung\u00b7ne", "Zwei\u00b7ge", "ran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Verschleiernd sich, ein Schirm f\u00fcr Wieg' und Gruft.", "tokens": ["Ver\u00b7schlei\u00b7ernd", "sich", ",", "ein", "Schirm", "f\u00fcr", "Wieg'", "und", "Gruft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PRF", "$,", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Aus Roseng\u00e4rten zieht auf Hauches Spuren", "tokens": ["Aus", "Ro\u00b7sen\u00b7g\u00e4r\u00b7ten", "zieht", "auf", "Hau\u00b7ches", "Spu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Der Sehnsucht zarte Gondel hoch empor,", "tokens": ["Der", "Sehn\u00b7sucht", "zar\u00b7te", "Gon\u00b7del", "hoch", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und was wir je an Lieb und Leid erfuhren,", "tokens": ["Und", "was", "wir", "je", "an", "Lieb", "und", "Leid", "er\u00b7fuh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Wird reiner Himmelskl\u00e4nge leiser Chor.", "tokens": ["Wird", "rei\u00b7ner", "Him\u00b7mels\u00b7kl\u00e4n\u00b7ge", "lei\u00b7ser", "Chor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}