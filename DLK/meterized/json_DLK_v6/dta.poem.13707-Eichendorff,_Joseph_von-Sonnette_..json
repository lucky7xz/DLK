{"dta.poem.13707": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnette .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Die Klugen, die nach Gott nicht wollten fragen,", "tokens": ["Die", "Klu\u00b7gen", ",", "die", "nach", "Gott", "nicht", "woll\u00b7ten", "fra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den heil'gen Kampf gern irdisch m\u00f6chten schlichten,", "tokens": ["Den", "heil'\u00b7gen", "Kampf", "gern", "ir\u00b7disch", "m\u00f6ch\u00b7ten", "schlich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum Tod kein Herz, nicht Lieb', sich aufzurichten,", "tokens": ["Zum", "Tod", "kein", "Herz", ",", "nicht", "Lieb'", ",", "sich", "auf\u00b7zu\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "$,", "PTKNEG", "NN", "$,", "PRF", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verzehren sich nur selbst in eitlen Klagen.", "tokens": ["Ver\u00b7zeh\u00b7ren", "sich", "nur", "selbst", "in", "eit\u00b7len", "Kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}