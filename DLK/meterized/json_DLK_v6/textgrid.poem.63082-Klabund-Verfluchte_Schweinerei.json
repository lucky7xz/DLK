{"textgrid.poem.63082": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Verfluchte Schweinerei", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verfluchte Schweinerei,", "tokens": ["Ver\u00b7fluch\u00b7te", "Schwei\u00b7ne\u00b7rei", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Als man mich machte,", "tokens": ["Als", "man", "mich", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "++-+-", "measure": "iambic.di"}, "line.3": {"text": "Da war ich nicht dabei,", "tokens": ["Da", "war", "ich", "nicht", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und meine Mutter lachte. \u2013", "tokens": ["Und", "mei\u00b7ne", "Mut\u00b7ter", "lach\u00b7te", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und als ich kam, ich dachte,", "tokens": ["Und", "als", "ich", "kam", ",", "ich", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich w\u00fcchse wie bisher,", "tokens": ["Ich", "w\u00fcch\u00b7se", "wie", "bis\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Z\u00f6g still als Wolke \u00fcberm Meer \u2013", "tokens": ["Z\u00f6g", "still", "als", "Wol\u00b7ke", "\u00fc\u00b7berm", "Meer", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KOKOM", "NE", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Aber Wolke wurde Regen,", "tokens": ["A\u00b7ber", "Wol\u00b7ke", "wur\u00b7de", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Aber Regen wurde Quelle,", "tokens": ["A\u00b7ber", "Re\u00b7gen", "wur\u00b7de", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und nun w\u00e4lze ich gewaltsam", "tokens": ["Und", "nun", "w\u00e4l\u00b7ze", "ich", "ge\u00b7walt\u00b7sam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Meine Welle", "tokens": ["Mei\u00b7ne", "Wel\u00b7le"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Hin zum Meere unaufhaltsam.", "tokens": ["Hin", "zum", "Mee\u00b7re", "un\u00b7auf\u00b7halt\u00b7sam", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Wann werd' ich wieder Wolke sein?", "tokens": ["Wann", "werd'", "ich", "wie\u00b7der", "Wol\u00b7ke", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Im Sonnenschein, im St\u00fcrmeschrein", "tokens": ["Im", "Son\u00b7nen\u00b7schein", ",", "im", "St\u00fcr\u00b7me\u00b7schrein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Hoch \u00fcber allem Volke sein?", "tokens": ["Hoch", "\u00fc\u00b7ber", "al\u00b7lem", "Vol\u00b7ke", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PIS", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}