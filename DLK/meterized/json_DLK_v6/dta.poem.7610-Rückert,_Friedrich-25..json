{"dta.poem.7610": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In unsers Herren Haus viel Knechte sind geschaart,", "tokens": ["In", "un\u00b7sers", "Her\u00b7ren", "Haus", "viel", "Knech\u00b7te", "sind", "ge\u00b7schaart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und jeder dient dem Herrn auf seine eigne Art.", "tokens": ["Und", "je\u00b7der", "dient", "dem", "Herrn", "auf", "sei\u00b7ne", "eig\u00b7ne", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Herr l\u00e4\u00dft jeden gern auf seine Weise dienen,", "tokens": ["Der", "Herr", "l\u00e4\u00dft", "je\u00b7den", "gern", "auf", "sei\u00b7ne", "Wei\u00b7se", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wei\u00df allein, wer ihm der liebste sei von ihnen.", "tokens": ["Und", "wei\u00df", "al\u00b7lein", ",", "wer", "ihm", "der", "liebs\u00b7te", "sei", "von", "ih\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "PWS", "PPER", "ART", "ADJA", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der eine dienet ihm, weil es sein Vater that,", "tokens": ["Der", "ei\u00b7ne", "die\u00b7net", "ihm", ",", "weil", "es", "sein", "Va\u00b7ter", "that", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein hausgeborner Knecht, ohn' eignen Sinn und Rath.", "tokens": ["Ein", "haus\u00b7ge\u00b7bor\u00b7ner", "Knecht", ",", "ohn'", "eig\u00b7nen", "Sinn", "und", "Rath", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der andre dienet ihm, weil einem Herren dienen", "tokens": ["Der", "and\u00b7re", "die\u00b7net", "ihm", ",", "weil", "ei\u00b7nem", "Her\u00b7ren", "die\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er eben will, und der ein guter Herr geschienen.", "tokens": ["Er", "e\u00b7ben", "will", ",", "und", "der", "ein", "gu\u00b7ter", "Herr", "ge\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "$,", "KON", "ART", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ein andrer lief vom Dienst, und ist dann wieder kommen,", "tokens": ["Ein", "an\u00b7drer", "lief", "vom", "Dienst", ",", "und", "ist", "dann", "wie\u00b7der", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPRART", "NN", "$,", "KON", "VAFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Gnaden hat der Herr ihn wieder aufgenommen.", "tokens": ["In", "Gna\u00b7den", "hat", "der", "Herr", "ihn", "wie\u00b7der", "auf\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ein andrer ist zu faul um aus dem Dienst zu laufen;", "tokens": ["Ein", "an\u00b7drer", "ist", "zu", "faul", "um", "aus", "dem", "Dienst", "zu", "lau\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PTKA", "ADJD", "APPR", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der gute Herr l\u00e4\u00dft ihn mitlaufen unterm Haufen.", "tokens": ["Der", "gu\u00b7te", "Herr", "l\u00e4\u00dft", "ihn", "mit\u00b7lau\u00b7fen", "un\u00b7term", "Hau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Der eine dient dem Herrn mit Eifer vorm Gesicht,", "tokens": ["Der", "ei\u00b7ne", "dient", "dem", "Herrn", "mit", "Ei\u00b7fer", "vorm", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "NN", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und hinter'm R\u00fccken tr\u00e4g', als s\u00e4h' der Herr da nicht.", "tokens": ["Und", "hin\u00b7ter'm", "R\u00fc\u00b7cken", "tr\u00e4g'", ",", "als", "s\u00e4h'", "der", "Herr", "da", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "$,", "KOUS", "NN", "ART", "NN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Der eine tr\u00e4gt ein Bild des Herrn vor seiner Brust,", "tokens": ["Der", "ei\u00b7ne", "tr\u00e4gt", "ein", "Bild", "des", "Herrn", "vor", "sei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Stets eingedenk des Herrn zu seyn und dienstbewust.", "tokens": ["Stets", "ein\u00b7ge\u00b7denk", "des", "Herrn", "zu", "seyn", "und", "dienst\u00b7be\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKZU", "VAINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Er hat das Bild von Holz nach seiner Kunst geschnitzt,", "tokens": ["Er", "hat", "das", "Bild", "von", "Holz", "nach", "sei\u00b7ner", "Kunst", "ge\u00b7schnitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und f\u00fchlt sich gl\u00e4ubig stolz, da\u00df er die Gunst besitzt.", "tokens": ["Und", "f\u00fchlt", "sich", "gl\u00e4u\u00b7big", "stolz", ",", "da\u00df", "er", "die", "Gunst", "be\u00b7sitzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "ADJD", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}