{"dta.poem.17859": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "102.  Das Auge und Hertze leiden nichts.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Das Hertz ist wie das Aug\u2019/ ein eintzigs gr\u00e4nelein/", "tokens": ["Das", "Hertz", "ist", "wie", "das", "Aug'", "/", "ein", "eint\u00b7zigs", "gr\u00e4\u00b7ne\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "ART", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo du\u2019s im Hertzen hast/ verurfacht dir schon Pein.", "tokens": ["Wo", "du's", "im", "Hert\u00b7zen", "hast", "/", "ver\u00b7ur\u00b7facht", "dir", "schon", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPRART", "NN", "VAFIN", "$(", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}