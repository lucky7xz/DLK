{"textgrid.poem.31418": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach schreib' auch in mein Herz / mit deines Fingers Krafft /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach schreib' auch in mein Herz / mit deines Fingers Krafft /", "tokens": ["Ach", "schreib'", "auch", "in", "mein", "Herz", "/", "mit", "dei\u00b7nes", "Fin\u00b7gers", "Krafft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$(", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der Wei\u00dfheit einigs Ziel / den hohen Gottes Willen.", "tokens": ["der", "Wei\u00df\u00b7heit", "ei\u00b7nigs", "Ziel", "/", "den", "ho\u00b7hen", "Got\u00b7tes", "Wil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "der scharffe Geist grab' aus / und Christus Blut woll f\u00fcllen", "tokens": ["der", "scharf\u00b7fe", "Geist", "grab'", "aus", "/", "und", "Chris\u00b7tus", "Blut", "woll", "f\u00fcl\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$(", "KON", "NE", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "der Lehr-Buchstaben Tieff mit w\u00fcrk-verm\u00f6gens-Safft.", "tokens": ["der", "Lehr\u00b7Buch\u00b7sta\u00b7ben", "Tieff", "mit", "w\u00fcrk\u00b7ver\u00b7m\u00f6\u00b7gens\u00b7\u00b7S\u00b7afft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.5": {"text": "Durch Thaten-Glauben werd' ihm gl\u00e4nzungs-Zier verschafft.", "tokens": ["Durch", "Tha\u00b7ten\u00b7Glau\u00b7ben", "werd'", "ihm", "gl\u00e4n\u00b7zungs\u00b7Zier", "ver\u00b7schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ach meine Lippen / lasst die Tafeln in euch h\u00fcllen:", "tokens": ["Ach", "mei\u00b7ne", "Lip\u00b7pen", "/", "lasst", "die", "Ta\u00b7feln", "in", "euch", "h\u00fcl\u00b7len", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "NN", "$(", "VVFIN", "ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "last / zuverk\u00fcnden / euch / des Herren Wort nicht stillen!", "tokens": ["last", "/", "zu\u00b7ver\u00b7k\u00fcn\u00b7den", "/", "euch", "/", "des", "Her\u00b7ren", "Wort", "nicht", "stil\u00b7len", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVINF", "$(", "PPER", "$(", "ART", "NN", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "sein' Ehrbekandtnu\u00df / wie in Steinen in euch hafft!", "tokens": ["sein'", "Ehr\u00b7be\u00b7kandt\u00b7nu\u00df", "/", "wie", "in", "Stei\u00b7nen", "in", "euch", "hafft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "KOKOM", "APPR", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "da\u00df meine Zunge doch m\u00f6cht eine Glutkohl werden /", "tokens": ["da\u00df", "mei\u00b7ne", "Zun\u00b7ge", "doch", "m\u00f6cht", "ei\u00b7ne", "Glut\u00b7kohl", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VMFIN", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "in andre Herzen schrieb' auch diesen Heiles Bund!", "tokens": ["in", "and\u00b7re", "Her\u00b7zen", "schrieb'", "auch", "die\u00b7sen", "Hei\u00b7les", "Bund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADV", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "mein' Hand schreib Gottes Ruhm / so lang sie lebt auf Erden!", "tokens": ["mein'", "Hand", "schreib", "Got\u00b7tes", "Ruhm", "/", "so", "lang", "sie", "lebt", "auf", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "NN", "$(", "ADV", "ADJD", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "kurz was nur an mir ist / mach Gottes Warheit kund!", "tokens": ["kurz", "was", "nur", "an", "mir", "ist", "/", "mach", "Got\u00b7tes", "War\u00b7heit", "kund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PWS", "ADV", "APPR", "PPER", "VAFIN", "$(", "VVFIN", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "denn dein Wort wissen / thun / und in die Welt ausbreiten:", "tokens": ["denn", "dein", "Wort", "wis\u00b7sen", "/", "thun", "/", "und", "in", "die", "Welt", "aus\u00b7brei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$(", "VVINF", "$(", "KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "war / ist / und bleibt mein Ziel / in allen F\u00e4ll-und Zeiten!", "tokens": ["war", "/", "ist", "/", "und", "bleibt", "mein", "Ziel", "/", "in", "al\u00b7len", "F\u00e4ll\u00b7\u00b7und", "Zei\u00b7ten", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "VAFIN", "$(", "KON", "VVFIN", "PPOSAT", "NN", "$(", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}