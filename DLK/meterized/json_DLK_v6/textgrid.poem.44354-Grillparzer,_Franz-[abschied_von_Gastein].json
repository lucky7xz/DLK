{"textgrid.poem.44354": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[abschied von Gastein]", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Trennungsstunde schl\u00e4gt und ich mu\u00df scheiden,", "tokens": ["Die", "Tren\u00b7nungs\u00b7stun\u00b7de", "schl\u00e4gt", "und", "ich", "mu\u00df", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So leb denn wohl, mein freundliches Gastein!", "tokens": ["So", "leb", "denn", "wohl", ",", "mein", "freund\u00b7li\u00b7ches", "Gas\u00b7tein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Du Tr\u00f6sterin so mancher bittrer Leiden,", "tokens": ["Du", "Tr\u00f6s\u00b7te\u00b7rin", "so", "man\u00b7cher", "bit\u00b7trer", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auch meine Leiden wiegtest du mir ein.", "tokens": ["Auch", "mei\u00b7ne", "Lei\u00b7den", "wieg\u00b7test", "du", "mir", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was Gott mir gab, worum sie mich beneiden,", "tokens": ["Was", "Gott", "mir", "gab", ",", "wo\u00b7rum", "sie", "mich", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PPER", "VVFIN", "$,", "PWAV", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und was der Quell doch ist von meiner Pein,", "tokens": ["Und", "was", "der", "Quell", "doch", "ist", "von", "mei\u00b7ner", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADV", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Qualen Grund, die wenige ermessen,", "tokens": ["Der", "Qua\u00b7len", "Grund", ",", "die", "we\u00b7ni\u00b7ge", "er\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Du lie\u00dfest michs auf kurze Zeit vergessen.", "tokens": ["Du", "lie\u00b7\u00dfest", "michs", "auf", "kur\u00b7ze", "Zeit", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn wie der Baum, auf den der Blitz gefallen,", "tokens": ["Denn", "wie", "der", "Baum", ",", "auf", "den", "der", "Blitz", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "APPR", "ART", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit einemmale strahlend sich verkl\u00e4rt,", "tokens": ["Mit", "ei\u00b7nem\u00b7ma\u00b7le", "strah\u00b7lend", "sich", "ver\u00b7kl\u00e4rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Rings h\u00f6rst du der Verwundrung Ruf erschallen,", "tokens": ["Rings", "h\u00f6rst", "du", "der", "Ver\u00b7wund\u00b7rung", "Ruf", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und jedes Aug ist staunend hingekehrt,", "tokens": ["Und", "je\u00b7des", "Aug", "ist", "stau\u00b7nend", "hin\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Indes in dieser Flammen gl\u00fchndem Wallen", "tokens": ["In\u00b7des", "in", "die\u00b7ser", "Flam\u00b7men", "gl\u00fchn\u00b7dem", "Wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PDAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des Stammes Mark und Leben sich verzehrt,", "tokens": ["Des", "Stam\u00b7mes", "Mark", "und", "Le\u00b7ben", "sich", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der, wie die Lohe steigt vom gl\u00fchnden Herde,", "tokens": ["Der", ",", "wie", "die", "Lo\u00b7he", "steigt", "vom", "gl\u00fchn\u00b7den", "Her\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Um desto tiefer niedersinkt zur Erde.", "tokens": ["Um", "des\u00b7to", "tie\u00b7fer", "nie\u00b7der\u00b7sinkt", "zur", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADJD", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und wie die Perlen, die die Sch\u00f6nheit schm\u00fccken,", "tokens": ["Und", "wie", "die", "Per\u00b7len", ",", "die", "die", "Sch\u00f6n\u00b7heit", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Wasserreiches wasserhelle Zier,", "tokens": ["Des", "Was\u00b7ser\u00b7rei\u00b7ches", "was\u00b7ser\u00b7hel\u00b7le", "Zier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Finder, nicht die Geberin begl\u00fccken,", "tokens": ["Den", "Fin\u00b7der", ",", "nicht", "die", "Ge\u00b7be\u00b7rin", "be\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das freudenlose, stille Muscheltier,", "tokens": ["Das", "freu\u00b7den\u00b7lo\u00b7se", ",", "stil\u00b7le", "Mu\u00b7schel\u00b7tier", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Denn Krankheit nur und lange Qual entr\u00fccken", "tokens": ["Denn", "Krank\u00b7heit", "nur", "und", "lan\u00b7ge", "Qual", "ent\u00b7r\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das hei\u00dfgesuchte, traurge Kleinod ihr,", "tokens": ["Das", "hei\u00df\u00b7ge\u00b7such\u00b7te", ",", "traur\u00b7ge", "Klei\u00b7nod", "ihr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und was euch so entz\u00fcckt mit seinen Strahlen,", "tokens": ["Und", "was", "euch", "so", "ent\u00b7z\u00fcckt", "mit", "sei\u00b7nen", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Es ward erzeugt in Todesnot und Qualen.", "tokens": ["Es", "ward", "er\u00b7zeugt", "in", "To\u00b7des\u00b7not", "und", "Qua\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wie der Wasserfall, des lautes Wogen", "tokens": ["Und", "wie", "der", "Was\u00b7ser\u00b7fall", ",", "des", "lau\u00b7tes", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Gegend f\u00fcllt mit Nebel und Getos,", "tokens": ["Die", "Ge\u00b7gend", "f\u00fcllt", "mit", "Ne\u00b7bel", "und", "Ge\u00b7tos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf seinem Busen ruht der Regenbogen,", "tokens": ["Auf", "sei\u00b7nem", "Bu\u00b7sen", "ruht", "der", "Re\u00b7gen\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Diamanten sch\u00fctteln rings sich los;", "tokens": ["Und", "Di\u00b7a\u00b7man\u00b7ten", "sch\u00fct\u00b7teln", "rings", "sich", "los", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er w\u00e4re gern im stillen Tal gezogen,", "tokens": ["Er", "w\u00e4\u00b7re", "gern", "im", "stil\u00b7len", "Tal", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gleich seinen Br\u00fcdern in der Wiesen Scho\u00df.", "tokens": ["Gleich", "sei\u00b7nen", "Br\u00fc\u00b7dern", "in", "der", "Wie\u00b7sen", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Klippen, die sich ihm entgegensetzen,", "tokens": ["Die", "Klip\u00b7pen", ",", "die", "sich", "ihm", "ent\u00b7ge\u00b7gen\u00b7set\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "PPER", "VVINF", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Versch\u00f6nen ihn, indem sie ihn verletzen.", "tokens": ["Ver\u00b7sch\u00f6\u00b7nen", "ihn", ",", "in\u00b7dem", "sie", "ihn", "ver\u00b7let\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der Dichter so, ob hoch vom Gl\u00fcck getragen,", "tokens": ["Der", "Dich\u00b7ter", "so", ",", "ob", "hoch", "vom", "Gl\u00fcck", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KOUS", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Umjubelt von des Beifalls lautem Schall,", "tokens": ["Um\u00b7ju\u00b7belt", "von", "des", "Bei\u00b7falls", "lau\u00b7tem", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er ist der welke Baum, vom Blitz geschlagen,", "tokens": ["Er", "ist", "der", "wel\u00b7ke", "Baum", ",", "vom", "Blitz", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das arme Muscheltier, der Wasserfall.", "tokens": ["Das", "ar\u00b7me", "Mu\u00b7schel\u00b7tier", ",", "der", "Was\u00b7ser\u00b7fall", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was ihr f\u00fcr Lieder haltet; es sind Klagen,", "tokens": ["Was", "ihr", "f\u00fcr", "Lie\u00b7der", "hal\u00b7tet", ";", "es", "sind", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VVFIN", "$.", "PPER", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gesprochen in ein freudenloses All,", "tokens": ["Ge\u00b7spro\u00b7chen", "in", "ein", "freu\u00b7den\u00b7lo\u00b7ses", "All", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und Flammen, Perlen, Schmuck, die euch umschweben,", "tokens": ["Und", "Flam\u00b7men", ",", "Per\u00b7len", ",", "Schmuck", ",", "die", "euch", "um\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "$,", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Gel\u00f6ste Teile sinds von ", "tokens": ["Ge\u00b7l\u00f6s\u00b7te", "Tei\u00b7le", "sinds", "von"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}