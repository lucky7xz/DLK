{"textgrid.poem.39704": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Freiheit ward einst C\u00e4sar hingeschlachtet,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Freiheit ward einst C\u00e4sar hingeschlachtet,", "tokens": ["Der", "Frei\u00b7heit", "ward", "einst", "C\u00e4\u00b7sar", "hin\u00b7ge\u00b7schlach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In seines Ruhmes, seines Lebens F\u00fclle.", "tokens": ["In", "sei\u00b7nes", "Ruh\u00b7mes", ",", "sei\u00b7nes", "Le\u00b7bens", "F\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Brutus schreitet zu dem hohen Ziele,", "tokens": ["Und", "Bru\u00b7tus", "schrei\u00b7tet", "zu", "dem", "ho\u00b7hen", "Zie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das zu erfassen er so sehnlich trachtet;", "tokens": ["Das", "zu", "er\u00b7fas\u00b7sen", "er", "so", "sehn\u00b7lich", "trach\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKZU", "VVFIN", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch bald wird es von Dunkel ihm umnachtet,", "tokens": ["Doch", "bald", "wird", "es", "von", "Dun\u00b7kel", "ihm", "um\u00b7nach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Es schwankt sein Gl\u00fcck in solchem k\u00fchnen Spiele,", "tokens": ["Es", "schwankt", "sein", "Gl\u00fcck", "in", "sol\u00b7chem", "k\u00fch\u00b7nen", "Spie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch ringt er muthig noch nach seinem Ziele", "tokens": ["Doch", "ringt", "er", "mut\u00b7hig", "noch", "nach", "sei\u00b7nem", "Zie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Bis zu dem Tode, den er stolz verachtet.", "tokens": ["Bis", "zu", "dem", "To\u00b7de", ",", "den", "er", "stolz", "ver\u00b7ach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn freudiger als einst in C\u00e4sars Seite", "tokens": ["Denn", "freu\u00b7di\u00b7ger", "als", "einst", "in", "C\u00e4\u00b7sars", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "ADV", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Senkt Brutus Dolch in Brutus Busen sich", "tokens": ["Senkt", "Bru\u00b7tus", "Dolch", "in", "Bru\u00b7tus", "Bu\u00b7sen", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "NE", "APPR", "NE", "NE", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und sterbend erst wird Freiheit seine Beute.", "tokens": ["Und", "ster\u00b7bend", "erst", "wird", "Frei\u00b7heit", "sei\u00b7ne", "Beu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VAFIN", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So opferte der Freiheit seinem Gotte", "tokens": ["So", "op\u00b7fer\u00b7te", "der", "Frei\u00b7heit", "sei\u00b7nem", "Got\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Ein wahrer Priester, Brutus selber, sich,", "tokens": ["Ein", "wah\u00b7rer", "Pries\u00b7ter", ",", "Bru\u00b7tus", "sel\u00b7ber", ",", "sich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NE", "ADV", "$,", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Doch wer ihm stirbt, der lebt in seinem Gotte.", "tokens": ["Doch", "wer", "ihm", "stirbt", ",", "der", "lebt", "in", "sei\u00b7nem", "Got\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}