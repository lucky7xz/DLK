{"dta.poem.21188": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Heil euch! ihr Wanderer!", "tokens": ["Heil", "euch", "!", "ihr", "Wan\u00b7de\u00b7rer", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPOSAT", "NN", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.2": {"text": "Die Andacht, die den Pilgerstab", "tokens": ["Die", "An\u00b7dacht", ",", "die", "den", "Pil\u00b7ger\u00b7stab"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu dieser Reis\u2019 euch gab,", "tokens": ["Zu", "die\u00b7ser", "Reis'", "euch", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hat aus dem Sturm der Welt auch mich hieher begleitet.", "tokens": ["Hat", "aus", "dem", "Sturm", "der", "Welt", "auch", "mich", "hie\u00b7her", "be\u00b7glei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "NN", "ADV", "PPER", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nicht tr\u00e4ger M\u00fc\u00dfiggang hat zur Einsiedeley", "tokens": ["Nicht", "tr\u00e4\u00b7ger", "M\u00fc\u00b7\u00dfig\u00b7gang", "hat", "zur", "Ein\u00b7sie\u00b7de\u00b7ley"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJA", "NN", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Voll Eigenliebe mich geleitet;", "tokens": ["Voll", "Ei\u00b7gen\u00b7lie\u00b7be", "mich", "ge\u00b7lei\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mein j\u00fcngers Leben flo\u00df nicht ungen\u00fctzt vorbey;", "tokens": ["Mein", "j\u00fcn\u00b7gers", "Le\u00b7ben", "flo\u00df", "nicht", "un\u00b7ge\u00b7n\u00fctzt", "vor\u00b7bey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PTKNEG", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch da ich meine Jugend", "tokens": ["Doch", "da", "ich", "mei\u00b7ne", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Dem Dienst der Welt geweiht,", "tokens": ["Dem", "Dienst", "der", "Welt", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "So hoft ich, w\u00fcrde mir der Himmel es vergeben,", "tokens": ["So", "hoft", "ich", ",", "w\u00fcr\u00b7de", "mir", "der", "Him\u00b7mel", "es", "ver\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VAFIN", "PPER", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "In dieser wilden Einsamkeit", "tokens": ["In", "die\u00b7ser", "wil\u00b7den", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Mein Alter ihm allein zu leben.", "tokens": ["Mein", "Al\u00b7ter", "ihm", "al\u00b7lein", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Bequemlichkeit und falsches Gl\u00fcck", "tokens": ["Be\u00b7quem\u00b7lich\u00b7keit", "und", "fal\u00b7sches", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Des vorgen Lebens, hilft die Gnade mir vergessen;", "tokens": ["Des", "vor\u00b7gen", "Le\u00b7bens", ",", "hilft", "die", "Gna\u00b7de", "mir", "ver\u00b7ges\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sie lispelt mir wahrhafte Ruh", "tokens": ["Sie", "lis\u00b7pelt", "mir", "wahr\u00b7haf\u00b7te", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Jm Schatten rauschender Cypressen", "tokens": ["Jm", "Schat\u00b7ten", "rau\u00b7schen\u00b7der", "Cyp\u00b7res\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Mitleidig zu.", "tokens": ["Mit\u00b7lei\u00b7dig", "zu", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "Der Wald, der diese H\u00f6hle", "tokens": ["Der", "Wald", ",", "der", "die\u00b7se", "H\u00f6h\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Mit dunklen Zweigen \u00fcberh\u00e4ngt,", "tokens": ["Mit", "dunk\u00b7len", "Zwei\u00b7gen", "\u00fc\u00b7ber\u00b7h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Beschirmet meine Seele", "tokens": ["Be\u00b7schir\u00b7met", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Mit einer einsamen best\u00e4ndgen Nacht", "tokens": ["Mit", "ei\u00b7ner", "ein\u00b7sa\u00b7men", "be\u00b7st\u00e4nd\u00b7gen", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Vor der Zerstreuung Macht.", "tokens": ["Vor", "der", "Zer\u00b7streu\u00b7ung", "Macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.23": {"text": "Jhr steht mit mir auf Golgatha,", "tokens": ["Ihr", "steht", "mit", "mir", "auf", "Gol\u00b7ga\u00b7tha", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Hier, wo der Thaten gr\u00f6\u00dfeste geschehen,", "tokens": ["Hier", ",", "wo", "der", "Tha\u00b7ten", "gr\u00f6\u00b7\u00dfes\u00b7te", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Die je die Welt gesehen,", "tokens": ["Die", "je", "die", "Welt", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Ob sie im Stillen gleich geschah;", "tokens": ["Ob", "sie", "im", "Stil\u00b7len", "gleich", "ge\u00b7schah", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Nicht von dem Pomp der eiteln Ehr umgeben,", "tokens": ["Nicht", "von", "dem", "Pomp", "der", "ei\u00b7teln", "Ehr", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Durch den die Menschen ihre Thaten heben.", "tokens": ["Durch", "den", "die", "Men\u00b7schen", "ih\u00b7re", "Tha\u00b7ten", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Hier starb ein Gott! \u2014 ein Gott, der f\u00fcr uns S\u00fcn-", "tokens": ["Hier", "starb", "ein", "Gott", "!", "ein", "Gott", ",", "der", "f\u00fcr", "uns", "S\u00fcn"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$(", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "TRUNC"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Ein Mensch erst ward;", "tokens": ["Ein", "Mensch", "erst", "ward", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.31": {"text": "Hier starb ein Mensch, der alle Menschenkinder", "tokens": ["Hier", "starb", "ein", "Mensch", ",", "der", "al\u00b7le", "Men\u00b7schen\u00b7kin\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "An Unschuld \u00fcbertraf!", "tokens": ["An", "Un\u00b7schuld", "\u00fc\u00b7bert\u00b7raf", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Und warum schweiget denn der Weltkrais, und die Lie-", "tokens": ["Und", "wa\u00b7rum", "schwei\u00b7get", "denn", "der", "Welt\u00b7krais", ",", "und", "die", "Lie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "VVFIN", "ADV", "ART", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Der V\u00f6lker schallen nicht um dies Gebirge wieder?", "tokens": ["Der", "V\u00f6l\u00b7ker", "schal\u00b7len", "nicht", "um", "dies", "Ge\u00b7bir\u00b7ge", "wie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "APPR", "PDS", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Warum liegt denn die weite Christenheit", "tokens": ["Wa\u00b7rum", "liegt", "denn", "die", "wei\u00b7te", "Chris\u00b7ten\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.36": {"text": "In tr\u00e4ger Unempfindlichkeit begraben?", "tokens": ["In", "tr\u00e4\u00b7ger", "Un\u00b7emp\u00b7find\u00b7lich\u00b7keit", "be\u00b7gra\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Will sie zu ihrer Dankbarkeit", "tokens": ["Will", "sie", "zu", "ih\u00b7rer", "Dank\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Mehr, als das gr\u00f6\u00dfeste von allen Wundern haben?", "tokens": ["Mehr", ",", "als", "das", "gr\u00f6\u00b7\u00dfes\u00b7te", "von", "al\u00b7len", "Wun\u00b7dern", "ha\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "ART", "ADJA", "APPR", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}