{"textgrid.poem.36605": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An den Schlaf", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du dumme Schl\u00e4frigkeit! hinweg! la\u00df mich doch trinken!", "tokens": ["Du", "dum\u00b7me", "Schl\u00e4f\u00b7rig\u00b7keit", "!", "hin\u00b7weg", "!", "la\u00df", "mich", "doch", "trin\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "PTKVZ", "$.", "VVIMP", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du nimmst von meiner Lebenszeit", "tokens": ["Du", "nimmst", "von", "mei\u00b7ner", "Le\u00b7bens\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir viel zu viel! ich seh', ich seh' die Sonne sinken,", "tokens": ["Mir", "viel", "zu", "viel", "!", "ich", "seh'", ",", "ich", "seh'", "die", "Son\u00b7ne", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKA", "PIS", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Tages Abend ist nicht weit!", "tokens": ["Des", "Ta\u00b7ges", "A\u00b7bend", "ist", "nicht", "weit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vielleicht ist auch nicht weit der Abend meines Lebens;", "tokens": ["Viel\u00b7leicht", "ist", "auch", "nicht", "weit", "der", "A\u00b7bend", "mei\u00b7nes", "Le\u00b7bens", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PTKNEG", "ADJD", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Halt, s\u00fc\u00dfer Schlaf, halt ein!", "tokens": ["Halt", ",", "s\u00fc\u00b7\u00dfer", "Schlaf", ",", "halt", "ein", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mich \u00fcberw\u00e4ltigen willst du? Es ist vergebens,", "tokens": ["Mich", "\u00fc\u00b7berw\u00b7\u00e4l\u00b7ti\u00b7gen", "willst", "du", "?", "Es", "ist", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "PPER", "$.", "PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du raubst mir Zeit und Wein!", "tokens": ["Du", "raubst", "mir", "Zeit", "und", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Tod, der st\u00e4rkre Tod, der alles \u00fcberwindet,", "tokens": ["Der", "Tod", ",", "der", "st\u00e4r\u00b7kre", "Tod", ",", "der", "al\u00b7les", "\u00fc\u00b7berw\u00b7in\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Zepter und den Hirtenstab,", "tokens": ["Den", "Zep\u00b7ter", "und", "den", "Hir\u00b7ten\u00b7stab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der, die ihn fliehen, sucht, und allzuleichte findet,", "tokens": ["Der", ",", "die", "ihn", "flie\u00b7hen", ",", "sucht", ",", "und", "all\u00b7zu\u00b7leich\u00b7te", "fin\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "PPER", "VVINF", "$,", "VVFIN", "$,", "KON", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der wirft nun bald auch mich ins Grab.", "tokens": ["Der", "wirft", "nun", "bald", "auch", "mich", "ins", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "ADV", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach! diesen Feind bewegt nicht Bitten und nicht Flehen,", "tokens": ["Ach", "!", "die\u00b7sen", "Feind", "be\u00b7wegt", "nicht", "Bit\u00b7ten", "und", "nicht", "Fle\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PDAT", "NN", "VVFIN", "PTKNEG", "NN", "KON", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nicht List und nicht Betrug,", "tokens": ["Nicht", "List", "und", "nicht", "Be\u00b7trug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "PTKNEG", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kommt er, so will ich gleich mit ihm geduldig gehen,", "tokens": ["Kommt", "er", ",", "so", "will", "ich", "gleich", "mit", "ihm", "ge\u00b7dul\u00b7dig", "ge\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VMFIN", "PPER", "ADV", "APPR", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und dann schlaf' ich genug.", "tokens": ["Und", "dann", "schlaf'", "ich", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}