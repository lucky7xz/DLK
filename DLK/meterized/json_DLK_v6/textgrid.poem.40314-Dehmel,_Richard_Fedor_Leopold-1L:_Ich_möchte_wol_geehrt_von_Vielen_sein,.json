{"textgrid.poem.40314": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich m\u00f6chte wol geehrt von Vielen sein,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich m\u00f6chte wol geehrt von Vielen sein,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "wol", "ge\u00b7ehrt", "von", "Vie\u00b7len", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVPP", "APPR", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und auch geliebt; ich wei\u00df es wohl.", "tokens": ["und", "auch", "ge\u00b7liebt", ";", "ich", "wei\u00df", "es", "wohl", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$.", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aber niemals soll", "tokens": ["A\u00b7ber", "nie\u00b7mals", "soll"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "VMFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "mein Stolz und Wert mir drum gemein", "tokens": ["mein", "Stolz", "und", "Wert", "mir", "drum", "ge\u00b7mein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "VVFIN", "PPER", "PAV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "mit hunderttausend Andern sein.", "tokens": ["mit", "hun\u00b7dert\u00b7tau\u00b7send", "An\u00b7dern", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich hab ein gro\u00dfes Vaterland,", "tokens": ["Ich", "hab", "ein", "gro\u00b7\u00dfes", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "zehn V\u00f6lkern schuldet meine Stirn", "tokens": ["zehn", "V\u00f6l\u00b7kern", "schul\u00b7det", "mei\u00b7ne", "Stirn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ihr bischen Hirn.", "tokens": ["ihr", "bi\u00b7schen", "Hirn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ich habe nie das Volk gekannt,", "tokens": ["Ich", "ha\u00b7be", "nie", "das", "Volk", "ge\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "in dem mein reinster Wert entstand.", "tokens": ["in", "dem", "mein", "reins\u00b7ter", "Wert", "ent\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "In meiner Heimat steht ein Baum,", "tokens": ["In", "mei\u00b7ner", "Hei\u00b7mat", "steht", "ein", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "den liebe ich, der steht so stolz", "tokens": ["den", "lie\u00b7be", "ich", ",", "der", "steht", "so", "stolz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "VVFIN", "PPER", "$,", "PRELS", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00fcber dem Mittelholz.", "tokens": ["\u00fc\u00b7ber", "dem", "Mit\u00b7tel\u00b7holz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Da tr\u00e4umt'ich manchen jungen Traum;", "tokens": ["Da", "tr\u00e4umt'\u00b7ich", "man\u00b7chen", "jun\u00b7gen", "Traum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "er wurzelt tief, der hohe Baum.", "tokens": ["er", "wur\u00b7zelt", "tief", ",", "der", "ho\u00b7he", "Baum", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da tr\u00e4umt'ich, da\u00df der Mensch allein,", "tokens": ["Da", "tr\u00e4umt'\u00b7ich", ",", "da\u00df", "der", "Mensch", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "von Hunderttausenden bewacht,", "tokens": ["von", "Hun\u00b7dert\u00b7tau\u00b7sen\u00b7den", "be\u00b7wacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "sich eigner macht,", "tokens": ["sich", "eig\u00b7ner", "macht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "bis auch die V\u00f6lker sich befrein", "tokens": ["bis", "auch", "die", "V\u00f6l\u00b7ker", "sich", "be\u00b7fr\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ART", "NN", "PRF", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "zum Volk ... mein Volk, wann wirst du sein?", "tokens": ["zum", "Volk", "...", "mein", "Volk", ",", "wann", "wirst", "du", "sein", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$(", "PPOSAT", "NN", "$,", "PWAV", "VAFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}