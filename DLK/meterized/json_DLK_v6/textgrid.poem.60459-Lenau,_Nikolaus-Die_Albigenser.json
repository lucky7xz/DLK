{"textgrid.poem.60459": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Die Albigenser", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Aug der Liebe wei\u00df im Freudensaale", "tokens": ["Das", "Aug", "der", "Lie\u00b7be", "wei\u00df", "im", "Freu\u00b7den\u00b7saa\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durchs Tanzgew\u00fchl, durch die Gestalten\u00dfucht,", "tokens": ["Durchs", "Tanz\u00b7ge\u00b7w\u00fchl", ",", "durch", "die", "Ge\u00b7stal\u00b7ten\u00b7\u00dfucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Liebesblick zu finden, den sie sucht,", "tokens": ["Den", "Lie\u00b7bes\u00b7blick", "zu", "fin\u00b7den", ",", "den", "sie", "sucht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und weidet sich an seinem s\u00fc\u00dfen Strahle.", "tokens": ["Und", "wei\u00b7det", "sich", "an", "sei\u00b7nem", "s\u00fc\u00b7\u00dfen", "Strah\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mein Auge sieht auf w\u00fcsten Degenklingen,", "tokens": ["Mein", "Au\u00b7ge", "sieht", "auf", "w\u00fcs\u00b7ten", "De\u00b7gen\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Feuer spr\u00fchend durch die Helme dringen,", "tokens": ["Die", "Feu\u00b7er", "spr\u00fc\u00b7hend", "durch", "die", "Hel\u00b7me", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und auf den Spitzen fluchbeschwingter Lanzen", "tokens": ["Und", "auf", "den", "Spit\u00b7zen", "fluch\u00b7be\u00b7schwing\u00b7ter", "Lan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Hier, dort verirrte Funken Gottes tanzen.", "tokens": ["Hier", ",", "dort", "ver\u00b7irr\u00b7te", "Fun\u00b7ken", "Got\u00b7tes", "tan\u00b7zen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}