{"textgrid.poem.46995": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Herr Kongre\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was hat Herr Kongre\u00df in Wien gethan?", "tokens": ["Was", "hat", "Herr", "Kon\u00b7gre\u00df", "in", "Wi\u00b7en", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "NE", "APPR", "NE", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er hat sich hingepflanzt", "tokens": ["Er", "hat", "sich", "hin\u00b7ge\u00b7pflanzt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hat nach einem sch\u00f6nen Plan,", "tokens": ["Und", "hat", "nach", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Anstatt zu gehn, getanzt;", "tokens": ["An\u00b7statt", "zu", "gehn", ",", "ge\u00b7tanzt", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUI", "PTKZU", "VVINF", "$,", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Frau Deutschheit war die T\u00e4nzerin,", "tokens": ["Frau", "Deutschheit", "war", "die", "T\u00e4n\u00b7ze\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Umtanzen mu\u00dfte sie her und hin,", "tokens": ["Um\u00b7tan\u00b7zen", "mu\u00df\u00b7te", "sie", "her", "und", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Was war ihr Gewinn?", "tokens": ["Was", "war", "ihr", "Ge\u00b7winn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Im Schwung franz\u00f6sischer T\u00e4nze", "tokens": ["Im", "Schwung", "fran\u00b7z\u00f6\u00b7si\u00b7scher", "T\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Verlor sie vom Haupte die Kr\u00e4nze.", "tokens": ["Ver\u00b7lor", "sie", "vom", "Haup\u00b7te", "die", "Kr\u00e4n\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Was hat Herr Kongre\u00df in Wien gethan?", "tokens": ["Was", "hat", "Herr", "Kon\u00b7gre\u00df", "in", "Wi\u00b7en", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "NE", "APPR", "NE", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er hat sich hin postiert,", "tokens": ["Er", "hat", "sich", "hin", "pos\u00b7tiert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hat, anstatt zu gehn voran,", "tokens": ["Und", "hat", ",", "an\u00b7statt", "zu", "gehn", "vo\u00b7ran", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "KOUI", "PTKZU", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Herum karusselliert.", "tokens": ["He\u00b7rum", "ka\u00b7rus\u00b7sel\u00b7liert", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Frau Deutschheit karussellieren sich lie\u00df", "tokens": ["Frau", "Deutschheit", "ka\u00b7rus\u00b7sel\u00b7lie\u00b7ren", "sich", "lie\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "PRF", "VVFIN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Im Kreis herum wie der Braten am Spie\u00df,", "tokens": ["Im", "Kreis", "he\u00b7rum", "wie", "der", "Bra\u00b7ten", "am", "Spie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "KOKOM", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Was war der Ersprie\u00df?", "tokens": ["Was", "war", "der", "Er\u00b7sprie\u00df", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Sie konnt' es nicht vertragen,", "tokens": ["Sie", "konnt'", "es", "nicht", "ver\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Es ward ihr \u00fcbel im Magen.", "tokens": ["Es", "ward", "ihr", "\u00fc\u00b7bel", "im", "Ma\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Was hat Herr Kongre\u00df in Wien gethan?", "tokens": ["Was", "hat", "Herr", "Kon\u00b7gre\u00df", "in", "Wi\u00b7en", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "NE", "APPR", "NE", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er war ein Mann von Welt,", "tokens": ["Er", "war", "ein", "Mann", "von", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er hat, da es war Schlittenbahn,", "tokens": ["Er", "hat", ",", "da", "es", "war", "Schlit\u00b7ten\u00b7bahn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PPER", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eine Schlittenfahrt angestellt.", "tokens": ["Ei\u00b7ne", "Schlit\u00b7ten\u00b7fahrt", "an\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Frau Deutschheit in dem Schlitten fuhr,", "tokens": ["Frau", "Deutschheit", "in", "dem", "Schlit\u00b7ten", "fuhr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "++--+-+", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Geh\u00fcllt in Zobel und Pelzwildschur,", "tokens": ["Ge\u00b7h\u00fcllt", "in", "Zo\u00b7bel", "und", "Pel\u00b7zwild\u00b7schur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "KON", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Wie bekam es ihr nur?", "tokens": ["Wie", "be\u00b7kam", "es", "ihr", "nur", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Sie hat die Ohren erfroren,", "tokens": ["Sie", "hat", "die", "Oh\u00b7ren", "er\u00b7fro\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Den guten Ruf noch verloren.", "tokens": ["Den", "gu\u00b7ten", "Ruf", "noch", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Was hat Herr Kongre\u00df in Wien gethan?", "tokens": ["Was", "hat", "Herr", "Kon\u00b7gre\u00df", "in", "Wi\u00b7en", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "NE", "APPR", "NE", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er war ein tapferer Held,", "tokens": ["Er", "war", "ein", "tap\u00b7fe\u00b7rer", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Er hat mit Ro\u00df und Speer und Fahn'", "tokens": ["Er", "hat", "mit", "Ro\u00df", "und", "Speer", "und", "Fahn'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein deutsch Turnier angestellt.", "tokens": ["Ein", "deutsch", "Tur\u00b7nier", "an\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Frau Deutschheit, das deutsche Turnier ihr gefiel,", "tokens": ["Frau", "Deutschheit", ",", "das", "deut\u00b7sche", "Tur\u00b7nier", "ihr", "ge\u00b7fiel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die alte Sitt' in neuem Spiel,", "tokens": ["Die", "al\u00b7te", "Sitt'", "in", "neu\u00b7em", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was war das Ziel?", "tokens": ["Was", "war", "das", "Ziel", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Die Lanz', ihr zu Ehren gebrochen,", "tokens": ["Die", "Lanz'", ",", "ihr", "zu", "Eh\u00b7ren", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text": "Hat ihr ein Aug' ausgestochen.", "tokens": ["Hat", "ihr", "ein", "Aug'", "aus\u00b7ge\u00b7sto\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVIZU", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Und als Herr Kongre\u00df nun m\u00fcde ward", "tokens": ["Und", "als", "Herr", "Kon\u00b7gre\u00df", "nun", "m\u00fc\u00b7de", "ward"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "NN", "NE", "ADV", "ADJD", "VAFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Von all dem Saus und Braus,", "tokens": ["Von", "all", "dem", "Saus", "und", "Braus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Tanz, Karussell und Schlittenfahrt", "tokens": ["Tanz", ",", "Ka\u00b7rus\u00b7sell", "und", "Schlit\u00b7ten\u00b7fahrt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Turnier, da turniert er nach Haus.", "tokens": ["Und", "Tur\u00b7nier", ",", "da", "tur\u00b7niert", "er", "nach", "Haus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Frau Deutschheit, und wenn du's zufrieden bist,", "tokens": ["Frau", "Deutschheit", ",", "und", "wenn", "du's", "zu\u00b7frie\u00b7den", "bist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "KON", "KOUS", "PIS", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "So lad' ich dich ein auf andere Frist,", "tokens": ["So", "lad'", "ich", "dich", "ein", "auf", "an\u00b7de\u00b7re", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Wann Zeit dazu ist,", "tokens": ["Wann", "Zeit", "da\u00b7zu", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PAV", "VAFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Zu Frankfurt an dem Maine,", "tokens": ["Zu", "Frank\u00b7furt", "an", "dem", "Mai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Da warte, bis ich erscheine.", "tokens": ["Da", "war\u00b7te", ",", "bis", "ich", "er\u00b7schei\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Du sollst mich als deutschen Bundestag", "tokens": ["Du", "sollst", "mich", "als", "deut\u00b7schen", "Bun\u00b7des\u00b7tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "KOUS", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Maskiert auftreten sehen;", "tokens": ["Mas\u00b7kiert", "auf\u00b7tre\u00b7ten", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVINF", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Wir wollen, wenn's Gott gefallen mag,", "tokens": ["Wir", "wol\u00b7len", ",", "wenn's", "Gott", "ge\u00b7fal\u00b7len", "mag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Uns wieder im Kreise drehen.", "tokens": ["Uns", "wie\u00b7der", "im", "Krei\u00b7se", "dre\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Frau Deutschheit, erhalte mir deine Huld,", "tokens": ["Frau", "Deutschheit", ",", "er\u00b7hal\u00b7te", "mir", "dei\u00b7ne", "Huld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und falle mir nicht in Ungeduld!", "tokens": ["Und", "fal\u00b7le", "mir", "nicht", "in", "Un\u00b7ge\u00b7duld", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Die Zeit ist schuld,", "tokens": ["Die", "Zeit", "ist", "schuld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Da\u00df alles mit Schaugepr\u00e4nge", "tokens": ["Da\u00df", "al\u00b7les", "mit", "Schau\u00b7ge\u00b7pr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "So geht in die Breit' und die L\u00e4nge.", "tokens": ["So", "geht", "in", "die", "Breit'", "und", "die", "L\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}