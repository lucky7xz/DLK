{"textgrid.poem.40889": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es blickte Pinzenauer von Kuffsteins Riesenwall", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es blickte Pinzenauer von Kuffsteins Riesenwall", "tokens": ["Es", "blick\u00b7te", "Pin\u00b7ze\u00b7nau\u00b7er", "von", "Kuffs\u00b7teins", "Rie\u00b7sen\u00b7wall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mit Hohn und sichrem Trotze auf Maxens Heeresschwall,", "tokens": ["Mit", "Hohn", "und", "sich\u00b7rem", "Trot\u00b7ze", "auf", "Ma\u00b7xens", "Hee\u00b7res\u00b7schwall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie ein Alpengeier sorglos auf den Verfolger blickt,", "tokens": ["Wie", "ein", "Al\u00b7pen\u00b7gei\u00b7er", "sorg\u00b7los", "auf", "den", "Ver\u00b7fol\u00b7ger", "blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Der fern im tiefen Thale auf ihn die B\u00fcchse z\u00fcckt.", "tokens": ["Der", "fern", "im", "tie\u00b7fen", "Tha\u00b7le", "auf", "ihn", "die", "B\u00fcch\u00b7se", "z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "ADJA", "NN", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Es blickte Max gen Kuffsteins hochtrotzende Felsenwand,", "tokens": ["Es", "blick\u00b7te", "Max", "gen", "Kuffs\u00b7teins", "hoch\u00b7trot\u00b7zen\u00b7de", "Fel\u00b7sen\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Voll Zuversicht und Ruhe, so k\u00fchn und muthentbrannt,", "tokens": ["Voll", "Zu\u00b7ver\u00b7sicht", "und", "Ru\u00b7he", ",", "so", "k\u00fchn", "und", "mu\u00b7thent\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$,", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Gleichwie zum Horst des Geiers der Sch\u00fctze blickt empor;", "tokens": ["Gleich\u00b7wie", "zum", "Horst", "des", "Gei\u00b7ers", "der", "Sch\u00fct\u00b7ze", "blickt", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Erreicht ihn auch sein Fu\u00df nicht, erreicht ihn doch sein Rohr.", "tokens": ["Er\u00b7reicht", "ihn", "auch", "sein", "Fu\u00df", "nicht", ",", "er\u00b7reicht", "ihn", "doch", "sein", "Rohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKNEG", "$,", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Aus hundert M\u00f6rsern aufw\u00e4rts flog donnernd Ball an Ball,", "tokens": ["Aus", "hun\u00b7dert", "M\u00f6r\u00b7sern", "auf\u00b7w\u00e4rts", "flog", "don\u00b7nernd", "Ball", "an", "Ball", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADV", "VVFIN", "ADJD", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ohnm\u00e4chtig, spurlos prallen zur\u00fcck die Kugeln all,", "tokens": ["Ohn\u00b7m\u00e4ch\u00b7tig", ",", "spur\u00b7los", "pral\u00b7len", "zu\u00b7r\u00fcck", "die", "Ku\u00b7geln", "all", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VVFIN", "ADV", "ART", "NN", "PIAT", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Gleichwie wenn Bl\u00fcthenflocken auf einen Panzer fielen,", "tokens": ["Gleich\u00b7wie", "wenn", "Bl\u00fc\u00b7then\u00b7flo\u00b7cken", "auf", "ei\u00b7nen", "Pan\u00b7zer", "fie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Gleichwie wenn Schaumestropfen um einen Felsblock spielen.", "tokens": ["Gleich\u00b7wie", "wenn", "Schau\u00b7mes\u00b7trop\u00b7fen", "um", "ei\u00b7nen", "Fels\u00b7block", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Da sah man Pinzenauern hoch auf der festen Wand,", "tokens": ["Da", "sah", "man", "Pin\u00b7ze\u00b7nau\u00b7ern", "hoch", "auf", "der", "fes\u00b7ten", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ein t\u00fcchtig Ruthenb\u00fcndel hielt er in seiner Hand.", "tokens": ["Ein", "t\u00fcch\u00b7tig", "Ru\u00b7then\u00b7b\u00fcn\u00b7del", "hielt", "er", "in", "sei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NE", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wo Maxens Kugeln schlugen, da b\u00fcckt' er sich hinab", "tokens": ["Wo", "Ma\u00b7xens", "Ku\u00b7geln", "schlu\u00b7gen", ",", "da", "b\u00fcckt'", "er", "sich", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PRF", "PTKVZ"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und fegte die Stellen h\u00f6hnisch mit seinem Besen ab.", "tokens": ["Und", "feg\u00b7te", "die", "Stel\u00b7len", "h\u00f6h\u00b7nisch", "mit", "sei\u00b7nem", "Be\u00b7sen", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbei, ei, du sp\u00f6ttischer Vogel, sieh dich nur weislich vor,", "tokens": ["\u00bb", "ei", ",", "ei", ",", "du", "sp\u00f6t\u00b7ti\u00b7scher", "Vo\u00b7gel", ",", "sieh", "dich", "nur", "weis\u00b7lich", "vor", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "ITJ", "$,", "PPER", "ADJA", "NN", "$,", "VVIMP", "PPER", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da\u00df dir aus deinem B\u00fcndel ein Beil nicht springt empor!\u00ab", "tokens": ["Da\u00df", "dir", "aus", "dei\u00b7nem", "B\u00fcn\u00b7del", "ein", "Beil", "nicht", "springt", "em\u00b7por", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "PTKNEG", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "So rief nun Max, sein Auge zuckt wie ein Wetterschlag;", "tokens": ["So", "rief", "nun", "Max", ",", "sein", "Au\u00b7ge", "zuckt", "wie", "ein", "Wet\u00b7ter\u00b7schlag", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NE", "$,", "PPOSAT", "NN", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Hohn schl\u00e4gt viel tiefre Wunden, als es ein Schwert vermag.", "tokens": ["Hohn", "schl\u00e4gt", "viel", "tief\u00b7re", "Wun\u00b7den", ",", "als", "es", "ein", "Schwert", "ver\u00b7mag", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "ADJA", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}}, "stanza.6": {"line.1": {"text": "Den Pechkranz lie\u00df er prasselnd jetzt auf zur Feste fliegen;", "tokens": ["Den", "Pech\u00b7kranz", "lie\u00df", "er", "pras\u00b7selnd", "jetzt", "auf", "zur", "Fes\u00b7te", "flie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVPP", "ADV", "APPR", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Umsonst, unsch\u00e4dlich blieb er auf breiten Mauern liegen!", "tokens": ["Um\u00b7sonst", ",", "un\u00b7sch\u00e4d\u00b7lich", "blieb", "er", "auf", "brei\u00b7ten", "Mau\u00b7ern", "lie\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "VVFIN", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "---+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Pinzenauer kochte dabei sein Mahl in Ruh.", "tokens": ["Der", "Pin\u00b7ze\u00b7nau\u00b7er", "koch\u00b7te", "da\u00b7bei", "sein", "Mahl", "in", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "\u00bbgeduld!\u00ab rief Max, \u00bbich send' euch als Gast den Hunger zu.\u00ab", "tokens": ["\u00bb", "ge\u00b7duld", "!", "\u00ab", "rief", "Max", ",", "\u00bb", "ich", "send'", "euch", "als", "Gast", "den", "Hun\u00b7ger", "zu", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$.", "$(", "VVFIN", "NE", "$,", "$(", "PPER", "VVFIN", "PPER", "KOUS", "NN", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Drei Wochen schon entschwanden. \u2013 Max hielt im Zelte Rast,", "tokens": ["Drei", "Wo\u00b7chen", "schon", "ent\u00b7schwan\u00b7den", ".", "\u2013", "Max", "hielt", "im", "Zel\u00b7te", "Rast", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "VVINF", "$.", "$(", "NE", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Schon lud zu seinem Mahle der Hunger sich als Gast.", "tokens": ["Schon", "lud", "zu", "sei\u00b7nem", "Mah\u00b7le", "der", "Hun\u00b7ger", "sich", "als", "Gast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "PRF", "KOUS", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Versprach er nicht, zu senden den Gast an Kuffsteins Thor?", "tokens": ["Ver\u00b7sprach", "er", "nicht", ",", "zu", "sen\u00b7den", "den", "Gast", "an", "Kuffs\u00b7teins", "Thor", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PTKZU", "VVINF", "ART", "NN", "APPR", "NN", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Man mu\u00df ja selbst erst kennen, wen man zum Boten erkor.", "tokens": ["Man", "mu\u00df", "ja", "selbst", "erst", "ken\u00b7nen", ",", "wen", "man", "zum", "Bo\u00b7ten", "er\u00b7kor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "ADV", "ADV", "VVINF", "$,", "PWS", "PIS", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Da br\u00fcllt es vor den Zelten, \u2013 hoho! was soll es sein?", "tokens": ["Da", "br\u00fcllt", "es", "vor", "den", "Zel\u00b7ten", ",", "\u2013", "ho\u00b7ho", "!", "was", "soll", "es", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "$(", "ITJ", "$.", "PWS", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sieh, Hirt' und Heerden ziehen ins Lager dr\u00e4ngend ein:", "tokens": ["Sieh", ",", "Hirt'", "und", "Heer\u00b7den", "zie\u00b7hen", "ins", "La\u00b7ger", "dr\u00e4n\u00b7gend", "ein", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NN", "VVFIN", "APPRART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u00bbhans Pinzenau l\u00e4\u00dft gr\u00fc\u00dfen und schickt, was er vermag,", "tokens": ["\u00bb", "hans", "Pin\u00b7ze\u00b7nau", "l\u00e4\u00dft", "gr\u00fc\u00b7\u00dfen", "und", "schickt", ",", "was", "er", "ver\u00b7mag", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "VVFIN", "VVINF", "KON", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Auf da\u00df auch ihr euch einmal macht einen guten Tag.\u00ab", "tokens": ["Auf", "da\u00df", "auch", "ihr", "euch", "ein\u00b7mal", "macht", "ei\u00b7nen", "gu\u00b7ten", "Tag", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "KOUS", "ADV", "PPER", "PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+--+--+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Da wurde K\u00f6nig Maxen die Zeit wohl etwas lang,", "tokens": ["Da", "wur\u00b7de", "K\u00f6\u00b7nig", "Ma\u00b7xen", "die", "Zeit", "wohl", "et\u00b7was", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "NE", "ART", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da\u00df pochend schon sein Herzschlag bis durch den Panzer klang;", "tokens": ["Da\u00df", "po\u00b7chend", "schon", "sein", "Herz\u00b7schlag", "bis", "durch", "den", "Pan\u00b7zer", "klang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "PPOSAT", "NN", "APPR", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da sandt' er gegen Innsbruck hinauf ins Waffenhaus:", "tokens": ["Da", "sandt'", "er", "ge\u00b7gen", "Inns\u00b7bruck", "hin\u00b7auf", "ins", "Waf\u00b7fen\u00b7haus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE", "ADV", "APPRART", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "\u00bbschickt doch einmal den Weckauf mir und den Purlepaus!\u00ab", "tokens": ["\u00bb", "schickt", "doch", "ein\u00b7mal", "den", "Weck\u00b7auf", "mir", "und", "den", "Pur\u00b7le\u00b7paus", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "ADV", "ADV", "ART", "NN", "PPER", "KON", "ART", "NN", "$.", "$("], "meter": "+--+-++-+-+-+", "measure": "iambic.septa.invert"}}, "stanza.10": {"line.1": {"text": "Der K\u00f6nig statt des Zepters fa\u00dft nun den Luntenbrand,", "tokens": ["Der", "K\u00f6\u00b7nig", "statt", "des", "Zep\u00b7ters", "fa\u00dft", "nun", "den", "Lun\u00b7ten\u00b7brand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wie f\u00fchrt so gut er beide mit sichrer Meisterhand!", "tokens": ["Wie", "f\u00fchrt", "so", "gut", "er", "bei\u00b7de", "mit", "sich\u00b7rer", "Meis\u00b7ter\u00b7hand", "!"], "tok