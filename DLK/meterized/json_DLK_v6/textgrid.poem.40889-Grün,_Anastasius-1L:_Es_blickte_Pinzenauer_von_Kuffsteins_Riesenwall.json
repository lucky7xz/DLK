measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Um Eins nur wollt' ich bitten: um einen Becher Wein.\u00ab", "tokens": ["Um", "Eins", "nur", "wollt'", "ich", "bit\u00b7ten", ":", "um", "ei\u00b7nen", "Be\u00b7cher", "Wein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUI", "NN", "ADV", "VMFIN", "PPER", "VVINF", "$.", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.18": {"line.1": {"text": "So sprach der Pinzenauer, nicht bebte seine Hand,", "tokens": ["So", "sprach", "der", "Pin\u00b7ze\u00b7nau\u00b7er", ",", "nicht", "beb\u00b7te", "sei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PTKNEG", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Nicht bleichte sich sein Antlitz, als er vor Maxen stand,", "tokens": ["Nicht", "bleich\u00b7te", "sich", "sein", "Ant\u00b7litz", ",", "als", "er", "vor", "Ma\u00b7xen", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PRF", "PPOSAT", "NN", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Gleich einem eh'rnen Kreuzbild auf einem Marmorsarg,", "tokens": ["Gleich", "ei\u00b7nem", "eh'r\u00b7nen", "Kreuz\u00b7bild", "auf", "ei\u00b7nem", "Mar\u00b7mor\u00b7sarg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So traurig und so d\u00fcster, doch auch so fest und stark.", "tokens": ["So", "trau\u00b7rig", "und", "so", "d\u00fcs\u00b7ter", ",", "doch", "auch", "so", "fest", "und", "stark", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$,", "ADV", "ADV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.19": {"line.1": {"text": "\u00bbauf euer Heil, mein K\u00f6nig! O da\u00df ihr's tief erw\u00e4gt,", "tokens": ["\u00bb", "auf", "eu\u00b7er", "Heil", ",", "mein", "K\u00f6\u00b7nig", "!", "O", "da\u00df", "ih\u00b7r's", "tief", "er\u00b7w\u00e4gt", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$.", "NE", "KOUS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Wieviel es hei\u00dft, wenn Einer, de\u00df Haupt zum Block ihr legt,", "tokens": ["Wie\u00b7viel", "es", "hei\u00dft", ",", "wenn", "Ei\u00b7ner", ",", "de\u00df", "Haupt", "zum", "Block", "ihr", "legt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "KOUS", "PIS", "$,", "ART", "NN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Aus voller Lust des Herzens noch zecht auf euer Heil!\u00ab", "tokens": ["Aus", "vol\u00b7ler", "Lust", "des", "Her\u00b7zens", "noch", "zecht", "auf", "eu\u00b7er", "Heil", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Er sprach's und beugte nieder sein Haupt dem rothen Beil.", "tokens": ["Er", "sprach's", "und", "beug\u00b7te", "nie\u00b7der", "sein", "Haupt", "dem", "ro\u00b7then", "Beil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.20": {"line.1": {"text": "Zehn der Genossen folgten ihm treu in Tod und Leben.", "tokens": ["Zehn", "der", "Ge\u00b7nos\u00b7sen", "folg\u00b7ten", "ihm", "treu", "in", "Tod", "und", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ART", "NN", "VVFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+--+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Schon sah man mild Erbarmen des K\u00f6nigs Blick umschweben,", "tokens": ["Schon", "sah", "man", "mild", "Er\u00b7bar\u00b7men", "des", "K\u00f6\u00b7nigs", "Blick", "um\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "NN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Schon will sein Herz begnad'gen, sein Eid verwehrt ihm's nur,", "tokens": ["Schon", "will", "sein", "Herz", "be\u00b7gna\u00b7d'\u00b7gen", ",", "sein", "Eid", "ver\u00b7wehrt", "ihm's", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "VVINF", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Und insgeheim verw\u00fcnscht er den argen, b\u00f6sen Schwur.", "tokens": ["Und", "ins\u00b7ge\u00b7heim", "ver\u00b7w\u00fcnscht", "er", "den", "ar\u00b7gen", ",", "b\u00f6\u00b7sen", "Schwur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "ADJA", "$,", "ADJD", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.21": {"line.1": {"text": "\u00bbhalt, halt, mein F\u00fcrst!\u00ab rief Erich von Braunschweig unverzagt,", "tokens": ["\u00bb", "halt", ",", "halt", ",", "mein", "F\u00fcrst", "!", "\u00ab", "rief", "E\u00b7rich", "von", "Braun\u00b7schweig", "un\u00b7ver\u00b7zagt", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "$,", "PPOSAT", "NN", "$.", "$(", "VVFIN", "NE", "APPR", "NE", "ADJD", "$,"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "\u00bbmag euer Zorn mich treffen, doch sei die\u00df Wort gewagt!", "tokens": ["\u00bb", "mag", "eu\u00b7er", "Zorn", "mich", "tref\u00b7fen", ",", "doch", "sei", "die\u00df", "Wort", "ge\u00b7wagt", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPOSAT", "NN", "PPER", "VVINF", "$,", "ADV", "VAFIN", "PDS", "NN", "VVPP", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Hinweg, ihr blutigen Schergen, und wahrt die Beile fromm,", "tokens": ["Hin\u00b7weg", ",", "ihr", "blu\u00b7ti\u00b7gen", "Scher\u00b7gen", ",", "und", "wahrt", "die", "Bei\u00b7le", "fromm", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "ADJA", "NN", "$,", "KON", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die roth vom besten Blute, das je durch Adern glomm.", "tokens": ["Die", "roth", "vom", "bes\u00b7ten", "Blu\u00b7te", ",", "das", "je", "durch", "A\u00b7dern", "glomm", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "ADJA", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.22": {"line.1": {"text": "F\u00fcr Schurkenpack, doch nimmer f\u00fcr Heldenvolk der Schlacht", "tokens": ["F\u00fcr", "Schur\u00b7ken\u00b7pack", ",", "doch", "nim\u00b7mer", "f\u00fcr", "Hel\u00b7den\u00b7volk", "der", "Schlacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "ADV", "ADV", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ist jenes Beil geschliffen, der Schandblock ausgedacht;", "tokens": ["Ist", "je\u00b7nes", "Beil", "ge\u00b7schlif\u00b7fen", ",", "der", "Schand\u00b7block", "aus\u00b7ge\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "VVPP", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wenn Tapferkeit und K\u00fchnheit ihr so zu lohnen glaubt,", "tokens": ["Wenn", "Tap\u00b7fer\u00b7keit", "und", "K\u00fchn\u00b7heit", "ihr", "so", "zu", "loh\u00b7nen", "glaubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Mein F\u00fcrst, dann beugt zuv\u00f6rderst dem Block das eigne Haupt!\u00ab", "tokens": ["Mein", "F\u00fcrst", ",", "dann", "beugt", "zu\u00b7v\u00f6r\u00b7derst", "dem", "Block", "das", "eig\u00b7ne", "Haupt", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.23": {"line.1": {"text": "Max, treu dem Schwur, gab leise ihm einen Backenstreich,", "tokens": ["Max", ",", "treu", "dem", "Schwur", ",", "gab", "lei\u00b7se", "ihm", "ei\u00b7nen", "Ba\u00b7cken\u00b7streich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "ART", "VVFIN", "$,", "VVFIN", "ADJD", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dr\u00fcckt ihm die Hand und st\u00fcrzte ihm an die Brust zugleich:", "tokens": ["Dr\u00fcckt", "ihm", "die", "Hand", "und", "st\u00fcrz\u00b7te", "ihm", "an", "die", "Brust", "zu\u00b7gleich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "\u00bbgepriesen sei, mein Erich, dein edles biedres Wort!", "tokens": ["\u00bb", "ge\u00b7prie\u00b7sen", "sei", ",", "mein", "E\u00b7rich", ",", "dein", "ed\u00b7les", "bie\u00b7dres", "Wort", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "VAFIN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ihr Andern aber ziehet in Ruh' und Frieden fort!\u00ab", "tokens": ["Ihr", "An\u00b7dern", "a\u00b7ber", "zie\u00b7het", "in", "Ruh'", "und", "Frie\u00b7den", "fort", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "ADV", "VVFIN", "APPR", "NN", "KON", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.24": {"line.1": {"text": "N\u00e4chst Kuffstein steht ein Kirchlein, Ainleffen hei\u00dft's noch heut,", "tokens": ["N\u00e4chst", "Kuffs\u00b7tein", "steht", "ein", "Kirch\u00b7lein", ",", "Ain\u00b7lef\u00b7fen", "hei\u00dft's", "noch", "heut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ART", "NN", "$,", "NN", "NE", "ADV", "ADV", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Weil's den gerichteten Eilfen zum Grabmal Max geweiht.", "tokens": ["Weil's", "den", "ge\u00b7rich\u00b7te\u00b7ten", "Eil\u00b7fen", "zum", "Grab\u00b7mal", "Max", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "APPRART", "NN", "NE", "VVPP", "$."], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Einst, als in Tyrol er wieder, erz\u00e4hlt' ein Bauernknab',", "tokens": ["Einst", ",", "als", "in", "Ty\u00b7rol", "er", "wie\u00b7der", ",", "er\u00b7z\u00e4hlt'", "ein", "Bau\u00b7ern\u00b7knab'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "APPR", "NE", "PPER", "ADV", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Er habe j\u00fcngst den K\u00f6nig gesehn dort knien am Grab.", "tokens": ["Er", "ha\u00b7be", "j\u00fcngst", "den", "K\u00f6\u00b7nig", "ge\u00b7sehn", "dort", "kni\u00b7en", "am", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "VVP