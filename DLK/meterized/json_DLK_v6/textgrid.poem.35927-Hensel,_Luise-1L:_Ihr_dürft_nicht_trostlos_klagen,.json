{"textgrid.poem.35927": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr d\u00fcrft nicht trostlos klagen,", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr d\u00fcrft nicht trostlos klagen,", "tokens": ["Ihr", "d\u00fcrft", "nicht", "trost\u00b7los", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Seelen, die ihr gl\u00e4ubt:", "tokens": ["Ihr", "See\u00b7len", ",", "die", "ihr", "gl\u00e4ubt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich will ein Wort euch sagen,", "tokens": ["Ich", "will", "ein", "Wort", "euch", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Davor der Gram zerst\u00e4ubt.", "tokens": ["Da\u00b7vor", "der", "Gram", "zer\u00b7st\u00e4ubt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bald wird der Herr erscheinen", "tokens": ["Bald", "wird", "der", "Herr", "er\u00b7schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Seiner Herrlichkeit", "tokens": ["In", "Sei\u00b7ner", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und retten all die Seinen", "tokens": ["Und", "ret\u00b7ten", "all", "die", "Sei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus dieser b\u00f6sen Zeit.", "tokens": ["Aus", "die\u00b7ser", "b\u00f6\u00b7sen", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Kirche mu\u00df geb\u00e4ren", "tokens": ["Die", "Kir\u00b7che", "mu\u00df", "ge\u00b7b\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Held, den Er erw\u00e4hlt,", "tokens": ["Den", "Held", ",", "den", "Er", "er\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der wird dem Satan wehren,", "tokens": ["Der", "wird", "dem", "Sa\u00b7tan", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der uns so lang' gequ\u00e4lt.", "tokens": ["Der", "uns", "so", "lang'", "ge\u00b7qu\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "La\u00dft nur von ihr euch deuten", "tokens": ["La\u00dft", "nur", "von", "ihr", "euch", "deu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "APPR", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Johanni Wunderbuch;", "tokens": ["Jo\u00b7han\u00b7ni", "Wun\u00b7der\u00b7buch", ";"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da steht von unsern Zeiten", "tokens": ["Da", "steht", "von", "un\u00b7sern", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ihrem Trost genug.", "tokens": ["Und", "ih\u00b7rem", "Trost", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O, lauscht nur in die Ferne,", "tokens": ["O", ",", "lauscht", "nur", "in", "die", "Fer\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie schon der L\u00f6we br\u00fcllt;", "tokens": ["Wie", "schon", "der", "L\u00f6\u00b7we", "br\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O, schaut nur an die Sterne:", "tokens": ["O", ",", "schaut", "nur", "an", "die", "Ster\u00b7ne", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bald ist die Zeit erf\u00fcllt.", "tokens": ["Bald", "ist", "die", "Zeit", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Viel m\u00fcssen wir noch dulden", "tokens": ["Viel", "m\u00fcs\u00b7sen", "wir", "noch", "dul\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nach Gottes weisem Rath:", "tokens": ["Nach", "Got\u00b7tes", "wei\u00b7sem", "Rath", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Blutroth sind unsre Schulden,", "tokens": ["Blut\u00b7roth", "sind", "uns\u00b7re", "Schul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schwer unsre Missethat.", "tokens": ["Schwer", "uns\u00b7re", "Mis\u00b7se\u00b7that", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "So wollen wir nun tragen,", "tokens": ["So", "wol\u00b7len", "wir", "nun", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was wir verschuldet, still,", "tokens": ["Was", "wir", "ver\u00b7schul\u00b7det", ",", "still", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu Allem Amen sagen,", "tokens": ["Zu", "Al\u00b7lem", "A\u00b7men", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was Er uns schicken will. \u2013", "tokens": ["Was", "Er", "uns", "schi\u00b7cken", "will", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "PRF", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O Herr! wann willst Du kommen?", "tokens": ["O", "Herr", "!", "wann", "willst", "Du", "kom\u00b7men", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Viel Herzen hoffen Dich;", "tokens": ["Viel", "Her\u00b7zen", "hof\u00b7fen", "Dich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es sehnen Deine Frommen", "tokens": ["Es", "seh\u00b7nen", "Dei\u00b7ne", "From\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von ganzer Seele sich.", "tokens": ["Von", "gan\u00b7zer", "See\u00b7le", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Herr! sage Deinen Schnittern:", "tokens": ["Herr", "!", "sa\u00b7ge", "Dei\u00b7nen", "Schnit\u00b7tern", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "\u00bbdie Saat ist reif und wei\u00df!\u00ab", "tokens": ["\u00bb", "die", "Saat", "ist", "reif", "und", "wei\u00df", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und la\u00df uns nicht erzittern,", "tokens": ["Und", "la\u00df", "uns", "nicht", "er\u00b7zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wird uns der Tag gleich hei\u00df.", "tokens": ["Wird", "uns", "der", "Tag", "gleich", "hei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Der Feigenbaum treibt Bl\u00e4tter,", "tokens": ["Der", "Fei\u00b7gen\u00b7baum", "treibt", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Drum wachet und seid fromm", "tokens": ["Drum", "wa\u00b7chet", "und", "seid", "fromm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "KON", "VAFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und fleht zu unserm Retter:", "tokens": ["Und", "fleht", "zu", "un\u00b7serm", "Ret\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ja, komm, Herr Jesu! komm.", "tokens": ["Ja", ",", "komm", ",", "Herr", "Je\u00b7su", "!", "kom\u00b7m."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "abbreviation"], "pos": ["PTKANT", "$,", "VVFIN", "$,", "NN", "NE", "$.", "XY"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "\u00bbwir merken wohl die Bl\u00e4tter", "tokens": ["\u00bb", "wir", "mer\u00b7ken", "wohl", "die", "Bl\u00e4t\u00b7ter"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und beten still und fromm:", "tokens": ["Und", "be\u00b7ten", "still", "und", "fromm", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ja, komm, Du Rachewetter!", "tokens": ["Ja", ",", "komm", ",", "Du", "Ra\u00b7che\u00b7wet\u00b7ter", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du Friedenbringer, komm!\u00ab", "tokens": ["Du", "Frie\u00b7den\u00b7brin\u00b7ger", ",", "komm", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "NN", "$,", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}