{"textgrid.poem.24104": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach, da\u00df mein Herz noch einmal beben k\u00f6nnte", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, da\u00df mein Herz noch einmal beben k\u00f6nnte", "tokens": ["Ach", ",", "da\u00df", "mein", "Herz", "noch", "ein\u00b7mal", "be\u00b7ben", "k\u00f6nn\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "PPOSAT", "NN", "ADV", "ADV", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In dieser ungest\u00fcmen Seligkeit,", "tokens": ["In", "die\u00b7ser", "un\u00b7ge\u00b7st\u00fc\u00b7men", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ich das Gl\u00fcck noch einmal leben k\u00f6nnte", "tokens": ["Da\u00df", "ich", "das", "Gl\u00fcck", "noch", "ein\u00b7mal", "le\u00b7ben", "k\u00f6nn\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADV", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der unbedachten Hingegebenheit.", "tokens": ["Der", "un\u00b7be\u00b7dach\u00b7ten", "Hin\u00b7ge\u00b7ge\u00b7ben\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Als ich mein Leben auf zwei Augen setzte,", "tokens": ["Als", "ich", "mein", "Le\u00b7ben", "auf", "zwei", "Au\u00b7gen", "setz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In denen ich die Himmel leuchten sah,", "tokens": ["In", "de\u00b7nen", "ich", "die", "Him\u00b7mel", "leuch\u00b7ten", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als ich Verstand wie einen Strohwisch sch\u00e4tzte,", "tokens": ["Als", "ich", "Ver\u00b7stand", "wie", "ei\u00b7nen", "Stroh\u00b7wisch", "sch\u00e4tz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie war ich Narr, wie war ich K\u00f6nig da.", "tokens": ["Wie", "war", "ich", "Narr", ",", "wie", "war", "ich", "K\u00f6\u00b7nig", "da", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "NE", "$,", "PWAV", "VAFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Heut wei\u00df ich viel und bin so voll Verstande,", "tokens": ["Heut", "wei\u00df", "ich", "viel", "und", "bin", "so", "voll", "Ver\u00b7stan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "VAFIN", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df Wahn und Gl\u00fcck mir gleich verboten sind;", "tokens": ["Da\u00df", "Wahn", "und", "Gl\u00fcck", "mir", "gleich", "ver\u00b7bo\u00b7ten", "sind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein Leben rinnt kalt und bedacht im Sande,", "tokens": ["Mein", "Le\u00b7ben", "rinnt", "kalt", "und", "be\u00b7dacht", "im", "San\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "KON", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und meine Augen sind den Himmeln blind.", "tokens": ["Und", "mei\u00b7ne", "Au\u00b7gen", "sind", "den", "Him\u00b7meln", "blind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich g\u00e4be viel um jene Thorenn\u00e4chte,", "tokens": ["Ich", "g\u00e4\u00b7be", "viel", "um", "je\u00b7ne", "Tho\u00b7ren\u00b7n\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da in die Kissen ich geweint, gest\u00f6hnt;", "tokens": ["Da", "in", "die", "Kis\u00b7sen", "ich", "ge\u00b7weint", ",", "ge\u00b7st\u00f6hnt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPER", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gebenedeit, wer mir es wiederbr\u00e4chte,", "tokens": ["Ge\u00b7be\u00b7ne\u00b7deit", ",", "wer", "mir", "es", "wie\u00b7der\u00b7br\u00e4ch\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dies Thr\u00e4nengl\u00fcck, das mein Verstand verh\u00f6hnt.", "tokens": ["Dies", "Thr\u00e4\u00b7nen\u00b7gl\u00fcck", ",", "das", "mein", "Ver\u00b7stand", "ver\u00b7h\u00f6hnt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Da sitz ich nun und bastele Figuren,", "tokens": ["Da", "sitz", "ich", "nun", "und", "bas\u00b7te\u00b7le", "Fi\u00b7gu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "KON", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und mir hei\u00dft Gl\u00fcck, da\u00df ich ein Meister bin;", "tokens": ["Und", "mir", "hei\u00dft", "Gl\u00fcck", ",", "da\u00df", "ich", "ein", "Meis\u00b7ter", "bin", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein Meisterst\u00fcck: Zwei gr\u00e4\u00dfliche Lemuren", "tokens": ["Mein", "Meis\u00b7ter\u00b7st\u00fcck", ":", "Zwei", "gr\u00e4\u00df\u00b7li\u00b7che", "Le\u00b7mu\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verscharren eine blonde K\u00f6nigin.", "tokens": ["Ver\u00b7schar\u00b7ren", "ei\u00b7ne", "blon\u00b7de", "K\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}