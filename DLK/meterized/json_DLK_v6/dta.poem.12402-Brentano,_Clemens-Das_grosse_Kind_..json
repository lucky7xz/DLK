{"dta.poem.12402": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Das grosse Kind .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich h\u00f6rt ein Fr\u00e4ulein klagen,               ", "tokens": ["Ich", "h\u00f6rt", "ein", "Fr\u00e4u\u00b7lein", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fcrwahr ein weiblich Bild,", "tokens": ["F\u00fcr\u00b7wahr", "ein", "weib\u00b7lich", "Bild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Herz wollt ihr verzagen,", "tokens": ["Ihr", "Herz", "wollt", "ihr", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch einen J\u00fcngling mild.", "tokens": ["Durch", "ei\u00b7nen", "J\u00fcng\u00b7ling", "mild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Fr\u00e4ulein sprach mit Listen:", "tokens": ["Das", "Fr\u00e4u\u00b7lein", "sprach", "mit", "Lis\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u201eer liegt an meinen Br\u00fcsten", "tokens": ["\u201e", "er", "liegt", "an", "mei\u00b7nen", "Br\u00fcs\u00b7ten"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u201eder Allerliebste mein.", "tokens": ["\u201e", "der", "Al\u00b7ler\u00b7liebs\u00b7te", "mein", "."], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u201ewarum sollt ich aufwecken", "tokens": ["\u201e", "wa\u00b7rum", "sollt", "ich", "auf\u00b7we\u00b7cken"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201eden Allerliebsten mein,", "tokens": ["\u201e", "den", "Al\u00b7ler\u00b7liebs\u00b7ten", "mein", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201eich f\u00fcrcht es m\u00f6cht erschrecken", "tokens": ["\u201e", "ich", "f\u00fcrcht", "es", "m\u00f6cht", "er\u00b7schre\u00b7cken"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201edas junge Herze sein;", "tokens": ["\u201e", "das", "jun\u00b7ge", "Her\u00b7ze", "sein", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "VVFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201eer ist mein Herz-Geselle,", "tokens": ["\u201e", "er", "ist", "mein", "Her\u00b7zGe\u00b7sel\u00b7le", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u201eer liegt an seiner Stelle,", "tokens": ["\u201e", "er", "liegt", "an", "sei\u00b7ner", "Stel\u00b7le", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u201ewie gern ich bey ihm bin.", "tokens": ["\u201e", "wie", "gern", "ich", "bey", "ihm", "bin", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADV", "PPER", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u201eer ist mein Kindlein kleine,", "tokens": ["\u201e", "er", "ist", "mein", "Kin\u00b7dlein", "klei\u00b7ne", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPOSAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201eer athmet noch so hei\u00df,", "tokens": ["\u201e", "er", "ath\u00b7met", "noch", "so", "hei\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201eund da\u00df er nur nicht weine,", "tokens": ["\u201e", "und", "da\u00df", "er", "nur", "nicht", "wei\u00b7ne", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "KOUS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201eich sang ihn ein so leis!\u201c", "tokens": ["\u201e", "ich", "sang", "ihn", "ein", "so", "leis", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ART", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Fr\u00e4ulein sagt mit Listen:", "tokens": ["Das", "Fr\u00e4u\u00b7lein", "sagt", "mit", "Lis\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u201ees schl\u00e4ft an meinen Br\u00fcsten,", "tokens": ["\u201e", "es", "schl\u00e4ft", "an", "mei\u00b7nen", "Br\u00fcs\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u201eder Allerliebste mein.\u201c", "tokens": ["\u201e", "der", "Al\u00b7ler\u00b7liebs\u00b7te", "mein", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "PPOSAT", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}