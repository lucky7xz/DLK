aic.tetra"}, "line.110": {"text": "Kalt und todt der \u2013 Todtengr\u00e4ber.", "tokens": ["Kalt", "und", "todt", "der", "\u2013", "Tod\u00b7ten\u00b7gr\u00e4\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "ADJD", "ART", "$(", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.111": {"text": "Mit den rauhen, derben H\u00e4nden", "tokens": ["Mit", "den", "rau\u00b7hen", ",", "der\u00b7ben", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.112": {"text": "Trug er selbst Dich in das St\u00fcbchen,", "tokens": ["Trug", "er", "selbst", "Dich", "in", "das", "St\u00fcb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.113": {"text": "Das bestimmt ist f\u00fcr die Leichen", "tokens": ["Das", "be\u00b7stimmt", "ist", "f\u00fcr", "die", "Lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVPP", "VAFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.114": {"text": "Jener, die am Wege sterben;", "tokens": ["Je\u00b7ner", ",", "die", "am", "We\u00b7ge", "ster\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "$,", "PRELS", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.115": {"text": "F\u00fcr die Gott- und Weltverlass'nen", "tokens": ["F\u00fcr", "die", "Got\u00b7t", "und", "Welt\u00b7ver\u00b7lass'\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "TRUNC", "KON", "NN"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.116": {"text": "Ist dies St\u00