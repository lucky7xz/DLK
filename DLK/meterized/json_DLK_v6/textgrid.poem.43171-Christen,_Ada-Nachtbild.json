{"textgrid.poem.43171": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "Nachtbild", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nacht bedeckt den kleinen Friedhof.", "tokens": ["Nacht", "be\u00b7deckt", "den", "klei\u00b7nen", "Fried\u00b7hof", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "In dem dumpfen Leichenhause", "tokens": ["In", "dem", "dum\u00b7pfen", "Lei\u00b7chen\u00b7hau\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Flackert zitternd einer Lampe", "tokens": ["Fla\u00b7ckert", "zit\u00b7ternd", "ei\u00b7ner", "Lam\u00b7pe"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVPP", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rothe Flamme. \u2013 Heiser knarren", "tokens": ["Ro\u00b7the", "Flam\u00b7me", ".", "\u2013", "Hei\u00b7ser", "knar\u00b7ren"], "token_info": ["word", "word", "punct", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "$(", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jene Th\u00fcren, die das Leben", "tokens": ["Je\u00b7ne", "Th\u00fc\u00b7ren", ",", "die", "das", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sorgsam von dem Tode trennen.", "tokens": ["Sorg\u00b7sam", "von", "dem", "To\u00b7de", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Meine Hand hat sichern Druckes", "tokens": ["Mei\u00b7ne", "Hand", "hat", "si\u00b7chern", "Dru\u00b7ckes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sie ge\u00f6ffnet; wie im Schlafe", "tokens": ["Sie", "ge\u00b7\u00f6ff\u00b7net", ";", "wie", "im", "Schla\u00b7fe"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVPP", "$.", "KOKOM", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Aber wandelnd, dacht' ich nimmer,", "tokens": ["A\u00b7ber", "wan\u00b7delnd", ",", "dacht'", "ich", "nim\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sie zu schlie\u00dfen. \u2013", "tokens": ["Sie", "zu", "schlie\u00b7\u00dfen", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$.", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Leise, wie mit Geisterstimmen", "tokens": ["Lei\u00b7se", ",", "wie", "mit", "Geis\u00b7ters\u00b7tim\u00b7men"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Klagt der Wind dort in den Weiden,", "tokens": ["Klagt", "der", "Wind", "dort", "in", "den", "Wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Pochet z\u00fcrnend an die Fenster,", "tokens": ["Po\u00b7chet", "z\u00fcr\u00b7nend", "an", "die", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Fl\u00fcstert mit den kranken Blumen,", "tokens": ["Fl\u00fcs\u00b7tert", "mit", "den", "kran\u00b7ken", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Die aus der Verwesung sprie\u00dfen,", "tokens": ["Die", "aus", "der", "Ver\u00b7we\u00b7sung", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Treibet mit den Wetterh\u00e4hnen", "tokens": ["Trei\u00b7bet", "mit", "den", "Wet\u00b7ter\u00b7h\u00e4h\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Auf dem Thurm sein \u00e4chzend Spiel,", "tokens": ["Auf", "dem", "Thurm", "sein", "\u00e4ch\u00b7zend", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Flieget wimmernd um das H\u00e4uschen,", "tokens": ["Flie\u00b7get", "wim\u00b7mernd", "um", "das", "H\u00e4usc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Da\u00df die Fenster \u00e4ngstlich klirren", "tokens": ["Da\u00df", "die", "Fens\u00b7ter", "\u00e4ngst\u00b7lich", "klir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Und die Flamme furchsam zuckt ...", "tokens": ["Und", "die", "Flam\u00b7me", "furch\u00b7sam", "zuckt", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Jener bangen rothen Flamme", "tokens": ["Je\u00b7ner", "ban\u00b7gen", "ro\u00b7then", "Flam\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Schwankend Leuchten schien ein Winken,", "tokens": ["Schwan\u00b7kend", "Leuch\u00b7ten", "schien", "ein", "Win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Dem ich folgte, traumbefangen,", "tokens": ["Dem", "ich", "folg\u00b7te", ",", "traum\u00b7be\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Und nun steh' ich in dem engen", "tokens": ["Und", "nun", "steh'", "ich", "in", "dem", "en\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Schaurig-\u00f6den, kahlen St\u00fcbchen, \u2013", "tokens": ["Schau\u00b7rig\u00b7\u00f6\u00b7den", ",", "kah\u00b7len", "St\u00fcb\u00b7chen", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Ich allein bei einem Todten.", "tokens": ["Ich", "al\u00b7lein", "bei", "ei\u00b7nem", "Tod\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}, "line.28": {"text": "Auf zwei Schragen und zwei Brettern", "tokens": ["Auf", "zwei", "Schra\u00b7gen", "und", "zwei", "Bret\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "KON", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Ruht der Todte, alt und h\u00e4\u00dflich,", "tokens": ["Ruht", "der", "Tod\u00b7te", ",", "alt", "und", "h\u00e4\u00df\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Nur in Lumpen eingeh\u00fcllet;", "tokens": ["Nur", "in", "Lum\u00b7pen", "ein\u00b7ge\u00b7h\u00fcl\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Ihm zu Haupte brennt die Lampe,", "tokens": ["Ihm", "zu", "Haup\u00b7te", "brennt", "die", "Lam\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Deren zuckend rothe Lichter", "tokens": ["De\u00b7ren", "zu\u00b7ckend", "ro\u00b7the", "Lich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVPP", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "\u00d6fter wie ein L\u00e4cheln gleiten", "tokens": ["\u00d6f\u00b7ter", "wie", "ein", "L\u00e4\u00b7cheln", "glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "\u00dcber die erstarrten Z\u00fcge", "tokens": ["\u00dc\u00b7ber", "die", "er\u00b7starr\u00b7ten", "Z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Des verkommenen Gesellen.", "tokens": ["Des", "ver\u00b7kom\u00b7me\u00b7nen", "Ge\u00b7sel\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.36": {"text": "Eine harmlos gl\u00e4ub'ge Hand", "tokens": ["Ei\u00b7ne", "harm\u00b7los", "gl\u00e4ub'\u00b7ge", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Suchte seine wildgeballten,", "tokens": ["Such\u00b7te", "sei\u00b7ne", "wild\u00b7ge\u00b7ball\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Nun im Tod gekrampften H\u00e4nde", "tokens": ["Nun", "im", "Tod", "ge\u00b7krampf\u00b7ten", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Fromm zu falten, wie bei Jenen,", "tokens": ["Fromm", "zu", "fal\u00b7ten", ",", "wie", "bei", "Je\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$,", "PWAV", "APPR", "PDAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Deren Leben schlo\u00df ein Beten. \u2013", "tokens": ["De\u00b7ren", "Le\u00b7ben", "schlo\u00df", "ein", "Be\u00b7ten", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "NN", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Auf zwei Schragen und zwei Brettern", "tokens": ["Auf", "zwei", "Schra\u00b7gen", "und", "zwei", "Bret\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "KON", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Ruht der Todte, still und einsam,", "tokens": ["Ruht", "der", "Tod\u00b7te", ",", "still", "und", "ein\u00b7sam", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Schl\u00e4ft den letzten, traumlos, leeren,", "tokens": ["Schl\u00e4ft", "den", "letz\u00b7ten", ",", "traum\u00b7los", ",", "lee\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ADJD", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Ewigen Schlaf.....", "tokens": ["E\u00b7wi\u00b7gen", "Schlaf", "....."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.45": {"text": "Noch am Morgen jagten Bosheit,", "tokens": ["Noch", "am", "Mor\u00b7gen", "jag\u00b7ten", "Bos\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Breit Behagen \u2013 dem das Elend", "tokens": ["Breit", "Be\u00b7ha\u00b7gen", "\u2013", "dem", "das", "E\u00b7lend"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$(", "ART", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Unverst\u00e4ndlich \u2013 Rohheit, Kaltsinn", "tokens": ["Un\u00b7ver\u00b7st\u00e4nd\u00b7lich", "\u2013", "Roh\u00b7heit", ",", "Kal\u00b7tsinn"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["ADJD", "$(", "NN", "$,", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.48": {"text": "Ruhlos ihn von Th\u00fcr zu Th\u00fcre,", "tokens": ["Ruh\u00b7los", "ihn", "von", "Th\u00fcr", "zu", "Th\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Und des Abends wankte jener", "tokens": ["Und", "des", "A\u00b7bends", "wank\u00b7te", "je\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PDAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Ungl\u00fccksel'ge, wie betrunken,", "tokens": ["Un\u00b7gl\u00fcck\u00b7sel'\u00b7ge", ",", "wie", "be\u00b7trun\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Durch die Stra\u00dfen. Hunger weinte", "tokens": ["Durch", "die", "Stra\u00b7\u00dfen", ".", "Hun\u00b7ger", "wein\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Aus den kranken, trock'nen Augen,", "tokens": ["Aus", "den", "kran\u00b7ken", ",", "trock'\u00b7nen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.53": {"text": "Aber Trotz zuckt um die Lippen,", "tokens": ["A\u00b7ber", "Trotz", "zuckt", "um", "die", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Als die Buben, die ihm folgten,", "tokens": ["Als", "die", "Bu\u00b7ben", ",", "die", "ihm", "folg\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "N\u00e4her trabten, um das Unthier", "tokens": ["N\u00e4\u00b7her", "trab\u00b7ten", ",", "um", "das", "Un\u00b7thier"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "KOUI", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Zu beschauen, das man eben", "tokens": ["Zu", "be\u00b7schau\u00b7en", ",", "das", "man", "e\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "PIS", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.57": {"text": "Auf Befehl der weisen, milden", "tokens": ["Auf", "Be\u00b7fehl", "der", "wei\u00b7sen", ",", "mil\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "ART", "ADJA", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"text": "Obrigkeit von dannen hetzet.", "tokens": ["Ob\u00b7rig\u00b7keit", "von", "dan\u00b7nen", "het\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.59": {"text": "Vagabund! so klingt es lachend", "tokens": ["Va\u00b7ga\u00b7bund", "!", "so", "klingt", "es", "la\u00b7chend"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.60": {"text": "Aus dem Munde wilder Kinder;", "tokens": ["Aus", "dem", "Mun\u00b7de", "wil\u00b7der", "Kin\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.61": {"text": "Vagabund! so klingt es h\u00f6hnend", "tokens": ["Va\u00b7ga\u00b7bund", "!", "so", "klingt", "es", "h\u00f6h\u00b7nend"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.62": {"text": "Aus dem Mund der klugen Alten;", "tokens": ["Aus", "dem", "Mund", "der", "klu\u00b7gen", "Al\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.63": {"text": "Vagabund! schreit roh der B\u00fcttel;", "tokens": ["Va\u00b7ga\u00b7bund", "!", "schreit", "roh", "der", "B\u00fct\u00b7tel", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.64": {"text": "Vagabund! so \u00e4chzt er selber,", "tokens": ["Va\u00b7ga\u00b7bund", "!", "so", "\u00e4chzt"