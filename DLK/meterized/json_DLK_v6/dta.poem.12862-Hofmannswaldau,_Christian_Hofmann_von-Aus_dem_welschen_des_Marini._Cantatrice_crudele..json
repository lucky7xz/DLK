{"dta.poem.12862": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Aus dem welschen des   Marini.  \n Cantatrice crudele.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr felsen und ihr b\u00e4um', entflieht doch Daphnens grimme,", "tokens": ["Ihr", "fel\u00b7sen", "und", "ihr", "b\u00e4um'", ",", "ent\u00b7flieht", "doch", "Daph\u00b7nens", "grim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und last ihr singen euch nicht ins verderben ziehn.", "tokens": ["Und", "last", "ihr", "sin\u00b7gen", "euch", "nicht", "ins", "ver\u00b7der\u00b7ben", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "PPER", "PTKNEG", "APPRART", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die unempfindligkeit wird zwar wol von euch fliehn;", "tokens": ["Die", "un\u00b7emp\u00b7find\u00b7lig\u00b7keit", "wird", "zwar", "wol", "von", "euch", "fliehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Allein so bald sie flieht, und ihr von Daphnens stimme", "tokens": ["Al\u00b7lein", "so", "bald", "sie", "flieht", ",", "und", "ihr", "von", "Daph\u00b7nens", "stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "PPER", "VVFIN", "$,", "KON", "PPER", "APPR", "NE", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Stimm und bewegung kriegt, so wird sie selbst ein stein,", "tokens": ["Stimm", "und", "be\u00b7we\u00b7gung", "kriegt", ",", "so", "wird", "sie", "selbst", "ein", "stein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Ja tauber gegen euch, als b\u00e4um\u2019 und felsen seyn.", "tokens": ["Ja", "tau\u00b7ber", "ge\u00b7gen", "euch", ",", "als", "b\u00e4um'", "und", "fel\u00b7sen", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "APPR", "PPER", "$,", "KOUS", "ADJD", "KON", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}