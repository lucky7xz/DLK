{"textgrid.poem.44312": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Das Grab im Walde", "genre": "verse", "period": "N.A.", "pub_year": 1808, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwie das treue Weib dem Mann,", "tokens": ["\u00bb", "wie", "das", "treu\u00b7e", "Weib", "dem", "Mann", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "bleib mir liebend zugetan,", "tokens": ["bleib", "mir", "lie\u00b7bend", "zu\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "bleib mir treu und hold,", "tokens": ["bleib", "mir", "treu", "und", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "treu und rein wie Gold\u00ab!", "tokens": ["treu", "und", "rein", "wie", "Gold", "\u00ab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "KON", "ADJD", "KOKOM", "NN", "$(", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "sprach zu Fr\u00e4ulein Edeltrud", "tokens": ["sprach", "zu", "Fr\u00e4u\u00b7lein", "E\u00b7delt\u00b7rud"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hans von Werden und voll Mut", "tokens": ["Hans", "von", "Wer\u00b7den", "und", "voll", "Mut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "NN", "KON", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "schwingt er sich aufs Ro\u00df", "tokens": ["schwingt", "er", "sich", "aufs", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "und verl\u00e4\u00dft das Schlo\u00df.", "tokens": ["und", "ver\u00b7l\u00e4\u00dft", "das", "Schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Dahin, wo den Sarazen", "tokens": ["Da\u00b7hin", ",", "wo", "den", "Sa\u00b7ra\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PAV", "$,", "PWAV", "ART", "NN"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.2": {"text": "tapfrer Christen Schwerter m\u00e4hn,", "tokens": ["tapf\u00b7rer", "Chris\u00b7ten", "Schwer\u00b7ter", "m\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "dahin ruft ihn schnell", "tokens": ["da\u00b7hin", "ruft", "ihn", "schnell"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Friederichs Befehl.", "tokens": ["Frie\u00b7de\u00b7richs", "Be\u00b7fehl", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Und nach manchem harten Strau\u00df", "tokens": ["Und", "nach", "man\u00b7chem", "har\u00b7ten", "Strau\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "schifft er sich zu Joppe aus,", "tokens": ["schifft", "er", "sich", "zu", "Jop\u00b7pe", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NE", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "sieht der Christen Blut", "tokens": ["sieht", "der", "Chris\u00b7ten", "Blut"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "und der Feinde Wut.", "tokens": ["und", "der", "Fein\u00b7de", "Wut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Seines tapfern Armes Schwert", "tokens": ["Sei\u00b7nes", "tap\u00b7fern", "Ar\u00b7mes", "Schwert"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "wird gef\u00fcrchtet und geehrt,", "tokens": ["wird", "ge\u00b7f\u00fcrch\u00b7tet", "und", "ge\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "KON", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und der Feind entweicht,", "tokens": ["und", "der", "Feind", "ent\u00b7weicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "wo es ihn erreicht.", "tokens": ["wo", "es", "ihn", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Und er h\u00e4ufet Sieg auf Sieg;", "tokens": ["Und", "er", "h\u00e4u\u00b7fet", "Sieg", "auf", "Sieg", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "doch ein Friede hemmt den Krieg;", "tokens": ["doch", "ein", "Frie\u00b7de", "hemmt", "den", "Krieg", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "zu dem Liebchen hin", "tokens": ["zu", "dem", "Lieb\u00b7chen", "hin"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKVZ"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "will der Held nun ziehn.", "tokens": ["will", "der", "Held", "nun", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Er verl\u00e4\u00dft des Landes Scho\u00df,", "tokens": ["Er", "ver\u00b7l\u00e4\u00dft", "des", "Lan\u00b7des", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "wo sein Blut f\u00fcr Christus flo\u00df,", "tokens": ["wo", "sein", "Blut", "f\u00fcr", "Chris\u00b7tus", "flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "eilt vom heilgem Strand", "tokens": ["eilt", "vom", "heil\u00b7gem", "Strand"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "in der Heimat Land.", "tokens": ["in", "der", "Hei\u00b7mat", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Als er nun durch Deutschland zoh,", "tokens": ["Als", "er", "nun", "durch", "Deutschland", "zoh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NE", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "eben einen Wald durchfloh,", "tokens": ["e\u00b7ben", "ei\u00b7nen", "Wald", "durch\u00b7floh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "h\u00f6rt er ein Geschrei,", "tokens": ["h\u00f6rt", "er", "ein", "Ge\u00b7schrei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "er zur Hilf herbei!", "tokens": ["er", "zur", "Hilf", "her\u00b7bei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Und er sieht auf hohem Ro\u00df", "tokens": ["Und", "er", "sieht", "auf", "ho\u00b7hem", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "hier ein M\u00e4dchen, hehr und gro\u00df", "tokens": ["hier", "ein", "M\u00e4d\u00b7chen", ",", "hehr", "und", "gro\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "war der Glieder Bau,", "tokens": ["war", "der", "Glie\u00b7der", "Bau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "sanft des Auges Blau,", "tokens": ["sanft", "des", "Au\u00b7ges", "Blau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Das um Hilf zum Himmel blickt,", "tokens": ["Das", "um", "Hilf", "zum", "Him\u00b7mel", "blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NE", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "fest von R\u00e4uberarm umstrickt;", "tokens": ["fest", "von", "R\u00e4u\u00b7be\u00b7rarm", "um\u00b7strickt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "k\u00fchn und stark bewehrt", "tokens": ["k\u00fchn", "und", "stark", "be\u00b7wehrt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "VVPP"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "folgt ein Trupp zu Pferd.", "tokens": ["folgt", "ein", "Trupp", "zu", "Pferd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "Werden scheut keine Gefahr,", "tokens": ["Wer\u00b7den", "scheut", "kei\u00b7ne", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "achtet nicht der Zahl der Schar,", "tokens": ["ach\u00b7tet", "nicht", "der", "Zahl", "der", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "st\u00fcrzt mit Hieb und Stich", "tokens": ["st\u00fcrzt", "mit", "Hieb", "und", "Stich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "in die Feinde sich.", "tokens": ["in", "die", "Fein\u00b7de", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "Diese stutzen, stehn verbl\u00fcfft,", "tokens": ["Die\u00b7se", "stut\u00b7zen", ",", "stehn", "ver\u00b7bl\u00fcfft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVINF", "$,", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "dann erst, als sein Schwert sie trifft,", "tokens": ["dann", "erst", ",", "als", "sein", "Schwert", "sie", "trifft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "jeder sich besinnt,", "tokens": ["je\u00b7der", "sich", "be\u00b7sinnt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "PRF", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "und der Kampf beginnt.", "tokens": ["und", "der", "Kampf", "be\u00b7ginnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.13": {"line.1": {"text": "Unsers Ritters tapfrer Arm", "tokens": ["Un\u00b7sers", "Rit\u00b7ters", "tapf\u00b7rer", "Arm"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "m\u00e4het in der Feinde Schwarm,", "tokens": ["m\u00e4\u00b7het", "in", "der", "Fein\u00b7de", "Schwarm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "strecket manchen hin,", "tokens": ["stre\u00b7cket", "man\u00b7chen", "hin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "und die andern fliehn.", "tokens": ["und", "die", "an\u00b7dern", "fliehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.14": {"line.1": {"text": "Jetzt naht er dem M\u00e4dchen sich;", "tokens": ["Jetzt", "naht", "er", "dem", "M\u00e4d\u00b7chen", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PRF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ha! durchbohrt von einem Stich", "tokens": ["Ha", "!", "durch\u00b7bohrt", "von", "ei\u00b7nem", "Stich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVPP", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "ist sie, weh, ihm graut!", "tokens": ["ist", "sie", ",", "weh", ",", "ihm", "graut", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PTKVZ", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "er erkennt die Braut!", "tokens": ["er", "er\u00b7kennt", "die", "Braut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.15": {"line.1": {"text": "\u00bbwerden, ach, ich bin verm\u00e4hlt\u00ab,", "tokens": ["\u00bb", "wer\u00b7den", ",", "ach", ",", "ich", "bin", "ver\u00b7m\u00e4hlt", "\u00ab", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAINF", "$,", "ITJ", "$,", "PPER", "VAFIN", "VVPP", "$(", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "st\u00f6hnt die Holde matt und f\u00e4llt", "tokens": ["st\u00f6hnt", "die", "Hol\u00b7de", "matt", "und", "f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "auf den Rasen hin,", "tokens": ["auf", "den", "Ra\u00b7sen", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "weg war Geist und Sinn!", "tokens": ["weg", "war", "Geist", "und", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.16": {"line.1": {"text": "Er h\u00e4lt sie mit stillem Harm", "tokens": ["Er", "h\u00e4lt", "sie", "mit", "stil\u00b7lem", "Harm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "weinend in dem lassem Arm,", "tokens": ["wei\u00b7nend", "in", "dem", "las\u00b7sem", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "aber nun durchschallt", "tokens": ["a\u00b7ber", "nun", "durch\u00b7schallt"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "H\u00f6rnerton den Wald.", "tokens": ["H\u00f6r\u00b7ner\u