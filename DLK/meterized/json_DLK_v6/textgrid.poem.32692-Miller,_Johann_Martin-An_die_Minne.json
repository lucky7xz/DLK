{"textgrid.poem.32692": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "An die Minne", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Minne, wie so wundersam", "tokens": ["Min\u00b7ne", ",", "wie", "so", "wun\u00b7der\u00b7sam"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kannst du alles machen!", "tokens": ["Kannst", "du", "al\u00b7les", "ma\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Einem giebst du lauter Gram,", "tokens": ["Ei\u00b7nem", "giebst", "du", "lau\u00b7ter", "Gram", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dem andern Lachen!", "tokens": ["Und", "dem", "an\u00b7dern", "La\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Dem machst du die Wangen rot,", "tokens": ["Dem", "machst", "du", "die", "Wan\u00b7gen", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und dem andern bla\u00df, wie Tod!", "tokens": ["Und", "dem", "an\u00b7dern", "bla\u00df", ",", "wie", "Tod", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJD", "$,", "PWAV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Liebchens Augen l\u00e4ssest du,", "tokens": ["Lieb\u00b7chens", "Au\u00b7gen", "l\u00e4s\u00b7sest", "du", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie die Sonne scheinen;", "tokens": ["Wie", "die", "Son\u00b7ne", "schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Meine m\u00fcssen sonder Ruh", "tokens": ["Mei\u00b7ne", "m\u00fcs\u00b7sen", "son\u00b7der", "Ruh"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihrethalben weinen.", "tokens": ["Ih\u00b7re\u00b7thal\u00b7ben", "wei\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sie hat immer Maienzeit,", "tokens": ["Sie", "hat", "im\u00b7mer", "Mai\u00b7en\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und ich lauter Winterleid.", "tokens": ["Und", "ich", "lau\u00b7ter", "Win\u00b7ter\u00b7leid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "La\u00df, o Minne, doch einmal", "tokens": ["La\u00df", ",", "o", "Min\u00b7ne", ",", "doch", "ein\u00b7mal"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "$,", "FM", "NN", "$,", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommen meinen Maien,", "tokens": ["Kom\u00b7men", "mei\u00b7nen", "Mai\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und den lieben Sonnenstrahl", "tokens": ["Und", "den", "lie\u00b7ben", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meinen Sinn erfreuen!", "tokens": ["Mei\u00b7nen", "Sinn", "er\u00b7freu\u00b7en", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Dann sing' ich, nach Vogelweis,", "tokens": ["Dann", "sing'", "ich", ",", "nach", "Vo\u00b7gel\u00b7weis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Dir ein Lied zu Lob und Preis!", "tokens": ["Dir", "ein", "Lied", "zu", "Lob", "und", "Preis", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}