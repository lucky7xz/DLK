{"dta.poem.5659": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom Geist und C\u00f6rper", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der C\u00f6rper, der bewegt, weil er gef\u00fcg't aus Erden;", "tokens": ["Der", "C\u00f6r\u00b7per", ",", "der", "be\u00b7wegt", ",", "weil", "er", "ge\u00b7f\u00fcg't", "aus", "Er\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$,", "KOUS", "PPER", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mu\u00df dadurch aufgel\u00f6st, ver\u00e4ndert, Erde werden.", "tokens": ["Mu\u00df", "da\u00b7durch", "auf\u00b7ge\u00b7l\u00f6st", ",", "ver\u00b7\u00e4n\u00b7dert", ",", "Er\u00b7de", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PAV", "VVPP", "$,", "VVPP", "$,", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Geist ist einfach, der in sich", "tokens": ["Der", "Geist", "ist", "ein\u00b7fach", ",", "der", "in", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PRELS", "APPR", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von keinen Theilen wei\u00df, und unver\u00e4nderlich.", "tokens": ["Von", "kei\u00b7nen", "Thei\u00b7len", "wei\u00df", ",", "und", "un\u00b7ver\u00b7\u00e4n\u00b7der\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Daher nat\u00fcrlich folgt: er sey", "tokens": ["Da\u00b7her", "na\u00b7t\u00fcr\u00b7lich", "folgt", ":", "er", "sey"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "ADV", "VVFIN", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von Aenderung und von Verwesung frey.", "tokens": ["Von", "A\u00b7en\u00b7de\u00b7rung", "und", "von", "Ver\u00b7we\u00b7sung", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "PTKVZ", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}}}}}