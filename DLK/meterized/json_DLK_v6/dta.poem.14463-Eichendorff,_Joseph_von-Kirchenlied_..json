{"dta.poem.14463": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Kirchenlied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "O Maria, meine Liebe!", "tokens": ["O", "Ma\u00b7ria", ",", "mei\u00b7ne", "Lie\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Denk' ich recht im Herzen Dein:", "tokens": ["Denk'", "ich", "recht", "im", "Her\u00b7zen", "Dein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPRART", "NN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwindet alles Schwer' und Tr\u00fcbe,", "tokens": ["Schwin\u00b7det", "al\u00b7les", "Schwer'", "und", "Tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und, wie heller Morgenschein,", "tokens": ["Und", ",", "wie", "hel\u00b7ler", "Mor\u00b7gen\u00b7schein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dringt's durch Lust und ird'schen Schmerz", "tokens": ["Dringt's", "durch", "Lust", "und", "ird'\u00b7schen", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Leuchtend mir durch's ganze Herz.", "tokens": ["Leuch\u00b7tend", "mir", "durch's", "gan\u00b7ze", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}