{"dta.poem.6814": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lasst uns nunmehr, entfernt von schwachen Dunckelheiten", "tokens": ["Lasst", "uns", "nun\u00b7mehr", ",", "ent\u00b7fernt", "von", "schwa\u00b7chen", "Dun\u00b7ckel\u00b7hei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der eitlen Wissenschafft, besch\u00e4fftigt seyn", "tokens": ["Der", "eit\u00b7len", "Wis\u00b7sen\u00b7schafft", ",", "be\u00b7sch\u00e4ff\u00b7tigt", "seyn"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP", "VAINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In ruhigen Vergn\u00fcglichkeiten!", "tokens": ["In", "ru\u00b7hi\u00b7gen", "Ver\u00b7gn\u00fcg\u00b7lich\u00b7kei\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Es ist ja nach dem Augenschein,", "tokens": ["Es", "ist", "ja", "nach", "dem", "Au\u00b7gen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Geist, indem er \u00fcberf\u00fchret,", "tokens": ["Der", "Geist", ",", "in\u00b7dem", "er", "\u00fc\u00b7berf\u00b7\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nunmehr ger\u00fchret.", "tokens": ["Nun\u00b7mehr", "ge\u00b7r\u00fch\u00b7ret", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Es steht ja hoffentlich nunmehr die Wahrheit", "tokens": ["Es", "steht", "ja", "hof\u00b7fent\u00b7lich", "nun\u00b7mehr", "die", "Wahr\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "ART", "NN"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "In \u00fcberzeugender und unleugbarer Klarheit.", "tokens": ["In", "\u00fc\u00b7berz\u00b7eu\u00b7gen\u00b7der", "und", "un\u00b7leug\u00b7ba\u00b7rer", "Klar\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wir wollen nun hinfort von C\u00f6rpern trennen", "tokens": ["Wir", "wol\u00b7len", "nun", "hin\u00b7fort", "von", "C\u00f6r\u00b7pern", "tren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der Kr\u00e4fft\u2019 und Formen Meng\u2019 die, mit so eitlen Nahmen,", "tokens": ["Der", "Kr\u00e4fft'", "und", "For\u00b7men", "Meng'", "die", ",", "mit", "so", "eit\u00b7len", "Nah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NE", "ART", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So h\u00e4uffig, da\u00df man sie kaum z\u00e4hlt,", "tokens": ["So", "h\u00e4uf\u00b7fig", ",", "da\u00df", "man", "sie", "kaum", "z\u00e4hlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PIS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Bisher uns stets vor Augen kamen,", "tokens": ["Bis\u00b7her", "uns", "stets", "vor", "Au\u00b7gen", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Als Eigenschafften, die wir nicht mehr kennen", "tokens": ["Als", "Ei\u00b7gen\u00b7schaff\u00b7ten", ",", "die", "wir", "nicht", "mehr", "ken\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "PRELS", "PPER", "PTKNEG", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "An Dingen, welche nicht beseelt.", "tokens": ["An", "Din\u00b7gen", ",", "wel\u00b7che", "nicht", "be\u00b7seelt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}