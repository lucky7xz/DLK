{"dta.poem.18323": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Wie's gemacht wird!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Da schrieen die Junker Hurrah", "tokens": ["Da", "schri\u00b7een", "die", "Jun\u00b7ker", "Hur\u00b7rah"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Und sangen das alte hochherrliche Lied:", "tokens": ["Und", "san\u00b7gen", "das", "al\u00b7te", "hoch\u00b7herr\u00b7li\u00b7che", "Lied", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+-++--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Hepphepp Juvivallerala!", "tokens": ["Hep\u00b7phepp", "Ju\u00b7vi\u00b7val\u00b7le\u00b7ra\u00b7la", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Doch das Volk stand auf und schrie Zeter und Mord!", "tokens": ["Doch", "das", "Volk", "stand", "auf", "und", "schrie", "Ze\u00b7ter", "und", "Mord", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "KON", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Hie Hecker und Robert Blum!", "tokens": ["Hie", "He\u00b7cker", "und", "Ro\u00b7bert", "Blum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NE", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da erfand man schleunigst das Kautschukwort:", "tokens": ["Da", "er\u00b7fand", "man", "schleu\u00b7nigst", "das", "Kaut\u00b7schuk\u00b7wort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Praktisches Christenthum!", "tokens": ["Prak\u00b7ti\u00b7sches", "Chris\u00b7ten\u00b7thum", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}