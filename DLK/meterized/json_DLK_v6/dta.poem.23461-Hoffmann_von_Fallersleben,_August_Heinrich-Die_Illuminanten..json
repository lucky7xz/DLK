{"dta.poem.23461": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Illuminanten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erfindungsreichste Zeit von allen Zeiten!", "tokens": ["Er\u00b7fin\u00b7dungs\u00b7reichs\u00b7te", "Zeit", "von", "al\u00b7len", "Zei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir schreiten fort um weiter fortzuschreiten.", "tokens": ["Wir", "schrei\u00b7ten", "fort", "um", "wei\u00b7ter", "fort\u00b7zu\u00b7schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Benutzt wird alles was uns Gott verliehn,", "tokens": ["Be\u00b7nutzt", "wird", "al\u00b7les", "was", "uns", "Gott", "ver\u00b7liehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIS", "PWS", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der ganze Mensch, sein Koth und sein Urin,", "tokens": ["Der", "gan\u00b7ze", "Mensch", ",", "sein", "Koth", "und", "sein", "U\u00b7rin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Sogar sein Leichnam \u2014 Lichter draus zu ziehn.", "tokens": ["So\u00b7gar", "sein", "Leich\u00b7nam", "Lich\u00b7ter", "draus", "zu", "ziehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$(", "NN", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Freut euch, ihr dummen finstern Schafsgesichter!", "tokens": ["Freut", "euch", ",", "ihr", "dum\u00b7men", "fins\u00b7tern", "Schafs\u00b7ge\u00b7sich\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach eurem Tode werdet ihr noch Lichter,", "tokens": ["Nach", "eu\u00b7rem", "To\u00b7de", "wer\u00b7det", "ihr", "noch", "Lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und jenen Schatten, den ihr habt gemacht,", "tokens": ["Und", "je\u00b7nen", "Schat\u00b7ten", ",", "den", "ihr", "habt", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "PRELS", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bezahlt ihr einst mit Lichtes Glanz und Pracht \u2014", "tokens": ["Be\u00b7zahlt", "ihr", "einst", "mit", "Lich\u00b7tes", "Glanz", "und", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ihr Schafsgesichter, habt ihr ", "tokens": ["Ihr", "Schafs\u00b7ge\u00b7sich\u00b7ter", ",", "habt", "ihr"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}