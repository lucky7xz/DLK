{"textgrid.poem.47539": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[nicht mit trister Miene]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht mit trister Miene,", "tokens": ["Nicht", "mit", "tris\u00b7ter", "Mie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Nicht mit Thr\u00e4nentr\u00fcbe", "tokens": ["Nicht", "mit", "Thr\u00e4\u00b7nen\u00b7tr\u00fc\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["PTKNEG", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Komm zu meinem Grabe;", "tokens": ["Komm", "zu", "mei\u00b7nem", "Gra\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Komm mit einem Becher,", "tokens": ["Komm", "mit", "ei\u00b7nem", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Komm mit einem Liede,", "tokens": ["Komm", "mit", "ei\u00b7nem", "Lie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Angestimmt aus voller", "tokens": ["An\u00b7ge\u00b7stimmt", "aus", "vol\u00b7ler"], "token_info": ["word", "word", "word"], "pos": ["APPR", "APPR", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Jubelheller Brust!", "tokens": ["Ju\u00b7bel\u00b7hel\u00b7ler", "Brust", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Steigen aus dem Dust", "tokens": ["Stei\u00b7gen", "aus", "dem", "Dust"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Wird der alte Zecher,", "tokens": ["Wird", "der", "al\u00b7te", "Ze\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Trinken aus dem Becher,", "tokens": ["Trin\u00b7ken", "aus", "dem", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Stimmen in dein Lied ein,", "tokens": ["Stim\u00b7men", "in", "dein", "Lied", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Sich im Tanze heben,", "tokens": ["Sich", "im", "Tan\u00b7ze", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Springen hoch vor Lust.", "tokens": ["Sprin\u00b7gen", "hoch", "vor", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}