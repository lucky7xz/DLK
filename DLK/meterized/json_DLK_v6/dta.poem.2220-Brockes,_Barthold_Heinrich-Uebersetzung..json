{"dta.poem.2220": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Uebersetzung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Owie ist doch das Verm\u00f6gen", "tokens": ["O\u00b7wie", "ist", "doch", "das", "Ver\u00b7m\u00f6\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohl zu thun, ein wahrer Segen!", "tokens": ["Wohl", "zu", "thun", ",", "ein", "wah\u00b7rer", "Se\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Es ist g\u00f6ttlich, ja es ist eine rechte Seelenfreude,", "tokens": ["Es", "ist", "g\u00f6tt\u00b7lich", ",", "ja", "es", "ist", "ei\u00b7ne", "rech\u00b7te", "See\u00b7len\u00b7freu\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "ADV", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-++-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Und das einzge, warum ich Reich\u2019 und M\u00e4chtige be-", "tokens": ["Und", "das", "einz\u00b7ge", ",", "wa\u00b7rum", "ich", "Reich'", "und", "M\u00e4ch\u00b7ti\u00b7ge", "be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "$,", "PWAV", "PPER", "NN", "KON", "ADJA", "TRUNC"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "neide.", "tokens": ["nei\u00b7de", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}