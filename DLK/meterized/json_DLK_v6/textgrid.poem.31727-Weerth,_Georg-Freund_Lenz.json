{"textgrid.poem.31727": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Freund Lenz", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus fernen Wolken braust ein dumpfer Ton.", "tokens": ["Aus", "fer\u00b7nen", "Wol\u00b7ken", "braust", "ein", "dum\u00b7pfer", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Donner sind es, so der Welt verk\u00fcnden,", "tokens": ["Die", "Don\u00b7ner", "sind", "es", ",", "so", "der", "Welt", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df wieder der Natur geliebter Sohn,", "tokens": ["Da\u00df", "wie\u00b7der", "der", "Na\u00b7tur", "ge\u00b7lieb\u00b7ter", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Fr\u00fchling, wandelt zu der Erde Gr\u00fcnden.", "tokens": ["Der", "Fr\u00fch\u00b7ling", ",", "wan\u00b7delt", "zu", "der", "Er\u00b7de", "Gr\u00fcn\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Bei andern V\u00f6lkern hat er lang geweilt,", "tokens": ["Bei", "an\u00b7dern", "V\u00f6l\u00b7kern", "hat", "er", "lang", "ge\u00b7weilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da war's, da\u00df j\u00fcngst die Kunde ihn ereilt,", "tokens": ["Da", "wa\u00b7r's", ",", "da\u00df", "j\u00fcngst", "die", "Kun\u00b7de", "ihn", "er\u00b7eilt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOUS", "ADV", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "----+-+-+-+", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Wir hier im Norden tr\u00fcgen hei\u00df Verlangen,", "tokens": ["Wir", "hier", "im", "Nor\u00b7den", "tr\u00fc\u00b7gen", "hei\u00df", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Aufs neu zu schauen seiner Bl\u00fcte Prangen.", "tokens": ["Aufs", "neu", "zu", "schau\u00b7en", "sei\u00b7ner", "Bl\u00fc\u00b7te", "Pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "PTKZU", "VVINF", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er kommt. Und aus des S\u00fcdens frohen Talen,", "tokens": ["Er", "kommt", ".", "Und", "aus", "des", "S\u00fc\u00b7dens", "fro\u00b7hen", "Ta\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KON", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo tr\u00e4umend er im Lorbeerwalde lag,", "tokens": ["Wo", "tr\u00e4u\u00b7mend", "er", "im", "Lor\u00b7beer\u00b7wal\u00b7de", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo er zum Fest bei glutgef\u00fcllten Schalen", "tokens": ["Wo", "er", "zum", "Fest", "bei", "glut\u00b7ge\u00b7f\u00fcll\u00b7ten", "Scha\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Myrtenhaines vollste Kr\u00e4nze brach,", "tokens": ["Des", "Myr\u00b7ten\u00b7hai\u00b7nes", "volls\u00b7te", "Kr\u00e4n\u00b7ze", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wo mit dem Zephir er die Wangen k\u00fchlte", "tokens": ["Wo", "mit", "dem", "Ze\u00b7phir", "er", "die", "Wan\u00b7gen", "k\u00fchl\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und buhlerisch in schwarzen Locken w\u00fchlte \u2013", "tokens": ["Und", "buh\u00b7le\u00b7risch", "in", "schwar\u00b7zen", "Lo\u00b7cken", "w\u00fchl\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Fern aus dem S\u00fcden hat er alle Pracht", "tokens": ["Fern", "aus", "dem", "S\u00fc\u00b7den", "hat", "er", "al\u00b7le", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "VAFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Herauf jetzt in den Norden uns gebracht.", "tokens": ["Her\u00b7auf", "jetzt", "in", "den", "Nor\u00b7den", "uns", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Er setzt sich l\u00e4chelnd auf die H\u00fcgel hin \u2013", "tokens": ["Er", "setzt", "sich", "l\u00e4\u00b7chelnd", "auf", "die", "H\u00fc\u00b7gel", "hin", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da weht ein Duften rings durch Fels und Auen,", "tokens": ["Da", "weht", "ein", "Duf\u00b7ten", "rings", "durch", "Fels", "und", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum Forste lustig Falk und Taube ziehn,", "tokens": ["Zum", "Fors\u00b7te", "lus\u00b7tig", "Falk", "und", "Tau\u00b7be", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Knospen r\u00f6tlich aus den G\u00e4rten schauen.", "tokens": ["Und", "Knos\u00b7pen", "r\u00f6t\u00b7lich", "aus", "den", "G\u00e4r\u00b7ten", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der B\u00e4che Lauf schm\u00fcckt er mit lichtem Samt,", "tokens": ["Der", "B\u00e4\u00b7che", "Lauf", "schm\u00fcckt", "er", "mit", "lich\u00b7tem", "Samt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Es blitzt der Tau, hellauf die Sonne flammt \u2013", "tokens": ["Es", "blitzt", "der", "Tau", ",", "hel\u00b7lauf", "die", "Son\u00b7ne", "flammt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und nieder steigt er von den H\u00fcgelthronen", "tokens": ["Und", "nie\u00b7der", "steigt", "er", "von", "den", "H\u00fc\u00b7gel\u00b7thro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Hinab zum Tale, wo die Menschen wohnen", "tokens": ["Hin\u00b7ab", "zum", "Ta\u00b7le", ",", "wo", "die", "Men\u00b7schen", "woh\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "PWAV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mit ihrer Lust, mit ihrem bittern Leid,", "tokens": ["Mit", "ih\u00b7rer", "Lust", ",", "mit", "ih\u00b7rem", "bit\u00b7tern", "Leid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit ihren Freuden, ach, und ihren Tr\u00e4nen,", "tokens": ["Mit", "ih\u00b7ren", "Freu\u00b7den", ",", "ach", ",", "und", "ih\u00b7ren", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ITJ", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit all dem Ringen, all dem herben Streit,", "tokens": ["Mit", "all", "dem", "Rin\u00b7gen", ",", "all", "dem", "her\u00b7ben", "Streit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "$,", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit all dem Hoffen, all dem stillen Sehnen.", "tokens": ["Mit", "all", "dem", "Hof\u00b7fen", ",", "all", "dem", "stil\u00b7len", "Seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "$,", "PIAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Er ist's, der in des Armen H\u00fctte schaut,", "tokens": ["Er", "ist's", ",", "der", "in", "des", "Ar\u00b7men", "H\u00fct\u00b7te", "schaut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der zu ihm spricht, wenn kaum der Morgen graut:", "tokens": ["Der", "zu", "ihm", "spricht", ",", "wenn", "kaum", "der", "Mor\u00b7gen", "graut", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "\u00bbgetrost, wie deine Freuden auch zerstieben,", "tokens": ["\u00bb", "ge\u00b7trost", ",", "wie", "dei\u00b7ne", "Freu\u00b7den", "auch", "zer\u00b7stie\u00b7ben", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "$,", "PWAV", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dir Armen ist der Lenz noch treu geblieben!", "tokens": ["Dir", "Ar\u00b7men", "ist", "der", "Lenz", "noch", "treu", "ge\u00b7blie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "ART", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Hinaus! Durch meine Blumen sollst du schreiten,", "tokens": ["Hin\u00b7aus", "!", "Durch", "mei\u00b7ne", "Blu\u00b7men", "sollst", "du", "schrei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "APPR", "PPOSAT", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich labe dich mit meiner W\u00e4lder Gr\u00fcn,", "tokens": ["Ich", "la\u00b7be", "dich", "mit", "mei\u00b7ner", "W\u00e4l\u00b7der", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durch Busch und Wiese will ich dich geleiten", "tokens": ["Durch", "Busch", "und", "Wie\u00b7se", "will", "ich", "dich", "ge\u00b7lei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Berg hinan, wo meine Rosen gl\u00fchn.", "tokens": ["Den", "Berg", "hi\u00b7nan", ",", "wo", "mei\u00b7ne", "Ro\u00b7sen", "gl\u00fchn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PWAV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich zeige dir, wie nieder zu den Fl\u00e4chen", "tokens": ["Ich", "zei\u00b7ge", "dir", ",", "wie", "nie\u00b7der", "zu", "den", "Fl\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Befreit die Str\u00f6me ihre Bahnen brechen,", "tokens": ["Be\u00b7freit", "die", "Str\u00f6\u00b7me", "ih\u00b7re", "Bah\u00b7nen", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und wie der Nacht erbl\u00fcht der Sterne Schein,", "tokens": ["Und", "wie", "der", "Nacht", "er\u00b7bl\u00fcht", "der", "Ster\u00b7ne", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Zieh ich, der Lenz, in deine Seele ein!", "tokens": ["Zieh", "ich", ",", "der", "Lenz", ",", "in", "dei\u00b7ne", "See\u00b7le", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ich k\u00fcsse deiner Kinder m\u00fcde Stirnen,", "tokens": ["Ich", "k\u00fcs\u00b7se", "dei\u00b7ner", "Kin\u00b7der", "m\u00fc\u00b7de", "Stir\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ob all ihr Glanz verloschen und verstaubt;", "tokens": ["Ob", "all", "ihr", "Glanz", "ver\u00b7lo\u00b7schen", "und", "ver\u00b7staubt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "PPOSAT", "NN", "VVINF", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich will gleich der Lawine von den Firnen", "tokens": ["Ich", "will", "gleich", "der", "La\u00b7wi\u00b7ne", "von", "den", "Fir\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "W\u00e4lzen den Gram von ihrer Mutter Haupt.", "tokens": ["W\u00e4l\u00b7zen", "den", "Gram", "von", "ih\u00b7rer", "Mut\u00b7ter", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Und Feuer menge ich mit deinem Blute,", "tokens": ["Und", "Feu\u00b7er", "men\u00b7ge", "ich", "mit", "dei\u00b7nem", "Blu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df bald die Hand, die nur am Pfluge ruhte,", "tokens": ["Da\u00df", "bald", "die", "Hand", ",", "die", "nur", "am", "Pflu\u00b7ge", "ruh\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Zum Sch