{"dta.poem.1209": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich beklagte Tag und Nacht", "tokens": ["Wenn", "ich", "be\u00b7klag\u00b7te", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Menge meiner herben Schmertzen/", "tokens": ["Die", "Men\u00b7ge", "mei\u00b7ner", "her\u00b7ben", "Schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn sie mit Blutt von meinem Hertzen", "tokens": ["Wenn", "sie", "mit", "Blutt", "von", "mei\u00b7nem", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gleich w\u00fcrden zu Papir gebracht/", "tokens": ["Gleich", "w\u00fcr\u00b7den", "zu", "Pa\u00b7pir", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So wird doch mehr als Schrifft und Mund", "tokens": ["So", "wird", "doch", "mehr", "als", "Schrifft", "und", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "KOUS", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Flammen/ die mein Hertze brennen/", "tokens": ["Die", "Flam\u00b7men", "/", "die", "mein", "Hert\u00b7ze", "bren\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "PPOSAT", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dein Auge geben zu erkennen/", "tokens": ["Dein", "Au\u00b7ge", "ge\u00b7ben", "zu", "er\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das meine Seele hat verwundt.", "tokens": ["Das", "mei\u00b7ne", "See\u00b7le", "hat", "ver\u00b7wundt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Was kein Papir zu melden wei\u00df", "tokens": ["Was", "kein", "Pa\u00b7pir", "zu", "mel\u00b7den", "wei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PIAT", "NN", "PTKZU", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und meine Zunge mu\u00df verschweigen/", "tokens": ["Und", "mei\u00b7ne", "Zun\u00b7ge", "mu\u00df", "ver\u00b7schwei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Wird dir zur Gn\u00fcge k\u00f6nnen zeigen", "tokens": ["Wird", "dir", "zur", "Gn\u00fc\u00b7ge", "k\u00f6n\u00b7nen", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Dein Bildnis und des Spiegels Ei\u00df.", "tokens": ["Dein", "Bild\u00b7nis", "und", "des", "Spie\u00b7gels", "Ei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}