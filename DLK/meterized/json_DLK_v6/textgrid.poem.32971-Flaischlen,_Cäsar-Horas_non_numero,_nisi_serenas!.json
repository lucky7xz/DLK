{"textgrid.poem.32971": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "Horas non numero, nisi serenas!", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sind es nicht Toren,", "tokens": ["Sind", "es", "nicht", "To\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "die da stets zittern", "tokens": ["die", "da", "stets", "zit\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "und sich das sch\u00f6ne", "tokens": ["und", "sich", "das", "sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PRF", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Leben verbittern?", "tokens": ["Le\u00b7ben", "ver\u00b7bit\u00b7tern", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wein-, lieb- und liederfroh", "tokens": ["Wein", ",", "lie\u00b7b", "und", "lie\u00b7der\u00b7froh"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["TRUNC", "$,", "TRUNC", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was dir auch zugelost", "tokens": ["Was", "dir", "auch", "zu\u00b7ge\u00b7lost"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "an Leid und Sorgen,", "tokens": ["an", "Leid", "und", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "selbst auf die l\u00e4ngste Nacht", "tokens": ["selbst", "auf", "die", "l\u00e4ngs\u00b7te", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "folgt noch ein Morgen!", "tokens": ["folgt", "noch", "ein", "Mor\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Tag-, licht- und sonnenfroh", "tokens": ["Tag", ",", "licht", "und", "son\u00b7nen\u00b7froh"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["TRUNC", "$,", "TRUNC", "KON", "XY"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wenn der Sommer", "tokens": ["Und", "wenn", "der", "Som\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "sich neigt zur Wende ...", "tokens": ["sich", "neigt", "zur", "Wen\u00b7de", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Einmal, so sch\u00f6n es war,", "tokens": ["Ein\u00b7mal", ",", "so", "sch\u00f6n", "es", "war", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "geht's doch zu Ende ...", "tokens": ["geht's", "doch", "zu", "En\u00b7de", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Dank- und erinnerungsfroh", "tokens": ["Dank", "und", "e\u00b7rin\u00b7ne\u00b7rungs\u00b7froh"], "token_info": ["word", "word", "word"], "pos": ["TRUNC", "KON", "VVFIN"], "meter": "+-++--+", "measure": "iambic.tetra.chol"}}}}}