{"textgrid.poem.67755": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der Pythagoreer", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Frucht, so wie sie f\u00e4llt,", "tokens": ["Die", "Frucht", ",", "so", "wie", "sie", "f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "KOKOM", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So bl\u00fcht sie weiter hin bis in ein andres Feld,", "tokens": ["So", "bl\u00fcht", "sie", "wei\u00b7ter", "hin", "bis", "in", "ein", "and\u00b7res", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das sie zu ihrem Stamm aufs Neue hingesellt.", "tokens": ["Das", "sie", "zu", "ih\u00b7rem", "Stamm", "aufs", "Neu\u00b7e", "hin\u00b7ge\u00b7sellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "PPOSAT", "NN", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch Frucht und Baum mu\u00df unser Leben flie\u00dfen,", "tokens": ["Durch", "Frucht", "und", "Baum", "mu\u00df", "un\u00b7ser", "Le\u00b7ben", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Um sich in unsers Ursprungs Meer zu gie\u00dfen.", "tokens": ["Um", "sich", "in", "un\u00b7sers", "Ur\u00b7sprungs", "Meer", "zu", "gie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APPR", "PPOSAT", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der letzte Augenblick,", "tokens": ["Der", "letz\u00b7te", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So wie er Dich erwischt, so stimmt er Dein Geschick.", "tokens": ["So", "wie", "er", "Dich", "er\u00b7wischt", ",", "so", "stimmt", "er", "Dein", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "PRF", "VVPP", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So ist die neue H\u00fctte auch \u2013 Dein Schwermuth oder Gl\u00fcck.", "tokens": ["So", "ist", "die", "neu\u00b7e", "H\u00fct\u00b7te", "auch", "\u2013", "Dein", "Schwer\u00b7muth", "o\u00b7der", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ADV", "$(", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}