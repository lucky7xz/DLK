{"textgrid.poem.66780": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Deutsche Jugend, sturmesm\u00e4chtig,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deutsche Jugend, sturmesm\u00e4chtig,", "tokens": ["Deut\u00b7sche", "Ju\u00b7gend", ",", "stur\u00b7mes\u00b7m\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Glockenl\u00e4utend, fr\u00fchlingspr\u00e4chtig", "tokens": ["Glo\u00b7cken\u00b7l\u00e4u\u00b7tend", ",", "fr\u00fch\u00b7lings\u00b7pr\u00e4ch\u00b7tig"], "token_info": ["word", "punct", "word"], "pos": ["VVPP", "$,", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruft dich auf mein stolzer Sang.", "tokens": ["Ruft", "dich", "auf", "mein", "stol\u00b7zer", "Sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Einig Band h\u00e4lt uns umschlossen,", "tokens": ["Ei\u00b7nig", "Band", "h\u00e4lt", "uns", "um\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00f6rt mich, wachsende Genossen,", "tokens": ["H\u00f6rt", "mich", ",", "wach\u00b7sen\u00b7de", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bruderbund in Sturm und Drang!", "tokens": ["Bru\u00b7der\u00b7bund", "in", "Sturm", "und", "Drang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Allen Sp\u00f6ttern schenkt Verachtung,", "tokens": ["Al\u00b7len", "Sp\u00f6t\u00b7tern", "schenkt", "Ver\u00b7ach\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die in nebelnder Umnachtung", "tokens": ["Die", "in", "ne\u00b7beln\u00b7der", "Um\u00b7nach\u00b7tung"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Eure Sehnsucht nicht verstehn!", "tokens": ["Eu\u00b7re", "Sehn\u00b7sucht", "nicht", "ver\u00b7stehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die da lachen und euch h\u00f6hnen,", "tokens": ["Die", "da", "la\u00b7chen", "und", "euch", "h\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "KON", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sollt sie nie mit euch vers\u00f6hnen,", "tokens": ["Sollt", "sie", "nie", "mit", "euch", "ver\u00b7s\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Blindlings la\u00dft sie untergehn!", "tokens": ["Blind\u00b7lings", "la\u00dft", "sie", "un\u00b7ter\u00b7gehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Die am Golde sich gen\u00fcgen,", "tokens": ["Die", "am", "Gol\u00b7de", "sich", "ge\u00b7n\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schm\u00e4hend euch mit feilen L\u00fcgen,", "tokens": ["Schm\u00e4\u00b7hend", "euch", "mit", "fei\u00b7len", "L\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00e4end Arglist und Verrat:", "tokens": ["S\u00e4\u00b7end", "Arg\u00b7list", "und", "Ver\u00b7rat", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "In den Kot die Sinnesdirnen!", "tokens": ["In", "den", "Kot", "die", "Sin\u00b7nes\u00b7dir\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hebt zur Sonne eure Stirnen,", "tokens": ["Hebt", "zur", "Son\u00b7ne", "eu\u00b7re", "Stir\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ratet und vollf\u00fchrt die Tat!", "tokens": ["Ra\u00b7tet", "und", "voll\u00b7f\u00fchrt", "die", "Tat", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Eins ist not, ach nur dies eine:", "tokens": ["Eins", "ist", "not", ",", "ach", "nur", "dies", "ei\u00b7ne", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$,", "ITJ", "ADV", "PDS", "ART", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df des Menschen Bild erscheine,", "tokens": ["Da\u00df", "des", "Men\u00b7schen", "Bild", "er\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rein von Schminke die Natur.", "tokens": ["Rein", "von", "Schmin\u00b7ke", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df mit innerstem Verstehen", "tokens": ["Da\u00df", "mit", "in\u00b7ners\u00b7tem", "Ver\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Augen sich in Augen sehen,", "tokens": ["Au\u00b7gen", "sich", "in", "Au\u00b7gen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eins ist not, dies eine nur!", "tokens": ["Eins", "ist", "not", ",", "dies", "ei\u00b7ne", "nur", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$,", "PDS", "ART", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Alle Ketten, die da lauern,", "tokens": ["Al\u00b7le", "Ket\u00b7ten", ",", "die", "da", "lau\u00b7ern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Alle morschen Scheidemauern", "tokens": ["Al\u00b7le", "mor\u00b7schen", "Schei\u00b7de\u00b7mau\u00b7ern"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schmettert in den tiefsten Grund!", "tokens": ["Schmet\u00b7tert", "in", "den", "tiefs\u00b7ten", "Grund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Arbeit soll die Arbeit achten,", "tokens": ["Ar\u00b7beit", "soll", "die", "Ar\u00b7beit", "ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Faule Schlemmer sollen schmachten", "tokens": ["Fau\u00b7le", "Schlem\u00b7mer", "sol\u00b7len", "schmach\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Einsam mit verdorrtem Mund!", "tokens": ["Ein\u00b7sam", "mit", "ver\u00b7dorr\u00b7tem", "Mund", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Freie Arbeit \u2013 sch\u00f6ne Tugend,", "tokens": ["Frei\u00b7e", "Ar\u00b7beit", "\u2013", "sch\u00f6\u00b7ne", "Tu\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6re mich, du deutsche Jugend,", "tokens": ["H\u00f6\u00b7re", "mich", ",", "du", "deut\u00b7sche", "Ju\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zukunftschaffendes Geschlecht!", "tokens": ["Zu\u00b7kunft\u00b7schaf\u00b7fen\u00b7des", "Ge\u00b7schlecht", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6ne Tugend, edles Wagen \u2013", "tokens": ["Sch\u00f6\u00b7ne", "Tu\u00b7gend", ",", "ed\u00b7les", "Wa\u00b7gen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Selbstverk\u00fcmmernd Jochertragen", "tokens": ["Selbst\u00b7ver\u00b7k\u00fcm\u00b7mernd", "Jo\u00b7cher\u00b7tra\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Preist voll Demut nur der Knecht.", "tokens": ["Preist", "voll", "De\u00b7mut", "nur", "der", "Knecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Lauscht dem Worte hoher Meister,", "tokens": ["Lauscht", "dem", "Wor\u00b7te", "ho\u00b7her", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tiefer Seelen, klarer Geister,", "tokens": ["Tie\u00b7fer", "See\u00b7len", ",", "kla\u00b7rer", "Geis\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ehrt des Genius' goldne Saat!", "tokens": ["Ehrt", "des", "Ge\u00b7ni\u00b7us'", "gold\u00b7ne", "Saat", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Vor dem Rohen scheu und scheuer,", "tokens": ["Vor", "dem", "Ro\u00b7hen", "scheu", "und", "scheu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "N\u00e4hrt der Sitte reines Feuer,", "tokens": ["N\u00e4hrt", "der", "Sit\u00b7te", "rei\u00b7nes", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das euch st\u00e4hlt zur h\u00f6chsten Tat:", "tokens": ["Das", "euch", "st\u00e4hlt", "zur", "h\u00f6chs\u00b7ten", "Tat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Tat der Wahrheit, Tat der Freiheit,", "tokens": ["Tat", "der", "Wahr\u00b7heit", ",", "Tat", "der", "Frei\u00b7heit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tat der Sch\u00f6nheit, heilige Dreiheit,", "tokens": ["Tat", "der", "Sch\u00f6n\u00b7heit", ",", "hei\u00b7li\u00b7ge", "Drei\u00b7heit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Komm, wie dich der Seher sieht!", "tokens": ["Komm", ",", "wie", "dich", "der", "Se\u00b7her", "sieht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deutsche Jugend, sturmesm\u00e4chtig,", "tokens": ["Deut\u00b7sche", "Ju\u00b7gend", ",", "stur\u00b7mes\u00b7m\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Glockenl\u00e4utend, fr\u00fchlingspr\u00e4chtig", "tokens": ["Glo\u00b7cken\u00b7l\u00e4u\u00b7tend", ",", "fr\u00fch\u00b7lings\u00b7pr\u00e4ch\u00b7tig"], "token_info": ["word", "punct", "word"], "pos": ["VVPP", "$,", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ruft dich auf mein stolzes Lied.", "tokens": ["Ruft", "dich", "auf", "mein", "stol\u00b7zes", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}