{"dta.poem.20474": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Verliebte Arien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du kennst mein treues hertze/", "tokens": ["Du", "kennst", "mein", "treu\u00b7es", "hert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es lieget ja in deiner hand/", "tokens": ["Es", "lie\u00b7get", "ja", "in", "dei\u00b7ner", "hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als meiner liebe treues pfand/", "tokens": ["Als", "mei\u00b7ner", "lie\u00b7be", "treu\u00b7es", "pfand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So dich bedient im ernst und schertze.", "tokens": ["So", "dich", "be\u00b7dient", "im", "ernst", "und", "schert\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "APPRART", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kein garten bl\u00fcht mir ohne dich/", "tokens": ["Kein", "gar\u00b7ten", "bl\u00fcht", "mir", "oh\u00b7ne", "dich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPR", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du sch\u00f6ne blume meiner sinnen/", "tokens": ["Du", "sch\u00f6\u00b7ne", "blu\u00b7me", "mei\u00b7ner", "sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie solte doch mein auge sich", "tokens": ["Wie", "sol\u00b7te", "doch", "mein", "au\u00b7ge", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ADV", "PPOSAT", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von dir entfernt ergetzen k\u00f6nnen?", "tokens": ["Von", "dir", "ent\u00b7fernt", "er\u00b7get\u00b7zen", "k\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kein amber will mir schmecken/", "tokens": ["Kein", "am\u00b7ber", "will", "mir", "schme\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wann du nicht kanst gef\u00e4hrtin seyn.", "tokens": ["Wann", "du", "nicht", "kanst", "ge\u00b7f\u00e4hr\u00b7tin", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der morgenr\u00f6the purpur-schein", "tokens": ["Der", "mor\u00b7gen\u00b7r\u00f6\u00b7the", "pur\u00b7pur\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verkehrt sich mir in tr\u00fcbe decken/", "tokens": ["Ver\u00b7kehrt", "sich", "mir", "in", "tr\u00fc\u00b7be", "de\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPER", "APPR", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn deiner augen sonnen-pracht", "tokens": ["Wenn", "dei\u00b7ner", "au\u00b7gen", "son\u00b7nen\u00b7pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die g\u00fcldnen strahlen mir entziehen/", "tokens": ["Die", "g\u00fcld\u00b7nen", "strah\u00b7len", "mir", "ent\u00b7zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und dieses/ was dich englisch macht/", "tokens": ["Und", "die\u00b7ses", "/", "was", "dich", "eng\u00b7lisch", "macht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "$(", "PWS", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von meiner seiten denckt zu fliehen.", "tokens": ["Von", "mei\u00b7ner", "sei\u00b7ten", "denckt", "zu", "flie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich k\u00fcsse noch die stunde/", "tokens": ["Ich", "k\u00fcs\u00b7se", "noch", "die", "stun\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da ich den ersten liebes-ku\u00df/", "tokens": ["Da", "ich", "den", "ers\u00b7ten", "lie\u00b7bes\u00b7ku\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus keuscher freundschafft \u00fcberflu\u00df/", "tokens": ["Aus", "keu\u00b7scher", "freund\u00b7schafft", "\u00fc\u00b7berf\u00b7lu\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geno\u00df aus deinem zucker-munde/", "tokens": ["Ge\u00b7no\u00df", "aus", "dei\u00b7nem", "zu\u00b7cke\u00b7rmun\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das reine siegel/ so von dir", "tokens": ["Das", "rei\u00b7ne", "sie\u00b7gel", "/", "so", "von", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auff meine lippen ward gedr\u00fccket/", "tokens": ["Auff", "mei\u00b7ne", "lip\u00b7pen", "ward", "ge\u00b7dr\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Hat auch die seele selbst aus mir", "tokens": ["Hat", "auch", "die", "see\u00b7le", "selbst", "aus", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In s\u00fcsse bande hinger\u00fccket.", "tokens": ["In", "s\u00fcs\u00b7se", "ban\u00b7de", "hin\u00b7ge\u00b7r\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch f\u00fcrcht ich das gel\u00fccke/", "tokens": ["Doch", "f\u00fcrcht", "ich", "das", "ge\u00b7l\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So nicht best\u00e4ndig farbe h\u00e4lt/", "tokens": ["So", "nicht", "be\u00b7st\u00e4n\u00b7dig", "far\u00b7be", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADJD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mir auff tausend wege stellt/", "tokens": ["Und", "mir", "auff", "tau\u00b7send", "we\u00b7ge", "stellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "CARD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Braucht gegen mich auch seine t\u00fccke/", "tokens": ["Braucht", "ge\u00b7gen", "mich", "auch", "sei\u00b7ne", "t\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mich daucht/ da\u00df eine fremde hand", "tokens": ["Mich", "daucht", "/", "da\u00df", "ei\u00b7ne", "frem\u00b7de", "hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Um deine rosen sich l\u00e4\u00dft sp\u00fcren/", "tokens": ["Um", "dei\u00b7ne", "ro\u00b7sen", "sich", "l\u00e4\u00dft", "sp\u00fc\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "VVFIN", "PRF", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und dich/ in einen andern stand", "tokens": ["Und", "dich", "/", "in", "ei\u00b7nen", "an\u00b7dern", "stand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$(", "APPR", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Aus meinen augen will entf\u00fchren.", "tokens": ["Aus", "mei\u00b7nen", "au\u00b7gen", "will", "ent\u00b7f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Mir aber mu\u00df belieben/", "tokens": ["Mir", "a\u00b7ber", "mu\u00df", "be\u00b7lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was endlich dein belieben heist;", "tokens": ["Was", "end\u00b7lich", "dein", "be\u00b7lie\u00b7ben", "heist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es scheint/ da\u00df mein verstrickter geist", "tokens": ["Es", "scheint", "/", "da\u00df", "mein", "ver\u00b7strick\u00b7ter", "geist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach deinem willen wird getrieben.", "tokens": ["Nach", "dei\u00b7nem", "wil\u00b7len", "wird", "ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dein wohlseyn pflantzt auch meine lust/", "tokens": ["Dein", "wohl\u00b7seyn", "pflantzt", "auch", "mei\u00b7ne", "lust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich acht es \u00fcber alle sch\u00e4tze", "tokens": ["Ich", "acht", "es", "\u00fc\u00b7ber", "al\u00b7le", "sch\u00e4t\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PIS", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich tadle niemahls was du thust/", "tokens": ["Ich", "tad\u00b7le", "nie\u00b7mahls", "was", "du", "thust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dein willen bleibet mein gesetze.", "tokens": ["Dein", "wil\u00b7len", "blei\u00b7bet", "mein", "ge\u00b7set\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Denn dir zu widerstreben/", "tokens": ["Denn", "dir", "zu", "wi\u00b7der\u00b7stre\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "W\u00e4r eine h\u00f6llen-harte schuld/", "tokens": ["W\u00e4r", "ei\u00b7ne", "h\u00f6l\u00b7len\u00b7har\u00b7te", "schuld", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich dencke nur/ da\u00df ungedult", "tokens": ["Ich", "den\u00b7cke", "nur", "/", "da\u00df", "un\u00b7ge\u00b7dult"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$(", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns wenig wieder wei\u00df zu geben;", "tokens": ["Uns", "we\u00b7nig", "wie\u00b7der", "wei\u00df", "zu", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mich stell ich dir gehorsam ein/", "tokens": ["Mich", "stell", "ich", "dir", "ge\u00b7hor\u00b7sam", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "ADJD", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was du begehrst aus mir zu machen/", "tokens": ["Was", "du", "be\u00b7gehrst", "aus", "mir", "zu", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Doch kanstu auff den rosen seyn/", "tokens": ["Doch", "kans\u00b7tu", "auff", "den", "ro\u00b7sen", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "ART", "ADJA", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So mu\u00df ich auff den dornen lachen.", "tokens": ["So", "mu\u00df", "ich", "auff", "den", "dor\u00b7nen", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}