{"dta.poem.2489": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Vir fortis \u00f4mnem patitur In j\u00fbri-  \n am f\u00f4rtun\u00e6.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Die Zytisus bleibt gr\u00fcn ob gleich in' Sommertagen/", "tokens": ["Die", "Zy\u00b7ti\u00b7sus", "bleibt", "gr\u00fcn", "ob", "gleich", "in'", "Som\u00b7mer\u00b7ta\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADJD", "KOUS", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Sonn als Fewer brennt ob gleich bey Win-", "tokens": ["Die", "Sonn", "als", "Fe\u00b7wer", "brennt", "ob", "gleich", "bey", "Win"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOUS", "NN", "VVFIN", "KOUS", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "terszeit", "tokens": ["ter\u00b7szeit"], "token_info": ["word"], "pos": ["NN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Dieselbe hart befriert/ beregnet und beschneyt;", "tokens": ["Die\u00b7sel\u00b7be", "hart", "be\u00b7friert", "/", "be\u00b7reg\u00b7net", "und", "be\u00b7schneyt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJD", "VVPP", "$(", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So sol ein \u00e4dler Sinn auch iedes Ungl\u00fckk tragen.", "tokens": ["So", "sol", "ein", "\u00e4d\u00b7ler", "Sinn", "auch", "ie\u00b7des", "Un\u00b7gl\u00fckk", "tra\u00b7gen", "."], "token_info":