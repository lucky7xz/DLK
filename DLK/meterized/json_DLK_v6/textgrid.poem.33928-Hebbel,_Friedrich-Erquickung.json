{"textgrid.poem.33928": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Erquickung", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Vater geht hinaus auf's Land,", "tokens": ["Der", "Va\u00b7ter", "geht", "hin\u00b7aus", "auf's", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein munt'res Kn\u00e4blein an der Hand;", "tokens": ["Sein", "munt'\u00b7res", "Kn\u00e4\u00b7blein", "an", "der", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Getragen ist des Tages Last,", "tokens": ["Ge\u00b7tra\u00b7gen", "ist", "des", "Ta\u00b7ges", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun geht er bei der Nacht zu Gast.", "tokens": ["Nun", "geht", "er", "bei", "der", "Nacht", "zu", "Gast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Solch frisches Menschenangesicht,", "tokens": ["Solch", "fri\u00b7sches", "Men\u00b7schen\u00b7an\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "D'raus Heiterkeit und Friede spricht,", "tokens": ["D'raus", "Hei\u00b7ter\u00b7keit", "und", "Frie\u00b7de", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das ist mir, wie ein Bibelbuch,", "tokens": ["Das", "ist", "mir", ",", "wie", "ein", "Bi\u00b7bel\u00b7buch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich schau' hinein, und hab' genug.", "tokens": ["Ich", "schau'", "hin\u00b7ein", ",", "und", "hab'", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Bin l\u00e4ngst nicht mehr der Thor, der fragt:", "tokens": ["Bin", "l\u00e4ngst", "nicht", "mehr", "der", "Thor", ",", "der", "fragt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADV", "ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was hast du selber dir erjagt?", "tokens": ["Was", "hast", "du", "sel\u00b7ber", "dir", "er\u00b7jagt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das aber giebt mir ein Gef\u00fchl,", "tokens": ["Das", "a\u00b7ber", "giebt", "mir", "ein", "Ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als g\u00e4b's f\u00fcr And're doch ein Ziel.", "tokens": ["Als", "g\u00e4b's", "f\u00fcr", "An\u00b7d'\u00b7re", "doch", "ein", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "NE", "ADV", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}