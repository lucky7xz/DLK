{"textgrid.poem.61936": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was Empedokles einst mich gelehrt, hier leg' ich es nieder,", "tokens": ["Was", "Em\u00b7pe\u00b7do\u00b7kles", "einst", "mich", "ge\u00b7lehrt", ",", "hier", "leg'", "ich", "es", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "PPER", "VVPP", "$,", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wie ich's im eignen Gem\u00fct h\u00e4ufig erwogen behielt:", "tokens": ["Wie", "ich's", "im", "eig\u00b7nen", "Ge\u00b7m\u00fct", "h\u00e4u\u00b7fig", "er\u00b7wo\u00b7gen", "be\u00b7hielt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPRART", "ADJA", "NN", "ADJD", "VVPP", "VVFIN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wandlung ist das Geheimnis der Welt. In steter Entfaltung", "tokens": ["Wand\u00b7lung", "ist", "das", "Ge\u00b7heim\u00b7nis", "der", "Welt", ".", "In", "ste\u00b7ter", "Ent\u00b7fal\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Unabsehlich gestuft bildet das Leben sich aus.", "tokens": ["Un\u00b7ab\u00b7seh\u00b7lich", "ge\u00b7stuft", "bil\u00b7det", "das", "Le\u00b7ben", "sich", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "-+---++--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Unter den gr\u00f6beren Stoffen gebunden zugleich und beh\u00fctet", "tokens": ["Un\u00b7ter", "den", "gr\u00f6\u00b7be\u00b7ren", "Stof\u00b7fen", "ge\u00b7bun\u00b7den", "zu\u00b7gleich", "und", "be\u00b7h\u00fc\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "ADV", "KON", "VVFIN"], "meter": "+--+--+--+-+-+-+-", "measure": "dactylic.tri.plus"}, "line.6": {"text": "Dehnt sich der edlere Keim still zur Befreiung empor.", "tokens": ["Dehnt", "sich", "der", "ed\u00b7le\u00b7re", "Keim", "still", "zur", "Be\u00b7frei\u00b7ung", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Also schl\u00e4ft in der Schale des Ei's das gefl\u00fcgelte V\u00f6glein,", "tokens": ["Al\u00b7so", "schl\u00e4ft", "in", "der", "Scha\u00b7le", "des", "Ei's", "das", "ge\u00b7fl\u00fc\u00b7gel\u00b7te", "V\u00f6\u00b7glein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.8": {"text": "So in der Puppe Geh\u00e4us reift sich der Schmetterling aus.", "tokens": ["So", "in", "der", "Pup\u00b7pe", "Ge\u00b7h\u00e4us", "reift", "sich", "der", "Schmet\u00b7ter\u00b7ling", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NE", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Und so tragen auch wir umh\u00fcllt vom irdischen K\u00f6rper", "tokens": ["Und", "so", "tra\u00b7gen", "auch", "wir", "um\u00b7h\u00fcllt", "vom", "ir\u00b7di\u00b7schen", "K\u00f6r\u00b7per"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Schon im Innern den Keim eines veredelten Leibs,", "tokens": ["Schon", "im", "In\u00b7nern", "den", "Keim", "ei\u00b7nes", "ver\u00b7e\u00b7del\u00b7ten", "Leibs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Jenen \u00e4therischen Strom, der, \u00fcber die Nerven ergossen,", "tokens": ["Je\u00b7nen", "\u00e4t\u00b7he\u00b7ri\u00b7schen", "Strom", ",", "der", ",", "\u00fc\u00b7ber", "die", "Ner\u00b7ven", "er\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,", "PRELS", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Fl\u00fcssig, empfindlich und zart jegliches Glied uns durchdringt.", "tokens": ["Fl\u00fcs\u00b7sig", ",", "emp\u00b7find\u00b7lich", "und", "zart", "jeg\u00b7li\u00b7ches", "Glied", "uns", "durch\u00b7dringt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "KON", "ADJD", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.13": {"text": "Dieser, sobald in den Staub die verwesliche H\u00fclle zur\u00fccksinkt,", "tokens": ["Die\u00b7ser", ",", "so\u00b7bald", "in", "den", "Staub", "die", "ver\u00b7wes\u00b7li\u00b7che", "H\u00fcl\u00b7le", "zu\u00b7r\u00fcc\u00b7ksinkt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+--+--+--+-+-+", "measure": "dactylic.tetra.plus"}, "line.14": {"text": "Str\u00f6mt mit dem ewigen Teil von der erkaltenden aus,", "tokens": ["Str\u00f6mt", "mit", "dem", "e\u00b7wi\u00b7gen", "Teil", "von", "der", "er\u00b7kal\u00b7ten\u00b7den", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "PTKVZ", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Und nach au\u00dfen gekehrt, zur Gestalt sich formend, umschlie\u00dft er", "tokens": ["Und", "nach", "au\u00b7\u00dfen", "ge\u00b7kehrt", ",", "zur", "Ge\u00b7stalt", "sich", "for\u00b7mend", ",", "um\u00b7schlie\u00dft", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ADV", "VVPP", "$,", "APPRART", "NN", "PRF", "VVPP", "$,", "VVFIN", "PPER"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.16": {"text": "Mit durchsichtigem Kleid leicht den unsterblichen Geist,", "tokens": ["Mit", "durch\u00b7sich\u00b7ti\u00b7gem", "Kleid", "leicht", "den", "uns\u00b7terb\u00b7li\u00b7chen", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "K\u00f6rperlich zwar, doch zartesten Stoffs, unfa\u00dflich dem Auge,", "tokens": ["K\u00f6r\u00b7per\u00b7lich", "zwar", ",", "doch", "zar\u00b7tes\u00b7ten", "Stoffs", ",", "un\u00b7fa\u00df\u00b7lich", "dem", "Au\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "ADV", "ADJA", "NN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "+-++-+--+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Nur im Schauder vielleicht noch von den Sinnen erkannt.", "tokens": ["Nur", "im", "Schau\u00b7der", "viel\u00b7leicht", "noch", "von", "den", "Sin\u00b7nen", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADV", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.19": {"text": "Aber das Neue geleitet alsdann ein verborgener Ratschlu\u00df", "tokens": ["A\u00b7ber", "das", "Neu\u00b7e", "ge\u00b7lei\u00b7tet", "als\u00b7dann", "ein", "ver\u00b7bor\u00b7ge\u00b7ner", "Rat\u00b7schlu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "VVPP", "ADV", "ART", "ADJA", "NN"], "meter": "+--+--+-+-+-+--+-", "measure": "dactylic.di.plus"}, "line.20": {"text": "Auf vielstufigem Pfad neuen Entfaltungen zu.", "tokens": ["Auf", "viel\u00b7stu\u00b7fi\u00b7gem", "Pfad", "neu\u00b7en", "Ent\u00b7fal\u00b7tun\u00b7gen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--++-+-+-+", "measure": "trochaic.septa.relaxed"}}}}}