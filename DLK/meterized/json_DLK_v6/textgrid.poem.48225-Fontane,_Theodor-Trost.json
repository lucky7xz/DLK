{"textgrid.poem.48225": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Trost", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00f6ste dich, die Stunden eilen,", "tokens": ["Tr\u00f6s\u00b7te", "dich", ",", "die", "Stun\u00b7den", "ei\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und was all dich dr\u00fccken mag,", "tokens": ["Und", "was", "all", "dich", "dr\u00fc\u00b7cken", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIAT", "PRF", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch das Schlimmste kann nicht weilen,", "tokens": ["Auch", "das", "Schlimms\u00b7te", "kann", "nicht", "wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es kommt ein andrer Tag.", "tokens": ["Und", "es", "kommt", "ein", "an\u00b7drer", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In dem ew'gen Kommen, Schwinden,", "tokens": ["In", "dem", "ew'\u00b7gen", "Kom\u00b7men", ",", "Schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie der Schmerz liegt auch das Gl\u00fcck,", "tokens": ["Wie", "der", "Schmerz", "liegt", "auch", "das", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und auch heitre Bilder finden", "tokens": ["Und", "auch", "heit\u00b7re", "Bil\u00b7der", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ihren Weg zu dir zur\u00fcck.", "tokens": ["Ih\u00b7ren", "Weg", "zu", "dir", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Harre, hoffe. Nicht vergebens", "tokens": ["Har\u00b7re", ",", "hof\u00b7fe", ".", "Nicht", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "VVFIN", "$.", "PTKNEG", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Z\u00e4hlest du der Stunden Schlag,", "tokens": ["Z\u00e4h\u00b7lest", "du", "der", "Stun\u00b7den", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wechsel ist das Los des Lebens,", "tokens": ["Wech\u00b7sel", "ist", "das", "Los", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und \u2013 es kommt ein andrer Tag.", "tokens": ["Und", "\u2013", "es", "kommt", "ein", "an\u00b7drer", "Tag", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}