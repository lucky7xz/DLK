{"textgrid.poem.33362": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Joseph der Zweite", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Joseph baut, und seinen Tempel gr\u00fcndet", "tokens": ["Jo\u00b7se\u00b7ph", "baut", ",", "und", "sei\u00b7nen", "Tem\u00b7pel", "gr\u00fcn\u00b7det"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Hohe Weisheit; Sch\u00f6nheit schm\u00fccket ihn;", "tokens": ["Ho\u00b7he", "Weis\u00b7heit", ";", "Sch\u00f6n\u00b7heit", "schm\u00fc\u00b7cket", "ihn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "St\u00e4rke, die mit beiden sich verbindet,", "tokens": ["St\u00e4r\u00b7ke", ",", "die", "mit", "bei\u00b7den", "sich", "ver\u00b7bin\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "PIAT", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Gibt ihm Dauer, und vollendet ihn.", "tokens": ["Gibt", "ihm", "Dau\u00b7er", ",", "und", "voll\u00b7en\u00b7det", "ihn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "KON", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Weisheit war es, die einst wie der helle", "tokens": ["Weis\u00b7heit", "war", "es", ",", "die", "einst", "wie", "der", "hel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$,", "PRELS", "ADV", "KOKOM", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Thau des Himmels auf Ihn niedersank,", "tokens": ["Thau", "des", "Him\u00b7mels", "auf", "Ihn", "nie\u00b7der\u00b7sank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Weisheit war's, aus deren reiner Quelle", "tokens": ["Weis\u00b7heit", "wa\u00b7r's", ",", "aus", "de\u00b7ren", "rei\u00b7ner", "Quel\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "$,", "APPR", "PRELAT", "ADJA", "NN"], "meter": "+--+---+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Er mit nie gestilltem Durste trank.", "tokens": ["Er", "mit", "nie", "ge\u00b7still\u00b7tem", "Durs\u00b7te", "trank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Sie nur war es, die nach fernen Zonen", "tokens": ["Sie", "nur", "war", "es", ",", "die", "nach", "fer\u00b7nen", "Zo\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Seines Geistes Forscherblicke zog,", "tokens": ["Sei\u00b7nes", "Geis\u00b7tes", "For\u00b7scher\u00b7bli\u00b7cke", "zog", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sie, auf deren Schal' Er Nationen", "tokens": ["Sie", ",", "auf", "de\u00b7ren", "Schal'", "Er", "Na\u00b7ti\u00b7o\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "APPR", "PRELAT", "NN", "PPER", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gegen Nationen ma\u00df und wog.", "tokens": ["Ge\u00b7gen", "Na\u00b7ti\u00b7o\u00b7nen", "ma\u00df", "und", "wog", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Dort erweiterten die scharfen Blicke", "tokens": ["Dort", "er\u00b7wei\u00b7ter\u00b7ten", "die", "schar\u00b7fen", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Seines Geistes sich an ihrer Hand,", "tokens": ["Sei\u00b7nes", "Geis\u00b7tes", "sich", "an", "ih\u00b7rer", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Eines Geist's, der nun mit Einem Blicke", "tokens": ["Ei\u00b7nes", "Geist's", ",", "der", "nun", "mit", "Ei\u00b7nem", "Bli\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Eine ganze V\u00f6lkerwelt umspannt.", "tokens": ["Ei\u00b7ne", "gan\u00b7ze", "V\u00f6l\u00b7ker\u00b7welt", "um\u00b7spannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Dort arbeitete sein Blick zur Klarheit", "tokens": ["Dort", "ar\u00b7bei\u00b7te\u00b7te", "sein", "Blick", "zur", "Klar\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Eigener Beschauung sich hinan,", "tokens": ["Ei\u00b7ge\u00b7ner", "Be\u00b7schau\u00b7ung", "sich", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "PTKVZ", "$,"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Sicher, da\u00df Ihn nun nicht mehr der Wahrheit", "tokens": ["Si\u00b7cher", ",", "da\u00df", "Ihn", "nun", "nicht", "mehr", "der", "Wahr\u00b7heit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Angeborgter Schimmer t\u00e4uschen kann.", "tokens": ["An\u00b7ge\u00b7borg\u00b7ter", "Schim\u00b7mer", "t\u00e4u\u00b7schen", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Weisheit und mit ihr Erfahrung liehen", "tokens": ["Weis\u00b7heit", "und", "mit", "ihr", "Er\u00b7fah\u00b7rung", "lie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ihm die Sch\u00e4tze zu dem gro\u00dfen Plan,", "tokens": ["Ihm", "die", "Sch\u00e4t\u00b7ze", "zu", "dem", "gro\u00b7\u00dfen", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Gr\u00f6sser als der Herrschertheorieen", "tokens": ["Gr\u00f6s\u00b7ser", "als", "der", "Herr\u00b7scher\u00b7the\u00b7o\u00b7ri\u00b7een"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Sch\u00f6nste jemals einen geben kann.", "tokens": ["Sch\u00f6ns\u00b7te", "je\u00b7mals", "ei\u00b7nen", "ge\u00b7ben", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Weisheit war's, die Ihn zum Menschensch\u00e4tzer,", "tokens": ["Weis\u00b7heit", "wa\u00b7r's", ",", "die", "Ihn", "zum", "Men\u00b7schen\u00b7sch\u00e4t\u00b7zer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PRELS", "PPER", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Nicht zum Seelensch\u00e4tzer werden lie\u00df,", "tokens": ["Nicht", "zum", "See\u00b7len\u00b7sch\u00e4t\u00b7zer", "wer\u00b7den", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "VAINF", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und den guten Menschen auch im Ketzer", "tokens": ["Und", "den", "gu\u00b7ten", "Men\u00b7schen", "auch", "im", "Ket\u00b7zer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Oder Juden gleich willkommen hie\u00df.", "tokens": ["O\u00b7der", "Ju\u00b7den", "gleich", "will\u00b7kom\u00b7men", "hie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Sie zerbrach die schwere Sklavenkette,", "tokens": ["Sie", "zer\u00b7brach", "die", "schwe\u00b7re", "Skla\u00b7ven\u00b7ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die an's Joch den armen Landmann schlo\u00df,", "tokens": ["Die", "an's", "Joch", "den", "ar\u00b7men", "Land\u00b7mann", "schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dessen Schwei\u00df einst auf die Pfl\u00fcgerst\u00e4tte", "tokens": ["Des\u00b7sen", "Schwei\u00df", "einst", "auf", "die", "Pfl\u00fc\u00b7ger\u00b7st\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nicht f\u00fcr ihn und seine Kinder flo\u00df.", "tokens": ["Nicht", "f\u00fcr", "ihn", "und", "sei\u00b7ne", "Kin\u00b7der", "flo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPER", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Sie nur r\u00e4umte willig dem Verstande", "tokens": ["Sie", "nur", "r\u00e4um\u00b7te", "wil\u00b7lig", "dem", "Ver\u00b7stan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Seine Freiheitsrechte wieder ein,", "tokens": ["Sei\u00b7ne", "Frei\u00b7heits\u00b7rech\u00b7te", "wie\u00b7der", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und gebot ihm, ferner ohne Bande,", "tokens": ["Und", "ge\u00b7bot", "ihm", ",", "fer\u00b7ner", "oh\u00b7ne", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nur der Wahrheit unterthan zu sein.", "tokens": ["Nur", "der", "Wahr\u00b7heit", "un\u00b7ter\u00b7than", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Sie nur war's, die Kirch' und Staat geschieden,", "tokens": ["Sie", "nur", "wa\u00b7r's", ",", "die", "Kirch'", "und", "Staat", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "$,", "ART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie man Geist und K\u00f6rper scheiden kann,", "tokens": ["Wie", "man", "Geist", "und", "K\u00f6r\u00b7per", "schei\u00b7den", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "NN", "KON", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sie wies F\u00fcrsten ihren Platz hienieden,", "tokens": ["Sie", "wies", "F\u00fcrs\u00b7ten", "ih\u00b7ren", "Platz", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-++-+-+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Und den Papst in h\u00f6hern Sph\u00e4ren an.", "tokens": ["Und", "den", "Papst", "in", "h\u00f6\u00b7hern", "Sph\u00e4\u00b7ren", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Sie verbannte Kl\u00f6ster aus den Staaten,", "tokens": ["Sie", "ver\u00b7bann\u00b7te", "Kl\u00f6s\u00b7ter", "aus", "den", "Staa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo man nur mit Beten Brod erwarb,", "tokens": ["Wo", "man", "nur", "mit", "Be\u00b7ten", "Brod", "er\u00b7warb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo mit jedem neuen Kandidaten,", "tokens": ["Wo", "mit", "je\u00b7dem", "neu\u00b7en", "Kan\u00b7di\u00b7da\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "F\u00fcr den Staat ein junger B\u00fcrger starb.", "tokens": ["F\u00fcr", "den", "Staat", "ein", "jun\u00b7ger", "B\u00fcr\u00b7ger", "starb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Sie verbot dem blinden Aberglauben", "tokens": ["Sie", "ver\u00b7bot", "dem", "blin\u00b7den", "A\u00b7berg\u00b7lau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "M\u00e4dchen mit Gel\u00fcbden einzuweih'n,", "tokens": ["M\u00e4d\u00b7chen", "mit", "Ge\u00b7l\u00fcb\u00b7den", "ein\u00b7zu\u00b7weih'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Die den B\u00fcrgern ihre Br\u00e4ute rauben,", "tokens": ["Die", "den", "B\u00fcr\u00b7gern", "ih\u00b7re", "Br\u00e4u\u00b7te", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und die Menschheit mit sich selbst entzwei'n.", "tokens": ["Und", "die", "Menschheit", "mit", "sich", "selbst", "ent\u00b7zwei'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "ART", "NN", "APPR", "PRF", "ADV", "VVFIN", "NE"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.13": {"line.1": {"text": "Doch der Weisheit folgte Sch\u00f6nheit: beide", "tokens": ["Doch", "der", "Weis\u00b7heit", "folg\u00b7te", "Sch\u00f6n\u00b7heit", ":", "bei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "NN", "$.", "PIS"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hatten sich in Josephs Geist verm\u00e4hlt,", "tokens": ["Hat\u00b7ten", "sich", "in", "Jo\u00b7se\u00b7phs", "Geist", "ver\u00b7m\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "APPR", "NE", "NN", "VVPP", "$,"], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und gebaren laute V\u00f6lkerfreude,", "tokens": ["Und", "ge\u00b7ba\u00b7ren", "lau\u00b7te", "V\u00f6l\u00b7ker\u00b7freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VVFIN", "NN", "$,"], "meter": "+-