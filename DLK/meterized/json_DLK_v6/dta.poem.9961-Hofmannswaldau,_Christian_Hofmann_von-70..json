{"dta.poem.9961": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "70.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man henckt uns in einen thurn auff mit stricken und mit eise\u0303/", "tokens": ["Man", "henckt", "uns", "in", "ei\u00b7nen", "thurn", "auff", "mit", "stri\u00b7cken", "und", "mit", "eis\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "APPR", "VVINF", "KON", "APPR", "NE", "$("], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "L\u00e4\u00dft uns hencken tag und nacht/ will uns keine gnad erweisen/", "tokens": ["L\u00e4\u00dft", "uns", "hen\u00b7cken", "tag", "und", "nacht", "/", "will", "uns", "kei\u00b7ne", "gnad", "er\u00b7wei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "NN", "KON", "NN", "$(", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$("], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ob wir auch schon werde\u0303 laut/ wen\u0303 man hin und her uns ziehet/", "tokens": ["Ob", "wir", "auch", "schon", "werd\u1ebd", "laut", "/", "we\u00f1", "man", "hin", "und", "her", "uns", "zie\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VAFIN", "ADJD", "$(", "KOUS", "PIS", "PTKVZ", "KON", "ADV", "PPER", "VVFIN", "$("], "meter": "----+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "L\u00e4sst mans schallen in der lufft: keine h\u00fclffe man ersiehe