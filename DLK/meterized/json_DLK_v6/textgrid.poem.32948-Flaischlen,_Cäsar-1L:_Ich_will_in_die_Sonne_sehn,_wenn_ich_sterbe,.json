{"textgrid.poem.32948": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich will in die Sonne sehn, wenn ich sterbe,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich will in die Sonne sehn, wenn ich sterbe,", "tokens": ["Ich", "will", "in", "die", "Son\u00b7ne", "sehn", ",", "wenn", "ich", "ster\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "wie sie in brennenden Wolken verloht ...", "tokens": ["wie", "sie", "in", "bren\u00b7nen\u00b7den", "Wol\u00b7ken", "ver\u00b7loht", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "ich will mit der Sonne gehn, wenn ich sterbe,", "tokens": ["ich", "will", "mit", "der", "Son\u00b7ne", "gehn", ",", "wenn", "ich", "ster\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "in sommerflammendem Abendrot.", "tokens": ["in", "som\u00b7mer\u00b7flam\u00b7men\u00b7dem", "A\u00b7ben\u00b7drot", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Fenster auf! dort dr\u00fcben ist meine", "tokens": ["Die", "Fens\u00b7ter", "auf", "!", "dort", "dr\u00fc\u00b7ben", "ist", "mei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "ADV", "ADV", "VAFIN", "PPOSAT"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Heimat und nicht in eurer Nacht und Not!", "tokens": ["Hei\u00b7mat", "und", "nicht", "in", "eu\u00b7rer", "Nacht", "und", "Not", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PTKNEG", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "ich will in die Sonne sehn, wenn ich sterbe,", "tokens": ["ich", "will", "in", "die", "Son\u00b7ne", "sehn", ",", "wenn", "ich", "ster\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und sinken gleich ihr in strahlendem Tod.", "tokens": ["und", "sin\u00b7ken", "gleich", "ihr", "in", "strah\u00b7len\u00b7dem", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}