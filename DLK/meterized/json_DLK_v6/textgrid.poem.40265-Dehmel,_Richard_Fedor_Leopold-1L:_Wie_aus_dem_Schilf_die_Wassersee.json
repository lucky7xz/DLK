{"textgrid.poem.40265": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie aus dem Schilf die Wassersee", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie aus dem Schilf die Wassersee", "tokens": ["Wie", "aus", "dem", "Schilf", "die", "Was\u00b7ser\u00b7see"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "tauchtest du winkend aus der Schaar", "tokens": ["tauch\u00b7test", "du", "win\u00b7kend", "aus", "der", "Schaar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "der Andern um uns zu mir her", "tokens": ["der", "An\u00b7dern", "um", "uns", "zu", "mir", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "APPR", "PPER", "APPR", "PPER", "APZR"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "mit deinem langen schwarzen Haar", "tokens": ["mit", "dei\u00b7nem", "lan\u00b7gen", "schwar\u00b7zen", "Haar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und deinem fernen Augenpaar.", "tokens": ["und", "dei\u00b7nem", "fer\u00b7nen", "Au\u00b7gen\u00b7paar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und standest nun und sahst mich an", "tokens": ["Und", "stan\u00b7dest", "nun", "und", "sahst", "mich", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "KON", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mit deinem blassen Uebermut,", "tokens": ["mit", "dei\u00b7nem", "blas\u00b7sen", "Ue\u00b7ber\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und deiner Fragen perlende Flut", "tokens": ["und", "dei\u00b7ner", "Fra\u00b7gen", "per\u00b7len\u00b7de", "Flut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "und deiner Lippen jungjunges Blut", "tokens": ["und", "dei\u00b7ner", "Lip\u00b7pen", "jung\u00b7jun\u00b7ges", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "lachte mich an, lachte mich an.", "tokens": ["lach\u00b7te", "mich", "an", ",", "lach\u00b7te", "mich", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Nur in deinen Augen blieb so fern,", "tokens": ["Nur", "in", "dei\u00b7nen", "Au\u00b7gen", "blieb", "so", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "so fern wie auf des Weihers Grund", "tokens": ["so", "fern", "wie", "auf", "des", "Wei\u00b7hers", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "in winkender Nacht der irre Stern,", "tokens": ["in", "win\u00b7ken\u00b7der", "Nacht", "der", "ir\u00b7re", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "ein Zittern und Leuchten stehen; und", "tokens": ["ein", "Zit\u00b7tern", "und", "Leuch\u00b7ten", "ste\u00b7hen", ";", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$.", "KON"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "mir log dein Mund, mir log dein Mund.", "tokens": ["mir", "log", "dein", "Mund", ",", "mir", "log", "dein", "Mund", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Denn in unsern Tr\u00e4umen \u2013 oh, ich wei\u00df:", "tokens": ["Denn", "in", "un\u00b7sern", "Tr\u00e4u\u00b7men", "\u2013", "oh", ",", "ich", "wei\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$(", "FM", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "auch du, auch du! dann tauchen wir", "tokens": ["auch", "du", ",", "auch", "du", "!", "dann", "tau\u00b7chen", "wir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "ADV", "PPER", "$.", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hand in Hand hinunter, stumm und hei\u00df", "tokens": ["Hand", "in", "Hand", "hin\u00b7un\u00b7ter", ",", "stumm", "und", "hei\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "PTKVZ", "$,", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "sucht Mund den Mund, holen wir leis,", "tokens": ["sucht", "Mund", "den", "Mund", ",", "ho\u00b7len", "wir", "leis", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.5": {"text": "wir uns vom Grund, den Stern vom Grund ...", "tokens": ["wir", "uns", "vom", "Grund", ",", "den", "Stern", "vom", "Grund", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPRART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}