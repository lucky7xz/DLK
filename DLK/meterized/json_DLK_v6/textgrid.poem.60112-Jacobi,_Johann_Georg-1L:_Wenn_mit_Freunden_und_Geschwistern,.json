{"textgrid.poem.60112": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn mit Freunden und Geschwistern,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn mit Freunden und Geschwistern,", "tokens": ["Wenn", "mit", "Freun\u00b7den", "und", "Ge\u00b7schwis\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich ein stilles Grab umschlie\u00dft;", "tokens": ["Mich", "ein", "stil\u00b7les", "Grab", "um\u00b7schlie\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht das Quell-Ger\u00e4usch und nicht das Fl\u00fcstern", "tokens": ["Nicht", "das", "Quell\u00b7Ge\u00b7r\u00e4usch", "und", "nicht", "das", "Fl\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "KON", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Im Kastanien-Hain uns mehr begr\u00fc\u00dft;", "tokens": ["Im", "Kas\u00b7ta\u00b7ni\u00b7en\u00b7Hain", "uns", "mehr", "be\u00b7gr\u00fc\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Kaum gekannt vielleicht, an \u00f6den W\u00e4nden,", "tokens": ["Kaum", "ge\u00b7kannt", "viel\u00b7leicht", ",", "an", "\u00f6\u00b7den", "W\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Von der Spinn' umwebt, noch unser Bildni\u00df h\u00e4ngt,", "tokens": ["Von", "der", "Spinn'", "um\u00b7webt", ",", "noch", "un\u00b7ser", "Bild\u00b7ni\u00df", "h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Und aus sp\u00e4ter Enkel H\u00e4nden", "tokens": ["Und", "aus", "sp\u00e4\u00b7ter", "En\u00b7kel", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dieses Buch ein Fremdling dann empf\u00e4ngt;", "tokens": ["Die\u00b7ses", "Buch", "ein", "Fremd\u00b7ling", "dann", "emp\u00b7f\u00e4ngt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Wenn es im Gew\u00fchl verge\u00dfner Brief' und Lieder", "tokens": ["Wenn", "es", "im", "Ge\u00b7w\u00fchl", "ver\u00b7ge\u00df\u00b7ner", "Brie\u00b7f'", "und", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADJA", "NN", "KON", "NN"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Nun versunken ist, der Motten Raub \u2013", "tokens": ["Nun", "ver\u00b7sun\u00b7ken", "ist", ",", "der", "Mot\u00b7ten", "Raub", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "$,", "ART", "NN", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "O so find es eine gute Seele wieder,", "tokens": ["O", "so", "find", "es", "ei\u00b7ne", "gu\u00b7te", "See\u00b7le", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.12": {"text": "Und die rett' es aus dem Staub!", "tokens": ["Und", "die", "rett'", "es", "aus", "dem", "Staub", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Ach! bevor die Bl\u00e4tter ganz verwesen,", "tokens": ["Ach", "!", "be\u00b7vor", "die", "Bl\u00e4t\u00b7ter", "ganz", "ver\u00b7we\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Gute Seele, blick hinein!", "tokens": ["Gu\u00b7te", "See\u00b7le", ",", "blick", "hin\u00b7ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVIMP", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Todte Namen nicht, der Liebe Gl\u00fcck zu lesen,", "tokens": ["Tod\u00b7te", "Na\u00b7men", "nicht", ",", "der", "Lie\u00b7be", "Gl\u00fcck", "zu", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKNEG", "$,", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.16": {"text": "Unser Gl\u00fcck am Quell, und im Kastanien-Hain.", "tokens": ["Un\u00b7ser", "Gl\u00fcck", "am", "Quell", ",", "und", "im", "Kas\u00b7ta\u00b7ni\u00b7en\u00b7Hain", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,", "KON", "APPRART", "NN", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Wo du gehst, da gingen, ohne Reue,", "tokens": ["Wo", "du", "gehst", ",", "da", "gin\u00b7gen", ",", "oh\u00b7ne", "Reu\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Wir Geschwister einst, geleitet von der Treue,", "tokens": ["Wir", "Ge\u00b7schwis\u00b7ter", "einst", ",", "ge\u00b7lei\u00b7tet", "von", "der", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADV", "$,", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.19": {"text": "Hand in Hand, im frohesten Verein;", "tokens": ["Hand", "in", "Hand", ",", "im", "fro\u00b7hes\u00b7ten", "Ver\u00b7ein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+---+", "measure": "unknown.measure.tetra"}, "line.20": {"text": "Waren gleich dem frischen Kranze,", "tokens": ["Wa\u00b7ren", "gleich", "dem", "fri\u00b7schen", "Kran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Den f\u00fcr eine Braut ein Chor von M\u00e4dchen pfl\u00fcckt,", "tokens": ["Den", "f\u00fcr", "ei\u00b7ne", "Braut", "ein", "Chor", "von", "M\u00e4d\u00b7chen", "pfl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.22": {"text": "Sorgend, da\u00df die Ros' in vollem Glanze", "tokens": ["Sor\u00b7gend", ",", "da\u00df", "die", "Ros'", "in", "vol\u00b7lem", "Glan\u00b7ze"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Nicht das zarte Bl\u00fcmchen dr\u00fcckt. \u2013", "tokens": ["Nicht", "das", "zar\u00b7te", "Bl\u00fcm\u00b7chen", "dr\u00fcckt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Weile, gute Seele, hier, und Allen,", "tokens": ["Wei\u00b7le", ",", "gu\u00b7te", "See\u00b7le", ",", "hier", ",", "und", "Al\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "ADV", "$,", "KON", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "Deren du dich freuest, sage du:", "tokens": ["De\u00b7ren", "du", "dich", "freu\u00b7est", ",", "sa\u00b7ge", "du", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PPER", "PRF", "VVFIN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}