{"textgrid.poem.37581": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Prosit Neujahr!", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da steht und kr\u00e4ht er.", "tokens": ["Da", "steht", "und", "kr\u00e4ht", "er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Vielleicht ger\u00e4t er.", "tokens": ["Viel\u00b7leicht", "ge\u00b7r\u00e4t", "er", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Das alte Jahr gar schnell entwich.", "tokens": ["Das", "al\u00b7te", "Jahr", "gar", "schnell", "ent\u00b7wich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es konnt sich kaum gedulden", "tokens": ["Es", "konnt", "sich", "kaum", "ge\u00b7dul\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und lie\u00df mit Freuden hinter sich", "tokens": ["Und", "lie\u00df", "mit", "Freu\u00b7den", "hin\u00b7ter", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den dicken Sack voll Schulden.", "tokens": ["Den", "di\u00b7cken", "Sack", "voll", "Schul\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}