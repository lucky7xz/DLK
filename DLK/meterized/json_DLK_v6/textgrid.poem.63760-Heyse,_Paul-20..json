{"textgrid.poem.63760": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich nenn' ich wohl des Gl\u00fcckes Lieblingssohn;", "tokens": ["Dich", "nenn'", "ich", "wohl", "des", "Gl\u00fc\u00b7ckes", "Lieb\u00b7lings\u00b7sohn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denn treulich folgend eines Gr\u00f6\u00dfern Tritten,", "tokens": ["Denn", "treu\u00b7lich", "fol\u00b7gend", "ei\u00b7nes", "Gr\u00f6\u00b7\u00dfern", "Trit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bist du Jahrhunderte hindurchgeschritten", "tokens": ["Bist", "du", "Jahr\u00b7hun\u00b7der\u00b7te", "hin\u00b7durch\u00b7ge\u00b7schrit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und glorreich der Vergessenheit entflohn.", "tokens": ["Und", "glor\u00b7reich", "der", "Ver\u00b7ges\u00b7sen\u00b7heit", "ent\u00b7flohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und wieder hob empor zu seinem Thron", "tokens": ["Und", "wie\u00b7der", "hob", "em\u00b7por", "zu", "sei\u00b7nem", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Gr\u00f6\u00dfrer dich, der durch der H\u00f6lle Mitten", "tokens": ["Ein", "Gr\u00f6\u00df\u00b7rer", "dich", ",", "der", "durch", "der", "H\u00f6l\u00b7le", "Mit\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "$,", "PRELS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum F\u00fchrer dich erkor, und wieder glitten", "tokens": ["Zum", "F\u00fch\u00b7rer", "dich", "er\u00b7kor", ",", "und", "wie\u00b7der", "glit\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "$,", "KON", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weltalter hin \u2013 du sprachst dem Wechsel Hohn.", "tokens": ["Welt\u00b7al\u00b7ter", "hin", "\u2013", "du", "sprachst", "dem", "Wech\u00b7sel", "Hohn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$(", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zwar was du sangst von Waffen, Hirt und Herde,", "tokens": ["Zwar", "was", "du", "sangst", "von", "Waf\u00b7fen", ",", "Hirt", "und", "Her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPER", "VVFIN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hat nie die Welt ersch\u00fcttert zauberm\u00e4chtig.", "tokens": ["Hat", "nie", "die", "Welt", "er\u00b7sch\u00fct\u00b7tert", "zau\u00b7ber\u00b7m\u00e4ch\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du aber bliebst der Zaubrer der Poeten.", "tokens": ["Du", "a\u00b7ber", "bliebst", "der", "Zaub\u00b7rer", "der", "Po\u00b7et\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es liegt am zaubersch\u00f6nsten Fleck der Erde", "tokens": ["Es", "liegt", "am", "zau\u00b7ber\u00b7sch\u00f6ns\u00b7ten", "Fleck", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein Grab, und zu ihm wallt die Welt and\u00e4chtig,", "tokens": ["Dein", "Grab", ",", "und", "zu", "ihm", "wallt", "die", "Welt", "an\u00b7d\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "APPR", "PPER", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie zu der Gruft der Heil'gen und Propheten.", "tokens": ["Wie", "zu", "der", "Gruft", "der", "Heil'\u00b7gen", "und", "Pro\u00b7phe\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}