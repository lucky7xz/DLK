{"textgrid.poem.36610": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Fliege", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht, Freunde, seht, die arme Fliege hier,", "tokens": ["Seht", ",", "Freun\u00b7de", ",", "seht", ",", "die", "ar\u00b7me", "Flie\u00b7ge", "hier", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "$,", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Beklagt, bejammert sie mit mir!", "tokens": ["Be\u00b7klagt", ",", "be\u00b7jam\u00b7mert", "sie", "mit", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sah den Wein in meinem Glase blinken;", "tokens": ["Sie", "sah", "den", "Wein", "in", "mei\u00b7nem", "Gla\u00b7se", "blin\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er lockte sie, zu ihm herab zu sinken,", "tokens": ["Er", "lock\u00b7te", "sie", ",", "zu", "ihm", "her\u00b7ab", "zu", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und auch, wie wir, Ambrosia zu trinken.", "tokens": ["Und", "auch", ",", "wie", "wir", ",", "A\u00b7mbro\u00b7sia", "zu", "trin\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "PPER", "$,", "NE", "PTKZU", "VVINF", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Sie sank herab,", "tokens": ["Sie", "sank", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und fand ihr Grab,", "tokens": ["Und", "fand", "ihr", "Grab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Und trank den Tod, wo wir das Leben trinken.", "tokens": ["Und", "trank", "den", "Tod", ",", "wo", "wir", "das", "Le\u00b7ben", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}