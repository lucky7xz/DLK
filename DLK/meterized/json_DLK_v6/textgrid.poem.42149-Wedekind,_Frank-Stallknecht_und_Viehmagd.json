{"textgrid.poem.42149": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Stallknecht und Viehmagd", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die B\u00e4rin wohnt im tiefen Walde,", "tokens": ["Die", "B\u00e4\u00b7rin", "wohnt", "im", "tie\u00b7fen", "Wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im tiefen Wald wohnt auch der B\u00e4r,", "tokens": ["Im", "tie\u00b7fen", "Wald", "wohnt", "auch", "der", "B\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und an demselben Aufenthalte,", "tokens": ["Und", "an", "dem\u00b7sel\u00b7ben", "Auf\u00b7ent\u00b7hal\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da wohnen B\u00e4ren bald noch mehr.", "tokens": ["Da", "woh\u00b7nen", "B\u00e4\u00b7ren", "bald", "noch", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und im Olymp, da wohnen G\u00f6tter,", "tokens": ["Und", "im", "O\u00b7lymp", ",", "da", "woh\u00b7nen", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Darunter Venus und Apoll;", "tokens": ["Da\u00b7run\u00b7ter", "Ve\u00b7nus", "und", "A\u00b7poll", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dort hat man ewig sch\u00f6nes Wetter", "tokens": ["Dort", "hat", "man", "e\u00b7wig", "sch\u00f6\u00b7nes", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und jeder Gott ist liebevoll.", "tokens": ["Und", "je\u00b7der", "Gott", "ist", "lie\u00b7be\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf \u00f6dem Felde schafft die Viehmagd,", "tokens": ["Auf", "\u00f6\u00b7dem", "Fel\u00b7de", "schafft", "die", "Vieh\u00b7magd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Tut ob der Arbeit manchen Schrei,", "tokens": ["Tut", "ob", "der", "Ar\u00b7beit", "man\u00b7chen", "Schrei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Jedoch Cupido, der sich nie plagt,", "tokens": ["Je\u00b7doch", "Cu\u00b7pi\u00b7do", ",", "der", "sich", "nie", "plagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$,", "PRELS", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "W\u00e4lzt sich im Grase nebenbei.", "tokens": ["W\u00e4lzt", "sich", "im", "Gra\u00b7se", "ne\u00b7ben\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Nun kommt der Stallknecht mit den K\u00fchen;", "tokens": ["Nun", "kommt", "der", "Stall\u00b7knecht", "mit", "den", "K\u00fc\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch Ochsen ziehen an dem Pflug,", "tokens": ["Auch", "Och\u00b7sen", "zie\u00b7hen", "an", "dem", "Pflug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch mu\u00df er selbst das meiste ziehen,", "tokens": ["Doch", "mu\u00df", "er", "selbst", "das", "meis\u00b7te", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ART", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dann geht es eben flott genug.", "tokens": ["Dann", "geht", "es", "e\u00b7ben", "flott", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Cupido duckt sich listig nieder,", "tokens": ["Cu\u00b7pi\u00b7do", "duckt", "sich", "lis\u00b7tig", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er legt den Bogen an mit Lust", "tokens": ["Er", "legt", "den", "Bo\u00b7gen", "an", "mit", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schie\u00dft die Viehmagd durch das Mieder", "tokens": ["Und", "schie\u00dft", "die", "Vieh\u00b7magd", "durch", "das", "Mie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In ihre ahnungslose Brust.", "tokens": ["In", "ih\u00b7re", "ah\u00b7nungs\u00b7lo\u00b7se", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Stallknecht kommt herbeigesprungen,", "tokens": ["Der", "Stall\u00b7knecht", "kommt", "her\u00b7bei\u00b7ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf da\u00df er rasch ihr Hilfe bringt;", "tokens": ["Auf", "da\u00df", "er", "rasch", "ihr", "Hil\u00b7fe", "bringt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Cupido trifft den guten Jungen,", "tokens": ["Cu\u00b7pi\u00b7do", "trifft", "den", "gu\u00b7ten", "Jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df er mit ihr zu Boden sinkt.", "tokens": ["Da\u00df", "er", "mit", "ihr", "zu", "Bo\u00b7den", "sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Da liegen Stallknecht nun und Viehmagd", "tokens": ["Da", "lie\u00b7gen", "Stall\u00b7knecht", "nun", "und", "Vieh\u00b7magd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und schauen sich verwundert an,", "tokens": ["Und", "schau\u00b7en", "sich", "ver\u00b7wun\u00b7dert", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nachher tun sie, was man nie sagt,", "tokens": ["Und", "nach\u00b7her", "tun", "sie", ",", "was", "man", "nie", "sagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "PRELS", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Doch was man leicht erraten kann.", "tokens": ["Doch", "was", "man", "leicht", "er\u00b7ra\u00b7ten", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIS", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}