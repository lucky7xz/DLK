{"textgrid.poem.46717": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ich wollt' eine Knospe pfl\u00fccken]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wollt' eine Knospe pfl\u00fccken", "tokens": ["Ich", "wollt'", "ei\u00b7ne", "Knos\u00b7pe", "pfl\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Im Morgenglanz,", "tokens": ["Im", "Mor\u00b7gen\u00b7glanz", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mich fr\u00fchlingsgem\u00e4\u00df zu schm\u00fccken", "tokens": ["Mich", "fr\u00fch\u00b7lings\u00b7ge\u00b7m\u00e4\u00df", "zu", "schm\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit einem Kranz.", "tokens": ["Mit", "ei\u00b7nem", "Kranz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Da h\u00f6rt' ich die Knospe sprechen", "tokens": ["Da", "h\u00f6rt'", "ich", "die", "Knos\u00b7pe", "spre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit leisem Ton:", "tokens": ["Mit", "lei\u00b7sem", "Ton", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und willst du mich also brechen", "tokens": ["Und", "willst", "du", "mich", "al\u00b7so", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PRF", "ADV", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Im Fr\u00fchthau schon?", "tokens": ["Im", "Fr\u00fch\u00b7thau", "schon", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ich bitte bei deinem Kinde,", "tokens": ["Ich", "bit\u00b7te", "bei", "dei\u00b7nem", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dir fr\u00fch geraubt:", "tokens": ["Dir", "fr\u00fch", "ge\u00b7raubt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mir sei noch im Fr\u00fchlingswinde", "tokens": ["Mir", "sei", "noch", "im", "Fr\u00fch\u00b7lings\u00b7win\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein Tag erlaubt!", "tokens": ["Ein", "Tag", "er\u00b7laubt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}