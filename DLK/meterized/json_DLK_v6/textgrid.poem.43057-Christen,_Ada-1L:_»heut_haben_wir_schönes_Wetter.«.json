{"textgrid.poem.43057": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbheut haben wir sch\u00f6nes Wetter.\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbheut haben wir sch\u00f6nes Wetter.\u00ab", "tokens": ["\u00bb", "heut", "ha\u00b7ben", "wir", "sch\u00f6\u00b7nes", "Wet\u00b7ter", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "\u00bb\u00bbO ja, recht sch\u00f6nes, mein Herr!\u00ab\u00ab", "tokens": ["\u00bb", "\u00bb", "O", "ja", ",", "recht", "sch\u00f6\u00b7nes", ",", "mein", "Herr", "!", "\u00ab", "\u00ab"], "token_info": ["punct", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "$(", "ITJ", "ITJ", "$,", "ADJD", "ADJA", "$,", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Das sind so unsre Gespr\u00e4che,", "tokens": ["Das", "sind", "so", "uns\u00b7re", "Ge\u00b7spr\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So kalt, so dumm, so leer.", "tokens": ["So", "kalt", ",", "so", "dumm", ",", "so", "leer", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du streichelst mir fragend die Wange,", "tokens": ["Du", "strei\u00b7chelst", "mir", "fra\u00b7gend", "die", "Wan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Du kennst das gewisse Roth;", "tokens": ["Du", "kennst", "das", "ge\u00b7wis\u00b7se", "Roth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "F\u00fcr dich ist's nichts als Schminke \u2013", "tokens": ["F\u00fcr", "dich", "ist's", "nichts", "als", "Schmin\u00b7ke", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIS", "KOKOM", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr mich: in der Brust der Tod.", "tokens": ["F\u00fcr", "mich", ":", "in", "der", "Brust", "der", "Tod", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}