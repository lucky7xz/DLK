{"textgrid.poem.26496": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fr\u00fchling war's. Im Abendschatten", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchling war's. Im Abendschatten", "tokens": ["Fr\u00fch\u00b7ling", "wa\u00b7r'", "s.", "Im", "A\u00b7bend\u00b7schat\u00b7ten"], "token_info": ["word", "word", "abbreviation", "word", "word"], "pos": ["NE", "VAFIN", "NE", "APPRART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ging ich durch das stille Thal \u2013", "tokens": ["Ging", "ich", "durch", "das", "stil\u00b7le", "Thal", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da, vor mir auf gr\u00fcnen Matten,", "tokens": ["Da", ",", "vor", "mir", "auf", "gr\u00fc\u00b7nen", "Mat\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "T\u00f6nt es sanft mit einem Mal.", "tokens": ["T\u00f6nt", "es", "sanft", "mit", "ei\u00b7nem", "Mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "N\u00e4her kam ich; zwei Gestalten", "tokens": ["N\u00e4\u00b7her", "kam", "ich", ";", "zwei", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sa\u00dfen ruhig, Hand in Hand;", "tokens": ["Sa\u00b7\u00dfen", "ru\u00b7hig", ",", "Hand", "in", "Hand", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00e4dchen, wie bei Tag sie walten", "tokens": ["M\u00e4d\u00b7chen", ",", "wie", "bei", "Tag", "sie", "wal\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "APPR", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf durchfurchtem Ackerland.", "tokens": ["Auf", "durch\u00b7furch\u00b7tem", "A\u00b7cker\u00b7land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Braun im Antlitz trugen Beide", "tokens": ["Braun", "im", "Ant\u00b7litz", "tru\u00b7gen", "Bei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "VVFIN", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spuren von der Sonne Ku\u00df,", "tokens": ["Spu\u00b7ren", "von", "der", "Son\u00b7ne", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter dem zerschliss'nen Kleide", "tokens": ["Un\u00b7ter", "dem", "zer\u00b7schliss'\u00b7nen", "Klei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sah hervor der nackte Fu\u00df.", "tokens": ["Sah", "her\u00b7vor", "der", "nack\u00b7te", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Aber sch\u00f6n das Haupt erhoben,", "tokens": ["A\u00b7ber", "sch\u00f6n", "das", "Haupt", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Holden Einklang in der Brust,", "tokens": ["Hol\u00b7den", "Ein\u00b7klang", "in", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu den ersten Sternen droben,", "tokens": ["Zu", "den", "ers\u00b7ten", "Ster\u00b7nen", "dro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sangen sie wie unbewu\u00dft.", "tokens": ["San\u00b7gen", "sie", "wie", "un\u00b7be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sangen sie die alte Weise", "tokens": ["San\u00b7gen", "sie", "die", "al\u00b7te", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der Liebe Lust und Leid,", "tokens": ["Von", "der", "Lie\u00b7be", "Lust", "und", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Achtlos, nur sich selbst zum Preise,", "tokens": ["Acht\u00b7los", ",", "nur", "sich", "selbst", "zum", "Prei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "PRF", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die weite Einsamkeit. \u2013", "tokens": ["Durch", "die", "wei\u00b7te", "Ein\u00b7sam\u00b7keit", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Seid getrost, ihr Dichterseelen,", "tokens": ["Seid", "ge\u00b7trost", ",", "ihr", "Dich\u00b7ter\u00b7see\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dacht' ich im Vor\u00fcbergang,", "tokens": ["Dacht'", "ich", "im", "Vor\u00b7\u00fc\u00b7ber\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6rt ihr noch aus solchen Kehlen", "tokens": ["H\u00f6rt", "ihr", "noch", "aus", "sol\u00b7chen", "Keh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Solchen tief empfund'nen Klang!", "tokens": ["Sol\u00b7chen", "tief", "emp\u00b7fun\u00b7d'\u00b7nen", "Klang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}