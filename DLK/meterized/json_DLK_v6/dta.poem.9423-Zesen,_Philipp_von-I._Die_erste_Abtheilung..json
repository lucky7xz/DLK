{"dta.poem.9423": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "I.  \n Die erste Abtheilung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Freundin/ wie sch\u00f6ne/ wie lieblich bistu!", "tokens": ["O", "Freun\u00b7din", "/", "wie", "sch\u00f6\u00b7ne", "/", "wie", "lieb\u00b7lich", "bis\u00b7tu", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "KOKOM", "ADJA", "$(", "PWAV", "ADJD", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ach h\u00f6re mier zu:", "tokens": ["Ach", "h\u00f6\u00b7re", "mier", "zu", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wie blincken und wincken die schw\u00e4rtzlichen Augen/", "tokens": ["Wie", "blin\u00b7cken", "und", "win\u00b7cken", "die", "schw\u00e4rtz\u00b7li\u00b7chen", "Au\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sie k\u00f6nnen mein Hertze so artlich aussaugen/", "tokens": ["Sie", "k\u00f6n\u00b7nen", "mein", "Hert\u00b7ze", "so", "art\u00b7lich", "aus\u00b7sau\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "VVFIN", "ADV", "ADJD", "VVPP", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Sie gleichen den Tauben an zier", "tokens": ["Sie", "glei\u00b7chen", "den", "Tau\u00b7ben", "an", "zier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "und zihn mich zu Dier.", "tokens": ["und", "zihn", "mich", "zu", "Dier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}