{"textgrid.poem.45757": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "M\u00fcssiggang", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Arbeiten soll er? da\u00df Gott erbarme!", "tokens": ["Ar\u00b7bei\u00b7ten", "soll", "er", "?", "da\u00df", "Gott", "er\u00b7bar\u00b7me", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$.", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da schob Natur schon vor den Riegel,", "tokens": ["Da", "schob", "Na\u00b7tur", "schon", "vor", "den", "Rie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn wo die andern ihre Arme,", "tokens": ["Denn", "wo", "die", "an\u00b7dern", "ih\u00b7re", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da hat er eben seine Fl\u00fcgel.", "tokens": ["Da", "hat", "er", "e\u00b7ben", "sei\u00b7ne", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}