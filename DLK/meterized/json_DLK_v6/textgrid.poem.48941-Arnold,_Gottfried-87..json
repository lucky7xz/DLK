{"textgrid.poem.48941": {"metadata": {"author": {"name": "Arnold, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "87.", "genre": "verse", "period": "N.A.", "pub_year": 1690, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ja wahr/ im Feld siehts lieblich aus/", "tokens": ["Es", "ist", "ja", "wahr", "/", "im", "Feld", "siehts", "lieb\u00b7lich", "aus", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$(", "APPRART", "NN", "ADV", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo alles sich mit Blumen kan bezieren:", "tokens": ["Wo", "al\u00b7les", "sich", "mit", "Blu\u00b7men", "kan", "be\u00b7zie\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PRF", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich aber geh auch hier in meinem Hau\u00df", "tokens": ["Ich", "a\u00b7ber", "geh", "auch", "hier", "in", "mei\u00b7nem", "Hau\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In aller Still mit meinem Lamm spatziren.", "tokens": ["In", "al\u00b7ler", "Still", "mit", "mei\u00b7nem", "Lamm", "spat\u00b7zi\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da scheint die Sonn/ da singt die Nachtigal/", "tokens": ["Da", "scheint", "die", "Sonn", "/", "da", "singt", "die", "Nach\u00b7ti\u00b7gal", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$(", "ADV", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da gr\u00fcnts und bl\u00fchts/ da rauschen frische Quellen.", "tokens": ["Da", "gr\u00fcnts", "und", "bl\u00fchts", "/", "da", "rau\u00b7schen", "fri\u00b7sche", "Quel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "$(", "ADV", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ich seh da nichts als Jesum \u00fcberall/", "tokens": ["Ich", "seh", "da", "nichts", "als", "Je\u00b7sum", "\u00fc\u00b7be\u00b7rall", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "KOKOM", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sein Engel-Chor erf\u00fcllet alle Stellen.", "tokens": ["Sein", "En\u00b7gel\u00b7Chor", "er\u00b7f\u00fcl\u00b7let", "al\u00b7le", "Stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Er ist die Sonn/ die Liebe/ der Gesang/", "tokens": ["Er", "ist", "die", "Sonn", "/", "die", "Lie\u00b7be", "/", "der", "Ge\u00b7sang", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "ART", "NN", "$(", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Dabey die Hoffnung gr\u00fcnt/ und reine Wasser springen.", "tokens": ["Da\u00b7bey", "die", "Hoff\u00b7nung", "gr\u00fcnt", "/", "und", "rei\u00b7ne", "Was\u00b7ser", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVFIN", "$(", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ist das nicht gnung bey meinem sch\u00f6nen Gang?", "tokens": ["Ist", "das", "nicht", "gnung", "bey", "mei\u00b7nem", "sch\u00f6\u00b7nen", "Gang", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Er soll mich ja zum Paradiese bringen.", "tokens": ["Er", "soll", "mich", "ja", "zum", "Pa\u00b7ra\u00b7die\u00b7se", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}