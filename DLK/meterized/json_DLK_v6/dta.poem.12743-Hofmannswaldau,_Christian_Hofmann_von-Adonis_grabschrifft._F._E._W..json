{"dta.poem.12743": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Adonis grabschrifft.  \n F. E. W.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es wolte Pluto mich in seinen diensten sehn;", "tokens": ["Es", "wol\u00b7te", "Plu\u00b7to", "mich", "in", "sei\u00b7nen", "diens\u00b7ten", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch weil er sich bef\u00fcrcht, er m\u00f6chte h\u00f6rner kriegen,", "tokens": ["Doch", "weil", "er", "sich", "be\u00b7f\u00fcrcht", ",", "er", "m\u00f6ch\u00b7te", "h\u00f6r\u00b7ner", "krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVPP", "$,", "PPER", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ich mit seiner frau nur unt- und oben liegen,", "tokens": ["Und", "ich", "mit", "sei\u00b7ner", "frau", "nur", "un\u00b7t", "und", "o\u00b7ben", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "ADV", "TRUNC", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Must ich zuvor entmannt in seine heymath gehn.", "tokens": ["Must", "ich", "zu\u00b7vor", "ent\u00b7mannt", "in", "sei\u00b7ne", "hey\u00b7math", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}