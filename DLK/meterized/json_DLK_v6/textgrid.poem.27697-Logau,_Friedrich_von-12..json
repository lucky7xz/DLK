{"textgrid.poem.27697": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was taug der alte Krieg? der neue Krieg ist besser;", "tokens": ["Was", "taug", "der", "al\u00b7te", "Krieg", "?", "der", "neu\u00b7e", "Krieg", "ist", "bes\u00b7ser", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn jener war ein Feind der Menschen, der der Schl\u00f6sser,", "tokens": ["Denn", "je\u00b7ner", "war", "ein", "Feind", "der", "Men\u00b7schen", ",", "der", "der", "Schl\u00f6s\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ART", "NN", "ART", "NN", "$,", "PRELS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und jener machte leer der Menschen Leib von Blut,", "tokens": ["Und", "je\u00b7ner", "mach\u00b7te", "leer", "der", "Men\u00b7schen", "Leib", "von", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADJD", "ART", "NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da dieser nur fegt au\u00df der Kasten altes Gut.", "tokens": ["Da", "die\u00b7ser", "nur", "fegt", "au\u00df", "der", "Kas\u00b7ten", "al\u00b7tes", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADV", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}}}}