{"textgrid.poem.53585": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Kino-Atelier", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da vorne klemmt ein Jraf sich das Monokel", "tokens": ["Da", "vor\u00b7ne", "klemmt", "ein", "Jraf", "sich", "das", "Mo\u00b7no\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "platt ins Gesicht \u2013 die Bogenlampe zischt.", "tokens": ["platt", "ins", "Ge\u00b7sicht", "\u2013", "die", "Bo\u00b7gen\u00b7lam\u00b7pe", "zischt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$(", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ein Gazefr\u00e4ulein steht auf einem Sockel \u2013", "tokens": ["Ein", "Ga\u00b7ze\u00b7fr\u00e4u\u00b7lein", "steht", "auf", "ei\u00b7nem", "So\u00b7ckel", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "der dicke Regisseur br\u00fcllt: \u00bbDas is nischt!\u00ab", "tokens": ["der", "di\u00b7cke", "Re\u00b7gis\u00b7seur", "br\u00fcllt", ":", "\u00bb", "Das", "is", "nischt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "$(", "ART", "FM", "PTKNEG", "$.", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Zweihundertvierzig M\u00e4dchen trippeln zierlich", "tokens": ["Zwei\u00b7hun\u00b7dert\u00b7vier\u00b7zig", "M\u00e4d\u00b7chen", "trip\u00b7peln", "zier\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "auf einer Treppe, steil bis unters Dach \u2013", "tokens": ["auf", "ei\u00b7ner", "Trep\u00b7pe", ",", "steil", "bis", "un\u00b7ters", "Dach", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "APPR", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein kleines dickes Baby schluckt manierlich", "tokens": ["Ein", "klei\u00b7nes", "di\u00b7ckes", "Ba\u00b7by", "schluckt", "ma\u00b7nier\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "die Milch \u2013", "tokens": ["die", "Milch", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "der Chef macht mit der Diva Krach.", "tokens": ["der", "Chef", "macht", "mit", "der", "Di\u00b7va", "Krach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "In dieser Ecke st\u00f6\u00dft ein Intrigante", "tokens": ["In", "die\u00b7ser", "E\u00b7cke", "st\u00f6\u00dft", "ein", "Int\u00b7ri\u00b7gan\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "dem Helden \u2013 brr! \u2013 das Messer in den Bauch.", "tokens": ["dem", "Hel\u00b7den", "\u2013", "brr", "!", "\u2013", "das", "Mes\u00b7ser", "in", "den", "Bauch", "."], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NE", "$.", "$(", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In jener Ecke spritzt die gute Tante", "tokens": ["In", "je\u00b7ner", "E\u00b7cke", "spritzt", "die", "gu\u00b7te", "Tan\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "der b\u00f6se Neffe mit dem Gartenschlauch.", "tokens": ["der", "b\u00f6\u00b7se", "Nef\u00b7fe", "mit", "dem", "Gar\u00b7ten\u00b7schlauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Dirne l\u00fcmmelt sich an ihren Buhlen.", "tokens": ["Die", "Dir\u00b7ne", "l\u00fcm\u00b7melt", "sich", "an", "ih\u00b7ren", "Buh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Herr Beleuchter macht sich nichts daraus", "tokens": ["Der", "Herr", "Be\u00b7leuch\u00b7ter", "macht", "sich", "nichts", "da\u00b7raus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "PRF", "PIS", "PAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und knipst behufs Erzeugung einer schwulen", "tokens": ["und", "knipst", "be\u00b7hufs", "Er\u00b7zeu\u00b7gung", "ei\u00b7ner", "schwu\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verf\u00fchrungsszene eine Lampe aus.", "tokens": ["Ver\u00b7f\u00fch\u00b7rungs\u00b7sze\u00b7ne", "ei\u00b7ne", "Lam\u00b7pe", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und wenn ich mir dies Atelier bekieke,", "tokens": ["Und", "wenn", "ich", "mir", "dies", "A\u00b7te\u00b7lier", "be\u00b7kie\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "PDS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "voll Kitsch und L\u00e4rm und Rummel, Schmerz und Spa\u00df \u2013:", "tokens": ["voll", "Kitsch", "und", "L\u00e4rm", "und", "Rum\u00b7mel", ",", "Schmerz", "und", "Spa\u00df", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "dann seh ich vor mir unsre Politike.", "tokens": ["dann", "seh", "ich", "vor", "mir", "uns\u00b7re", "Po\u00b7li\u00b7ti\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da spielt auch jeder nur die eigene Musike \u2013", "tokens": ["Da", "spielt", "auch", "je\u00b7der", "nur", "die", "ei\u00b7ge\u00b7ne", "Mu\u00b7si\u00b7ke", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIS", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "und an das Ganze denkt kein Aas.", "tokens": ["und", "an", "das", "Gan\u00b7ze", "denkt", "kein", "Aas", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}