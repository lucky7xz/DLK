{"dta.poem.21348": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Im n\u00e4chtlichen Walde humpelt dahin", "tokens": ["Im", "n\u00e4cht\u00b7li\u00b7chen", "Wal\u00b7de", "hum\u00b7pelt", "da\u00b7hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PAV"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Chaise. Da kracht es pl\u00f6tzlich \u2013", "tokens": ["Die", "Chai\u00b7se", ".", "Da", "kracht", "es", "pl\u00f6tz\u00b7lich", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADV", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein Rad ging los. Wir halten still.", "tokens": ["Ein", "Rad", "ging", "los", ".", "Wir", "hal\u00b7ten", "still", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ist nicht sehr erg\u00f6tzlich.", "tokens": ["Das", "ist", "nicht", "sehr", "er\u00b7g\u00f6tz\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}