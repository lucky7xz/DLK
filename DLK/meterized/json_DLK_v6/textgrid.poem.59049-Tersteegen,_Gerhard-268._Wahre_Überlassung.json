{"textgrid.poem.59049": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "268. Wahre \u00dcberlassung", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du dich selbst besehn und halten,", "tokens": ["Willst", "du", "dich", "selbst", "be\u00b7sehn", "und", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So bleibt dein Herz in Zwang und Pein;", "tokens": ["So", "bleibt", "dein", "Herz", "in", "Zwang", "und", "Pein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df Gott mit dir freim\u00e4chtig schalten,", "tokens": ["La\u00df", "Gott", "mit", "dir", "frei\u00b7m\u00e4ch\u00b7tig", "schal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "APPR", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Folg ohne Seh'n und liebe rein!", "tokens": ["Folg", "oh\u00b7ne", "Seh'n", "und", "lie\u00b7be", "rein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}