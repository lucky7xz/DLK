{"dta.poem.21269": {"metadata": {"author": {"name": "Ramler, Karl Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Auf die Wiederkunft des K\u00f6niges.  \n Berlin, den 30 M\u00e4rz 1763.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20476-3", "language": ["de:0.99"], "booktitle": "Ramler, Karl Wilhelm: Oden. Berlin, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Der Held, um den du bebtest, wann\nim Streite,", "tokens": ["Der", "Held", ",", "um", "den", "du", "beb\u00b7test", ",", "wann", "im", "Strei\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "ART", "PPER", "VVFIN", "$,", "PWAV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohin ihn dein Verh\u00e4ngniss trug,", "tokens": ["Wo\u00b7hin", "ihn", "dein", "Ver\u00b7h\u00e4ng\u00b7niss", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der ehrne Donner von den Bergen ihm", "tokens": ["Der", "ehr\u00b7ne", "Don\u00b7ner", "von", "den", "Ber\u00b7gen", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Feldherrn niederschlug:", "tokens": ["Die", "Feld\u00b7herrn", "nie\u00b7der\u00b7schlug", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da wider ihn mehr Feinde sich ge-", "tokens": ["Da", "wi\u00b7der", "ihn", "mehr", "Fein\u00b7de", "sich", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "PIAT", "NN", "PRF", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als dir die Nachwelt glauben darf,", "tokens": ["Als", "dir", "die", "Nach\u00b7welt", "glau\u00b7ben", "darf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und er sich mit entschlossner Seele zweyen", "tokens": ["Und", "er", "sich", "mit", "ent\u00b7schloss\u00b7ner", "See\u00b7le", "zwe\u00b7yen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PRF", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Allein entgegen warf;", "tokens": ["Al\u00b7lein", "ent\u00b7ge\u00b7gen", "warf", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dein K\u00f6nig, o Berlin, durch den du", "tokens": ["Dein", "K\u00f6\u00b7nig", ",", "o", "Ber\u00b7lin", ",", "durch", "den", "du"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "FM", "NE", "$,", "APPR", "ART", "PPER"], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Als alle deine Schwestern bist,", "tokens": ["Als", "al\u00b7le", "dei\u00b7ne", "Schwes\u00b7tern", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Voll K\u00fcnste deine Thore, Felsen deine", "tokens": ["Voll", "K\u00fcns\u00b7te", "dei\u00b7ne", "Tho\u00b7re", ",", "Fel\u00b7sen", "dei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "NN", "PPOSAT", "NN", "$,", "NN", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Flur ein Garten ist;", "tokens": ["Die", "Flur", "ein", "Gar\u00b7ten", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dein Vater, der dich oft in deinem", "tokens": ["Dein", "Va\u00b7ter", ",", "der", "dich", "oft", "in", "dei\u00b7nem"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gespeist, \u2014 kehrt wieder in dein Land,", "tokens": ["Ge\u00b7speist", ",", "kehrt", "wie\u00b7der", "in", "dein", "Land", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "$(", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und hat in Fesseln an der H\u00f6llenpforten", "tokens": ["Und", "hat", "in", "Fes\u00b7seln", "an", "der", "H\u00f6l\u00b7len\u00b7pfor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Zwietracht hingebannt.", "tokens": ["Die", "Zwiet\u00b7racht", "hin\u00b7ge\u00b7bannt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Fall\u2019 an sein Herz, o K\u00f6niginn, mit", "tokens": ["Fall'", "an", "sein", "Herz", ",", "o", "K\u00f6\u00b7ni\u00b7ginn", ",", "mit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,", "FM", "NN", "$,", "APPR"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der Freude! Fleuch an seine Brust,", "tokens": ["Der", "Freu\u00b7de", "!", "Fleuch", "an", "sei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Amalia, von deinen frommen Dank-", "tokens": ["A\u00b7ma\u00b7lia", ",", "von", "dei\u00b7nen", "from\u00b7men", "Dank"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "PPOSAT", "ADJA", "TRUNC"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und rede, wenn die Lust", "tokens": ["Und", "re\u00b7de", ",", "wenn", "die", "Lust"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dich reden l\u00e4sst! Verm\u00e4hlte seiner", "tokens": ["Dich", "re\u00b7den", "l\u00e4sst", "!", "Ver\u00b7m\u00e4hl\u00b7te", "sei\u00b7ner"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVINF", "VVFIN", "$.", "VVFIN", "PPOSAT"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "K\u00fcsst sein friedselig Angesicht:", "tokens": ["K\u00fcsst", "sein", "fried\u00b7se\u00b7lig", "An\u00b7ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Willkommen, Schutzgeist deines Volkes!", "tokens": ["Will\u00b7kom\u00b7men", ",", "Schutz\u00b7geist", "dei\u00b7nes", "Vol\u00b7kes", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Willkommen! und mehr nicht.", "tokens": ["Will\u00b7kom\u00b7men", "!", "und", "mehr", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "ADV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ihr Jungfraun deckt mit immergr\u00fcnen", "tokens": ["Ihr", "Jung\u00b7fraun", "deckt", "mit", "im\u00b7mer\u00b7gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit einem ganzen Lorbeerhain", "tokens": ["Mit", "ei\u00b7nem", "gan\u00b7zen", "Lor\u00b7beer\u00b7hain"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Weg! mischt Bluhmen, die der offnen", "tokens": ["Den", "Weg", "!", "mischt", "Bluh\u00b7men", ",", "die", "der", "off\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "VVFIN", "NN", "$,", "PRELS", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und fr\u00fche Bl\u00fcthe drein!", "tokens": ["Und", "fr\u00fc\u00b7he", "Bl\u00fc\u00b7the", "drein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ihr edeln M\u00fctter, opfert Spece-", "tokens": ["Ihr", "e\u00b7deln", "M\u00fct\u00b7ter", ",", "op\u00b7fert", "Spe\u00b7ce"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Maraba den Tempeln zollt,", "tokens": ["Die", "Ma\u00b7ra\u00b7ba", "den", "Tem\u00b7peln", "zollt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da wo sein goldner Wagen durch ge-", "tokens": ["Da", "wo", "sein", "gold\u00b7ner", "Wa\u00b7gen", "durch", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "PPOSAT", "ADJA", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Entz\u00fcckter Augen rollt.", "tokens": ["Ent\u00b7z\u00fcck\u00b7ter", "Au\u00b7gen", "rollt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Heil uns, dass unser Morgen in die", "tokens": ["Heil", "uns", ",", "dass", "un\u00b7ser", "Mor\u00b7gen", "in", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "APPR", "ART"], "meter": "-+-+-+-++", "measure": "unknown.measure.penta"}, "line.2": {"text": "Des ", "tokens": ["Des"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "So sagt ihr J\u00fcnglinge. Du, Chor der Alten,", "tokens": ["So", "sagt", "ihr", "J\u00fcng\u00b7lin\u00b7ge", ".", "Du", ",", "Chor", "der", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Heil uns, dass wir das Ziel", "tokens": ["Heil", "uns", ",", "dass", "wir", "das", "Ziel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.10": {"line.1": {"text": "So viel gekr\u00f6nter Thaten sahn! wir", "tokens": ["So", "viel", "ge\u00b7kr\u00f6n\u00b7ter", "Tha\u00b7ten", "sahn", "!", "wir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VVFIN", "$.", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Wonne trunken: ", "tokens": ["Von", "Won\u00b7ne", "trun\u00b7ken", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Bleibt hinter uns; ihr stolzen Enkel sollt", "tokens": ["Bleibt", "hin\u00b7ter", "uns", ";", "ihr", "stol\u00b7zen", "En\u00b7kel", "sollt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "$.", "PPOSAT", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Triumph! so sag\u2019 auch ich,", "tokens": ["Tri\u00b7umph", "!", "so", "sag'", "auch", "ich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "ADV", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Wenn, unter hohen, jubelvollen Zun-", "tokens": ["Wenn", ",", "un\u00b7ter", "ho\u00b7hen", ",", "ju\u00b7bel\u00b7vol\u00b7len", "Zun"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "$,", "APPR", "ADJA", "$,", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein s&#252;sser Ton auch mir gerieth:", "tokens": ["Ein", "s", "&#252;", "sser", "Ton", "auch", "mir", "ge\u00b7rieth", ":"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "ADJA", "NN", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Triumph! ich hab\u2019 ein Lied dem G\u00f6tt-", "tokens": ["Tri\u00b7umph", "!", "ich", "hab'", "ein", "Lied", "dem", "G\u00f6t\u00b7t"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VAFIN", "ART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihm gef\u00e4llt mein Lied.", "tokens": ["Und", "ihm", "ge\u00b7f\u00e4llt", "mein", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}