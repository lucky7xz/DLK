{"textgrid.poem.31241": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ohnvergleichliche Salene/", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ohnvergleichliche Salene/", "tokens": ["Ohn\u00b7ver\u00b7gleich\u00b7li\u00b7che", "Sa\u00b7le\u00b7ne", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "ja/ ich war dein Cawallir/", "tokens": ["ja", "/", "ich", "war", "dein", "Ca\u00b7wal\u00b7lir", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PPER", "VAFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "deine zwo mahl sechzehn Zehne", "tokens": ["dei\u00b7ne", "zwo", "mahl", "sech\u00b7zehn", "Zeh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "CARD", "ADV", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "bl\u00fczzten bl\u00e4ncker al\u00df Porfir.", "tokens": ["bl\u00fczz\u00b7ten", "bl\u00e4n\u00b7cker", "al\u00df", "Por\u00b7fir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KOKOM", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch du weilst an frembden Fl\u00fcssen", "tokens": ["Doch", "du", "weilst", "an", "fremb\u00b7den", "Fl\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und die sch\u00f6ne Zeit verflo\u00df/", "tokens": ["und", "die", "sch\u00f6\u00b7ne", "Zeit", "ver\u00b7flo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "die mit hundret tausend K\u00fcssen", "tokens": ["die", "mit", "hund\u00b7ret", "tau\u00b7send", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "VVFIN", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "mich in deine Arme schlo\u00df.", "tokens": ["mich", "in", "dei\u00b7ne", "Ar\u00b7me", "schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dorilillgen/ r\u00fchr die Sch\u00e4nckel/", "tokens": ["Do\u00b7ri\u00b7lill\u00b7gen", "/", "r\u00fchr", "die", "Sch\u00e4n\u00b7ckel", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "denn da\u00df ist der Welt ihr Lauff/", "tokens": ["denn", "da\u00df", "ist", "der", "Welt", "ihr", "Lauff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "lupff dein R\u00f6kkgen/ zeig die \u00c4nckel/", "tokens": ["lupff", "dein", "R\u00f6kk\u00b7gen", "/", "zeig", "die", "\u00c4n\u00b7ckel", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "dantz mir einen Pollschen auff!", "tokens": ["dantz", "mir", "ei\u00b7nen", "Poll\u00b7schen", "auff", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "S\u00e4lbst die weissesten Zeitlosen", "tokens": ["S\u00e4lbst", "die", "weis\u00b7ses\u00b7ten", "Zeit\u00b7lo\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "sind nicht halb so wei\u00df wie du/", "tokens": ["sind", "nicht", "halb", "so", "wei\u00df", "wie", "du", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "ADV", "VVFIN", "KOKOM", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "wirffstu mir au\u00df nichts al\u00df Rohsen", "tokens": ["wirffs\u00b7tu", "mir", "au\u00df", "nichts", "al\u00df", "Roh\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PIS", "KOKOM", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "lautter lihbe Blikker zu.", "tokens": ["laut\u00b7ter", "lih\u00b7be", "Blik\u00b7ker", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6ner bistu fast al\u00df ", "tokens": ["Sch\u00f6\u00b7ner", "bis\u00b7tu", "fast", "al\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "das zertheilt mir alle Pein;", "tokens": ["das", "zer\u00b7theilt", "mir", "al\u00b7le", "Pein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "komb/ du kleine ", "tokens": ["komb", "/", "du", "klei\u00b7ne"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVFIN", "$(", "PPER", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "sizz mir in den Schoo\u00df hinein!", "tokens": ["sizz", "mir", "in", "den", "Schoo\u00df", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dein mit Roht gemischt Erblassen", "tokens": ["Dein", "mit", "Roht", "ge\u00b7mischt", "Er\u00b7blas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "APPR", "NN", "VVPP", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "macht mir tausend s\u00fcsse Lust/", "tokens": ["macht", "mir", "tau\u00b7send", "s\u00fcs\u00b7se", "Lust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "CARD", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "meine Finger rund \u00fcmbfassen", "tokens": ["mei\u00b7ne", "Fin\u00b7ger", "rund", "\u00fcmb\u00b7fas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "deine duppel Silber-Brust.", "tokens": ["dei\u00b7ne", "dup\u00b7pel", "Sil\u00b7ber\u00b7Brust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Blau durchs Fenster blinckt der Flihder/", "tokens": ["Blau", "durchs", "Fens\u00b7ter", "blinckt", "der", "Flih\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "nein/ ich la\u00df dir keine Ruh;", "tokens": ["nein", "/", "ich", "la\u00df", "dir", "kei\u00b7ne", "Ruh", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PPER", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "trukk mich/ Kind/ ich trukk dich wihder/", "tokens": ["trukk", "mich", "/", "Kind", "/", "ich", "trukk", "dich", "wih\u00b7der", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "NN", "$(", "PPER", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "denn ich b\u00fcn ein M\u00e4ntsch wie du!", "tokens": ["denn", "ich", "b\u00fcn", "ein", "M\u00e4ntsch", "wie", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "KOKOM", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zwar dein sch\u00f6nstes Belvedere", "tokens": ["Zwar", "dein", "sch\u00f6ns\u00b7tes", "Bel\u00b7ve\u00b7de\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "h\u00e4ltstu listig noch verst\u00e4kkt;", "tokens": ["h\u00e4lts\u00b7tu", "lis\u00b7tig", "noch", "ver\u00b7st\u00e4kkt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "da\u00df ich nichts nicht mehr entpehre/", "tokens": ["da\u00df", "ich", "nichts", "nicht", "mehr", "ent\u00b7peh\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "PTKNEG", "ADV", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "halt es nicht mehr zubed\u00e4kkt!", "tokens": ["halt", "es", "nicht", "mehr", "zu\u00b7be\u00b7d\u00e4kkt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ohnvergleichliche Salene/", "tokens": ["Ohn\u00b7ver\u00b7gleich\u00b7li\u00b7che", "Sa\u00b7le\u00b7ne", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "dihsen Ku\u00df hihr auff dein Wohl!", "tokens": ["dih\u00b7sen", "Ku\u00df", "hihr", "auff", "dein", "Wohl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine zwo mal sechzehn Zehne", "tokens": ["Dei\u00b7ne", "zwo", "mal", "sech\u00b7zehn", "Zeh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "CARD", "ADV", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "sind von innen durchau\u00df hohl!", "tokens": ["sind", "von", "in\u00b7nen", "durch\u00b7au\u00df", "hohl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nichts wie Nasen sih mich drehen", "tokens": ["Nichts", "wie", "Na\u00b7sen", "sih", "mich", "dre\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "KOKOM", "NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "dir f\u00fcr dihsem Rohsen-Dhor \u2013", "tokens": ["dir", "f\u00fcr", "dih\u00b7sem", "Roh\u00b7sen\u00b7\u00b7D\u00b7hor", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PDAT", "NN", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "die ", "tokens": ["die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "hau ich auff ihr ", "tokens": ["hau", "ich", "auff", "ihr"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPER", "APPR", "PPOSAT"], "meter": "+-+-", "measure": "trochaic.di"}}}}}