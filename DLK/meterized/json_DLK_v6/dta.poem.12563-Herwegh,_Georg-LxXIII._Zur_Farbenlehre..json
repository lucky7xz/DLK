{"dta.poem.12563": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "LxXIII.  \n  Zur Farbenlehre.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Pocht nicht auf Eurer Lehre Reinheit!", "tokens": ["Pocht", "nicht", "auf", "Eu\u00b7rer", "Leh\u00b7re", "Rein\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denn, w\u00e4r' der F\u00fcrst des Staates Einheit", "tokens": ["Denn", ",", "w\u00e4r'", "der", "F\u00fcrst", "des", "Staa\u00b7tes", "Ein\u00b7heit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "VAFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und Wei\u00df und Schwarz der Staat, o schau:", "tokens": ["Und", "Wei\u00df", "und", "Schwarz", "der", "Staat", ",", "o", "schau", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NE", "ART", "NN", "$,", "FM", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da w\u00e4r' der F\u00fcrst \u2014 bedenklich grau.", "tokens": ["Da", "w\u00e4r'", "der", "F\u00fcrst", "be\u00b7denk\u00b7lich", "grau", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$(", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}