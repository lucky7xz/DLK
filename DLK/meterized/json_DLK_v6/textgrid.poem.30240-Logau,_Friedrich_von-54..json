{"textgrid.poem.30240": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "54.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Phyllis schickte Thyrsis zu durch ein Brieflein einen Ku\u00df;", "tokens": ["Phyl\u00b7lis", "schick\u00b7te", "Thyr\u00b7sis", "zu", "durch", "ein", "Brie\u00b7flein", "ei\u00b7nen", "Ku\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "PTKZU", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Unter Wegens ward er kalt, bracht ihm so nicht viel Genu\u00df;", "tokens": ["Un\u00b7ter", "We\u00b7gens", "ward", "er", "kalt", ",", "bracht", "ihm", "so", "nicht", "viel", "Ge\u00b7nu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADJD", "$,", "VVFIN", "PPER", "ADV", "PTKNEG", "PIAT", "NN", "$."], "meter": "+-+-+-++-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.3": {"text": "Drum so schrieb er, wann sie wolte, solte sie zwar schrifftlich gr\u00fcssen,", "tokens": ["Drum", "so", "schrieb", "er", ",", "wann", "sie", "wol\u00b7te", ",", "sol\u00b7te", "sie", "zwar", "schrifft\u00b7lich", "gr\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$,", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Immer aber selbsten kummen, wann sie wolt, und m\u00fcndlich k\u00fcssen.", "tokens": ["Im\u00b7mer", "a\u00b7ber", "selbs\u00b7ten", "kum\u00b7men", ",", "wann", "sie", "wolt", ",", "und", "m\u00fcnd\u00b7lich", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$,", "PWAV", "PPER", "VMFIN", "$,", "KON", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}