{"textgrid.poem.43316": {"metadata": {"author": {"name": "Gerstenberg, Heinrich Wilhelm von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wild scho\u00df ihr reizend Aug umher,", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wild scho\u00df ihr reizend Aug umher,", "tokens": ["Wild", "scho\u00df", "ihr", "rei\u00b7zend", "Aug", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVPP", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sah den erlegten Raub nicht mehr,", "tokens": ["Sah", "den", "er\u00b7leg\u00b7ten", "Raub", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sah mich nur! ihre Haare flogen", "tokens": ["Sah", "mich", "nur", "!", "ih\u00b7re", "Haa\u00b7re", "flo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um Hals und Stirn und Brust; sie stand", "tokens": ["Um", "Hals", "und", "Stirn", "und", "Brust", ";", "sie", "stand"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUI", "NN", "KON", "NN", "KON", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So ernst, wie Juno, da! in ihrer rechten Hand", "tokens": ["So", "ernst", ",", "wie", "Ju\u00b7no", ",", "da", "!", "in", "ih\u00b7rer", "rech\u00b7ten", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "PWAV", "NN", "$,", "ADV", "$.", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Schwung sie den k\u00fchnen Pfeil; die linke trug den Bogen.", "tokens": ["Schwung", "sie", "den", "k\u00fch\u00b7nen", "Pfeil", ";", "die", "lin\u00b7ke", "trug", "den", "Bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$.", "ART", "ADJA", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "So ists ein Fr\u00fchlingstag, der durch ein W\u00f6lkchen lacht:", "tokens": ["So", "ists", "ein", "Fr\u00fch\u00b7lings\u00b7tag", ",", "der", "durch", "ein", "W\u00f6lk\u00b7chen", "lacht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr Mund, zum Ku\u00df so sanft gemacht,", "tokens": ["Ihr", "Mund", ",", "zum", "Ku\u00df", "so", "sanft", "ge\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPRART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weis nur zu seufzen, nicht zu dr\u00e4uen,", "tokens": ["Weis", "nur", "zu", "seuf\u00b7zen", ",", "nicht", "zu", "dr\u00e4u\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKZU", "VVINF", "$,", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und droht er ja, es pl\u00f6tzlich zu bereuen.", "tokens": ["Und", "droht", "er", "ja", ",", "es", "pl\u00f6tz\u00b7lich", "zu", "be\u00b7reu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nenn auf der Welt mir eine Lust:", "tokens": ["Nenn", "auf", "der", "Welt", "mir", "ei\u00b7ne", "Lust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch K\u00fcsse zaubr' ich sie in eine sch\u00f6ne Brust.", "tokens": ["Durch", "K\u00fcs\u00b7se", "zau\u00b7br'", "ich", "sie", "in", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+----+-+-+-+", "measure": "dactylic.init"}}}}}