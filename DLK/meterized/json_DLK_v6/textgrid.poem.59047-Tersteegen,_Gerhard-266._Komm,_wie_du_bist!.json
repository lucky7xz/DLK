{"textgrid.poem.59047": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "266. Komm, wie du bist!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was willst du lang dich selbst besehen?", "tokens": ["Was", "willst", "du", "lang", "dich", "selbst", "be\u00b7se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADJD", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gott wei\u00df f\u00fcr alle Wunden Rat.", "tokens": ["Gott", "wei\u00df", "f\u00fcr", "al\u00b7le", "Wun\u00b7den", "Rat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Willst du noch erst ans Rechnen gehen?", "tokens": ["Willst", "du", "noch", "erst", "ans", "Rech\u00b7nen", "ge\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du suchst ja nichts als pure Gnad'.", "tokens": ["Du", "suchst", "ja", "nichts", "als", "pu\u00b7re", "Gnad'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}