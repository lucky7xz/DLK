{"textgrid.poem.58943": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "162. Bald ist's getan", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Wesen f\u00fcr den Schein erw\u00e4hle,", "tokens": ["Das", "We\u00b7sen", "f\u00fcr", "den", "Schein", "er\u00b7w\u00e4h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die kurzen Tag' und Stunden z\u00e4hle,", "tokens": ["Die", "kur\u00b7zen", "Tag'", "und", "Stun\u00b7den", "z\u00e4h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zur Ewigkeit richt deinen Sinn:", "tokens": ["Zur", "E\u00b7wig\u00b7keit", "richt", "dei\u00b7nen", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bald, bald ist deine Zeit dahin!", "tokens": ["Bald", ",", "bald", "ist", "dei\u00b7ne", "Zeit", "da\u00b7hin", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}