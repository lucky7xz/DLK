{"textgrid.poem.67887": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "12. Wettstreit des Fr\u00fchlings", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Vater aller Lieblichkeit,", "tokens": ["Du", "Va\u00b7ter", "al\u00b7ler", "Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Fr\u00fchling, Kleinod unsrer Jahre,", "tokens": ["O", "Fr\u00fch\u00b7ling", ",", "Klei\u00b7nod", "uns\u00b7rer", "Jah\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bestreu die Erde weit und breit", "tokens": ["Be\u00b7streu", "die", "Er\u00b7de", "weit", "und", "breit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit deiner sch\u00f6nsten Blumenwaare.", "tokens": ["Mit", "dei\u00b7ner", "sch\u00f6ns\u00b7ten", "Blu\u00b7men\u00b7waa\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df deiner bunten V\u00f6gelschaar", "tokens": ["La\u00df", "dei\u00b7ner", "bun\u00b7ten", "V\u00f6\u00b7gel\u00b7schaar"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Welt mit tausend Liedern gr\u00fcssen:", "tokens": ["Die", "Welt", "mit", "tau\u00b7send", "Lie\u00b7dern", "gr\u00fcs\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df deine Sonne noch so klar", "tokens": ["La\u00df", "dei\u00b7ne", "Son\u00b7ne", "noch", "so", "klar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die angenehme Stralen schiessen.", "tokens": ["Die", "an\u00b7ge\u00b7neh\u00b7me", "Stra\u00b7len", "schies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du bist darum das Sch\u00f6nste nicht:", "tokens": ["Du", "bist", "da\u00b7rum", "das", "Sch\u00f6ns\u00b7te", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Denn all dein Glanz ist hier verdunkelt,", "tokens": ["Denn", "all", "dein", "Glanz", "ist", "hier", "ver\u00b7dun\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo mir Rosettens Angesicht", "tokens": ["Wo", "mir", "Ro\u00b7set\u00b7tens", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "NE", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Weit \u00fcber deine Sonne funkelt.", "tokens": ["Weit", "\u00fc\u00b7ber", "dei\u00b7ne", "Son\u00b7ne", "fun\u00b7kelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wenn sich ihrer Stimme Schall", "tokens": ["Und", "wenn", "sich", "ih\u00b7rer", "Stim\u00b7me", "Schall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PRF", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu einem Liede will bequemen;", "tokens": ["Zu", "ei\u00b7nem", "Lie\u00b7de", "will", "be\u00b7que\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So schweiget deine Nachtigal", "tokens": ["So", "schwei\u00b7get", "dei\u00b7ne", "Nach\u00b7ti\u00b7gal"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mu\u00df sich aller K\u00fcnste sch\u00e4men.", "tokens": ["Und", "mu\u00df", "sich", "al\u00b7ler", "K\u00fcns\u00b7te", "sch\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Ros' auf deren Lieblichkeit", "tokens": ["Die", "Ros'", "auf", "de\u00b7ren", "Lieb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PRELAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du doch am meisten pflegst zu prangen,", "tokens": ["Du", "doch", "am", "meis\u00b7ten", "pflegst", "zu", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKA", "PIS", "VVFIN", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist bleich und welk und stehet weit", "tokens": ["Ist", "bleich", "und", "welk", "und", "ste\u00b7het", "weit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom frischen Leben ihrer Wangen.", "tokens": ["Vom", "fri\u00b7schen", "Le\u00b7ben", "ih\u00b7rer", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du hast kein Bild, das zeigen kann,", "tokens": ["Du", "hast", "kein", "Bild", ",", "das", "zei\u00b7gen", "kann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PDS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was mich zu ihrer Liebe treibet:", "tokens": ["Was", "mich", "zu", "ih\u00b7rer", "Lie\u00b7be", "trei\u00b7bet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Weil alles bei dir um und an", "tokens": ["Weil", "al\u00b7les", "bei", "dir", "um", "und", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "PPER", "APPR", "KON", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur irrdisch ist und Geistlos bleibet.", "tokens": ["Nur", "irr\u00b7disch", "ist", "und", "Geist\u00b7los", "blei\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ihr Geist, der Tugend lichter Schein,", "tokens": ["Ihr", "Geist", ",", "der", "Tu\u00b7gend", "lich\u00b7ter", "Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der sich in Thun und Reden weiset,", "tokens": ["Der", "sich", "in", "Thun", "und", "Re\u00b7den", "wei\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bezeuget, da\u00df an ihr allein", "tokens": ["Be\u00b7zeu\u00b7get", ",", "da\u00df", "an", "ihr", "al\u00b7lein"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "APPR", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Himmel seine Gaben preiset.", "tokens": ["Der", "Him\u00b7mel", "sei\u00b7ne", "Ga\u00b7ben", "prei\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}