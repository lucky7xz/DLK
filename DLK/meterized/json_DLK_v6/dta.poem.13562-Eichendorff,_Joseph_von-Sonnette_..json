{"dta.poem.13562": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnette .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00dfschauernd dehnt der Geist die gro\u00dfen Fl\u00fcgel,", "tokens": ["S\u00fc\u00df\u00b7schau\u00b7ernd", "dehnt", "der", "Geist", "die", "gro\u00b7\u00dfen", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es gl\u00e4nzt das Meer \u2014 die muth'gen Schiffe fahren,", "tokens": ["Es", "gl\u00e4nzt", "das", "Meer", "die", "muth'\u00b7gen", "Schif\u00b7fe", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da ist nichts mehr, was ihm nicht sollte gl\u00fccken!", "tokens": ["Da", "ist", "nichts", "mehr", ",", "was", "ihm", "nicht", "soll\u00b7te", "gl\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "$,", "PWS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}