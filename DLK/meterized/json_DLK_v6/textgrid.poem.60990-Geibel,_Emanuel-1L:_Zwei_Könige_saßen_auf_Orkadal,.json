{"textgrid.poem.60990": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zwei K\u00f6nige sa\u00dfen auf Orkadal,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei K\u00f6nige sa\u00dfen auf Orkadal,", "tokens": ["Zwei", "K\u00f6\u00b7ni\u00b7ge", "sa\u00b7\u00dfen", "auf", "Or\u00b7ka\u00b7dal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Hell flammten die Kerzen im Pfeilersaal.", "tokens": ["Hell", "flamm\u00b7ten", "die", "Ker\u00b7zen", "im", "Pfei\u00b7ler\u00b7saal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Die Harfner sangen, es perlte der Wein,", "tokens": ["Die", "Harf\u00b7ner", "san\u00b7gen", ",", "es", "perl\u00b7te", "der", "Wein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die K\u00f6nige schauten finster drein.", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7ge", "schau\u00b7ten", "fins\u00b7ter", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Da sprach der eine: \u00bbGib mir die Dirn'!", "tokens": ["Da", "sprach", "der", "ei\u00b7ne", ":", "\u00bb", "Gib", "mir", "die", "Dirn'", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ART", "$.", "$(", "VVIMP", "PPER", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ihr Aug' ist blau, schneewei\u00df ihre Stirn.\u00ab", "tokens": ["Ihr", "Aug'", "ist", "blau", ",", "schnee\u00b7wei\u00df", "ih\u00b7re", "Stirn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Der andre versetzte in grimmem Zorn:", "tokens": ["Der", "and\u00b7re", "ver\u00b7setz\u00b7te", "in", "grim\u00b7mem", "Zorn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "\u00bbmein ist sie und bleibt sie, ich hab's geschworn.\u00ab", "tokens": ["\u00bb", "mein", "ist", "sie", "und", "bleibt", "sie", ",", "ich", "hab's", "ge\u00b7schworn", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "VAFIN", "PPER", "KON", "VVFIN", "PPER", "$,", "PPER", "NE", "NE", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Kein Wort mehr sprachen die K\u00f6nige drauf,", "tokens": ["Kein", "Wort", "mehr", "spra\u00b7chen", "die", "K\u00f6\u00b7ni\u00b7ge", "drauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie nahmen die Schwerter und stunden auf.", "tokens": ["Sie", "nah\u00b7men", "die", "Schwer\u00b7ter", "und", "stun\u00b7den", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Sie schritten herf\u00fcr aus der leuchtenden Hall';", "tokens": ["Sie", "schrit\u00b7ten", "her\u00b7f\u00fcr", "aus", "der", "leuch\u00b7ten\u00b7den", "Hall'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Tief lag der Schnee an des Schlosses Wall.", "tokens": ["Tief", "lag", "der", "Schnee", "an", "des", "Schlos\u00b7ses", "Wall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "NE", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Es spr\u00fchten die Fackeln, es blitzte der Stahl -", "tokens": ["Es", "spr\u00fch\u00b7ten", "die", "Fa\u00b7ckeln", ",", "es", "blitz\u00b7te", "der", "Stahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Zwei K\u00f6nige sanken auf Orkadal.", "tokens": ["Zwei", "K\u00f6\u00b7ni\u00b7ge", "san\u00b7ken", "auf", "Or\u00b7ka\u00b7dal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}