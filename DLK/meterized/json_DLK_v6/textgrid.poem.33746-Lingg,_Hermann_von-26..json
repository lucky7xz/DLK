{"textgrid.poem.33746": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Freiheit schien f\u00fcr Korsika verloren,", "tokens": ["Die", "Frei\u00b7heit", "schien", "f\u00fcr", "Kor\u00b7si\u00b7ka", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da pl\u00f6tzlich stieg ihr Held ", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "stieg", "ihr", "Held"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zum Volk aus Schluchten des Gebirgs hernieder", "tokens": ["Zum", "Volk", "aus", "Schluch\u00b7ten", "des", "Ge\u00b7birgs", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und hat zum Kampf die J\u00fcnglinge beschworen.", "tokens": ["Und", "hat", "zum", "Kampf", "die", "J\u00fcng\u00b7lin\u00b7ge", "be\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Umsonst! die alte Kampflust war vergoren,", "tokens": ["Um\u00b7sonst", "!", "die", "al\u00b7te", "Kampf\u00b7lust", "war", "ver\u00b7go\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verschollen war der Klang der Rachelieder;", "tokens": ["Ver\u00b7schol\u00b7len", "war", "der", "Klang", "der", "Ra\u00b7che\u00b7lie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da barg er, wie ein Aar in sein Gefieder,", "tokens": ["Da", "barg", "er", ",", "wie", "ein", "Aar", "in", "sein", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Felsen sich, fern von der St\u00e4dte Toren.", "tokens": ["In", "Fel\u00b7sen", "sich", ",", "fern", "von", "der", "St\u00e4d\u00b7te", "To\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "$,", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Nur selten sah man ihn noch da und dorten,", "tokens": ["Nur", "sel\u00b7ten", "sah", "man", "ihn", "noch", "da", "und", "dor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PIS", "PPER", "ADV", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und einst erschlug er einen Feind und zeigte", "tokens": ["Und", "einst", "er\u00b7schlug", "er", "ei\u00b7nen", "Feind", "und", "zeig\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf dessen Pferd sich und mit Racheworten.", "tokens": ["Auf", "des\u00b7sen", "Pferd", "sich", "und", "mit", "Ra\u00b7che\u00b7wor\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PRF", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ermordet lag er, als der Tag sich neigte,", "tokens": ["Er\u00b7mor\u00b7det", "lag", "er", ",", "als", "der", "Tag", "sich", "neig\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und man begrub ihn, statt in Kirchhofpforten,", "tokens": ["Und", "man", "be\u00b7grub", "ihn", ",", "statt", "in", "Kirch\u00b7hof\u00b7pfor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "$,", "KOUI", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo Dickicht wilder Rosen sich verzweigte.", "tokens": ["Wo", "Di\u00b7ckicht", "wil\u00b7der", "Ro\u00b7sen", "sich", "ver\u00b7zweig\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}