{"textgrid.poem.58335": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "373. Gott allein dein Ziel mu\u00df sein!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Eigenheit sucht immer 'was f\u00fcr sich,", "tokens": ["Die", "Ei\u00b7gen\u00b7heit", "sucht", "im\u00b7mer", "'was", "f\u00fcr", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "APPR", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie fragt nach Gott und meint nur seine Gaben;", "tokens": ["Sie", "fragt", "nach", "Gott", "und", "meint", "nur", "sei\u00b7ne", "Ga\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer Gott nur meint in allem lauterlich,", "tokens": ["Wer", "Gott", "nur", "meint", "in", "al\u00b7lem", "lau\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADV", "VVFIN", "APPR", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der kann Gott stets, auch in der D\u00fcrre, haben.", "tokens": ["Der", "kann", "Gott", "stets", ",", "auch", "in", "der", "D\u00fcr\u00b7re", ",", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VMFIN", "NN", "ADV", "$,", "ADV", "APPR", "ART", "NN", "$,", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}