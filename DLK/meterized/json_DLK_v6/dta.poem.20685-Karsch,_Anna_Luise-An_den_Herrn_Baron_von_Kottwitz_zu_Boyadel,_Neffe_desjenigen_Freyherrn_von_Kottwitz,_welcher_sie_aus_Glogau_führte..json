{"dta.poem.20685": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  den Herrn Baron  \n von Kottwitz zu Boyadel,  \n Neffe desjenigen Freyherrn von Kottwitz, welcher  \n sie aus Glogau f\u00fchrte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kammerherr des K\u00f6niges,                 ", "tokens": ["Kam\u00b7mer\u00b7herr", "des", "K\u00f6\u00b7ni\u00b7ges", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Enkel tapferer Baronen!", "tokens": ["En\u00b7kel", "tap\u00b7fe\u00b7rer", "Ba\u00b7ro\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Du verlangst nur weniges", "tokens": ["Du", "ver\u00b7langst", "nur", "we\u00b7ni\u00b7ges"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von der Muse, die mit Kronen", "tokens": ["Von", "der", "Mu\u00b7se", ",", "die", "mit", "Kro\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Des Gesanges, m\u00f6chte lohnen", "tokens": ["Des", "Ge\u00b7san\u00b7ges", ",", "m\u00f6ch\u00b7te", "loh\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dich, Du Folger des Barons,", "tokens": ["Dich", ",", "Du", "Fol\u00b7ger", "des", "Ba\u00b7rons", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der mich aus dem Staube g\u00fctig", "tokens": ["Der", "mich", "aus", "dem", "Stau\u00b7be", "g\u00fc\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "ART", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hob, und schnell und edelm\u00fcthig", "tokens": ["Hob", ",", "und", "schnell", "und", "e\u00b7del\u00b7m\u00fct\u00b7hig"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KON", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Zog zur gro\u00dfen Stadt des Throns.", "tokens": ["Zog", "zur", "gro\u00b7\u00dfen", "Stadt", "des", "Throns", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Viel, sehr viel hab ich zu sagen;", "tokens": ["Viel", ",", "sehr", "viel", "hab", "ich", "zu", "sa\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ADV", "VAFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Aber bald, bald schwingst Du Dich", "tokens": ["A\u00b7ber", "bald", ",", "bald", "schwingst", "Du", "Dich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "ADV", "VVFIN", "PPER", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Auf den raschen Reisewagen,", "tokens": ["Auf", "den", "ra\u00b7schen", "Rei\u00b7se\u00b7wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Hinzueilen, wo man Dich", "tokens": ["Hin\u00b7zu\u00b7ei\u00b7len", ",", "wo", "man", "Dich"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PIS", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Ganze Tage lang wird fragen:", "tokens": ["Gan\u00b7ze", "Ta\u00b7ge", "lang", "wird", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Was Du zu Berlin geh\u00f6rt,", "tokens": ["Was", "Du", "zu", "Ber\u00b7lin", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.16": {"text": "Und gesehn mit dem Begleiter,", "tokens": ["Und", "ge\u00b7sehn", "mit", "dem", "Be\u00b7glei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Dessen Liebe Dich verehrt,", "tokens": ["Des\u00b7sen", "Lie\u00b7be", "Dich", "ver\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Der Dich \u00fcbers Meer und weiter", "tokens": ["Der", "Dich", "\u00fc\u00b7bers", "Meer", "und", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPRART", "NN", "KON", "ADV"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.19": {"text": "Gern begleitete, wenn nicht", "tokens": ["Gern", "be\u00b7glei\u00b7te\u00b7te", ",", "wenn", "nicht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Dich und Ihn so manche Pflicht", "tokens": ["Dich", "und", "Ihn", "so", "man\u00b7che", "Pflicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "KON", "PPER", "ADV", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Hinderten am Reisewillen", "tokens": ["Hin\u00b7der\u00b7ten", "am", "Rei\u00b7se\u00b7wil\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.22": {"text": "Und am Vorsatz, hier zu seyn", "tokens": ["Und", "am", "Vor\u00b7satz", ",", "hier", "zu", "seyn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "$,", "ADV", "PTKZU", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "L\u00e4nger noch als wenig Tage,", "tokens": ["L\u00e4n\u00b7ger", "noch", "als", "we\u00b7nig", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KOKOM", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Wo ich k\u00fchn zu hoffen wage,", "tokens": ["Wo", "ich", "k\u00fchn", "zu", "hof\u00b7fen", "wa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Da\u00df wir uns bei \u00e4chterm Wein", "tokens": ["Da\u00df", "wir", "uns", "bei", "\u00e4ch\u00b7term", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Und ganz unter uns allein", "tokens": ["Und", "ganz", "un\u00b7ter", "uns", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Herzlich freun.", "tokens": ["Herz\u00b7lich", "freun", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Denn der Wein war eine L\u00fcgen,", "tokens": ["Denn", "der", "Wein", "war", "ei\u00b7ne", "L\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War gekeltert nicht am Rhein,", "tokens": ["War", "ge\u00b7kel\u00b7tert", "nicht", "am", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "PTKNEG", "APPRART", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schw\u00f6re Dir\u2019s bei dem Vergn\u00fcgen,", "tokens": ["Schw\u00f6\u00b7re", "Dir's", "bei", "dem", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Bey der Ehre, die Du mir", "tokens": ["Bey", "der", "Eh\u00b7re", ",", "die", "Du", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zugedacht, da\u00df ich mit Dir", "tokens": ["Zu\u00b7ge\u00b7dacht", ",", "da\u00df", "ich", "mit", "Dir"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "PPER", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mit Conrad speisen sollte ", "tokens": ["Und", "mit", "Con\u00b7rad", "spei\u00b7sen", "soll\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "VVINF", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber wenn ich feuerhei\u00df", "tokens": ["A\u00b7ber", "wenn", "ich", "feu\u00b7er\u00b7hei\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mich der Ehre freuen sollte,", "tokens": ["Mich", "der", "Eh\u00b7re", "freu\u00b7en", "soll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Muste kein so gro\u00dfer Krei\u00df", "tokens": ["Mus\u00b7te", "kein", "so", "gro\u00b7\u00dfer", "Krei\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIAT", "ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Horchen, was ich schwatzen wollte,", "tokens": ["Hor\u00b7chen", ",", "was", "ich", "schwat\u00b7zen", "woll\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Was ich leicht zu reimen wei\u00df.", "tokens": ["Was", "ich", "leicht", "zu", "rei\u00b7men", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Darum hoff ich, Musenvetter!", "tokens": ["Da\u00b7rum", "hoff", "ich", ",", "Mu\u00b7sen\u00b7vet\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Da\u00df wir Drei beisammen nur", "tokens": ["Da\u00df", "wir", "Drei", "bei\u00b7sam\u00b7men", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "CARD", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Flammen bei dem kalten Wetter,", "tokens": ["Flam\u00b7men", "bei", "dem", "kal\u00b7ten", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Bei des Schneegest\u00f6bers Spur,", "tokens": ["Bei", "des", "Schnee\u00b7ge\u00b7st\u00f6\u00b7bers", "Spur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Welche k\u00e4rglich Deine Flur", "tokens": ["Wel\u00b7che", "k\u00e4rg\u00b7lich", "Dei\u00b7ne", "Flur"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ADJD", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Deckt, so da\u00df die wollnen Heerden", "tokens": ["Deckt", ",", "so", "da\u00df", "die", "woll\u00b7nen", "Heer\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADV", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Saaten schmausen werden", "tokens": ["Saa\u00b7ten", "schmau\u00b7sen", "wer\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVINF", "VAFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "Auf dem Tische der Natur.", "tokens": ["Auf", "dem", "Ti\u00b7sche", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}