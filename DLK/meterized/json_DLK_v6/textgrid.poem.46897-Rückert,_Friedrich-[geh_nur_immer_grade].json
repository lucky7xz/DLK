{"textgrid.poem.46897": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[geh nur immer grade]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geh nur immer grade", "tokens": ["Geh", "nur", "im\u00b7mer", "gra\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "ADV", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Fort auf deinem Pfade!", "tokens": ["Fort", "auf", "dei\u00b7nem", "Pfa\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Nur nicht so mit schwerem", "tokens": ["Nur", "nicht", "so", "mit", "schwe\u00b7rem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "ADV", "APPR", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "B\u00fcndel dich belade!", "tokens": ["B\u00fcn\u00b7del", "dich", "be\u00b7la\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ob dich Sonne senge,", "tokens": ["Ob", "dich", "Son\u00b7ne", "sen\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Oder Thau dich bade;", "tokens": ["O\u00b7der", "Thau", "dich", "ba\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Geh nur immer grade", "tokens": ["Geh", "nur", "im\u00b7mer", "gra\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "ADV", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Fort auf deinem Pfade!", "tokens": ["Fort", "auf", "dei\u00b7nem", "Pfa\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Frage nicht, wie bald man", "tokens": ["Fra\u00b7ge", "nicht", ",", "wie", "bald", "man"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "PWAV", "ADV", "PIS"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Dich zur Einkehr lade,", "tokens": ["Dich", "zur", "Ein\u00b7kehr", "la\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nicht, wie weit der Strom noch", "tokens": ["Nicht", ",", "wie", "weit", "der", "Strom", "noch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "$,", "PWAV", "ADJD", "ART", "NN", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Hat zum Meergestade!", "tokens": ["Hat", "zum", "Meer\u00b7ge\u00b7sta\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Geh nur immer grade", "tokens": ["Geh", "nur", "im\u00b7mer", "gra\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "ADV", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Fort auf deinen Pfade!", "tokens": ["Fort", "auf", "dei\u00b7nen", "Pfa\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Wenn es l\u00e4nger w\u00e4hret,", "tokens": ["Wenn", "es", "l\u00e4n\u00b7ger", "w\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ist es eine Gnade;", "tokens": ["Ist", "es", "ei\u00b7ne", "Gna\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn es schneller endet,", "tokens": ["Wenn", "es", "schnel\u00b7ler", "en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ist es auch kein Schade:", "tokens": ["Ist", "es", "auch", "kein", "Scha\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Geh nur immer grade", "tokens": ["Geh", "nur", "im\u00b7mer", "gra\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "ADV", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Fort auf deinem Pfade!", "tokens": ["Fort", "auf", "dei\u00b7nem", "Pfa\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}