{"dta.poem.15743": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Poeten, lasset uns treulich zusammen halten!", "tokens": ["Po\u00b7et\u00b7en", ",", "las\u00b7set", "uns", "treu\u00b7lich", "zu\u00b7sam\u00b7men", "hal\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Erk\u00e4lten d\u00fcrf' uns nicht die Welt, noch selbst erkalten.", "tokens": ["Er\u00b7k\u00e4l\u00b7ten", "d\u00fcr\u00b7f'", "uns", "nicht", "die", "Welt", ",", "noch", "selbst", "er\u00b7kal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "$,", "ADV", "ADV", "VVINF", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Haucht aus euch nur die Glut, die Gott in euch gehaucht,", "tokens": ["Haucht", "aus", "euch", "nur", "die", "Glut", ",", "die", "Gott", "in", "euch", "ge\u00b7haucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "ART", "NN", "$,", "ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bleibet wohlgemuth, wenn drau\u00dfen sie verraucht.", "tokens": ["Und", "blei\u00b7bet", "wohl\u00b7ge\u00b7muth", ",", "wenn", "drau\u00b7\u00dfen", "sie", "ver\u00b7raucht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "KOUS", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wer gr\u00f6\u00dfer, kleiner sei, das lasset uns nicht streiten;", "tokens": ["Wer", "gr\u00f6\u00b7\u00dfer", ",", "klei\u00b7ner", "sei", ",", "das", "las\u00b7set", "uns", "nicht", "strei\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "$,", "ADJD", "VAFIN", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Uns richtet diese Zeit, sie richten k\u00fcnft'ge Zeiten.", "tokens": ["Uns", "rich\u00b7tet", "die\u00b7se", "Zeit", ",", "sie", "rich\u00b7ten", "k\u00fcnft'\u00b7ge", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$,", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Gar viel was heute glimmt, wird \u00fcber Nacht verglimmen;", "tokens": ["Gar", "viel", "was", "heu\u00b7te", "glimmt", ",", "wird", "\u00fc\u00b7ber", "Nacht", "ver\u00b7glim\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PWS", "ADV", "VVFIN", "$,", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was nun oben schwimmt, wird fort im Strome schwimmen.", "tokens": ["Und", "was", "nun", "o\u00b7ben", "schwimmt", ",", "wird", "fort", "im", "Stro\u00b7me", "schwim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADV", "VVFIN", "$,", "VAFIN", "PTKVZ", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Was dem das meiste gilt, wird der am meisten schelten,", "tokens": ["Was", "dem", "das", "meis\u00b7te", "gilt", ",", "wird", "der", "am", "meis\u00b7ten", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ART", "PIS", "VVFIN", "$,", "VAFIN", "ART", "APPRART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und drum, was dieser schilt, wird jenem doppelt gelten.", "tokens": ["Und", "drum", ",", "was", "die\u00b7ser", "schilt", ",", "wird", "je\u00b7nem", "dop\u00b7pelt", "gel\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "$,", "PWS", "PDS", "VVFIN", "$,", "VAFIN", "PDAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Gut Werk ist Dichterei, die feine wie die plumpe,", "tokens": ["Gut", "Werk", "ist", "Dich\u00b7te\u00b7rei", ",", "die", "fei\u00b7ne", "wie", "die", "plum\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VAFIN", "NN", "$,", "ART", "ADJA", "KOKOM", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nur Kunstrichterei ist ein Gesch\u00e4ft f\u00fcr Lumpe.", "tokens": ["Und", "nur", "Kuns\u00b7trich\u00b7te\u00b7rei", "ist", "ein", "Ge\u00b7sch\u00e4ft", "f\u00fcr", "Lum\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VAFIN", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}