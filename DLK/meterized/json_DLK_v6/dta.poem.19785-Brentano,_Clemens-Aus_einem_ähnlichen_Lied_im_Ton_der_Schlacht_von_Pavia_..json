{"dta.poem.19785": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Aus einem \u00e4hnlichen Lied im Ton der  \n Schlacht von Pavia .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Frankfurt, die hochgelobte Stadt!               ", "tokens": ["Frank\u00b7furt", ",", "die", "hoch\u00b7ge\u00b7lob\u00b7te", "Stadt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sag mir, wie sie's verdienet hat,", "tokens": ["Sag", "mir", ",", "wie", "sie's", "ver\u00b7die\u00b7net", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PWAV", "PIS", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um F\u00fcrsten und gro\u00df Herren,", "tokens": ["Um", "F\u00fcrs\u00b7ten", "und", "gro\u00df", "Her\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sechs F\u00fcrsten kamen auf eine Zeit,", "tokens": ["Sechs", "F\u00fcrs\u00b7ten", "ka\u00b7men", "auf", "ei\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die wollten sie umkehren.", "tokens": ["Die", "woll\u00b7ten", "sie", "um\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Kaiser Karl, der hielt die Stadt in Hut,", "tokens": ["Kai\u00b7ser", "Karl", ",", "der", "hielt", "die", "Stadt", "in", "Hut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PRELS", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Versammelt da ein Haufen gut,", "tokens": ["Ver\u00b7sam\u00b7melt", "da", "ein", "Hau\u00b7fen", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Reutern und Landsknechten,", "tokens": ["Von", "Reu\u00b7tern", "und", "Lands\u00b7knech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die waren stets ganz wohlgemuth", "tokens": ["Die", "wa\u00b7ren", "stets", "ganz", "wohl\u00b7ge\u00b7muth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit ihm ums Blut zu fechten.", "tokens": ["Mit", "ihm", "ums", "Blut", "zu", "fech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Konrad von Hanstein, dem edlen Held,", "tokens": ["Kon\u00b7rad", "von", "Hans\u00b7tein", ",", "dem", "ed\u00b7len", "Held", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "Dem war die Stadt anheim gestellt", "tokens": ["Dem", "war", "die", "Stadt", "an\u00b7heim", "ge\u00b7stellt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "NE", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu frommen treuen H\u00e4nden,", "tokens": ["Zu", "from\u00b7men", "treu\u00b7en", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der hielt sich wohl; drum alle Welt", "tokens": ["Der", "hielt", "sich", "wohl", ";", "drum", "al\u00b7le", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "$.", "PAV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihn prei\u00dft in allen Landen.", "tokens": ["Ihn", "prei\u00dft", "in", "al\u00b7len", "Lan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die F\u00fcrsten schossen Tag und Nacht,", "tokens": ["Die", "F\u00fcrs\u00b7ten", "schos\u00b7sen", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bewiesen ihre gro\u00dfe Macht,", "tokens": ["Be\u00b7wie\u00b7sen", "ih\u00b7re", "gro\u00b7\u00dfe", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und lie\u00dfen sich nichts dauren,", "tokens": ["Und", "lie\u00b7\u00dfen", "sich", "nichts", "dau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Tauben in ihren H\u00e4uslein klein", "tokens": ["Die", "Tau\u00b7ben", "in", "ih\u00b7ren", "H\u00e4us\u00b7lein", "klein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die mu\u00dften darum trauren.", "tokens": ["Die", "mu\u00df\u00b7ten", "da\u00b7rum", "trau\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PAV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Zu N\u00fcrnberg in der werthen Stadt", "tokens": ["Zu", "N\u00fcrn\u00b7berg", "in", "der", "wert\u00b7hen", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Ocklesmann sein Wohnung hat,", "tokens": ["Ein", "Ock\u00b7les\u00b7mann", "sein", "Woh\u00b7nung", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kann gut Pilullen machen,", "tokens": ["Kann", "gut", "Pi\u00b7lul\u00b7len", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die h\u00f6rt man hie stets fr\u00fch und spat", "tokens": ["Die", "h\u00f6rt", "man", "hie", "stets", "fr\u00fch", "und", "spat"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PIS", "ADV", "ADV", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit gro\u00dfer Macht herkrachen.", "tokens": ["Mit", "gro\u00b7\u00dfer", "Macht", "her\u00b7kra\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Unfall fahr ihm in die H\u00e4nd,", "tokens": ["Der", "Un\u00b7fall", "fahr", "ihm", "in", "die", "H\u00e4nd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schlag den Kopf ihm um die W\u00e4nd,", "tokens": ["Und", "schlag", "den", "Kopf", "ihm", "um", "die", "W\u00e4nd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit seiner gro\u00dfen Taschen!", "tokens": ["Mit", "sei\u00b7ner", "gro\u00b7\u00dfen", "Ta\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich mein, der Marggraf sey ein Mann,", "tokens": ["Ich", "mein", ",", "der", "Marg\u00b7graf", "sey", "ein", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "$,", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der k\u00f6nn ihm daraus naschen.", "tokens": ["Der", "k\u00f6nn", "ihm", "da\u00b7raus", "na\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PAV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Aber Markgraf, wie gefiel es dir?", "tokens": ["A\u00b7ber", "Mark\u00b7graf", ",", "wie", "ge\u00b7fiel", "es", "dir", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "PWAV", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Willst du nicht kommen wieder schier?", "tokens": ["Willst", "du", "nicht", "kom\u00b7men", "wie\u00b7der", "schier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Wein wollen wir dir schenken,", "tokens": ["Den", "Wein", "wol\u00b7len", "wir", "dir", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den Mecklenburg bring auch mit dir,", "tokens": ["Den", "Meck\u00b7len\u00b7burg", "bring", "auch", "mit", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So springen wir \u00fcbrr die B\u00e4nke.", "tokens": ["So", "sprin\u00b7gen", "wir", "\u00fcbrr", "die", "B\u00e4n\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Ein'n ", "tokens": ["Ein'n"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Ein Lanzknecht der ist wohlgemuth,               ", "tokens": ["Ein", "Lanz\u00b7knecht", "der", "ist", "wohl\u00b7ge\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der wollt gern mit dir tanzen.", "tokens": ["Der", "wollt", "gern", "mit", "dir", "tan\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Es ist auch neulich kommen her,", "tokens": ["Es", "ist", "auch", "neu\u00b7lich", "kom\u00b7men", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Thier, das hei\u00dft der leidig ", "tokens": ["Ein", "Thier", ",", "das", "hei\u00dft", "der", "lei\u00b7dig"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PDS", "VVFIN", "ART", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den f\u00fchrt ", "tokens": ["Den", "f\u00fchrt"], "token_info": ["word", "word"], "pos": ["NE", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Die werden dich wohl zwicken,", "tokens": ["Die", "wer\u00b7den", "dich", "wohl", "zwi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Sie haben sich all wohlbedacht,", "tokens": ["Sie", "ha\u00b7ben", "sich", "all", "wohl\u00b7be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "PIAT", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Sack mit Ingwer mit sich bracht,", "tokens": ["Ein", "Sack", "mit", "Ing\u00b7wer", "mit", "sich", "bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Viel Lorbern und Muskaten,", "tokens": ["Viel", "Lor\u00b7bern", "und", "Mus\u00b7ka\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$,"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wann dir darnach der Bauch thut weh,", "tokens": ["Wann", "dir", "dar\u00b7nach", "der", "Bauch", "thut", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PAV", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie k\u00f6nnen ihrer wohl entrathen.", "tokens": ["Sie", "k\u00f6n\u00b7nen", "ih\u00b7rer", "wohl", "ent\u00b7ra\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ich wollt, da\u00df nie dem wohl erging,", "tokens": ["Ich", "wollt", ",", "da\u00df", "nie", "dem", "wohl", "er\u00b7ging", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "ADV", "ART", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Unlust und gro\u00df Krieg anfing,", "tokens": ["Der", "Un\u00b7lust", "und", "gro\u00df", "Krieg", "an\u00b7fing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu verderben St\u00e4dt und Lande,", "tokens": ["Zu", "ver\u00b7der\u00b7ben", "St\u00e4dt", "und", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O Gott, wer r\u00e4cht der Armen Blut?", "tokens": ["O", "Gott", ",", "wer", "r\u00e4cht", "der", "Ar\u00b7men", "Blut", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es steht in deinen Handen.", "tokens": ["Es", "steht", "in", "dei\u00b7nen", "Han\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Man spricht: Arm Leut dr\u00fcckt jedermann,", "tokens": ["Man", "spricht", ":", "Arm", "Leut", "dr\u00fcckt", "je\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$.", "NN", "NN", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das wir dann jetzt vor Augen han,", "tokens": ["Das", "wir", "dann", "jetzt", "vor", "Au\u00b7gen", "han", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Freund will sie erretten,", "tokens": ["Kein", "Freund", "will", "sie", "er\u00b7ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Man schickt eh Pulver und grob 