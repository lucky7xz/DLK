{"textgrid.poem.35203": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "[ihr Hirten/ die ihr klagt und klinget]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Hirten/ die ihr klagt und klinget/", "tokens": ["Ihr", "Hir\u00b7ten", "/", "die", "ihr", "klagt", "und", "klin\u00b7get", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "PPER", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihr den Kriegergreul besinget/", "tokens": ["Die", "ihr", "den", "Krie\u00b7ger\u00b7greul", "be\u00b7sin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr solt nicht also gar verfluchen", "tokens": ["Ihr", "solt", "nicht", "al\u00b7so", "gar", "ver\u00b7flu\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Krieg/ hier unter diesen Buchen.", "tokens": ["Den", "Krieg", "/", "hier", "un\u00b7ter", "die\u00b7sen", "Bu\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Tugend w\u00fcrket auch in Waffen/", "tokens": ["Die", "Tu\u00b7gend", "w\u00fcr\u00b7ket", "auch", "in", "Waf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Waffen mu\u00df man Frieden schaffen.", "tokens": ["Mit", "Waf\u00b7fen", "mu\u00df", "man", "Frie\u00b7den", "schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PIS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Hertz/ das L\u00f6wenmuht bewohnet/", "tokens": ["Ein", "Hertz", "/", "das", "L\u00f6\u00b7wen\u00b7muht", "be\u00b7woh\u00b7net", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein dapfres Hertz/ wird auch belohnet.", "tokens": ["Ein", "dapf\u00b7res", "Hertz", "/", "wird", "auch", "be\u00b7loh\u00b7net", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Es \u00fcberlebt ein Held sein Leben/", "tokens": ["Es", "\u00fc\u00b7ber\u00b7lebt", "ein", "Held", "sein", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Vnd darf im Zelt der Sternen schweben/", "tokens": ["Vnd", "darf", "im", "Zelt", "der", "Ster\u00b7nen", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPRART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Er wandert hoch mit dem Ger\u00fcchte/", "tokens": ["Er", "wan\u00b7dert", "hoch", "mit", "dem", "Ge\u00b7r\u00fcch\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Sein Ruhm wird nimmer nicht zu nichte.", "tokens": ["Sein", "Ruhm", "wird", "nim\u00b7mer", "nicht", "zu", "nich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PTKNEG", "PTKA", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Kommt/ last/ ihr Hirten/ jetzt die Auen/", "tokens": ["Kommt", "/", "last", "/", "ihr", "Hir\u00b7ten", "/", "jetzt", "die", "Au\u00b7en", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "$(", "PPOSAT", "NN", "$(", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Ich la\u00df euch Lust und Wunder schauen/", "tokens": ["Ich", "la\u00df", "euch", "Lust", "und", "Wun\u00b7der", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Ich/ euer Gott und eurer Heerden:", "tokens": ["Ich", "/", "eu\u00b7er", "Gott", "und", "eu\u00b7rer", "Heer\u00b7den", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Kommt/ dieser soll geh\u00fctet werden/", "tokens": ["Kommt", "/", "die\u00b7ser", "soll", "ge\u00b7h\u00fc\u00b7tet", "wer\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "PDS", "VMFIN", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}