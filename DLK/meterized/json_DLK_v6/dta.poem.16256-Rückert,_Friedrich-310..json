{"dta.poem.16256": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "310.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wol dient ein freier Mann in mehr als einem Feld,", "tokens": ["Wol", "dient", "ein", "frei\u00b7er", "Mann", "in", "mehr", "als", "ei\u00b7nem", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "PIAT", "KOKOM", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Er dient dem Freund, dem Haus, der Stadt, dem Staat, der We", "tokens": ["Er", "dient", "dem", "Freund", ",", "dem", "Haus", ",", "der", "Stadt", ",", "dem", "Staat", ",", "der", "We"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Dienste mancherlei wei\u00df er, die sich verschlingen,", "tokens": ["Die", "Diens\u00b7te", "man\u00b7cher\u00b7lei", "wei\u00df", "er", ",", "die", "sich", "ver\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VVFIN", "PPER", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In weit- und engerm Kreis, in Einklang auch zu bringen.", "tokens": ["In", "weit", "und", "en\u00b7germ", "Kreis", ",", "in", "Ein\u00b7klang", "auch", "zu", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "ADJA", "NN", "$,", "APPR", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es tritt der fernste Dienst dem n\u00e4chsten nicht zu nah,", "tokens": ["Es", "tritt", "der", "ferns\u00b7te", "Dienst", "dem", "n\u00e4chs\u00b7ten", "nicht", "zu", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch auch vor ihm zur\u00fcck, zur Stell' ist jeder da.", "tokens": ["Noch", "auch", "vor", "ihm", "zu\u00b7r\u00fcck", ",", "zur", "Stell'", "ist", "je\u00b7der", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "PTKVZ", "$,", "APPRART", "NN", "VAFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Begl\u00fcckt, wenn jeder Dienst fand, unter der Benennung", "tokens": ["Be\u00b7gl\u00fcckt", ",", "wenn", "je\u00b7der", "Dienst", "fand", ",", "un\u00b7ter", "der", "Be\u00b7nen\u00b7nung"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "PIAT", "NN", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verdienst, verdienten Lohn, verdiente Anerkennung.", "tokens": ["Ver\u00b7dienst", ",", "ver\u00b7dien\u00b7ten", "Lohn", ",", "ver\u00b7dien\u00b7te", "An\u00b7er\u00b7ken\u00b7nung", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wenn er die nicht erdient, hab' er sie nur verdient;", "tokens": ["Wenn", "er", "die", "nicht", "er\u00b7di\u00b7ent", ",", "hab'", "er", "sie", "nur", "ver\u00b7dient", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "PTKNEG", "VVPP", "$,", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+---+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zum Lohn dient dis Gef\u00fchl, und macht den frei, der dient.", "tokens": ["Zum", "Lohn", "dient", "dis", "Ge\u00b7f\u00fchl", ",", "und", "macht", "den", "frei", ",", "der", "dient", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PDS", "NN", "$,", "KON", "VVFIN", "ART", "ADJD", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}