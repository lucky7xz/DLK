{"dta.poem.5667": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von GOTT.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die\u00df ist der grosse Punet und Vorwurff aller Weisen.", "tokens": ["Die\u00df", "ist", "der", "gros\u00b7se", "Pu\u00b7net", "und", "Vor\u00b7wurff", "al\u00b7ler", "Wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "KON", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Man mu\u00df, will man zu Jhm sich in die H\u00f6he ziehn,", "tokens": ["Man", "mu\u00df", ",", "will", "man", "zu", "Jhm", "sich", "in", "die", "H\u00f6\u00b7he", "ziehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "$,", "VMFIN", "PIS", "APPR", "PPER", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erwegt man das Gesch\u00f6pff, darinn den ", "tokens": ["Er\u00b7wegt", "man", "das", "Ge\u00b7sch\u00f6pff", ",", "da\u00b7rinn", "den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PIS", "ART", "NN", "$,", "PAV", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "GoTT zeigt sich \u00fcberall, und alles zeiget Jhn.", "tokens": ["GoTT", "zeigt", "sich", "\u00fc\u00b7be\u00b7rall", ",", "und", "al\u00b7les", "zei\u00b7get", "Jhn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ADV", "$,", "KON", "PIS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man mag den Geist, man mag was c\u00f6rperlichs ergr\u00fcnden;", "tokens": ["Man", "mag", "den", "Geist", ",", "man", "mag", "was", "c\u00f6r\u00b7per\u00b7lichs", "er\u00b7gr\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "$,", "PIS", "VMFIN", "PWS", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}}}}}