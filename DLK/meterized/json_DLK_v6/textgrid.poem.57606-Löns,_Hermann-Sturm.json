{"textgrid.poem.57606": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Sturm", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mondschein auf W\u00e4ldern, dunkel und tief,", "tokens": ["Mond\u00b7schein", "auf", "W\u00e4l\u00b7dern", ",", "dun\u00b7kel", "und", "tief", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wolken vom Nordwind getrieben", "tokens": ["Wol\u00b7ken", "vom", "Nord\u00b7wind", "ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "VVPP"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Wie ein zerrissener Liebesbrief", "tokens": ["Wie", "ein", "zer\u00b7ris\u00b7se\u00b7ner", "Lie\u00b7bes\u00b7brief"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von treuloser Hand einst geschrieben.", "tokens": ["Von", "treu\u00b7lo\u00b7ser", "Hand", "einst", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Gellendes Kreischen im schwarzen Ge\u00e4st,", "tokens": ["Gel\u00b7len\u00b7des", "Krei\u00b7schen", "im", "schwar\u00b7zen", "Ge\u00b7\u00e4st", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Sturmlieder, grausiger Pracht voll,", "tokens": ["Sturm\u00b7lie\u00b7der", ",", "grau\u00b7si\u00b7ger", "Pracht", "voll", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Tobe, Orkan, ich stehe hier fest,", "tokens": ["To\u00b7be", ",", "Or\u00b7kan", ",", "ich", "ste\u00b7he", "hier", "fest", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "PPER", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "F\u00fchle mich sicher und machtvoll.", "tokens": ["F\u00fch\u00b7le", "mich", "si\u00b7cher", "und", "macht\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "KON", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Nie fa\u00dft mich Schwindel auf senkrechtem Fels", "tokens": ["Nie", "fa\u00dft", "mich", "Schwin\u00b7del", "auf", "sen\u00b7krech\u00b7tem", "Fels"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bei deinem Schimpfen und Toben,", "tokens": ["Bei", "dei\u00b7nem", "Schimp\u00b7fen", "und", "To\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "K\u00fchnre Gedanken im Herzen ich w\u00e4lz',", "tokens": ["K\u00fchn\u00b7re", "Ge\u00b7dan\u00b7ken", "im", "Her\u00b7zen", "ich", "w\u00e4lz'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Wild von dem Brausen umstoben.", "tokens": ["Wild", "von", "dem", "Brau\u00b7sen", "um\u00b7sto\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Br\u00fclle und heule nur toll darauf zu,", "tokens": ["Br\u00fcl\u00b7le", "und", "heu\u00b7le", "nur", "toll", "da\u00b7rauf", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "ADV", "ADJD", "PAV", "PTKVZ", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Gut passen beid' wir zusammen,", "tokens": ["Gut", "pas\u00b7sen", "beid'", "wir", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Besser als faulen in lebloser Ruh", "tokens": ["Bes\u00b7ser", "als", "fau\u00b7len", "in", "leb\u00b7lo\u00b7ser", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Ist zu verbrennen in Flammen.", "tokens": ["Ist", "zu", "ver\u00b7bren\u00b7nen", "in", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKZU", "VVINF", "APPR", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}