{"textgrid.poem.52779": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Mode-Ode", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alk\u00e4isch, sapphisch, asklepiad\u00e9isch, traun!", "tokens": ["Al\u00b7k\u00e4\u00b7isch", ",", "sap\u00b7phisch", ",", "as\u00b7kle\u00b7pi\u00b7a\u00b7d\u00e9isch", ",", "traun", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "VVINF", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Ist wied'rum Mode, also versuch's auch ich,", "tokens": ["Ist", "wie\u00b7d'\u00b7rum", "Mo\u00b7de", ",", "al\u00b7so", "ver\u00b7such's", "auch", "ich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "$,", "ADV", "NE", "ADV", "PPER", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Obschon das eitle Publi", "tokens": ["Ob\u00b7schon", "das", "eit\u00b7le", "Pub\u00b7li"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Viele Notiz wohl ", "tokens": ["Vie\u00b7le", "No\u00b7tiz", "wohl"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Indessen d\u00e1rauf kommt es ja ", "tokens": ["In\u00b7des\u00b7sen", "d\u00e1rauf", "kommt", "es", "ja"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PAV", "VVFIN", "PPER", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die eigne Plage ist der Bem\u00fchung Lohn,", "tokens": ["Die", "eig\u00b7ne", "Pla\u00b7ge", "ist", "der", "Be\u00b7m\u00fc\u00b7hung", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "O besser freilich w\u00e4r's, die deutsche", "tokens": ["O", "bes\u00b7ser", "frei\u00b7lich", "w\u00e4r's", ",", "die", "deut\u00b7sche"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJD", "ADV", "VAFIN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sprache nicht also zu radebrechen.", "tokens": ["Spra\u00b7che", "nicht", "al\u00b7so", "zu", "ra\u00b7de\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Erf\u00e4nde Einer ", "tokens": ["Er\u00b7f\u00e4n\u00b7de", "Ei\u00b7ner"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Im Heimathgeiste; tausend der W\u00f6rter ja", "tokens": ["Im", "Hei\u00b7math\u00b7geis\u00b7te", ";", "tau\u00b7send", "der", "W\u00f6r\u00b7ter", "ja"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$.", "CARD", "ART", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In Versen m\u00f6chten eine St\u00e4tte", "tokens": ["In", "Ver\u00b7sen", "m\u00f6ch\u00b7ten", "ei\u00b7ne", "St\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Finden und richtigen Klangs gesprochen.", "tokens": ["Fin\u00b7den", "und", "rich\u00b7ti\u00b7gen", "Klangs", "ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Ich f\u00fchl's, sie betteln, Keiner erbarmt sich doch,", "tokens": ["Ich", "f\u00fchl's", ",", "sie", "bet\u00b7teln", ",", "Kei\u00b7ner", "er\u00b7barmt", "sich", "doch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVINF", "$,", "PIS", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Viel lieber bricht er Arm und Gelenk' entzwei,", "tokens": ["Viel", "lie\u00b7ber", "bricht", "er", "Arm", "und", "Ge\u00b7lenk'", "ent\u00b7zwei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und zwingt in sein Prokrustes", "tokens": ["Und", "zwingt", "in", "sein", "Pro\u00b7krus\u00b7tes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aermsten, ein St\u00fcmper, ein ganz herz-", "tokens": ["A\u00b7erms\u00b7ten", ",", "ein", "St\u00fcm\u00b7per", ",", "ein", "ganz", "her\u00b7z"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ART", "NN", "$,", "ART", "ADV", "TRUNC"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Ich aber huld'ge lieber der Mode auch,", "tokens": ["Ich", "a\u00b7ber", "huld'\u00b7ge", "lie\u00b7ber", "der", "Mo\u00b7de", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Von einflu\u00dfreichen Philologen", "tokens": ["Von", "ein\u00b7flu\u00df\u00b7rei\u00b7chen", "Phi\u00b7lo\u00b7lo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aber vielleicht noch ein Lob erhasche.", "tokens": ["A\u00b7ber", "viel\u00b7leicht", "noch", "ein", "Lob", "er\u00b7ha\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}