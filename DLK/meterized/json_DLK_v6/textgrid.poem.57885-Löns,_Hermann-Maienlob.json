{"textgrid.poem.57885": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Maienlob", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist der Mai, der holde Mai,", "tokens": ["Das", "ist", "der", "Mai", ",", "der", "hol\u00b7de", "Mai", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den alle Welt so liebt,", "tokens": ["Den", "al\u00b7le", "Welt", "so", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Bl\u00fcten uns und Vogelsang", "tokens": ["Der", "Bl\u00fc\u00b7ten", "uns", "und", "Vo\u00b7gel\u00b7sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und frischen Spargel gibt.", "tokens": ["Und", "fri\u00b7schen", "Spar\u00b7gel", "gibt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "In Schnee h\u00fcllt sich der Birnenbaum,", "tokens": ["In", "Schnee", "h\u00fcllt", "sich", "der", "Bir\u00b7nen\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gr\u00fcn wogt die junge Saat,", "tokens": ["Gr\u00fcn", "wogt", "die", "jun\u00b7ge", "Saat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Radieschen a\u00df ich gestern schon,", "tokens": ["Ra\u00b7die\u00b7schen", "a\u00df", "ich", "ge\u00b7stern", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Bald gibt's auch Kopfsalat.", "tokens": ["Bald", "gibt's", "auch", "Kopf\u00b7sa\u00b7lat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie wonnesam die Nachtigall", "tokens": ["Wie", "won\u00b7ne\u00b7sam", "die", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Nachbargarten singt,", "tokens": ["Im", "Nach\u00b7bar\u00b7gar\u00b7ten", "singt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und meine Wirtin mir vom Markt", "tokens": ["Und", "mei\u00b7ne", "Wir\u00b7tin", "mir", "vom", "Markt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den ersten Pfingstlauch bringt.", "tokens": ["Den", "ers\u00b7ten", "Pfingst\u00b7lauch", "bringt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und morgen prangt auf meinem Tisch", "tokens": ["Und", "mor\u00b7gen", "prangt", "auf", "mei\u00b7nem", "Tisch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Spinat mit Spiegelei.", "tokens": ["Spi\u00b7nat", "mit", "Spie\u00b7ge\u00b7lei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Drum singe ich aus voller Brust:", "tokens": ["Drum", "sin\u00b7ge", "ich", "aus", "vol\u00b7ler", "Brust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sch\u00f6n ist doch der Mai!", "tokens": ["Wie", "sch\u00f6n", "ist", "doch", "der", "Mai", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}