{"dta.poem.13730": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Nachtfeier .  \n 1810.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Decket Schlaf die weite Runde,", "tokens": ["De\u00b7cket", "Schlaf", "die", "wei\u00b7te", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df ich oft am Fenster lauschen,", "tokens": ["Mu\u00df", "ich", "oft", "am", "Fens\u00b7ter", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die Str\u00f6me unten rauschen,", "tokens": ["Wie", "die", "Str\u00f6\u00b7me", "un\u00b7ten", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "R\u00e4der sausen k\u00fchl im Grunde,", "tokens": ["R\u00e4\u00b7der", "sau\u00b7sen", "k\u00fchl", "im", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mir ist so wohl zur Stunde;", "tokens": ["Und", "mir", "ist", "so", "wohl", "zur", "Stun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn hinab vom Felsenrande", "tokens": ["Denn", "hin\u00b7ab", "vom", "Fel\u00b7sen\u00b7ran\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sp\u00fcr' ich Freiheit, uralt Sehnen,", "tokens": ["Sp\u00fcr'", "ich", "Frei\u00b7heit", ",", "ur\u00b7alt", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Fromm zerbrechend alle Bande,", "tokens": ["Fromm", "zer\u00b7bre\u00b7chend", "al\u00b7le", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ueber W\u00e4lder, Strom und Lande", "tokens": ["Ue\u00b7ber", "W\u00e4l\u00b7der", ",", "Strom", "und", "Lan\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Keck die gro\u00dfen Fl\u00fcgel dehnen.", "tokens": ["Keck", "die", "gro\u00b7\u00dfen", "Fl\u00fc\u00b7gel", "deh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}