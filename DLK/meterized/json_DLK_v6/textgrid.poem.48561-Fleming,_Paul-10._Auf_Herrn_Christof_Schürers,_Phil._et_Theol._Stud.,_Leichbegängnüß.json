{"textgrid.poem.48561": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "10. Auf Herrn Christof Sch\u00fcrers, Phil. et Theol. Stud., Leichbeg\u00e4ngn\u00fc\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Preis der Jugend, Lob der Stadt,", "tokens": ["Preis", "der", "Ju\u00b7gend", ",", "Lob", "der", "Stadt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zier des Stammes und der Deinen,", "tokens": ["Zier", "des", "Stam\u00b7mes", "und", "der", "Dei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wohnhaus mancher Wissenheit,", "tokens": ["Wohn\u00b7haus", "man\u00b7cher", "Wis\u00b7sen\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vieler Freude vor, nun Leid,", "tokens": ["Vie\u00b7ler", "Freu\u00b7de", "vor", ",", "nun", "Leid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "o du Ursach unserm Weinen,", "tokens": ["o", "du", "Ur\u00b7sach", "un\u00b7serm", "Wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "das kein Endmal wei\u00df noch hat!", "tokens": ["das", "kein", "End\u00b7mal", "wei\u00df", "noch", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "ADV", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "War nun di\u00df des Himmels Schlu\u00df,", "tokens": ["War", "nun", "di\u00df", "des", "Him\u00b7mels", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PDS", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df er dich mit Gaben schm\u00fcckte,", "tokens": ["da\u00df", "er", "dich", "mit", "Ga\u00b7ben", "schm\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "die sonst nicht gemeine sind,", "tokens": ["die", "sonst", "nicht", "ge\u00b7mei\u00b7ne", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKNEG", "ADJA", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "und doch allzu gar geschwind'", "tokens": ["und", "doch", "all\u00b7zu", "gar", "ge\u00b7schwind'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PTKA", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "in den Ort der Stille schickte,", "tokens": ["in", "den", "Ort", "der", "Stil\u00b7le", "schick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "welchem Alles werden mu\u00df?", "tokens": ["wel\u00b7chem", "Al\u00b7les", "wer\u00b7den", "mu\u00df", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "PIS", "VAINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ph\u00f6bus sah dich g\u00fcnstig an,", "tokens": ["Ph\u00f6\u00b7bus", "sah", "dich", "g\u00fcns\u00b7tig", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "die gelehrten Kastalinnen", "tokens": ["die", "ge\u00b7lehr\u00b7ten", "Kas\u00b7ta\u00b7lin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "zeigten dir den Helikon,", "tokens": ["zeig\u00b7ten", "dir", "den", "He\u00b7li\u00b7kon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Plato hie\u00df dich deinen Sohn,", "tokens": ["Pla\u00b7to", "hie\u00df", "dich", "dei\u00b7nen", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "und Porphyr wird zeugen k\u00f6nnen,", "tokens": ["und", "Por\u00b7phyr", "wird", "zeu\u00b7gen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "VVINF", "VMINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "was er schon an dir getan.", "tokens": ["was", "er", "schon", "an", "dir", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Wer dich sahe, liebte dich", "tokens": ["Wer", "dich", "sa\u00b7he", ",", "lieb\u00b7te", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "hoch um Sch\u00f6nheit, mehr um Tugend,", "tokens": ["hoch", "um", "Sch\u00f6n\u00b7heit", ",", "mehr", "um", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "so vor billich Allem geht,", "tokens": ["so", "vor", "bil\u00b7lich", "Al\u00b7lem", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJD", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "ob sie gleich zur\u00fccke steht", "tokens": ["ob", "sie", "gleich", "zu\u00b7r\u00fc\u00b7cke", "steht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "bevorab bei unsrer Jugend,", "tokens": ["be\u00b7vor\u00b7ab", "bei", "uns\u00b7rer", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "so f\u00fcr sie mehr liebet sich.", "tokens": ["so", "f\u00fcr", "sie", "mehr", "lie\u00b7bet", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADV", "VVFIN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Itzo war es fast nun Zeit,", "tokens": ["It\u00b7zo", "war", "es", "fast", "nun", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df du deiner Reisen Z\u00fcgel", "tokens": ["da\u00df", "du", "dei\u00b7ner", "Rei\u00b7sen", "Z\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "lie\u00dfest schie\u00dfen durch die Welt,", "tokens": ["lie\u00b7\u00dfest", "schie\u00b7\u00dfen", "durch", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "da dir schon war f\u00fcrgestellt", "tokens": ["da", "dir", "schon", "war", "f\u00fcr\u00b7ge\u00b7stellt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "durch der Sinnen schnelle Fl\u00fcgel", "tokens": ["durch", "der", "Sin\u00b7nen", "schnel\u00b7le", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "was sich hoch h\u00e4lt weit und breit,", "tokens": ["was", "sich", "hoch", "h\u00e4lt", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADJD", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "als vor diesem denn getan", "tokens": ["als", "vor", "die\u00b7sem", "denn", "ge\u00b7tan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PDAT", "KON", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "dein so weit gewes'ner Bruder.", "tokens": ["dein", "so", "weit", "ge\u00b7wes'\u00b7ner", "Bru\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Keiner wird ber\u00fchmt und gro\u00df,", "tokens": ["Kei\u00b7ner", "wird", "be\u00b7r\u00fchmt", "und", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "welcher liebt der Mutter Scho\u00df", "tokens": ["wel\u00b7cher", "liebt", "der", "Mut\u00b7ter", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "f\u00fcr die Reisen, Pferd' und Ruder.", "tokens": ["f\u00fcr", "die", "Rei\u00b7sen", ",", "Pferd'", "und", "Ru\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wer nichts wagt, der wird kein Man.", "tokens": ["Wer", "nichts", "wagt", ",", "der", "wird", "kein", "Man", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "$,", "PRELS", "VAFIN", "PIAT", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Dieses war dein Wundsch und Sin.", "tokens": ["Die\u00b7ses", "war", "dein", "Wund\u00b7sch", "und", "Sin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "KON", "NE", "$."], "meter": "+---++-+", "measure": "dactylic.init"}, "line.2": {"text": "Dem nur war es nicht versehen,", "tokens": ["Dem", "nur", "war", "es", "nicht", "ver\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "der sein Ja zu Allem spricht,", "tokens": ["der", "sein", "Ja", "zu", "Al\u00b7lem", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "wenn es uns soll fehlen nicht.", "tokens": ["wenn", "es", "uns", "soll", "feh\u00b7len", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbnein\u00ab, sagt' er: \u00bbdi\u00df soll geschehen!\u00ab", "tokens": ["\u00bb", "nein", "\u00ab", ",", "sagt'", "er", ":", "\u00bb", "di\u00df", "soll", "ge\u00b7sche\u00b7hen", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKANT", "$(", "$,", "VVFIN", "PPER", "$.", "$(", "PDS", "VMFIN", "VVINF", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "und gab dich den Parzen hin.", "tokens": ["und", "gab", "dich", "den", "Par\u00b7zen", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Wie der kecke Rosenkopf", "tokens": ["Wie", "der", "ke\u00b7cke", "Ro\u00b7sen\u00b7kopf"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "seinen jungen Hals erhebet,", "tokens": ["sei\u00b7nen", "jun\u00b7gen", "Hals", "er\u00b7he\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "weil der Blumen Wirt, der West,", "tokens": ["weil", "der", "Blu\u00b7men", "Wirt", ",", "der", "West", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "ihn noch mit sich bulen l\u00e4\u00dft,", "tokens": ["ihn", "noch", "mit", "sich", "bu\u00b7len", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PRF", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "bald doch vor dem Nord erbebet", "tokens": ["bald", "doch", "vor", "dem", "Nord", "er\u00b7be\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und h\u00e4ngt ab den welken Knopf:", "tokens": ["und", "h\u00e4ngt", "ab", "den", "wel\u00b7ken", "Knopf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "so war deines Lebens Zier,", "tokens": ["so", "war", "dei\u00b7nes", "Le\u00b7bens", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "junger ", "tokens": ["jun\u00b7ger"], "token_info": ["word"], "pos": ["ADJA"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "war ein kurzer Blumenschein,", "tokens": ["war", "ein", "kur\u00b7zer", "Blu\u00b7men\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "der bald k\u00f6mt und bald geht ein.", "tokens": ["der", "bald", "k\u00f6mt", "und", "bald", "geht", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "KON", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur dein feuriges Gem\u00fcte", "tokens": ["Nur", "dein", "feu\u00b7ri\u00b7ges", "Ge\u00b7m\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "funkelt noch bei uns nach dir.", "tokens": ["fun\u00b7kelt", "noch", "bei", "uns", "nach", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und was ist es Neues doch", "tokens": ["Und", "was", "ist", "es", "Neu\u00b7es", "doch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VAFIN", "PPER", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "in der frischen Jugend sterben?", "tokens": ["in", "der", "fri\u00b7schen", "Ju\u00b7gend", "ster\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Polyxene ward nicht alt;", "tokens": ["Po\u00b7ly\u00b7xe\u00b7ne", "ward", "nicht", "alt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Alexander ginge bald;", "tokens": ["A\u00b7lex\u00b7an\u00b7der", "gin\u00b7ge", "bald", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"