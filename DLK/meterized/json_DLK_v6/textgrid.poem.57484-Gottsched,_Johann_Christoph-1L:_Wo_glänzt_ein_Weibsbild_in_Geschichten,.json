{"textgrid.poem.57484": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo gl\u00e4nzt ein Weibsbild in Geschichten,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo gl\u00e4nzt ein Weibsbild in Geschichten,", "tokens": ["Wo", "gl\u00e4nzt", "ein", "Weibs\u00b7bild", "in", "Ge\u00b7schich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So blendend als ", "tokens": ["So", "blen\u00b7dend", "als"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die zwar den Sohn, das Volk zu richten,", "tokens": ["Die", "zwar", "den", "Sohn", ",", "das", "Volk", "zu", "rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch Feinden nur zum Spotte lie\u00df.", "tokens": ["Doch", "Fein\u00b7den", "nur", "zum", "Spot\u00b7te", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Zweig, aus ", "tokens": ["Ein", "Zweig", ",", "aus"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Besch\u00e4mt hier Babels Stifterinn:", "tokens": ["Be\u00b7sch\u00e4mt", "hier", "Ba\u00b7bels", "Stif\u00b7te\u00b7rinn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sein Reis erwirbt sich Reichsgenossen,", "tokens": ["Sein", "Reis", "er\u00b7wirbt", "sich", "Reichs\u00b7ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und st\u00fcrzt der H\u00f6llen Schaaren hin.", "tokens": ["Und", "st\u00fcrzt", "der", "H\u00f6l\u00b7len", "Schaa\u00b7ren", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Durch ihren ", "tokens": ["Durch", "ih\u00b7ren"], "token_info": ["word", "word"], "pos": ["APPR", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "So wird ein neues Reich geschaffen,", "tokens": ["So", "wird", "ein", "neu\u00b7es", "Reich", "ge\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vor dem das stolze Babel wich.", "tokens": ["Vor", "dem", "das", "stol\u00b7ze", "Ba\u00b7bel", "wich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Indus bis zum Mittelmeere,", "tokens": ["Vom", "In\u00b7dus", "bis", "zum", "Mit\u00b7tel\u00b7mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Stund dieses Siegers Thron erh\u00f6ht.", "tokens": ["Stund", "die\u00b7ses", "Sie\u00b7gers", "Thron", "er\u00b7h\u00f6ht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie klein wird seiner Herrschaft Ehre,", "tokens": ["Wie", "klein", "wird", "sei\u00b7ner", "Herr\u00b7schaft", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Bey deinem Glanz, o ", "tokens": ["Bey", "dei\u00b7nem", "Glanz", ",", "o"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "FM"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Dianens Tempel steht in Flammen,", "tokens": ["Di\u00b7a\u00b7nens", "Tem\u00b7pel", "steht", "in", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von dem der Griechen Ruhm soll stammen;", "tokens": ["Von", "dem", "der", "Grie\u00b7chen", "Ruhm", "soll", "stam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie aller Asiater Hohn.", "tokens": ["Wie", "al\u00b7ler", "A\u00b7sia\u00b7ter", "Hohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Doch, ", "tokens": ["Doch", ","], "token_info": ["word", "punct"], "pos": ["KON", "$,"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Du zwingst die Welt mit blo\u00dfer Macht:", "tokens": ["Du", "zwingst", "die", "Welt", "mit", "blo\u00b7\u00dfer", "Macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "Zum gro\u00dfen Rom den Grund zu legen,", "tokens": ["Zum", "gro\u00b7\u00dfen", "Rom", "den", "Grund", "zu", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NE", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "War dort der ", "tokens": ["War", "dort", "der"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Doch Wuth und Mord leiht ihm den Degen;", "tokens": ["Doch", "Wuth", "und", "Mord", "leiht", "ihm", "den", "De\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da er auf Bruderblut ergrimmt.", "tokens": ["Da", "er", "auf", "Bru\u00b7der\u00b7blut", "er\u00b7grimmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Rom hat der V\u00f6lker Fall geschworen:", "tokens": ["Rom", "hat", "der", "V\u00f6l\u00b7ker", "Fall", "ge\u00b7schwo\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es ruht nicht, bis es alles hat.", "tokens": ["Es", "ruht", "nicht", ",", "bis", "es", "al\u00b7les", "hat", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hier wird ein ", "tokens": ["Hier", "wird", "ein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.8": {"text": "Sein weites Reich ist ", "tokens": ["Sein", "wei\u00b7tes", "Reich", "ist"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Du, ", "tokens": ["Du", ","], "token_info": ["word", "punct"], "pos": ["PPER", "$,"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Der Sachsen nie besiegtes Land.", "tokens": ["Der", "Sach\u00b7sen", "nie", "be\u00b7sieg\u00b7tes", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Eifer nennt es Glaubenssiege:", "tokens": ["Dein", "Ei\u00b7fer", "nennt", "es", "Glau\u00b7bens\u00b7sie\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der Christen Gott bleibt unbekannt.", "tokens": ["Der", "Chris\u00b7ten", "Gott", "bleibt", "un\u00b7be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bekehre nur mit Mord und Rauben!", "tokens": ["Be\u00b7keh\u00b7re", "nur", "mit", "Mord", "und", "Rau\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im Herzen ha\u00dft dich ", "tokens": ["Im", "Her\u00b7zen", "ha\u00dft", "dich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Wenn Er ", "tokens": ["Wenn", "Er"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Du Zweig aus ", "tokens": ["Du", "Zweig", "aus"], "token_info": ["word", "word", "word"], "pos": ["PPER", "NN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Hier ehrt ", "tokens": ["Hier", "ehrt"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "++", "measure": "spondeus"}, "line.10": {"text": "So wie der Mei\u00dfner Musensitz.", "tokens": ["So", "wie", "der", "Mei\u00df\u00b7ner", "Mu\u00b7sen\u00b7sitz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Blo\u00df ", "tokens": ["Blo\u00df"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Ahmt er der ", "tokens": ["Ahmt", "er", "der"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.13": {"text": "O sch\u00fctz auch Deutschlands Pierinnen,", "tokens": ["O", "sch\u00fctz", "auch", "Deutschlands", "Pie\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}