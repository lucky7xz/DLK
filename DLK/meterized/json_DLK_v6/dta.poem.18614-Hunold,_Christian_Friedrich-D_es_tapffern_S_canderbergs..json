{"dta.poem.18614": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "D es tapffern  S canderbergs.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Frolockst du/ Saracen/ da\u00df dieser Held erblasset/", "tokens": ["Fro\u00b7lockst", "du", "/", "Sa\u00b7ra\u00b7cen", "/", "da\u00df", "die\u00b7ser", "Held", "er\u00b7blas\u00b7set", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "NE", "$(", "KOUS", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sein Gebeine dir zu Theile werden mu\u00df?", "tokens": ["Und", "sein", "Ge\u00b7bei\u00b7ne", "dir", "zu", "Thei\u00b7le", "wer\u00b7den", "mu\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "APPR", "NN", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du Narr/ du hast allhier die Schaalen nur umfasset/", "tokens": ["Du", "Narr", "/", "du", "hast", "all\u00b7hier", "die", "Schaa\u00b7len", "nur", "um\u00b7fas\u00b7set", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$(", "PPER", "VAFIN", "ADV", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Kern hat Ludewig \u2020 und auch Eugenius.", "tokens": ["Den", "Kern", "hat", "Lu\u00b7de\u00b7wig", "\u2020", "und", "auch", "Eu\u00b7ge\u00b7nius", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NE", "$(", "KON", "ADV", "NE", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "&#8224; von Baden", "tokens": ["&#8224;", "von", "Ba\u00b7den"], "token_info": ["XML_entity", "word", "word"], "pos": ["$(", "APPR", "NN"], "meter": "-+-", "measure": "amphibrach.single"}}}}}