{"textgrid.poem.28369": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "84.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nivula brennt ihrer viel;", "tokens": ["Ni\u00b7vu\u00b7la", "brennt", "ih\u00b7rer", "viel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Ieder, der sie siht, der wil", "tokens": ["Ie\u00b7der", ",", "der", "sie", "siht", ",", "der", "wil"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Di\u00df und das an sie verwagen;", "tokens": ["Di\u00df", "und", "das", "an", "sie", "ver\u00b7wa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KON", "PDS", "APPR", "PPER", "VVINF", "$."], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Was dann wird es Nutzen tragen?", "tokens": ["Was", "dann", "wird", "es", "Nut\u00b7zen", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was sie gab, das bleibt ihr doch;", "tokens": ["Was", "sie", "gab", ",", "das", "bleibt", "ihr", "doch", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wer es hatte, sucht es noch.", "tokens": ["Wer", "es", "hat\u00b7te", ",", "sucht", "es", "noch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}