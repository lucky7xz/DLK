{"textgrid.poem.32788": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Trauerlied einer Braut", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Totenglocke l\u00e4utet", "tokens": ["Die", "To\u00b7ten\u00b7glo\u00b7cke", "l\u00e4u\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Entsetzen in mein Ohr;", "tokens": ["Ent\u00b7set\u00b7zen", "in", "mein", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Bahre k\u00f6mmt, begleitet", "tokens": ["Die", "Bah\u00b7re", "k\u00f6mmt", ",", "be\u00b7glei\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von banger Freunde Chor;", "tokens": ["Von", "ban\u00b7ger", "Freun\u00b7de", "Chor", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie tragen den Geliebten,", "tokens": ["Sie", "tra\u00b7gen", "den", "Ge\u00b7lieb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den mir die Liebe gab,", "tokens": ["Den", "mir", "die", "Lie\u00b7be", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hinweg von der betr\u00fcbten,", "tokens": ["Hin\u00b7weg", "von", "der", "be\u00b7tr\u00fcb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gekr\u00e4nkten Braut ins Grab.", "tokens": ["Ge\u00b7kr\u00e4nk\u00b7ten", "Braut", "ins", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Des Chores dumpfe T\u00f6ne", "tokens": ["Des", "Cho\u00b7res", "dump\u00b7fe", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erschallen feierlich!", "tokens": ["Er\u00b7schal\u00b7len", "fei\u00b7er\u00b7lich", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbwelt, packe dich! ich sehne", "tokens": ["\u00bb", "welt", ",", "pa\u00b7cke", "dich", "!", "ich", "seh\u00b7ne"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "PPER", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur nach dem Himmel mich!\u00ab", "tokens": ["Nur", "nach", "dem", "Him\u00b7mel", "mich", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ja, guter, guter Himmel,", "tokens": ["Ja", ",", "gu\u00b7ter", ",", "gu\u00b7ter", "Him\u00b7mel", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich sehne mich nach dir!", "tokens": ["Ich", "seh\u00b7ne", "mich", "nach", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In diesem Weltget\u00fcmmel", "tokens": ["In", "die\u00b7sem", "Welt\u00b7ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bleibt keine Freude mir.", "tokens": ["Bleibt", "kei\u00b7ne", "Freu\u00b7de", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ach Gott! zu Freudenfesten", "tokens": ["Ach", "Gott", "!", "zu", "Freu\u00b7den\u00b7fes\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ITJ", "NN", "$.", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "War schon mein Haupt geschm\u00fcckt!", "tokens": ["War", "schon", "mein", "Haupt", "ge\u00b7schm\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da hast du mir den besten", "tokens": ["Da", "hast", "du", "mir", "den", "bes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der J\u00fcnglinge entr\u00fcckt.", "tokens": ["Der", "J\u00fcng\u00b7lin\u00b7ge", "ent\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+---+", "measure": "dactylic.init"}, "line.5": {"text": "Ihn ri\u00df aus meinen Armen,", "tokens": ["Ihn", "ri\u00df", "aus", "mei\u00b7nen", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auf deinen Wink, der Tod;", "tokens": ["Auf", "dei\u00b7nen", "Wink", ",", "der", "Tod", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "O f\u00fchl auch jetzt Erbarmen,", "tokens": ["O", "f\u00fchl", "auch", "jetzt", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und ende meine Not!", "tokens": ["Und", "en\u00b7de", "mei\u00b7ne", "Not", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "La\u00df bald die Stunde kommen,", "tokens": ["La\u00df", "bald", "die", "Stun\u00b7de", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die meinen Br\u00e4utigam,", "tokens": ["Die", "mei\u00b7nen", "Br\u00e4u\u00b7ti\u00b7gam", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Z\u00e4rtlichen, den Frommen,", "tokens": ["Den", "Z\u00e4rt\u00b7li\u00b7chen", ",", "den", "From\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit sich gen Himmel nahm!", "tokens": ["Mit", "sich", "gen", "Him\u00b7mel", "nahm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da sucht er jetzt, bek\u00fcmmert", "tokens": ["Da", "sucht", "er", "jetzt", ",", "be\u00b7k\u00fcm\u00b7mert"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auf Auen voller Licht,", "tokens": ["Auf", "Au\u00b7en", "vol\u00b7ler", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mich, die hier einsam wimmert,", "tokens": ["Mich", ",", "die", "hier", "ein\u00b7sam", "wim\u00b7mert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und ach, erblickt sie nicht.", "tokens": ["Und", "ach", ",", "er\u00b7blickt", "sie", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Willst du, da\u00df seine Freude", "tokens": ["Willst", "du", ",", "da\u00df", "sei\u00b7ne", "Freu\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ganz Himmelsfreude wird,", "tokens": ["Ganz", "Him\u00b7mels\u00b7freu\u00b7de", "wird", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So seh' die Flur uns beide,", "tokens": ["So", "seh'", "die", "Flur", "uns", "bei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo er jetzt einsam irrt!", "tokens": ["Wo", "er", "jetzt", "ein\u00b7sam", "irrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bring unter lautem Segen,", "tokens": ["Bring", "un\u00b7ter", "lau\u00b7tem", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von Engeln angestimmt,", "tokens": ["Von", "En\u00b7geln", "an\u00b7ge\u00b7stimmt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ihm seine Braut entgegen,", "tokens": ["Ihm", "sei\u00b7ne", "Braut", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die hier in Thr\u00e4nen schwimmt!", "tokens": ["Die", "hier", "in", "Thr\u00e4\u00b7nen", "schwimmt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}