{"dta.poem.4168": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Einleitung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dwehrtes Land, geliebtes Feld,", "tokens": ["Dwehr\u00b7tes", "Land", ",", "ge\u00b7lieb\u00b7tes", "Feld", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das so viel Lieblichkeit enth\u00e4lt,", "tokens": ["Das", "so", "viel", "Lieb\u00b7lich\u00b7keit", "ent\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo die Natur gedoppelt sch\u00f6n,", "tokens": ["Wo", "die", "Na\u00b7tur", "ge\u00b7dop\u00b7pelt", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jm Land, und auf dem Meer, zu sehn!", "tokens": ["Jm", "Land", ",", "und", "auf", "dem", "Meer", ",", "zu", "sehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "APPR", "ART", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun wird es mir verg\u00f6nnet werden,", "tokens": ["Nun", "wird", "es", "mir", "ver\u00b7g\u00f6n\u00b7net", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu ruhn in deinem Schoo\u00df, auf bunt bebl\u00fchmter Erden!", "tokens": ["Zu", "ruhn", "in", "dei\u00b7nem", "Schoo\u00df", ",", "auf", "bunt", "be\u00b7bl\u00fchm\u00b7ter", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "PPOSAT", "NN", "$,", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nun werd\u2019 ich dieses Gl\u00fcck erhalten,", "tokens": ["Nun", "werd'", "ich", "die\u00b7ses", "Gl\u00fcck", "er\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df, nach, mit allem Flei\u00df und Kr\u00e4ften,", "tokens": ["Da\u00df", ",", "nach", ",", "mit", "al\u00b7lem", "Flei\u00df", "und", "Kr\u00e4f\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "$,", "APPR", "PIS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Von mir vollf\u00fchrten Amts-Gesch\u00e4ften,", "tokens": ["Von", "mir", "voll\u00b7f\u00fchr\u00b7ten", "Amts\u00b7Ge\u00b7sch\u00e4f\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Bald mit den Schriften kluger Alten,", "tokens": ["Bald", "mit", "den", "Schrif\u00b7ten", "klu\u00b7ger", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Bald einem sanften Schlaf, ich meine stille Zeit,", "tokens": ["Bald", "ei\u00b7nem", "sanf\u00b7ten", "Schlaf", ",", "ich", "mei\u00b7ne", "stil\u00b7le", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "In unbesorgter Ruh, in stolzer Sicherheit,", "tokens": ["In", "un\u00b7be\u00b7sorg\u00b7ter", "Ruh", ",", "in", "stol\u00b7zer", "Si\u00b7cher\u00b7heit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und edlem M\u00fc\u00dfiggang, entfernt von Neid und Streit,", "tokens": ["Und", "ed\u00b7lem", "M\u00fc\u00b7\u00dfig\u00b7gang", ",", "ent\u00b7fernt", "von", "Neid", "und", "Streit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}