{"dta.poem.11576": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Ideale Wahrheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Gestern entschlief ich im Wald: da sah ich im Traume", "tokens": ["Ge\u00b7stern", "ent\u00b7schlief", "ich", "im", "Wald", ":", "da", "sah", "ich", "im", "Trau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "$.", "ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "M\u00e4dchen, mit dem ich als Kind immer am liebsten", "tokens": ["M\u00e4d\u00b7chen", ",", "mit", "dem", "ich", "als", "Kind", "im\u00b7mer", "am", "liebs\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "PRELS", "PPER", "KOUS", "NN", "ADV", "APPRART", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Und sie zeigte mir hoch in der Eiche Gipfel den Gukuk,", "tokens": ["Und", "sie", "zeig\u00b7te", "mir", "hoch", "in", "der", "Ei\u00b7che", "Gip\u00b7fel", "den", "Gu\u00b7kuk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Wie die Kindheit ihn denkt, pr\u00e4chtig gefiedert und", "tokens": ["Wie", "die", "Kind\u00b7heit", "ihn", "denkt", ",", "pr\u00e4ch\u00b7tig", "ge\u00b7fie\u00b7dert", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PPER", "VVFIN", "$,", "ADJD", "VVPP", "KON"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Drum! \u2014 so rief ich \u2014 die\u00df ist der \u00e4chte Gukuk! Wer", "tokens": ["Drum", "!", "so", "rief", "ich", "die\u00df", "ist", "der", "\u00e4ch\u00b7te", "Gu\u00b7kuk", "!", "Wer"], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PAV", "$.", "$(", "ADV", "VVFIN", "PPER", "$(", "PDS", "VAFIN", "ART", "ADJA", "NN", "$.", "PWS"], "meter": "-+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.6": {"text": "Mir doch neulich, er sey klein nur, unscheinbar und", "tokens": ["Mir", "doch", "neu\u00b7lich", ",", "er", "sey", "klein", "nur", ",", "un\u00b7schein\u00b7bar", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "ADV", "$,", "PPER", "VAFIN", "ADJD", "ADV", "$,", "ADJD", "KON"], "meter": "+-+---+-+--+", "measure": "iambic.penta.chol"}}}}}