{"dta.poem.12028": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was Gott in der Natur und dir im Herzen spricht,", "tokens": ["Was", "Gott", "in", "der", "Na\u00b7tur", "und", "dir", "im", "Her\u00b7zen", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPR", "ART", "NN", "KON", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Andacht merke drauf, und \u00fcberh\u00f6r' es nicht.", "tokens": ["Mit", "An\u00b7dacht", "mer\u00b7ke", "drauf", ",", "und", "\u00fc\u00b7ber\u00b7h\u00f6r'", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wenn du's andern nicht kannst machen offenbar,", "tokens": ["Und", "wenn", "du's", "an\u00b7dern", "nicht", "kannst", "ma\u00b7chen", "of\u00b7fen\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PIS", "PTKNEG", "VMFIN", "VVINF", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch dir zur eigenen Erbauung mach es klar.", "tokens": ["Doch", "dir", "zur", "ei\u00b7ge\u00b7nen", "Er\u00b7bau\u00b7ung", "mach", "es", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und ist es dir nur klar, so wird's auch andern werden,", "tokens": ["Und", "ist", "es", "dir", "nur", "klar", ",", "so", "wird's", "auch", "an\u00b7dern", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "ADV", "ADJD", "$,", "ADV", "VAFIN", "ADV", "PIS", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn nicht in Worten, doch in Mienen und Geberden.", "tokens": ["Wenn", "nicht", "in", "Wor\u00b7ten", ",", "doch", "in", "Mie\u00b7nen", "und", "Ge\u00b7ber\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "APPR", "NN", "$,", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}}}}}