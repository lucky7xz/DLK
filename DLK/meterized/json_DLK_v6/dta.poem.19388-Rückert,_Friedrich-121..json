{"dta.poem.19388": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "121.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In Waldeseinsamkeit, von Wurzeln und von Wasser", "tokens": ["In", "Wal\u00b7de\u00b7sein\u00b7sam\u00b7keit", ",", "von", "Wur\u00b7zeln", "und", "von", "Was\u00b7ser"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich n\u00e4hrend, lebt ein Mann, und hei\u00dft ein Menschenhasser.", "tokens": ["Sich", "n\u00e4h\u00b7rend", ",", "lebt", "ein", "Mann", ",", "und", "hei\u00dft", "ein", "Men\u00b7schen\u00b7has\u00b7ser", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "$,", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Den fragt' ein Wandrer einst: Was trieb dich an zu hassen", "tokens": ["Den", "fragt'", "ein", "Wand\u00b7rer", "einst", ":", "Was", "trieb", "dich", "an", "zu", "has\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "ADV", "$.", "PWS", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Menschen, und warum hast du die Welt verlassen?", "tokens": ["Die", "Men\u00b7schen", ",", "und", "wa\u00b7rum", "hast", "du", "die", "Welt", "ver\u00b7las\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PWAV", "VAFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Da sprach er: Nicht aus Ha\u00df verlassen hab' ich sie,", "tokens": ["Da", "sprach", "er", ":", "Nicht", "aus", "Ha\u00df", "ver\u00b7las\u00b7sen", "hab'", "ich", "sie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PTKNEG", "APPR", "NN", "VVPP", "VAFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus Liebe that ich es, und will dir sagen, wie.", "tokens": ["Aus", "Lie\u00b7be", "that", "ich", "es", ",", "und", "will", "dir", "sa\u00b7gen", ",", "wie", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPER", "$,", "KON", "VMFIN", "PPER", "VVINF", "$,", "PWAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "In meinem Herzen wohnt ein innres Freudenlicht,", "tokens": ["In", "mei\u00b7nem", "Her\u00b7zen", "wohnt", "ein", "inn\u00b7res", "Freu\u00b7den\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ist kein Schein davon auf meinem Angesicht.", "tokens": ["Doch", "ist", "kein", "Schein", "da\u00b7von", "auf", "mei\u00b7nem", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "PAV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die Menschen, die das Licht nicht sahn in meinem Herzen,", "tokens": ["Die", "Men\u00b7schen", ",", "die", "das", "Licht", "nicht", "sahn", "in", "mei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "PTKNEG", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Ernst im Angesicht war St\u00f6rung ihren Scherzen.", "tokens": ["Der", "Ernst", "im", "An\u00b7ge\u00b7sicht", "war", "St\u00f6\u00b7rung", "ih\u00b7ren", "Scher\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VAFIN", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ungl\u00fcckweissagend war der Ausdruck meiner Mienen,", "tokens": ["Un\u00b7gl\u00fcck\u00b7weis\u00b7sa\u00b7gend", "war", "der", "Aus\u00b7druck", "mei\u00b7ner", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Wie Trauerboten, die beim Freudenfest erschienen.", "tokens": ["Wie", "Trau\u00b7er\u00b7bo\u00b7ten", ",", "die", "beim", "Freu\u00b7den\u00b7fest", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Und um die Weltlichkeit nicht dort in ihrem Gl\u00fcck", "tokens": ["Und", "um", "die", "Welt\u00b7lich\u00b7keit", "nicht", "dort", "in", "ih\u00b7rem", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu hindern, zog ich mich mit meinem hier zur\u00fcck.", "tokens": ["Zu", "hin\u00b7dern", ",", "zog", "ich", "mich", "mit", "mei\u00b7nem", "hier", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ich f\u00fchle mir genug das Licht in meiner Brust,", "tokens": ["Ich", "f\u00fch\u00b7le", "mir", "ge\u00b7nug", "das", "Licht", "in", "mei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und w\u00fcnsche da\u00df der Welt gen\u00fcg' auch ihre Lust.", "tokens": ["Und", "w\u00fcn\u00b7sche", "da\u00df", "der", "Welt", "ge\u00b7n\u00fcg'", "auch", "ih\u00b7re", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "ART", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}