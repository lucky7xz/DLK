{"dta.poem.2640": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "S\u00fcssester Todes-Schlaff  \n Hn. G. S. d. R. in B. den 9. Octobr.  \n 1672.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "St\u00f6rt nicht den Seligen mit Thr\u00e4nen-reichem Klagen/", "tokens": ["St\u00f6rt", "nicht", "den", "Se\u00b7li\u00b7gen", "mit", "Thr\u00e4\u00b7nen\u00b7rei\u00b7chem", "Kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Betr&#252;btste/ denn er schl\u00e4fft in angenehmer Ruh:", "tokens": ["Be\u00b7tr", "&#252;", "bts\u00b7te", "/", "denn", "er", "schl\u00e4fft", "in", "an\u00b7ge\u00b7neh\u00b7mer", "Ruh", ":"], "token_info": ["word", "XML_entity", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVFIN", "$(", "KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die B\u00fcrde dr\u00fcckt ihn nicht/ so er bi\u00dfher getragen/", "tokens": ["Die", "B\u00fcr\u00b7de", "dr\u00fcckt", "ihn", "nicht", "/", "so", "er", "bi\u00df\u00b7her", "ge\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$(", "ADV", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und GOttes Fl\u00fcgel deckt die m\u00fcden Glieder zu.", "tokens": ["Und", "Got\u00b7tes", "Fl\u00fc\u00b7gel", "deckt", "die", "m\u00fc\u00b7den", "Glie\u00b7der", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach h\u00f6chst-verlangte Ruh! ach Frieden-volles Schlaffen!", "tokens": ["Ach", "h\u00f6chst\u00b7ver\u00b7lang\u00b7te", "Ruh", "!", "ach", "Frie\u00b7den\u00b7vol\u00b7les", "Schlaf\u00b7fen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$.", "XY", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ach stete Sicherheit die nie kein Unfall kr\u00e4nckt!", "tokens": ["Ach", "ste\u00b7te", "Si\u00b7cher\u00b7heit", "die", "nie", "kein", "Un\u00b7fall", "kr\u00e4nckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "NN", "ART", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In dem der Erdenkrei\u00df ersch\u00fcttert von den Waffen/", "tokens": ["In", "dem", "der", "Er\u00b7den\u00b7krei\u00df", "er\u00b7sch\u00fct\u00b7tert", "von", "den", "Waf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und Auff- und Niedergang uns hinzurichten denckt!", "tokens": ["Und", "Auf\u00b7f", "und", "Nie\u00b7der\u00b7gang", "uns", "hin\u00b7zu\u00b7rich\u00b7ten", "denckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "TRUNC", "KON", "NN", "PPER", "VVIZU", "VVFIN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Es mag der Heyden Mund den Tod erschrecklich nennen/", "tokens": ["Es", "mag", "der", "Hey\u00b7den", "Mund", "den", "Tod", "er\u00b7schreck\u00b7lich", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und ihrer K\u00fcnstler Hand ihn he\u00dflich stellen f\u00fcr;", "tokens": ["Und", "ih\u00b7rer", "K\u00fcnst\u00b7ler", "Hand", "ihn", "he\u00df\u00b7lich", "stel\u00b7len", "f\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "PPER", "ADJD", "VVINF", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wir Christen werden ihn vor heilsam nur erkennen/", "tokens": ["Wir", "Chris\u00b7ten", "wer\u00b7den", "ihn", "vor", "heil\u00b7sam", "nur", "er\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "PPER", "APPR", "ADJD", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Bahn zur Seligkeit/ zum Himmelreich die Th\u00fcr.", "tokens": ["Die", "Bahn", "zur", "Se\u00b7lig\u00b7keit", "/", "zum", "Him\u00b7mel\u00b7reich", "die", "Th\u00fcr."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "APPRART", "NN", "$(", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Der Schlaff/ der seeliger f\u00fcr jenem ist zu schatzen/", "tokens": ["Der", "Schlaff", "/", "der", "see\u00b7li\u00b7ger", "f\u00fcr", "je\u00b7nem", "ist", "zu", "schat\u00b7zen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "APPR", "PDAT", "VAFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Den uns die M\u00fcdigkeit zuwege hat gebracht:", "tokens": ["Den", "uns", "die", "M\u00fc\u00b7dig\u00b7keit", "zu\u00b7we\u00b7ge", "hat", "ge\u00b7bracht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Hier folgt ein ewiges und himmlisches Ergetzen/", "tokens": ["Hier", "folgt", "ein", "e\u00b7wi\u00b7ges", "und", "himm\u00b7li\u00b7sches", "Er\u00b7get\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Wenn jenes Lust und Ruh\u2019 verschwindet mit der Nacht.", "tokens": ["Wenn", "je\u00b7nes", "Lust", "und", "Ruh'", "ver\u00b7schwin\u00b7det", "mit", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "KON", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Doch was ist ohne Schlaff der Menschen gantzes Leben?", "tokens": ["Doch", "was", "ist", "oh\u00b7ne", "Schlaff", "der", "Men\u00b7schen", "gant\u00b7zes", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "APPR", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Kan auch ein sterblich Leib entbehren solche Krafft?", "tokens": ["Kan", "auch", "ein", "sterb\u00b7lich", "Leib", "ent\u00b7beh\u00b7ren", "sol\u00b7che", "Krafft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJD", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Mu\u00df nicht die s\u00fcsse Ruh die St\u00e4rckung wieder geben?", "tokens": ["Mu\u00df", "nicht", "die", "s\u00fcs\u00b7se", "Ruh", "die", "St\u00e4r\u00b7ckung", "wie\u00b7der", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Den Beinen neues Marck/ den Adern frischen Safft?", "tokens": ["Den", "Bei\u00b7nen", "neu\u00b7es", "Marck", "/", "den", "A\u00b7dern", "fri\u00b7schen", "Safft", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der Tod der sanffte Schlaff bedeckt uns mit der Erde/", "tokens": ["Der", "Tod", "der", "sanff\u00b7te", "Schlaff", "be\u00b7deckt", "uns", "mit", "der", "Er\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Verwahret Fleisch und Blut in seiner kalten Scho\u00df/", "tokens": ["Ver\u00b7wah\u00b7ret", "Fleisch", "und", "Blut", "in", "sei\u00b7ner", "kal\u00b7ten", "Scho\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Doch da\u00df es dermaleins gleich jungen Adlern werde/", "tokens": ["Doch", "da\u00df", "es", "der\u00b7ma\u00b7leins", "gleich", "jun\u00b7gen", "Ad\u00b7lern", "wer\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und schwinge sich verkl\u00e4rt an das gestirnte Schlo\u00df.", "tokens": ["Und", "schwin\u00b7ge", "sich", "ver\u00b7kl\u00e4rt", "an", "das", "ge\u00b7stirn\u00b7te", "Schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wer schl\u00e4fft/ empfindet nichts/ auch in des Grabes Kammer/", "tokens": ["Wer", "schl\u00e4fft", "/", "emp\u00b7fin\u00b7det", "nichts", "/", "auch", "in", "des", "Gra\u00b7bes", "Kam\u00b7mer", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "VVFIN", "PIS", "$(", "ADV", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der Wohnung wahrer Ruh/ und sichrer Einsamkeit/", "tokens": ["Der", "Woh\u00b7nung", "wah\u00b7rer", "Ruh", "/", "und", "sich\u00b7rer", "Ein\u00b7sam\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ist unsers C\u00f6rpers Rest entnommen allem Jammer", "tokens": ["Ist", "un\u00b7sers", "C\u00f6r\u00b7pers", "Rest", "ent\u00b7nom\u00b7men", "al\u00b7lem", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "VVFIN", "PIS", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und vor der Zeiten Sturm/ und Tyranney befreyt.", "tokens": ["Und", "vor", "der", "Zei\u00b7ten", "Sturm", "/", "und", "Ty\u00b7ran\u00b7ney", "be\u00b7freyt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$(", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Des Leibes Mattigkeit bef\u00f6dert uns zu Bette/", "tokens": ["Des", "Lei\u00b7bes", "Mat\u00b7tig\u00b7keit", "be\u00b7f\u00f6\u00b7dert", "uns", "zu", "Bet\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Doch da\u00df wir mit dem Tag an unser Arbeit gehn:", "tokens": ["Doch", "da\u00df", "wir", "mit", "dem", "Tag", "an", "un\u00b7ser", "Ar\u00b7beit", "gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wir ruhen/ nicht/ da\u00df uns das Grab auf ewig h\u00e4tte/", "tokens": ["Wir", "ru\u00b7hen", "/", "nicht", "/", "da\u00df", "uns", "das", "Grab", "auf", "e\u00b7wig", "h\u00e4t\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PTKNEG", "$(", "KOUS", "PPER", "ART", "NN", "APPR", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Es wird des H\u00f6chsten Stimm uns heissen aufferstehn.", "tokens": ["Es", "wird", "des", "H\u00f6chs\u00b7ten", "Stimm", "uns", "heis\u00b7sen", "auf\u00b7fer\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Der Gorgias hat Recht/ als er nun sollen wandern", "tokens": ["Der", "Gor\u00b7gi\u00b7as", "hat", "Recht", "/", "als", "er", "nun", "sol\u00b7len", "wan\u00b7dern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VAFIN", "NN", "$(", "KOUS", "PPER", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und hatte hundert Jahr nechst sieben hingelegt/", "tokens": ["Und", "hat\u00b7te", "hun\u00b7dert", "Jahr", "nechst", "sie\u00b7ben", "hin\u00b7ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "CARD", "NN", "ADV", "CARD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So sprach er: Schlaff und Tod ein Bruder k\u00fcsst den andern/", "tokens": ["So", "sprach", "er", ":", "Schlaff", "und", "Tod", "ein", "Bru\u00b7der", "k\u00fcsst", "den", "an\u00b7dern", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "NN", "KON", "NN", "ART", "NN", "VVFIN", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Di\u00df isi der letzte Freund so mich zu Grabe tr\u00e4gt.", "tokens": ["Di\u00df", "i\u00b7si", "der", "letz\u00b7te", "Freund", "so", "mich", "zu", "Gra\u00b7be", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "ADV", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.37": {"text": "Ja was ist unser Bett in das wir uns vergraben?", "tokens": ["Ja", "was", "ist", "un\u00b7ser", "Bett", "in", "das", "wir", "uns", "ver\u00b7gra\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PWS", "VAFIN", "PPOSAT", "NN", "APPR", "PRELS", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ein Sarch/ der lebend uns wie jener tod beschleust.", "tokens": ["Ein", "Sarch", "/", "der", "le\u00b7bend", "uns", "wie", "je\u00b7ner", "tod", "be\u00b7schleust", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJD", "PPER", "KOKOM", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wenn wir uns satt gem\u00fcht/ und la\u00df gequ\u00e4let haben/", "tokens": ["Wenn", "wir", "uns", "satt", "ge\u00b7m\u00fcht", "/", "und", "la\u00df", "ge\u00b7qu\u00e4\u00b7let", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVPP", "$(", "KON", "VVIMP", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ist di\u00df allein der Ort wo man der Ruh geneust.", "tokens": ["Ist", "di\u00df", "al\u00b7lein", "der", "Ort", "wo", "man", "der", "Ruh", "ge\u00b7neust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "ART", "NN", "PWAV", "PIS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": 