{"textgrid.poem.47146": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich lag von sanftem Traum umflossen", "tokens": ["Ich", "lag", "von", "sanf\u00b7tem", "Traum", "um\u00b7flos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und f\u00fchlte selig mich in dir.", "tokens": ["Und", "f\u00fchl\u00b7te", "se\u00b7lig", "mich", "in", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als ich die Augen aufgeschlossen,", "tokens": ["Als", "ich", "die", "Au\u00b7gen", "auf\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da hingst du l\u00e4chelnd \u00fcber mir.", "tokens": ["Da", "hingst", "du", "l\u00e4\u00b7chelnd", "\u00fc\u00b7ber", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie gerne mag dein Traum zerstieben,", "tokens": ["Wie", "ger\u00b7ne", "mag", "dein", "Traum", "zer\u00b7stie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von deinem Ku\u00df hinweggefl\u00f6\u00dft.", "tokens": ["Von", "dei\u00b7nem", "Ku\u00df", "hin\u00b7weg\u00b7ge\u00b7fl\u00f6\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie hast du sch\u00f6n dich selb vertrieben,", "tokens": ["Wie", "hast", "du", "sch\u00f6n", "dich", "selb", "ver\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sch\u00f6n dich selbst hier abgel\u00f6st!", "tokens": ["Wie", "sch\u00f6n", "dich", "selbst", "hier", "ab\u00b7ge\u00b7l\u00f6st", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}