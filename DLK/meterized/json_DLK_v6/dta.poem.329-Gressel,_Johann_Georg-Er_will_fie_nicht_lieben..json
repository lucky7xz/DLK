{"dta.poem.329": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er will fie nicht lieben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Dein Hoffen ist nichts/ du wartest umsonst", "tokens": ["Dein", "Hof\u00b7fen", "ist", "nichts", "/", "du", "war\u00b7test", "um\u00b7sonst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PIS", "$(", "PPER", "VAFIN", "ADV"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vertilge die Liebe/ verbanne die Gunst/", "tokens": ["Ver\u00b7til\u00b7ge", "die", "Lie\u00b7be", "/", "ver\u00b7ban\u00b7ne", "die", "Gunst", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "VVFIN", "ART", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich fliehe die Flammen", "tokens": ["Ich", "flie\u00b7he", "die", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und werde verdammen", "tokens": ["Und", "wer\u00b7de", "ver\u00b7dam\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["KON", "VAFIN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die schmeichelnde Kunst/", "tokens": ["Die", "schmei\u00b7cheln\u00b7de", "Kunst", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Die immerfort sucht", "tokens": ["Die", "im\u00b7mer\u00b7fort", "sucht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADV", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Mein Hertze zu binden/", "tokens": ["Mein", "Hert\u00b7ze", "zu", "bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "In Fesselu zu winden/", "tokens": ["In", "Fes\u00b7se\u00b7lu", "zu", "win\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Die es doch verflucht.", "tokens": ["Die", "es", "doch", "ver\u00b7flucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}