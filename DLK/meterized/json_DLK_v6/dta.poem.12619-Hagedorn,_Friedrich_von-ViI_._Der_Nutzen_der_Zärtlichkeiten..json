{"dta.poem.12619": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "ViI .  \n  Der Nutzen der Z\u00e4rtlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unmuth und Beschwerden", "tokens": ["Un\u00b7muth", "und", "Be\u00b7schwer\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "W\u00fcrden uns auf Erden", "tokens": ["W\u00fcr\u00b7den", "uns", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Unertr\u00e4glich werden,", "tokens": ["Un\u00b7er\u00b7tr\u00e4g\u00b7lich", "wer\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Unverge\u00dflich seyn:", "tokens": ["Un\u00b7ver\u00b7ge\u00df\u00b7lich", "seyn", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "K\u00f6nnten nicht, zu Zeiten,", "tokens": ["K\u00f6nn\u00b7ten", "nicht", ",", "zu", "Zei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Treue Z\u00e4rtlichkeiten", "tokens": ["Treu\u00b7e", "Z\u00e4rt\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Den Verdru\u00df bestreiten", "tokens": ["Den", "Ver\u00b7dru\u00df", "be\u00b7strei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Und das Herz befreyn.", "tokens": ["Und", "das", "Herz", "be\u00b7freyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "L\u00e4chelt, muntre Sch\u00f6nen!", "tokens": ["L\u00e4\u00b7chelt", ",", "mun\u00b7tre", "Sch\u00f6\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Unsern Ernst zu h\u00f6hnen:", "tokens": ["Un\u00b7sern", "Ernst", "zu", "h\u00f6h\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Singt in s\u00fcssen T\u00f6nen;", "tokens": ["Singt", "in", "s\u00fcs\u00b7sen", "T\u00f6\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Jeder Ton entz\u00fcckt.", "tokens": ["Je\u00b7der", "Ton", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "B\u00fcrden, die dem Leben", "tokens": ["B\u00fcr\u00b7den", ",", "die", "dem", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Qual und Schwermuth geben,", "tokens": ["Qual", "und", "Schwer\u00b7muth", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Kann ein Scherz oft heben:", "tokens": ["Kann", "ein", "Scherz", "oft", "he\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Auch der Scherz begl\u00fcckt.", "tokens": ["Auch", "der", "Scherz", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Land und Volk regieren,", "tokens": ["Land", "und", "Volk", "re\u00b7gie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ganze Heere f\u00fchren,", "tokens": ["Gan\u00b7ze", "Hee\u00b7re", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sich mit Purpur zieren,", "tokens": ["Sich", "mit", "Pur\u00b7pur", "zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Hemmt die Sorgen nie.", "tokens": ["Hemmt", "die", "Sor\u00b7gen", "nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Seht der Hirten Freuden,", "tokens": ["Seht", "der", "Hir\u00b7ten", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Die auf sichern Weiden", "tokens": ["Die", "auf", "si\u00b7chern", "Wei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Grosse nicht beneiden.", "tokens": ["Gros\u00b7se", "nicht", "be\u00b7nei\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Wie vergn\u00fcgt sind die!", "tokens": ["Wie", "ver\u00b7gn\u00fcgt", "sind", "die", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VAFIN", "ART", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "M\u00e4chtigen und Reichen", "tokens": ["M\u00e4ch\u00b7ti\u00b7gen", "und", "Rei\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Will kein Sch\u00e4fer gleichen:", "tokens": ["Will", "kein", "Sch\u00e4\u00b7fer", "glei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Jhrer Vorzugs-Zeichen", "tokens": ["Ih\u00b7rer", "Vor\u00b7zugs\u00b7Zei\u00b7chen"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Lacht der Hirten Zunft.", "tokens": ["Lacht", "der", "Hir\u00b7ten", "Zunft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Eintracht, Spiel und Scherzen", "tokens": ["Ein\u00b7tracht", ",", "Spiel", "und", "Scher\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Sch\u00fctzen ihre Herzen", "tokens": ["Sch\u00fct\u00b7zen", "ih\u00b7re", "Her\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Vor den eiteln Schmerzen", "tokens": ["Vor", "den", "ei\u00b7teln", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Stolzer Unvernunft.", "tokens": ["Stol\u00b7zer", "Un\u00b7ver\u00b7nunft", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}