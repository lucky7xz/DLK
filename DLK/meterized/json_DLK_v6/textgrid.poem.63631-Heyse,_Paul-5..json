{"textgrid.poem.63631": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor unsern Fenstern nachts erklingt die Zither;", "tokens": ["Vor", "un\u00b7sern", "Fens\u00b7tern", "nachts", "er\u00b7klingt", "die", "Zi\u00b7ther", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "H\u00f6rst du? ", "tokens": ["H\u00f6rst", "du", "?"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Wie klingt uns nun die s\u00fc\u00dfe Weise bitter,", "tokens": ["Wie", "klingt", "uns", "nun", "die", "s\u00fc\u00b7\u00dfe", "Wei\u00b7se", "bit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie w\u00fchlt sie aus dem Schlaf Erinnerungen!", "tokens": ["Wie", "w\u00fchlt", "sie", "aus", "dem", "Schlaf", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das Stimmchen, das geliebte, t\u00f6nt nicht wieder,", "tokens": ["Das", "Stimm\u00b7chen", ",", "das", "ge\u00b7lieb\u00b7te", ",", "t\u00f6nt", "nicht", "wie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das oft uns sang dies liebste seiner Lieder.", "tokens": ["Das", "oft", "uns", "sang", "dies", "liebs\u00b7te", "sei\u00b7ner", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PPER", "VVFIN", "PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Vom andern Ufer lockt es: ", "tokens": ["Vom", "an\u00b7dern", "U\u00b7fer", "lockt", "es", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}