{"dta.poem.21719": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Ihre Flur.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Abend bl&#252;ht.", "tokens": ["Der", "A\u00b7bend", "bl", "&#252;", "ht", "."], "token_info": ["word", "word", "word", "XML_entity", "word", "punct"], "pos": ["ART", "NN", "NE", "$(", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Arkona gl\u00fcht", "tokens": ["Ar\u00b7ko\u00b7na", "gl\u00fcht"], "token_info": ["word", "word"], "pos": ["NE", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Im Glanz der tiefgesunknen Sonne.", "tokens": ["Im", "Glanz", "der", "tief\u00b7ge\u00b7sun\u00b7knen", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es k\u00fcsst die See", "tokens": ["Es", "k\u00fcsst", "die", "See"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die Sinkende,", "tokens": ["Die", "Sin\u00b7ken\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.6": {"text": "Von Ehrfurcht schauernd und von Wonne.", "tokens": ["Von", "Ehr\u00b7furcht", "schau\u00b7ernd", "und", "von", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein grauer Duft", "tokens": ["Ein", "grau\u00b7er", "Duft"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Durchwebt die Luft,", "tokens": ["Durch\u00b7webt", "die", "Luft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Umschleyert Wittow's g\u00fcldne Auen.", "tokens": ["Um\u00b7schley\u00b7ert", "Wit\u00b7tow's", "g\u00fcld\u00b7ne", "Au\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es rauscht umher", "tokens": ["Es", "rauscht", "um\u00b7her"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Das d\u00fcstre Meer,", "tokens": ["Das", "d\u00fcst\u00b7re", "Meer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und rings herrscht ahnungreiches Grauen.", "tokens": ["Und", "rings", "herrscht", "ah\u00b7nun\u00b7grei\u00b7ches", "Grau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O trautes Land!", "tokens": ["O", "trau\u00b7tes", "Land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "O heil'ger Strand!", "tokens": ["O", "heil'\u00b7ger", "Strand", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "O Flur, die jede Flur verdunkelt.", "tokens": ["O", "Flur", ",", "die", "je\u00b7de", "Flur", "ver\u00b7dun\u00b7kelt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Flur, deren Schooss", "tokens": ["Flur", ",", "de\u00b7ren", "Scho\u00b7oss"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PRELAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Die Blum' entspross,", "tokens": ["Die", "Blum'", "ent\u00b7spross", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Die alle Blumen \u00fcberfunkelt.", "tokens": ["Die", "al\u00b7le", "Blu\u00b7men", "\u00fc\u00b7ber\u00b7fun\u00b7kelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Paart nicht den Schnee", "tokens": ["Paart", "nicht", "den", "Schnee"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Der Lilie", "tokens": ["Der", "Li\u00b7lie"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Die Holde mit der Glut der Rosen.", "tokens": ["Die", "Hol\u00b7de", "mit", "der", "Glut", "der", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Au', Ein Kranz", "tokens": ["Die", "Au'", ",", "Ein", "Kranz"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Voll Duft und Glanz", "tokens": ["Voll", "Duft", "und", "Glanz"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Reicht ihr den Preiss, der Tadellosen.", "tokens": ["Reicht", "ihr", "den", "Preiss", ",", "der", "Ta\u00b7del\u00b7lo\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ihr Ambraduft", "tokens": ["Ihr", "Am\u00b7bra\u00b7duft"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Durchweht die Luft,", "tokens": ["Durch\u00b7weht", "die", "Luft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und w\u00fcrzet rings die N\u00e4h' und Ferne.", "tokens": ["Und", "w\u00fcr\u00b7zet", "rings", "die", "N\u00e4h'", "und", "Fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und stirbt das Licht", "tokens": ["Und", "stirbt", "das", "Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Des Liedes nicht,", "tokens": ["Des", "Lie\u00b7des", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "So reicht ihr Nam' einst an die Sterne.", "tokens": ["So", "reicht", "ihr", "Nam'", "einst", "an", "die", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O trautes Land,", "tokens": ["O", "trau\u00b7tes", "Land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "O hehrer Strand,", "tokens": ["O", "heh\u00b7rer", "Strand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sey stolz auf deiner Blumen Blume.", "tokens": ["Sey", "stolz", "auf", "dei\u00b7ner", "Blu\u00b7men", "Blu\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das heil'ge Meer", "tokens": ["Das", "heil'\u00b7ge", "Meer"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und rings umher", "tokens": ["Und", "rings", "um\u00b7her"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Die Inseln huld'gen deinem Ruhme \u2014 \u2014", "tokens": ["Die", "In\u00b7seln", "huld'\u00b7gen", "dei\u00b7nem", "Ruh\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nacht h\u00fcllt den Strand.", "tokens": ["Nacht", "h\u00fcllt", "den", "Strand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Arkona schwand.", "tokens": ["Ar\u00b7ko\u00b7na", "schwand", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Verlodert sind des Spatroths Gluthen.", "tokens": ["Ver\u00b7lo\u00b7dert", "sind", "des", "Spat\u00b7roths", "Glu\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Weltmeer grollt,", "tokens": ["Das", "Welt\u00b7meer", "grollt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und gluthroth rollt", "tokens": ["Und", "gluth\u00b7roth", "rollt"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Der Vollmond aus den d\u00fcstern Fluthen.", "tokens": ["Der", "Voll\u00b7mond", "aus", "den", "d\u00fcs\u00b7tern", "Flut\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}