{"textgrid.poem.24617": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Als einer in einer gro\u00dfen und ber\u00fchmten Stadt B\u00fcrgermeister ward", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer offt die See durchf\u00e4hrt/ und in den tiefen Wellen/", "tokens": ["Wer", "offt", "die", "See", "durch\u00b7f\u00e4hrt", "/", "und", "in", "den", "tie\u00b7fen", "Wel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVPP", "$(", "KON", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Klippen kennen lernt/ ja ob es Sturm und Wind/", "tokens": ["Die", "Klip\u00b7pen", "ken\u00b7nen", "lernt", "/", "ja", "ob", "es", "Sturm", "und", "Wind", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$(", "ADV", "KOUS", "PPER", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch nach dem Himmel wei\u00df den Schiff-Compass zu stellen/", "tokens": ["Doch", "nach", "dem", "Him\u00b7mel", "wei\u00df", "den", "Schiff\u00b7Com\u00b7pass", "zu", "stel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da sichre Wege macht/ wo sonsten keine sind/", "tokens": ["Da", "sich\u00b7re", "We\u00b7ge", "macht", "/", "wo", "sons\u00b7ten", "kei\u00b7ne", "sind", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "VVFIN", "$(", "PWAV", "ADV", "PIAT", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Verdienet da\u00df wir ihm ein Schiff allein vertrauen.", "tokens": ["Ver\u00b7die\u00b7net", "da\u00df", "wir", "ihm", "ein", "Schiff", "al\u00b7lein", "ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "PPER", "PPER", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und so regieret auch in einer Stadt geschickt/", "tokens": ["Und", "so", "re\u00b7gie\u00b7ret", "auch", "in", "ei\u00b7ner", "Stadt", "ge\u00b7schickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der sich in Sturm getrost/ klug in Gefahr l\u00e4st schauen/", "tokens": ["Der", "sich", "in", "Sturm", "ge\u00b7trost", "/", "klug", "in", "Ge\u00b7fahr", "l\u00e4st", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "VVPP", "$(", "ADJD", "APPR", "NN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und zwischen Felsen/ Sand/ auch Wege hat erblickt.", "tokens": ["Und", "zwi\u00b7schen", "Fel\u00b7sen", "/", "Sand", "/", "auch", "We\u00b7ge", "hat", "er\u00b7blickt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$(", "NN", "$(", "ADV", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wer/ Hochgeprie\u00dfner Mann/ die vielen Jahre zehlet/", "tokens": ["Wer", "/", "Hoch\u00b7ge\u00b7prie\u00df\u00b7ner", "Mann", "/", "die", "vie\u00b7len", "Jah\u00b7re", "zeh\u00b7let", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$(", "ADJA", "NN", "$(", "ART", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "In welchen du der Stadt bey mancher Noht gedient/", "tokens": ["In", "wel\u00b7chen", "du", "der", "Stadt", "bey", "man\u00b7cher", "Noht", "ge\u00b7dient", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PPER", "ART", "NN", "APPR", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der spricht aus Danckbarkeit: Gott hat dich auserwehlet/", "tokens": ["Der", "spricht", "aus", "Dan\u00b7ck\u00b7bar\u00b7keit", ":", "Gott", "hat", "dich", "au\u00b7ser\u00b7weh\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "$.", "NN", "VAFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Da\u00df durch dein weises Haupt die Wohlfahrt \u2013 \u2013 gr\u00fcn't.", "tokens": ["Da\u00df", "durch", "dein", "wei\u00b7ses", "Haupt", "die", "Wohl\u00b7fahrt", "\u2013", "\u2013", "gr\u00fcn'", "t."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "abbreviation"], "pos": ["KOUS", "APPR", "PPOSAT", "ADJA", "NN", "ART", "NN", "$(", "$(", "XY", "XY"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Gott hat dich aus ersehn: Was aber Gott erkohren/", "tokens": ["Gott", "hat", "dich", "aus", "er\u00b7sehn", ":", "Was", "a\u00b7ber", "Gott", "er\u00b7koh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PRF", "APPR", "VVINF", "$.", "PWS", "ADV", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dem fehlt an W\u00fcrden nichts/ der ist vors Vaterland", "tokens": ["Dem", "fehlt", "an", "W\u00fcr\u00b7den", "nichts", "/", "der", "ist", "vors", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "NN", "PIS", "$(", "ART", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Dem Geist und Leibe nach/ wie du zweyfach gebohren/", "tokens": ["Dem", "Geist", "und", "Lei\u00b7be", "nach", "/", "wie", "du", "zwey\u00b7fach", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "$(", "PWAV", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Und wird ein Kind der Stadt und Vater auch genannt.", "tokens": ["Und", "wird", "ein", "Kind", "der", "Stadt", "und", "Va\u00b7ter", "auch", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "NN", "KON", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der hat den klugen Kopf mit Wissenschafft geschm\u00fccket/", "tokens": ["Der", "hat", "den", "klu\u00b7gen", "Kopf", "mit", "Wis\u00b7sen\u00b7schafft", "ge\u00b7schm\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Das Hertz mit Tugenden/ den Geist mit grauer Zeit/", "tokens": ["Das", "Hertz", "mit", "Tu\u00b7gen\u00b7den", "/", "den", "Geist", "mit", "grau\u00b7er", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$(", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die wahre Weisheit bringt/ die Vater-Stadt begl\u00fccket/", "tokens": ["Die", "wah\u00b7re", "Weis\u00b7heit", "bringt", "/", "die", "Va\u00b7ter\u00b7Stadt", "be\u00b7gl\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und steiget/ gleich wie du/ nach viel Erfahrenheit.", "tokens": ["Und", "stei\u00b7get", "/", "gleich", "wie", "du", "/", "nach", "viel", "Er\u00b7fah\u00b7ren\u00b7heit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "ADV", "KOKOM", "PPER", "$(", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ist unser \u2013 \u2013 \u2013 \u2013 nicht bisher ein Ziel gewesen/", "tokens": ["Ist", "un\u00b7ser", "\u2013", "\u2013", "\u2013", "\u2013", "nicht", "bis\u00b7her", "ein", "Ziel", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "$(", "$(", "$(", "$(", "PTKNEG", "ADV", "ART", "NN", "VAPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Nachdem des Todes Grimm die Pfeile fliegen lie\u00df?", "tokens": ["Nach\u00b7dem", "des", "To\u00b7des", "Grimm", "die", "Pfei\u00b7le", "flie\u00b7gen", "lie\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NE", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Das Ungl\u00fcck hat es auch zum Sammel-Platz erlesen/", "tokens": ["Das", "Un\u00b7gl\u00fcck", "hat", "es", "auch", "zum", "Sam\u00b7mel\u00b7Platz", "er\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Zum Felde/ wo gar oft die Geldsucht lermen blie\u00df.", "tokens": ["Zum", "Fel\u00b7de", "/", "wo", "gar", "oft", "die", "Geld\u00b7sucht", "ler\u00b7men", "blie\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$(", "PWAV", "ADV", "ADV", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "In aller dieser Noht/ in diesen b\u00f6sen Tagen/", "tokens": ["In", "al\u00b7ler", "die\u00b7ser", "Noht", "/", "in", "die\u00b7sen", "b\u00f6\u00b7sen", "Ta\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PDAT", "NN", "$(", "APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ist dein Gem\u00fcht bewehrt/ klug und geschickt gemacht.", "tokens": ["Ist", "dein", "Ge\u00b7m\u00fcht", "be\u00b7wehrt", "/", "klug", "und", "ge\u00b7schickt", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$(", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wie nun dein Helden-Muht viel leiden kan ertragen.", "tokens": ["Wie", "nun", "dein", "Hel\u00b7den\u00b7\u00b7M\u00b7uht", "viel", "lei\u00b7den", "kan", "er\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "ADV", "VVINF", "VMFIN", "VVINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "So ist dein weises Haupt auf Heil davor bedacht.", "tokens": ["So", "ist", "dein", "wei\u00b7ses", "Haupt", "auf", "Heil", "da\u00b7vor", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der Perle Kostbarkeit zeugt sich gleich den Corallen", "tokens": ["Der", "Per\u00b7le", "Kost\u00b7bar\u00b7keit", "zeugt", "sich", "gleich", "den", "Co\u00b7ral\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "PRF", "ADV", "ART", "NN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Im ungest\u00fcmen Meer. Auf hohen Vergen stehn", "tokens": ["Im", "un\u00b7ge\u00b7st\u00fc\u00b7men", "Meer", ".", "Auf", "ho\u00b7hen", "Ver\u00b7gen", "stehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$.", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die Cedern unbewegt: an statt von Sturm zu fallen/", "tokens": ["Die", "Ce\u00b7dern", "un\u00b7be\u00b7wegt", ":", "an", "statt", "von", "Sturm", "zu", "fal\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$.", "APPR", "ADJD", "APPR", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Mu\u00df zur Standhafftigkeit die Wurtzel tiefer gehn.", "tokens": ["Mu\u00df", "zur", "Stand\u00b7haff\u00b7tig\u00b7keit", "die", "Wurt\u00b7zel", "tie\u00b7fer", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.33": {"text": "Du Hochbewehrter Mann/ du wirst in diesen Zeiten", "tokens": ["Du", "Hoch\u00b7be\u00b7wehr\u00b7ter", "Mann", "/", "du", "wirst", "in", "die\u00b7sen", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$(", "PPER", "VAFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Uns ein vortrefflich Gut/ im Sturm die Perle seyn;", "tokens": ["Uns", "ein", "vor\u00b7treff\u00b7lich", "Gut", "/", "im", "Sturm", "die", "Per\u00b7le", "seyn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJD", "ADJD", "$(", "APPRART", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die Ceder dieser Stadt/ wenn Wind und Wetter streiten.", "tokens": ["Die", "Ce\u00b7der", "die\u00b7ser", "Stadt", "/", "wenn", "Wind", "und", "Wet\u00b7ter", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$(", "KOUS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Auf Bergen unsers Heils bestehst du ungemein.", "tokens": ["Auf", "Ber\u00b7gen", "un\u00b7sers", "Heils", "be\u00b7stehst", "du", "un\u00b7ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Di\u00df alles kan die Stadt wie mein Gem\u00fcht erqvicken/", "tokens": ["Di\u00df", "al\u00b7les", "kan", "die", "Stadt", "wie", "mein", "Ge\u00b7m\u00fcht", "er\u00b7qvi\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VMFIN", "ART", "NN", "KOKOM", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Er ruft derselben Geist mit meinen Hertzen aus:", "tokens": ["Er", "ruft", "der\u00b7sel\u00b7ben", "Geist", "mit", "mei\u00b7nen", "Hert\u00b7zen", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Es wolle/ theurer Mann/ der h\u00f6chste dich begl\u00fccken/", "tokens": ["Es", "wol\u00b7le", "/", "theu\u00b7rer", "Mann", "/", "der", "h\u00f6chs\u00b7te", "dich", "be\u00b7gl\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "ADJD", "NN", "$(", "ART", "ADJA", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und aller Sturm sey schwach/ auf dein Hochadlichs Haus.", "tokens": ["Und", "al\u00b7ler", "Sturm", "sey", "schwach", "/", "auf", "dein", "Hoc\u00b7had\u00b7lichs", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "