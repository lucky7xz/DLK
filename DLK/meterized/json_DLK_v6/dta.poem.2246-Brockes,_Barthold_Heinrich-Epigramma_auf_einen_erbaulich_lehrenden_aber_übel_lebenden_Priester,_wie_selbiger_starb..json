{"dta.poem.2246": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Epigramma  \n auf einen erbaulich lehrenden  \n  aber \u00fcbel lebenden Priester,  \n  wie selbiger starb.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist, das ist nicht, was es ist, es ist, f\u00fcr uns", "tokens": ["Was", "ist", ",", "das", "ist", "nicht", ",", "was", "es", "ist", ",", "es", "ist", ",", "f\u00fcr", "uns"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "PDS", "VAFIN", "PTKNEG", "$,", "PWS", "PPER", "VAFIN", "$,", "PPER", "VAFIN", "$,", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "nur, was es scheinet.", "tokens": ["nur", ",", "was", "es", "schei\u00b7net", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Obs Aug\u2019, als ein Vergr\u00f6\u00dfrungsglas, der Dinge We-", "tokens": ["Obs", "Aug'", ",", "als", "ein", "Ver\u00b7gr\u00f6\u00df\u00b7rungs\u00b7glas", ",", "der", "Din\u00b7ge", "We"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KOUS", "ART", "NN", "$,", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "sen gr\u00f6\u00dfer sieht,", "tokens": ["sen", "gr\u00f6\u00b7\u00dfer", "sieht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ob es dieselbigen verkleint und sie in engre Grenzen zieht,", "tokens": ["Ob", "es", "die\u00b7sel\u00b7bi\u00b7gen", "ver\u00b7kleint", "und", "sie", "in", "en\u00b7gre", "Gren\u00b7zen", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "KON", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-++-+-+-+-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.6": {"text": "Wie, oder wahre Gr\u00f6\u00dfen schauet; das ist, was man", "tokens": ["Wie", ",", "o\u00b7der", "wah\u00b7re", "Gr\u00f6\u00b7\u00dfen", "schau\u00b7et", ";", "das", "ist", ",", "was", "man"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "$,", "KON", "ADJA", "NN", "VVFIN", "$.", "PDS", "VAFIN", "$,", "PRELS", "PIS"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "nicht weis, nur meynet.", "tokens": ["nicht", "weis", ",", "nur", "mey\u00b7net", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "PTKVZ", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}