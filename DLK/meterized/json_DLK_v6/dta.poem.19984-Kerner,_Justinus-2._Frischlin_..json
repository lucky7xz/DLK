{"dta.poem.19984": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "2.  \n  Frischlin .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihn schlossen sie in starre Felsen ein,", "tokens": ["Ihn", "schlos\u00b7sen", "sie", "in", "star\u00b7re", "Fel\u00b7sen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihn, dem zu eng der Erde weite Lande.", "tokens": ["Ihn", ",", "dem", "zu", "eng", "der", "Er\u00b7de", "wei\u00b7te", "Lan\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PTKA", "ADJD", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er doch, voll Kraft, zerbrach den Felsenstein,", "tokens": ["Er", "doch", ",", "voll", "Kraft", ",", "zer\u00b7brach", "den", "Fel\u00b7sen\u00b7stein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJD", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und lie\u00df sich abw\u00e4rts am unsichern Bande.", "tokens": ["Und", "lie\u00df", "sich", "ab\u00b7w\u00e4rts", "am", "un\u00b7si\u00b7chern", "Ban\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Da fanden sie im bleichen Mondenschein", "tokens": ["Da", "fan\u00b7den", "sie", "im", "blei\u00b7chen", "Mon\u00b7den\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zerschmettert ihn, zerrissen die Gewande.", "tokens": ["Zer\u00b7schmet\u00b7tert", "ihn", ",", "zer\u00b7ris\u00b7sen", "die", "Ge\u00b7wan\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Weh! Muttererde, da\u00df mit linden Armen", "tokens": ["Weh", "!", "Mut\u00b7ter\u00b7er\u00b7de", ",", "da\u00df", "mit", "lin\u00b7den", "Ar\u00b7men"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "NN", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Du ihn nicht auffiengst, sch\u00fctzend, voll Erbarmen.", "tokens": ["Du", "ihn", "nicht", "auf\u00b7fiengst", ",", "sch\u00fct\u00b7zend", ",", "voll", "Er\u00b7bar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPER", "PTKNEG", "VVFIN", "$,", "ADJD", "$,", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}