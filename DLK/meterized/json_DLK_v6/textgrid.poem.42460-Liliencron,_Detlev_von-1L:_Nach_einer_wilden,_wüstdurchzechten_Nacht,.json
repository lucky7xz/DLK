{"textgrid.poem.42460": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nach einer wilden, w\u00fcstdurchzechten Nacht,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach einer wilden, w\u00fcstdurchzechten Nacht,", "tokens": ["Nach", "ei\u00b7ner", "wil\u00b7den", ",", "w\u00fcst\u00b7durch\u00b7zech\u00b7ten", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schon r\u00e4nderte das erste Rot die Wolken,", "tokens": ["Schon", "r\u00e4n\u00b7der\u00b7te", "das", "ers\u00b7te", "Rot", "die", "Wol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Stahl ich mich aus dem Saale, die Genossen", "tokens": ["Stahl", "ich", "mich", "aus", "dem", "Saa\u00b7le", ",", "die", "Ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "PRF", "APPR", "ART", "NN", "$,", "ART", "NN"], "meter": "+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Im Streite, lachend, lallend, unter'm Tische,", "tokens": ["Im", "Strei\u00b7te", ",", "la\u00b7chend", ",", "lal\u00b7lend", ",", "un\u00b7ter'm", "Ti\u00b7sche", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "$,", "ADJD", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Im weinerlichen Elend, schwer betrunken", "tokens": ["Im", "wei\u00b7ner\u00b7li\u00b7chen", "E\u00b7lend", ",", "schwer", "be\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zur\u00fcck in ihrem Durcheinander lassend.", "tokens": ["Zu\u00b7r\u00fcck", "in", "ih\u00b7rem", "Durch\u00b7ein\u00b7an\u00b7der", "las\u00b7send", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch eh' ich ging, bat einen meiner Runde", "tokens": ["Doch", "eh'", "ich", "ging", ",", "bat", "ei\u00b7nen", "mei\u00b7ner", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "ART", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich mitzugehn, um frische Luft zu sch\u00f6pfen.", "tokens": ["Ich", "mit\u00b7zu\u00b7gehn", ",", "um", "fri\u00b7sche", "Luft", "zu", "sch\u00f6p\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "KOUI", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Im Nebenzimmer, das wir nun durchschritten,", "tokens": ["Im", "Ne\u00b7ben\u00b7zim\u00b7mer", ",", "das", "wir", "nun", "durch\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Stand ein Klavier, und wie dort hingezogen,", "tokens": ["Stand", "ein", "Kla\u00b7vier", ",", "und", "wie", "dort", "hin\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KON", "PWAV", "ADV", "VVPP", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "Setzt an die Tasten sich mein junger Freund", "tokens": ["Setzt", "an", "die", "Tas\u00b7ten", "sich", "mein", "jun\u00b7ger", "Freund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "PRF", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und spielte die Ballade G-moll Chopins.", "tokens": ["Und", "spiel\u00b7te", "die", "Bal\u00b7la\u00b7de", "G\u00b7\u00b7moll", "Cho\u00b7pins", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NE", "NE", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.13": {"text": "Und wie vom Geist des Weines nur befeuert,", "tokens": ["Und", "wie", "vom", "Geist", "des", "Wei\u00b7nes", "nur", "be\u00b7feu\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPRART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Begeistert nur zu h\u00f6herem Seelenflug,", "tokens": ["Be\u00b7geis\u00b7tert", "nur", "zu", "h\u00f6\u00b7he\u00b7rem", "See\u00b7len\u00b7flug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Erwuchs zu m\u00e4chtigem Wesen jenes St\u00fcck.", "tokens": ["Er\u00b7wuchs", "zu", "m\u00e4ch\u00b7ti\u00b7gem", "We\u00b7sen", "je\u00b7nes", "St\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKZU", "ADJA", "NN", "PDAT", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Nie hatt' ich herrlicher sie spielen h\u00f6ren.", "tokens": ["Nie", "hatt'", "ich", "herr\u00b7li\u00b7cher", "sie", "spie\u00b7len", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Ich unterdessen schlich zum Fenster hin", "tokens": ["Ich", "un\u00b7ter\u00b7des\u00b7sen", "schlich", "zum", "Fens\u00b7ter", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und schlug die Fl\u00fcgel auf, soweit ich konnte.", "tokens": ["Und", "schlug", "die", "Fl\u00fc\u00b7gel", "auf", ",", "so\u00b7weit", "ich", "konn\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KOUS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Der Sommermorgen friedet keusch vor mir,", "tokens": ["Der", "Som\u00b7mer\u00b7mor\u00b7gen", "frie\u00b7det", "keusch", "vor", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Das Gras, die Blumen schlafen noch im Tau,", "tokens": ["Das", "Gras", ",", "die", "Blu\u00b7men", "schla\u00b7fen", "noch", "im", "Tau", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Kein L\u00fcftchen regte sich, kein Vogel zwitschert.", "tokens": ["Kein", "L\u00fcft\u00b7chen", "reg\u00b7te", "sich", ",", "kein", "Vo\u00b7gel", "zwit\u00b7schert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "$,", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.22": {"text": "Doch da, in dieser leidenlosen Ruhe,", "tokens": ["Doch", "da", ",", "in", "die\u00b7ser", "lei\u00b7den\u00b7lo\u00b7sen", "Ru\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Entdeckt' an einem schm\u00e4chtigen Ahornstamm", "tokens": ["Ent\u00b7deckt'", "an", "ei\u00b7nem", "schm\u00e4ch\u00b7ti\u00b7gen", "A\u00b7horns\u00b7tamm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Ein blasses M\u00e4dchen ich. Die rechte Schl\u00e4fe", "tokens": ["Ein", "blas\u00b7ses", "M\u00e4d\u00b7chen", "ich", ".", "Die", "rech\u00b7te", "Schl\u00e4\u00b7fe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Lehnt an den Baum; und aus den gro\u00dfen Augen", "tokens": ["Lehnt", "an", "den", "Baum", ";", "und", "aus", "den", "gro\u00b7\u00dfen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "$.", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.26": {"text": "Tropft Thr\u00e4n' auf Thr\u00e4ne langsam auf die H\u00e4nde,", "tokens": ["Tropft", "Thr\u00e4n'", "auf", "Thr\u00e4\u00b7ne", "lang\u00b7sam", "auf", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Die schwach das Taschent\u00fcchlein drehn und zupfen", "tokens": ["Die", "schwach", "das", "Ta\u00b7schen\u00b7t\u00fcch\u00b7lein", "drehn", "und", "zup\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Und zitternd auseinanderzerren ...", "tokens": ["Und", "zit\u00b7ternd", "aus\u00b7ein\u00b7an\u00b7der\u00b7zer\u00b7ren", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVIZU", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}