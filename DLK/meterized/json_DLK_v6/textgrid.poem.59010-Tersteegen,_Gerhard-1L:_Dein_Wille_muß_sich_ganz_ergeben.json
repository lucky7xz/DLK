{"textgrid.poem.59010": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dein Wille mu\u00df sich ganz ergeben", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Wille mu\u00df sich ganz ergeben", "tokens": ["Dein", "Wil\u00b7le", "mu\u00df", "sich", "ganz", "er\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PRF", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wahrheit; hast du Wahrheitsgrund,", "tokens": ["Der", "Wahr\u00b7heit", ";", "hast", "du", "Wahr\u00b7heits\u00b7grund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Beweis' die Wahrheit mit dem Leben,", "tokens": ["Be\u00b7weis'", "die", "Wahr\u00b7heit", "mit", "dem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sonst wird sie L\u00fcg'n in deinem Mund!", "tokens": ["Sonst", "wird", "sie", "L\u00fcg'n", "in", "dei\u00b7nem", "Mund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}