{"textgrid.poem.34981": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist Herr Ludwig von Bayerland,", "tokens": ["Das", "ist", "Herr", "Lud\u00b7wig", "von", "Bay\u00b7er\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "NE", "APPR", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Desgleichen gibt es wenig;", "tokens": ["Des\u00b7glei\u00b7chen", "gibt", "es", "we\u00b7nig", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Volk der Bavaren verehrt in ihm", "tokens": ["Das", "Volk", "der", "Ba\u00b7va\u00b7ren", "ver\u00b7ehrt", "in", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "APPR", "PPER"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Den angestammelten K\u00f6nig.", "tokens": ["Den", "an\u00b7ge\u00b7stam\u00b7mel\u00b7ten", "K\u00f6\u00b7nig", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Er liebt die Kunst, und die sch\u00f6nsten Fraun,", "tokens": ["Er", "liebt", "die", "Kunst", ",", "und", "die", "sch\u00f6ns\u00b7ten", "Fraun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die l\u00e4\u00dft er portr\u00e4tieren;", "tokens": ["Die", "l\u00e4\u00dft", "er", "por\u00b7tr\u00e4\u00b7tie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er geht in diesem gemalten Serail", "tokens": ["Er", "geht", "in", "die\u00b7sem", "ge\u00b7mal\u00b7ten", "Se\u00b7rail"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als Kunsteunuch spazieren.", "tokens": ["Als", "Kuns\u00b7teu\u00b7nuch", "spa\u00b7zie\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Bei Regensburg l\u00e4\u00dft er erbaun", "tokens": ["Bei", "Re\u00b7gens\u00b7burg", "l\u00e4\u00dft", "er", "er\u00b7baun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "PPER", "VVINF"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Eine marmorne Sch\u00e4delst\u00e4tte,", "tokens": ["Ei\u00b7ne", "mar\u00b7mor\u00b7ne", "Sch\u00e4\u00b7del\u00b7st\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und er hat h\u00f6chstselbst f\u00fcr jeden Kopf", "tokens": ["Und", "er", "hat", "h\u00f6chst\u00b7selbst", "f\u00fcr", "je\u00b7den", "Kopf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Verfertigt die Etikette.", "tokens": ["Ver\u00b7fer\u00b7tigt", "die", "E\u00b7ti\u00b7ket\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "\u00bbwalhallagenossen\u00ab, ein Meisterwerk,", "tokens": ["\u00bb", "wal\u00b7hal\u00b7la\u00b7ge\u00b7nos\u00b7sen", "\u00ab", ",", "ein", "Meis\u00b7ter\u00b7werk", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "VVINF", "$(", "$,", "ART", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Worin er jedweden Mannes", "tokens": ["Wo\u00b7rin", "er", "jed\u00b7we\u00b7den", "Man\u00b7nes"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PIAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Verdienste, Charakter und Taten ger\u00fchmt,", "tokens": ["Ver\u00b7diens\u00b7te", ",", "Cha\u00b7rak\u00b7ter", "und", "Ta\u00b7ten", "ge\u00b7r\u00fchmt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Von Teut bis Schinderhannes.", "tokens": ["Von", "Teut", "bis", "Schin\u00b7der\u00b7han\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nur Luther, der Dickkopf, fehlt in Walhall,", "tokens": ["Nur", "Lu\u00b7ther", ",", "der", "Dick\u00b7kopf", ",", "fehlt", "in", "Wal\u00b7hall", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$,", "ART", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und es feiert ihn nicht der Walhall-Wisch;", "tokens": ["Und", "es", "fei\u00b7ert", "ihn", "nicht", "der", "Wal\u00b7hall\u00b7Wisch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "In Naturaliensammlungen fehlt", "tokens": ["In", "Na\u00b7tu\u00b7ra\u00b7li\u00b7en\u00b7samm\u00b7lun\u00b7gen", "fehlt"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVFIN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Oft unter den Fischen der Walfisch.", "tokens": ["Oft", "un\u00b7ter", "den", "Fi\u00b7schen", "der", "Wal\u00b7fisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--++", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Herr Ludwig ist ein gro\u00dfer Poet,", "tokens": ["Herr", "Lud\u00b7wig", "ist", "ein", "gro\u00b7\u00dfer", "Po\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und singt er, so st\u00fcrzt Apollo", "tokens": ["Und", "singt", "er", ",", "so", "st\u00fcrzt", "A\u00b7pol\u00b7lo"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor ihm auf die Knie und bittet und fleht:", "tokens": ["Vor", "ihm", "auf", "die", "Knie", "und", "bit\u00b7tet", "und", "fleht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "NN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "\u00bbhalt ein! ich werde sonst toll, oh!\u00ab", "tokens": ["\u00bb", "halt", "ein", "!", "ich", "wer\u00b7de", "sonst", "toll", ",", "oh", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$.", "PPER", "VAFIN", "ADV", "ADJD", "$,", "FM", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Herr Ludwig ist ein mutiger Held,", "tokens": ["Herr", "Lud\u00b7wig", "ist", "ein", "mu\u00b7ti\u00b7ger", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wie Otto, das Kind, sein S\u00f6hnchen;", "tokens": ["Wie", "Ot\u00b7to", ",", "das", "Kind", ",", "sein", "S\u00f6hn\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der kriegte den Durchfall zu Athen,", "tokens": ["Der", "krieg\u00b7te", "den", "Durch\u00b7fall", "zu", "A\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und hat dort besudelt sein Thr\u00f6nchen.", "tokens": ["Und", "hat", "dort", "be\u00b7su\u00b7delt", "sein", "Thr\u00f6n\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Stirbt einst Herr Ludwig, so kanonisiert", "tokens": ["Stirbt", "einst", "Herr", "Lud\u00b7wig", ",", "so", "ka\u00b7no\u00b7ni\u00b7siert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "NN", "NE", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu Rom ihn der Heilige Vater \u2013", "tokens": ["Zu", "Rom", "ihn", "der", "Hei\u00b7li\u00b7ge", "Va\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die Glorie pa\u00dft f\u00fcr ein solches Gesicht,", "tokens": ["Die", "Glo\u00b7rie", "pa\u00dft", "f\u00fcr", "ein", "sol\u00b7ches", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "PIAT", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie Manschetten f\u00fcr unseren Kater!", "tokens": ["Wie", "Man\u00b7schet\u00b7ten", "f\u00fcr", "un\u00b7se\u00b7ren", "Ka\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.9": {"line.1": {"text": "Sobald auch die Affen und K\u00e4nguruhs", "tokens": ["So\u00b7bald", "auch", "die", "Af\u00b7fen", "und", "K\u00e4n\u00b7gu\u00b7ruhs"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "KON", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Zum Christentum sich bekehren,", "tokens": ["Zum", "Chris\u00b7ten\u00b7tum", "sich", "be\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie werden gewi\u00df Sankt Ludewig", "tokens": ["Sie", "wer\u00b7den", "ge\u00b7wi\u00df", "Sankt", "Lu\u00b7de\u00b7wig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als Schutzpatron verehren.", "tokens": ["Als", "Schutz\u00b7pat\u00b7ron", "ver\u00b7eh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}