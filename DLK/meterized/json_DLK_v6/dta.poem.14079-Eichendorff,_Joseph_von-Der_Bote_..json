{"dta.poem.14079": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Bote .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Schwang sich auf dann vom Gitter", "tokens": ["Schwang", "sich", "auf", "dann", "vom", "Git\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ADV", "APPRART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Ueber die Berge, \u00fcber'n Wald \u2014", "tokens": ["Ue\u00b7ber", "die", "Ber\u00b7ge", ",", "\u00fc\u00b7ber'n", "Wald"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPRART", "NN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Mein Herz ist die Zitter,", "tokens": ["Mein", "Herz", "ist", "die", "Zit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Giebt ein'n fr\u00f6hlichen Schall.", "tokens": ["Giebt", "ein'n", "fr\u00f6h\u00b7li\u00b7chen", "Schall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}