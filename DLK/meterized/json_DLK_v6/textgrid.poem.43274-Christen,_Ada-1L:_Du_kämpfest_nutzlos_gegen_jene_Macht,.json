{"textgrid.poem.43274": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du k\u00e4mpfest nutzlos gegen jene Macht,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du k\u00e4mpfest nutzlos gegen jene Macht,", "tokens": ["Du", "k\u00e4mp\u00b7fest", "nutz\u00b7los", "ge\u00b7gen", "je\u00b7ne", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die alle Worte nicht ersch\u00f6pfend nennen,", "tokens": ["Die", "al\u00b7le", "Wor\u00b7te", "nicht", "er\u00b7sch\u00f6p\u00b7fend", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PTKNEG", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Woran die Brust wir stets uns blutig rennen,", "tokens": ["Wo\u00b7ran", "die", "Brust", "wir", "stets", "uns", "blu\u00b7tig", "ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVFIN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die unsre tiefsten Schmerzen frech verlacht.", "tokens": ["Die", "uns\u00b7re", "tiefs\u00b7ten", "Schmer\u00b7zen", "frech", "ver\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was liebevoll der Welt Du zugebracht,", "tokens": ["Was", "lie\u00b7be\u00b7voll", "der", "Welt", "Du", "zu\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wof\u00fcr begeistert treue Herzen brennen,", "tokens": ["Wo\u00b7f\u00fcr", "be\u00b7geis\u00b7tert", "treu\u00b7e", "Her\u00b7zen", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es scheitert doch ... Du wirst es noch erkennen", "tokens": ["Es", "schei\u00b7tert", "doch", "...", "Du", "wirst", "es", "noch", "er\u00b7ken\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$(", "PPER", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An des Gemeinen ewig starker Macht.", "tokens": ["An", "des", "Ge\u00b7mei\u00b7nen", "e\u00b7wig", "star\u00b7ker", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}