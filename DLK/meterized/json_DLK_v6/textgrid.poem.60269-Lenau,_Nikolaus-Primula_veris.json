{"textgrid.poem.60269": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Primula veris", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebliche Blume,", "tokens": ["Lieb\u00b7li\u00b7che", "Blu\u00b7me", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Bist du so fr\u00fch schon", "tokens": ["Bist", "du", "so", "fr\u00fch", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "ADV"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wiedergekommen?", "tokens": ["Wie\u00b7der\u00b7ge\u00b7kom\u00b7men", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Sei mir gegr\u00fc\u00dfet,", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Primula veris!", "tokens": ["Pri\u00b7mu\u00b7la", "ve\u00b7ris", "!"], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Leiser denn alle", "tokens": ["Lei\u00b7ser", "denn", "al\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "PIS"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Blumen der Wiese", "tokens": ["Blu\u00b7men", "der", "Wie\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Hast du geschlummert,", "tokens": ["Hast", "du", "ge\u00b7schlum\u00b7mert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Liebliche Blume,", "tokens": ["Lieb\u00b7li\u00b7che", "Blu\u00b7me", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Primula veris!", "tokens": ["Pri\u00b7mu\u00b7la", "ve\u00b7ris", "!"], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Dir nur vernehmbar", "tokens": ["Dir", "nur", "ver\u00b7nehm\u00b7bar"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADV", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Lockte das erste", "tokens": ["Lock\u00b7te", "das", "ers\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sanfte Gefl\u00fcster", "tokens": ["Sanf\u00b7te", "Ge\u00b7fl\u00fcs\u00b7ter"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Weckenden Fr\u00fchlings,", "tokens": ["We\u00b7cken\u00b7den", "Fr\u00fch\u00b7lings", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Primula veris!", "tokens": ["Pri\u00b7mu\u00b7la", "ve\u00b7ris", "!"], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Mir auch im Herzen", "tokens": ["Mir", "auch", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPRART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Bl\u00fchte vor Zeiten,", "tokens": ["Bl\u00fch\u00b7te", "vor", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sch\u00f6ner denn alle", "tokens": ["Sch\u00f6\u00b7ner", "denn", "al\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "PIS"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Blumen der Liebe,", "tokens": ["Blu\u00b7men", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Primula veris!", "tokens": ["Pri\u00b7mu\u00b7la", "ve\u00b7ris", "!"], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}