{"textgrid.poem.45589": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[bestellt als eines Volkes Leiter]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bestellt als eines Volkes Leiter,", "tokens": ["Be\u00b7stellt", "als", "ei\u00b7nes", "Vol\u00b7kes", "Lei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zeigst du dich s\u00fc\u00df und liebenswert;", "tokens": ["Zeigst", "du", "dich", "s\u00fc\u00df", "und", "lie\u00b7bens\u00b7wert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So tun wohl auch die schlechten Reiter,", "tokens": ["So", "tun", "wohl", "auch", "die", "schlech\u00b7ten", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die streicheln immerfort das Pferd.", "tokens": ["Die", "strei\u00b7cheln", "im\u00b7mer\u00b7fort", "das", "Pferd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}