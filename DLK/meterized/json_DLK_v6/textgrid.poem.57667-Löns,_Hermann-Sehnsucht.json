{"textgrid.poem.57667": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Sehnsucht", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Lungenblumen bl\u00fchen", "tokens": ["Die", "Lun\u00b7gen\u00b7blu\u00b7men", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus dunkelgr\u00fcnem Moos,", "tokens": ["Aus", "dun\u00b7kel\u00b7gr\u00fc\u00b7nem", "Moos", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Herz das bebt und zittert", "tokens": ["Mein", "Herz", "das", "bebt", "und", "zit\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PDS", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Meine Sehnsucht ist so gro\u00df.", "tokens": ["Mei\u00b7ne", "Sehn\u00b7sucht", "ist", "so", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Die beiden blauen Bl\u00fcten", "tokens": ["Die", "bei\u00b7den", "blau\u00b7en", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erinnern mich so sehr", "tokens": ["E\u00b7rin\u00b7nern", "mich", "so", "sehr"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An deine lieben Augen,", "tokens": ["An", "dei\u00b7ne", "lie\u00b7ben", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Herz das schl\u00e4gt so schwer.", "tokens": ["Mein", "Herz", "das", "schl\u00e4gt", "so", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDS", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es geht ein Zittern und Beben", "tokens": ["Es", "geht", "ein", "Zit\u00b7tern", "und", "Be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Durch meiner Seele Grund,", "tokens": ["Durch", "mei\u00b7ner", "See\u00b7le", "Grund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Rot ist die eine Bl\u00fcte,", "tokens": ["Rot", "ist", "die", "ei\u00b7ne", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ART", "NN", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Rot wie dein roter Mund.", "tokens": ["Rot", "wie", "dein", "ro\u00b7ter", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}