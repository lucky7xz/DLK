{"textgrid.poem.25752": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bist du bestimmt zum Dulden:", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bist du bestimmt zum Dulden:", "tokens": ["Bist", "du", "be\u00b7stimmt", "zum", "Dul\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohlan! so duld', o Herz!", "tokens": ["Wo\u00b7hlan", "!", "so", "duld'", ",", "o", "Herz", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "VVFIN", "$,", "FM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schmerz wider dein Verschulden,", "tokens": ["Schmerz", "wi\u00b7der", "dein", "Ver\u00b7schul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist nur ein halber Schmerz.", "tokens": ["Ist", "nur", "ein", "hal\u00b7ber", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Eh' noch die Sonn' entweicht,", "tokens": ["Eh'", "noch", "die", "Sonn'", "ent\u00b7weicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wird g\u00fctig sie vielleicht", "tokens": ["Wird", "g\u00fc\u00b7tig", "sie", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den Horizont vergulden,", "tokens": ["Den", "Ho\u00b7ri\u00b7zont", "ver\u00b7gul\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der traurig jetzt erbleicht.", "tokens": ["Der", "trau\u00b7rig", "jetzt", "er\u00b7bleicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und w\u00fcrd' auf meinem Stiege", "tokens": ["Und", "w\u00fcrd'", "auf", "mei\u00b7nem", "Stie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Auch nie es wieder licht,", "tokens": ["Auch", "nie", "es", "wie\u00b7der", "licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Doch th\u00e4t' ich wohl, ich schwiege", "tokens": ["Doch", "th\u00e4t'", "ich", "wohl", ",", "ich", "schwie\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Und seufzt' und klagte nicht.", "tokens": ["Und", "seufzt'", "und", "klag\u00b7te", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Wer ahnet wohl die Last,", "tokens": ["Wer", "ah\u00b7net", "wohl", "die", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Erdr\u00fcckt sie gleich mich fast,", "tokens": ["Er\u00b7dr\u00fcckt", "sie", "gleich", "mich", "fast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Worunter ich mich schmiege,", "tokens": ["Wo\u00b7run\u00b7ter", "ich", "mich", "schmie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Mich schmiege sonder Rast?", "tokens": ["Mich", "schmie\u00b7ge", "son\u00b7der", "Rast", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "O Freund! seit ich so weise", "tokens": ["O", "Freund", "!", "seit", "ich", "so", "wei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "KOUS", "PPER", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Durch dich geworden bin,", "tokens": ["Durch", "dich", "ge\u00b7wor\u00b7den", "bin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Fliegt oft zu dir noch leise", "tokens": ["Fliegt", "oft", "zu", "dir", "noch", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Der Wunsch, der Seufzer hin:", "tokens": ["Der", "Wunsch", ",", "der", "Seuf\u00b7zer", "hin", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "H\u00e4tt' ich nur seine Hand,", "tokens": ["H\u00e4tt'", "ich", "nur", "sei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Zu halten festern Stand,", "tokens": ["Zu", "hal\u00b7ten", "fes\u00b7tern", "Stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Wenn Kleinmuth auf dem Eise", "tokens": ["Wenn", "Klein\u00b7muth", "auf", "dem", "Ei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Ab alle Sehnen spannt.", "tokens": ["Ab", "al\u00b7le", "Seh\u00b7nen", "spannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}