{"dta.poem.6505": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Licht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein m\u00e4\u00dfig ein gemildert Licht,", "tokens": ["Ein", "m\u00e4\u00b7\u00dfig", "ein", "ge\u00b7mil\u00b7dert", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "VVPP", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das, nur gelind und sanft beweget,", "tokens": ["Das", ",", "nur", "ge\u00b7lind", "und", "sanft", "be\u00b7we\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf unser Aug\u2019 ein schwach Gef\u00fchl erreget,", "tokens": ["Auf", "un\u00b7ser", "Aug'", "ein", "schwach", "Ge\u00b7f\u00fchl", "er\u00b7re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erg\u00f6tzet und vergn\u00fcgt uns das Gesicht.", "tokens": ["Er\u00b7g\u00f6t\u00b7zet", "und", "ver\u00b7gn\u00fcgt", "uns", "das", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn aber durch die Schnelligkeit der Theile,", "tokens": ["Wenn", "a\u00b7ber", "durch", "die", "Schnel\u00b7lig\u00b7keit", "der", "Thei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und durch der Klarheit spitze Pfeile", "tokens": ["Und", "durch", "der", "Klar\u00b7heit", "spit\u00b7ze", "Pfei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Augen sehr ersch\u00fcttert seyn;", "tokens": ["Die", "Au\u00b7gen", "sehr", "er\u00b7sch\u00fct\u00b7tert", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Erreget es uns eine Pein,", "tokens": ["Er\u00b7re\u00b7get", "es", "uns", "ei\u00b7ne", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "NN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Wie eine strenge Hitz auf eine Hand,", "tokens": ["Wie", "ei\u00b7ne", "stren\u00b7ge", "Hitz", "auf", "ei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wenn selbe schon vorhin verbrannt.", "tokens": ["Wenn", "sel\u00b7be", "schon", "vor\u00b7hin", "ver\u00b7brannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}