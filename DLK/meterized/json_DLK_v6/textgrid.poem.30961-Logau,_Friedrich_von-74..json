{"textgrid.poem.30961": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "74.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Person, die ich ietzt f\u00fchre, auff dem Spielplatz dieser Welt,", "tokens": ["Die", "Per\u00b7son", ",", "die", "ich", "ietzt", "f\u00fch\u00b7re", ",", "auff", "dem", "Spiel\u00b7platz", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,", "APPR", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-++-+-+-+-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.2": {"text": "Wil ich nach Verm\u00fcgen f\u00fchren, weil sie mir so zugestellt;", "tokens": ["Wil", "ich", "nach", "Ver\u00b7m\u00fc\u00b7gen", "f\u00fch\u00b7ren", ",", "weil", "sie", "mir", "so", "zu\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVINF", "$,", "KOUS", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Denn ich hab sie nie gesucht. Wird was andres mir gegeben,", "tokens": ["Denn", "ich", "hab", "sie", "nie", "ge\u00b7sucht", ".", "Wird", "was", "and\u00b7res", "mir", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$.", "VAFIN", "PWS", "PIS", "PPER", "VVPP", "$,"], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wil ich nach de\u00df Sch\u00f6pffers ruff, nie nach meinen L\u00fcsten leben.", "tokens": ["Wil", "ich", "nach", "de\u00df", "Sch\u00f6pf\u00b7fers", "ruff", ",", "nie", "nach", "mei\u00b7nen", "L\u00fcs\u00b7ten", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}