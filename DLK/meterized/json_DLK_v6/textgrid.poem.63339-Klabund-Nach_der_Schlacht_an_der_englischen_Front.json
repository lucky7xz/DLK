{"textgrid.poem.63339": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Nach der Schlacht an der englischen Front", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Totengr\u00e4ber haben schon", "tokens": ["Die", "To\u00b7ten\u00b7gr\u00e4\u00b7ber", "ha\u00b7ben", "schon"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Schaufeln angesetzt, da naht sich holpernd", "tokens": ["Die", "Schau\u00b7feln", "an\u00b7ge\u00b7setzt", ",", "da", "naht", "sich", "hol\u00b7pernd"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "ADV", "VVFIN", "PRF", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Viererzug, und ihm entsteigen stolpernd", "tokens": ["Ein", "Vier\u00b7er\u00b7zug", ",", "und", "ihm", "ent\u00b7stei\u00b7gen", "stol\u00b7pernd"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "PPER", "VVFIN", "ADJD"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Die Reisenden der Firma Cook '", "tokens": ["Die", "Rei\u00b7sen\u00b7den", "der", "Fir\u00b7ma", "Cook", "'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "And Son.", "tokens": ["And", "Son", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Eifrig und ernst begibt man sich ans Sammeln", "tokens": ["Eif\u00b7rig", "und", "ernst", "be\u00b7gibt", "man", "sich", "ans", "Sam\u00b7meln"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "PIS", "PRF", "APPRART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Leerer Patronenh\u00fclsen oder -taschen.", "tokens": ["Lee\u00b7rer", "Pat\u00b7ro\u00b7nen\u00b7h\u00fcl\u00b7sen", "o\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Indem die steifen Missis Kognakbohnen naschen,", "tokens": ["In\u00b7dem", "die", "stei\u00b7fen", "Mis\u00b7sis", "Ko\u00b7gnak\u00b7boh\u00b7nen", "na\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "H\u00f6rt man Verwundete nach Wasser stammeln.", "tokens": ["H\u00f6rt", "man", "Ver\u00b7wun\u00b7de\u00b7te", "nach", "Was\u00b7ser", "stam\u00b7meln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVFIN", "APPR", "NN", "VVFIN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}, "stanza.3": {"line.1": {"text": "Ein toter Belgier... Man h\u00e4tte beinah was verpa\u00dft...", "tokens": ["Ein", "to\u00b7ter", "Bel\u00b7gier", "...", "Man", "h\u00e4t\u00b7te", "bei\u00b7nah", "was", "ver\u00b7pa\u00dft", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PIS", "VAFIN", "ADV", "PWS", "VVFIN", "$("], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein Fu\u00dfballspieler sch\u00e4tzt den gr\u00fcnen Rasen.", "tokens": ["Ein", "Fu\u00df\u00b7ball\u00b7spie\u00b7ler", "sch\u00e4tzt", "den", "gr\u00fc\u00b7nen", "Ra\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein leiser Knall... Trompetenblasen...", "tokens": ["Ein", "lei\u00b7ser", "Knall", "...", "Trom\u00b7pe\u00b7ten\u00b7bla\u00b7sen", "..."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ein ergrauter Lord erbla\u00dft.", "tokens": ["Und", "ein", "er\u00b7grau\u00b7ter", "Lord", "er\u00b7bla\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}