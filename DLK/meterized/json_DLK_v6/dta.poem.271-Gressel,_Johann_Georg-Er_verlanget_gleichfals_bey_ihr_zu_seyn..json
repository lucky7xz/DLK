{"dta.poem.271": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er verlanget gleichfals bey ihr zu seyn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Unter tausend S\u00e4bel blitzen/", "tokens": ["Un\u00b7ter", "tau\u00b7send", "S\u00e4\u00b7bel", "blit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schl\u00e4ffet doch die Liebe nicht/", "tokens": ["Schl\u00e4f\u00b7fet", "doch", "die", "Lie\u00b7be", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKNEG", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie weis sich sowol zu sch\u00fctzen", "tokens": ["Sie", "weis", "sich", "so\u00b7wol", "zu", "sch\u00fct\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "PRF", "ADV", "PTKZU", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da\u00df ihr gar kein Leid geschicht.", "tokens": ["Da\u00df", "ihr", "gar", "kein", "Leid", "ge\u00b7schicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Den/ so ihren Fahnen schw\u00f6ren/", "tokens": ["Den", "/", "so", "ih\u00b7ren", "Fah\u00b7nen", "schw\u00f6\u00b7ren", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kan sie gleiches Heyl gewehren.", "tokens": ["Kan", "sie", "glei\u00b7ches", "Heyl", "ge\u00b7weh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Selbst ihr Wesen gleicht dem Kriege/", "tokens": ["Selbst", "ihr", "We\u00b7sen", "gleicht", "dem", "Krie\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bald hat Lust die Ober-Hand/", "tokens": ["Bald", "hat", "Lust", "die", "O\u00b7ber\u00b7Hand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bald hat Schmertz die s\u00fcssen Siege", "tokens": ["Bald", "hat", "Schmertz", "die", "s\u00fcs\u00b7sen", "Sie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Anmuth umgewandt.", "tokens": ["Und", "die", "An\u00b7muth", "um\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch wer ist best\u00e4ndig blieben", "tokens": ["Doch", "wer", "ist", "be\u00b7st\u00e4n\u00b7dig", "blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VAFIN", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kriegt die Krone in dem Lieben.", "tokens": ["Kriegt", "die", "Kro\u00b7ne", "in", "dem", "Lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wer best\u00e4ndig bleibt im Hoffen/", "tokens": ["Wer", "be\u00b7st\u00e4n\u00b7dig", "bleibt", "im", "Hof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "APPRART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und im Lieben trotzt der Zeit/", "tokens": ["Und", "im", "Lie\u00b7ben", "trotzt", "der", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem steht ", "tokens": ["Dem", "steht"], "token_info": ["word", "word"], "pos": ["PDS", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Und das Haus der Lieblichkeit/", "tokens": ["Und", "das", "Haus", "der", "Lieb\u00b7lich\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo er auf der Liebsten Br\u00fcste", "tokens": ["Wo", "er", "auf", "der", "Liebs\u00b7ten", "Br\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schmeckt die s\u00fcssen Himmels-L\u00fcste.", "tokens": ["Schmeckt", "die", "s\u00fcs\u00b7sen", "Him\u00b7mels\u00b7L\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sch\u00f6nster Gegen-Stand der Seelen", "tokens": ["Sch\u00f6ns\u00b7ter", "Ge\u00b7gen\u00b7Stand", "der", "See\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fasse dich nur in Gedult;", "tokens": ["Fas\u00b7se", "dich", "nur", "in", "Ge\u00b7dult", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mein Abwesen macht dein Qu\u00e4hlen/", "tokens": ["Mein", "Ab\u00b7we\u00b7sen", "macht", "dein", "Qu\u00e4h\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und das Schicksahl hat die Schuld/", "tokens": ["Und", "das", "Schick\u00b7sahl", "hat", "die", "Schuld", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das dich bald in Freude setzen", "tokens": ["Das", "dich", "bald", "in", "Freu\u00b7de", "set\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "ADV", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und dich wird durch mich erg\u00f6tzen.", "tokens": ["Und", "dich", "wird", "durch", "mich", "er\u00b7g\u00f6t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}