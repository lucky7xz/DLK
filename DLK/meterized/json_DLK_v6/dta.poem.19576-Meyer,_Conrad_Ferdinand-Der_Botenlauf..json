{"dta.poem.19576": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der Botenlauf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Blicke gen Himmel gewandt! Gebreitete flehende Arme!", "tokens": ["Bli\u00b7cke", "gen", "Him\u00b7mel", "ge\u00b7wandt", "!", "Ge\u00b7brei\u00b7te\u00b7te", "fle\u00b7hen\u00b7de", "Ar\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$.", "NN", "ADJA", "NN", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Murmeln und schallender Ruf! Knieende M\u00e4dchen und", "tokens": ["Mur\u00b7meln", "und", "schal\u00b7len\u00b7der", "Ruf", "!", "Kni\u00b7e\u00b7en\u00b7de", "M\u00e4d\u00b7chen", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN", "$.", "ADJA", "NN", "KON"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.3": {"text": "\u201eg\u00f6tter, befl\u00fcgelt den Boten! Entscheidung! Lieber als Bangni\u00df!", "tokens": ["\u201e", "g\u00f6t\u00b7ter", ",", "be\u00b7fl\u00fc\u00b7gelt", "den", "Bo\u00b7ten", "!", "Ent\u00b7schei\u00b7dung", "!", "Lie\u00b7ber", "als", "Bang\u00b7ni\u00df", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "$,", "VVFIN", "ART", "NN", "$.", "NN", "$.", "ADJD", "KOKOM", "NN", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Seit sich die Sonne erhob, ringen die Stadt und Tarquin.", "tokens": ["Seit", "sich", "die", "Son\u00b7ne", "er\u00b7hob", ",", "rin\u00b7gen", "die", "Stadt", "und", "Tar\u00b7quin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "KON", "NN", "$."], "meter": "+--+--+---+-+-", "measure": "dactylic.tri.plus"}, "line.5": {"text": "Siehe, die Sonne versinkt! Mitk\u00e4mpfer, Castor und Pollux!", "tokens": ["Sie\u00b7he", ",", "die", "Son\u00b7ne", "ver\u00b7sinkt", "!", "Mit\u00b7k\u00e4mp\u00b7fer", ",", "Cas\u00b7tor", "und", "Pol\u00b7lux", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ART", "NN", "VVPP", "$.", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+--++--+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Denkt der verlassenen Fraun! Sendet den Boten geschwind!\u201c", "tokens": ["Denkt", "der", "ver\u00b7las\u00b7se\u00b7nen", "Fraun", "!", "Sen\u00b7det", "den", "Bo\u00b7ten", "ge\u00b7schwind", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "VVFIN", "ART", "NN", "VVPP", "$.", "$("], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Horch! Achthufig Geklirr bergan! Zwei reisige Reiter!", "tokens": ["Horch", "!", "Acht\u00b7hu\u00b7fig", "Ge\u00b7klirr", "ber\u00b7gan", "!", "Zwei", "rei\u00b7si\u00b7ge", "Rei\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "CARD", "NN", "PTKVZ", "$.", "CARD", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Schon am heiligen Quell sp\u00fclen die Waffen sie rein.", "tokens": ["Schon", "am", "hei\u00b7li\u00b7gen", "Quell", "sp\u00fc\u00b7len", "die", "Waf\u00b7fen", "sie", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Dann, zwei gewaltige J\u00fcnglinge, stehn auf der ragenden Burg sie,", "tokens": ["Dann", ",", "zwei", "ge\u00b7wal\u00b7ti\u00b7ge", "J\u00fcng\u00b7lin\u00b7ge", ",", "stehn", "auf", "der", "ra\u00b7gen\u00b7den", "Burg", "sie", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "CARD", "ADJA", "NN", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+--++-+--+--+-", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Gegen die schauernden Fraun hat sich der eine gekehrt:", "tokens": ["Ge\u00b7gen", "die", "schau\u00b7ern\u00b7den", "Fraun", "hat", "sich", "der", "ei\u00b7ne", "ge\u00b7kehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "PRF", "ART", "ART", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "\u201efreude, knospendes M\u00e4dchen! Entschlossene R\u00f6merin, Freude!", "tokens": ["\u201e", "freu\u00b7de", ",", "knos\u00b7pen\u00b7des", "M\u00e4d\u00b7chen", "!", "Ent\u00b7schlos\u00b7se\u00b7ne", "R\u00f6\u00b7me\u00b7rin", ",", "Freu\u00b7de", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "NN", "$,", "ADJA", "NN", "$.", "ADJA", "NN", "$,", "NN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Herrlicher Sieg ist erk\u00e4mpft! Geht ihr entgegen dem Heer?\u201c", "tokens": ["Herr\u00b7li\u00b7cher", "Sieg", "ist", "er\u00b7k\u00e4mpft", "!", "Geht", "ihr", "ent\u00b7ge\u00b7gen", "dem", "Heer", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "VAFIN", "VVPP", "$.", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.13": {"text": "Einer spricht's und der Andere lauscht, zu dem Bruder gewendet.", "tokens": ["Ei\u00b7ner", "spricht's", "und", "der", "An\u00b7de\u00b7re", "lauscht", ",", "zu", "dem", "Bru\u00b7der", "ge\u00b7wen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "ART", "PIS", "VVFIN", "$,", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Jetzt in das bleichende Licht springen die Rosse empor.", "tokens": ["Jetzt", "in", "das", "blei\u00b7chen\u00b7de", "Licht", "sprin\u00b7gen", "die", "Ros\u00b7se", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Einer der J\u00fcnglinge schwindet im Abend, es schwindet der andre,", "tokens": ["Ei\u00b7ner", "der", "J\u00fcng\u00b7lin\u00b7ge", "schwin\u00b7det", "im", "A\u00b7bend", ",", "es", "schwin\u00b7det", "der", "and\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "VVFIN", "APPRART", "NN", "$,", "PPER", "VVFIN", "ART", "ADJA", "$,"], "meter": "+--++-+--+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.16": {"text": "Denn wie ein liebendes Paar lassen die Br\u00fcder sich nicht.", "tokens": ["Denn", "wie", "ein", "lie\u00b7ben\u00b7des", "Paar", "las\u00b7sen", "die", "Br\u00fc\u00b7der", "sich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "PRF", "PTKNEG", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Ueber der r\u00f6mischen Feste gewaltigem dunkelndem Umri\u00df", "tokens": ["Ue\u00b7ber", "der", "r\u00f6\u00b7mi\u00b7schen", "Fes\u00b7te", "ge\u00b7wal\u00b7ti\u00b7gem", "dun\u00b7keln\u00b7dem", "Um\u00b7ri\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "ADJA", "NN"], "meter": "+--+--+--+--+-+-+", "measure": "dactylic.tetra.plus"}, "line.18": {"text": "Hebt sich in d\u00e4mmernder Nacht seliges Doppelgestirn.", "tokens": ["Hebt", "sich", "in", "d\u00e4m\u00b7mern\u00b7der", "Nacht", "se\u00b7li\u00b7ges", "Dop\u00b7pel\u00b7ges\u00b7tirn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}}}}