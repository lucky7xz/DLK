{"dta.poem.2396": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Nachrede und hertzlicher Gl\u00fcks-  \n wunsch.  \n an den  \n hoch\u00e4delen Breutigam und seine  \n holdselige Braut.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer ists hoch\u00e4dles Paar was ich in Eyl geschrieben", "tokens": ["Hjer", "ists", "hoc\u00b7h\u00e4d\u00b7les", "Paar", "was", "ich", "in", "Eyl", "ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADJA", "NN", "PWS", "PPER", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf schlechte Sch\u00e4ferahrt. Ich bitt\u2019/ es zu belieben", "tokens": ["Auf", "schlech\u00b7te", "Sch\u00e4\u00b7fe\u00b7rahrt", ".", "Ich", "bitt'", "/", "es", "zu", "be\u00b7lie\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "PPER", "VVFIN", "$(", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Obs gleich gering und schlecht. Es giebts ein treuer", "tokens": ["Obs", "gleich", "ge\u00b7ring", "und", "schlecht", ".", "Es", "giebts", "ein", "treu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADJD", "KON", "ADJD", "$.", "PPER", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sinn/", "tokens": ["Sinn", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Der seinen Zwek und Ziel gerichtet nur dahin/", "tokens": ["Der", "sei\u00b7nen", "Zwek", "und", "Ziel", "ge\u00b7rich\u00b7tet", "nur", "da\u00b7hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "KON", "NN", "VVPP", "ADV", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie er euch dienen mag. Zwar mancher viele schenket", "tokens": ["Wie", "er", "euch", "die\u00b7nen", "mag.", "Zwar", "man\u00b7cher", "vie\u00b7le", "schen\u00b7ket"], "token_info": ["word", "word", "word", "word", "abbreviation", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "VVINF", "VMFIN", "ADV", "PIAT", "PIS", "VVFIN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Au\u00df falscher H\u00f6fligkeit/ und doch im Hertzen denket:", "tokens": ["Au\u00df", "fal\u00b7scher", "H\u00f6f\u00b7lig\u00b7keit", "/", "und", "doch", "im", "Hert\u00b7zen", "den\u00b7ket", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "KON", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Pfuy/ da\u00df ich doch nicht m\u00fcst\u2019! Ach nein/ mein Hertze", "tokens": ["Pfuy", "/", "da\u00df", "ich", "doch", "nicht", "m\u00fcst'", "!", "Ach", "nein", "/", "mein", "Hert\u00b7ze"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$(", "KOUS", "PPER", "ADV", "PTKNEG", "VMFIN", "$.", "NN", "PTKANT", "$(", "PPOSAT", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.9": {"text": "giebt", "tokens": ["giebt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Ein solches/ was der Mensch auch selbst der Himmel", "tokens": ["Ein", "sol\u00b7ches", "/", "was", "der", "Mensch", "auch", "selbst", "der", "Him\u00b7mel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "$(", "PWS", "ART", "NN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "liebt.", "tokens": ["liebt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Ich meine Redligkeit. Ich w\u00fcndsch\u2019 euch Gl\u00fckk und Se-", "tokens": ["Ich", "mei\u00b7ne", "Red\u00b7lig\u00b7keit", ".", "Ich", "w\u00fcnd\u00b7sch'", "euch", "Gl\u00fckk", "und", "Se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "gen/", "tokens": ["gen", "/"], "token_info": ["word", "punct"], "pos": ["APPR", "$("], "meter": "-", "measure": "single.down"}, "line.14": {"text": "Vom aller h\u00f6chsten Gott auf allen euren Wegen/", "tokens": ["Vom", "al\u00b7ler", "h\u00f6chs\u00b7ten", "Gott", "auf", "al\u00b7len", "eu\u00b7ren", "We\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "PIAT", "ADJA", "NN", "APPR", "PIAT", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Lebt selig allezeit/ Jhr lebt auch wie ihr lebt/", "tokens": ["Lebt", "se\u00b7lig", "al\u00b7le\u00b7zeit", "/", "Ihr", "lebt", "auch", "wie", "ihr", "lebt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "$(", "PPER", "VVFIN", "ADV", "KOKOM", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Mein bester Wunsch ist der/ den ihr euch selber gebt.", "tokens": ["Mein", "bes\u00b7ter", "Wunsch", "ist", "der", "/", "den", "ihr", "euch", "sel\u00b7ber", "gebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ART", "$(", "ART", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}