{"textgrid.poem.39571": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "4. Schiller im Spiegel seiner Theorie", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil kein frisches Gef\u00fchl dem vertrockneten Herzen entstr\u00f6mte,", "tokens": ["Weil", "kein", "fri\u00b7sches", "Ge\u00b7f\u00fchl", "dem", "ver\u00b7trock\u00b7ne\u00b7ten", "Her\u00b7zen", "ent\u00b7str\u00f6m\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Alles in R\u00f6hren gepumpt, nannt' er sich sentimental.", "tokens": ["Al\u00b7les", "in", "R\u00f6h\u00b7ren", "ge\u00b7pumpt", ",", "nannt'", "er", "sich", "sen\u00b7ti\u00b7men\u00b7tal", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "NN", "VVPP", "$,", "VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "+--+--++-+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Weil er die Nacht in Toboso vergeblich gesucht die Prinzessin,", "tokens": ["Weil", "er", "die", "Nacht", "in", "To\u00b7bo\u00b7so", "ver\u00b7geb\u00b7lich", "ge\u00b7sucht", "die", "Prin\u00b7zes\u00b7sin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NE", "ADJD", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Auch Windm\u00fchlen bek\u00e4mpft, nannt' er sich Idealist.", "tokens": ["Auch", "Wind\u00b7m\u00fch\u00b7len", "be\u00b7k\u00e4mpft", ",", "nannt'", "er", "sich", "I\u00b7dea\u00b7list", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVPP", "$,", "VVFIN", "PPER", "PRF", "NN", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}}}}}