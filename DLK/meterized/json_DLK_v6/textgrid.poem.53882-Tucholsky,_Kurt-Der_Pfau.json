{"textgrid.poem.53882": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Der Pfau", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin ein Pfau.", "tokens": ["Ich", "bin", "ein", "Pfau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "In meinen wei\u00dfen Schwingen", "tokens": ["In", "mei\u00b7nen", "wei\u00b7\u00dfen", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "f\u00e4ngt sich das Schleierlicht der Sonne ein.", "tokens": ["f\u00e4ngt", "sich", "das", "Schlei\u00b7er\u00b7licht", "der", "Son\u00b7ne", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Und alle Frauen, die vor\u00fcbergingen,", "tokens": ["Und", "al\u00b7le", "Frau\u00b7en", ",", "die", "vor\u00b7\u00fc\u00b7ber\u00b7gin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "liebkosten mit dem Blick den Silberschein.", "tokens": ["lieb\u00b7kos\u00b7ten", "mit", "dem", "Blick", "den", "Sil\u00b7ber\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df, da\u00df ich sehr sch\u00f6n bin.", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "ich", "sehr", "sch\u00f6n", "bin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Meine Federn", "tokens": ["Mei\u00b7ne", "Fe\u00b7dern"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "auf meinem Kopf stell ich oft kaprizi\u00f6s . . .", "tokens": ["auf", "mei\u00b7nem", "Kopf", "stell", "ich", "oft", "kap\u00b7ri\u00b7zi\u00b7\u00f6s", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$.", "$.", "$."], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ich hab das wei\u00dfeste von allen Pfauenr\u00e4dern;", "tokens": ["Ich", "hab", "das", "wei\u00b7\u00dfes\u00b7te", "von", "al\u00b7len", "Pfau\u00b7en\u00b7r\u00e4\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "ich bin sehr teuer, selten und nerv\u00f6s.", "tokens": ["ich", "bin", "sehr", "teu\u00b7er", ",", "sel\u00b7ten", "und", "ner\u00b7v\u00f6s", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADJD", "KON", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich habe leider ziemlich gro\u00dfe Krallen,", "tokens": ["Ich", "ha\u00b7be", "lei\u00b7der", "ziem\u00b7lich", "gro\u00b7\u00dfe", "Kral\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und wenn ich fliege, sieht es kl\u00e4glich aus.", "tokens": ["und", "wenn", "ich", "flie\u00b7ge", ",", "sieht", "es", "kl\u00e4g\u00b7lich", "aus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch, wer mich liebt, dem werde ich gefallen,", "tokens": ["Doch", ",", "wer", "mich", "liebt", ",", "dem", "wer\u00b7de", "ich", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "PPER", "VVFIN", "$,", "PDS", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und alle Welt steht vor dem Vogelhaus.", "tokens": ["und", "al\u00b7le", "Welt", "steht", "vor", "dem", "Vo\u00b7gel\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Klug bin ich nicht. Klugheit ist nicht bei allen,", "tokens": ["Klug", "bin", "ich", "nicht", ".", "Klug\u00b7heit", "ist", "nicht", "bei", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PTKNEG", "$.", "NN", "VAFIN", "PTKNEG", "APPR", "PIS", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "viel liegt nicht hinter meiner Vogelstirn.", "tokens": ["viel", "liegt", "nicht", "hin\u00b7ter", "mei\u00b7ner", "Vo\u00b7gel\u00b7stirn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich will gefallen \u2013 immer nur gefallen \u2013", "tokens": ["Ich", "will", "ge\u00b7fal\u00b7len", "\u2013", "im\u00b7mer", "nur", "ge\u00b7fal\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "$(", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich bin ein sch\u00f6ner Pfau. Ich brauche kein Gehirn.", "tokens": ["Ich", "bin", "ein", "sch\u00f6\u00b7ner", "Pfau", ".", "Ich", "brau\u00b7che", "kein", "Ge\u00b7hirn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nur singen darf ich nicht. Das ordin\u00e4re", "tokens": ["Nur", "sin\u00b7gen", "darf", "ich", "nicht", ".", "Das", "or\u00b7di\u00b7n\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVINF", "VMFIN", "PPER", "PTKNEG", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gekr\u00e4chz ist nicht zu sehen \u2013 wie mein Bildnis zeigt.", "tokens": ["Ge\u00b7kr\u00e4chz", "ist", "nicht", "zu", "se\u00b7hen", "\u2013", "wie", "mein", "Bild\u00b7nis", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$(", "KOKOM", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich bin ein Pfau.", "tokens": ["Ich", "bin", "ein", "Pfau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und eine sch\u00f6ne Lehre:", "tokens": ["Und", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Leh\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wer dumm und sch\u00f6n ist, setzt sich. Siegt. Und schweigt.", "tokens": ["Wer", "dumm", "und", "sch\u00f6n", "ist", ",", "setzt", "sich", ".", "Siegt", ".", "Und", "schweigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "VAFIN", "$,", "VVFIN", "PRF", "$.", "VVFIN", "$.", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}