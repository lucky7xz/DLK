{"textgrid.poem.65130": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kein Mittel seh' ich zu entkommen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Mittel seh' ich zu entkommen", "tokens": ["Kein", "Mit\u00b7tel", "seh'", "ich", "zu", "ent\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem fest in sich verschlung'nen Kreis:", "tokens": ["Dem", "fest", "in", "sich", "ver\u00b7schlung'\u00b7nen", "Kreis", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PRF", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Dichters Lied kann Keinem frommen,", "tokens": ["Des", "Dich\u00b7ters", "Lied", "kann", "Kei\u00b7nem", "from\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der nicht, was Jener singt, schon wei\u00df.", "tokens": ["Der", "nicht", ",", "was", "Je\u00b7ner", "singt", ",", "schon", "wei\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "$,", "PWS", "PDAT", "VVFIN", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Den Wissenden bed\u00fcnkt hinwieder,", "tokens": ["Den", "Wis\u00b7sen\u00b7den", "be\u00b7d\u00fcnkt", "hin\u00b7wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Wort so d\u00fcrftig und so hohl! \u2013", "tokens": ["Das", "Wort", "so", "d\u00fcrf\u00b7tig", "und", "so", "hohl", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KON", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach! wozu singt man dann noch Lieder?", "tokens": ["Ach", "!", "wo\u00b7zu", "singt", "man", "dann", "noch", "Lie\u00b7der", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VVFIN", "PIS", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wozu n\u00fctzt der Dichter wohl?", "tokens": ["Und", "wo\u00b7zu", "n\u00fctzt", "der", "Dich\u00b7ter", "wohl", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}