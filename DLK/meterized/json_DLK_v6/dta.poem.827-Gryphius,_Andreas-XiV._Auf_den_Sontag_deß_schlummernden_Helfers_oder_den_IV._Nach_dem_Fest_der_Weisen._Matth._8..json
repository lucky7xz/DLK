{"dta.poem.827": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XiV.  Auf den Sontag de\u00df schlummernden  \n Helfers/ oder den  IV.  Nach dem Fest der  \n Weisen. Matth. 8.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Avff! Auff! wach auff HERR Christ/ schaw wie dis ", "tokens": ["Avff", "!", "Auff", "!", "wach", "auff", "HeRR", "Christ", "/", "schaw", "wie", "dis"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "NN", "$.", "ADJD", "APPR", "NN", "NE", "$(", "ADJD", "KOKOM", "PDS"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Winde toben! ", "tokens": ["Win\u00b7de", "to\u00b7ben", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Wie Mast vnd Ruder knackt! ietzt sinckt dein Schiff zu", "tokens": ["Wie", "Mast", "vnd", "Ru\u00b7der", "knackt", "!", "ietzt", "sinckt", "dein", "Schiff", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "PTKZU"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "grund ", "tokens": ["grund"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Jtzt schaum\u2019t die wilde flutt wo flack vnd Segel stund", "tokens": ["Jtzt", "schaum't", "die", "wil\u00b7de", "flutt", "wo", "flack", "vnd", "Se\u00b7gel", "stund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "VVFIN", "PWAV", "VVFIN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vns fehlt an st\u00e4rck vnd rath! bald kracht die Lufft von oben/", "tokens": ["Vns", "fehlt", "an", "st\u00e4rck", "vnd", "rath", "!", "bald", "kracht", "die", "Lufft", "von", "o\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "APPR", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Bald schluckt die t\u00e4uff\u2019 vns eyn! wird dich denn jemand lo-", "tokens": ["Bald", "schluckt", "die", "t\u00e4uff'", "vns", "eyn", "!", "wird", "dich", "denn", "je\u00b7mand", "lo"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "VVFIN", "PPER", "VAINF", "$.", "VAFIN", "PPER", "ADV", "PIS", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ben ", "tokens": ["ben"], "token_info": ["word"], "pos": ["VVINF"], "meter": "-", "measure": "single.down"}}, "stanza.3": {"line.1": {"text": "Der ins verderben f\u00e4hrt? Ist di\u00df der feste bund/", "tokens": ["Der", "ins", "ver\u00b7der\u00b7ben", "f\u00e4hrt", "?", "Ist", "di\u00df", "der", "fes\u00b7te", "bund", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "VVFIN", "VVFIN", "$.", "VAFIN", "PDS", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der stets vns hoffen hies/ wenn gleich der weite schlund", "tokens": ["Der", "stets", "vns", "hof\u00b7fen", "hies", "/", "wenn", "gleich", "der", "wei\u00b7te", "schlund"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PPER", "VVFIN", "ADV", "$(", "KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der H\u00f6llen ri\u00df entzwey? wo hast du hin verschoben", "tokens": ["Der", "H\u00f6l\u00b7len", "ri\u00df", "ent\u00b7zwey", "?", "wo", "hast", "du", "hin", "ver\u00b7scho\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "PWAV", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was deine trew versprach? hilff ehr der Kahn sich trenn\u2019t", "tokens": ["Was", "dei\u00b7ne", "trew", "ver\u00b7sprach", "?", "hilff", "ehr", "der", "Kahn", "sich", "trenn't"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "ADJD", "VVFIN", "$.", "NN", "NN", "ART", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hilff ehr das schwache Brett an jene Klippen renn\u2019t", "tokens": ["Hilff", "ehr", "das", "schwa\u00b7che", "Brett", "an", "je\u00b7ne", "Klip\u00b7pen", "renn't"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "ART", "ADJA", "NN", "APPR", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kan denn kein zetter schrey\u2019n dich au\u00df dem schlaff erwecken?", "tokens": ["Kan", "denn", "kein", "zet\u00b7ter", "schrey'n", "dich", "au\u00df", "dem", "schlaff", "er\u00b7we\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIAT", "NN", "VVFIN", "PRF", "APPR", "ART", "ADJD", "VVINF", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Auff! auff! schilt Flutt vnd Meer; so bald du auf-wirst-", "tokens": ["Auff", "!", "auff", "!", "schilt", "Flutt", "vnd", "Meer", ";", "so", "bald", "du", "auf\u00b7wir\u00b7st"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "$.", "PTKVZ", "$.", "VVFIN", "NN", "KON", "NN", "$.", "ADV", "ADV", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "stehn ", "tokens": ["stehn"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Wird Brausen/ Sturm vnd Wind in einem nun vergehn/", "tokens": ["Wird", "Brau\u00b7sen", "/", "Sturm", "vnd", "Wind", "in", "ei\u00b7nem", "nun", "ver\u00b7gehn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$(", "NN", "KON", "NN", "APPR", "ART", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch dein Wortt mu\u00df was vns mit n\u00f6then schreckt, er-", "tokens": ["Durch", "dein", "Wortt", "mu\u00df", "was", "vns", "mit", "n\u00f6\u00b7then", "schreckt", ",", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PWS", "PPER", "APPR", "ADJA", "VVFIN", "$,", "TRUNC"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "schrecken. ", "tokens": ["schre\u00b7cken", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}