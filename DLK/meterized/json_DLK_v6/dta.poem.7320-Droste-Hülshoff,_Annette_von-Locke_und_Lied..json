{"dta.poem.7320": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Locke und Lied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Meine Lieder sandte ich dir,", "tokens": ["Mei\u00b7ne", "Lie\u00b7der", "sand\u00b7te", "ich", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Meines Herzens str\u00f6mende Quellen,", "tokens": ["Mei\u00b7nes", "Her\u00b7zens", "str\u00f6\u00b7men\u00b7de", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Deine Locke sandtest du mir,", "tokens": ["Dei\u00b7ne", "Lo\u00b7cke", "sand\u00b7test", "du", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Deines Hauptes ringelnde Wellen;", "tokens": ["Dei\u00b7nes", "Haup\u00b7tes", "rin\u00b7geln\u00b7de", "Wel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Hauptes Welle und Herzens Fluth", "tokens": ["Haup\u00b7tes", "Wel\u00b7le", "und", "Her\u00b7zens", "Fluth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Sie zogen einander vor\u00fcber,", "tokens": ["Sie", "zo\u00b7gen", "ein\u00b7an\u00b7der", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Haben sie nicht im Kusse geruht?", "tokens": ["Ha\u00b7ben", "sie", "nicht", "im", "Kus\u00b7se", "ge\u00b7ruht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "APPRART", "NN", "VVPP", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Scho\u00df nicht ein Leuchten dar\u00fcber?", "tokens": ["Scho\u00df", "nicht", "ein", "Leuch\u00b7ten", "da\u00b7r\u00fc\u00b7ber", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "PAV", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Und du klagest: verblichen sey", "tokens": ["Und", "du", "kla\u00b7gest", ":", "ver\u00b7bli\u00b7chen", "sey"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "VVPP", "VAFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die Farbe der wandernden Zeichen;", "tokens": ["Die", "Far\u00b7be", "der", "wan\u00b7dern\u00b7den", "Zei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Scheiden thut weh, mein Liebchen, ey,", "tokens": ["Schei\u00b7den", "thut", "weh", ",", "mein", "Lieb\u00b7chen", ",", "ey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Die Scheidenden d\u00fcrfen erbleichen;", "tokens": ["Die", "Schei\u00b7den\u00b7den", "d\u00fcr\u00b7fen", "er\u00b7blei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Warst du bla\u00df nicht, zitternd und kalt,", "tokens": ["Warst", "du", "bla\u00df", "nicht", ",", "zit\u00b7ternd", "und", "kalt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "PTKNEG", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Als ich von dir mich gerissen?", "tokens": ["Als", "ich", "von", "dir", "mich", "ge\u00b7ris\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "PRF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Blicke sie an, du Milde, und bald,", "tokens": ["Bli\u00b7cke", "sie", "an", ",", "du", "Mil\u00b7de", ",", "und", "bald", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "PPER", "NN", "$,", "KON", "ADV", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Bald werden den Herrn sie nicht missen.", "tokens": ["Bald", "wer\u00b7den", "den", "Herrn", "sie", "nicht", "mis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Auch deine Locke hat sich gestreckt,", "tokens": ["Auch", "dei\u00b7ne", "Lo\u00b7cke", "hat", "sich", "ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Verdrossen, gleich schlafendem Kinde,", "tokens": ["Ver\u00b7dros\u00b7sen", ",", "gleich", "schla\u00b7fen\u00b7dem", "Kin\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Doch ich hab' sie mit K\u00fcssen geweckt,", "tokens": ["Doch", "ich", "hab'", "sie", "mit", "K\u00fcs\u00b7sen", "ge\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Hab' sie gestreichelt so linde,", "tokens": ["Hab'", "sie", "ge\u00b7strei\u00b7chelt", "so", "lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "ADV", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ihr gefl\u00fcstert von unserer Treu',", "tokens": ["Ihr", "ge\u00b7fl\u00fcs\u00b7tert", "von", "un\u00b7se\u00b7rer", "Treu'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Sie geschlungen um deine Kr\u00e4nze,", "tokens": ["Sie", "ge\u00b7schlun\u00b7gen", "um", "dei\u00b7ne", "Kr\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Und nun ringelt sie sich aufs neu,", "tokens": ["Und", "nun", "rin\u00b7gelt", "sie", "sich", "aufs", "neu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "APPRART", "ADJD", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Wie eine Rebe im Lenze.", "tokens": ["Wie", "ei\u00b7ne", "Re\u00b7be", "im", "Len\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Wenig Wochen, dann gr\u00fcnet der Stamm,", "tokens": ["We\u00b7nig", "Wo\u00b7chen", ",", "dann", "gr\u00fc\u00b7net", "der", "Stamm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Hat Sonnenschein sich ergossen,", "tokens": ["Hat", "Son\u00b7nen\u00b7schein", "sich", "er\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und wir sitzen am rieselnden Damm,", "tokens": ["Und", "wir", "sit\u00b7zen", "am", "rie\u00b7seln\u00b7den", "Damm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Die H\u00e4nd' in einander geschlossen,", "tokens": ["Die", "H\u00e4nd'", "in", "ein\u00b7an\u00b7der", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Schaun in die Welle, und schaun in das Aug'", "tokens": ["Schaun", "in", "die", "Wel\u00b7le", ",", "und", "schaun", "in", "das", "Aug'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "$,", "KON", "ADJD", "APPR", "ART", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Uns wieder und wieder und lachen,", "tokens": ["Uns", "wie\u00b7der", "und", "wie\u00b7der", "und", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KON", "ADV", "KON", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Und Bekanntschaft m\u00f6gen dann auch", "tokens": ["Und", "Be\u00b7kannt\u00b7schaft", "m\u00f6\u00b7gen", "dann", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VMFIN", "ADV", "ADV"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Die Lock' und der Liederstrom machen.", "tokens": ["Die", "Lock'", "und", "der", "Lie\u00b7der\u00b7strom", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}