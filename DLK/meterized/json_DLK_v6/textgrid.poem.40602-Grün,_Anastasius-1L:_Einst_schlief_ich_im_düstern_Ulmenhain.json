{"textgrid.poem.40602": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einst schlief ich im d\u00fcstern Ulmenhain", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst schlief ich im d\u00fcstern Ulmenhain", "tokens": ["Einst", "schlief", "ich", "im", "d\u00fcs\u00b7tern", "Ul\u00b7men\u00b7hain"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "VVFIN", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nicht fern von den S\u00e4rgen der Barden ein,", "tokens": ["Nicht", "fern", "von", "den", "S\u00e4r\u00b7gen", "der", "Bar\u00b7den", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Mich sangen die V\u00f6gel des Waldes in Ruh,", "tokens": ["Mich", "san\u00b7gen", "die", "V\u00f6\u00b7gel", "des", "Wal\u00b7des", "in", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Es rauschten die Zweige wie Lieder dazu.", "tokens": ["Es", "rauschten", "die", "Zwei\u00b7ge", "wie", "Lie\u00b7der", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOKOM", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Als jegliches Aug' in Schlummer schon brach", "tokens": ["Als", "jeg\u00b7li\u00b7ches", "Aug'", "in", "Schlum\u00b7mer", "schon", "brach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "APPR", "NN", "ADV", "VVFIN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und Kummer allein und Liebe noch wach,", "tokens": ["Und", "Kum\u00b7mer", "al\u00b7lein", "und", "Lie\u00b7be", "noch", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "KON", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da r\u00fcttelt's und sch\u00fcttelt's an Riegel und Sarg,", "tokens": ["Da", "r\u00fct\u00b7telt's", "und", "sch\u00fct\u00b7telt's", "an", "Rie\u00b7gel", "und", "Sarg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Da r\u00fcttelt und sprengt es Riegel und Sarg.", "tokens": ["Da", "r\u00fct\u00b7telt", "und", "sprengt", "es", "Rie\u00b7gel", "und", "Sarg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie Woge an Woge im brausenden Meer,", "tokens": ["Wie", "Wo\u00b7ge", "an", "Wo\u00b7ge", "im", "brau\u00b7sen\u00b7den", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ersteht aus den S\u00e4rgen ein Harfnerheer,", "tokens": ["Er\u00b7steht", "aus", "den", "S\u00e4r\u00b7gen", "ein", "Harf\u00b7ner\u00b7heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wohl tausend Gestalten im regen Gew\u00fchl,", "tokens": ["Wohl", "tau\u00b7send", "Ge\u00b7stal\u00b7ten", "im", "re\u00b7gen", "Ge\u00b7w\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "In kn\u00f6chernen Armen ein Saitenspiel.", "tokens": ["In", "kn\u00f6\u00b7cher\u00b7nen", "Ar\u00b7men", "ein", "Sai\u00b7ten\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Die Lippen sind d\u00fcrr und der Blick ist kalt,", "tokens": ["Die", "Lip\u00b7pen", "sind", "d\u00fcrr", "und", "der", "Blick", "ist", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die bleiche Wange verfallen und alt,", "tokens": ["Die", "blei\u00b7che", "Wan\u00b7ge", "ver\u00b7fal\u00b7len", "und", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "KON", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und mit den H\u00e4nden ohne Gef\u00fchl", "tokens": ["Und", "mit", "den", "H\u00e4n\u00b7den", "oh\u00b7ne", "Ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Gepocht und geh\u00e4mmert am Saitenspiel.", "tokens": ["Ge\u00b7pocht", "und", "ge\u00b7h\u00e4m\u00b7mert", "am", "Sai\u00b7ten\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "APPRART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Und wie sie auch pochen und h\u00e4mmern im Chor,", "tokens": ["Und", "wie", "sie", "auch", "po\u00b7chen", "und", "h\u00e4m\u00b7mern", "im", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVINF", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Kein Ton und kein Laut schl\u00e4gt an mein Ohr;", "tokens": ["Kein", "Ton", "und", "kein", "Laut", "schl\u00e4gt", "an", "mein", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "APPR", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nur Eulen flattern aus dem Versteck", "tokens": ["Nur", "Eu\u00b7len", "flat\u00b7tern", "aus", "dem", "Ver\u00b7steck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVINF", "APPR", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und Kobolde grinsen im Felsenleck.", "tokens": ["Und", "Ko\u00b7bol\u00b7de", "grin\u00b7sen", "im", "Fel\u00b7sen\u00b7leck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Und unter den Harfnern das Gras verdorrt,", "tokens": ["Und", "un\u00b7ter", "den", "Harf\u00b7nern", "das", "Gras", "ver\u00b7dorrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der Mond sein z\u00fcchtig Antlitz umflort;", "tokens": ["Der", "Mond", "sein", "z\u00fcch\u00b7tig", "Ant\u00b7litz", "um\u00b7flort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "So klimpern alln\u00e4chtlich zur Mitternachtzeit", "tokens": ["So", "klim\u00b7pern", "all\u00b7n\u00e4cht\u00b7lich", "zur", "Mit\u00b7ter\u00b7nacht\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ihr ewiges Lied sie: Vergessenheit!", "tokens": ["Ihr", "e\u00b7wi\u00b7ges", "Lied", "sie", ":", "Ver\u00b7ges\u00b7sen\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "$.", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Jetzt schallt's wie der Engel Posannenruf,", "tokens": ["Jetzt", "schallt's", "wie", "der", "En\u00b7gel", "Po\u00b7san\u00b7nen\u00b7ruf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Als Welten und Leben der Ewige schuf;", "tokens": ["Als", "Wel\u00b7ten", "und", "Le\u00b7ben", "der", "E\u00b7wi\u00b7ge", "schuf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ART", "ADJA", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Es rauschen des Haines Gezweige so hell,", "tokens": ["Es", "rau\u00b7schen", "des", "Hai\u00b7nes", "Ge\u00b7zwei\u00b7ge", "so", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Es s\u00e4uselt die Wiese, es rieselt der Quell.", "tokens": ["Es", "s\u00e4u\u00b7selt", "die", "Wie\u00b7se", ",", "es", "rie\u00b7selt", "der", "Quell", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.8": {"line.1": {"text": "Da klappen wohl tausend der S\u00e4rge zu:", "tokens": ["Da", "klap\u00b7pen", "wohl", "tau\u00b7send", "der", "S\u00e4r\u00b7ge", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "CARD", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das Lei'rergesindel taumelt zur Ruh;", "tokens": ["Das", "Lei'\u00b7rer\u00b7ge\u00b7sin\u00b7del", "tau\u00b7melt", "zur", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da springen wohl tausend S\u00e4rge auf:", "tokens": ["Da", "sprin\u00b7gen", "wohl", "tau\u00b7send", "S\u00e4r\u00b7ge", "auf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "CARD", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein S\u00e4ngergeschlecht beginnt seinen Lauf!", "tokens": ["Ein", "S\u00e4n\u00b7ger\u00b7ge\u00b7schlecht", "be\u00b7ginnt", "sei\u00b7nen", "Lauf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Ein k\u00f6rnig Geschlecht f\u00fcr endlose Zeit,", "tokens": ["Ein", "k\u00f6r\u00b7nig", "Ge\u00b7schlecht", "f\u00fcr", "end\u00b7lo\u00b7se", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ges\u00e4ugt an den Br\u00fcsten der Ewigkeit,", "tokens": ["Ge\u00b7s\u00e4ugt", "an", "den", "Br\u00fcs\u00b7ten", "der", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Das Auge ein Blitz und doch so mild,", "tokens": ["Das", "Au\u00b7ge", "ein", "Blitz", "und", "doch", "so", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ADV", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Antlitz der Liebe rosiges Bild.", "tokens": ["Das", "Ant\u00b7litz", "der", "Lie\u00b7be", "ro\u00b7si\u00b7ges", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Und siehe, der herrliche Bardenchor", "tokens": ["Und", "sie\u00b7he", ",", "der", "herr\u00b7li\u00b7che", "Bar\u00b7denc\u00b7hor"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "$,", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Hebt rauschend die klingenden Harfen empor,", "tokens": ["Hebt", "rau\u00b7schend", "die", "klin\u00b7gen\u00b7den", "Har\u00b7fen", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wie Seraphsgebet, wie Lavinenklang", "tokens": ["Wie", "Se\u00b7raphs\u00b7ge\u00b7bet", ",", "wie", "La\u00b7vi\u00b7nen\u00b7klang"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAV", "NE", "$,", "PWAV", "NN"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.4": {"text": "Verhallt' es die weiten Gefild' entlang.", "tokens": ["Ver\u00b7hallt'", "es", "die", "wei\u00b7ten", "Ge\u00b7fild'", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.11": {"line.1": {"text": "Es horchen die Wasser und hemmen den Lauf,", "tokens": ["Es", "hor\u00b7chen", "die", "Was\u00b7ser", "und", "hem\u00b7men", "den", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Rosen bl\u00fchn, als sei Fr\u00fchling, auf,", "tokens": ["Die", "Ro\u00b7sen", "bl\u00fchn", ",", "als", "sei", "Fr\u00fch\u00b7ling", ",", "auf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "KOKOM", "VAFIN", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und um sie in vollerem Mondenschein", "tokens": ["Und", "um", "sie", "in", "vol\u00b7le\u00b7rem", "Mon\u00b7den\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Drehn sch\u00f6ne Elfenkinder den Reihn.", "tokens": ["Drehn", "sch\u00f6\u00b7ne", "El\u00b7fen\u00b7kin\u00b7der", "den", "Reihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.12": {"line.1": {"text": "In Wonne sch\u00fcttelt sein Haupt der Baum,", "tokens": ["In", "Won\u00b7ne", "sch\u00fct\u00b7telt", "sein", "Haupt", "der", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Vogel am Ast tr\u00e4umt s\u00fc\u00dferen Traum;", "tokens": ["Der", "Vo\u00b7gel", "am", "Ast", "tr\u00e4umt", "s\u00fc\u00b7\u00dfe\u00b7ren", "Traum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPRART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So singen alln\u00e4chtlich zur Mitternachtzeit", "tokens": ["So", "sin\u00b7gen", "all\u00b7n\u00e4cht\u00b7lich", "zur", "Mit\u00b7ter\u00b7nacht\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ihr ewiges Lied sie: Unsterblichkeit!", "tokens": ["Ihr", "e\u00b7wi\u00b7ges", "Lied", "sie", ":", "U\u00b7nsterb\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "$.", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.13": {"line.1": {"text": "Wie liederbegr\u00fc\u00dft und rosenbekr\u00e4nzt", "tokens": ["Wie", "lie\u00b7der\u00b7be\u00b7gr\u00fc\u00dft", "und", "ro\u00