{"dta.poem.2811": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nur ein Gedanken ists, an welchen du gew\u00f6hnen", "tokens": ["Nur", "ein", "Ge\u00b7dan\u00b7ken", "ists", ",", "an", "wel\u00b7chen", "du", "ge\u00b7w\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "$,", "APPR", "PWAT", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dich mu\u00dft, um dein Geschick im Geiste zu vers\u00f6hnen.", "tokens": ["Dich", "mu\u00dft", ",", "um", "dein", "Ge\u00b7schick", "im", "Geis\u00b7te", "zu", "ver\u00b7s\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUI", "PPOSAT", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und an wie mangerlei Gedanken hast du dich", "tokens": ["Und", "an", "wie", "man\u00b7ger\u00b7lei", "Ge\u00b7dan\u00b7ken", "hast", "du", "dich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "KOKOM", "PIAT", "NN", "VAFIN", "PPER", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht schon gew\u00f6hnt! man denkt zuletzt in Alles sich.", "tokens": ["Nicht", "schon", "ge\u00b7w\u00f6hnt", "!", "man", "denkt", "zu\u00b7letzt", "in", "Al\u00b7les", "sich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVPP", "$.", "PIS", "VVFIN", "ADV", "APPR", "PIS", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das Unverhoffteste, wenn es getreten ein,", "tokens": ["Das", "Un\u00b7ver\u00b7hoff\u00b7tes\u00b7te", ",", "wenn", "es", "ge\u00b7tre\u00b7ten", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVPP", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sieht endlich aus als k\u00f6nnt' es gar nicht anders seyn.", "tokens": ["Sieht", "end\u00b7lich", "aus", "als", "k\u00f6nnt'", "es", "gar", "nicht", "an\u00b7ders", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "KOKOM", "VMFIN", "PPER", "ADV", "PTKNEG", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und wenn gleichg\u00fcltig uns durch die Gewohnheit werden", "tokens": ["Und", "wenn", "gleich\u00b7g\u00fcl\u00b7tig", "uns", "durch", "die", "Ge\u00b7wohn\u00b7heit", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADJD", "PPER", "APPR", "ART", "NN", "VAINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am Ende Freuden selbst, warum nicht auch Beschwerden?", "tokens": ["Am", "En\u00b7de", "Freu\u00b7den", "selbst", ",", "wa\u00b7rum", "nicht", "auch", "Be\u00b7schwer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "ADV", "$,", "PWAV", "PTKNEG", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}