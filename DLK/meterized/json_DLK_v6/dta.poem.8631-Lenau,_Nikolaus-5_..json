{"dta.poem.8631": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "5 .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Auf dem Teich, dem regungslosen,", "tokens": ["Auf", "dem", "Teich", ",", "dem", "re\u00b7gungs\u00b7lo\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weilt des Mondes holder Glanz,", "tokens": ["Weilt", "des", "Mon\u00b7des", "hol\u00b7der", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Flechtend seine bleichen Rosen", "tokens": ["Flech\u00b7tend", "sei\u00b7ne", "blei\u00b7chen", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In des Schilfes gr\u00fcnen Kranz.", "tokens": ["In", "des", "Schil\u00b7fes", "gr\u00fc\u00b7nen", "Kranz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hirsche wandeln dort am H\u00fcgel,", "tokens": ["Hir\u00b7sche", "wan\u00b7deln", "dort", "am", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blicken in die Nacht empor;", "tokens": ["Bli\u00b7cken", "in", "die", "Nacht", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Manchmal regt sich das Gefl\u00fcgel", "tokens": ["Manch\u00b7mal", "regt", "sich", "das", "Ge\u00b7fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00e4umerisch im tiefen Rohr.", "tokens": ["Tr\u00e4u\u00b7me\u00b7risch", "im", "tie\u00b7fen", "Rohr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Weinend mu\u00df mein Blick sich senken;", "tokens": ["Wei\u00b7nend", "mu\u00df", "mein", "Blick", "sich", "sen\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPOSAT", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die tiefste Seele geht", "tokens": ["Durch", "die", "tiefs\u00b7te", "See\u00b7le", "geht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir ein s\u00fc\u00dfes Deingedenken,", "tokens": ["Mir", "ein", "s\u00fc\u00b7\u00dfes", "Dein\u00b7ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein stilles Nachtgebet!", "tokens": ["Wie", "ein", "stil\u00b7les", "Nacht\u00b7ge\u00b7bet", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}