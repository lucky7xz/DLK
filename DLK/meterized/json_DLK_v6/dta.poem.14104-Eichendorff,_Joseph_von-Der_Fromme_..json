{"dta.poem.14104": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Fromme .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "So walte Gott! \u2014 ich l\u00f6s' des Schiffleins Bande,", "tokens": ["So", "wal\u00b7te", "Gott", "!", "ich", "l\u00f6s'", "des", "Schif\u00b7fleins", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$.", "$(", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wegweiser sind die Stern', die ewig hellen,", "tokens": ["Weg\u00b7wei\u00b7ser", "sind", "die", "Stern'", ",", "die", "e\u00b7wig", "hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Viel Seegel fahren da und frisch' Gesellen", "tokens": ["Viel", "See\u00b7gel", "fah\u00b7ren", "da", "und", "frisch'", "Ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Begr\u00fc\u00dfend uns von ihrer Schiffe Rande.", "tokens": ["Be\u00b7gr\u00fc\u00b7\u00dfend", "uns", "von", "ih\u00b7rer", "Schif\u00b7fe", "Ran\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}