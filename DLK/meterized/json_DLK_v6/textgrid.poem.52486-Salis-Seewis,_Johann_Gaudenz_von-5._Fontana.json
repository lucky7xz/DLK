{"textgrid.poem.52486": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "5. Fontana", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Preisend soll den Helden mein Gesang erheben!", "tokens": ["Prei\u00b7send", "soll", "den", "Hel\u00b7den", "mein", "Ge\u00b7sang", "er\u00b7he\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Vaterland! weih ihm dein Dankgef\u00fchl:", "tokens": ["Va\u00b7ter\u00b7land", "!", "weih", "ihm", "dein", "Dank\u00b7ge\u00b7f\u00fchl", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Sieh, er weihte dir sein edles Leben;", "tokens": ["Sieh", ",", "er", "weih\u00b7te", "dir", "sein", "ed\u00b7les", "Le\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Starb f\u00fcr dich im wilden Schlachtgew\u00fchl!", "tokens": ["Starb", "f\u00fcr", "dich", "im", "wil\u00b7den", "Schlacht\u00b7ge\u00b7w\u00fchl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "So stehn deine Berge fest in Ungewittern,", "tokens": ["So", "stehn", "dei\u00b7ne", "Ber\u00b7ge", "fest", "in", "Un\u00b7ge\u00b7wit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie Fontana dort im Treffen stand:", "tokens": ["Wie", "Fon\u00b7ta\u00b7na", "dort", "im", "Tref\u00b7fen", "stand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Deinen Helden konnte nichts ersch\u00fcttern;", "tokens": ["Dei\u00b7nen", "Hel\u00b7den", "konn\u00b7te", "nichts", "er\u00b7sch\u00fct\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Niemals bebt ihm weder Herz noch Hand.", "tokens": ["Nie\u00b7mals", "bebt", "ihm", "we\u00b7der", "Herz", "noch", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "NN", "ADV", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Immer tiefer st\u00fcrzt' er sich ins Kampfget\u00fcmmel,", "tokens": ["Im\u00b7mer", "tie\u00b7fer", "st\u00fcrzt'", "er", "sich", "ins", "Kampf\u00b7ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Schritt entgegen heiter der Gefahr;", "tokens": ["Schritt", "ent\u00b7ge\u00b7gen", "hei\u00b7ter", "der", "Ge\u00b7fahr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPO", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Opfert sich \u2013 ihn st\u00e4rkte Gott vom Himmel \u2013", "tokens": ["Op\u00b7fert", "sich", "\u2013", "ihn", "st\u00e4rk\u00b7te", "Gott", "vom", "Him\u00b7mel", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$(", "PPER", "VVFIN", "NN", "APPRART", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Auf der Freiheit heiligem Altar.", "tokens": ["Auf", "der", "Frei\u00b7heit", "hei\u00b7li\u00b7gem", "Al\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Blutig, schwer verwund't, begann er nun zu sinken;", "tokens": ["Blu\u00b7tig", ",", "schwer", "ver\u00b7wund't", ",", "be\u00b7gann", "er", "nun", "zu", "sin\u00b7ken", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VVPP", "$,", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Und noch klirrten Schwerter um ihn her:", "tokens": ["Und", "noch", "klirr\u00b7ten", "Schwer\u00b7ter", "um", "ihn", "her", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Seine Wunde deckt er mit der Linken,", "tokens": ["Sei\u00b7ne", "Wun\u00b7de", "deckt", "er", "mit", "der", "Lin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mit der Rechten hielt er noch den Speer.", "tokens": ["Mit", "der", "Rech\u00b7ten", "hielt", "er", "noch", "den", "Speer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "\u00bbzaget nicht um Eines Mannes Fall, ihr Br\u00fcder!\u00ab", "tokens": ["\u00bb", "za\u00b7get", "nicht", "um", "Ei\u00b7nes", "Man\u00b7nes", "Fall", ",", "ihr", "Br\u00fc\u00b7der", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "NN", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Rief er. \u00bbGilt es doch das Vaterland!\u00ab", "tokens": ["Rief", "er", ".", "\u00bb", "Gilt", "es", "doch", "das", "Va\u00b7ter\u00b7land", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$.", "$(", "VVFIN", "PPER", "ADV", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Winkelried sah segnend auf ihn nieder,", "tokens": ["Win\u00b7kel\u00b7ried", "sah", "seg\u00b7nend", "auf", "ihn", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Als er's sprach, die Palme in der Hand.", "tokens": ["Als", "er's", "sprach", ",", "die", "Pal\u00b7me", "in", "der", "Hand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Itzt, da schon sein Geist, frei von des Lebens M\u00fchen,", "tokens": ["Itzt", ",", "da", "schon", "sein", "Geist", ",", "frei", "von", "des", "Le\u00b7bens", "M\u00fc\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ADV", "PPOSAT", "NN", "$,", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Strahlend zu der Gottheit Thron entfliegt,", "tokens": ["Strah\u00b7lend", "zu", "der", "Got\u00b7theit", "Thron", "ent\u00b7fliegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sieht er noch das Heer der Feinde fliehen;", "tokens": ["Sieht", "er", "noch", "das", "Heer", "der", "Fein\u00b7de", "flie\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sieht es, wie sein kleiner Haufe siegt. \u2013", "tokens": ["Sieht", "es", ",", "wie", "sein", "klei\u00b7ner", "Hau\u00b7fe", "siegt", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPOSAT", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Rinnen wird ihm der Bewundrung stille Thr\u00e4ne,", "tokens": ["Rin\u00b7nen", "wird", "ihm", "der", "Be\u00b7wund\u00b7rung", "stil\u00b7le", "Thr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Wann, voll Ehrfurcht, ihn die Nachwelt nennt;", "tokens": ["Wann", ",", "voll", "Ehr\u00b7furcht", ",", "ihn", "die", "Nach\u00b7welt", "nennt", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "ADJD", "NN", "$,", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ach! zur Schande jedem seiner S\u00f6hne,", "tokens": ["Ach", "!", "zur", "Schan\u00b7de", "je\u00b7dem", "sei\u00b7ner", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPRART", "NN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der itzt kaum den gro\u00dfen Namen kennt.", "tokens": ["Der", "itzt", "kaum", "den", "gro\u00b7\u00dfen", "Na\u00b7men", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Heilig ist der Ort, der einst dein Blut getrunken!", "tokens": ["Hei\u00b7lig", "ist", "der", "Ort", ",", "der", "einst", "dein", "Blut", "ge\u00b7trun\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Heilig uns dein Grab, du edler Mann!", "tokens": ["Hei\u00b7lig", "uns", "dein", "Grab", ",", "du", "ed\u00b7ler", "Mann", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "PPER", "PPOSAT", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ist gleich l\u00e4ngst dein H\u00fcgel eingesunken;", "tokens": ["Ist", "gleich", "l\u00e4ngst", "dein", "H\u00fc\u00b7gel", "ein\u00b7ge\u00b7sun\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Zeigt ihn nur kein Stein dem Wandrer an!", "tokens": ["Zeigt", "ihn", "nur", "kein", "Stein", "dem", "Wand\u00b7rer", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}