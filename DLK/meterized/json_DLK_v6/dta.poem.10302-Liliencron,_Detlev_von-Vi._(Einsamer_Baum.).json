{"dta.poem.10302": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Vi.  \n (Einsamer Baum.)", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Funkelt dort die S\u00e4ulenfronte,", "tokens": ["Fun\u00b7kelt", "dort", "die", "S\u00e4u\u00b7len\u00b7fron\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueberdacht von einer Pinie?", "tokens": ["Ue\u00b7berd\u00b7acht", "von", "ei\u00b7ner", "Pi\u00b7nie", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Einsam, fern am Horizonte,", "tokens": ["Ein\u00b7sam", ",", "fern", "am", "Ho\u00b7ri\u00b7zon\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fern am Deich, der blassen Linie,", "tokens": ["Fern", "am", "Deich", ",", "der", "blas\u00b7sen", "Li\u00b7nie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Steht ein B\u00e4umchen, krank und ruppig,", "tokens": ["Steht", "ein", "B\u00e4um\u00b7chen", ",", "krank", "und", "rup\u00b7pig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ohne Bl\u00e4tter, ohne Nest,", "tokens": ["Oh\u00b7ne", "Bl\u00e4t\u00b7ter", ",", "oh\u00b7ne", "Nest", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schwarz vom Seesalz, kraus und struppig,", "tokens": ["Schwarz", "vom", "See\u00b7salz", ",", "kraus", "und", "strup\u00b7pig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Arg zerzaust vom ewigen West.", "tokens": ["Arg", "zer\u00b7zaust", "vom", "e\u00b7wi\u00b7gen", "West", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Einmal ist er gr\u00fcn geworden,", "tokens": ["Ein\u00b7mal", "ist", "er", "gr\u00fcn", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als ein hei\u00dfes Land im S\u00fcden", "tokens": ["Als", "ein", "hei\u00b7\u00dfes", "Land", "im", "S\u00fc\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sandte seinen Gru\u00df nach Norden,", "tokens": ["Sand\u00b7te", "sei\u00b7nen", "Gru\u00df", "nach", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ku\u00df und Trost dem Lebensm\u00fcden.", "tokens": ["Ku\u00df", "und", "Trost", "dem", "Le\u00b7bens\u00b7m\u00fc\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Einmal bl\u00fchten seine Zweige,", "tokens": ["Ein\u00b7mal", "bl\u00fch\u00b7ten", "sei\u00b7ne", "Zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Einmal zog ein Cymbelzug,", "tokens": ["Ein\u00b7mal", "zog", "ein", "Cym\u00b7bel\u00b7zug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Als in roter Sonnenneige", "tokens": ["Als", "in", "ro\u00b7ter", "Son\u00b7nen\u00b7nei\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dort ein Herz am andern schlug.", "tokens": ["Dort", "ein", "Herz", "am", "an\u00b7dern", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Leise kam die Flut gezogen,", "tokens": ["Lei\u00b7se", "kam", "die", "Flut", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00fcmmer hob sie von den Watten,", "tokens": ["Tr\u00fcm\u00b7mer", "hob", "sie", "von", "den", "Wat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dunkle Halligwerften trogen,", "tokens": ["Dunk\u00b7le", "Hal\u00b7lig\u00b7werf\u00b7ten", "tro\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Todesfeuchte Kasematten.", "tokens": ["To\u00b7des\u00b7feuch\u00b7te", "Ka\u00b7se\u00b7mat\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch die Luft, wie m\u00fcde Greise,", "tokens": ["Durch", "die", "Luft", ",", "wie", "m\u00fc\u00b7de", "Grei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schleppt ein wei\u00df Gew\u00f6lke sich,", "tokens": ["Schleppt", "ein", "wei\u00df", "Ge\u00b7w\u00f6l\u00b7ke", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "VVFIN", "NN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Abgemattet von der Reise,", "tokens": ["Ab\u00b7ge\u00b7mat\u00b7tet", "von", "der", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Marsch aus fremdem Himmelstrich.", "tokens": ["Marsch", "aus", "frem\u00b7dem", "Him\u00b7mel\u00b7strich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Bleicher Stern im Wolkenspalte,", "tokens": ["Blei\u00b7cher", "Stern", "im", "Wol\u00b7ken\u00b7spal\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wild phantastische Gebilde,", "tokens": ["Wild", "phan\u00b7tas\u00b7ti\u00b7sche", "Ge\u00b7bil\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Menschen, nordisch n\u00fcchtern kalte,", "tokens": ["Men\u00b7schen", ",", "nor\u00b7disch", "n\u00fcch\u00b7tern", "kal\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "ADJD", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Odins Schwert und Asenschilde.", "tokens": ["O\u00b7dins", "Schwert", "und", "A\u00b7sen\u00b7schil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hohe Flut, gelispellose,", "tokens": ["Ho\u00b7he", "Flut", ",", "ge\u00b7lis\u00b7pel\u00b7lo\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Spielt herauf zu Deich und Baum.", "tokens": ["Spielt", "her\u00b7auf", "zu", "Deich", "und", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKA", "ADJD", "KON", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Meine blasse K\u00fcstenrose", "tokens": ["Mei\u00b7ne", "blas\u00b7se", "K\u00fcs\u00b7ten\u00b7ro\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lehnt an mir, ein s\u00fc\u00dfer Traum.", "tokens": ["Lehnt", "an", "mir", ",", "ein", "s\u00fc\u00b7\u00dfer", "Traum", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nun von meinem Fenster seh\u2019 ich", "tokens": ["Nun", "von", "mei\u00b7nem", "Fens\u00b7ter", "seh'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oft den Baum mit toten Zweigen.", "tokens": ["Oft", "den", "Baum", "mit", "to\u00b7ten", "Zwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter seinen \u00c4sten steh\u2019 ich", "tokens": ["Un\u00b7ter", "sei\u00b7nen", "\u00c4s\u00b7ten", "steh'", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oft im tiefen Winterschweigen.", "tokens": ["Oft", "im", "tie\u00b7fen", "Win\u00b7ter\u00b7schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Oft, ich halt\u2019 des Hutes Krempe,", "tokens": ["Oft", ",", "ich", "halt'", "des", "Hu\u00b7tes", "Krem\u00b7pe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Freut mich dort der Wetterstreit,", "tokens": ["Freut", "mich", "dort", "der", "Wet\u00b7ter\u00b7streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Singt der Sturm, der rasche K\u00e4mpe,", "tokens": ["Singt", "der", "Sturm", ",", "der", "ra\u00b7sche", "K\u00e4m\u00b7pe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Grenzenloser Einsamkeit.", "tokens": ["Gren\u00b7zen\u00b7lo\u00b7ser", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}