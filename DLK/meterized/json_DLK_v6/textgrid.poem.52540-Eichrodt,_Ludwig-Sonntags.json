{"textgrid.poem.52540": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Sonntags", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die gro\u00dfen wei\u00dfen Flocken", "tokens": ["Die", "gro\u00b7\u00dfen", "wei\u00b7\u00dfen", "Flo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fliegen so leis und matt,", "tokens": ["Flie\u00b7gen", "so", "leis", "und", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Freudige Festtagsglocken", "tokens": ["Freu\u00b7di\u00b7ge", "Fest\u00b7tags\u00b7glo\u00b7cken"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "L\u00e4uten \u00fcber die Stadt.", "tokens": ["L\u00e4u\u00b7ten", "\u00fc\u00b7ber", "die", "Stadt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Da sitz ich am Tisch und schreibe,", "tokens": ["Da", "sitz", "ich", "am", "Tisch", "und", "schrei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "APPRART", "NN", "KON", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schreibe wie mir es gl\u00fcckt,", "tokens": ["Schrei\u00b7be", "wie", "mir", "es", "gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPER", "PPER", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Dort an eisblumiger Scheibe", "tokens": ["Dort", "an", "eis\u00b7blu\u00b7mi\u00b7ger", "Schei\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hungrig ein V\u00f6glein pickt.", "tokens": ["Hung\u00b7rig", "ein", "V\u00f6\u00b7glein", "pickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "In meiner Stub hierinnen", "tokens": ["In", "mei\u00b7ner", "Stub", "hie\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist es so warm und still,", "tokens": ["Ist", "es", "so", "warm", "und", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich fange mich an zu besinnen", "tokens": ["Ich", "fan\u00b7ge", "mich", "an", "zu", "be\u00b7sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APZR", "PTKZU", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Was dir ich schreiben will.", "tokens": ["Was", "dir", "ich", "schrei\u00b7ben", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "In kalten Kirchenr\u00e4umen", "tokens": ["In", "kal\u00b7ten", "Kir\u00b7chen\u00b7r\u00e4u\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Betest du jetzt f\u00fcr mich,", "tokens": ["Be\u00b7test", "du", "jetzt", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Ich wei\u00df nur zu tr\u00e4umen", "tokens": ["Und", "Ich", "wei\u00df", "nur", "zu", "tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PTKZU", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und thue ", "tokens": ["Und", "thue"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+", "measure": "iambic.single"}}}}}