{"dta.poem.14783": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Reitersmann .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Sie konnte nicht weiter singen,", "tokens": ["Sie", "konn\u00b7te", "nicht", "wei\u00b7ter", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vor bitterem Schmerz und Leid,", "tokens": ["Vor", "bit\u00b7te\u00b7rem", "Schmerz", "und", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Augen ihr \u00fcbergingen", "tokens": ["Die", "Au\u00b7gen", "ihr", "\u00fc\u00b7ber\u00b7gin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In ihrer Einsamkeit.", "tokens": ["In", "ih\u00b7rer", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}