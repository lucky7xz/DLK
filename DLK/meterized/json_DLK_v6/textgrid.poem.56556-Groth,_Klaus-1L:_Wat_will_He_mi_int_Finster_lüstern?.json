{"textgrid.poem.56556": {"metadata": {"author": {"name": "Groth, Klaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wat will He mi int Finster l\u00fcstern?", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wat will He mi int Finster l\u00fcstern?", "tokens": ["Wat", "will", "He", "mi", "int", "Fins\u00b7ter", "l\u00fcs\u00b7tern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "NE", "NE", "VVFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ik seet je ganz tofr\u0119dn in D\u00fcstern", "tokens": ["Ik", "seet", "je", "ganz", "to\u00b7fr\u0119dn", "in", "D\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "ADV", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Un h\u00f6r min braten Appeln gr\u00fcstern", "tokens": ["Un", "h\u00f6r", "min", "bra\u00b7ten", "Ap\u00b7peln", "gr\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "\u2013 Dat mag ik geern \u2013", "tokens": ["\u2013", "Dat", "mag", "ik", "ge\u00b7ern", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Un wull mi \u0119bn en Pip anp\u00fcstern", "tokens": ["Un", "wull", "mi", "\u0119bn", "en", "Pip", "an\u00b7p\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Un spikeleern.", "tokens": ["Un", "spi\u00b7ke\u00b7leern", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ik weet ni, seeg ik Em sodenni,", "tokens": ["Ik", "weet", "ni", ",", "seeg", "ik", "Em", "so\u00b7den\u00b7ni", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "FM", "FM", "FM", "FM", "$,"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So ward mi op en Art elenni,", "tokens": ["So", "ward", "mi", "op", "en", "Art", "e\u00b7len\u00b7ni", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.3": {"text": "Mi kamt, ik mark ni rech wodenni,", "tokens": ["Mi", "kamt", ",", "ik", "mark", "ni", "rech", "wo\u00b7den\u00b7ni", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "De Rimelsch op,", "tokens": ["De", "Ri\u00b7mel\u00b7sch", "op", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Un brummt mi, as de Im, lebenni", "tokens": ["Un", "brummt", "mi", ",", "as", "de", "Im", ",", "le\u00b7ben\u00b7ni"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "VVFIN", "NE", "$,", "FM.la", "FM.la", "FM.la", "$,", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Herum in Kopp.", "tokens": ["He\u00b7rum", "in", "Kopp", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Dat treckt mi rein mit Macht na't Finster,", "tokens": ["Dat", "treckt", "mi", "rein", "mit", "Macht", "na't", "Fins\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "ADJD", "APPR", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "As weert wat rechts, was buten glinster,", "tokens": ["As", "weert", "wat", "rechts", ",", "was", "bu\u00b7ten", "glins\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "PWS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Un dreiht mi denn en Barg Gespinster", "tokens": ["Un", "dreiht", "mi", "denn", "en", "Barg", "Ge\u00b7spins\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NE", "KON", "FM", "FM", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "V\u0153rn Ogen rund \u2013", "tokens": ["V\u0153rn", "O\u00b7gen", "rund", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ik kenn ni \u00bbheure\u00ab oder \u00bbwinster\u00ab,", "tokens": ["Ik", "kenn", "ni", "\u00bb", "heu\u00b7re", "\u00ab", "o\u00b7der", "\u00bb", "wins\u00b7ter", "\u00ab", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$(", "ADJA", "$(", "KON", "$(", "ADJD", "$(", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ik dwattsche Hund.", "tokens": ["Ik", "dwatt\u00b7sche", "Hund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ik weet wul, dat He Allns ant Band hett,", "tokens": ["Ik", "weet", "wul", ",", "dat", "He", "Allns", "ant", "Band", "hett", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wat phantaseert un keen Verstand hett,", "tokens": ["Wat", "phan\u00b7ta\u00b7seert", "un", "ke\u00b7en", "Ver\u00b7stand", "hett", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "VVFIN", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "As Rimers, oder wat en Brand hett", "tokens": ["As", "Ri\u00b7mers", ",", "o\u00b7der", "wat", "en", "Brand", "hett"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "KON", "FM", "FM", "NN", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bi'n Hochtid kr\u0119gn,", "tokens": ["Bi'n", "Hoch\u00b7tid", "kr\u0119gn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Doch dacht ik nich, dat He de Hand sett", "tokens": ["Doch", "dacht", "ik", "nich", ",", "dat", "He", "de", "Hand", "sett"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NE", "PTKNEG", "$,", "ART", "NN", "NE", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ann plattd\u00fctsch Br\u0119gn.", "tokens": ["Ann", "platt\u00b7d\u00fctsch", "Br\u0119gn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Wi s\u00fcnd je ganz und gar vern\u00fcnfti,", "tokens": ["Wi", "s\u00fcnd", "je", "ganz", "und", "gar", "ver\u00b7n\u00fcnf\u00b7ti", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ADV", "KON", "ADV", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Un mank de Rimsm\u0119d nich mal z\u00fcnfti,", "tokens": ["Un", "mank", "de", "Rims\u00b7m\u0119d", "nich", "mal", "z\u00fcnf\u00b7ti", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "NE", "PTKNEG", "ADV", "NE", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ok hebbt wi j\u00fcmmer unvern\u00fcnfti", "tokens": ["Ok", "hebbt", "wi", "j\u00fcm\u00b7mer", "un\u00b7ver\u00b7n\u00fcnf\u00b7ti"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "V\u0119l Klei to kn\u0119dn;", "tokens": ["V\u0119l", "Klei", "to", "kn\u0119dn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "So b\u0119 ik, lat He mi insk\u00fcnfti", "tokens": ["So", "b\u0119", "ik", ",", "lat", "He", "mi", "ins\u00b7k\u00fcnf\u00b7ti"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NE", "NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Man ganz tofr\u0119dn.", "tokens": ["Man", "ganz", "to\u00b7fr\u0119dn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "He lurt vun mi wul op sin Kringel?", "tokens": ["He", "lurt", "vun", "mi", "wul", "op", "sin", "Krin\u00b7gel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "He meent, Em h\u00f6rt sin Deel Geklingel", "tokens": ["He", "meent", ",", "Em", "h\u00f6rt", "sin", "Deel", "Ge\u00b7klin\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "NE", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vun jede Nachtigal und Singel-", "tokens": ["Vun", "je\u00b7de", "Nach\u00b7ti\u00b7gal", "und", "Sin\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "tr\u00fctjen, wat rimt,", "tokens": ["tr\u00fct\u00b7jen", ",", "wat", "rimt", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Un denkt, man sleiten ut den Swingel,", "tokens": ["Un", "denkt", ",", "man", "slei\u00b7ten", "ut", "den", "Swin\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PIS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn man't vers\u00fcmt.", "tokens": ["Wenn", "man't", "ver\u00b7s\u00fcmt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Ik heff man h\u00f6rt, dat He dat geern s\u00fcht,", "tokens": ["Ik", "heff", "man", "h\u00f6rt", ",", "dat", "He", "dat", "ge\u00b7ern", "s\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "VVFIN", "$,", "FM.nl", "FM.nl", "FM.nl", "FM.nl", "FM.nl", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.2": {"text": "Un Em dat sm\u00f6di umme Neern t\u00fcht,", "tokens": ["Un", "Em", "dat", "sm\u00f6\u00b7di", "um\u00b7me", "Neern", "t\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wenn man nothdrefti inne Feern s\u00fcht", "tokens": ["Wenn", "man", "noth\u00b7dref\u00b7ti", "in\u00b7ne", "Feern", "s\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "NE", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.4": {"text": "Na Sin Gesich \u2013", "tokens": ["Na", "Sin", "Ge\u00b7sich", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Un denn Een fleit, as op en Scheernt\u00fct,", "tokens": ["Un", "denn", "E\u00b7en", "fleit", ",", "as", "op", "en", "Scheern\u00b7t\u00fct", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJA", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Recht barmhartig.", "tokens": ["Recht", "barm\u00b7har\u00b7tig", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.8": {"line.1": {"text": "Uns feilt darto man ganz de Snawel,", "tokens": ["Uns", "feilt", "dar\u00b7to", "man", "ganz", "de", "Sna\u00b7wel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PIS", "ADV", "NE", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wi seggt en Keesmess to en Sawel,", "tokens": ["Wi", "seggt", "en", "Ke\u00b7es\u00b7mess", "to", "en", "Sa\u00b7wel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "FM", "FM", "FM", "FM", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Un Fork un Gaffel to en Gawel,", "tokens": ["Un", "Fork", "un", "Gaf\u00b7fel", "to", "en", "Ga\u00b7wel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "FM", "FM", "FM", "FM", "NN", "$,"], "meter": "+----+---", "measure": "dactylic.init"}, "line.4": {"text": "Wi s\u00fcnd wat drulli,", "tokens": ["Wi", "s\u00fcnd", "wat", "drul\u00b7li", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wi s\u00fcnd warafti ni cumpawel", "tokens": ["Wi", "s\u00fcnd", "wa\u00b7raf\u00b7ti", "ni", "cum\u00b7pa\u00b7wel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Un gar to knulli.", "tokens": ["Un", "gar", "to", "knul\u00b7li", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "NE", "NE", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "He treckt je doch dat ganze Land um,", "tokens": ["He", "treckt", "je", "doch", "dat", "gan\u00b7ze", "Land", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Un sl\u0119pt dat Weltmeer oppen Strand rum,", "tokens": ["Un", "sl\u0119pt", "dat", "Welt\u00b7meer", "op\u00b