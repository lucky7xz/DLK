{"dta.poem.12408": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Romanze von den Schneidern .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es sind einmal drey Schneider gewesen,               ", "tokens": ["Es", "sind", "ein\u00b7mal", "drey", "Schnei\u00b7der", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "CARD", "NN", "VAPP", "$,"], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "O Je, es sind einmal drey Schneider gewesen,", "tokens": ["O", "Je", ",", "es", "sind", "ein\u00b7mal", "drey", "Schnei\u00b7der", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PPER", "VAFIN", "ADV", "CARD", "NN", "VAPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Sie haben ein Schnecken f\u00fcr ein B\u00e4ren angesehen,", "tokens": ["Sie", "ha\u00b7ben", "ein", "Schne\u00b7cken", "f\u00fcr", "ein", "B\u00e4\u00b7ren", "an\u00b7ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "O Je, O Je, O Je!", "tokens": ["O", "Je", ",", "O", "Je", ",", "O", "Je", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "ITJ", "ADV", "$,", "ITJ", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie waren dessen so voller Sorgen, O Je, u. s. w.", "tokens": ["Sie", "wa\u00b7ren", "des\u00b7sen", "so", "vol\u00b7ler", "Sor\u00b7gen", ",", "O", "Je", ",", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["PPER", "VAFIN", "PDS", "ADV", "ADJA", "NN", "$,", "ITJ", "ADV", "$,", "KON", "VVIMP", "NN"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie haben sich hinter ein Zaun verborgen, O Je, u. s. w.", "tokens": ["Sie", "ha\u00b7ben", "sich", "hin\u00b7ter", "ein", "Zaun", "ver\u00b7bor\u00b7gen", ",", "O", "Je", ",", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["PPER", "VAFIN", "PRF", "APPR", "ART", "NN", "VVPP", "$,", "ITJ", "ADV", "$,", "KON", "VVIMP", "NN"], "meter": "-+--+--+-+--+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Der erste sprach: Geh du voran, O Je, u. s. w.", "tokens": ["Der", "ers\u00b7te", "sprach", ":", "Geh", "du", "vo\u00b7ran", ",", "O", "Je", ",", "u.", "s.", "w."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["ART", "ADJA", "VVFIN", "$.", "NE", "PPER", "PTKVZ", "$,", "ITJ", "ADV", "$,", "KON", "VVIMP", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der andre sprach: Ich trau mich nicht vor, O Je, u. s. w.", "tokens": ["Der", "and\u00b7re", "sprach", ":", "Ich", "trau", "mich", "nicht", "vor", ",", "O", "Je", ",", "u.", "s.", "w."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["ART", "PIS", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,", "ITJ", "ADV", "$,", "KON", "VVIMP", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Der dritte der war wohl auch dabey, O Je, u. s. w.", "tokens": ["Der", "drit\u00b7te", "der", "war", "wohl", "auch", "da\u00b7bey", ",", "O", "Je", ",", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["ART", "ADJA", "ART", "VAFIN", "ADV", "ADV", "PAV", "$,", "ITJ", "ADV", "$,", "KON", "VVIMP", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Er sprach: der fri\u00dft uns alle drey. O Je, u. s. w.", "tokens": ["Er", "sprach", ":", "der", "fri\u00dft", "uns", "al\u00b7le", "drey", ".", "O", "Je", ",", "u.", "s.", "w."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["PPER", "VVFIN", "$.", "ART", "VVFIN", "PPER", "PIAT", "CARD", "$.", "NE", "ADV", "$,", "KON", "VVIMP", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und als sie sind zusammen kommen, O Je, u. s. w.", "tokens": ["Und", "als", "sie", "sind", "zu\u00b7sam\u00b7men", "kom\u00b7men", ",", "O", "Je", ",", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["KON", "KOUS", "PPER", "VAFIN", "ADV", "VVINF", "$,", "ITJ", "ADV", "$,", "KON", "VVIMP", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "So haben sie das Gewehr genommen. O Je, u. s. w.", "tokens": ["So", "ha\u00b7ben", "sie", "das", "Ge\u00b7wehr", "ge\u00b7nom\u00b7men", ".", "O", "Je", ",", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$.", "NE", "ADV", "$,", "KON", "VVIMP", "NN"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Und da sie kommen zu dem Streit, O Je, u. s. w.", "tokens": ["Und", "da", "sie", "kom\u00b7men", "zu", "dem", "Streit", ",", "O", "Je", ",", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,", "ITJ", "ADV", "$,", "KON", "VVIMP", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da macht ein jeder Reu und Leid, O Je, u. s. w.", "tokens": ["Da", "macht", "ein", "je\u00b7der", "Reu", "und", "Leid", ",", "O", "Je", ",", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["ADV", "VVFIN", "ART", "PIAT", "NN", "KON", "NN", "$,", "ITJ", "ADV", "$,", "KON", "VVIMP", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Und da sie auf ihn wollten hin, O Je, u. s. w.", "tokens": ["Und", "da", "sie", "auf", "ihn", "woll\u00b7ten", "hin", ",", "O", "Je", ",", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPER", "VMFIN", "ADV", "$,", "ITJ", "ADV", "$,", "KON", "VVIMP", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da ging es ihnen durch den Sinn: O Je, u. s. w.", "tokens": ["Da", "ging", "es", "ih\u00b7nen", "durch", "den", "Sinn", ":", "O", "Je", ",", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "$.", "NE", "ADV", "$,", "KON", "VVIMP", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "\u201eheraus mit dir du Teuxels Vieh, O Je, u. s. w.", "tokens": ["\u201e", "he\u00b7raus", "mit", "dir", "du", "Teu\u00b7xels", "Vieh", ",", "O", "Je", ",", "u.", "s.", "w."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["$(", "ADV", "APPR", "PPER", "PPER", "NE", "NN", "$,", "ITJ", "ADV", "$,", "KON", "VVIMP", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u201ewann du willt haben einen Stich.\u201c O Je, u. s. w.", "tokens": ["\u201e", "wann", "du", "willt", "ha\u00b7ben", "ei\u00b7nen", "Stich", ".", "\u201c", "O", "Je", ",", "u.", "s.", "w."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["$(", "PWAV", "PPER", "VMFIN", "VAFIN", "ART", "NN", "$.", "$(", "ITJ", "ADV", "$,", "KON", "VVIMP", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Der Schneck, der streckt die Ohren heraus, O Je, u. s. w.", "tokens": ["Der", "Schneck", ",", "der", "streckt", "die", "Oh\u00b7ren", "he\u00b7raus", ",", "O", "Je", ",", "u.", "s.", "w."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ITJ", "ADV", "$,", "KON", "VVIMP", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Schneider zittern, es ist ein Grau\u00df. O Je, u. s. w.", "tokens": ["Die", "Schnei\u00b7der", "zit\u00b7tern", ",", "es", "ist", "ein", "Grau\u00df", ".", "O", "Je", ",", "u.", "s.", "w."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["ART", "NE", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "$.", "NE", "ADV", "$,", "KON", "VVIMP", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.11": {"line.1": {"text": "Und da der Schneck das Haus bewegt, O Je, u. s. w.", "tokens": ["Und", "da", "der", "Schneck", "das", "Haus", "be\u00b7wegt", ",", "O", "Je", ",", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,", "ITJ", "ADV", "$,", "KON", "VVIMP", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So haben die Schneider das Gewehr abgelegt, O Je, u. s. w.", "tokens": ["So", "ha\u00b7ben", "die", "Schnei\u00b7der", "das", "Ge\u00b7wehr", "ab\u00b7ge\u00b7legt", ",", "O", "Je", ",", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["ADV", "VAFIN", "ART", "NE", "ART", "NN", "VVPP", "$,", "ITJ", "ADV", "$,", "KON", "VVIMP", "NN"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.12": {"line.1": {"text": "Der Schneck der kroch zum Haus heraus, O Je, u. s. w.", "tokens": ["Der", "Schneck", "der", "kroch", "zum", "Haus", "he\u00b7raus", ",", "O", "Je", ",", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["ART", "NN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,", "ITJ", "ADV", "$,", "KON", "VVIMP", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er jagt die Schneider beym Plunder hinaus. O Je, u. s. w.", "tokens": ["Er", "jagt", "die", "Schnei\u00b7der", "beym", "Plun\u00b7der", "hin\u00b7aus", ".", "O", "Je", ",", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["PPER", "VVFIN", "ART", "NE", "APPRART", "NN", "PTKVZ", "$.", "NE", "ADV", "$,", "KON", "VVIMP", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}