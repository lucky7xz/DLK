{"textgrid.poem.26614": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Am \u00f6den Schalter sitzest du", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am \u00f6den Schalter sitzest du", "tokens": ["Am", "\u00f6\u00b7den", "Schal\u00b7ter", "sit\u00b7zest", "du"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei grellem Lampenscheine;", "tokens": ["Bei", "grel\u00b7lem", "Lam\u00b7pen\u00b7schei\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Federn liegen rings in Ruh' \u2013", "tokens": ["Die", "Fe\u00b7dern", "lie\u00b7gen", "rings", "in", "Ruh'", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur emsig schreibt die deine.", "tokens": ["Nur", "em\u00b7sig", "schreibt", "die", "dei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "PPOSAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und wie ich trete jetzt heran,", "tokens": ["Und", "wie", "ich", "tre\u00b7te", "jetzt", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hebst du die Stirn, die bleiche,", "tokens": ["Hebst", "du", "die", "Stirn", ",", "die", "blei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und fast erschrocken nimmst du dann", "tokens": ["Und", "fast", "er\u00b7schro\u00b7cken", "nimmst", "du", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVINF", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Brief, den ich dir reiche.", "tokens": ["Den", "Brief", ",", "den", "ich", "dir", "rei\u00b7che", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und rasch umgl\u00fcht dein Angesicht", "tokens": ["Und", "rasch", "um\u00b7gl\u00fcht", "dein", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein fliegendes Err\u00f6then \u2013", "tokens": ["Ein", "flie\u00b7gen\u00b7des", "Er\u00b7r\u00f6\u00b7then", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch ist es deines Amtes Pflicht,", "tokens": ["Doch", "ist", "es", "dei\u00b7nes", "Am\u00b7tes", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Solch' holde Schaam zu t\u00f6dten.", "tokens": ["Solch'", "hol\u00b7de", "Schaam", "zu", "t\u00f6d\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dennoch, wie du den Schein mir schreibst,", "tokens": ["Den\u00b7noch", ",", "wie", "du", "den", "Schein", "mir", "schreibst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seh' ich die Finger beben,", "tokens": ["Seh'", "ich", "die", "Fin\u00b7ger", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ob du abgewandt mir bleibst,", "tokens": ["Und", "ob", "du", "ab\u00b7ge\u00b7wandt", "mir", "bleibst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVPP", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die zarte Brust sich heben.", "tokens": ["Die", "zar\u00b7te", "Brust", "sich", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nein, jenen Schwestern gleichst du nicht,", "tokens": ["Nein", ",", "je\u00b7nen", "Schwes\u00b7tern", "gleichst", "du", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDAT", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die mit verschnitt'nen Haaren", "tokens": ["Die", "mit", "ver\u00b7schnitt'\u00b7nen", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Brillen vor dem Angesicht", "tokens": ["Und", "Bril\u00b7len", "vor", "dem", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich zum Erwerben schaaren.", "tokens": ["Sich", "zum", "Er\u00b7wer\u00b7ben", "schaa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du f\u00fchlst, ich ahn' es, tief den Bruch,", "tokens": ["Du", "f\u00fchlst", ",", "ich", "ahn'", "es", ",", "tief", "den", "Bruch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$,", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der sich im Weib vollzogen,", "tokens": ["Der", "sich", "im", "Weib", "voll\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und siehst dich mit dem Contobuch", "tokens": ["Und", "siehst", "dich", "mit", "dem", "Con\u00b7to\u00b7buch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um's beste Theil betrogen.", "tokens": ["Um's", "bes\u00b7te", "Theil", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "In dieser harten Tage Lauf", "tokens": ["In", "die\u00b7ser", "har\u00b7ten", "Ta\u00b7ge", "Lauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gilt Nutzen nur und N\u00fctzen \u2013", "tokens": ["Gilt", "Nut\u00b7zen", "nur", "und", "N\u00fct\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "KON", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und m\u00f6ge dich besch\u00fctzen!", "tokens": ["Und", "m\u00f6\u00b7ge", "dich", "be\u00b7sch\u00fct\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}