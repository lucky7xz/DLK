{"textgrid.poem.26575": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da liegen sie vor mir jetzt deine Briefe,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da liegen sie vor mir jetzt deine Briefe,", "tokens": ["Da", "lie\u00b7gen", "sie", "vor", "mir", "jetzt", "dei\u00b7ne", "Brie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So arm an Inhalt \u2013 ja selbst arm an Trug;", "tokens": ["So", "arm", "an", "In\u00b7halt", "\u2013", "ja", "selbst", "arm", "an", "Trug", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "$(", "ADV", "ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erzwung'ne Worte, halbe nur und schiefe \u2013", "tokens": ["Er\u00b7zwung'\u00b7ne", "Wor\u00b7te", ",", "hal\u00b7be", "nur", "und", "schie\u00b7fe", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADV", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So fl\u00fcchtig wie dein fl\u00fccht'ger Federzug.", "tokens": ["So", "fl\u00fcch\u00b7tig", "wie", "dein", "fl\u00fccht'\u00b7ger", "Fe\u00b7der\u00b7zug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und hier dein Bild. Gezeichnet von der Sonne,", "tokens": ["Und", "hier", "dein", "Bild", ".", "Ge\u00b7zeich\u00b7net", "von", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "$.", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sieht es verbla\u00dft, verschwommen, wie im Traum;", "tokens": ["Sieht", "es", "ver\u00b7bla\u00dft", ",", "ver\u00b7schwom\u00b7men", ",", "wie", "im", "Traum", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$,", "VVPP", "$,", "PWAV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was ich betrachtet einst mit Leid und Wonne \u2013", "tokens": ["Was", "ich", "be\u00b7trach\u00b7tet", "einst", "mit", "Leid", "und", "Won\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Jetzt durch die Lupe unterscheid' ich's kaum.", "tokens": ["Jetzt", "durch", "die", "Lu\u00b7pe", "un\u00b7ter\u00b7scheid'", "ich's", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und nicht ein Nachgef\u00fchl des Schmerzes zittert", "tokens": ["Und", "nicht", "ein", "Nach\u00b7ge\u00b7f\u00fchl", "des", "Schmer\u00b7zes", "zit\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In meiner Brust, den ich dereinst gef\u00fchlt \u2013", "tokens": ["In", "mei\u00b7ner", "Brust", ",", "den", "ich", "de\u00b7reinst", "ge\u00b7f\u00fchlt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von jenem Schmerz, der mir den Tag verbittert", "tokens": ["Von", "je\u00b7nem", "Schmerz", ",", "der", "mir", "den", "Tag", "ver\u00b7bit\u00b7tert"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und meiner N\u00e4chte leisen Schlaf durchw\u00fchlt.", "tokens": ["Und", "mei\u00b7ner", "N\u00e4ch\u00b7te", "lei\u00b7sen", "Schlaf", "durch\u00b7w\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und staunend mu\u00df ich jetzt mich selber fragen:", "tokens": ["Und", "stau\u00b7nend", "mu\u00df", "ich", "jetzt", "mich", "sel\u00b7ber", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "PPER", "ADV", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie war es m\u00f6glich, da\u00df ich sie geliebt \u2013", "tokens": ["Wie", "war", "es", "m\u00f6g\u00b7lich", ",", "da\u00df", "ich", "sie", "ge\u00b7liebt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Geliebt in vollen, kr\u00e4ft'gen Mannestagen,", "tokens": ["Ge\u00b7liebt", "in", "vol\u00b7len", ",", "kr\u00e4ft'\u00b7gen", "Man\u00b7nes\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo man doch pr\u00fcft, eh' man sich ganz ergiebt?!", "tokens": ["Wo", "man", "doch", "pr\u00fcft", ",", "eh'", "man", "sich", "ganz", "er\u00b7giebt", "?!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "VVFIN", "$,", "KOUS", "PIS", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wie war es m\u00f6glich, diese schlaffen Z\u00fcge", "tokens": ["Wie", "war", "es", "m\u00f6g\u00b7lich", ",", "die\u00b7se", "schlaf\u00b7fen", "Z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "$,", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Einst sch\u00f6n zu finden \u2013 diesen schlaffen Leib?", "tokens": ["Einst", "sch\u00f6n", "zu", "fin\u00b7den", "\u2013", "die\u00b7sen", "schlaf\u00b7fen", "Leib", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "$(", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcrwahr, es weht mich an wie grelle L\u00fcge,", "tokens": ["F\u00fcr\u00b7wahr", ",", "es", "weht", "mich", "an", "wie", "grel\u00b7le", "L\u00fc\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df ich gegl\u00fcht, fast weibisch, f\u00fcr dies Weib.", "tokens": ["Da\u00df", "ich", "ge\u00b7gl\u00fcht", ",", "fast", "wei\u00b7bisch", ",", "f\u00fcr", "dies", "Weib", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "ADV", "ADJD", "$,", "APPR", "PDS", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "F\u00fcr dieses Weib, das, l\u00e4ngst in sich gebrochen,", "tokens": ["F\u00fcr", "die\u00b7ses", "Weib", ",", "das", ",", "l\u00e4ngst", "in", "sich", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PDS", "$,", "ADV", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich aufrecht nur durch harten Stolz erhielt,", "tokens": ["Sich", "auf\u00b7recht", "nur", "durch", "har\u00b7ten", "Stolz", "er\u00b7hielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und mit des eig'nen Herzens letztem Pochen", "tokens": ["Und", "mit", "des", "eig'\u00b7nen", "Her\u00b7zens", "letz\u00b7tem", "Po\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In kaltem, frechem Uebermuth gespielt.", "tokens": ["In", "kal\u00b7tem", ",", "fre\u00b7chem", "Ue\u00b7ber\u00b7muth", "ge\u00b7spielt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "F\u00fcr dieses Weib, das selbst verschm\u00e4ht, zu heucheln", "tokens": ["F\u00fcr", "die\u00b7ses", "Weib", ",", "das", "selbst", "ver\u00b7schm\u00e4ht", ",", "zu", "heu\u00b7cheln"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "ADV", "VVPP", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Liebe Wallung, wenn es sich ergab,", "tokens": ["Der", "Lie\u00b7be", "Wal\u00b7lung", ",", "wenn", "es", "sich", "er\u00b7gab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und, nicht begn\u00fcgt, mich lautlos hinzumeucheln,", "tokens": ["Und", ",", "nicht", "be\u00b7gn\u00fcgt", ",", "mich", "laut\u00b7los", "hin\u00b7zu\u00b7meu\u00b7cheln", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PTKNEG", "VVPP", "$,", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor aller Welt gebrochen mir den Stab.", "tokens": ["Vor", "al\u00b7ler", "Welt", "ge\u00b7bro\u00b7chen", "mir", "den", "Stab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Und doch die Beute, die es tief verachtet,", "tokens": ["Und", "doch", "die", "Beu\u00b7te", ",", "die", "es", "tief", "ver\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus festen Banden grausam nicht entlie\u00df \u2013", "tokens": ["Aus", "fes\u00b7ten", "Ban\u00b7den", "grau\u00b7sam", "nicht", "ent\u00b7lie\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bis es zuletzt, da ich nicht mehr getrachtet,", "tokens": ["Bis", "es", "zu\u00b7letzt", ",", "da", "ich", "nicht", "mehr", "ge\u00b7trach\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Befreit zu werden, pl\u00f6tzlich mich verstie\u00df ...", "tokens": ["Be\u00b7freit", "zu", "wer\u00b7den", ",", "pl\u00f6tz\u00b7lich", "mich", "ver\u00b7stie\u00df", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VAINF", "$,", "ADJD", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Wie Alles kam? So Manches kommt im Leben \u2013", "tokens": ["Wie", "Al\u00b7les", "kam", "?", "So", "Man\u00b7ches", "kommt", "im", "Le\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "$.", "ADV", "PIS", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Begreift ihr das warum, das wie und was?", "tokens": ["Be\u00b7greift", "ihr", "das", "wa\u00b7rum", ",", "das", "wie", "und", "was", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "PWAV", "$,", "PRELS", "KOKOM", "KON", "PWS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erfahren mu\u00df man es, erleiden eben,", "tokens": ["Er\u00b7fah\u00b7ren", "mu\u00df", "man", "es", ",", "er\u00b7lei\u00b7den", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "PPER", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Damit man sagen k\u00f6nne: seht, auch das!", "tokens": ["Da\u00b7mit", "man", "sa\u00b7gen", "k\u00f6n\u00b7ne", ":", "seht", ",", "auch", "das", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVINF", "VMFIN", "$.", "VVFIN", "$,", "ADV", "PDS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}