{"textgrid.poem.63016": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vor Zeiten stand an einem heitern Bach", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor Zeiten stand an einem heitern Bach", "tokens": ["Vor", "Zei\u00b7ten", "stand", "an", "ei\u00b7nem", "hei\u00b7tern", "Bach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Cocosbaum, in dessen breiten Schatten", "tokens": ["Ein", "Co\u00b7cos\u00b7baum", ",", "in", "des\u00b7sen", "brei\u00b7ten", "Schat\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die V\u00f6gel oft ihr Kr\u00e4nzchen hatten:", "tokens": ["Die", "V\u00f6\u00b7gel", "oft", "ihr", "Kr\u00e4nz\u00b7chen", "hat\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie heckten unter seinem Dach,", "tokens": ["Sie", "heck\u00b7ten", "un\u00b7ter", "sei\u00b7nem", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie n\u00e4hrten sich vom Marke seiner N\u00fcsse,", "tokens": ["Sie", "n\u00e4hr\u00b7ten", "sich", "vom", "Mar\u00b7ke", "sei\u00b7ner", "N\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Adler selbst verlie\u00df Kronions Vorgemach", "tokens": ["Der", "Ad\u00b7ler", "selbst", "ver\u00b7lie\u00df", "Kro\u00b7nions", "Vor\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "NE", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Und buhlte hier um Zephyrs laue K\u00fcsse.", "tokens": ["Und", "buhl\u00b7te", "hier", "um", "Ze\u00b7phyrs", "lau\u00b7e", "K\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Einst brach ein Wetter aus. Der aufgeschwollne Flu\u00df", "tokens": ["Einst", "brach", "ein", "Wet\u00b7ter", "aus", ".", "Der", "auf\u00b7ge\u00b7schwoll\u00b7ne", "Flu\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zernagt des Baums entbl\u00f6\u00dfte F\u00fc\u00dfe;", "tokens": ["Zer\u00b7nagt", "des", "Baums", "ent\u00b7bl\u00f6\u00df\u00b7te", "F\u00fc\u00b7\u00dfe", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der Hauch des wilden Aeolus", "tokens": ["Der", "Hauch", "des", "wil\u00b7den", "A\u00b7e\u00b7o\u00b7lus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Streift seine Bl\u00e4tter ab, zermalmet seine Fr\u00fcchte.", "tokens": ["Streift", "sei\u00b7ne", "Bl\u00e4t\u00b7ter", "ab", ",", "zer\u00b7mal\u00b7met", "sei\u00b7ne", "Fr\u00fcch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die V\u00f6gel sahn mit traurigem Gesichte", "tokens": ["Die", "V\u00f6\u00b7gel", "sahn", "mit", "trau\u00b7ri\u00b7gem", "Ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Herab in der Verw\u00fcstung Graus.", "tokens": ["Her\u00b7ab", "in", "der", "Ver\u00b7w\u00fcs\u00b7tung", "Graus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "O Schade! rief der Geyer aus:", "tokens": ["O", "Scha\u00b7de", "!", "rief", "der", "Ge\u00b7yer", "aus", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$.", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Hier giebt es nun nichts mehr zu knacken,", "tokens": ["Hier", "giebt", "es", "nun", "nichts", "mehr", "zu", "kna\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIS", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Ich ziehe fort! Auch ich; versetzt der Specht:", "tokens": ["Ich", "zie\u00b7he", "fort", "!", "Auch", "ich", ";", "ver\u00b7setzt", "der", "Specht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "ADV", "PPER", "$.", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Ich richte nicht; allein der Baum hob seinen Nacken", "tokens": ["Ich", "rich\u00b7te", "nicht", ";", "al\u00b7lein", "der", "Baum", "hob", "sei\u00b7nen", "Na\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$.", "ADV", "ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Auch allzu stolz empor. Die G\u00f6tter sind gerecht!", "tokens": ["Auch", "all\u00b7zu", "stolz", "em\u00b7por", ".", "Die", "G\u00f6t\u00b7ter", "sind", "ge\u00b7recht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADJD", "PTKVZ", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Fiel ihm die Elster ein; das hab ich stets gefunden;", "tokens": ["Fiel", "ihm", "die", "Els\u00b7ter", "ein", ";", "das", "hab", "ich", "stets", "ge\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$.", "PDS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "O Freunde, seyd ihr klug, so warnt euch dieser Fall! \u2013", "tokens": ["O", "Freun\u00b7de", ",", "seyd", "ihr", "klug", ",", "so", "warnt", "euch", "die\u00b7ser", "Fall", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wer ist nicht gerne klug? Auch war in wenig Stunden", "tokens": ["Wer", "ist", "nicht", "ger\u00b7ne", "klug", "?", "Auch", "war", "in", "we\u00b7nig", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PTKNEG", "ADV", "ADJD", "$.", "ADV", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der V\u00f6gel ganzes Chor verschwunden.", "tokens": ["Der", "V\u00f6\u00b7gel", "gan\u00b7zes", "Chor", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Nur eine Taube blieb und eine Nachtigall.", "tokens": ["Nur", "ei\u00b7ne", "Tau\u00b7be", "blieb", "und", "ei\u00b7ne", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die Taube sprach: wir wollen hier verweilen", "tokens": ["Die", "Tau\u00b7be", "sprach", ":", "wir", "wol\u00b7len", "hier", "ver\u00b7wei\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Und mit dem Baum, der uns so manches Gute gab,", "tokens": ["Und", "mit", "dem", "Baum", ",", "der", "uns", "so", "man\u00b7ches", "Gu\u00b7te", "gab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Sein trauriges Verh\u00e4ngni\u00df theilen.", "tokens": ["Sein", "trau\u00b7ri\u00b7ges", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "thei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Ja, Freundin, du hast Recht! sein Grab sey unser Grab,", "tokens": ["Ja", ",", "Freun\u00b7din", ",", "du", "hast", "Recht", "!", "sein", "Grab", "sey", "un\u00b7ser", "Grab", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "PPER", "VAFIN", "NN", "$.", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Versetzt die holde Philomele:", "tokens": ["Ver\u00b7setzt", "die", "hol\u00b7de", "Phi\u00b7lo\u00b7me\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Vielleicht bewegt mein sanftes Klagelied", "tokens": ["Viel\u00b7leicht", "be\u00b7wegt", "mein", "sanf\u00b7tes", "Kla\u00b7ge\u00b7lied"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Noch einen Mann mit einer weichen Seele,", "tokens": ["Noch", "ei\u00b7nen", "Mann", "mit", "ei\u00b7ner", "wei\u00b7chen", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Da\u00df er des Baumes Fu\u00df mit einem Damm umzieht;", "tokens": ["Da\u00df", "er", "des", "Bau\u00b7mes", "Fu\u00df", "mit", "ei\u00b7nem", "Damm", "um\u00b7zieht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Dann lebt er wieder auf und eine neue Krone", "tokens": ["Dann", "lebt", "er", "wie\u00b7der", "auf", "und", "ei\u00b7ne", "neu\u00b7e", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Umlaubt sein welkes Haupt. \u00bbHa,\u00ab rief in leisem Tone", "tokens": ["Um\u00b7laubt", "sein", "wel\u00b7kes", "Haupt", ".", "\u00bb", "Ha", ",", "\u00ab", "rief", "in", "lei\u00b7sem", "To\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "VAINF", "PWAT", "NN", "$.", "$(", "ITJ", "$,", "$(", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die Dryas aus dem Stamm: \u00bbHeil dir, du frommes Paar!", "tokens": ["Die", "Dryas", "aus", "dem", "Stamm", ":", "\u00bb", "Heil", "dir", ",", "du", "from\u00b7mes", "Paar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$.", "$(", "NN", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Mein Herz vergi\u00dft den Hohn der Heuchler und der Feinde,", "tokens": ["Mein", "Herz", "ver\u00b7gi\u00dft", "den", "Hohn", "der", "Heuch\u00b7ler", "und", "der", "Fein\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und schl\u00e4gt nur noch f\u00fcr euch. Wenn Ungl\u00fcck, wenn Gefahr", "tokens": ["Und", "schl\u00e4gt", "nur", "noch", "f\u00fcr", "euch", ".", "Wenn", "Un\u00b7gl\u00fcck", ",", "wenn", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPR", "PPER", "$.", "KOUS", "NN", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Uns Freunde gibt, so sind es wahre Freunde.\u00ab", "tokens": ["Uns", "Freun\u00b7de", "gibt", ",", "so", "sind", "es", "wah\u00b7re", "Freun\u00b7de", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "NN", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}