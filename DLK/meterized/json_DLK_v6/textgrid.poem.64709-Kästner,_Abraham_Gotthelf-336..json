{"textgrid.poem.64709": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "336.", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der j\u00fcngst zu Ehren seiner Uhr,", "tokens": ["Der", "j\u00fcngst", "zu", "Eh\u00b7ren", "sei\u00b7ner", "Uhr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Karren stolz geschoben fuhr,", "tokens": ["Im", "Kar\u00b7ren", "stolz", "ge\u00b7scho\u00b7ben", "fuhr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verdiente nicht, verlacht zu werden:", "tokens": ["Ver\u00b7dien\u00b7te", "nicht", ",", "ver\u00b7lacht", "zu", "wer\u00b7den", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er kam geschwinder fort, als wir mit Kutsch' und Pferden.", "tokens": ["Er", "kam", "ge\u00b7schwin\u00b7der", "fort", ",", "als", "wir", "mit", "Kut\u00b7sch'", "und", "Pfer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$,", "KOUS", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}}}}