{"dta.poem.19342": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "75.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der K\u00f6nig auf der Pirsch' hat einen Hirsch erjagt;", "tokens": ["Der", "K\u00f6\u00b7nig", "auf", "der", "Pir\u00b7sch'", "hat", "ei\u00b7nen", "Hirsch", "er\u00b7jagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-++-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Mit Zittern steht der Hirsch, der um sein Leben zagt.", "tokens": ["Mit", "Zit\u00b7tern", "steht", "der", "Hirsch", ",", "der", "um", "sein", "Le\u00b7ben", "zagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der blickt den K\u00f6nig an, und beugt vor ihm die Glieder,", "tokens": ["Der", "blickt", "den", "K\u00f6\u00b7nig", "an", ",", "und", "beugt", "vor", "ihm", "die", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Selbst eine Thr\u00e4ne rann von seinem Auge nieder.", "tokens": ["Selbst", "ei\u00b7ne", "Thr\u00e4\u00b7ne", "rann", "von", "sei\u00b7nem", "Au\u00b7ge", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der K\u00f6nig will ger\u00fchrt dem Thier das Leben schenken,", "tokens": ["Der", "K\u00f6\u00b7nig", "will", "ge\u00b7r\u00fchrt", "dem", "Thier", "das", "Le\u00b7ben", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVPP", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und stiftet, wies geb\u00fchrt, davon ein Angedenken.", "tokens": ["Und", "stif\u00b7tet", ",", "wies", "ge\u00b7b\u00fchrt", ",", "da\u00b7von", "ein", "An\u00b7ge\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "VVPP", "$,", "PAV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Man legt ums Hirschgeweih ein Reiflein Gold, da war", "tokens": ["Man", "legt", "ums", "Hirschge\u00b7weih", "ein", "Rei\u00b7flein", "Gold", ",", "da", "war"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "ART", "NN", "NN", "$,", "ADV", "VAFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem K\u00f6nigsnamen bei geschrieben Tag und Jahr.", "tokens": ["Dem", "K\u00f6\u00b7nigs\u00b7na\u00b7men", "bei", "ge\u00b7schrie\u00b7ben", "Tag", "und", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der Hirsch enteilt mit Dank, und heim der K\u00f6nig kehrt;", "tokens": ["Der", "Hirsch", "ent\u00b7eilt", "mit", "Dank", ",", "und", "heim", "der", "K\u00f6\u00b7nig", "kehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,", "KON", "NE", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bald wird der K\u00f6nig krank, der Hirsch lebt unversehrt.", "tokens": ["Bald", "wird", "der", "K\u00f6\u00b7nig", "krank", ",", "der", "Hirsch", "lebt", "un\u00b7ver\u00b7sehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$,", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der K\u00f6nig stirbt, ihm folgt ein Sohn, und dem ein Sohn;", "tokens": ["Der", "K\u00f6\u00b7nig", "stirbt", ",", "ihm", "folgt", "ein", "Sohn", ",", "und", "dem", "ein", "Sohn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "KON", "ART", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der jagt im selben Wald, wo einst der Hirsch entflohn.", "tokens": ["Der", "jagt", "im", "sel\u00b7ben", "Wald", ",", "wo", "einst", "der", "Hirsch", "ent\u00b7flohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "NN", "$,", "PWAV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Da stellt der Hirsch sich dar, den Nacken altersteif,", "tokens": ["Da", "stellt", "der", "Hirsch", "sich", "dar", ",", "den", "Na\u00b7cken", "al\u00b7ter\u00b7steif", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch um die Stirne war noch hell der goldne Reif.", "tokens": ["Doch", "um", "die", "Stir\u00b7ne", "war", "noch", "hell", "der", "gold\u00b7ne", "Reif", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "ADV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Verwundert schauet ihn der junge K\u00f6nig an,", "tokens": ["Ver\u00b7wun\u00b7dert", "schau\u00b7et", "ihn", "der", "jun\u00b7ge", "K\u00f6\u00b7nig", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis dort ihm klar erschien der Ruhm von seinem Ahn.", "tokens": ["Bis", "dort", "ihm", "klar", "er\u00b7schien", "der", "Ruhm", "von", "sei\u00b7nem", "Ahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPER", "ADJD", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Und als man Jahr und Tag zusammenz\u00e4hlte, war", "tokens": ["Und", "als", "man", "Jahr", "und", "Tag", "zu\u00b7sam\u00b7men\u00b7z\u00e4hl\u00b7te", ",", "war"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "KOUS", "PIS", "NN", "KON", "NN", "VVFIN", "$,", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von damals der Betrag bis heute hundert Jahr.", "tokens": ["Von", "da\u00b7mals", "der", "Be\u00b7trag", "bis", "heu\u00b7te", "hun\u00b7dert", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "APPR", "ADV", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Die hundert Jahre froh hat in dem Wald gewohnt", "tokens": ["Die", "hun\u00b7dert", "Jah\u00b7re", "froh", "hat", "in", "dem", "Wald", "ge\u00b7wohnt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "CARD", "NN", "ADJD", "VAFIN", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Lebendes, weil so ein K\u00f6nig es geschont.", "tokens": ["Ein", "Le\u00b7ben\u00b7des", ",", "weil", "so", "ein", "K\u00f6\u00b7nig", "es", "ge\u00b7schont", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ADV", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Gro\u00df ist des K\u00f6nigs Gl\u00fcck, der, wenn man ihn begr\u00e4bt,", "tokens": ["Gro\u00df", "ist", "des", "K\u00f6\u00b7nigs", "Gl\u00fcck", ",", "der", ",", "wenn", "man", "ihn", "be\u00b7gr\u00e4bt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "NN", "$,", "PRELS", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Denkmal l\u00e4\u00dft zur\u00fcck, das hundert Jahre lebt.", "tokens": ["Ein", "Denk\u00b7mal", "l\u00e4\u00dft", "zu\u00b7r\u00fcck", ",", "das", "hun\u00b7dert", "Jah\u00b7re", "lebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PRELS", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}