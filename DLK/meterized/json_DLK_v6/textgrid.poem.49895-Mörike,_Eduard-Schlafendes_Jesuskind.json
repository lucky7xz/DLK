{"textgrid.poem.49895": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Schlafendes Jesuskind", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sohn der Jungfrau, Himmelskind! am Boden", "tokens": ["Sohn", "der", "Jung\u00b7frau", ",", "Him\u00b7mel\u00b7skind", "!", "am", "Bo\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "NN", "$.", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Auf dem Holz der Schmerzen eingeschlafen,", "tokens": ["Auf", "dem", "Holz", "der", "Schmer\u00b7zen", "ein\u00b7ge\u00b7schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Das der fromme Meister sinnvoll spielend", "tokens": ["Das", "der", "from\u00b7me", "Meis\u00b7ter", "sinn\u00b7voll", "spie\u00b7lend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "ADJA", "NN", "ADJD", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Deinen leichten Tr\u00e4umen unterlegte;", "tokens": ["Dei\u00b7nen", "leich\u00b7ten", "Tr\u00e4u\u00b7men", "un\u00b7ter\u00b7leg\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Blume du, noch in der Knospe d\u00e4mmernd", "tokens": ["Blu\u00b7me", "du", ",", "noch", "in", "der", "Knos\u00b7pe", "d\u00e4m\u00b7mernd"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ADV", "APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Eingeh\u00fcllt die Herrlichkeit des Vaters!", "tokens": ["Ein\u00b7ge\u00b7h\u00fcllt", "die", "Herr\u00b7lich\u00b7keit", "des", "Va\u00b7ters", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "O wer sehen k\u00f6nnte, welche Bilder", "tokens": ["O", "wer", "se\u00b7hen", "k\u00f6nn\u00b7te", ",", "wel\u00b7che", "Bil\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "PWS", "VVINF", "VMFIN", "$,", "PWAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Hinter dieser Stirne, diesen schwarzen", "tokens": ["Hin\u00b7ter", "die\u00b7ser", "Stir\u00b7ne", ",", "die\u00b7sen", "schwar\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PDAT", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Wimpern, sich in sanftem Wechsel malen!", "tokens": ["Wim\u00b7pern", ",", "sich", "in", "sanf\u00b7tem", "Wech\u00b7sel", "ma\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}