{"dta.poem.4255": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gem\u00e4hetes Getrayde,  \n  1739 .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott Lob! es ist die Erndte-Zeit,", "tokens": ["Gott", "Lob", "!", "es", "ist", "die", "Ernd\u00b7te\u00b7Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die wir so lang\u2019 gew\u00fcnschet haben!", "tokens": ["Die", "wir", "so", "lang'", "ge\u00b7w\u00fcn\u00b7schet", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die reife Saat ist abgemeyt,", "tokens": ["Die", "rei\u00b7fe", "Saat", "ist", "ab\u00b7ge\u00b7meyt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Betrachtet denn des Himmels Gaben!", "tokens": ["Be\u00b7trach\u00b7tet", "denn", "des", "Him\u00b7mels", "Ga\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Man sieht vom Gersten, Weizen, Rocken", "tokens": ["Man", "sieht", "vom", "Gers\u00b7ten", ",", "Wei\u00b7zen", ",", "Ro\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die hohen, gross- und schwehren Hocken,", "tokens": ["Die", "ho\u00b7hen", ",", "gross", "und", "schweh\u00b7ren", "Ho\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gleich kleinen Bergen, aufgeth\u00fcrmt.", "tokens": ["Gleich", "klei\u00b7nen", "Ber\u00b7gen", ",", "auf\u00b7ge\u00b7th\u00fcrmt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du hast, HErr! unsern Wunsch gew\u00e4hret,", "tokens": ["Du", "hast", ",", "Herr", "!", "un\u00b7sern", "Wunsch", "ge\u00b7w\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "NN", "$.", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du hast den Segen uns beschehret,", "tokens": ["Du", "hast", "den", "Se\u00b7gen", "uns", "be\u00b7scheh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du hast ihn auch bisher beschirmt.", "tokens": ["Du", "hast", "ihn", "auch", "bis\u00b7her", "be\u00b7schirmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Jtzt sehen wir ihn, mit Vergn\u00fcgen,", "tokens": ["Jtzt", "se\u00b7hen", "wir", "ihn", ",", "mit", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Scheuren fertig, vor uns liegen,", "tokens": ["Zur", "Scheu\u00b7ren", "fer\u00b7tig", ",", "vor", "uns", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$,", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Feld voll Segens-H\u00fcgel steh\u2019n.", "tokens": ["Das", "Feld", "voll", "Se\u00b7gens\u00b7H\u00fc\u00b7gel", "steh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "ADJD", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir sehen itzt, so weit wir seh\u2019n,", "tokens": ["Wir", "se\u00b7hen", "itzt", ",", "so", "weit", "wir", "seh'n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So weit sich unser Blick erstreckt,", "tokens": ["So", "weit", "sich", "un\u00b7ser", "Blick", "er\u00b7streckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie sich das Feld, in gelben H\u00f6h\u2019n,", "tokens": ["Wie", "sich", "das", "Feld", ",", "in", "gel\u00b7ben", "H\u00f6h'n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit lauter g\u00fcldnen Bergen deckt.", "tokens": ["Mit", "lau\u00b7ter", "g\u00fcld\u00b7nen", "Ber\u00b7gen", "deckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es mehrt sich noch ihr gelber Schein", "tokens": ["Es", "mehrt", "sich", "noch", "ihr", "gel\u00b7ber", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dadurch, da\u00df sie so reich beladen,", "tokens": ["Da\u00b7durch", ",", "da\u00df", "sie", "so", "reich", "be\u00b7la\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zumahl, da sie von ", "tokens": ["Zu\u00b7mahl", ",", "da", "sie", "von"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Von Bluhmen und von Unkraut rein.", "tokens": ["Von", "Bluh\u00b7men", "und", "von", "Un\u00b7kraut", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Man kann auf den gebundnen Spitzen", "tokens": ["Man", "kann", "auf", "den", "ge\u00b7bund\u00b7nen", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nichts als gef\u00fcllte Aehren sitzen,", "tokens": ["Nichts", "als", "ge\u00b7f\u00fcll\u00b7te", "A\u00b7eh\u00b7ren", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+----+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Vor Aehren keine Halmen, seh\u2019n.", "tokens": ["Vor", "A\u00b7eh\u00b7ren", "kei\u00b7ne", "Hal\u00b7men", ",", "seh'", "n."], "token_info": ["word", "word", "word", "word", "punct", "word", "abbreviation"], "pos": ["APPR", "NN", "PIAT", "NN", "$,", "VVFIN", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Auf dieser kleinen Berge Gipfeln", "tokens": ["Auf", "die\u00b7ser", "klei\u00b7nen", "Ber\u00b7ge", "Gip\u00b7feln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sieht man, in den gebognen Wipfeln,", "tokens": ["Sieht", "man", ",", "in", "den", "ge\u00b7bog\u00b7nen", "Wip\u00b7feln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht Bl\u00e4tter, lauter Fr\u00fcchte, steh\u2019n.", "tokens": ["Nicht", "Bl\u00e4t\u00b7ter", ",", "lau\u00b7ter", "Fr\u00fcch\u00b7te", ",", "steh'", "n."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "abbreviation"], "pos": ["PTKNEG", "NN", "$,", "PIAT", "NN", "$,", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Derselben Gipfel scheinen Kronen,", "tokens": ["Der\u00b7sel\u00b7ben", "Gip\u00b7fel", "schei\u00b7nen", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Landmanns M\u00fche zu belohnen,", "tokens": ["Des", "Land\u00b7manns", "M\u00fc\u00b7he", "zu", "be\u00b7loh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein jeder scheint ein Segens-Kranz;", "tokens": ["Ein", "je\u00b7der", "scheint", "ein", "Se\u00b7gens\u00b7Kranz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein jedes K\u00f6rnchen, das man siehet,", "tokens": ["Ein", "je\u00b7des", "K\u00f6rn\u00b7chen", ",", "das", "man", "sie\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da es im Strahl der Sonnen gl\u00fchet,", "tokens": ["Da", "es", "im", "Strahl", "der", "Son\u00b7nen", "gl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Empf\u00e4nget einen g\u00fcldnen Glanz.", "tokens": ["Emp\u00b7f\u00e4n\u00b7get", "ei\u00b7nen", "g\u00fcld\u00b7nen", "Glanz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Macht die\u00df gem\u00e4hete Getrayde,", "tokens": ["Macht", "die\u00df", "ge\u00b7m\u00e4\u00b7he\u00b7te", "Ge\u00b7tray\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "ADJA", "NN", "$,"], "meter": "+--+---+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Geliebter Mensch, dir keine Freude,", "tokens": ["Ge\u00b7lieb\u00b7ter", "Mensch", ",", "dir", "kei\u00b7ne", "Freu\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und siehst du es gleichg\u00fcltig an;", "tokens": ["Und", "siehst", "du", "es", "gleich\u00b7g\u00fcl\u00b7tig", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So wei\u00df ich nicht, was dich erfreuen,", "tokens": ["So", "wei\u00df", "ich", "nicht", ",", "was", "dich", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$,", "PWS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was Lob und Dank bey dir erneuen,", "tokens": ["Was", "Lob", "und", "Dank", "bey", "dir", "er\u00b7neu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und dich zum Sch\u00f6pfer f\u00fchren kann.", "tokens": ["Und", "dich", "zum", "Sch\u00f6p\u00b7fer", "f\u00fch\u00b7ren", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wenn wir, als Menschen, \u00fcberlegen,", "tokens": ["Wenn", "wir", ",", "als", "Men\u00b7schen", ",", "\u00fc\u00b7berl\u00b7e\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie viel von K\u00e4lte, Sturm und Regen", "tokens": ["Wie", "viel", "von", "K\u00e4l\u00b7te", ",", "Sturm", "und", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die schwache Frucht erduldet hat,", "tokens": ["Die", "schwa\u00b7che", "Frucht", "er\u00b7dul\u00b7det", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Seit so viel Tagen, wie viel Stunden,", "tokens": ["Seit", "so", "viel", "Ta\u00b7gen", ",", "wie", "viel", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "$,", "PWAV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So viel Minuten und Secunden,", "tokens": ["So", "viel", "Mi\u00b7nu\u00b7ten", "und", "Se\u00b7cun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bey Tag und Nacht, so fr\u00fch als spat;", "tokens": ["Bey", "Tag", "und", "Nacht", ",", "so", "fr\u00fch", "als", "spat", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ADV", "ADJD", "KOKOM", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So k\u00f6nnen wir es nicht begreifen,", "tokens": ["So", "k\u00f6n\u00b7nen", "wir", "es", "nicht", "be\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie sich das Korn erhalten kann,", "tokens": ["Wie", "sich", "das", "Korn", "er\u00b7hal\u00b7ten", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und sehens als ein Wunder an,", "tokens": ["Und", "se\u00b7hens", "als", "ein", "Wun\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "ART", "NN", "PTKVZ", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.10": {"text": "Wodurch vom Wachsen und vom Reifen,", "tokens": ["Wo\u00b7durch", "vom", "Wach\u00b7sen", "und", "vom", "Rei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die Wunder-Werke sich noch h\u00e4ufen.", "tokens": ["Die", "Wun\u00b7der\u00b7Wer\u00b7ke", "sich", "noch", "h\u00e4u\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADV", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.12": {"text": "Wir singen denn f\u00fcr alle G\u00fcte,", "tokens": ["Wir", "sin\u00b7gen", "denn", "f\u00fcr", "al\u00b7le", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Mit recht erkenntlichem Gem\u00fchte:", "tokens": ["Mit", "recht", "er\u00b7kennt\u00b7li\u00b7chem", "Ge\u00b7m\u00fch\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Herr! Sch\u00f6pfer und Erhalter! Dir", "tokens": ["Herr", "!", "Sch\u00f6p\u00b7fer", "und", "Er\u00b7hal\u00b7ter", "!", "Dir"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "$.", "NN", "KON", "NN", "$.", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Sey einzig Lob und Dank daf\u00fcr!", "tokens": ["Sey", "ein\u00b7zig", "Lob", "und", "Dank", "da\u00b7f\u00fcr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "NN", "KON", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}