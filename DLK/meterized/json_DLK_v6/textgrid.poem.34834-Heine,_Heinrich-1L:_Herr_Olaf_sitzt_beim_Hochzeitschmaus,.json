{"textgrid.poem.34834": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr Olaf sitzt beim Hochzeitschmaus,", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Olaf sitzt beim Hochzeitschmaus,", "tokens": ["Herr", "O\u00b7laf", "sitzt", "beim", "Hoch\u00b7zeit\u00b7schmaus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er trinkt den letzten Becher aus.", "tokens": ["Er", "trinkt", "den", "letz\u00b7ten", "Be\u00b7cher", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An seine Schulter lehnt", "tokens": ["An", "sei\u00b7ne", "Schul\u00b7ter", "lehnt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sein Weib und st\u00f6hnt \u2013", "tokens": ["Sein", "Weib", "und", "st\u00f6hnt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Der Henker steht vor der T\u00fcre.", "tokens": ["Der", "Hen\u00b7ker", "steht", "vor", "der", "T\u00fc\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Reigen beginnt, und Herr Olaf erfa\u00dft", "tokens": ["Der", "Rei\u00b7gen", "be\u00b7ginnt", ",", "und", "Herr", "O\u00b7laf", "er\u00b7fa\u00dft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "NN", "NE", "VVPP"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sein junges Weib, und mit wilder Hast", "tokens": ["Sein", "jun\u00b7ges", "Weib", ",", "und", "mit", "wil\u00b7der", "Hast"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie tanzen, bei Fackelglanz,", "tokens": ["Sie", "tan\u00b7zen", ",", "bei", "Fa\u00b7ckel\u00b7glanz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den letzten Tanz \u2013", "tokens": ["Den", "letz\u00b7ten", "Tanz", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Der Henker steht vor der T\u00fcre.", "tokens": ["Der", "Hen\u00b7ker", "steht", "vor", "der", "T\u00fc\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Geigen geben so lustigen Klang,", "tokens": ["Die", "Gei\u00b7gen", "ge\u00b7ben", "so", "lus\u00b7ti\u00b7gen", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Fl\u00f6ten seufzen so traurig und bang!", "tokens": ["Die", "Fl\u00f6\u00b7ten", "seuf\u00b7zen", "so", "trau\u00b7rig", "und", "bang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wer die beiden tanzen sieht,", "tokens": ["Wer", "die", "bei\u00b7den", "tan\u00b7zen", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem erbebt das Gem\u00fct \u2013", "tokens": ["Dem", "er\u00b7bebt", "das", "Ge\u00b7m\u00fct", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Der Henker steht vor der T\u00fcre.", "tokens": ["Der", "Hen\u00b7ker", "steht", "vor", "der", "T\u00fc\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und wie sie tanzen, im dr\u00f6hnenden Saal,", "tokens": ["Und", "wie", "sie", "tan\u00b7zen", ",", "im", "dr\u00f6h\u00b7nen\u00b7den", "Saal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVINF", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Herr Olaf fl\u00fcstert zu seinem Gemahl:", "tokens": ["Herr", "O\u00b7laf", "fl\u00fcs\u00b7tert", "zu", "sei\u00b7nem", "Ge\u00b7mahl", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "++-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "\u00bbdu wei\u00dft nicht, wie lieb ich dich hab \u2013", "tokens": ["\u00bb", "du", "wei\u00dft", "nicht", ",", "wie", "lieb", "ich", "dich", "hab", "\u2013"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "ADJD", "PPER", "PPER", "VAFIN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "So kalt ist das Grab \u2013\u00ab", "tokens": ["So", "kalt", "ist", "das", "Grab", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$(", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Der Henker steht vor der T\u00fcre.", "tokens": ["Der", "Hen\u00b7ker", "steht", "vor", "der", "T\u00fc\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}