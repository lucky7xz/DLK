{"textgrid.poem.25161": {"metadata": {"author": {"name": "Keats, John", "birth": "N.A.", "death": "N.A."}, "title": "1L: S\u00fcsses, denk nicht dran, la\u00df ruhn;", "genre": "verse", "period": "N.A.", "pub_year": 1808, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fcsses, denk nicht dran, la\u00df ruhn;", "tokens": ["S\u00fcs\u00b7ses", ",", "denk", "nicht", "dran", ",", "la\u00df", "ruhn", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKNEG", "PAV", "$,", "VVIMP", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weine nicht, sei still.", "tokens": ["Wei\u00b7ne", "nicht", ",", "sei", "still", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "VAFIN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Seufze nur, doch la\u00df es ruhn,", "tokens": ["Seuf\u00b7ze", "nur", ",", "doch", "la\u00df", "es", "ruhn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "KON", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df es gehn wie's will.", "tokens": ["La\u00df", "es", "gehn", "wie's", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "NE", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "S\u00fc\u00dfes Lieb, blick nicht so tr\u00fcb,", "tokens": ["S\u00fc\u00b7\u00dfes", "Lieb", ",", "blick", "nicht", "so", "tr\u00fcb", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht so tr\u00fcb und matt.", "tokens": ["Nicht", "so", "tr\u00fcb", "und", "matt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Einen Tropfen Tr\u00e4ne gib,", "tokens": ["Ei\u00b7nen", "Trop\u00b7fen", "Tr\u00e4\u00b7ne", "gib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVIMP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der den Tod schon hat.", "tokens": ["Der", "den", "Tod", "schon", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Noch so bleich? So wein dich aus!", "tokens": ["Noch", "so", "bleich", "?", "So", "wein", "dich", "aus", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$.", "ADV", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich sammle dann", "tokens": ["Und", "ich", "samm\u00b7le", "dann"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Alle Tr\u00e4nen, da\u00df daraus", "tokens": ["Al\u00b7le", "Tr\u00e4\u00b7nen", ",", "da\u00df", "da\u00b7raus"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "KOUS", "PAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Segen perlen kann.", "tokens": ["Se\u00b7gen", "per\u00b7len", "kann", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Klarer als ein B\u00e4chlein rinnt,", "tokens": ["Kla\u00b7rer", "als", "ein", "B\u00e4ch\u00b7lein", "rinnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir's vom Auge go\u00df,", "tokens": ["Dir's", "vom", "Au\u00b7ge", "go\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Linder noch als Wellchen sind,", "tokens": ["Lin\u00b7der", "noch", "als", "Well\u00b7chen", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "KOUS", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dein Gefl\u00fcster flo\u00df. \u2013", "tokens": ["Dein", "Ge\u00b7fl\u00fcs\u00b7ter", "flo\u00df", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Um ein Gl\u00fcck, das von ihm schied,", "tokens": ["Um", "ein", "Gl\u00fcck", ",", "das", "von", "ihm", "schied", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Klagt wohl jedermann \u2013", "tokens": ["Klagt", "wohl", "je\u00b7der\u00b7mann", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Gut nur, da\u00df solch Klagelied", "tokens": ["Gut", "nur", ",", "da\u00df", "solch", "Kla\u00b7ge\u00b7lied"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ADV", "$,", "KOUS", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Man auch k\u00fcssen kann.", "tokens": ["Man", "auch", "k\u00fcs\u00b7sen", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}