{"textgrid.poem.40648": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Doch wenn einmal verbraust des Sturmes Schwinge", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Doch wenn einmal verbraust des Sturmes Schwinge", "tokens": ["Doch", "wenn", "ein\u00b7mal", "ver\u00b7braust", "des", "Stur\u00b7mes", "Schwin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Ruh', so tiefe seltne Ruh' im Alle,", "tokens": ["Und", "Ruh'", ",", "so", "tie\u00b7fe", "selt\u00b7ne", "Ruh'", "im", "Al\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADV", "ADJA", "ADJA", "NN", "APPRART", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df st\u00f6rend dir der eigne Athem walle,", "tokens": ["Da\u00df", "st\u00f6\u00b7rend", "dir", "der", "eig\u00b7ne", "A\u00b7them", "wal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und da\u00df dir bang' vor jedem Schmetterlinge;", "tokens": ["Und", "da\u00df", "dir", "bang'", "vor", "je\u00b7dem", "Schmet\u00b7ter\u00b7lin\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn klar und rein und glatt im weiten Ringe", "tokens": ["Wenn", "klar", "und", "rein", "und", "glatt", "im", "wei\u00b7ten", "Rin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "KON", "ADJD", "KON", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Meer, wie ein Scheibe von Kristalle,", "tokens": ["Das", "Meer", ",", "wie", "ein", "Schei\u00b7be", "von", "Kris\u00b7tal\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+---+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df du am Grunde z\u00e4hlst die Steinlein alle,", "tokens": ["Da\u00df", "du", "am", "Grun\u00b7de", "z\u00e4hlst", "die", "Stein\u00b7lein", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "ART", "NN", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann steig ins Boot, seew\u00e4rts dein Ruder schwinge!", "tokens": ["Dann", "steig", "ins", "Boot", ",", "see\u00b7w\u00e4rts", "dein", "Ru\u00b7der", "schwin\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$,", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "Die Sage f\u00fchrt dich an die heil'ge Stelle", "tokens": ["Die", "Sa\u00b7ge", "f\u00fchrt", "dich", "an", "die", "heil'\u00b7ge", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Meer weit drau\u00dfen; dort zur Tiefe schaue!", "tokens": ["Im", "Meer", "weit", "drau\u00b7\u00dfen", ";", "dort", "zur", "Tie\u00b7fe", "schau\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ADV", "$.", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du siehst, o Wunder, Wald und gr\u00fcne Wiese,", "tokens": ["Du", "siehst", ",", "o", "Wun\u00b7der", ",", "Wald", "und", "gr\u00fc\u00b7ne", "Wie\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "$,", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Siehst fruchtbelad'ne B\u00e4ume, Bl\u00fcthenb\u00e4lle,", "tokens": ["Siehst", "frucht\u00b7be\u00b7la\u00b7d'\u00b7ne", "B\u00e4u\u00b7me", ",", "Bl\u00fc\u00b7then\u00b7b\u00e4l\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und Palmen f\u00e4chelnd \u00fcber goldner Aue,", "tokens": ["Und", "Pal\u00b7men", "f\u00e4\u00b7chelnd", "\u00fc\u00b7ber", "gold\u00b7ner", "Au\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein wonnig St\u00fcck versunkner Paradiese.", "tokens": ["Ein", "won\u00b7nig", "St\u00fcck", "ver\u00b7sun\u00b7kner", "Pa\u00b7ra\u00b7die\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}