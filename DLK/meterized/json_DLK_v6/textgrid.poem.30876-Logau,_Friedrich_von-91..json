{"textgrid.poem.30876": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "91.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man gibt den Weibern Schuld, da\u00df ihre Worte leichter", "tokens": ["Man", "gibt", "den", "Wei\u00b7bern", "Schuld", ",", "da\u00df", "ih\u00b7re", "Wor\u00b7te", "leich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "NN", "$,", "KOUS", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als leichte Bletter sind, da\u00df ihre Sinnen seichter", "tokens": ["Als", "leich\u00b7te", "Blet\u00b7ter", "sind", ",", "da\u00df", "ih\u00b7re", "Sin\u00b7nen", "seich\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "VAFIN", "$,", "KOUS", "PPOSAT", "NN", "PPOSAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als Regenb\u00e4che sind. O, M\u00e4nner k\u00fcnnens auch!", "tokens": ["Als", "Re\u00b7gen\u00b7b\u00e4\u00b7che", "sind", ".", "O", ",", "M\u00e4n\u00b7ner", "k\u00fcn\u00b7nens", "auch", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "$.", "NE", "$,", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Viel Worte, wenig Hertz ist ein ger\u00fchmter Brauch.", "tokens": ["Viel", "Wor\u00b7te", ",", "we\u00b7nig", "Hertz", "ist", "ein", "ge\u00b7r\u00fchm\u00b7ter", "Brauch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$.