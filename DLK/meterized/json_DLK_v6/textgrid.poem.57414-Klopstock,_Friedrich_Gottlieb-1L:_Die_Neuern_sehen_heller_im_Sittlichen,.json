{"textgrid.poem.57414": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Neuern sehen heller im Sittlichen,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Neuern sehen heller im Sittlichen,", "tokens": ["Die", "Neu\u00b7ern", "se\u00b7hen", "hel\u00b7ler", "im", "Sitt\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als einst die Alten sahn. Durch das reinere", "tokens": ["Als", "einst", "die", "Al\u00b7ten", "sahn", ".", "Durch", "das", "rei\u00b7ne\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$.", "APPR", "ART", "ADJA"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Licht, diese reife Kenntnis, hebt sich", "tokens": ["Licht", ",", "die\u00b7se", "rei\u00b7fe", "Kennt\u00b7nis", ",", "hebt", "sich"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PDAT", "ADJA", "NN", "$,", "VVFIN", "PRF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "H\u00f6her ihr Herz, wie das Herz der Alten.", "tokens": ["H\u00f6\u00b7her", "ihr", "Herz", ",", "wie", "das", "Herz", "der", "Al\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "PWAV", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Drum d\u00fcrfet ihr auch, wenns in den Schranken nun", "tokens": ["Drum", "d\u00fcr\u00b7fet", "ihr", "auch", ",", "wenns", "in", "den", "Schran\u00b7ken", "nun"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "APPR", "ART", "NN", "ADV"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der K\u00fcnste Sieg gilt, k\u00e4mpfen beseelt vom Mut,", "tokens": ["Der", "K\u00fcns\u00b7te", "Sieg", "gilt", ",", "k\u00e4mp\u00b7fen", "be\u00b7seelt", "vom", "Mut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "D\u00fcrft, wenn der Herold hoch den Lorbeer", "tokens": ["D\u00fcrft", ",", "wenn", "der", "He\u00b7rold", "hoch", "den", "Lor\u00b7beer"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "ART", "NN", "ADJD", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "H\u00e4lt, mit den Kalokagathen k\u00e4mpfen!", "tokens": ["H\u00e4lt", ",", "mit", "den", "Ka\u00b7lo\u00b7ka\u00b7ga\u00b7then", "k\u00e4mp\u00b7fen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+---+-+-+-", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Viel Zweig' und Sprosse haben die Tugenden;", "tokens": ["Viel", "Zweig'", "und", "Spros\u00b7se", "ha\u00b7ben", "die", "Tu\u00b7gen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zu jedem stimmen laut die Empfindungen:", "tokens": ["Zu", "je\u00b7dem", "stim\u00b7men", "laut", "die", "Emp\u00b7fin\u00b7dun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da gr\u00fcnet, bl\u00fcht nichts bis zum hohen", "tokens": ["Da", "gr\u00fc\u00b7net", ",", "bl\u00fcht", "nichts", "bis", "zum", "ho\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "PIS", "APPR", "APPRART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wipfel, das nicht in die Seele dringe.", "tokens": ["Wip\u00b7fel", ",", "das", "nicht", "in", "die", "See\u00b7le", "drin\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Viel Zweig' und Sprosse hat auch die b\u00f6se Tat;", "tokens": ["Viel", "Zweig'", "und", "Spros\u00b7se", "hat", "auch", "die", "b\u00f6\u00b7se", "Tat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vor jedem schauern auf die Empfindungen:", "tokens": ["Vor", "je\u00b7dem", "schau\u00b7ern", "auf", "die", "Emp\u00b7fin\u00b7dun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da welket, dorrt nichts bis zum hohen", "tokens": ["Da", "wel\u00b7ket", ",", "dorrt", "nichts", "bis", "zum", "ho\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "PIS", "APPR", "APPRART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wipfel, das nicht in die Seele dringe.", "tokens": ["Wip\u00b7fel", ",", "das", "nicht", "in", "die", "See\u00b7le", "drin\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Die mehr der Stufen zu dem Unendlichen", "tokens": ["Die", "mehr", "der", "Stu\u00b7fen", "zu", "dem", "Un\u00b7end\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aufstiegen, schauen h\u00f6here Sch\u00f6nheit. Er,", "tokens": ["Auf\u00b7stie\u00b7gen", ",", "schau\u00b7en", "h\u00f6\u00b7he\u00b7re", "Sch\u00f6n\u00b7heit", ".", "Er", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "ADJA", "NN", "$.", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das Sein, ward durch des Altertumes", "tokens": ["Das", "Sein", ",", "ward", "durch", "des", "Al\u00b7ter\u00b7tu\u00b7mes"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "M\u00e4rchen entstellt, die von G\u00f6ttern sangen.", "tokens": ["M\u00e4r\u00b7chen", "ent\u00b7stellt", ",", "die", "von", "G\u00f6t\u00b7tern", "san\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "PRELS", "APPR", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Hei\u00df ist, wie weit auch strahle der Kenntnis Licht,", "tokens": ["Hei\u00df", "ist", ",", "wie", "weit", "auch", "strah\u00b7le", "der", "Kennt\u00b7nis", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PWAV", "ADJD", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Kampf ums Kleinod! Wem bei der Fackel Glanz", "tokens": ["Der", "Kampf", "ums", "Klei\u00b7nod", "!", "Wem", "bei", "der", "Fa\u00b7ckel", "Glanz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "$.", "PWS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nicht laut das Herz schl\u00e4gt, froh nicht bebet,", "tokens": ["Nicht", "laut", "das", "Herz", "schl\u00e4gt", ",", "froh", "nicht", "be\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,", "ADJD", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Flieht, ist er weise, die Ebnen Delphi's.", "tokens": ["Flieht", ",", "ist", "er", "wei\u00b7se", ",", "die", "Eb\u00b7nen", "Del\u00b7phi'", "s."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["VVFIN", "$,", "VAFIN", "PPER", "VVFIN", "$,", "ART", "NN", "NE", "NE"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.7": {"line.1": {"text": "Der ersten Zauberin in des Dichters Hain,", "tokens": ["Der", "ers\u00b7ten", "Zau\u00b7be\u00b7rin", "in", "des", "Dich\u00b7ters", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Darstellung hei\u00dft sie, weihet der, opfert ihr", "tokens": ["Dar\u00b7stel\u00b7lung", "hei\u00dft", "sie", ",", "wei\u00b7het", "der", ",", "op\u00b7fert", "ihr"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "VVFIN", "ART", "$,", "VVFIN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Bl\u00fcten j\u00fcngste! Diese G\u00f6ttin,", "tokens": ["Der", "Bl\u00fc\u00b7ten", "j\u00fcngs\u00b7te", "!", "Die\u00b7se", "G\u00f6t\u00b7tin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "$.", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Streitende, mu\u00df euch mit Huld umschweben.", "tokens": ["Strei\u00b7ten\u00b7de", ",", "mu\u00df", "euch", "mit", "Huld", "um\u00b7schwe\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Wenn Geist mit Mut ihr einet, und wenn in euch", "tokens": ["Wenn", "Geist", "mit", "Mut", "ihr", "ei\u00b7net", ",", "und", "wenn", "in", "euch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "NN", "PPER", "VVFIN", "$,", "KON", "KOUS", "APPR", "PPER"], "meter": "-+-+----+-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Des Schweren Reiz nieschlummernde Funken n\u00e4hrt;", "tokens": ["Des", "Schwe\u00b7ren", "Reiz", "niesc\u00b7hlum\u00b7mern\u00b7de", "Fun\u00b7ken", "n\u00e4hrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dann werden selbst der Apollona", "tokens": ["Dann", "wer\u00b7den", "selbst", "der", "A\u00b7pol\u00b7lo\u00b7na"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NE"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Eifrigste Priester euch nicht verkennen.", "tokens": ["Eif\u00b7rigs\u00b7te", "Pries\u00b7ter", "euch", "nicht", "ver\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Denn ihnen winkt der amphiktyonische", "tokens": ["Denn", "ih\u00b7nen", "winkt", "der", "am\u00b7phik\u00b7ty\u00b7o\u00b7ni\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Kampfrichter; sie sind seiner Gesetze, sind", "tokens": ["Kam\u00b7pfrich\u00b7ter", ";", "sie", "sind", "sei\u00b7ner", "Ge\u00b7set\u00b7ze", ",", "sind"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "$,", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des eingedenk, da\u00df in der Tafeln", "tokens": ["Des", "ein\u00b7ge\u00b7denk", ",", "da\u00df", "in", "der", "Ta\u00b7feln"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erste gegraben war: Keuscher Ausspruch!", "tokens": ["Ers\u00b7te", "ge\u00b7gra\u00b7ben", "war", ":", "Keu\u00b7scher", "Aus\u00b7spruch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "VVPP", "VAFIN", "$.", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Der Enkel siehet einst von Elysium", "tokens": ["Der", "En\u00b7kel", "sie\u00b7het", "einst", "von", "E\u00b7ly\u00b7si\u00b7um"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NE"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ach\u00e4as Schemen kommen, und (in dem Hain", "tokens": ["A\u00b7ch\u00e4\u00b7as", "Sche\u00b7men", "kom\u00b7men", ",", "und", "(", "in", "dem", "Hain"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "VVINF", "$,", "KON", "$(", "APPR", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Umweht es sie melodisch) euren", "tokens": ["Um\u00b7weht", "es", "sie", "me\u00b7lo\u00b7disch", ")", "eu\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "$(", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sieg ihm verk\u00fcnden mit edlem L\u00e4cheln.", "tokens": ["Sieg", "ihm", "ver\u00b7k\u00fcn\u00b7den", "mit", "ed\u00b7lem", "L\u00e4\u00b7cheln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}