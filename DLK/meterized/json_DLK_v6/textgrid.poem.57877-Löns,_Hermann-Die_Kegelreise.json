{"textgrid.poem.57877": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Die Kegelreise", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Urlaubs- und Ferienzeit ist da,", "tokens": ["Die", "Ur\u00b7laubs", "und", "Fe\u00b7ri\u00b7en\u00b7zeit", "ist", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "VAFIN", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Kegelreisen bl\u00fchen,", "tokens": ["Die", "Ke\u00b7gel\u00b7rei\u00b7sen", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sehr reich mit Zigarren und Kognak versehn", "tokens": ["Sehr", "reich", "mit", "Zi\u00b7gar\u00b7ren", "und", "Ko\u00b7gnak", "ver\u00b7sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN", "KON", "NN", "VVINF"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die Klubs zum Bahnhofe ziehen.", "tokens": ["Die", "Klubs", "zum", "Bahn\u00b7ho\u00b7fe", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Zug rasselt fort; bald zeigen sich schon", "tokens": ["Der", "Zug", "ras\u00b7selt", "fort", ";", "bald", "zei\u00b7gen", "sich", "schon"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "ADV", "VVFIN", "PRF", "ADV"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In der Ferne die Berge verschwommen,", "tokens": ["In", "der", "Fer\u00b7ne", "die", "Ber\u00b7ge", "ver\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Naturempfindung bew\u00e4ltigt die Schar,", "tokens": ["Na\u00b7tu\u00b7remp\u00b7fin\u00b7dung", "be\u00b7w\u00e4l\u00b7tigt", "die", "Schar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schnell wird erst einer genommen.", "tokens": ["Schnell", "wird", "erst", "ei\u00b7ner", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "PIS", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Jetzt steigt man aus: wie wunderbar dort", "tokens": ["Jetzt", "steigt", "man", "aus", ":", "wie", "wun\u00b7der\u00b7bar", "dort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PTKVZ", "$.", "PWAV", "ADJD", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die gr\u00fcnen Berge winken,", "tokens": ["Die", "gr\u00fc\u00b7nen", "Ber\u00b7ge", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O Kinder, wie sch\u00f6n ist doch die Natur,", "tokens": ["O", "Kin\u00b7der", ",", "wie", "sch\u00f6n", "ist", "doch", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "ADJD", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Hier m\u00fcssen wir erst eins trinken!", "tokens": ["Hier", "m\u00fcs\u00b7sen", "wir", "erst", "eins", "trin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PIS", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Der Tag ist hei\u00df und der Weg ist steil,", "tokens": ["Der", "Tag", "ist", "hei\u00df", "und", "der", "Weg", "ist", "steil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo's Bier gibt, l\u00e4\u00dft man sich nieder,", "tokens": ["Wo's", "Bier", "gibt", ",", "l\u00e4\u00dft", "man", "sich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$,", "VVFIN", "PIS", "PRF", "PTKVZ", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und kegelt einer den Berg hinab,", "tokens": ["Und", "ke\u00b7gelt", "ei\u00b7ner", "den", "Berg", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Daf\u00fcr sind's Kegelbr\u00fcder.", "tokens": ["Da\u00b7f\u00fcr", "sin\u00b7d's", "Ke\u00b7gel\u00b7br\u00fc\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}