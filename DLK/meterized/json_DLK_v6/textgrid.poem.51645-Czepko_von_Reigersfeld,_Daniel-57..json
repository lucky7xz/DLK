{"textgrid.poem.51645": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "57.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist S\u00fcnde was? Wer hat ihr Wesen ausgemacht,", "tokens": ["Ist", "S\u00fcn\u00b7de", "was", "?", "Wer", "hat", "ihr", "We\u00b7sen", "aus\u00b7ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PWS", "$.", "PWS", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ist sie aber nichts? Was hat den Fall gebracht.", "tokens": ["Und", "ist", "sie", "a\u00b7ber", "nichts", "?", "Was", "hat", "den", "Fall", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PIS", "$.", "PWS", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}