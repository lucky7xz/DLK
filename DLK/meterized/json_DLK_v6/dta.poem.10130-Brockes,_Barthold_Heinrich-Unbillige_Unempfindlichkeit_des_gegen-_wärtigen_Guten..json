{"dta.poem.10130": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Unbillige Unempfindlichkeit des gegen-  \n w\u00e4rtigen Guten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Recht erbarmens-w\u00fcrdig ist, da\u00df wir mit so kaltem\nMuthe,", "tokens": ["Recht", "er\u00b7bar\u00b7mens\u00b7w\u00fcr\u00b7dig", "ist", ",", "da\u00df", "wir", "mit", "so", "kal\u00b7tem", "Mu\u00b7the", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Ja fast ohn\u2019 Empfindlichkeit, alles gegenw\u00e4rtge Gute,", "tokens": ["Ja", "fast", "ohn'", "Emp\u00b7find\u00b7lich\u00b7keit", ",", "al\u00b7les", "ge\u00b7gen\u00b7w\u00e4rt\u00b7ge", "Gu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "APPR", "NN", "$,", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und hingegen", "tokens": ["Und", "hin\u00b7ge\u00b7gen"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "-++-", "measure": "unknown.measure.di"}, "line.4": {"text": "Mit der innersten Bewegung, was verdrie\u00dflich ist, erwegen.", "tokens": ["Mit", "der", "in\u00b7ners\u00b7ten", "Be\u00b7we\u00b7gung", ",", "was", "ver\u00b7drie\u00df\u00b7lich", "ist", ",", "er\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Gesundheit nicht einmahl, nicht einmahl ein gut Ge-", "tokens": ["Die", "Ge\u00b7sund\u00b7heit", "nicht", "ein\u00b7mahl", ",", "nicht", "ein\u00b7mahl", "ein", "gut", "Ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKNEG", "ADV", "$,", "PTKNEG", "ADV", "ART", "ADJD", "TRUNC"], "meter": "--+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "K\u00f6nnen uns, wie sie doch sollten, eine lange Lust erregen.", "tokens": ["K\u00f6n\u00b7nen", "uns", ",", "wie", "sie", "doch", "soll\u00b7ten", ",", "ei\u00b7ne", "lan\u00b7ge", "Lust", "er\u00b7re\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PWAV", "PPER", "ADV", "VMFIN", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Aber, sind sie etwa fort; ach wie deutlich, hell und klar", "tokens": ["A\u00b7ber", ",", "sind", "sie", "et\u00b7wa", "fort", ";", "ach", "wie", "deut\u00b7lich", ",", "hell", "und", "klar"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "VAFIN", "PPER", "ADV", "PTKVZ", "$.", "ADV", "KOKOM", "ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "+---+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Wird man dann derselben G\u00fcte, Wehrt und Sch\u00e4tzbarkeit", "tokens": ["Wird", "man", "dann", "der\u00b7sel\u00b7ben", "G\u00fc\u00b7te", ",", "Wehrt", "und", "Sch\u00e4tz\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PIS", "ADV", "PDAT", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "Ach wie elend sind wir dann! wie so gro\u00df ist unsre Noth!", "tokens": ["Ach", "wie", "e\u00b7lend", "sind", "wir", "dann", "!", "wie", "so", "gro\u00df", "ist", "uns\u00b7re", "Noth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJD", "VAFIN", "PPER", "ADV", "$.", "PWAV", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Dann wird kein Juweel so rein und so sch\u00f6n, kein Gold so", "tokens": ["Dann", "wird", "kein", "Ju\u00b7weel", "so", "rein", "und", "so", "sch\u00f6n", ",", "kein", "Gold", "so"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "ADV", "ADJD", "KON", "ADV", "ADJD", "$,", "PIAT", "NN", "ADV"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Wenn es auch aus Ophir w\u00e4re, so ans Hertz gepicht gefunden;", "tokens": ["Wenn", "es", "auch", "aus", "Op\u00b7hir", "w\u00e4\u00b7re", ",", "so", "ans", "Hertz", "ge\u00b7picht", "ge\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NE", "VAFIN", "$,", "ADV", "APPRART", "NN", "VVPP", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Da\u00df man sie mit Lust nicht g\u00e4be, eine Sache zu besitzen,", "tokens": ["Da\u00df", "man", "sie", "mit", "Lust", "nicht", "g\u00e4\u00b7be", ",", "ei\u00b7ne", "Sa\u00b7che", "zu", "be\u00b7sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "APPR", "NN", "PTKNEG", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Die man erst besa\u00df, iedoch nicht gekostet, nicht empfunden,", "tokens": ["Die", "man", "erst", "be\u00b7sa\u00df", ",", "ie\u00b7doch", "nicht", "ge\u00b7kos\u00b7tet", ",", "nicht", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "VVFIN", "$,", "ADV", "PTKNEG", "VVPP", "$,", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "Die\u00df ist unser rechter Lohn, weil man sich, von Jugend an,", "tokens": ["Die\u00df", "ist", "un\u00b7ser", "rech\u00b7ter", "Lohn", ",", "weil", "man", "sich", ",", "von", "Ju\u00b7gend", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,", "KOUS", "PIS", "PRF", "$,", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Nicht dazu bequemen wollen, nicht dazu entschliessen kann,", "tokens": ["Nicht", "da\u00b7zu", "be\u00b7que\u00b7men", "wol\u00b7len", ",", "nicht", "da\u00b7zu", "ent\u00b7schlies\u00b7sen", "kann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PAV", "VVINF", "VMINF", "$,", "PTKNEG", "PAV", "VVINF", "VMFIN", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Die Gedancken, worin doch unser Wohl allein bestehet,", "tokens": ["Die", "Ge\u00b7dan\u00b7cken", ",", "wo\u00b7rin", "doch", "un\u00b7ser", "Wohl", "al\u00b7lein", "be\u00b7ste\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADV", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Mit den Sinnen zu verbinden, und nicht, wenn wir schme-", "tokens": ["Mit", "den", "Sin\u00b7nen", "zu", "ver\u00b7bin\u00b7den", ",", "und", "nicht", ",", "wenn", "wir", "schme"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$,", "KON", "PTKNEG", "$,", "KOUS", "PPER", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "Wenn wir riechen, f\u00fchlen, sehn,", "tokens": ["Wenn", "wir", "rie\u00b7chen", ",", "f\u00fch\u00b7len", ",", "sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "$,", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Uns bestreben, da\u00df es mag, zu des grossen Sch\u00f6pfers Ehren,", "tokens": ["Uns", "be\u00b7stre\u00b7ben", ",", "da\u00df", "es", "mag", ",", "zu", "des", "gros\u00b7sen", "Sch\u00f6p\u00b7fers", "Eh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "KOUS", "PPER", "VMFIN", "$,", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Mit Bedacht und Lust geschehn.", "tokens": ["Mit", "Be\u00b7dacht", "und", "Lust", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}