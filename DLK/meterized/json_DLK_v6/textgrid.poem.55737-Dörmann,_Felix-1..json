{"textgrid.poem.55737": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und bist Du wie Marmor so starr und so kalt", "tokens": ["Und", "bist", "Du", "wie", "Mar\u00b7mor", "so", "starr", "und", "so", "kalt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "KOKOM", "NN", "ADV", "ADJD", "KON", "ADV", "ADJD"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und kennst Du der Liebe D\u00e4monengewalt", "tokens": ["Und", "kennst", "Du", "der", "Lie\u00b7be", "D\u00e4\u00b7mo\u00b7nen\u00b7ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Bisher nur vom Sagen und Singen,", "tokens": ["Bis\u00b7her", "nur", "vom", "Sa\u00b7gen", "und", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "So wallet wie siedende Lava mein Blut", "tokens": ["So", "wal\u00b7let", "wie", "sie\u00b7den\u00b7de", "La\u00b7va", "mein", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KOKOM", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Und weckt auch in Dir die verhaltene Glut \u2013", "tokens": ["Und", "weckt", "auch", "in", "Dir", "die", "ver\u00b7hal\u00b7te\u00b7ne", "Glut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Ich will Deine Lieb' noch erzwingen!", "tokens": ["Ich", "will", "Dei\u00b7ne", "Lieb'", "noch", "er\u00b7zwin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Lagst Du mir nur einmal, nur einmal im Arm,", "tokens": ["Lagst", "Du", "mir", "nur", "ein\u00b7mal", ",", "nur", "ein\u00b7mal", "im", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "$,", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Und saugten die Lippen nur einmal sich warm,", "tokens": ["Und", "saug\u00b7ten", "die", "Lip\u00b7pen", "nur", "ein\u00b7mal", "sich", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ADV", "PRF", "ADJD", "$,"], "meter": "-+--+-++--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im tobenden Aufruhr der Sinne,", "tokens": ["Im", "to\u00b7ben\u00b7den", "Auf\u00b7ruhr", "der", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Dann bricht auch in Deine gef\u00fchllose Brust", "tokens": ["Dann", "bricht", "auch", "in", "Dei\u00b7ne", "ge\u00b7f\u00fchl\u00b7lo\u00b7se", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Bacchantisch, unz\u00e4hmbar, f\u00fcr immer die Lust,", "tokens": ["Bac\u00b7chan\u00b7tisch", ",", "un\u00b7z\u00e4hm\u00b7bar", ",", "f\u00fcr", "im\u00b7mer", "die", "Lust", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "APPR", "ADV", "ART", "NN", "$,"], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Dann schw\u00f6rest auch Du zu der Minne.", "tokens": ["Dann", "schw\u00f6\u00b7rest", "auch", "Du", "zu", "der", "Min\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}