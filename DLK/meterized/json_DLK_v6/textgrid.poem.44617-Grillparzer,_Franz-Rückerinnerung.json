{"textgrid.poem.44617": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "R\u00fcckerinnerung", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bin ein M\u00e4dchen gewesen", "tokens": ["Bin", "ein", "M\u00e4d\u00b7chen", "ge\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "VAPP"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Auf heimischer Flur,", "tokens": ["Auf", "hei\u00b7mi\u00b7scher", "Flur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mein Denken, mein F\u00fchlen,", "tokens": ["Mein", "Den\u00b7ken", ",", "mein", "F\u00fch\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Galt dir, o Natur!", "tokens": ["Galt", "dir", ",", "o", "Na\u00b7tur", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "FM", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Nun h\u00e4lt mich gefangen", "tokens": ["Nun", "h\u00e4lt", "mich", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ein liebender Blick,", "tokens": ["Ein", "lie\u00b7ben\u00b7der", "Blick", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Doch die Ruhe der Heimat", "tokens": ["Doch", "die", "Ru\u00b7he", "der", "Hei\u00b7mat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "War auch schon ein Gl\u00fcck!", "tokens": ["War", "auch", "schon", "ein", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}