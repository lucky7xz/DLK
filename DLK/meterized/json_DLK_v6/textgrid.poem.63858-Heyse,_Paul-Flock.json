{"textgrid.poem.63858": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Flock", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.85", "it:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "T\u00e4glich steig' ich in stiller Mittagssonne", "tokens": ["T\u00e4g\u00b7lich", "steig'", "ich", "in", "stil\u00b7ler", "Mit\u00b7tags\u00b7son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Nach Gardone di sopra und Morgnaga", "tokens": ["Nach", "Gar\u00b7do\u00b7ne", "di", "sop\u00b7ra", "und", "Mor\u00b7gna\u00b7ga"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NE", "NE", "KON", "NN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Oder h\u00f6her hinauf den weitgeschwungnen", "tokens": ["O\u00b7der", "h\u00f6\u00b7her", "hin\u00b7auf", "den", "weit\u00b7ge\u00b7schwung\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "ART", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Bergpfad \u00fcber die Schluchten bis Fasano.", "tokens": ["Berg\u00b7pfad", "\u00fc\u00b7ber", "die", "Schluch\u00b7ten", "bis", "Fa\u00b7sa\u00b7no", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "NE", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Mir zur Seite, doch \u00f6fter weit voraus mir,", "tokens": ["Mir", "zur", "Sei\u00b7te", ",", "doch", "\u00f6f\u00b7ter", "weit", "vo\u00b7raus", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "ADV", "ADV", "ADJD", "PTKVZ", "PPER", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Trabt mein Wandergeno\u00df, mein kleines H\u00fcndchen", "tokens": ["Trabt", "mein", "Wan\u00b7der\u00b7ge\u00b7no\u00df", ",", "mein", "klei\u00b7nes", "H\u00fcnd\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Flock, ein munterer Spitz. Zwar reiner Rasse", "tokens": ["Flock", ",", "ein", "mun\u00b7te\u00b7rer", "Spitz", ".", "Zwar", "rei\u00b7ner", "Ras\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$.", "ADV", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Kann er nicht sich ber\u00fchmen, doch so manchem", "tokens": ["Kann", "er", "nicht", "sich", "be\u00b7r\u00fch\u00b7men", ",", "doch", "so", "man\u00b7chem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "PRF", "VVINF", "$,", "ADV", "ADV", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Preishund l\u00e4uft er den Rang wohl ab an Sch\u00f6nheit", "tokens": ["Preis\u00b7hund", "l\u00e4uft", "er", "den", "Rang", "wohl", "ab", "an", "Sch\u00f6n\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "ADV", "PTKVZ", "APPR", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.10": {"text": "Und an Temperament. Wenn er so hinspringt,", "tokens": ["Und", "an", "Tem\u00b7pe\u00b7ra\u00b7ment", ".", "Wenn", "er", "so", "hin\u00b7springt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$.", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.11": {"text": "Hoch das N\u00e4schen, den Schweif wie eine Fahne", "tokens": ["Hoch", "das", "N\u00e4\u00b7schen", ",", "den", "Schweif", "wie", "ei\u00b7ne", "Fah\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "$,", "ART", "NN", "KOKOM", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Aufrecht tragend, gespitzt die braunen \u00d6hrchen,", "tokens": ["Auf\u00b7recht", "tra\u00b7gend", ",", "ge\u00b7spitzt", "die", "brau\u00b7nen", "\u00d6hr\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Gr\u00fc\u00dfen ihn die Gardoner Gassenkinder", "tokens": ["Gr\u00fc\u00b7\u00dfen", "ihn", "die", "Gar\u00b7do\u00b7ner", "Gas\u00b7sen\u00b7kin\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.14": {"text": "Schon von weitem, und Flocki! Flocki! rufend,", "tokens": ["Schon", "von", "wei\u00b7tem", ",", "und", "Flo\u00b7cki", "!", "Flo\u00b7cki", "!", "ru\u00b7fend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "$,", "KON", "NN", "$.", "NE", "$.", "VVPP", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "Suchen schmeichelnd sie ihn heranzulocken.", "tokens": ["Su\u00b7chen", "schmei\u00b7chelnd", "sie", "ihn", "her\u00b7an\u00b7zu\u00b7lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "PPER", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Doch er windet sich unger\u00fchrt und vornehm", "tokens": ["Doch", "er", "win\u00b7det", "sich", "un\u00b7ge\u00b7r\u00fchrt", "und", "vor\u00b7nehm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADJD", "KON", "ADJD"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.17": {"text": "An der jungen Verehrerbrut vor\u00fcber,", "tokens": ["An", "der", "jun\u00b7gen", "Ver\u00b7eh\u00b7rer\u00b7brut", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.18": {"text": "L\u00e4\u00dft zuweilen zu kleiner K\u00f6ter Spielen", "tokens": ["L\u00e4\u00dft", "zu\u00b7wei\u00b7len", "zu", "klei\u00b7ner", "K\u00f6\u00b7ter", "Spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.19": {"text": "Sich leutselig herab und rauft mit gro\u00dfen,", "tokens": ["Sich", "leut\u00b7se\u00b7lig", "her\u00b7ab", "und", "rauft", "mit", "gro\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADV", "KON", "VVFIN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Die ihm neidisch am Hals das rote Schleifchen", "tokens": ["Die", "ihm", "nei\u00b7disch", "am", "Hals", "das", "ro\u00b7te", "Schleif\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.21": {"text": "B\u00f6s zerzausen, worauf aus ehrenvollen", "tokens": ["B\u00f6s", "zer\u00b7zau\u00b7sen", ",", "wo\u00b7rauf", "aus", "eh\u00b7ren\u00b7vol\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVPP", "$,", "PWAV", "APPR", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.22": {"text": "Wunden blutend er heiter mir zur\u00fcckkehrt,", "tokens": ["Wun\u00b7den", "blu\u00b7tend", "er", "hei\u00b7ter", "mir", "zu\u00b7r\u00fcck\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PPER", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.23": {"text": "Ein geschlagener Held, doch ein Charakter.", "tokens": ["Ein", "ge\u00b7schla\u00b7ge\u00b7ner", "Held", ",", "doch", "ein", "Cha\u00b7rak\u00b7ter", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ART", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.24": {"text": "Manchmal, wenn ich im duft'gen Lorbeerschatten", "tokens": ["Manch\u00b7mal", ",", "wenn", "ich", "im", "duft'\u00b7gen", "Lor\u00b7beer\u00b7schat\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "M\u00fcd vom Klettern auf einem B\u00e4nklein raste \u2013", "tokens": ["M\u00fcd", "vom", "Klet\u00b7tern", "auf", "ei\u00b7nem", "B\u00e4n\u00b7klein", "ras\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.26": {"text": "Sch\u00f6n ist's droben; die sanften L\u00fcfte f\u00e4cheln", "tokens": ["Sch\u00f6n", "ist's", "dro\u00b7ben", ";", "die", "sanf\u00b7ten", "L\u00fcf\u00b7te", "f\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADV", "$.", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.27": {"text": "Rings das silberne Laub der \u00d6lbaumhalden,", "tokens": ["Rings", "das", "sil\u00b7ber\u00b7ne", "Laub", "der", "\u00d6l\u00b7baum\u00b7hal\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.28": {"text": "Und mit purpurnem Blau durch ihr Gezweige", "tokens": ["Und", "mit", "pur\u00b7pur\u00b7nem", "Blau", "durch", "ihr", "Ge\u00b7zwei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.29": {"text": "Schimmert drunten der See \u2013 mein Flock ist freilich,", "tokens": ["Schim\u00b7mert", "drun\u00b7ten", "der", "See", "\u2013", "mein", "Flock", "ist", "frei\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$(", "PPOSAT", "NN", "VAFIN", "ADV", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.30": {"text": "Wie die Hunde gew\u00f6hnlich, kein Naturfreund.", "tokens": ["Wie", "die", "Hun\u00b7de", "ge\u00b7w\u00f6hn\u00b7lich", ",", "kein", "Na\u00b7tur\u00b7freund", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "$,", "PIAT", "NN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.31": {"text": "Ruhig liegt er im Gras an meiner Seite,", "tokens": ["Ru\u00b7hig", "liegt", "er", "im", "Gras", "an", "mei\u00b7ner", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.32": {"text": "Manchmal schnappend nach einer kleinen Fliege,", "tokens": ["Manch\u00b7mal", "schnap\u00b7pend", "nach", "ei\u00b7ner", "klei\u00b7nen", "Flie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.33": {"text": "Manchmal still mich betrachtend, gleich als fragt' er:", "tokens": ["Manch\u00b7mal", "still", "mich", "be\u00b7trach\u00b7tend", ",", "gleich", "als", "fragt'", "er", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVPP", "$,", "ADV", "KOKOM", "VVFIN", "PPER", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.34": {"text": "Woran denkst du nur jetzt? 's ist unbegreiflich,", "tokens": ["Wo\u00b7ran", "denkst", "du", "nur", "jetzt", "?", "'s", "ist", "un\u00b7be\u00b7greif\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "$.", "PPER", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.35": {"text": "Da\u00df die Menschen best\u00e4ndig denken m\u00fcssen.", "tokens": ["Da\u00df", "die", "Men\u00b7schen", "be\u00b7st\u00e4n\u00b7dig", "den\u00b7ken", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVINF", "VMINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.36": {"text": "Wir sind kl\u00fcger. Schon Lessing sagt: wer m\u00f6chte", "tokens": ["Wir", "sind", "kl\u00fc\u00b7ger", ".", "Schon", "Les\u00b7sing", "sagt", ":", "wer", "m\u00f6ch\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$.", "ADV", "NE", "VVFIN", "$.", "PWS", "VMFIN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.37": {"text": "Denken, wenn er genie\u00dft! Und ich genie\u00dfe", "tokens": ["Den\u00b7ken", ",", "wenn", "er", "ge\u00b7nie\u00dft", "!", "Und", "ich", "ge\u00b7nie\u00b7\u00dfe"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "VVFIN", "$.", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Meine Ruhe, die wohlverdient, nachdem ich", "tokens": ["Mei\u00b7ne", "Ru\u00b7he", ",", "die", "wohl\u00b7ver\u00b7di\u00b7ent", ",", "nach\u00b7dem", "ich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADJD", "$,", "KOUS", "PPER"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.39": {"text": "Heut Lazerten gejagt und ganze sieben", "tokens": ["Heut", "La\u00b7zer\u00b7ten", "ge\u00b7jagt", "und", "gan\u00b7ze", "sie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVPP", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Tot zur Strecke gebracht. (Denn dies ist leider", "tokens": ["Tot", "zur", "Stre\u00b7cke", "ge\u00b7bracht", ".", "(", "Denn", "dies", "ist", "lei\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "VVPP", "$.", "$(", "KON", "PDS", "VAFIN", "ADV"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.41": {"text": "Eine noble Passion, von der bisher ihn", "tokens": ["Ei\u00b7ne", "nob\u00b7le", "Pas\u00b7si\u00b7on", ",", "von", "der", "bis\u00b7her", "ihn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ART", "ADV", "PPER"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.42": {"text": "Zu entw\u00f6hnen mir nicht gelang. Es fr\u00f6hnen", "tokens": ["Zu", "ent\u00b7w\u00f6h\u00b7nen", "mir", "nicht", "ge\u00b7lang", ".", "Es", "fr\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKZU", "VVINF", "PPER", "PTKNEG", "VVFIN", "$.", "PPER", "VVINF"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.43": {"text": "Hohe Herren ja auch des edlen Mordwerks,", "tokens": ["Ho\u00b7he", "Her\u00b7ren", "ja", "auch", "des", "ed\u00b7len", "Mord\u00b7werks", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.44": {"text": "Nicht zur Ehre der Menschheit.) Und zur Antwort", "tokens": ["Nicht", "zur", "Eh\u00b7re", "der", "Menschheit", ".", ")", "Und", "zur", "Ant\u00b7wort"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PTKNEG", "APPRART", "NN", "ART", "NN", "$.", "$(", "KON", "APPRART", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.45": {"text": "Auf die schweigende Frage sag' ich: Fl\u00f6ckchen,", "tokens": ["Auf", "die", "schwei\u00b7gen\u00b7de", "Fra\u00b7ge", "sag'", "ich", ":", "Fl\u00f6ck\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "$.", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.46": {"text": "Was ich eben gedacht, hat einem andern", "tokens": ["Was", "ich", "e\u00b7ben", "ge\u00b7dacht", ",", "hat", "ei\u00b7nem", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "VVPP", "$,", "VAFIN", "ART", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.47": {"text": "Deiner Br\u00fcder gegolten \u2013 Schnautz gehei\u00dfen,", "tokens": ["Dei\u00b7ner", "Br\u00fc\u00b7der", "ge\u00b7gol\u00b7ten", "\u2013", "Schnautz", "ge\u00b7hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$(", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.48": {"text": "Schn\u00e4utzlein nannten wir ihn \u2013 der auch vor Zeiten", "tokens": ["Schn\u00e4utz\u00b7lein", "nann\u00b7ten", "wir", "ihn", "\u2013", "der", "auch", "vor", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PPER", "$(", "PRELS", "ADV", "APPR", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.49": {"text": "Mich getreu zu begleiten pflag auf manchem", "tokens": ["Mich", "ge\u00b7treu", "zu", "be\u00b7glei\u00b7ten", "pflag", "auf", "man\u00b7chem"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "VVFIN", "APPR", "PIAT"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.50": {"text": "Bergweg, bis er zuletzt so fett geworden,", "tokens": ["Berg\u00b7weg", ",", "bis", "er", "zu\u00b7letzt", "so", "fett", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "ADV", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.51": {"text": "Da\u00df nur keuchend er aufw\u00e4rts kroch. Ich mu\u00dft' ihm,", "tokens": ["Da\u00df", "nur", "keu\u00b7chend", "er", "auf\u00b7w\u00e4rts", "kroch", ".", "Ich", "mu\u00dft'", "ihm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "PPER", "ADV", "VVFIN", "$.", "PPER", "VMFIN", "PPER", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.52": {"text": "Da ihm irdische Freuden nicht mehr bl\u00fchten,", "tokens": ["Da", "ihm", "ir\u00b7di\u00b7sche", "Freu\u00b7den", "nicht", "mehr", "bl\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.53": {"text": "Selbst verk\u00fcrzen die Qual. Doch wie er dalag,", "tokens": ["Selbst", "ver\u00b7k\u00fcr\u00b7zen", "die", "Qual", ".", "Doch", "wie", "er", "da\u00b7lag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "KON", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.54": {"text": "Um das struppige Haut \u2013 er war vom Stamm der", "tokens": ["Um", "das", "strup\u00b7pi\u00b7ge", "Haut", "\u2013", "er", "war", "vom", "Stamm", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "$(", "PPER", "VAFIN", "APPRART", "NN", "ART"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.55": {"text": "Rattenf\u00e4nger \u2013 ein dickes Tuch, getr\u00e4nkt mit", "tokens": ["Rat\u00b7ten\u00b7f\u00e4n\u00b7ger", "\u2013", "ein", "di\u00b7ckes", "Tuch", ",", "ge\u00b7tr\u00e4nkt", "mit"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$(", "ART", "ADJA", "NN", "$,", "VVPP", "APPR"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.56": {"text": "Chloroform: an des Herzens Schl\u00e4gen sah ich,", "tokens": ["Chlo\u00b7ro\u00b7form", ":", "an", "des", "Her\u00b7zens", "Schl\u00e4\u00b7gen", "sah", "ich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPR", "ART", "NN", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.57": {"text": "Da\u00df schon nahe das Ende. Da auf einmal", "tokens": ["Da\u00df", "schon", "na\u00b7he", "das", "En\u00b7de", ".", "Da", "auf", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "$.", "ADV", "APPR", "ADV"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.58": {"text": "Aus dem feuchten Verband hervor sich windend,", "tokens": ["Aus", "dem", "feuch\u00b7ten", "Ver\u00b7band", "her\u00b7vor", "sich", "win\u00b7dend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "PRF", "VVPP", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.59": {"text": "Hob der Sterbende auf zu mir sein treues,", "tokens": ["Hob", "der", "Ster\u00b7ben\u00b7de", "auf", "zu", "mir", "sein", "treu\u00b7es", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "APPR", "PPER", "PPOSAT", "ADJA", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.60": {"text": "Still anklagendes Aug', als wollt' er sagen:", "tokens": ["Still", "an\u00b7kla\u00b7gen\u00b7des", "Aug'", ",", "als", "wollt'", "er", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "KOUS", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.61": {"text": "Bessres hofft' ich um dich verdient zu haben,", "tokens": ["Bess\u00b7res", "hofft'", "ich", "um", "dich", "ver\u00b7dient", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "PPER", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.62": {"text": "Als so kl\u00e4glichen Tod von deinen H\u00e4nden! \u2013", "tokens": ["Als", "so", "kl\u00e4g\u00b7li\u00b7chen", "Tod", "von", "dei\u00b7nen", "H\u00e4n\u00b7den", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADV", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.63": {"text": "Ach, er wu\u00dfte ja nicht: zu seinem Besten", "tokens": ["Ach", ",", "er", "wu\u00df\u00b7te", "ja", "nicht", ":", "zu", "sei\u00b7nem", "Bes\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ADV", "PTKNEG", "$.", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.64": {"text": "Ihm durchschnitt ich den morschen Lebensfaden,", "tokens": ["Ihm", "durch\u00b7schnitt", "ich", "den", "mor\u00b7schen", "Le\u00b7bens\u00b7fa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.65": {"text": "Der nur qu\u00e4lend die zott'ge Brust umschn\u00fcrte.", "tokens": ["Der", "nur", "qu\u00e4\u00b7lend", "die", "zott'\u00b7ge", "Brust", "um\u00b7schn\u00fcr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.66": {"text": "Doch mir folgte noch lang in meine Tr\u00e4ume", "tokens": ["Doch", "mir", "folg\u00b7te", "noch", "lang", "in", "mei\u00b7ne", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.67": {"text": "Dieser scheidende Blick. Nicht w\u00fcnsch' ich, Fl\u00f6ckchen,", "tokens": ["Die\u00b7ser", "schei\u00b7den\u00b7de", "Blick", ".", "Nicht", "w\u00fcn\u00b7sch'", "ich", ",", "Fl\u00f6ck\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$.", "PTKNEG", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.68": {"text": "Je dein Auge mit \u00e4hnlich stummem Vorwurf", "tokens": ["Je", "dein", "Au\u00b7ge", "mit", "\u00e4hn\u00b7lich", "stum\u00b7mem", "Vor\u00b7wurf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.69": {"text": "Auf mir ruhen zu sehn. Du bist der J\u00fcngre,", "tokens": ["Auf", "mir", "ru\u00b7hen", "zu", "sehn", ".", "Du", "bist", "der", "J\u00fcng\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "PTKZU", "VVINF", "$.", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.70": {"text": "Sollst noch lange, wenn ich dahingeschieden,", "tokens": ["Sollst", "noch", "lan\u00b7ge", ",", "wenn", "ich", "da\u00b7hin\u00b7ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.71": {"text": "Deines Lebens dich freu'n, Lazerten jagend", "tokens": ["Dei\u00b7nes", "