{"dta.poem.10719": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Ein Gebet/ da\u00df Gott die Spanier wide-  \n rumb vom Rheinstrom wolle treiben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Sch lag doch/ du starcker Heldt/ die Schen\u00dflichen Maranen/", "tokens": ["Sch", "lag", "doch", "/", "du", "star\u00b7cker", "Heldt", "/", "die", "Schen\u00df\u00b7li\u00b7chen", "Ma\u00b7ra\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$(", "PPER", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "So leyder jhre Zelt vnd Blutgef\u00e4rbten Fahnen", "tokens": ["So", "ley\u00b7der", "jhre", "Zelt", "vnd", "Blut\u00b7ge\u00b7f\u00e4rb\u00b7ten", "Fah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "KON", "NN", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Auch jetzt in Teutschlandt bracht/ an vnsern sch\u00f6nen Rhein/", "tokens": ["Auch", "jetzt", "in", "Teutschlandt", "bracht", "/", "an", "vn\u00b7sern", "sch\u00f6\u00b7nen", "Rhein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NE", "VVFIN", "$(", "APPR", "PPOSAT", "ADJA", "NE", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der Waffen tragen mu\u00df/ vor seinen guten Wein/", "tokens": ["Der", "Waf\u00b7fen", "tra\u00b7gen", "mu\u00df", "/", "vor", "sei\u00b7nen", "gu\u00b7ten", "Wein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$(", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es ist genug gespielt mit eisernen Ballonen/", "tokens": ["Es", "ist", "ge\u00b7nug", "ge\u00b7spielt", "mit", "ei\u00b7ser\u00b7nen", "Bal\u00b7lo\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Du grosser Capitain/ h\u00f6r\u2019 auff/ fang\u2019 an zu schonen/", "tokens": ["Du", "gros\u00b7ser", "Ca\u00b7pi\u00b7tain", "/", "h\u00f6r'", "auff", "/", "fang'", "an", "zu", "scho\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "VVFIN", "APPR", "$(", "VVIMP", "PTKVZ", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es ist genug/ genug/ die G\u00f6tter sein verheert", "tokens": ["Es", "ist", "ge\u00b7nug", "/", "ge\u00b7nug", "/", "die", "G\u00f6t\u00b7ter", "sein", "ver\u00b7heert"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$(", "ADV", "$(", "ART", "NN", "PPOSAT", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Durch die/ so sie gemacht/ Statt/ Dorff/ vnd Feld verkehrt/", "tokens": ["Durch", "die", "/", "so", "sie", "ge\u00b7macht", "/", "Statt", "/", "Dorff", "/", "vnd", "Feld", "ver\u00b7kehrt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$(", "ADV", "PPER", "VVPP", "$(", "NN", "$(", "NE", "$(", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "La\u00df die/ durch deren grimm die Str\u00f6me kaum geflossen", "tokens": ["La\u00df", "die", "/", "durch", "de\u00b7ren", "grimm", "die", "Str\u00f6\u00b7me", "kaum", "ge\u00b7flos\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "$(", "APPR", "PDS", "VVFIN", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Von Leichen zugestopfft/ nit au\u00dfgehen vngenossen/", "tokens": ["Von", "Lei\u00b7chen", "zu\u00b7ge\u00b7stopfft", "/", "nit", "au\u00df\u00b7ge\u00b7hen", "vn\u00b7ge\u00b7nos\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$(", "PTKNEG", "VVINF", "VVINF", "$("], "meter": "-+---+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Vnd mache kundt/ da\u00df der/ der dir zugegen strebt/", "tokens": ["Vnd", "ma\u00b7che", "kundt", "/", "da\u00df", "der", "/", "der", "dir", "zu\u00b7ge\u00b7gen", "strebt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$(", "KOUS", "ART", "$(", "PRELS", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "St\u00fcrtzt/ oder bleibt er ja/ jhm selbst zur straffe lebt.", "tokens": ["St\u00fcrtzt", "/", "o\u00b7der", "bleibt", "er", "ja", "/", "jhm", "selbst", "zur", "straf\u00b7fe", "lebt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KON", "VVFIN", "PPER", "ADV", "$(", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}