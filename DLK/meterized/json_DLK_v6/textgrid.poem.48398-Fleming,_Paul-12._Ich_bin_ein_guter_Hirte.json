{"textgrid.poem.48398": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "12. Ich bin ein guter Hirte", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja freilich, freilich ja, du bist der gute Hirte,", "tokens": ["Ja", "frei\u00b7lich", ",", "frei\u00b7lich", "ja", ",", "du", "bist", "der", "gu\u00b7te", "Hir\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$,", "ADV", "ADV", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ich bin ein b\u00f6ses Schaf, das in der W\u00fcsten irrte,", "tokens": ["ich", "bin", "ein", "b\u00f6\u00b7ses", "Schaf", ",", "das", "in", "der", "W\u00fcs\u00b7ten", "irr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "von dir weit, weit von dir. Ich gieng der Weide nach,", "tokens": ["von", "dir", "weit", ",", "weit", "von", "dir", ".", "Ich", "gieng", "der", "Wei\u00b7de", "nach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "$,", "ADJD", "APPR", "PPER", "$.", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die mich zur Hellen stie\u00df und dir das Leben brach.", "tokens": ["die", "mich", "zur", "Hel\u00b7len", "stie\u00df", "und", "dir", "das", "Le\u00b7ben", "brach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "APPRART", "NN", "VVFIN", "KON", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein Leben war dein Tod, dein Hunger mein Verm\u00fcgen,", "tokens": ["Mein", "Le\u00b7ben", "war", "dein", "Tod", ",", "dein", "Hun\u00b7ger", "mein", "Ver\u00b7m\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "mein \u00dcberflu\u00df dein Durst. Ich w\u00e4re blieben liegen.", "tokens": ["mein", "\u00dc\u00b7berf\u00b7lu\u00df", "dein", "Durst", ".", "Ich", "w\u00e4\u00b7re", "blie\u00b7ben", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Mietling flohe weg, der wilde Wolf brach ein", "tokens": ["Der", "Miet\u00b7ling", "flo\u00b7he", "weg", ",", "der", "wil\u00b7de", "Wolf", "brach", "ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "ADJA", "NE", "VVFIN", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und lie\u00df mich schwaches Vieh kaum, kaum noch \u00fcbrig sein.", "tokens": ["und", "lie\u00df", "mich", "schwa\u00b7ches", "Vieh", "kaum", ",", "kaum", "noch", "\u00fcb\u00b7rig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "ADV", "$,", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du, Jesu, suchtest mich, du fundest mich, mich Armen,", "tokens": ["Du", ",", "Je\u00b7su", ",", "such\u00b7test", "mich", ",", "du", "fun\u00b7dest", "mich", ",", "mich", "Ar\u00b7men", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$,", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "und trugst mich wieder heim; es ist blo\u00df ein Erbarmen,", "tokens": ["und", "trugst", "mich", "wie\u00b7der", "heim", ";", "es", "ist", "blo\u00df", "ein", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}, "line.11": {"text": "da\u00df ich bin, der ich bin. Herr, weide ferner mich!", "tokens": ["da\u00df", "ich", "bin", ",", "der", "ich", "bin", ".", "Herr", ",", "wei\u00b7de", "fer\u00b7ner", "mich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "$,", "PRELS", "PPER", "VAFIN", "$.", "NN", "$,", "VVFIN", "ADV", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Herr, speise mich mit dir! ich d\u00fcrst', ich hunger, dich.", "tokens": ["Herr", ",", "spei\u00b7se", "mich", "mit", "dir", "!", "ich", "d\u00fcr\u00b7st'", ",", "ich", "hun\u00b7ger", ",", "dich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PRF", "APPR", "PPER", "$.", "PPER", "VMFIN", "$,", "PPER", "ADJD", "$,", "PPER", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Du bist das Himmelbrot; wer dich i\u00dft, der wird leben.", "tokens": ["Du", "bist", "das", "Him\u00b7mel\u00b7brot", ";", "wer", "dich", "i\u00dft", ",", "der", "wird", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "PWS", "PPER", "VVFIN", "$,", "PRELS", "VAFIN", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "O Brunnen Israel, du, du kanst Wasser geben,", "tokens": ["O", "Brun\u00b7nen", "Is\u00b7rael", ",", "du", ",", "du", "kanst", "Was\u00b7ser", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NE", "$,", "PPER", "$,", "PPER", "VMFIN", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "das aus dem Himmel quillt und wieder rinnt hinein.", "tokens": ["das", "aus", "dem", "Him\u00b7mel", "quillt", "und", "wie\u00b7der", "rinnt", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "KON", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wer dich i\u00dft, wer dich trinkt, wird stets ges\u00e4ttigt sein.", "tokens": ["Wer", "dich", "i\u00dft", ",", "wer", "dich", "trinkt", ",", "wird", "stets", "ge\u00b7s\u00e4t\u00b7tigt", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}}}}}