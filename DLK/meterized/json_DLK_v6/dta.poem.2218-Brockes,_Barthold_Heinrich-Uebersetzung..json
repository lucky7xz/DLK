{"dta.poem.2218": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Uebersetzung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O! sollt in meinen Sinn einst der Gedanke kommen,", "tokens": ["O", "!", "sollt", "in", "mei\u00b7nen", "Sinn", "einst", "der", "Ge\u00b7dan\u00b7ke", "kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VMFIN", "APPR", "PPOSAT", "NN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich, vom Stolz verf\u00fchrt, vom Hochmuth ein-", "tokens": ["Da\u00df", "ich", ",", "vom", "Stolz", "ver\u00b7f\u00fchrt", ",", "vom", "Hoch\u00b7muth", "ein"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPRART", "NN", "VVPP", "$,", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "genommen,", "tokens": ["ge\u00b7nom\u00b7men", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Je niedertr\u00e4chtig w\u00fcnscht\u2019: auf Erden", "tokens": ["Je", "nie\u00b7der\u00b7tr\u00e4ch\u00b7tig", "w\u00fcnscht'", ":", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "$.", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Erh\u00f6ht zu seyn und gro\u00df zu werden;", "tokens": ["Er\u00b7h\u00f6ht", "zu", "seyn", "und", "gro\u00df", "zu", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PTKZU", "VAINF", "KON", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "O Himmel, ach so fahre fort mir alle Seegen zu ent-", "tokens": ["O", "Him\u00b7mel", ",", "ach", "so", "fah\u00b7re", "fort", "mir", "al\u00b7le", "See\u00b7gen", "zu", "ent"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "ITJ", "ADV", "VVFIN", "PTKVZ", "PPER", "PIAT", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "ziehn,", "tokens": ["ziehn", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Die hier, in meinem stillen Leben, die holde Demuth mir", "tokens": ["Die", "hier", ",", "in", "mei\u00b7nem", "stil\u00b7len", "Le\u00b7ben", ",", "die", "hol\u00b7de", "De\u00b7muth", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.9": {"text": "verliehn!", "tokens": ["ver\u00b7liehn", "!"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}}}}}