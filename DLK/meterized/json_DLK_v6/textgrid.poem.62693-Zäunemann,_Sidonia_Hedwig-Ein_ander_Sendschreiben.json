{"textgrid.poem.62693": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Ein ander Sendschreiben", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df das ungebundne Schreiben, so ich j\u00fcngstenshin von dir", "tokens": ["Da\u00df", "das", "un\u00b7ge\u00b7bund\u00b7ne", "Schrei\u00b7ben", ",", "so", "ich", "j\u00fcng\u00b7sten\u00b7shin", "von", "dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "ADV", "PPER", "ADV", "APPR", "PPER"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Mit erfreuten H\u00e4nden brach, mir so angenehm gewesen,", "tokens": ["Mit", "er\u00b7freu\u00b7ten", "H\u00e4n\u00b7den", "brach", ",", "mir", "so", "an\u00b7ge\u00b7nehm", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "PPER", "ADV", "ADJD", "VAPP", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Als ob ich in Vers und Reim einen Brief von dir gelesen.", "tokens": ["Als", "ob", "ich", "in", "Vers", "und", "Reim", "ei\u00b7nen", "Brief", "von", "dir", "ge\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "NN", "KON", "NN", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Zeit, Gelegenheit und Stunden lassen uns nicht allemal", "tokens": ["Zeit", ",", "Ge\u00b7le\u00b7gen\u00b7heit", "und", "Stun\u00b7den", "las\u00b7sen", "uns", "nicht", "al\u00b7le\u00b7mal"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "PPER", "PTKNEG", "ADV"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Zu den Musen-H\u00fcgel eilen. Oft ist auch die Neunte Zahl", "tokens": ["Zu", "den", "Mu\u00b7sen\u00b7H\u00fc\u00b7gel", "ei\u00b7len", ".", "Oft", "ist", "auch", "die", "Neun\u00b7te", "Zahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "$.", "ADV", "VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Wider uns in Zorn entbrannt, und l\u00e4\u00dft uns kein Gl\u00fcck gelingen,", "tokens": ["Wi\u00b7der", "uns", "in", "Zorn", "ent\u00b7brannt", ",", "und", "l\u00e4\u00dft", "uns", "kein", "Gl\u00fcck", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "VVPP", "$,", "KON", "VVFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Ob wir auch gleich noch so sehr nach der Musen Freundschafft ringen.", "tokens": ["Ob", "wir", "auch", "gleich", "noch", "so", "sehr", "nach", "der", "Mu\u00b7sen", "Freund\u00b7schafft", "rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "ADV", "ADV", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Dieses hab ich gnug erfahren, dieses ist mir oft geschehn.", "tokens": ["Die\u00b7ses", "hab", "ich", "gnug", "er\u00b7fah\u00b7ren", ",", "die\u00b7ses", "ist", "mir", "oft", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVINF", "$,", "PDS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Jetzo mu\u00df ich gleiche Feindschaft von den Pierinnen sehn.", "tokens": ["Jet\u00b7zo", "mu\u00df", "ich", "glei\u00b7che", "Feind\u00b7schaft", "von", "den", "Pie\u00b7rin\u00b7nen", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Von Gedanken bin ich leer, ich weis nicht, was ich soll schreiben,", "tokens": ["Von", "Ge\u00b7dan\u00b7ken", "bin", "ich", "leer", ",", "ich", "weis", "nicht", ",", "was", "ich", "soll", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADJD", "$,", "PPER", "PTKVZ", "PTKNEG", "$,", "PWS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Doch ich will bey deinem Brief, ", "tokens": ["Doch", "ich", "will", "bey", "dei\u00b7nem", "Brief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Dieser wird mir Anla\u00df geben, da\u00df ich etwas reimen kan;", "tokens": ["Die\u00b7ser", "wird", "mir", "An\u00b7la\u00df", "ge\u00b7ben", ",", "da\u00df", "ich", "et\u00b7was", "rei\u00b7men", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "NN", "VVINF", "$,", "KOUS", "PPER", "PIS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Denn von Dichten schweig ich stille. Diese Frage geht dich an:", "tokens": ["Denn", "von", "Dich\u00b7ten", "schweig", "ich", "stil\u00b7le", ".", "Die\u00b7se", "Fra\u00b7ge", "geht", "dich", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "VVFIN", "$.", "PDAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Da\u00df es einen muntern Geist zum beliebten Folgen zwinget?", "tokens": ["Da\u00df", "es", "ei\u00b7nen", "mun\u00b7tern", "Geist", "zum", "be\u00b7lieb\u00b7ten", "Fol\u00b7gen", "zwin\u00b7get", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.15": {"text": "Nein! du hast hiebey geschmeichelt. Ein geschickter Musen-Sohn", "tokens": ["Nein", "!", "du", "hast", "hie\u00b7bey", "ge\u00b7schmei\u00b7chelt", ".", "Ein", "ge\u00b7schick\u00b7ter", "Mu\u00b7sen\u00b7Sohn"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$.", "PPER", "VAFIN", "ADV", "VVPP", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Wei\u00df sich selbsten aufzumuntern. Er besteigt den Helicon", "tokens": ["Wei\u00df", "sich", "selbs\u00b7ten", "auf\u00b7zu\u00b7mun\u00b7tern", ".", "Er", "be\u00b7steigt", "den", "He\u00b7li\u00b7con"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADV", "VVINF", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Durch den Trieb, den seine Brust bey der Musen Sch\u00f6nheit f\u00fchlet,", "tokens": ["Durch", "den", "Trieb", ",", "den", "sei\u00b7ne", "Brust", "bey", "der", "Mu\u00b7sen", "Sch\u00f6n\u00b7heit", "f\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Er ist selbst darauf bedacht, da\u00df er sch\u00f6n und lieblich spielet.", "tokens": ["Er", "ist", "selbst", "da\u00b7rauf", "be\u00b7dacht", ",", "da\u00df", "er", "sch\u00f6n", "und", "lieb\u00b7lich", "spie\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PAV", "VVPP", "$,", "KOUS", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Und wie st\u00fcnd mit meiner Feder doch die Gro\u00dfmuth in dem Bund?", "tokens": ["Und", "wie", "st\u00fcnd", "mit", "mei\u00b7ner", "Fe\u00b7der", "doch", "die", "Gro\u00df\u00b7muth", "in", "dem", "Bund", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "APPR", "PPOSAT", "NN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Diese schm\u00fcckt vornehmlich F\u00fcrsten und wird durch die Thaten kund.", "tokens": ["Die\u00b7se", "schm\u00fcckt", "vor\u00b7nehm\u00b7lich", "F\u00fcrs\u00b7ten", "und", "wird", "durch", "die", "Tha\u00b7ten", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "NN", "KON", "VAFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+--+--+-+", "measure": "trochaic.septa.relaxed"}, "line.21": {"text": "Bey den Helden pflegt sie nur oft mit Macht hervor zu blicken.", "tokens": ["Bey", "den", "Hel\u00b7den", "pflegt", "sie", "nur", "oft", "mit", "Macht", "her\u00b7vor", "zu", "bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "APPR", "NN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.22": {"text": "Aber wie kan alles die\u00df sich zu meinem Stande schicken?", "tokens": ["A\u00b7ber", "wie", "kan", "al\u00b7les", "die\u00df", "sich", "zu", "mei\u00b7nem", "Stan\u00b7de", "schi\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VMFIN", "PIS", "PDS", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.23": {"text": "Doch was sag ich, diese Tugend zeigt sich durch gar vielerley,", "tokens": ["Doch", "was", "sag", "ich", ",", "die\u00b7se", "Tu\u00b7gend", "zeigt", "sich", "durch", "gar", "vie\u00b7ler\u00b7ley", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "$,", "PDAT", "NN", "VVFIN", "PRF", "APPR", "ADV", "PIAT", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.24": {"text": "Und es weiset sich gar \u00f6fters, da\u00df sie auch bey Weibern sey.", "tokens": ["Und", "es", "wei\u00b7set", "sich", "gar", "\u00f6f\u00b7ters", ",", "da\u00df", "sie", "auch", "bey", "Wei\u00b7bern", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADV", "ADV", "$,", "KOUS", "PPER", "ADV", "APPR", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.25": {"text": "Aber, ob sie auch in mir ihre Wohnung aufgeschlagen,", "tokens": ["A\u00b7ber", ",", "ob", "sie", "auch", "in", "mir", "ih\u00b7re", "Woh\u00b7nung", "auf\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "APPR", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.26": {"text": "Die\u00df weist du so klar wohl nicht, hier mu\u00df ich mich selber fragen.", "tokens": ["Die\u00df", "weist", "du", "so", "klar", "wohl", "nicht", ",", "hier", "mu\u00df", "ich", "mich", "sel\u00b7ber", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADJD", "ADV", "PTKNEG", "$,", "ADV", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.27": {"text": "Ich mu\u00df deinen Worten glauben, so in deinem Schreiben stehn,", "tokens": ["Ich", "mu\u00df", "dei\u00b7nen", "Wor\u00b7ten", "glau\u00b7ben", ",", "so", "in", "dei\u00b7nem", "Schrei\u00b7ben", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$,", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.28": {"text": "Da\u00df du meine Gl\u00fcckwunsch Ode mit Vergn\u00fcgen angesehn,", "tokens": ["Da\u00df", "du", "mei\u00b7ne", "Gl\u00fcck\u00b7wunsch", "O\u00b7de", "mit", "Ver\u00b7gn\u00fc\u00b7gen", "an\u00b7ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NE", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.29": {"text": "Und auch angenommen hast. Nimm vorlieb mit meinen Gaben,", "tokens": ["Und", "auch", "an\u00b7ge\u00b7nom\u00b7men", "hast", ".", "Nimm", "vor\u00b7lieb", "mit", "mei\u00b7nen", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "$.", "NE", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.30": {"text": "Wenn mein Kiel einst netter schreibt, solst du auch was sch\u00f6nres haben.", "tokens": ["Wenn", "mein", "Kiel", "einst", "net\u00b7ter", "schreibt", ",", "solst", "du", "auch", "was", "sch\u00f6n\u00b7res", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$,", "VMFIN", "PPER", "ADV", "PWS", "ADJA", "VAINF", "$."], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Deine abgelegte Rede, wie der Ruf mir dargebracht,", "tokens": ["Dei\u00b7ne", "ab\u00b7ge\u00b7leg\u00b7te", "Re\u00b7de", ",", "wie", "der", "Ruf", "mir", "dar\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PWAV", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.32": {"text": "Hat ", "tokens": ["Hat"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "-", "measure": "single.down"}, "line.33": {"text": "Deine Rede hat mein Lob ohne meinen Reim bekommen,", "tokens": ["Dei\u00b7ne", "Re\u00b7de", "hat", "mein", "Lob", "oh\u00b7ne", "mei\u00b7nen", "Reim", "be\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.34": {"text": "Dieses wird ihr wahrlich nicht von den Neidern weggenommen.", "tokens": ["Die\u00b7ses", "wird", "ihr", "wahr\u00b7lich", "nicht", "von", "den", "Nei\u00b7dern", "weg\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "PTKNEG", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.35": {"text": "Weit mehr Ohren, als die Rede j\u00fcngst nach sich gezogen hat.", "tokens": ["Weit", "mehr", "Oh\u00b7ren", ",", "als", "die", "Re\u00b7de", "j\u00fcngst", "nach", "sich", "ge\u00b7zo\u00b7gen", "hat", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "NN", "$,", "KOUS", "ART", "NN", "ADV", "APPR", "PRF", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.36": {"text": "Deiner Rede Zierlichkeit, und die festgegr\u00fcndten S\u00e4tze,", "tokens": ["Dei\u00b7ner", "Re\u00b7de", "Zier\u00b7lich\u00b7keit", ",", "und", "die", "fest\u00b7ge\u00b7gr\u00fcnd\u00b7ten", "S\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.37": {"text": "Die du vorgetragen hast ohne leeres Wort-Geschw\u00e4tze;", "tokens": ["Die", "du", "vor\u00b7ge\u00b7tra\u00b7gen", "hast", "oh\u00b7ne", "lee\u00b7res", "Wor\u00b7tGe\u00b7schw\u00e4t\u00b7ze", ";"], "token_info": ["wo