{"textgrid.poem.58580": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Gott die Seinen f\u00fchrt im Wege der Gerichten,", "tokens": ["Wenn", "Gott", "die", "Sei\u00b7nen", "f\u00fchrt", "im", "We\u00b7ge", "der", "Ge\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "PPOSS", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da sich der Herr verbirgt und alles dunkel scheint,", "tokens": ["Da", "sich", "der", "Herr", "ver\u00b7birgt", "und", "al\u00b7les", "dun\u00b7kel", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "KON", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So gehet doch nach ihm ihr Denken und ihr Dichten,", "tokens": ["So", "ge\u00b7het", "doch", "nach", "ihm", "ihr", "Den\u00b7ken", "und", "ihr", "Dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Herzens Lust nur Gott und seinen Willen meint,", "tokens": ["Des", "Her\u00b7zens", "Lust", "nur", "Gott", "und", "sei\u00b7nen", "Wil\u00b7len", "meint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ihr stetes Herzens-Ach, ihr inniges Begehren", "tokens": ["Ihr", "ste\u00b7tes", "Her\u00b7zens\u00b7Ach", ",", "ihr", "in\u00b7ni\u00b7ges", "Be\u00b7geh\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}, "line.6": {"text": "Geht nur nach ihrem Schatz auch in der dunkeln Nacht,", "tokens": ["Geht", "nur", "nach", "ih\u00b7rem", "Schatz", "auch", "in", "der", "dun\u00b7keln", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie warten, ohne sich zur Kreatur zu kehren,", "tokens": ["Sie", "war\u00b7ten", ",", "oh\u00b7ne", "sich", "zur", "Kre\u00b7a\u00b7tur", "zu", "keh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUI", "PRF", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ihr Geist, so sp\u00e4t als fr\u00fch, zu Gott alleine wacht.", "tokens": ["Ihr", "Geist", ",", "so", "sp\u00e4t", "als", "fr\u00fch", ",", "zu", "Gott", "al\u00b7lei\u00b7ne", "wacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ADJD", "KOKOM", "ADJD", "$,", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}