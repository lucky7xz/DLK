{"dta.poem.202": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die gl\u00fcckliche Mittags-Ruhe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Es war sehr hefftig hei\u00df/ der Morgen war vergangen/", "tokens": ["Es", "war", "sehr", "heff\u00b7tig", "hei\u00df", "/", "der", "Mor\u00b7gen", "war", "ver\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ADJD", "$(", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich legte mich aufs Bett als ich gespeist zur Ruh/", "tokens": ["Ich", "leg\u00b7te", "mich", "aufs", "Bett", "als", "ich", "ge\u00b7speist", "zur", "Ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "KOUS", "PPER", "VVPP", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Fenster-Lade war halb offen und halb zu/", "tokens": ["Die", "Fens\u00b7ter\u00b7La\u00b7de", "war", "halb", "of\u00b7fen", "und", "halb", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADJD", "KON", "ADJD", "PTKZU", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Fast wie mit ihrem Licht die d\u00fcstern H\u00e4ynen prangen/", "tokens": ["Fast", "wie", "mit", "ih\u00b7rem", "Licht", "die", "d\u00fcs\u00b7tern", "H\u00e4y\u00b7nen", "pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So/ wie/ wenn ", "tokens": ["So", "/", "wie", "/", "wenn"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["ADV", "$(", "PWAV", "$(", "KOUS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Wie/ oder nach der Nacht des Tages Vortrab scheint/", "tokens": ["Wie", "/", "o\u00b7der", "nach", "der", "Nacht", "des", "Ta\u00b7ges", "Vor\u00b7trab", "scheint", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "KON", "APPR", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In welcher Dunckelheit kein M\u00e4dgen es verneint/", "tokens": ["In", "wel\u00b7cher", "Dun\u00b7ckel\u00b7heit", "kein", "M\u00e4d\u00b7gen", "es", "ver\u00b7neint", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PIAT", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil der Furchtsahmen Schaam darinn gesichert d\u00e4uchtet.", "tokens": ["Weil", "der", "Furcht\u00b7sah\u00b7men", "Schaam", "da\u00b7rinn", "ge\u00b7si\u00b7chert", "d\u00e4uch\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "PAV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Corinna kahm darauf mit einem Rock bedecket/", "tokens": ["Co\u00b7rin\u00b7na", "kahm", "da\u00b7rauf", "mit", "ei\u00b7nem", "Rock", "be\u00b7de\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PAV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Lo\u00df um den weissen Hals des Haar ohn Schleyer hing/", "tokens": ["Lo\u00df", "um", "den", "weis\u00b7sen", "Hals", "des", "Haar", "ohn", "Schle\u00b7yer", "hing", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So wie ", "tokens": ["So", "wie"], "token_info": ["word", "word"], "pos": ["ADV", "KOKOM"], "meter": "-+", "measure": "iambic.single"}, "line.12": {"text": "Und wie die ", "tokens": ["Und", "wie", "die"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWAV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Ich raubte ihr den Rock/ und dachte sie zu kriegen/", "tokens": ["Ich", "raub\u00b7te", "ihr", "den", "Rock", "/", "und", "dach\u00b7te", "sie", "zu", "krie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$(", "KON", "VVFIN", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sie aber griff auch zu/ und fa\u00dfte das Gewand", "tokens": ["Sie", "a\u00b7ber", "griff", "auch", "zu", "/", "und", "fa\u00df\u00b7te", "das", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "PTKZU", "$(", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es wieder anzuziehn/ doch war ihr Widerstand", "tokens": ["Es", "wie\u00b7der", "an\u00b7zu\u00b7ziehn", "/", "doch", "war", "ihr", "Wi\u00b7der\u00b7stand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVIZU", "$(", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Kein Ernst/ sie ward besiegt durch willigs Unterliegen.", "tokens": ["Kein", "Ernst", "/", "sie", "ward", "be\u00b7siegt", "durch", "wil\u00b7ligs", "Un\u00b7ter\u00b7lie\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$(", "PPER", "VAFIN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Da\u00df sie nun gantz entbl\u00f6\u00dft vor unsern Augen stehet/", "tokens": ["Da\u00df", "sie", "nun", "gantz", "ent\u00b7bl\u00f6\u00dft", "vor", "un\u00b7sern", "Au\u00b7gen", "ste\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Es war so gar kein Fehl an dem ", "tokens": ["Es", "war", "so", "gar", "kein", "Fehl", "an", "dem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PIAT", "NN", "APPR", "ART"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.19": {"text": "Als ich die Schulter sah so war der Armen auch/", "tokens": ["Als", "ich", "die", "Schul\u00b7ter", "sah", "so", "war", "der", "Ar\u00b7men", "auch", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "ADV", "VAFIN", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die Brust/ die war gerecht zur Tastung aufgebl\u00e4het.", "tokens": ["Die", "Brust", "/", "die", "war", "ge\u00b7recht", "zur", "Tas\u00b7tung", "auf\u00b7ge\u00b7bl\u00e4\u00b7het", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PDS", "VAFIN", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ich wei\u00df nicht alle Pracht/ und was sich kostbahr zeigte/", "tokens": ["Ich", "wei\u00df", "nicht", "al\u00b7le", "Pracht", "/", "und", "was", "sich", "kost\u00b7bahr", "zeig\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PIAT", "NN", "$(", "KON", "PWS", "PRF", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So wie die Seite war/ so war der H\u00fcffte-Zier", "tokens": ["So", "wie", "die", "Sei\u00b7te", "war", "/", "so", "war", "der", "H\u00fcff\u00b7te\u00b7Zier"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "VAFIN", "$(", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ich fand/ mit einem Wort/ nichts Tadelhaffts an ihr/", "tokens": ["Ich", "fand", "/", "mit", "ei\u00b7nem", "Wort", "/", "nichts", "Ta\u00b7del\u00b7haffts", "an", "ihr", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "APPR", "ART", "NN", "$(", "PIS", "NN", "APPR", "PPOSAT", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da\u00df ich auch meinen Leib an ihre Glieder beugte.", "tokens": ["Da\u00df", "ich", "auch", "mei\u00b7nen", "Leib", "an", "ih\u00b7re", "Glie\u00b7der", "beug\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wers Ubrige nicht wei\u00df/ der rahte in die Wette", "tokens": ["Wers", "Ub\u00b7ri\u00b7ge", "nicht", "wei\u00df", "/", "der", "rah\u00b7te", "in", "die", "Wet\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "PTKNEG", "VVFIN", "$(", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Bis ihn die Reitzung hin auf die Gedancken zieht/", "tokens": ["Bis", "ihn", "die", "Reit\u00b7zung", "hin", "auf", "die", "Ge\u00b7dan\u00b7cken", "zieht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Wir schlieffen endlich ein/ weil wir zu sehr erm\u00fcdt/", "tokens": ["Wir", "schlief\u00b7fen", "end\u00b7lich", "ein", "/", "weil", "wir", "zu", "sehr", "er\u00b7m\u00fcdt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "$(", "KOUS", "PPER", "PTKA", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Doch w\u00fcnsch ich/ da\u00df ich stets so guten Mittag h\u00e4tte.", "tokens": ["Doch", "w\u00fcnsch", "ich", "/", "da\u00df", "ich", "stets", "so", "gu\u00b7ten", "Mit\u00b7tag", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$(", "KOUS", "PPER", "ADV", "ADV", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}