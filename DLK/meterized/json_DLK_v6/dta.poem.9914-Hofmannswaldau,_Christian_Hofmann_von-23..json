{"dta.poem.9914": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der die gantze welt durchrennet/ kom\u0303t/ wo ich/ niemahls nicht", "tokens": ["Der", "die", "gant\u00b7ze", "welt", "durch\u00b7ren\u00b7net", "/", "kom\u0303t", "/", "wo", "ich", "/", "nie\u00b7mahls", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$(", "VVFIN", "$(", "PWAV", "PPER", "$(", "ADV", "PTKNEG"], "meter": "--+---+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und da/ wo er sich einfindet/ ich auch nicht zu sehen bin.", "tokens": ["Und", "da", "/", "wo", "er", "sich", "ein\u00b7fin\u00b7det", "/", "ich", "auch", "nicht", "zu", "se\u00b7hen", "bin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "PWAV", "PPER", "PRF", "VVPP", "$(", "PPER", "ADV", "PTKNEG", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Ich flieh ihn/ er fliehet mich/ und niemahls ist noch geschehen/", "tokens": ["Ich", "flieh", "ihn", "/", "er", "flie\u00b7het", "mich", "/", "und", "nie\u00b7mahls", "ist", "noch", "ge\u00b7sche\u00b7hen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "PPER", "VVFIN", "PPER", "$(", "KON", "ADV", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+--+-+-+-++-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Da\u00df man ihn und mich zugleich h\u00e4tt auf dieser welt gesehen.", "tokens": ["Da\u00df", "man", "ihn", "und", "mich", "zu\u00b7gleich", "h\u00e4tt", "auf", "die\u00b7ser", "welt", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "KON", "PPER", "ADV", "VAFIN", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}