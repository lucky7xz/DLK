{"textgrid.poem.67312": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der Neid", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Neide nicht, o junges M\u00e4dchen,", "tokens": ["Nei\u00b7de", "nicht", ",", "o", "jun\u00b7ges", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "FM", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner Schwester Lieblichkeit!", "tokens": ["Dei\u00b7ner", "Schwes\u00b7ter", "Lieb\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ahme nicht mit hei\u00dfem Eifer", "tokens": ["Ah\u00b7me", "nicht", "mit", "hei\u00b7\u00dfem", "Ei\u00b7fer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach, was die Natur verbeut!", "tokens": ["Nach", ",", "was", "die", "Na\u00b7tur", "ver\u00b7beut", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eine Blume, noch im Werden,", "tokens": ["Ei\u00b7ne", "Blu\u00b7me", ",", "noch", "im", "Wer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sah die Lilie vor sich stehn", "tokens": ["Sah", "die", "Li\u00b7lie", "vor", "sich", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "PRF", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und, vergessend ihrer selber", "tokens": ["Und", ",", "ver\u00b7ges\u00b7send", "ih\u00b7rer", "sel\u00b7ber"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "VVPP", "PPOSAT", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "(denn auch sie war hold und sch\u00f6n),", "tokens": ["(", "denn", "auch", "sie", "war", "hold", "und", "sch\u00f6n", ")", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "ADV", "PPER", "VAFIN", "ADJD", "KON", "ADJD", "$(", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Neidet, z\u00fcrnt sie, brennet \u00e4ngstig,", "tokens": ["Nei\u00b7det", ",", "z\u00fcrnt", "sie", ",", "bren\u00b7net", "\u00e4ngs\u00b7tig", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lilie zu werden. Weh!", "tokens": ["Li\u00b7lie", "zu", "wer\u00b7den", ".", "Weh", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PTKZU", "VAINF", "$.", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was geschieht? Die arme Blume", "tokens": ["Was", "ge\u00b7schieht", "?", "Die", "ar\u00b7me", "Blu\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird zu Feuerlilie.", "tokens": ["Wird", "zu", "Feu\u00b7er\u00b7li\u00b7lie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NE", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}