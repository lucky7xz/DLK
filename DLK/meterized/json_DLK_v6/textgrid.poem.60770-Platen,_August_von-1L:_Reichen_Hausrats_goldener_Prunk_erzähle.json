{"textgrid.poem.60770": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Reichen Hausrats goldener Prunk erz\u00e4hle", "genre": "verse", "period": "N.A.", "pub_year": 1828, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Reichen Hausrats goldener Prunk erz\u00e4hle", "tokens": ["Rei\u00b7chen", "Haus\u00b7rats", "gol\u00b7de\u00b7ner", "Prunk", "er\u00b7z\u00e4h\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN", "VVFIN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Jenes Manns Glorreichsten Moment der Nachwelt,", "tokens": ["Je\u00b7nes", "Manns", "Glor\u00b7reichs\u00b7ten", "Mo\u00b7ment", "der", "Nach\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Jenes Manns, der kaum in der Gruft, und doch schon", "tokens": ["Je\u00b7nes", "Manns", ",", "der", "kaum", "in", "der", "Gruft", ",", "und", "doch", "schon"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "$,", "KON", "ADV", "ADV"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Lange dahin scheint.", "tokens": ["Lan\u00b7ge", "da\u00b7hin", "scheint", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PAV", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Denk ich sein jetzt, dessen ich kaum gedachte,", "tokens": ["Denk", "ich", "sein", "jetzt", ",", "des\u00b7sen", "ich", "kaum", "ge\u00b7dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADV", "$,", "PRELAT", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Als ich j\u00fcngst, blo\u00df wenige Tage sind es,", "tokens": ["Als", "ich", "j\u00fcngst", ",", "blo\u00df", "we\u00b7ni\u00b7ge", "Ta\u00b7ge", "sind", "es", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "ADV", "PIAT", "NN", "VAFIN", "PPER", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Schaute, durch Herbstnebel hindurch, Marengos", "tokens": ["Schau\u00b7te", ",", "durch", "Herbst\u00b7ne\u00b7bel", "hin\u00b7durch", ",", "Ma\u00b7ren\u00b7gos"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "APPR", "NN", "PAV", "$,", "NE"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "D\u00fcsteres Blachfeld?", "tokens": ["D\u00fcs\u00b7te\u00b7res", "Blach\u00b7feld", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Ach, es stand damals in der Jahre sch\u00f6nstem", "tokens": ["Ach", ",", "es", "stand", "da\u00b7mals", "in", "der", "Jah\u00b7re", "sch\u00f6ns\u00b7tem"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mai der Held! Mi\u00dftrauischer Sorge fremd noch,", "tokens": ["Mai", "der", "Held", "!", "Mi\u00df\u00b7trau\u00b7i\u00b7scher", "Sor\u00b7ge", "fremd", "noch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "NN", "NN", "ADJD", "ADV", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Frug er noch, was r\u00fchmlicher sei, die Krone,", "tokens": ["Frug", "er", "noch", ",", "was", "r\u00fchm\u00b7li\u00b7cher", "sei", ",", "die", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PRELS", "ADJD", "VAFIN", "$,", "ART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Oder der Lorbeer?", "tokens": ["O\u00b7der", "der", "Lor\u00b7beer", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Beide flocht tollk\u00fchn er in eins! Emporschlug", "tokens": ["Bei\u00b7de", "flocht", "toll\u00b7k\u00fchn", "er", "in", "eins", "!", "Em\u00b7por\u00b7schlug"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PIS", "VVFIN", "ADJD", "PPER", "APPR", "PIS", "$.", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Seines Gl\u00fccks aufsteigender Dampf, wie Abels:", "tokens": ["Sei\u00b7nes", "Gl\u00fccks", "auf\u00b7stei\u00b7gen\u00b7der", "Dampf", ",", "wie", "A\u00b7bels", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,", "PWAV", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Siege, Herrschaft \u00fcber die Erde, h\u00f6chstes", "tokens": ["Sie\u00b7ge", ",", "Herr\u00b7schaft", "\u00fc\u00b7ber", "die", "Er\u00b7de", ",", "h\u00f6chs\u00b7tes"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "APPR", "ART", "NN", "$,", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Friedliches B\u00fcndnis!", "tokens": ["Fried\u00b7li\u00b7ches", "B\u00fcnd\u00b7nis", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Gro\u00dfe Nacht, doch schwanger an jedem Unheil,", "tokens": ["Gro\u00b7\u00dfe", "Nacht", ",", "doch", "schwan\u00b7ger", "an", "je\u00b7dem", "Un\u00b7heil", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Als des Ruhms Brautbette bestieg die blonde", "tokens": ["Als", "des", "Ruhms", "Braut\u00b7bet\u00b7te", "be\u00b7stieg", "die", "blon\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN", "ART", "ADJA"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Tochter Habsburgs; aber mit ihr des Schicksals", "tokens": ["Toch\u00b7ter", "Habs\u00b7burgs", ";", "a\u00b7ber", "mit", "ihr", "des", "Schick\u00b7sals"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "$.", "ADV", "APPR", "PPOSAT", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "M\u00e4chtiger Neuling!", "tokens": ["M\u00e4ch\u00b7ti\u00b7ger", "Neu\u00b7ling", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Horch! Die sonst mordspr\u00fchenden Feuerschl\u00fcnde", "tokens": ["Horch", "!", "Die", "sonst", "mord\u00b7spr\u00fc\u00b7hen\u00b7den", "Feu\u00b7er\u00b7schl\u00fcn\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "ART", "ADV", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "K\u00fcnden jetzt blo\u00df z\u00e4rtlichen Vaterjubel,", "tokens": ["K\u00fcn\u00b7den", "jetzt", "blo\u00df", "z\u00e4rt\u00b7li\u00b7chen", "Va\u00b7ter\u00b7ju\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "+---+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und das Volk weiht freudeberauscht die goldne", "tokens": ["Und", "das", "Volk", "weiht", "freu\u00b7de\u00b7be\u00b7rauscht", "die", "gold\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "VVFIN", "ART", "ADJA"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wiege der F\u00fcrstin.", "tokens": ["Wie\u00b7ge", "der", "F\u00fcrs\u00b7tin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Aber ach! Kein Wiegengesang der Liebe,", "tokens": ["A\u00b7ber", "ach", "!", "Kein", "Wie\u00b7gen\u00b7ge\u00b7sang", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "PIAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Waffenl\u00e4rm schlug hart an das Ohr des S\u00e4uglings:", "tokens": ["Waf\u00b7fen\u00b7l\u00e4rm", "schlug", "hart", "an", "das", "Ohr", "des", "S\u00e4ug\u00b7lings", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Eine Welt, schon lagert sie sich um seine", "tokens": ["Ei\u00b7ne", "Welt", ",", "schon", "la\u00b7gert", "sie", "sich", "um", "sei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Tragische Kindheit.", "tokens": ["Tra\u00b7gi\u00b7sche", "Kind\u00b7heit", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Todesbleich steht zwischen Gemahl und Vater,", "tokens": ["To\u00b7des\u00b7bleich", "steht", "zwi\u00b7schen", "Ge\u00b7mahl", "und", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-++-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Bietend stets, den keiner ergreift, den \u00d6lzweig,", "tokens": ["Bie\u00b7tend", "stets", ",", "den", "kei\u00b7ner", "er\u00b7greift", ",", "den", "\u00d6l\u00b7zweig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "PRELS", "PIS", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Noch im Flor zartbl\u00fchender Jugend, h\u00fclflos,", "tokens": ["Noch", "im", "Flor", "zart\u00b7bl\u00fc\u00b7hen\u00b7der", "Ju\u00b7gend", ",", "h\u00fcl\u00b7flos", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADJA", "NN", "$,", "ADJD", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Flehend und h\u00fclflos", "tokens": ["Fle\u00b7hend", "und", "h\u00fcl\u00b7flos"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "KON", "ADJD"], "meter": "+-++-", "measure": "unknown.measure.tri"}}, "stanza.9": {"line.1": {"text": "Sie, die Zier weitherrschenden Throns, von dem nun", "tokens": ["Sie", ",", "die", "Zier", "weit\u00b7herr\u00b7schen\u00b7den", "Throns", ",", "von", "dem", "nun"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "NN", "ADJA", "NN", "$,", "APPR", "PRELS", "ADV"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Steigt herab ihr zagender Fu\u00df bescheiden:", "tokens": ["Steigt", "her\u00b7ab", "ihr", "za\u00b7gen\u00b7der", "Fu\u00df", "be\u00b7schei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Wer verlor je stolzere G\u00fcter? Wer hat", "tokens": ["Wer", "ver\u00b7lor", "je", "stol\u00b7ze\u00b7re", "G\u00fc\u00b7ter", "?", "Wer", "hat"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ADJA", "NN", "$.", "PWS", "VAFIN"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mehr zu verlieren?", "tokens": ["Mehr", "zu", "ver\u00b7lie\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.10": {"line.1": {"text": "Weib des stets Siegreichen, so vieler C\u00e4sarn,", "tokens": ["Weib", "des", "stets", "Sieg\u00b7rei\u00b7chen", ",", "so", "vie\u00b7ler", "C\u00e4\u00b7sarn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Welche Karls Reichsapfel und Zepter trugen,", "tokens": ["Wel\u00b7che", "Karls", "Reichs\u00b7ap\u00b7fel", "und", "Zep\u00b7ter", "tru\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Enkelin, (weh, Alles umsonst!) so vieler", "tokens": ["En\u00b7ke\u00b7lin", ",", "(", "weh", ",", "Al\u00b7les", "um\u00b7sonst", "!", ")", "so", "vie\u00b7ler"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "punct", "punct", "word", "word"], "pos": ["NE", "$,", "$(", "PTKVZ", "$,", "PIS", "ADV", "$.", "$(", "ADV", "PIAT"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "K\u00f6nige Schw\u00e4grin!", "tokens": ["K\u00f6\u00b7ni\u00b7ge", "Schw\u00e4g\u00b7rin", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.11": {"line.1": {"text": "Mag verkl\u00e4rt nun oder umw\u00f6lkt die Sonne", "tokens": ["Mag", "ver\u00b7kl\u00e4rt", "nun", "o\u00b7der", "um\u00b7w\u00f6lkt", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "VVFIN", "ADV", "KON", "VVFIN", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Leuchten, mag was immer geschehn, es f\u00fcllt ja", "tokens": ["Leuch\u00b7ten", ",", "mag", "was", "im\u00b7mer", "ge\u00b7schehn", ",", "es", "f\u00fcllt", "ja"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VMFIN", "PIS", "ADV", "VVPP", "$,", "PPER", "VVFIN", "ADV"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Nie ein Herz mehr, dem so gering die Welt scheint,", "tokens": ["Nie", "ein", "Herz", "mehr", ",", "dem", "so", "ge\u00b7ring", "die", "Welt", "scheint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "$,", "PRELS", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Alles so tief liegt!", "tokens": ["Al\u00b7les", "so", "tief", "liegt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADJD", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}