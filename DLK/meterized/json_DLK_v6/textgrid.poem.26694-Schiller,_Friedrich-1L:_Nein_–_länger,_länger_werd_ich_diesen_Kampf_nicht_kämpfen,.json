{"textgrid.poem.26694": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nein \u2013 l\u00e4nger, l\u00e4nger werd ich diesen Kampf nicht k\u00e4mpfen,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nein \u2013 l\u00e4nger, l\u00e4nger werd ich diesen Kampf nicht k\u00e4mpfen,", "tokens": ["Nein", "\u2013", "l\u00e4n\u00b7ger", ",", "l\u00e4n\u00b7ger", "werd", "ich", "die\u00b7sen", "Kampf", "nicht", "k\u00e4mp\u00b7fen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "ADJD", "$,", "ADJD", "VAFIN", "PPER", "PDAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Riesenkampf der Pflicht.", "tokens": ["Den", "Rie\u00b7sen\u00b7kampf", "der", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kannst du des Herzens Flammentrieb nicht d\u00e4mpfen,", "tokens": ["Kannst", "du", "des", "Her\u00b7zens", "Flam\u00b7men\u00b7trieb", "nicht", "d\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So fodre, Tugend, dieses Opfer nicht.", "tokens": ["So", "fod\u00b7re", ",", "Tu\u00b7gend", ",", "die\u00b7ses", "Op\u00b7fer", "nicht", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "NN", "$,", "PDAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Geschworen hab ichs, ja, ich habs geschworen,", "tokens": ["Ge\u00b7schwo\u00b7ren", "hab", "ichs", ",", "ja", ",", "ich", "habs", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIS", "$,", "PTKANT", "$,", "PPER", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mich selbst zu b\u00e4ndigen.", "tokens": ["Mich", "selbst", "zu", "b\u00e4n\u00b7di\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hier ist dein Kranz. Er sei auf ewig mir verloren,", "tokens": ["Hier", "ist", "dein", "Kranz", ".", "Er", "sei", "auf", "e\u00b7wig", "mir", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "APPR", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nimm ihn zur\u00fcck, und la\u00df mich s\u00fcndigen.", "tokens": ["Nimm", "ihn", "zu\u00b7r\u00fcck", ",", "und", "la\u00df", "mich", "s\u00fcn\u00b7di\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKVZ", "$,", "KON", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sieh, G\u00f6ttin, mich zu deines Thrones Stufen,", "tokens": ["Sieh", ",", "G\u00f6t\u00b7tin", ",", "mich", "zu", "dei\u00b7nes", "Thro\u00b7nes", "Stu\u00b7fen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo ich noch j\u00fcngst, ein frecher Beter, lag,", "tokens": ["Wo", "ich", "noch", "j\u00fcngst", ",", "ein", "fre\u00b7cher", "Be\u00b7ter", ",", "lag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "$,", "ART", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein \u00fcbereilter Eid sei widerrufen,", "tokens": ["Mein", "\u00fc\u00b7be\u00b7reil\u00b7ter", "Eid", "sei", "wi\u00b7der\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vernichtet sei der schreckliche Vertrag,", "tokens": ["Ver\u00b7nich\u00b7tet", "sei", "der", "schreck\u00b7li\u00b7che", "Ver\u00b7trag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Den du im s\u00fc\u00dfen Taumel einer warmen Stunde", "tokens": ["Den", "du", "im", "s\u00fc\u00b7\u00dfen", "Tau\u00b7mel", "ei\u00b7ner", "war\u00b7men", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Tr\u00e4umenden erzwangst,", "tokens": ["Vom", "Tr\u00e4u\u00b7men\u00b7den", "er\u00b7zwangst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit meinem hei\u00dfen Blut in unerlaubtem Bunde,", "tokens": ["Mit", "mei\u00b7nem", "hei\u00b7\u00dfen", "Blut", "in", "un\u00b7er\u00b7laub\u00b7tem", "Bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Betr\u00fcgerisch aus meinem Busen rangst.", "tokens": ["Be\u00b7tr\u00fc\u00b7ge\u00b7risch", "aus", "mei\u00b7nem", "Bu\u00b7sen", "rangst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wo sind die Feuer, die elektrisch mich durchwallten,", "tokens": ["Wo", "sind", "die", "Feu\u00b7er", ",", "die", "e\u00b7lekt\u00b7risch", "mich", "durch\u00b7wall\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und wo der starke, k\u00fchne Talisman?", "tokens": ["Und", "wo", "der", "star\u00b7ke", ",", "k\u00fch\u00b7ne", "Ta\u00b7lis\u00b7man", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In jenem Wahnwitz will ich meinen Schwur dir halten,", "tokens": ["In", "je\u00b7nem", "Wahn\u00b7witz", "will", "ich", "mei\u00b7nen", "Schwur", "dir", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "PPER", "VVFIN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Worin ich unbesonnen ihn getan.", "tokens": ["Wo\u00b7rin", "ich", "un\u00b7be\u00b7son\u00b7nen", "ihn", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJA", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Zerrissen sei, was du und ich bedungen haben,", "tokens": ["Zer\u00b7ris\u00b7sen", "sei", ",", "was", "du", "und", "ich", "be\u00b7dun\u00b7gen", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "PWS", "PPER", "KON", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie liebt mich \u2013 deine Krone sei verscherzt.", "tokens": ["Sie", "liebt", "mich", "\u2013", "dei\u00b7ne", "Kro\u00b7ne", "sei", "ver\u00b7scherzt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gl\u00fcckselig, wer, in Wonnetrunkenheit begraben,", "tokens": ["Gl\u00fcck\u00b7se\u00b7lig", ",", "wer", ",", "in", "Won\u00b7ne\u00b7trun\u00b7ken\u00b7heit", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWS", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So leicht wie ich den tiefen Fall verschmerzt.", "tokens": ["So", "leicht", "wie", "ich", "den", "tie\u00b7fen", "Fall", "ver\u00b7schmerzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Sie sieht den Wurm an meiner Jugend Blume nagen", "tokens": ["Sie", "sieht", "den", "Wurm", "an", "mei\u00b7ner", "Ju\u00b7gend", "Blu\u00b7me", "na\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und meinen Lenz entflohn,", "tokens": ["Und", "mei\u00b7nen", "Lenz", "ent\u00b7flohn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bewundert still mein heldenm\u00fctiges Entsagen,", "tokens": ["Be\u00b7wun\u00b7dert", "still", "mein", "hel\u00b7den\u00b7m\u00fc\u00b7ti\u00b7ges", "Ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und gro\u00dfmutsvoll beschlie\u00dft sie meinen Lohn.", "tokens": ["Und", "gro\u00df\u00b7muts\u00b7voll", "be\u00b7schlie\u00dft", "sie", "mei\u00b7nen", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Mi\u00dftraue, sch\u00f6ne Seele, dieser Engelg\u00fcte!", "tokens": ["Mi\u00df\u00b7trau\u00b7e", ",", "sch\u00f6\u00b7ne", "See\u00b7le", ",", "die\u00b7ser", "En\u00b7gel\u00b7g\u00fc\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Mitleid waffnet zum Verbrecher mich,", "tokens": ["Dein", "Mit\u00b7leid", "waff\u00b7net", "zum", "Ver\u00b7bre\u00b7cher", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gibts in des Lebens unerme\u00dflichem Gebiete,", "tokens": ["Gibts", "in", "des", "Le\u00b7bens", "un\u00b7er\u00b7me\u00df\u00b7li\u00b7chem", "Ge\u00b7bie\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Gibts einen andern sch\u00f6nern Lohn \u2013 als dich?", "tokens": ["Gibts", "ei\u00b7nen", "an\u00b7dern", "sch\u00f6\u00b7nern", "Lohn", "\u2013", "als", "dich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "ADJA", "NN", "$(", "KOUS", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Als das Verbrechen, das ich ewig fliehen wollte?", "tokens": ["Als", "das", "Ver\u00b7bre\u00b7chen", ",", "das", "ich", "e\u00b7wig", "flie\u00b7hen", "woll\u00b7te", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entsetzliches Geschick!", "tokens": ["Ent\u00b7setz\u00b7li\u00b7ches", "Ge\u00b7schick", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der einzge Lohn, der meine Tugend kr\u00f6nen sollte,", "tokens": ["Der", "einz\u00b7ge", "Lohn", ",", "der", "mei\u00b7ne", "Tu\u00b7gend", "kr\u00f6\u00b7nen", "soll\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist meiner Tugend letzter Augenblick.", "tokens": ["Ist", "mei\u00b7ner", "Tu\u00b7gend", "letz\u00b7ter", "Au\u00b7gen\u00b7blick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Des wollustreichen Giftes voll \u2013 vergessen,", "tokens": ["Des", "wol\u00b7lust\u00b7rei\u00b7chen", "Gif\u00b7tes", "voll", "\u2013", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$(", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor wem ich zittern mu\u00df,", "tokens": ["Vor", "wem", "ich", "zit\u00b7tern", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wag ich es stumm, an meinen Busen sie zu pressen,", "tokens": ["Wag", "ich", "es", "stumm", ",", "an", "mei\u00b7nen", "Bu\u00b7sen", "sie", "zu", "pres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ADJD", "$,", "APPR", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf ihren Lippen brennt mein erster Ku\u00df.", "tokens": ["Auf", "ih\u00b7ren", "Lip\u00b7pen", "brennt", "mein", "ers\u00b7ter", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Wie schnell auf sein allm\u00e4chtig gl\u00fchendes Ber\u00fchren,", "tokens": ["Wie", "schnell", "auf", "sein", "all\u00b7m\u00e4ch\u00b7tig", "gl\u00fc\u00b7hen\u00b7des", "Be\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie schnell o Laura, flo\u00df", "tokens": ["Wie", "schnell", "o", "Lau\u00b7ra", ",", "flo\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ADJD", "FM", "FM", "$,", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das d\u00fcnne Siegel ab von \u00fcbereilten Schw\u00fcren,", "tokens": ["Das", "d\u00fcn\u00b7ne", "Sie\u00b7gel", "ab", "von", "\u00fc\u00b7be\u00b7reil\u00b7ten", "Schw\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sprang deiner Pflicht Tyrannenkette los,", "tokens": ["Sprang", "dei\u00b7ner", "Pflicht", "Ty\u00b7ran\u00b7nen\u00b7ket\u00b7te", "los", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Jetzt schlug sie laut, die hei\u00dferflehte Sch\u00e4ferstunde,", "tokens": ["Jetzt", "schlug", "sie", "laut", ",", "die", "hei\u00b7\u00dfer\u00b7fleh\u00b7te", "Sch\u00e4\u00b7fer\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jetzt d\u00e4mmerte mein Gl\u00fcck \u2013", "tokens": ["Jetzt", "d\u00e4m\u00b7mer\u00b7te", "mein", "Gl\u00fcck", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$("], "meter": 