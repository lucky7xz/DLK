{"dta.poem.19972": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Bey des Kronprinzen von W\u00fcrtemberg,  \n  jetzigen K\u00f6nigs, Zur\u00fcckkunft aus Frank-  \n reich, im Fr\u00fchling 1815 .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was sollen all' die s\u00fc\u00dfen Lieder,", "tokens": ["Was", "sol\u00b7len", "all'", "die", "s\u00fc\u00b7\u00dfen", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die rings die junge Erde singt?", "tokens": ["Die", "rings", "die", "jun\u00b7ge", "Er\u00b7de", "singt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es kam der reiche Fr\u00fchling wieder,", "tokens": ["Es", "kam", "der", "rei\u00b7che", "Fr\u00fch\u00b7ling", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist er's, dem sie den Jubel bringt?", "tokens": ["Ist", "er's", ",", "dem", "sie", "den", "Ju\u00b7bel", "bringt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Licht, T\u00f6ne, kommen hergeflogen,", "tokens": ["Licht", ",", "T\u00f6\u00b7ne", ",", "kom\u00b7men", "her\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVINF", "VVINF", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Rasch st\u00fcrzt der Strom vom Felsenhang,", "tokens": ["Rasch", "st\u00fcrzt", "der", "Strom", "vom", "Fel\u00b7sen\u00b7hang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er braust in alter Eichen Wogen,", "tokens": ["Er", "braust", "in", "al\u00b7ter", "Ei\u00b7chen", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie singen, Held, Dir Siegsgesang.", "tokens": ["Sie", "sin\u00b7gen", ",", "Held", ",", "Dir", "Siegs\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ringsum ert\u00f6nt's: wie Du die Bande", "tokens": ["Ring\u00b7sum", "er\u00b7t\u00f6nt's", ":", "wie", "Du", "die", "Ban\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$.", "PWAV", "PPER", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Gepre\u00dfter Menschheit mit zerschlugst,", "tokens": ["Ge\u00b7pre\u00df\u00b7ter", "Menschheit", "mit", "zer\u00b7schlugst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sieghaft, ein Sohn vom deutschen Lande,", "tokens": ["Sieg\u00b7haft", ",", "ein", "Sohn", "vom", "deut\u00b7schen", "Lan\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Des Reiches heil'ge Fahne trugst.", "tokens": ["Des", "Rei\u00b7ches", "heil'\u00b7ge", "Fah\u00b7ne", "trugst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch h\u00f6r' durch all' die Jubelt\u00f6ne", "tokens": ["Doch", "h\u00f6r'", "durch", "all'", "die", "Ju\u00b7bel\u00b7t\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PIS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Ruf vom s\u00fc\u00dfen Heimatland:", "tokens": ["Den", "Ruf", "vom", "s\u00fc\u00b7\u00dfen", "Hei\u00b7mat\u00b7land", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201ekomm! nimm, Du liebster meiner S\u00f6hne!", "tokens": ["\u201e", "komm", "!", "nimm", ",", "Du", "liebs\u00b7ter", "mei\u00b7ner", "S\u00f6h\u00b7ne", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "VVIMP", "$,", "PPER", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eden Kranz aus zarter Frauenhand!\u201c", "tokens": ["\u201e", "den", "Kranz", "aus", "zar\u00b7ter", "Frau\u00b7en\u00b7hand", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u201ekomm! sieh viel starker M\u00e4nner Arme,", "tokens": ["\u201e", "komm", "!", "sieh", "viel", "star\u00b7ker", "M\u00e4n\u00b7ner", "Ar\u00b7me", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "VVIMP", "ADV", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u201edie all' nach Dir sich breiten aus!", "tokens": ["\u201e", "die", "all'", "nach", "Dir", "sich", "brei\u00b7ten", "aus", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PIS", "APPR", "PPER", "PRF", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201ekomm! da\u00df nach lang verbiss'nem Harme", "tokens": ["\u201e", "komm", "!", "da\u00df", "nach", "lang", "ver\u00b7biss'\u00b7nem", "Har\u00b7me"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "$.", "KOUS", "APPR", "ADJD", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "\u201egesang erschall' aus H\u00fctt' und Haus!\u201c", "tokens": ["\u201e", "ge\u00b7sang", "er\u00b7schall'", "aus", "H\u00fctt'", "und", "Haus", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "PIS", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ja! sieh bekr\u00e4nzt von Bl\u00fcthenzweigen", "tokens": ["Ja", "!", "sieh", "be\u00b7kr\u00e4nzt", "von", "Bl\u00fc\u00b7then\u00b7zwei\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Land in jugendlicher Pracht;", "tokens": ["Dein", "Land", "in", "ju\u00b7gend\u00b7li\u00b7cher", "Pracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die W\u00e4lder sich melodisch neigen,", "tokens": ["Die", "W\u00e4l\u00b7der", "sich", "me\u00b7lo\u00b7disch", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie rufen Dich in ihre Nacht.", "tokens": ["Sie", "ru\u00b7fen", "Dich", "in", "ih\u00b7re", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die stolze Alp in Himmels-Bl\u00e4ue,", "tokens": ["Die", "stol\u00b7ze", "Alp", "in", "Him\u00b7mels\u00b7Bl\u00e4ue", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drauf manch' gekr\u00f6ntes Heldenhaus,", "tokens": ["Drauf", "man\u00b7ch'", "ge\u00b7kr\u00f6n\u00b7tes", "Hel\u00b7den\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Schaut nach dem deutschen Sohn voll Treue", "tokens": ["Schaut", "nach", "dem", "deut\u00b7schen", "Sohn", "voll", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sehns\u00fcchtig in das Land hinaus.", "tokens": ["Sehn\u00b7s\u00fcch\u00b7tig", "in", "das", "Land", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wild rauscht des Neckars blaue Welle,", "tokens": ["Wild", "rauscht", "des", "Ne\u00b7ckars", "blau\u00b7e", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Rennt eilend, wie sie nie gethan,", "tokens": ["Rennt", "ei\u00b7lend", ",", "wie", "sie", "nie", "ge\u00b7than", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum alten Rheine treibt sie's schnelle,", "tokens": ["Zum", "al\u00b7ten", "Rhei\u00b7ne", "treibt", "sie's", "schnel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NE", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Sieggekr\u00f6nten zu empfah'n.", "tokens": ["Den", "Sieg\u00b7ge\u00b7kr\u00f6n\u00b7ten", "zu", "emp\u00b7fah'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "PTKZU", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "O d\u00fcrften wir mitwogen fr\u00f6hlich,", "tokens": ["O", "d\u00fcrf\u00b7ten", "wir", "mit\u00b7wo\u00b7gen", "fr\u00f6h\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Strom nach dem entbund'nen Rhein,", "tokens": ["Ein", "Strom", "nach", "dem", "ent\u00b7bun\u00b7d'\u00b7nen", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und tragen Dich auf Armen selig", "tokens": ["Und", "tra\u00b7gen", "Dich", "auf", "Ar\u00b7men", "se\u00b7lig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In's bl\u00fcthenreiche Land herein!", "tokens": ["In's", "bl\u00fc\u00b7then\u00b7rei\u00b7che", "Land", "her\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}