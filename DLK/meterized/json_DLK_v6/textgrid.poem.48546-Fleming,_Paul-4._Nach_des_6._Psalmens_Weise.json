{"textgrid.poem.48546": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "4. Nach des 6. Psalmens Weise", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In allen meinen Taten", "tokens": ["In", "al\u00b7len", "mei\u00b7nen", "Ta\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "la\u00df ich den H\u00f6chsten raten,", "tokens": ["la\u00df", "ich", "den", "H\u00f6chs\u00b7ten", "ra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "der Alles kan und hat;", "tokens": ["der", "Al\u00b7les", "kan", "und", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "KON", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "er mu\u00df zu allen Dingen,", "tokens": ["er", "mu\u00df", "zu", "al\u00b7len", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "solls anders wol gelingen,", "tokens": ["solls", "an\u00b7ders", "wol", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "selbst geben Rat und Tat.", "tokens": ["selbst", "ge\u00b7ben", "Rat", "und", "Tat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nichts ist es spat und fr\u00fche", "tokens": ["Nichts", "ist", "es", "spat", "und", "fr\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PPER", "VVFIN", "KON", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "um alle meine M\u00fche,", "tokens": ["um", "al\u00b7le", "mei\u00b7ne", "M\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "mein Sorgen ist umsonst:", "tokens": ["mein", "Sor\u00b7gen", "ist", "um\u00b7sonst", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "er mags mit meinen Sachen", "tokens": ["er", "mags", "mit", "mei\u00b7nen", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "nach seinem Willen machen,", "tokens": ["nach", "sei\u00b7nem", "Wil\u00b7len", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "ich stells in seine Gunst.", "tokens": ["ich", "stells", "in", "sei\u00b7ne", "Gunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es kan mir nichts geschehen,", "tokens": ["Es", "kan", "mir", "nichts", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "als was er hat versehen", "tokens": ["als", "was", "er", "hat", "ver\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRELS", "PPER", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "und was mir selig ist.", "tokens": ["und", "was", "mir", "se\u00b7lig", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich nehm' es, wie ers giebet;", "tokens": ["Ich", "nehm'", "es", ",", "wie", "ers", "gie\u00b7bet", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "was ihm von mir geliebet,", "tokens": ["was", "ihm", "von", "mir", "ge\u00b7lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-----+-", "measure": "unknown.measure.single"}, "line.6": {"text": "das hab' auch ich erkiest.", "tokens": ["das", "hab'", "auch", "ich", "er\u00b7kiest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich traue seiner Gnaden,", "tokens": ["Ich", "trau\u00b7e", "sei\u00b7ner", "Gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "die mich f\u00fcr allem Schaden,", "tokens": ["die", "mich", "f\u00fcr", "al\u00b7lem", "Scha\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "f\u00fcr allem \u00dcbel sch\u00fctzt.", "tokens": ["f\u00fcr", "al\u00b7lem", "\u00dc\u00b7bel", "sch\u00fctzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Leb' ich nach seinen S\u00e4tzen,", "tokens": ["Leb'", "ich", "nach", "sei\u00b7nen", "S\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "so wird mich nichts verletzen,", "tokens": ["so", "wird", "mich", "nichts", "ver\u00b7let\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "nichts fehlen, was mir n\u00fctzt.", "tokens": ["nichts", "feh\u00b7len", ",", "was", "mir", "n\u00fctzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er wolle meiner S\u00fcnden", "tokens": ["Er", "wol\u00b7le", "mei\u00b7ner", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "in Gnaden mich entbinden,", "tokens": ["in", "Gna\u00b7den", "mich", "ent\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "durchstreichen meine Schuld!", "tokens": ["durch\u00b7strei\u00b7chen", "mei\u00b7ne", "Schuld", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er wird auf mein Verbrechen", "tokens": ["Er", "wird", "auf", "mein", "Ver\u00b7bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "nicht stracks das Urteil sprechen", "tokens": ["nicht", "stracks", "das", "Ur\u00b7teil", "spre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "und haben noch Gedult.", "tokens": ["und", "ha\u00b7ben", "noch", "Ge\u00b7dult", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich zieh' in ferne Lande,", "tokens": ["Ich", "zieh'", "in", "fer\u00b7ne", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "zu n\u00fctzen einem Stande,", "tokens": ["zu", "n\u00fct\u00b7zen", "ei\u00b7nem", "Stan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "an den er mich bestellt.", "tokens": ["an", "den", "er", "mich", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sein Segen wird mir lassen", "tokens": ["Sein", "Se\u00b7gen", "wird", "mir", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "was gut und recht ist fassen", "tokens": ["was", "gut", "und", "recht", "ist", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "KON", "ADJD", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "zu dienen seiner Welt.", "tokens": ["zu", "die\u00b7nen", "sei\u00b7ner", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Bin ich in wilder W\u00fcsten,", "tokens": ["Bin", "ich", "in", "wil\u00b7der", "W\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "so bin ich doch bei Christen", "tokens": ["so", "bin", "ich", "doch", "bei", "Chris\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "und Christus ist bei mir.", "tokens": ["und", "Chris\u00b7tus", "ist", "bei", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Helfer in Gefahren,", "tokens": ["Der", "Hel\u00b7fer", "in", "Ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "der kan mich doch bewahren,", "tokens": ["der", "kan", "mich", "doch", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "wie dorte, so auch hier.", "tokens": ["wie", "dor\u00b7te", ",", "so", "auch", "hier", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Er wird zu diesen Reisen", "tokens": ["Er", "wird", "zu", "die\u00b7sen", "Rei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "gew\u00fcndschten Fortgang weisen,", "tokens": ["ge\u00b7w\u00fcnd\u00b7schten", "Fort\u00b7gang", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "wol helfen hin und her,", "tokens": ["wol", "hel\u00b7fen", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gesundheit, Heil und Leben,", "tokens": ["Ge\u00b7sund\u00b7heit", ",", "Heil", "und", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zeit, Wind und Wetter geben", "tokens": ["Zeit", ",", "Wind", "und", "Wet\u00b7ter", "ge\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "und Alles nach Begehr.", "tokens": ["und", "Al\u00b7les", "nach", "Be\u00b7gehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Sein Engel, der getreue,", "tokens": ["Sein", "En\u00b7gel", ",", "der", "ge\u00b7treu\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "macht meine Feinde scheue,", "tokens": ["macht", "mei\u00b7ne", "Fein\u00b7de", "scheu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "trit zwischen mich und sie.", "tokens": ["trit", "zwi\u00b7schen", "mich", "und", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Durch seinen Zug, den frommen,", "tokens": ["Durch", "sei\u00b7nen", "Zug", ",", "den", "from\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "sind wir so weit nun kommen", "tokens": ["sind", "wir", "so", "weit", "nun", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "und wissen fast nicht wie.", "tokens": ["und", "wis\u00b7sen", "fast", "nicht", "wie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "PWAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Leg' ich mich sp\u00e4te nieder,", "tokens": ["Leg'", "ich", "mich", "sp\u00e4\u00b7te", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "erwach' ich fr\u00fche wieder,", "tokens": ["er\u00b7wach'", "ich", "fr\u00fc\u00b7he", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "lieg' oder zieh' ich fort,", "tokens": ["lieg'", "o\u00b7der", "zieh'", "ich", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "in Schwachheit und in Banden,", "tokens": ["in", "Schwach\u00b7heit", "und", "in", "Ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "und was mir st\u00f6\u00dft zu Handen,", "tokens": ["und", "was", "mir", "st\u00f6\u00dft", "zu", "Han\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "so tr\u00f6stet mich sein Wort.", "tokens": ["so", "tr\u00f6s\u00b7tet", "mich", "sein", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Hat er es denn beschlossen,", "tokens": ["Hat", "er", "es", "denn", "be\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "so will ich unverdrossen", "tokens": ["so", "will", "ich", "un\u00b7ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "an mein Verh\u00e4ngn\u00fc\u00df gehn;", "tokens": ["an", "mein", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "gehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."]