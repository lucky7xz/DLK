rd", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.7": {"text": "hab Sonne im Herzen,", "tokens": ["hab", "Son\u00b7ne", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "und alles wird gut!", "tokens": ["und", "al\u00b7les", "wird", "gut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}