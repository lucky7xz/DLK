{"textgrid.poem.32973": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hab Sonne im Herzen,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hab Sonne im Herzen,", "tokens": ["Hab", "Son\u00b7ne", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "ob's st\u00fcrmt oder schneit,", "tokens": ["ob's", "st\u00fcrmt", "o\u00b7der", "schneit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "ob der Himmel voll Wolken,", "tokens": ["ob", "der", "Him\u00b7mel", "voll", "Wol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "die Erde voll Streit ...", "tokens": ["die", "Er\u00b7de", "voll", "Streit", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "hab Sonne im Herzen,", "tokens": ["hab", "Son\u00b7ne", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "dann komme was mag:", "tokens": ["dann", "kom\u00b7me", "was", "mag", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "VMFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "das leuchtet voll Licht dir", "tokens": ["das", "leuch\u00b7tet", "voll", "Licht", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJD", "NN", "PPER"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "den dunkelsten Tag!", "tokens": ["den", "dun\u00b7kels\u00b7ten", "Tag", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Hab ein Lied auf den Lippen", "tokens": ["Hab", "ein", "Lied", "auf", "den", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "mit fr\u00f6hlichem Klang,", "tokens": ["mit", "fr\u00f6h\u00b7li\u00b7chem", "Klang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "und macht auch des Alltags", "tokens": ["und", "macht", "auch", "des", "All\u00b7tags"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Gedr\u00e4nge dich bang ...", "tokens": ["Ge\u00b7dr\u00e4n\u00b7ge", "dich", "bang", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "hab ein Lied auf den Lippen,", "tokens": ["hab", "ein", "Lied", "auf", "den", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "dann komme was mag:", "tokens": ["dann", "kom\u00b7me", "was", "mag", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "VMFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"tex