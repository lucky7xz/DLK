{"textgrid.poem.34005": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Welt und Ich", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im gro\u00dfen ungeheuren Oceane", "tokens": ["Im", "gro\u00b7\u00dfen", "un\u00b7ge\u00b7heu\u00b7ren", "O\u00b7cea\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Willst du, der Tropfe, dich in dich verschlie\u00dfen?", "tokens": ["Willst", "du", ",", "der", "Trop\u00b7fe", ",", "dich", "in", "dich", "ver\u00b7schlie\u00b7\u00dfen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "ART", "NN", "$,", "PRF", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So wirst du nie zur Perl' zusammen schie\u00dfen,", "tokens": ["So", "wirst", "du", "nie", "zur", "Perl'", "zu\u00b7sam\u00b7men", "schie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie dich auch Fluten sch\u00fctteln und Orcane!", "tokens": ["Wie", "dich", "auch", "Flu\u00b7ten", "sch\u00fct\u00b7teln", "und", "Or\u00b7ca\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "NN", "VVINF", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nein! \u00f6ffne deine innersten Organe", "tokens": ["Nein", "!", "\u00f6ff\u00b7ne", "dei\u00b7ne", "in\u00b7ners\u00b7ten", "Or\u00b7ga\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und mische dich im Leiden und Genie\u00dfen", "tokens": ["Und", "mi\u00b7sche", "dich", "im", "Lei\u00b7den", "und", "Ge\u00b7nie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit allen Str\u00f6men, die vor\u00fcber flie\u00dfen;", "tokens": ["Mit", "al\u00b7len", "Str\u00f6\u00b7men", ",", "die", "vor\u00b7\u00fc\u00b7ber", "flie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann dienst du dir und dienst dem h\u00f6chsten Plane.", "tokens": ["Dann", "dienst", "du", "dir", "und", "dienst", "dem", "h\u00f6chs\u00b7ten", "Pla\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und f\u00fcrchte nicht, so in die Welt versunken,", "tokens": ["Und", "f\u00fcrch\u00b7te", "nicht", ",", "so", "in", "die", "Welt", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dich selbst und dein Ur-Eig'nes zu verlieren:", "tokens": ["Dich", "selbst", "und", "dein", "Ur\u00b7Eig'\u00b7nes", "zu", "ver\u00b7lie\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Weg zu dir f\u00fchrt eben durch das Ganze!", "tokens": ["Der", "Weg", "zu", "dir", "f\u00fchrt", "e\u00b7ben", "durch", "das", "Gan\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Erst, wenn du k\u00fchn von jedem Wein getrunken,", "tokens": ["Erst", ",", "wenn", "du", "k\u00fchn", "von", "je\u00b7dem", "Wein", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADJD", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wirst du die Kraft im tiefsten Innern sp\u00fcren,", "tokens": ["Wirst", "du", "die", "Kraft", "im", "tiefs\u00b7ten", "In\u00b7nern", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die jedem Sturm zu steh'n vermag im Tanze!", "tokens": ["Die", "je\u00b7dem", "Sturm", "zu", "steh'n", "ver\u00b7mag", "im", "Tan\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PTKZU", "VVINF", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}