{"dta.poem.11719": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vom G\u00e4rtner kauft' ich mir ein sch\u00f6nes Blumenst\u00f6ckchen,", "tokens": ["Vom", "G\u00e4rt\u00b7ner", "kauft'", "ich", "mir", "ein", "sch\u00f6\u00b7nes", "Blu\u00b7men\u00b7st\u00f6ck\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So reich an Hoffnungen in halberschlossnen Gl\u00f6ckchen.", "tokens": ["So", "reich", "an", "Hoff\u00b7nun\u00b7gen", "in", "hal\u00b7ber\u00b7schloss\u00b7nen", "Gl\u00f6ck\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich wandte meine M\u00fch und meine Zeit darauf;", "tokens": ["Ich", "wand\u00b7te", "mei\u00b7ne", "M\u00fch", "und", "mei\u00b7ne", "Zeit", "da\u00b7rauf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Gl\u00f6ckchen bl\u00fchten zu, doch bl\u00fchten sie nicht auf.", "tokens": ["Die", "Gl\u00f6ck\u00b7chen", "bl\u00fch\u00b7ten", "zu", ",", "doch", "bl\u00fch\u00b7ten", "sie", "nicht", "auf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sie bl\u00fchten immer zu, bis sie unaufgebl\u00fcht", "tokens": ["Sie", "bl\u00fch\u00b7ten", "im\u00b7mer", "zu", ",", "bis", "sie", "un\u00b7auf\u00b7ge\u00b7bl\u00fcht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "ADJD"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Abwelkten, und betr\u00fcbt darob ward mein Gem\u00fct.", "tokens": ["Ab\u00b7welk\u00b7ten", ",", "und", "be\u00b7tr\u00fcbt", "da\u00b7rob", "ward", "mein", "Ge\u00b7m\u00fct", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVFIN", "PAV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Hat dich der G\u00e4rtner, hat die Hoffnung dich betrogen?", "tokens": ["Hat", "dich", "der", "G\u00e4rt\u00b7ner", ",", "hat", "die", "Hoff\u00b7nung", "dich", "be\u00b7tro\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie w\u00e4ren aufgebl\u00fcht, vom G\u00e4rtner selbst gezogen.", "tokens": ["Sie", "w\u00e4\u00b7ren", "auf\u00b7ge\u00b7bl\u00fcht", ",", "vom", "G\u00e4rt\u00b7ner", "selbst", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die Freude bl\u00fchet auf nur in des G\u00e4rtners Hand,", "tokens": ["Die", "Freu\u00b7de", "bl\u00fc\u00b7het", "auf", "nur", "in", "des", "G\u00e4rt\u00b7ners", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bei dir zu knospen ist die Hoffnung nur im Stand.", "tokens": ["Bei", "dir", "zu", "knos\u00b7pen", "ist", "die", "Hoff\u00b7nung", "nur", "im", "Stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "VAFIN", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}