{"textgrid.poem.61079": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Cita mors ruit", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der schnellste Reiter ist der Tod;", "tokens": ["Der", "schnells\u00b7te", "Rei\u00b7ter", "ist", "der", "Tod", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er \u00fcberreitet das Morgenrot,", "tokens": ["Er", "\u00fc\u00b7berr\u00b7ei\u00b7tet", "das", "Mor\u00b7gen\u00b7rot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Des Wetters rasches Blitzen;", "tokens": ["Des", "Wet\u00b7ters", "ra\u00b7sches", "Blit\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein Ro\u00df ist fahl und ungeschirrt,", "tokens": ["Sein", "Ro\u00df", "ist", "fahl", "und", "un\u00b7ge\u00b7schirrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Senne schwirrt, der Pfeil erklirrt", "tokens": ["Die", "Sen\u00b7ne", "schwirrt", ",", "der", "Pfeil", "er\u00b7klirrt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und mu\u00df im Herze sitzen.", "tokens": ["Und", "mu\u00df", "im", "Her\u00b7ze", "sit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPRART", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Durch Stadt und Dorf, \u00fcber Berg und Tal,", "tokens": ["Durch", "Stadt", "und", "Dorf", ",", "\u00fc\u00b7ber", "Berg", "und", "Tal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Morgenrot, im Abendstrahl", "tokens": ["Im", "Mor\u00b7gen\u00b7rot", ",", "im", "A\u00b7bend\u00b7strahl"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geht's fort in wildem Jagen,", "tokens": ["Geht's", "fort", "in", "wil\u00b7dem", "Ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und wo er floh mit Ungest\u00fcm,", "tokens": ["Und", "wo", "er", "floh", "mit", "Un\u00b7ge\u00b7st\u00fcm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da schallen die Glocken hinter ihm,", "tokens": ["Da", "schal\u00b7len", "die", "Glo\u00b7cken", "hin\u00b7ter", "ihm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und Grabeslieder klagen.", "tokens": ["Und", "Gra\u00b7bes\u00b7lie\u00b7der", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er tritt herein in den Prunkpalast,", "tokens": ["Er", "tritt", "her\u00b7ein", "in", "den", "Prunk\u00b7pa\u00b7last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da wird so bla\u00df der stolze Gast", "tokens": ["Da", "wird", "so", "bla\u00df", "der", "stol\u00b7ze", "Gast"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und l\u00e4\u00dft von Wein und Buhle;", "tokens": ["Und", "l\u00e4\u00dft", "von", "Wein", "und", "Buh\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er tritt zum lustigen Hochzeitsschmaus,", "tokens": ["Er", "tritt", "zum", "lus\u00b7ti\u00b7gen", "Hoch\u00b7zeits\u00b7schmaus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ein Windsto\u00df l\u00f6scht die Kerzen aus,", "tokens": ["Ein", "Wind\u00b7sto\u00df", "l\u00f6scht", "die", "Ker\u00b7zen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bleich lehnt die Braut im Stuhle.", "tokens": ["Bleich", "lehnt", "die", "Braut", "im", "Stuh\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dem Sch\u00f6ffen blickt er ins Gesicht,", "tokens": ["Dem", "Sch\u00f6f\u00b7fen", "blickt", "er", "ins", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der just das wei\u00dfe St\u00e4blein bricht,", "tokens": ["Der", "just", "das", "wei\u00b7\u00dfe", "St\u00e4\u00b7blein", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sinkt's ihm aus den H\u00e4nden;", "tokens": ["Da", "sinkt's", "ihm", "aus", "den", "H\u00e4n\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein M\u00e4gdlein windet Bl\u00fct' und Klee,", "tokens": ["Ein", "M\u00e4gd\u00b7lein", "win\u00b7det", "Bl\u00fct'", "und", "Klee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er tritt heran; ihr wird so weh -", "tokens": ["Er", "tritt", "he\u00b7ran", ";", "ihr", "wird", "so", "weh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "PPER", "VAFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wer mag den Strau\u00df vollenden!", "tokens": ["Wer", "mag", "den", "Strau\u00df", "voll\u00b7en\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NE", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Drum sei nicht stolz, o Menschenkind!", "tokens": ["Drum", "sei", "nicht", "stolz", ",", "o", "Men\u00b7schen\u00b7kind", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PTKNEG", "ADJD", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist dem Tod wie Spreu im Wind,", "tokens": ["Du", "bist", "dem", "Tod", "wie", "Spreu", "im", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KOKOM", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und magst du Kronen tragen.", "tokens": ["Und", "magst", "du", "Kro\u00b7nen", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Sand verrinnt, die Stunde schl\u00e4gt,", "tokens": ["Der", "Sand", "ver\u00b7rinnt", ",", "die", "Stun\u00b7de", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und eh' ein Hauch dies Blatt bewegt,", "tokens": ["Und", "eh'", "ein", "Hauch", "dies", "Blatt", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PDS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kann auch die deine schlagen.", "tokens": ["Kann", "auch", "die", "dei\u00b7ne", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}