{"textgrid.poem.32932": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: So regnet es sich langsam ein", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.85", "fr:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So regnet es sich langsam ein", "tokens": ["So", "reg\u00b7net", "es", "sich", "lang\u00b7sam", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "und immer k\u00fcrzer wird der Tag und immer seltener der Sonnenschein ...", "tokens": ["und", "im\u00b7mer", "k\u00fcr\u00b7zer", "wird", "der", "Tag", "und", "im\u00b7mer", "sel\u00b7te\u00b7ner", "der", "Son\u00b7nen\u00b7schein", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "ART", "NN", "KON", "ADV", "ADJD", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+---+-+", "measure": "unknown.measure.octa.plus"}}, "stanza.3": {"line.1": {"text": "Ich sah am Waldrand gestern ein paar Rosen stehn ...", "tokens": ["Ich", "sah", "am", "Wald\u00b7rand", "ge\u00b7stern", "ein", "paar", "Ro\u00b7sen", "stehn", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "ART", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "gib mir die Hand und komm ... wir wollen sie uns pfl\u00fccken gehn ...", "tokens": ["gib", "mir", "die", "Hand", "und", "komm", "...", "wir", "wol\u00b7len", "sie", "uns", "pfl\u00fc\u00b7cken", "gehn", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "KON", "VVFIN", "$(", "PPER", "VMFIN", "PPER", "PRF", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.4": {"line.1": {"text": "Es werden wohl die letzten sein!", "tokens": ["Es", "wer\u00b7den", "wohl", "die", "letz\u00b7ten", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}