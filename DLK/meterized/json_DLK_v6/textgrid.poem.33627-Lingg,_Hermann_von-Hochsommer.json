{"textgrid.poem.33627": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Hochsommer", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Goldbeschwingte s\u00fc\u00dfe Stunden", "tokens": ["Gold\u00b7be\u00b7schwing\u00b7te", "s\u00fc\u00b7\u00dfe", "Stun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ungest\u00f6rter Tr\u00e4umerei,", "tokens": ["Un\u00b7ge\u00b7st\u00f6r\u00b7ter", "Tr\u00e4u\u00b7me\u00b7rei", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie zu sch\u00f6nem Kranz gewunden", "tokens": ["Wie", "zu", "sch\u00f6\u00b7nem", "Kranz", "ge\u00b7wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "VAPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwebt ihr \u00fcber uns verbunden,", "tokens": ["Schwebt", "ihr", "\u00fc\u00b7ber", "uns", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Eilet nicht so bald vorbei!", "tokens": ["Ei\u00b7let", "nicht", "so", "bald", "vor\u00b7bei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Waldher locken Vogelstimmen,", "tokens": ["Wald\u00b7her", "lo\u00b7cken", "Vo\u00b7gel\u00b7stim\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter Blumen schl\u00e4ft der Hirt,", "tokens": ["Un\u00b7ter", "Blu\u00b7men", "schl\u00e4ft", "der", "Hirt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Um ihn summt die Schar der Immen,", "tokens": ["Um", "ihn", "summt", "die", "Schar", "der", "Im\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Goldbeschwingte K\u00e4fer glimmen,", "tokens": ["Gold\u00b7be\u00b7schwing\u00b7te", "K\u00e4\u00b7fer", "glim\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles leuchtet, gl\u00fcht und schwirrt.", "tokens": ["Al\u00b7les", "leuch\u00b7tet", ",", "gl\u00fcht", "und", "schwirrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lautre Wonne will ich trinken,", "tokens": ["Laut\u00b7re", "Won\u00b7ne", "will", "ich", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wonne reiner Lebensflut,", "tokens": ["Won\u00b7ne", "rei\u00b7ner", "Le\u00b7bens\u00b7flut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis herauf die Sterne blinken,", "tokens": ["Bis", "her\u00b7auf", "die", "Ster\u00b7ne", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis die Purpurwolken sinken", "tokens": ["Bis", "die", "Pur\u00b7pur\u00b7wol\u00b7ken", "sin\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In den Scho\u00df der Abendglut.", "tokens": ["In", "den", "Scho\u00df", "der", "A\u00b7bend\u00b7glut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}