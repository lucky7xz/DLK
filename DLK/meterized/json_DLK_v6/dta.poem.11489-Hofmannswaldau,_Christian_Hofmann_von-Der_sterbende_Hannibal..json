{"dta.poem.11489": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der sterbende Hannibal.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje ist mir? Bin ich blind? Ha! sch\u00e4ndlicher verr\u00e4ther!", "tokens": ["Wie", "ist", "mir", "?", "Bin", "ich", "blind", "?", "Ha", "!", "sch\u00e4nd\u00b7li\u00b7cher", "ver\u00b7r\u00e4\u00b7ther", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$.", "VAFIN", "PPER", "ADJD", "$.", "ITJ", "$.", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O himmel! wo ist treu, wenn sie ein k\u00f6nig bricht?", "tokens": ["O", "him\u00b7mel", "!", "wo", "ist", "treu", ",", "wenn", "sie", "ein", "k\u00f6\u00b7nig", "bricht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAV", "VAFIN", "ADJD", "$,", "KOUS", "PPER", "ART", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verdammter Prusias! bin ich ein misseth\u00e4ter?", "tokens": ["Ver\u00b7damm\u00b7ter", "Pru\u00b7si\u00b7as", "!", "bin", "ich", "ein", "mis\u00b7set\u00b7h\u00e4\u00b7ter", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vergi\u00dft der wilde feind denn aller helden pflicht?", "tokens": ["Ver\u00b7gi\u00dft", "der", "wil\u00b7de", "feind", "denn", "al\u00b7ler", "hel\u00b7den", "pflicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "KON", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Rom! Rom! wo bleibt dein ruhm, den du dir in der jugend", "tokens": ["Rom", "!", "Rom", "!", "wo", "bleibt", "dein", "ruhm", ",", "den", "du", "dir", "in", "der", "ju\u00b7gend"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "NE", "$.", "PWAV", "VVFIN", "PPOSAT", "NN", "$,", "PRELS", "PPER", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Durch seltne gro\u00dfmuth auch bey feinden hast gemacht?", "tokens": ["Durch", "selt\u00b7ne", "gro\u00df\u00b7muth", "auch", "bey", "fein\u00b7den", "hast", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "APPR", "ADJA", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich schaue nur zuviel, du giebst der edlen tugend", "tokens": ["Ich", "schau\u00b7e", "nur", "zu\u00b7viel", ",", "du", "giebst", "der", "ed\u00b7len", "tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Aus frechem \u00fcbermuth itzt eine gute nacht.", "tokens": ["Aus", "fre\u00b7chem", "\u00fc\u00b7ber\u00b7muth", "itzt", "ei\u00b7ne", "gu\u00b7te", "nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wilst du im alter erst zu einem wolffe werden?", "tokens": ["Wilst", "du", "im", "al\u00b7ter", "erst", "zu", "ei\u00b7nem", "wolf\u00b7fe", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "ADJA", "ADV", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Rom! Rom! wo bleibt dein ruhm, wenn dessen grund verf\u00e4llt?", "tokens": ["Rom", "!", "Rom", "!", "wo", "bleibt", "dein", "ruhm", ",", "wenn", "des\u00b7sen", "grund", "ver\u00b7f\u00e4llt", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "$.", "PWAV", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PDS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Du machst dich endlich gar zum ungeheur auf erden,", "tokens": ["Du", "machst", "dich", "end\u00b7lich", "gar", "zum", "un\u00b7ge\u00b7heur", "auf", "er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil t\u00fcck und meichel-mord dein ehren-bild verstellt.", "tokens": ["Weil", "t\u00fcck", "und", "mei\u00b7chel\u00b7mord", "dein", "eh\u00b7ren\u00b7bild", "ver\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NE", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Bist du das grosse Rom, wo witz und tugend wohnet?", "tokens": ["Bist", "du", "das", "gros\u00b7se", "Rom", ",", "wo", "witz", "und", "tu\u00b7gend", "woh\u00b7net", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NE", "$,", "PWAV", "NE", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und wo die raths-herrn gar den g\u00f6ttern \u00e4hnlich sind?", "tokens": ["Und", "wo", "die", "raths\u00b7herrn", "gar", "den", "g\u00f6t\u00b7tern", "\u00e4hn\u00b7lich", "sind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "ART", "ADJA", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Bist du das grosse Rom, das auch der feinde schonet:", "tokens": ["Bist", "du", "das", "gros\u00b7se", "Rom", ",", "das", "auch", "der", "fein\u00b7de", "scho\u00b7net", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NE", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wann nicht die tapfferkeit den sieges-krantz gewinnt?", "tokens": ["Wann", "nicht", "die", "tapf\u00b7fer\u00b7keit", "den", "sie\u00b7ges\u00b7krantz", "ge\u00b7winnt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wo sind die helden hin, so deine schos gebohren?", "tokens": ["Wo", "sind", "die", "hel\u00b7den", "hin", ",", "so", "dei\u00b7ne", "schos", "ge\u00b7boh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "PTKVZ", "$,", "ADV", "PPOSAT", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wo ist Fabricius, der helden-wercke that?", "tokens": ["Wo", "ist", "Fab\u00b7ri\u00b7ci\u00b7us", ",", "der", "hel\u00b7den\u00b7\u00b7wer\u00b7cke", "that", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NE", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ach ja! die tugend hat das b\u00fcrger-recht verlohren,", "tokens": ["Ach", "ja", "!", "die", "tu\u00b7gend", "hat", "das", "b\u00fcr\u00b7ger\u00b7recht", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Nachdem der bosheit grimm das hefft in h\u00e4nden hat.", "tokens": ["Nach\u00b7dem", "der", "bos\u00b7heit", "grimm", "das", "hefft", "in", "h\u00e4n\u00b7den", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "ART", "NN", "APPR", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Man denckt itzund nicht mehr an tapffrer ahnen thaten:", "tokens": ["Man", "denckt", "it\u00b7zund", "nicht", "mehr", "an", "tapf\u00b7frer", "ah\u00b7nen", "tha\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PTKNEG", "ADV", "APPR", "ADJA", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ein jeder folget blos dem sporne seiner wuth;", "tokens": ["Ein", "je\u00b7der", "fol\u00b7get", "blos", "dem", "spor\u00b7ne", "sei\u00b7ner", "wuth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "ART", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Doch kan der tugend macht nicht ihren helden rathen?", "tokens": ["Doch", "kan", "der", "tu\u00b7gend", "macht", "nicht", "ih\u00b7ren", "hel\u00b7den", "ra\u00b7then", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "VVFIN", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Straft denn der himmel nicht, was grimm und bosheit thut?", "tokens": ["Straft", "denn", "der", "him\u00b7mel", "nicht", ",", "was", "grimm", "und", "bos\u00b7heit", "thut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "PTKNEG", "$,", "PRELS", "ADJD", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Darff meineyd und betrug auf sichern thronen sitzen?", "tokens": ["Darff", "mei\u00b7neyd", "und", "be\u00b7trug", "auf", "si\u00b7chern", "thro\u00b7nen", "sit\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "KON", "VVFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Lacht des gel\u00fcckes gunst nur die verr\u00e4ther an?", "tokens": ["Lacht", "des", "ge\u00b7l\u00fc\u00b7ckes", "gunst", "nur", "die", "ver\u00b7r\u00e4\u00b7ther", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADV", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Verh\u00e4ngni\u00df sonder recht! Du l\u00e4\u00dft die tugend schwitzen,", "tokens": ["Ver\u00b7h\u00e4ng\u00b7ni\u00df", "son\u00b7der", "recht", "!", "Du", "l\u00e4\u00dft", "die", "tu\u00b7gend", "schwit\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$.", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wann ein verdammter schelm in rosen schlafen kan.", "tokens": ["Wann", "ein", "ver\u00b7damm\u00b7ter", "schelm", "in", "ro\u00b7sen", "schla\u00b7fen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Best\u00fcrtzter Hannibal! in was vor labyrinthe", "tokens": ["Be\u00b7st\u00fcrtz\u00b7ter", "Han\u00b7ni\u00b7bal", "!", "in", "was", "vor", "la\u00b7by\u00b7rin\u00b7the"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "$.", "APPR", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Rei\u00dft dich die ungeduld, das blendwerck der vernunfft?", "tokens": ["Rei\u00dft", "dich", "die", "un\u00b7ge\u00b7duld", ",", "das", "blend\u00b7werck", "der", "ver\u00b7nunfft", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.31": {"text": "Wenn gleich dein eyfer itzt auf donner-worte sinnte;", "tokens": ["Wenn", "gleich", "dein", "ey\u00b7fer", "itzt", "auf", "don\u00b7ner\u00b7wor\u00b7te", "sinn\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So wei\u00df die freyheit doch von keiner wiederkunfft.", "tokens": ["So", "wei\u00df", "die", "frey\u00b7heit", "doch", "von", "kei\u00b7ner", "wie\u00b7der\u00b7kunfft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die freyheit, so bi\u00dfher mein abgott ist gewesen,", "tokens": ["Die", "frey\u00b7heit", ",", "so", "bi\u00df\u00b7her", "mein", "ab\u00b7gott", "ist", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "PPOSAT", "VVPP", "VAFIN", "VAPP", "$,"], "meter": "-+--++-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "Geht, eh\u2019 ich selbst vergeh, wie flammen in den wind,", "tokens": ["Geht", ",", "eh'", "ich", "selbst", "ver\u00b7geh", ",", "wie", "flam\u00b7men", "in", "den", "wind", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,", "PWAV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und hei\u00dft mich zum verdru\u00df den schweren ausspruch lesen:", "tokens": ["Und", "hei\u00dft", "mich", "zum", "ver\u00b7dru\u00df", "den", "schwe\u00b7ren", "aus\u00b7spruch", "le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da\u00df adler eben so, wie k\u00e4fer, sterblich sind.", "tokens": ["Da\u00df", "ad\u00b7ler", "e\u00b7ben", "so", ",", "wie", "k\u00e4\u00b7fer", ",", "sterb\u00b7lich", "sind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "ADV", "$,", "PWAV", "ADJD", "$,", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Doch soll ein edler fu\u00df beschimpffte fessel tragen?", "tokens": ["Doch", "soll", "ein", "ed\u00b7ler", "fu\u00df", "be\u00b7schimpff\u00b7te", "fes\u00b7sel", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Mu\u00df Hannibal nun erst ein feiger sclave seyn?", "tokens": ["Mu\u00df", "Han\u00b7ni\u00b7bal", "nun", "erst", "ein", "fei\u00b7ger", "scla\u00b7ve", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "ADV", "ADV", "ART", "ADJA", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Soll man den helden-arm in band und eisen schlagen?", "tokens": ["Soll", "man", "den", "hel\u00b7den\u00b7arm", "in", "band", "und", "ei\u00b7sen", "schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "NN", "APPR", "NE", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Schliest deinen freyen geist ein d\u00fcstrer kercker ein?", "tokens": ["Schliest", "dei\u00b7nen", "frey\u00b7en", "geist", "ein", "d\u00fcst\u00b7rer", "ker\u00b7cker", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "wor