{"dta.poem.13028": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aufrichtigkeit sah zu, was es dann wurde seyn,", "tokens": ["Auf\u00b7rich\u00b7tig\u00b7keit", "sah", "zu", ",", "was", "es", "dann", "wur\u00b7de", "seyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$,", "PWS", "PPER", "ADV", "VAFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sprach: \u201eSo nennest du die goldne Bl\u00e4tter dein?", "tokens": ["Und", "sprach", ":", "\u201e", "So", "nen\u00b7nest", "du", "die", "gold\u00b7ne", "Bl\u00e4t\u00b7ter", "dein", "?"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ewann unser ", "tokens": ["\u201e", "wann", "un\u00b7ser"], "token_info": ["punct", "word", "word"], "pos": ["$(", "PWAV", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "\u201ewer wei\u00df ob dein Gespr\u00e4ch den Schaz so sehr erhebte?", "tokens": ["\u201e", "wer", "wei\u00df", "ob", "dein", "Ge\u00b7spr\u00e4ch", "den", "Schaz", "so", "sehr", "er\u00b7heb\u00b7te", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "KOUS", "PPOSAT", "NN", "ART", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201ewann hat er dich so viel, als mich, um Rath gefragt?", "tokens": ["\u201e", "wann", "hat", "er", "dich", "so", "viel", ",", "als", "mich", ",", "um", "Rath", "ge\u00b7fragt", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "PPER", "PRF", "ADV", "ADV", "$,", "KOUS", "PPER", "$,", "KOUI", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "170\u201dWie manch Mahl hat er dich von seinem Thron verjagt?", "tokens": ["\"", "Wie", "manch", "Mahl", "hat", "er", "dich", "von", "sei\u00b7nem", "Thron", "ver\u00b7jagt", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PIAT", "NN", "VAFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}