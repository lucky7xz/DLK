{"textgrid.poem.39287": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schlage zum Himmel, Champagnergezisch,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlage zum Himmel, Champagnergezisch,", "tokens": ["Schla\u00b7ge", "zum", "Him\u00b7mel", ",", "Cham\u00b7pag\u00b7ner\u00b7ge\u00b7zisch", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Springe in silbernen Strudelkaskaden,", "tokens": ["Sprin\u00b7ge", "in", "sil\u00b7ber\u00b7nen", "Stru\u00b7del\u00b7kas\u00b7ka\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Schie\u00dfe in pochenden,", "tokens": ["Schie\u00b7\u00dfe", "in", "po\u00b7chen\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.4": {"text": "B\u00e4umenden Fluten,", "tokens": ["B\u00e4u\u00b7men\u00b7den", "Flu\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Flie\u00dfe in kochenden,", "tokens": ["Flie\u00b7\u00dfe", "in", "ko\u00b7chen\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.6": {"text": "Sch\u00e4umenden Gluten,", "tokens": ["Sch\u00e4u\u00b7men\u00b7den", "Glu\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "\u00c4hnlich dem Bronnen der Quellennajaden,", "tokens": ["\u00c4hn\u00b7lich", "dem", "Bron\u00b7nen", "der", "Quel\u00b7len\u00b7na\u00b7ja\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Drin sich die Glieder der Artemis baden,", "tokens": ["Drin", "sich", "die", "Glie\u00b7der", "der", "Ar\u00b7te\u00b7mis", "ba\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.9": {"text": "Tief in des Idas Zypressengeb\u00fcsch.", "tokens": ["Tief", "in", "des", "I\u00b7das", "Zyp\u00b7res\u00b7sen\u00b7ge\u00b7b\u00fcsch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NE", "NE", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Forme die Perlen von silbernem Schaum,", "tokens": ["For\u00b7me", "die", "Per\u00b7len", "von", "sil\u00b7ber\u00b7nem", "Schaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Die sich erheben aus siedendem Spiegel,", "tokens": ["Die", "sich", "er\u00b7he\u00b7ben", "aus", "sie\u00b7den\u00b7dem", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVINF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die in den spitzigen", "tokens": ["Die", "in", "den", "spit\u00b7zi\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "Trichterpokalen", "tokens": ["Trich\u00b7ter\u00b7po\u00b7ka\u00b7len"], "token_info": ["word"], "pos": ["NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Funkelnd dem hitzigen", "tokens": ["Fun\u00b7kelnd", "dem", "hit\u00b7zi\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ART", "NN"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.6": {"text": "Sprudel entstrahlen,", "tokens": ["Spru\u00b7del", "ent\u00b7strah\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Die aus der Flasche gebrochenem Siegel", "tokens": ["Die", "aus", "der", "Fla\u00b7sche", "ge\u00b7bro\u00b7che\u00b7nem", "Sie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Schweben und tanzen auf duftigem Fl\u00fcgel,", "tokens": ["Schwe\u00b7ben", "und", "tan\u00b7zen", "auf", "duf\u00b7ti\u00b7gem", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.9": {"text": "Steigen und sinken im goldigen Raum.", "tokens": ["Stei\u00b7gen", "und", "sin\u00b7ken", "im", "gol\u00b7di\u00b7gen", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Schlagt auf die Becher mit wirbelndem Schlag,", "tokens": ["Schlagt", "auf", "die", "Be\u00b7cher", "mit", "wir\u00b7beln\u00b7dem", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df sie erbrausen in rollendem Falle;", "tokens": ["Da\u00df", "sie", "er\u00b7brau\u00b7sen", "in", "rol\u00b7len\u00b7dem", "Fal\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "La\u00dft in den duftigen", "tokens": ["La\u00dft", "in", "den", "duf\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "ART", "ADJA"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Tiefen des Nasses", "tokens": ["Tie\u00b7fen", "des", "Nas\u00b7ses"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Tanzen die luftigen", "tokens": ["Tan\u00b7zen", "die", "luf\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Geister des Fasses;", "tokens": ["Geis\u00b7ter", "des", "Fas\u00b7ses", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "La\u00dft sie in spritzendem, staubendem Falle", "tokens": ["La\u00dft", "sie", "in", "sprit\u00b7zen\u00b7dem", ",", "stau\u00b7ben\u00b7dem", "Fal\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "St\u00fcrzen aus blitzendem Becherkristalle;", "tokens": ["St\u00fcr\u00b7zen", "aus", "blit\u00b7zen\u00b7dem", "Be\u00b7cher\u00b7kris\u00b7tal\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.9": {"text": "Kurz ist der Jugend moussierender Tag.", "tokens": ["Kurz", "ist", "der", "Ju\u00b7gend", "mous\u00b7sie\u00b7ren\u00b7der", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}