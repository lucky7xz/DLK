{"textgrid.poem.46956": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Landsturmliedchen", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer warst du? Ein Schneider,", "tokens": ["Wer", "warst", "du", "?", "Ein", "Schnei\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$.", "ART", "NE", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ich flickte franz\u00f6sische Kleider.", "tokens": ["Ich", "flick\u00b7te", "fran\u00b7z\u00f6\u00b7si\u00b7sche", "Klei\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wer warst du? Ein Schuster,", "tokens": ["Wer", "warst", "du", "?", "Ein", "Schus\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$.", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Schnitt Schuh' nach franz\u00f6sischem Muster.", "tokens": ["Schnitt", "Schuh'", "nach", "fran\u00b7z\u00f6\u00b7si\u00b7schem", "Mus\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ADJA", "NN", "$."], "meter": "----+--+-", "measure": "iambic.di.relaxed"}, "line.5": {"text": "Nun denn, ihr beiden,", "tokens": ["Nun", "denn", ",", "ihr", "bei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPER", "PIAT", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Was wollt ihr jetzt schneiden?", "tokens": ["Was", "wollt", "ihr", "jetzt", "schnei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Mit Scheren und mit Pfriemen", "tokens": ["Mit", "Sche\u00b7ren", "und", "mit", "Pfrie\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Franz\u00f6sische H\u00e4ute zu Riemen;", "tokens": ["Fran\u00b7z\u00f6\u00b7si\u00b7sche", "H\u00e4u\u00b7te", "zu", "Rie\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text": "Solch Handwerk will jetzt uns geziemen.", "tokens": ["Solch", "Hand\u00b7werk", "will", "jetzt", "uns", "ge\u00b7zie\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer warst du? Ein Bauer,", "tokens": ["Wer", "warst", "du", "?", "Ein", "Bau\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$.", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ich pfl\u00fcgte meinen Acker,", "tokens": ["Ich", "pfl\u00fcg\u00b7te", "mei\u00b7nen", "A\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Franzmann machte mir's sauer,", "tokens": ["Der", "Franz\u00b7mann", "mach\u00b7te", "mir's", "sau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hie\u00df mich Hund und Racker.", "tokens": ["Hie\u00df", "mich", "Hund", "und", "Ra\u00b7cker", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wie denkst du mit Glimpfe", "tokens": ["Wie", "denkst", "du", "mit", "Glimp\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Dich zu r\u00e4chen am Schimpfe?", "tokens": ["Dich", "zu", "r\u00e4\u00b7chen", "am", "Schimp\u00b7fe", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "APPRART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Der Ernte mu\u00dft' ich entraten,", "tokens": ["Der", "Ern\u00b7te", "mu\u00dft'", "ich", "ent\u00b7ra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Die mir die Feinde zertraten:", "tokens": ["Die", "mir", "die", "Fein\u00b7de", "zer\u00b7tra\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "M\u00e4h'n will ich sie selber wie Saaten.", "tokens": ["M\u00e4h'n", "will", "ich", "sie", "sel\u00b7ber", "wie", "Saa\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "ADV", "KOKOM", "NN", "$."], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Mein Rock hat nicht viel Taschen;", "tokens": ["Mein", "Rock", "hat", "nicht", "viel", "Ta\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In einer hier hab' ich mein Brot.", "tokens": ["In", "ei\u00b7ner", "hier", "hab'", "ich", "mein", "Brot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sagt mir, in welche Taschen", "tokens": ["Sagt", "mir", ",", "in", "wel\u00b7che", "Ta\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PWAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Steck' ich das Blei, das mir not?", "tokens": ["Steck'", "ich", "das", "Blei", ",", "das", "mir", "not", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Steck's in Gottes Namen", "tokens": ["Steck's", "in", "Got\u00b7tes", "Na\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "In eine Tasche zusammen.", "tokens": ["In", "ei\u00b7ne", "Ta\u00b7sche", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "M\u00f6g' es wohl behagen!", "tokens": ["M\u00f6g'", "es", "wohl", "be\u00b7ha\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Dein Brot in deinen Magen,", "tokens": ["Dein", "Brot", "in", "dei\u00b7nen", "Ma\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Dein Blei dem Feind in den Kragen!", "tokens": ["Dein", "Blei", "dem", "Feind", "in", "den", "Kra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Der Feind hat Achselb\u00e4nder,", "tokens": ["Der", "Feind", "hat", "Ach\u00b7sel\u00b7b\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und geht in Golde pur;", "tokens": ["Und", "geht", "in", "Gol\u00b7de", "pur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir haben nicht sch\u00f6ne Gew\u00e4nder,", "tokens": ["Wir", "ha\u00b7ben", "nicht", "sch\u00f6\u00b7ne", "Ge\u00b7w\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wir haben gar keine Montur.", "tokens": ["Wir", "ha\u00b7ben", "gar", "kei\u00b7ne", "Mon\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Will das euch verdrie\u00dfen?", "tokens": ["Will", "das", "euch", "ver\u00b7drie\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Hinan mit den Spie\u00dfen!", "tokens": ["Hi\u00b7nan", "mit", "den", "Spie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Stecht Feinde tot, mit ihren", "tokens": ["Stecht", "Fein\u00b7de", "tot", ",", "mit", "ih\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "ADJD", "$,", "APPR", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Kleidern dann sollt ihr euch zieren", "tokens": ["Klei\u00b7dern", "dann", "sollt", "ihr", "euch", "zie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VMFIN", "PPER", "PPER", "VVFIN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.9": {"text": "Gleich lauter Offizieren.", "tokens": ["Gleich", "lau\u00b7ter", "Of\u00b7fi\u00b7zie\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}