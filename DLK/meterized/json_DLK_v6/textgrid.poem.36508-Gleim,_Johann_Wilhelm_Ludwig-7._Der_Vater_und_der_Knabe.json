{"textgrid.poem.36508": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "7. Der Vater und der Knabe", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Knabe nahm aus einem Vogelnest", "tokens": ["Ein", "Kna\u00b7be", "nahm", "aus", "ei\u00b7nem", "Vo\u00b7gel\u00b7nest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Jungen aus! Die Alten flogen,", "tokens": ["Die", "Jun\u00b7gen", "aus", "!", "Die", "Al\u00b7ten", "flo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In Winkeln und in Bogen,", "tokens": ["In", "Win\u00b7keln", "und", "in", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ums leere Nest,", "tokens": ["Ums", "lee\u00b7re", "Nest", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und jammerten um ihre Jungen! Und da kam", "tokens": ["Und", "jam\u00b7mer\u00b7ten", "um", "ih\u00b7re", "Jun\u00b7gen", "!", "Und", "da", "kam"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "KON", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des Knaben Vater. \u2013 Du, du hast gewi\u00df,", "tokens": ["Des", "Kna\u00b7ben", "Va\u00b7ter", ".", "\u2013", "Du", ",", "du", "hast", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$.", "$(", "PPER", "$,", "PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich seh's an deiner Augen Finsternis,", "tokens": ["Ich", "seh's", "an", "dei\u00b7ner", "Au\u00b7gen", "Fins\u00b7ter\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die Jungen ausgenommen! \u2013 Ich? ich nahm", "tokens": ["Die", "Jun\u00b7gen", "aus\u00b7ge\u00b7nom\u00b7men", "!", "\u2013", "Ich", "?", "ich", "nahm"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVPP", "$.", "$(", "PPER", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die Jungen aus! \u2013 Wo sind sie? \u2013 In der Stube!", "tokens": ["Die", "Jun\u00b7gen", "aus", "!", "\u2013", "Wo", "sind", "sie", "?", "\u2013", "In", "der", "Stu\u00b7be", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "$(", "PWAV", "VAFIN", "PPER", "$.", "$(", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hab' ich, du kleiner Bube!", "tokens": ["Hab'", "ich", ",", "du", "klei\u00b7ner", "Bu\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dir nicht gesagt, du solltest das nicht thun?", "tokens": ["Dir", "nicht", "ge\u00b7sagt", ",", "du", "soll\u00b7test", "das", "nicht", "thun", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVPP", "$,", "PPER", "VMFIN", "PDS", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da jammern um die Kinder nun", "tokens": ["Da", "jam\u00b7mern", "um", "die", "Kin\u00b7der", "nun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Vater und die Mutter; denk einmal!", "tokens": ["Der", "Va\u00b7ter", "und", "die", "Mut\u00b7ter", ";", "denk", "ein\u00b7mal", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$.", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich, Vater, h\u00e4tte Herzensqual,", "tokens": ["Ich", ",", "Va\u00b7ter", ",", "h\u00e4t\u00b7te", "Her\u00b7zens\u00b7qual", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn du, mein Kind,", "tokens": ["Wenn", "du", ",", "mein", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Mir so genommen w\u00e4rst! Geschwind!", "tokens": ["Mir", "so", "ge\u00b7nom\u00b7men", "w\u00e4rst", "!", "Ge\u00b7schwind", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "VAFIN", "$.", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Trag sie zur\u00fcck ins Nest; du wirst,", "tokens": ["Trag", "sie", "zu\u00b7r\u00fcck", "ins", "Nest", ";", "du", "wirst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "APPRART", "NN", "$.", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Hoff' ich, gehorsam sein!", "tokens": ["Hoff'", "ich", ",", "ge\u00b7hor\u00b7sam", "sein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "War er gehorsam? Nein!", "tokens": ["War", "er", "ge\u00b7hor\u00b7sam", "?", "Nein", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$.", "PTKANT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und ach! der Vater war ein F\u00fcrst!", "tokens": ["Und", "ach", "!", "der", "Va\u00b7ter", "war", "ein", "F\u00fcrst", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}