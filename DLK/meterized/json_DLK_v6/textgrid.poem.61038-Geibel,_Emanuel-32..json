{"textgrid.poem.61038": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "32.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mai ist gekommen, die B\u00e4ume schlagen aus,", "tokens": ["Der", "Mai", "ist", "ge\u00b7kom\u00b7men", ",", "die", "B\u00e4u\u00b7me", "schla\u00b7gen", "aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da bleibe, wer Lust hat, mit Sorgen zu Haus;", "tokens": ["Da", "blei\u00b7be", ",", "wer", "Lust", "hat", ",", "mit", "Sor\u00b7gen", "zu", "Haus", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWS", "NN", "VAFIN", "$,", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wie die Wolken wandern am himmlischen Zelt,", "tokens": ["Wie", "die", "Wol\u00b7ken", "wan\u00b7dern", "am", "himm\u00b7li\u00b7schen", "Zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So steht auch mir der Sinn in die weite, weite Welt.", "tokens": ["So", "steht", "auch", "mir", "der", "Sinn", "in", "die", "wei\u00b7te", ",", "wei\u00b7te", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ART", "NN", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Herr Vater, Frau Mutter, da\u00df Gott euch beh\u00fct!", "tokens": ["Herr", "Va\u00b7ter", ",", "Frau", "Mut\u00b7ter", ",", "da\u00df", "Gott", "euch", "be\u00b7h\u00fct", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "NN", "$,", "KOUS", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wer wei\u00df, wo in der Ferne mein Gl\u00fcck mir noch bl\u00fcht!", "tokens": ["Wer", "wei\u00df", ",", "wo", "in", "der", "Fer\u00b7ne", "mein", "Gl\u00fcck", "mir", "noch", "bl\u00fcht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "APPR", "ART", "NN", "PPOSAT", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Es gibt so manche Stra\u00dfe, da nimmer ich marschiert,", "tokens": ["Es", "gibt", "so", "man\u00b7che", "Stra\u00b7\u00dfe", ",", "da", "nim\u00b7mer", "ich", "mar\u00b7schiert", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "$,", "KOUS", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Es gibt so manchen Wein, den ich nimmer noch probiert.", "tokens": ["Es", "gibt", "so", "man\u00b7chen", "Wein", ",", "den", "ich", "nim\u00b7mer", "noch", "pro\u00b7biert", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Frisch auf drum, frisch auf im hellen Sonnenstrahl", "tokens": ["Frisch", "auf", "drum", ",", "frisch", "auf", "im", "hel\u00b7len", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PAV", "$,", "ADJD", "APPR", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wohl \u00fcber die Berge, wohl durch das tie e Tal!", "tokens": ["Wohl", "\u00fc\u00b7ber", "die", "Ber\u00b7ge", ",", "wohl", "durch", "das", "tie", "e", "Tal", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+----+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Quellen erklingen, die B\u00e4ume rauschen all,", "tokens": ["Die", "Quel\u00b7len", "er\u00b7klin\u00b7gen", ",", "die", "B\u00e4u\u00b7me", "rau\u00b7schen", "all", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVFIN", "PIAT", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Mein Herz ist wie'ne Lerche und stimmet ein mit Schall.", "tokens": ["Mein", "Herz", "ist", "wie'\u00b7ne", "Ler\u00b7che", "und", "stim\u00b7met", "ein", "mit", "Schall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "NN", "KON", "VVFIN", "ART", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Und abends im St\u00e4dtlein da kehr' ich durstig ein:", "tokens": ["Und", "a\u00b7bends", "im", "St\u00e4dt\u00b7lein", "da", "kehr'", "ich", "durs\u00b7tig", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "\u00bbherr Wirt, Herr Wirt, eine Kanne blanken Wein!", "tokens": ["\u00bb", "herr", "Wirt", ",", "Herr", "Wirt", ",", "ei\u00b7ne", "Kan\u00b7ne", "blan\u00b7ken", "Wein", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NN", "$,", "NN", "NN", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Ergreife die Fiedel, du lust'ger Spielmann du,", "tokens": ["Er\u00b7grei\u00b7fe", "die", "Fie\u00b7del", ",", "du", "lust'\u00b7ger", "Spiel\u00b7mann", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PPER", "ADJA", "NN", "PPER", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Von meinem Schatz das Liedel sing' ich dazu.\u00ab", "tokens": ["Von", "mei\u00b7nem", "Schatz", "das", "Lie\u00b7del", "sing'", "ich", "da\u00b7zu", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "PPER", "PAV", "$.", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.5": {"line.1": {"text": "Und find' ich keine Herberg', so lieg' ich zu Nacht", "tokens": ["Und", "find'", "ich", "kei\u00b7ne", "Her\u00b7ber\u00b7g'", ",", "so", "lieg'", "ich", "zu", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Wohl unter blauem Himmel, die Sterne halten Wacht:", "tokens": ["Wohl", "un\u00b7ter", "blau\u00b7em", "Him\u00b7mel", ",", "die", "Ster\u00b7ne", "hal\u00b7ten", "Wacht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Im Winde die Linde, die rauscht mich ein gemach,", "tokens": ["Im", "Win\u00b7de", "die", "Lin\u00b7de", ",", "die", "rauscht", "mich", "ein", "ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NE", "$,", "PRELS", "VVFIN", "PPER", "ART", "ADV", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Es k\u00fcsset in der Fr\u00fch' das Morgenrot mich wach.", "tokens": ["Es", "k\u00fcs\u00b7set", "in", "der", "Fr\u00fch'", "das", "Mor\u00b7gen\u00b7rot", "mich", "wach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "O Wandern, o Wandern, du freie Burschenlust!", "tokens": ["O", "Wan\u00b7dern", ",", "o", "Wan\u00b7dern", ",", "du", "frei\u00b7e", "Bur\u00b7schen\u00b7lust", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da wehet Gottes Odem so frisch in die Brust;", "tokens": ["Da", "we\u00b7het", "Got\u00b7tes", "O\u00b7dem", "so", "frisch", "in", "die", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NN", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da singet und jauchzet das Herz zum Himmelszelt:", "tokens": ["Da", "sin\u00b7get", "und", "jauch\u00b7zet", "das", "Herz", "zum", "Him\u00b7mels\u00b7zelt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wie bist du doch so sch\u00f6n, o du weite, weite Welt!", "tokens": ["Wie", "bist", "du", "doch", "so", "sch\u00f6n", ",", "o", "du", "wei\u00b7te", ",", "wei\u00b7te", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,", "FM", "PPER", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}