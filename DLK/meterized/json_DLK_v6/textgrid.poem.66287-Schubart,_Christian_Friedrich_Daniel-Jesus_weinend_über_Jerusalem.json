{"textgrid.poem.66287": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Jesus weinend \u00fcber Jerusalem", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wen seh ich dort auf deines Oelbergs H\u00f6hen,", "tokens": ["Wen", "seh", "ich", "dort", "auf", "dei\u00b7nes", "O\u00b7el\u00b7bergs", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Jerusalem, mit nassen Augen stehen?", "tokens": ["Je\u00b7ru\u00b7sa\u00b7lem", ",", "mit", "nas\u00b7sen", "Au\u00b7gen", "ste\u00b7hen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wie z\u00e4rtlich weint Der Menschenfreund,", "tokens": ["Wie", "z\u00e4rt\u00b7lich", "weint", "Der", "Men\u00b7schen\u00b7freund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil seine Augen Deinen Jammer sehen!", "tokens": ["Weil", "sei\u00b7ne", "Au\u00b7gen", "Dei\u00b7nen", "Jam\u00b7mer", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der stolze Tempel steht in Gluth und Flammen,", "tokens": ["Der", "stol\u00b7ze", "Tem\u00b7pel", "steht", "in", "Gluth", "und", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Wunderwerk der Erde f\u00e4llt zusammen,", "tokens": ["Das", "Wun\u00b7der\u00b7werk", "der", "Er\u00b7de", "f\u00e4llt", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und in der Gluth Zischt Priesterblut,", "tokens": ["Und", "in", "der", "Gluth", "Zischt", "Pries\u00b7ter\u00b7blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Priester, die von Levis Lenden stammen.", "tokens": ["Der", "Pries\u00b7ter", ",", "die", "von", "Le\u00b7vis", "Len\u00b7den", "stam\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die marmornen Pal\u00e4ste st\u00fcrzen nieder,", "tokens": ["Die", "mar\u00b7mor\u00b7nen", "Pa\u00b7l\u00e4s\u00b7te", "st\u00fcr\u00b7zen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und lautes Ach schallt in den Wolken wieder,", "tokens": ["Und", "lau\u00b7tes", "Ach", "schallt", "in", "den", "Wol\u00b7ken", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Mutter fleht, Der Vater sieht", "tokens": ["Die", "Mut\u00b7ter", "fleht", ",", "Der", "Va\u00b7ter", "sieht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am Spie\u00df des R\u00f6mers seiner Kinder Glieder.", "tokens": ["Am", "Spie\u00df", "des", "R\u00f6\u00b7mers", "sei\u00b7ner", "Kin\u00b7der", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbach,\u00ab seufzt der Herr, und seine Thr\u00e4nen flie\u00dfen,", "tokens": ["\u00bb", "ach", ",", "\u00ab", "seufzt", "der", "Herr", ",", "und", "sei\u00b7ne", "Thr\u00e4\u00b7nen", "flie\u00b7\u00dfen", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "$(", "VVFIN", "ART", "NN", "$,", "KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbwillst du dein hartes Herz vor mir verschlie\u00dfen?\u00ab", "tokens": ["\u00bb", "willst", "du", "dein", "har\u00b7tes", "Herz", "vor", "mir", "ver\u00b7schlie\u00b7\u00dfen", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jerusalem, Jerusalem,", "tokens": ["Je\u00b7ru\u00b7sa\u00b7lem", ",", "Je\u00b7ru\u00b7sa\u00b7lem", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach soll ich dich im Staube sehen m\u00fcssen!", "tokens": ["Ach", "soll", "ich", "dich", "im", "Stau\u00b7be", "se\u00b7hen", "m\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VMFIN", "PPER", "PRF", "APPRART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ach Kinder, seht auf Salems schwarzen Steinen", "tokens": ["Ach", "Kin\u00b7der", ",", "seht", "auf", "Sa\u00b7lems", "schwar\u00b7zen", "Stei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$,", "VVFIN", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Geist der Rache f\u00fcrchterlich erscheinen;", "tokens": ["Den", "Geist", "der", "Ra\u00b7che", "f\u00fcrch\u00b7ter\u00b7lich", "er\u00b7schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ach, zwinget nicht Das Angesicht", "tokens": ["Ach", ",", "zwin\u00b7get", "nicht", "Das", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des G\u00f6ttlichen auch \u00fcber euch zu weinen!", "tokens": ["Des", "G\u00f6tt\u00b7li\u00b7chen", "auch", "\u00fc\u00b7ber", "euch", "zu", "wei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}