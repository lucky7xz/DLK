{"textgrid.poem.41416": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Versprechen machet Schuld; drum send' ich dir die Zeilen,", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Versprechen machet Schuld; drum send' ich dir die Zeilen,", "tokens": ["Ver\u00b7spre\u00b7chen", "ma\u00b7chet", "Schuld", ";", "drum", "send'", "ich", "dir", "die", "Zei\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$.", "PAV", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die meine Dichterei zu deiner Lust entwarf.", "tokens": ["Die", "mei\u00b7ne", "Dich\u00b7te\u00b7rei", "zu", "dei\u00b7ner", "Lust", "ent\u00b7warf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Daf\u00fcr entdecke mir: Ob sich ein Kranker heilen,", "tokens": ["Da\u00b7f\u00fcr", "ent\u00b7de\u00b7cke", "mir", ":", "Ob", "sich", "ein", "Kran\u00b7ker", "hei\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$.", "KOUS", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und dem besorgten Arzt die M\u00fch' erleichtern darf?", "tokens": ["Und", "dem", "be\u00b7sorg\u00b7ten", "Arzt", "die", "M\u00fch'", "er\u00b7leich\u00b7tern", "darf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Freund, dem des Himmels Huld die schwere Kunst zu scherzen,", "tokens": ["Freund", ",", "dem", "des", "Him\u00b7mels", "Huld", "die", "schwe\u00b7re", "Kunst", "zu", "scher\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Die Ort und H\u00f6rer w\u00e4hlt, die Zeit und Stunde kennt,", "tokens": ["Die", "Ort", "und", "H\u00f6\u00b7rer", "w\u00e4hlt", ",", "die", "Zeit", "und", "Stun\u00b7de", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und die Gef\u00e4lligkeit, das Vorrecht edler Herzen,", "tokens": ["Und", "die", "Ge\u00b7f\u00e4l\u00b7lig\u00b7keit", ",", "das", "Vor\u00b7recht", "ed\u00b7ler", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wahre Tugenden ohn' eitlen Schein geg\u00f6nnt;", "tokens": ["Und", "wah\u00b7re", "Tu\u00b7gen\u00b7den", "ohn'", "eit\u00b7len", "Schein", "ge\u00b7g\u00f6nnt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jetzt r\u00fchm' ich nicht in dir dein hilferb\u00f6tig Wissen,", "tokens": ["Jetzt", "r\u00fchm'", "ich", "nicht", "in", "dir", "dein", "hil\u00b7fer\u00b7b\u00f6\u00b7tig", "Wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "APPR", "PPER", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die kluge Fertigkeit, die Treue deiner Hand.", "tokens": ["Die", "klu\u00b7ge", "Fer\u00b7tig\u00b7keit", ",", "die", "Treu\u00b7e", "dei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das wird ein andres Blatt mit Dank erheben m\u00fcssen;", "tokens": ["Das", "wird", "ein", "and\u00b7res", "Blatt", "mit", "Dank", "er\u00b7he\u00b7ben", "m\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die\u00df aber macht dir nur den theuren Fritz bekannt.", "tokens": ["Die\u00df", "a\u00b7ber", "macht", "dir", "nur", "den", "theu\u00b7ren", "Fritz", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Fritz war ein guter M\u00f6nch, ein Feind der fr\u00fchen Mette,", "tokens": ["Fritz", "war", "ein", "gu\u00b7ter", "M\u00f6nch", ",", "ein", "Feind", "der", "fr\u00fc\u00b7hen", "Met\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den auch der Bischof nicht an Weisheit \u00fcbertraf.", "tokens": ["Den", "auch", "der", "Bi\u00b7schof", "nicht", "an", "Weis\u00b7heit", "\u00fc\u00b7bert\u00b7raf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Oft schlief er in dem Chor, oft trank er in dem Bette,", "tokens": ["Oft", "schlief", "er", "in", "dem", "Chor", ",", "oft", "trank", "er", "in", "dem", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und schlief auf seinen Trunk, und trank auf seinen Schlaf.", "tokens": ["Und", "schlief", "auf", "sei\u00b7nen", "Trunk", ",", "und", "trank", "auf", "sei\u00b7nen", "Schlaf", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ihn warf zur Sommerszeit ein hitzig Fieber nieder,", "tokens": ["Ihn", "warf", "zur", "Som\u00b7mers\u00b7zeit", "ein", "hit\u00b7zig", "Fie\u00b7ber", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "ADJD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und folterte den Mann auf seinem Polstersitz;", "tokens": ["Und", "fol\u00b7ter\u00b7te", "den", "Mann", "auf", "sei\u00b7nem", "Pols\u00b7ter\u00b7sitz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sogleich besuchten ihn die feisten Ordensbr\u00fcder,", "tokens": ["Sog\u00b7leich", "be\u00b7such\u00b7ten", "ihn", "die", "feis\u00b7ten", "Or\u00b7dens\u00b7br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und alle tr\u00f6steten den matten Bruder Fritz.", "tokens": ["Und", "al\u00b7le", "tr\u00f6s\u00b7te\u00b7ten", "den", "mat\u00b7ten", "Bru\u00b7der", "Fritz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sein Abt, dem, sonder ihn, auch nicht sein Mundwein schmeckte,", "tokens": ["Sein", "Abt", ",", "dem", ",", "son\u00b7der", "ihn", ",", "auch", "nicht", "sein", "Mund\u00b7wein", "schmeck\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "$,", "KON", "PPER", "$,", "ADV", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Weil keiner so im Trunk Bescheid und Wunder that,", "tokens": ["Weil", "kei\u00b7ner", "so", "im", "Trunk", "Be\u00b7scheid", "und", "Wun\u00b7der", "that", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPRART", "NN", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Berief den besten Arzt, dem er die Noth endeckte,", "tokens": ["Be\u00b7rief", "den", "bes\u00b7ten", "Arzt", ",", "dem", "er", "die", "Noth", "en\u00b7deck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Den Segen doppelt gab, und ihn um Hilfe bat.", "tokens": ["Den", "Se\u00b7gen", "dop\u00b7pelt", "gab", ",", "und", "ihn", "um", "Hil\u00b7fe", "bat", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,", "KON", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Er sprach: W\u00e4hlt ein Geschenk aus jenem vollen Kasten,", "tokens": ["Er", "sprach", ":", "W\u00e4hlt", "ein", "Ge\u00b7schenk", "aus", "je\u00b7nem", "vol\u00b7len", "Kas\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "ART", "NN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Nur lindert, kann es sein, des armen Bruders Qual.", "tokens": ["Nur", "lin\u00b7dert", ",", "kann", "es", "sein", ",", "des", "ar\u00b7men", "Bru\u00b7ders", "Qual", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VMFIN", "PPER", "VAINF", "$,", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich bete schon f\u00fcr ihn; ich will auch f\u00fcr ihn fasten,", "tokens": ["Ich", "be\u00b7te", "schon", "f\u00fcr", "ihn", ";", "ich", "will", "auch", "f\u00fcr", "ihn", "fas\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "$.", "PPER", "VMFIN", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und dieses th\u00e4t' ich doch f\u00fcr keinen Cardinal.", "tokens": ["Und", "die\u00b7ses", "th\u00e4t'", "ich", "doch", "f\u00fcr", "kei\u00b7nen", "Car\u00b7di\u00b7nal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der Doctor streichelt sich, und eilt in Fritzens Zelle.", "tokens": ["Der", "Doc\u00b7tor", "strei\u00b7chelt", "sich", ",", "und", "eilt", "in", "Frit\u00b7zens", "Zel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "KON", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da wird des Kranken Harn mit stummem Ernst besehn;", "tokens": ["Da", "wird", "des", "Kran\u00b7ken", "Harn", "mit", "stum\u00b7mem", "Ernst", "be\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NE", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Er fingert um den Puls, erw\u00e4gt auch alle F\u00e4lle,", "tokens": ["Er", "fin\u00b7gert", "um", "den", "Puls", ",", "er\u00b7w\u00e4gt", "auch", "al\u00b7le", "F\u00e4l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die theils vorhanden sind, theils zu bef\u00fcrchten stehn.", "tokens": ["Die", "theils", "vor\u00b7han\u00b7den", "sind", ",", "theils", "zu", "be\u00b7f\u00fcrch\u00b7ten", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VAFIN", "$,", "ADV", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Drauf spricht er: Kraft der Kunst, die ich, als Arzt, besitze,", "tokens": ["Drauf", "spricht", "er", ":", "Kraft", "der", "Kunst", ",", "die", "ich", ",", "als", "Arzt", ",", "be\u00b7sit\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$.", "NN", "ART", "NN", "$,", "PRELS", "PPER", "$,", "KOUS", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Bemerk' ich hier den Durst, ein Zeichen b\u00f6ser Art;", "tokens": ["Be\u00b7merk'", "ich", "hier", "den", "Durst", ",", "ein", "Zei\u00b7chen", "b\u00f6\u00b7ser", "Art", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So find' ich, zweitens, auch den h\u00f6chsten Grad der Hitze,", "tokens": ["So", "find'", "ich", ",", "zwei\u00b7tens", ",", "auch", "den", "h\u00f6chs\u00b7ten", "Grad", "der", "Hit\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "$,", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und die beschleunigt oft der Frommen Himmelfahrt.", "tokens": ["Und", "die", "be\u00b7schleu\u00b7nigt", "oft", "der", "From\u00b7men", "Him\u00b7mel\u00b7fahrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und dem Hippocrates getreulich nachzuleben,", "tokens": ["Und", "dem", "Hip\u00b7po\u00b7cra\u00b7tes", "ge\u00b7treu\u00b7lich", "nach\u00b7zu\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVIZU", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.26": {"text": "Mu\u00df keine Neuerung die Heilungskunst entweihn.", "tokens": ["Mu\u00df", "kei\u00b7ne", "Neu\u00b7e\u00b7rung", "die", "Hei\u00b7lungs\u00b7kunst", "ent\u00b7weihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.27": {"text": "Er hei\u00dft uns erst den Durst, und dann das Fieber heben;", "tokens": ["Er", "hei\u00dft", "uns", "erst", "den", "Durst", ",", "und", "dann", "das", "Fie\u00b7ber", "he\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$,", "KON", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und folglich wird der Durst mein erster Vorwurf sein,", "tokens": [