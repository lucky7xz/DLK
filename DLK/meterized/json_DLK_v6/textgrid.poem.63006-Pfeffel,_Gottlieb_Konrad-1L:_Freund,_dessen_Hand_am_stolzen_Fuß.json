{"textgrid.poem.63006": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freund, dessen Hand am stolzen Fu\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund, dessen Hand am stolzen Fu\u00df", "tokens": ["Freund", ",", "des\u00b7sen", "Hand", "am", "stol\u00b7zen", "Fu\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELAT", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des traubenreichen Vogesus", "tokens": ["Des", "trau\u00b7ben\u00b7rei\u00b7chen", "Vo\u00b7ge\u00b7sus"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Segel durch die Wolken f\u00fchrte;", "tokens": ["Ein", "Se\u00b7gel", "durch", "die", "Wol\u00b7ken", "f\u00fchr\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als man mit skoptischem Geschrey", "tokens": ["Als", "man", "mit", "skop\u00b7ti\u00b7schem", "Ge\u00b7schrey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Am Harz noch immer demonstrierte,", "tokens": ["Am", "Harz", "noch", "im\u00b7mer", "de\u00b7monst\u00b7rier\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Da\u00df diese Fahrt unm\u00f6glich sey:", "tokens": ["Da\u00df", "die\u00b7se", "Fahrt", "un\u00b7m\u00f6g\u00b7lich", "sey", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "La\u00df dir von meiner Muse sagen,", "tokens": ["La\u00df", "dir", "von", "mei\u00b7ner", "Mu\u00b7se", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Was mit dem Sch\u00f6ps sich zugetragen,", "tokens": ["Was", "mit", "dem", "Sch\u00f6ps", "sich", "zu\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Den Mongolfier auf gutes Gl\u00fcck", "tokens": ["Den", "Mon\u00b7gol\u00b7fier", "auf", "gu\u00b7tes", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Zu seinem Luftkundschafter machte,", "tokens": ["Zu", "sei\u00b7nem", "Luft\u00b7kund\u00b7schaf\u00b7ter", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und den sein g\u00fcnstiges Geschick", "tokens": ["Und", "den", "sein", "g\u00fcns\u00b7ti\u00b7ges", "Ge\u00b7schick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Gesund aufs Land zur\u00fccke brachte.", "tokens": ["Ge\u00b7sund", "aufs", "Land", "zu\u00b7r\u00fc\u00b7cke", "brach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Zwar hat schon Fama dich belehrt,", "tokens": ["Zwar", "hat", "schon", "Fa\u00b7ma", "dich", "be\u00b7lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Da\u00df Ludwigs huldgewohnte H\u00e4nde", "tokens": ["Da\u00df", "Lud\u00b7wigs", "huld\u00b7ge\u00b7wohn\u00b7te", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Mit einer stattlichen Pr\u00e4bende", "tokens": ["Mit", "ei\u00b7ner", "statt\u00b7li\u00b7chen", "Pr\u00e4\u00b7ben\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "So fort den neuen Cook beehrt:", "tokens": ["So", "fort", "den", "neu\u00b7en", "Cook", "be\u00b7ehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Doch dieses, Freund, blieb dir verborgen,", "tokens": ["Doch", "die\u00b7ses", ",", "Freund", ",", "blieb", "dir", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "NN", "$,", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Da\u00df ihm ein Leu entgegen kam,", "tokens": ["Da\u00df", "ihm", "ein", "Leu", "ent\u00b7ge\u00b7gen", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPO", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Als er an einem sch\u00f6nen Morgen", "tokens": ["Als", "er", "an", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Besitz von seiner Stelle nahm.", "tokens": ["Be\u00b7sitz", "von", "sei\u00b7ner", "Stel\u00b7le", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Was will der Sch\u00f6ps im K\u00f6nigsgarten?", "tokens": ["Was", "will", "der", "Sch\u00f6ps", "im", "K\u00f6\u00b7nigs\u00b7gar\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Rief der Numide, fort mit dir,", "tokens": ["Rief", "der", "Nu\u00b7mi\u00b7de", ",", "fort", "mit", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PTKVZ", "APPR", "PPER", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.23": {"text": "Alltagsgesch\u00f6pf! Sonst wird dich hier", "tokens": ["All\u00b7tags\u00b7ge\u00b7sch\u00f6pf", "!", "Sonst", "wird", "dich", "hier"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Der Tod des Marsias erwarten.", "tokens": ["Der", "Tod", "des", "Mar\u00b7si\u00b7as", "er\u00b7war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Nun, nun, bl\u00f6ckt ihm der Hammel zu,", "tokens": ["Nun", ",", "nun", ",", "bl\u00f6ckt", "ihm", "der", "Ham\u00b7mel", "zu", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Mein guter Freund, h\u00f6r auf zu schnarchen,", "tokens": ["Mein", "gu\u00b7ter", "Freund", ",", "h\u00f6r", "auf", "zu", "schnar\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Ich bin ein seltners Thier als du", "tokens": ["Ich", "bin", "ein", "selt\u00b7ners", "Thier", "als", "du"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "KOUS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Und auch ein Pfr\u00fcndner des Monarchen.", "tokens": ["Und", "auch", "ein", "Pfr\u00fcnd\u00b7ner", "des", "Mon\u00b7ar\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Du siehst, hier hob er seinen Ton,", "tokens": ["Du", "siehst", ",", "hier", "hob", "er", "sei\u00b7nen", "Ton", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Den ersten Sch\u00f6ps, der durch die L\u00fcfte", "tokens": ["Den", "ers\u00b7ten", "Sch\u00f6ps", ",", "der", "durch", "die", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Mit einer zwilchnen Gondel schiffte", "tokens": ["Mit", "ei\u00b7ner", "zwilch\u00b7nen", "Gon\u00b7del", "schiff\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Und kecker als Bellerophon", "tokens": ["Und", "ke\u00b7cker", "als", "Bel\u00b7le\u00b7ro\u00b7phon"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "NE"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.33": {"text": "Sich bis ins Reich der Sylphen wagte.", "tokens": ["Sich", "bis", "ins", "Reich", "der", "Syl\u00b7phen", "wag\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Vergebung, sprach der Leu, man sagte", "tokens": ["Ver\u00b7ge\u00b7bung", ",", "sprach", "der", "Leu", ",", "man", "sag\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$,", "PIS", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Uns viel von deinem Ritterzug:", "tokens": ["Uns", "viel", "von", "dei\u00b7nem", "Rit\u00b7ter\u00b7zug", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Allein was hast du dort gesehen?", "tokens": ["Al\u00b7lein", "was", "hast", "du", "dort", "ge\u00b7se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Gesehen, Freund? In solchen H\u00f6hen", "tokens": ["Ge\u00b7se\u00b7hen", ",", "Freund", "?", "In", "sol\u00b7chen", "H\u00f6\u00b7hen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$.", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Sieht unser einer nichts. Genug,", "tokens": ["Sieht", "un\u00b7ser", "ei\u00b7ner", "nichts", ".", "Ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ART", "PIS", "$.", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Da\u00df ich das Wolkenmeer durchkreutzet,", "tokens": ["Da\u00df", "ich", "das", "Wol\u00b7ken\u00b7meer", "durch\u00b7kreut\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Und da\u00df mein gro\u00dfes Beyspiel nun", "tokens": ["Und", "da\u00df", "mein", "gro\u00b7\u00dfes", "Bey\u00b7spiel", "nun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Die feigen Philosophen reitzet,", "tokens": ["Die", "fei\u00b7gen", "Phi\u00b7lo\u00b7so\u00b7phen", "reit\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Den k\u00fchnen Schritt mir nachzuthun.", "tokens": ["Den", "k\u00fch\u00b7nen", "Schritt", "mir", "nach\u00b7zu\u00b7thun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Kaum kam ich von der Fahrt zur\u00fccke,", "tokens": ["Kaum", "kam", "ich", "von", "der", "Fahrt", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "So drangen sie mit gleichem Gl\u00fccke", "tokens": ["So", "dran\u00b7gen", "sie", "mit", "glei\u00b7chem", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "In einem fliegenden Gezelt", "tokens": ["In", "ei\u00b7nem", "flie\u00b7gen\u00b7den", "Ge\u00b7zelt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Durch des Olymps saphirne Schranken", "tokens": ["Durch", "des", "O\u00b7lymps", "sa\u00b7phir\u00b7ne", "Schran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Ins Heiligthum der Oberwelt:", "tokens": ["Ins", "Hei\u00b7lig\u00b7thum", "der", "O\u00b7ber\u00b7welt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Und alles das, so schlo\u00df der Held,", "tokens": ["Und", "al\u00b7les", "das", ",", "so", "schlo\u00df", "der", "Held", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PDS", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Hat doch die Menschheit mir zu danken!", "tokens": ["Hat", "doch", "die", "Menschheit", "mir", "zu", "dan\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.50": {"text": "Gut, sprach der Thiere Gro\u00dfsultan,", "tokens": ["Gut", ",", "sprach", "der", "Thie\u00b7re", "Gro\u00df\u00b7sul\u00b7tan", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Das will ich dir nicht widersprechen:", "tokens": ["Das", "will", "ich", "dir", "nicht", "wi\u00b7der\u00b7spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Oft schickt man einen Sch\u00f6ps voran,", "tokens": ["Oft", "schickt", "man", "ei\u00b7nen", "Sch\u00f6ps", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Dem klugen Kopf die Bahn zu brechen.", "tokens": ["Dem", "klu\u00b7gen", "Kopf", "die", "Bahn", "zu", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}