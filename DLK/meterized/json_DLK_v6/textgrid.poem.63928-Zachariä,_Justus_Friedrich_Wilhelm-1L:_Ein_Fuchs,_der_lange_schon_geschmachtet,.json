{"textgrid.poem.63928": {"metadata": {"author": {"name": "Zachari\u00e4, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Fuchs, der lange schon geschmachtet,", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Fuchs, der lange schon geschmachtet,", "tokens": ["Ein", "Fuchs", ",", "der", "lan\u00b7ge", "schon", "ge\u00b7schmach\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Umsonst nach manchem Huhn getrachtet,", "tokens": ["Um\u00b7sonst", "nach", "man\u00b7chem", "Huhn", "ge\u00b7trach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erfuhr, da\u00df eines Affen Frau", "tokens": ["Er\u00b7fuhr", ",", "da\u00df", "ei\u00b7nes", "Af\u00b7fen", "Frau"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Kindbette sey. Ha! (dacht' er schlau)", "tokens": ["Im", "Kind\u00b7bet\u00b7te", "sey", ".", "Ha", "!", "(", "dacht'", "er", "schlau", ")"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "$.", "ITJ", "$.", "$(", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+--++--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Vielleicht trag' ich vom Wochenschmaus", "tokens": ["Viel\u00b7leicht", "trag'", "ich", "vom", "Wo\u00b7chen\u00b7schmaus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auch wohl ein fettes Maul nach Haus,", "tokens": ["Auch", "wohl", "ein", "fet\u00b7tes", "Maul", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Man mu\u00df sich in die Zeiten schicken,", "tokens": ["Man", "mu\u00df", "sich", "in", "die", "Zei\u00b7ten", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gef\u00e4llig seyn und viel sich b\u00fccken:", "tokens": ["Ge\u00b7f\u00e4l\u00b7lig", "seyn", "und", "viel", "sich", "b\u00fc\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "KON", "ADV", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sonst bleibet B\u00f6rs' und Magen schlapp.", "tokens": ["Sonst", "blei\u00b7bet", "B\u00f6r\u00b7s'", "und", "Ma\u00b7gen", "schlapp", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "KON", "NN", "ADJD", "$."], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.10": {"text": "So dacht' er bei sich und begab", "tokens": ["So", "dacht'", "er", "bei", "sich", "und", "be\u00b7gab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PRF", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sich auf den Weg zum Aufenthalt", "tokens": ["Sich", "auf", "den", "Weg", "zum", "Auf\u00b7ent\u00b7halt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der Affen in den n\u00e4chsten Wald.", "tokens": ["Der", "Af\u00b7fen", "in", "den", "n\u00e4chs\u00b7ten", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Er fand die W\u00f6chnerin im Lager", "tokens": ["Er", "fand", "die", "W\u00f6ch\u00b7ne\u00b7rin", "im", "La\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Aus weichem Heu; von Mann und Schwager", "tokens": ["Aus", "wei\u00b7chem", "Heu", ";", "von", "Mann", "und", "Schwa\u00b7ger"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und Muhm' und Schwiegerin umringt,", "tokens": ["Und", "Muhm'", "und", "Schwie\u00b7ge\u00b7rin", "um\u00b7ringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Von denen jeder etwas bringt,", "tokens": ["Von", "de\u00b7nen", "je\u00b7der", "et\u00b7was", "bringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIAT", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "So da\u00df Herr Reineke, gar klug,", "tokens": ["So", "da\u00df", "Herr", "Rei\u00b7ne\u00b7ke", ",", "gar", "klug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "NE", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Bemerkte, hier sey Schmaus genug.", "tokens": ["Be\u00b7merk\u00b7te", ",", "hier", "sey", "Schmaus", "ge\u00b7nug", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VAFIN", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Er machte sich deshalb gar zierlich", "tokens": ["Er", "mach\u00b7te", "sich", "des\u00b7halb", "gar", "zier\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PAV", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Zur Affin, b\u00fcckte sich manierlich", "tokens": ["Zur", "Af\u00b7fin", ",", "b\u00fcck\u00b7te", "sich", "ma\u00b7nier\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "VVFIN", "PRF", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Bis auf die Schuh, und sprach: Madam,", "tokens": ["Bis", "auf", "die", "Schuh", ",", "und", "sprach", ":", "Ma\u00b7dam", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "$.", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Da\u00df ich den Weg zu Ihnen nahm,", "tokens": ["Da\u00df", "ich", "den", "Weg", "zu", "Ih\u00b7nen", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Ist aus der Ursach' blos geschehn,", "tokens": ["Ist", "aus", "der", "Ur\u00b7sach'", "blos", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Die sch\u00f6nen Kinderchen zu sehn,", "tokens": ["Die", "sch\u00f6\u00b7nen", "Kin\u00b7der\u00b7chen", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Womit sie vor gar kurzer Zeit", "tokens": ["Wo\u00b7mit", "sie", "vor", "gar", "kur\u00b7zer", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Den werthen Herrn Gemahl erfreut.", "tokens": ["Den", "wert\u00b7hen", "Herrn", "Ge\u00b7mahl", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "O, zeigen Sie doch Ihrem Knecht,", "tokens": ["O", ",", "zei\u00b7gen", "Sie", "doch", "Ih\u00b7rem", "Knecht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Von Ihrem adlichen Geschlecht", "tokens": ["Von", "Ih\u00b7rem", "ad\u00b7li\u00b7chen", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Die beiden liebensw\u00fcrd'gen Zweige,", "tokens": ["Die", "bei\u00b7den", "lie\u00b7bens\u00b7w\u00fcrd'\u00b7gen", "Zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Da\u00df ich mich auch vor ihnen beuge!", "tokens": ["Da\u00df", "ich", "mich", "auch", "vor", "ih\u00b7nen", "beu\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Ihr Gnaden glauben sicherlich,", "tokens": ["Ihr", "Gna\u00b7den", "glau\u00b7ben", "si\u00b7cher\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Ein rechter Kindernarr bin ich!", "tokens": ["Ein", "rech\u00b7ter", "Kin\u00b7der\u00b7narr", "bin", "ich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Als dieses die Frau Affin h\u00f6rt,", "tokens": ["Als", "die\u00b7ses", "die", "Frau", "Af\u00b7fin", "h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.34": {"text": "Gar freundlich sie sich zu ihm kehrt,", "tokens": ["Gar", "freund\u00b7lich", "sie", "sich", "zu", "ihm", "kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Reicht ihm die Hand, und spricht zum Mann:", "tokens": ["Reicht", "ihm", "die", "Hand", ",", "und", "spricht", "zum", "Mann", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.36": {"text": "Sieh' doch den art'gen Fremdling an!", "tokens": ["Sieh'", "doch", "den", "art'\u00b7gen", "Fremd\u00b7ling", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Er k\u00f6mmt hieher mit m\u00fcden F\u00fc\u00dfen,", "tokens": ["Er", "k\u00f6mmt", "hie\u00b7her", "mit", "m\u00fc\u00b7den", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Blos, unsre Kinderchen zu k\u00fcssen.", "tokens": ["Blos", ",", "uns\u00b7re", "Kin\u00b7der\u00b7chen", "zu", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "++-+---+-", "measure": "unknown.measure.tetra"}, "line.39": {"text": "Hier, Freund, (sprach sie zum Fuchs) im Heu", "tokens": ["Hier", ",", "Freund", ",", "(", "sprach", "sie", "zum", "Fuchs", ")", "im", "Heu"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "NN", "$,", "$(", "VVFIN", "PPER", "APPRART", "NE", "$(", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Ruhn sie im Schlummer alle zwei.", "tokens": ["Ruhn", "sie", "im", "Schlum\u00b7mer", "al\u00b7le", "zwei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PIAT", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Sie sagt' es, und zog mit der Hand", "tokens": ["Sie", "sagt'", "es", ",", "und", "zog", "mit", "der", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.42": {"text": "Ein pelzgef\u00fcttertes Gewand", "tokens": ["Ein", "pelz\u00b7ge\u00b7f\u00fct\u00b7ter\u00b7tes", "Ge\u00b7wand"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Hinweg von ihrem Zwillingspaar,", "tokens": ["Hin\u00b7weg", "von", "ih\u00b7rem", "Zwil\u00b7lings\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Und sprach zum Fuchs: Mein Herr, nicht wahr,", "tokens": ["Und", "sprach", "zum", "Fuchs", ":", "Mein", "Herr", ",", "nicht", "wahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NE", "$.", "PPOSAT", "NN", "$,", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Wenn Ihr's aufrichtig wollt gestehn,", "tokens": ["Wenn", "Ih\u00b7r's", "auf\u00b7rich\u00b7tig", "wollt", "ge\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VMFIN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.46": {"text": "Was Sch\u00f6ner's habt Ihr nie gesehn?", "tokens": ["Was", "Sch\u00f6\u00b7ner's", "habt", "Ihr", "nie", "ge\u00b7sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Der Fuchs erschrak. In langer Zeit", "tokens": ["Der", "Fuchs", "er\u00b7schrak", ".", "In", "lan\u00b7ger", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Hatt' er nicht so viel H\u00e4\u00dflichkeit", "tokens": ["Hatt'", "er", "nicht", "so", "viel", "H\u00e4\u00df\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "PIAT", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.49": {"text": "An irgend einem Thier erblickt;", "tokens": ["An", "ir\u00b7gend", "ei\u00b7nem", "Thier", "er\u00b7blickt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Doch rief er listig, wie entz\u00fcckt:", "tokens": ["Doch", "rief", "er", "lis\u00b7tig", ",", "wie", "ent\u00b7z\u00fcckt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,", "PWAV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "O, froher Tag! So seh' ich denn", "tokens": ["O", ",", "fro\u00b7her", "Tag", "!", "So", "seh'", "ich", "denn"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Die beiden kleinen Engelchen", "tokens": ["Die", "bei\u00b7den", "klei\u00b7nen", "En\u00b7gel\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIAT", "ADJA", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.53": {"text": "In jedem Liebreiz vor mir liegen?", "tokens": ["In", "je\u00b7dem", "Lieb\u00b7reiz", "vor", "mir", "lie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "O, welche Freude, welch' Vergn\u00fcgen", "tokens": ["O", ",", "wel\u00b7che", "Freu\u00b7de", ",", "welch'", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "PWAT", "NN", "$,", "PWAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.55": {"text": "Mu\u00df dies den hohen Eltern seyn!", "tokens": ["Mu\u00df", "dies", "den", "ho\u00b7hen", "El\u00b7tern", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.56": {"text": "F\u00fcrwahr, trifft mein Vermuthen ein,", "tokens": ["F\u00fcr\u00b7wahr", ",", "trifft", "mein", "Ver\u00b7mu\u00b7then", "ein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.57": {"text": "So werden sie, das ahnet mir,", "tokens": ["So", "wer\u00b7den", "sie", ",", "das", "ah\u00b7net", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PDS", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.58": {"text": "Die Lust der Welt, der Affen Zier.", "tokens": ["Die", "Lust", "der", "Welt", ",", "der", "Af\u00b7fen", "Zier", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Als dieses die Frau Affin h\u00f6rt,", "tokens": ["Als", "die\u00b7ses", "die", "Frau", "Af\u00b7fin", "h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ward sie von Freude ganz beth\u00f6rt,", "tokens": ["Ward", "sie", "von", "Freu\u00b7de", "ganz", "be\u00b7th\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie ihr Herr Eh'mann ebenfalls.", "tokens": ["Wie", "ihr", "Herr", "Eh'\u00b7mann", "e\u00b7ben\u00b7falls", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NE", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er warf dem Fuchs sich um den Hals", "tokens": ["Er", "warf", "dem", "Fuchs", "sich", "um", "den", "Hals"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NE", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bat ihn auf's Freundlichste zu Tische,", "tokens": ["Bat", "ihn", "auf's", "Freund\u00b7lichs\u00b7te", "zu", "Ti\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Trug auf Pasteten, Braten, Fische,", "tokens": ["Trug", "auf", "Pas\u00b7te\u00b7ten", ",", "Bra\u00b7ten", ",", "Fi\u00b7sche", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Viel Obst, und N\u00fcsse gro\u00df und klein,", "tokens": ["Viel", "Obst", ",", "und", "N\u00fcs\u00b7se", "gro\u00df", "und", "klein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KON", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und trank ihm zu vom besten Wein;", "tokens": ["Und", "trank", "ihm", "zu", "vom", "bes\u00b7ten", "Wein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "So da\u00df der Fuchs, sehr wohl gespeist", "tokens": ["So", "da\u00df", "der", "Fuchs", ",", "sehr", "wohl", "ge\u00b7speist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NE", "$,", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und halb berauscht, von dannen reist.", "tokens": ["Und", "halb", "be\u00b7rauscht", ",", "von", "dan\u00b7nen", "reist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$,", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein Wolf traf auf dem Weg ihn an", "tokens": ["Ein", "Wolf", "traf", "auf", "dem", "Weg", "ihn", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "APPR", "ART", "NN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sprach zu ihm: Mein lieber Mann,", "tokens": ["Und", "sprach", "zu", "ihm", ":", "Mein", "lie\u00b7ber", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich seh' an deinem vollen Bauch,", "tokens": ["Ich", "seh'", "an", "dei\u00b7nem", "vol\u00b7len", "Bauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du hast geschmaust. K\u00f6nnt' ich nicht auch", "tokens": ["Du", "hast", "ge\u00b7schmaust", ".", "K\u00f6nnt'", "ich", "nicht", "auch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "VMFIN", "PPER", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu einem solchen Fest gelangen,", "tokens": ["Zu", "ei\u00b7nem", "sol\u00b7chen", "Fest", "ge\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bei dem es dir so wohl gegangen?", "tokens": ["Bei", "dem", "es", "dir", "so", "wohl", "ge\u00b7gan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "I, Freund! (versetzt der Fuchs) gar leicht", "tokens": ["I", ",", "Freund", "!", "(", "ver\u00b7setzt", "der", "Fuchs", ")", "gar", "leicht"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["XY", "$,", "NN", "$.", "$(", "VVFIN", "ART", "NE", "$(", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wird dieser Wunsch von dir erreicht.", "tokens": ["Wird", "die\u00b7ser", "Wunsch", "von", "dir", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Des Affen Frau liegt in den Wochen,", "tokens": ["Des", "Af\u00b7fen", "Frau", "liegt", "in", "den", "Wo\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der hab' ich eben zugesprochen,", "tokens": ["Der", "hab'", "ich", "e\u00b7ben", "zu\u00b7ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Sie hat zwei allerliebste Kinder,", "tokens": ["Sie", "hat", "zwei", "al\u00b7ler\u00b7liebs\u00b7te", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Sie zeigt sie gern; wo du nicht minder,", "tokens": ["Sie", "zeigt", "sie", "gern", ";", "wo", "du", "nicht", "min\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "PWAV", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Als ich, sie lobst, so gibt sie dir", "tokens": ["Als", "ich", ",", "sie", "lobst", ",", "so", "gibt", "sie", "dir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Mit Dank zu schmausen g'nug daf\u00fcr.", "tokens": ["Mit", "Dank", "zu", "schmau\u00b7sen", "g'\u00b7nug", "da\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "VVFIN", "PAV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "So? (sprach der Wolf) brauch' ich nur dies,", "tokens": ["So", "?", "(", "sprach", "der", "Wolf", ")", "brauch'", "ich", "nur", "dies", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "$(", "VVFIN", "ART", "NE", "$(", "VVFIN", "PPER", "ADV", "PDS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann hab' ich meinen Fra\u00df gewi\u00df!", "tokens": ["Dann", "hab'", "ich", "mei\u00b7nen", "Fra\u00df", "ge\u00b7wi\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er eilte zu den Affen hin,", "tokens": ["Er", "eil\u00b7te", "zu", "den", "Af\u00b7fen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und traf sie an bei frohem Sinn;", "tokens": ["Und", "traf", "sie", "an", "bei", "fro\u00b7hem", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ward freundlich von dem Mann empfangen,", "tokens": ["Ward", "freund\u00b7lich", "von", "dem", "Mann", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nach seinem h\u00f6flichen Verlangen", "tokens": ["Nach", "sei\u00b7nem", "h\u00f6f\u00b7li\u00b7chen", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zur Frau gef\u00fchrt, die, sehr geneigt,", "tokens": ["Zur", "Frau", "ge\u00b7f\u00fchrt", ",", "die", ",", "sehr", "ge\u00b7neigt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "PRELS", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ihm alsobald die Kleinen zeigt.", "tokens": ["Ihm", "al\u00b7so\u00b7bald", "die", "Klei\u00b7nen", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Herr Eisengrimm mit starrem Blick", "tokens": ["Herr", "Ei\u00b7sen\u00b7grimm", "mit", "star\u00b7rem", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NE", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Fuhr ganz erstaunensvoll zur\u00fcck.", "tokens": ["Fuhr", "ganz", "er\u00b7stau\u00b7nens\u00b7voll", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Was Kuckuk! (schrie und lacht' er laut)", "tokens": ["Was", "Kuc\u00b7kuk", "!", "(", "schrie", "und", "lacht'", "er", "laut", ")"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$.", "$(", "VVFIN", "KON", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Hier schaudert einem fast die Haut!", "tokens": ["Hier", "schau\u00b7dert", "ei\u00b7nem", "fast", "die", "Haut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Dies sind ja wahre Ungeheuer!", "tokens": ["Dies", "sind", "ja", "wah\u00b7re", "Un\u00b7ge\u00b7heu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und die Scheus\u00e4ligen sind euer?", "tokens": ["Und", "die", "Scheu\u00b7s\u00e4\u00b7li\u00b7gen", "sind", "eu\u00b7er", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPOSAT", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.15": {"text": "Ei! schrien die Affen allesammt,", "tokens": ["Ei", "!", "schri\u00b7en", "die", "Af\u00b7fen", "al\u00b7le\u00b7sammt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Die Mutter mit, von Wuth entflammt,", "tokens": ["Die", "Mut\u00b7ter", "mit", ",", "von", "Wuth", "ent\u00b7flammt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ei! seht mir doch den Grobian", "tokens": ["Ei", "!", "seht", "mir", "doch", "den", "Gro\u00b7bi\u00b7an"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Mit seinen Schmeicheleien an!", "tokens": ["Mit", "sei\u00b7nen", "Schmei\u00b7che\u00b7lei\u00b7en", "an", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Was braucht er denn hieher zu gehn,", "tokens": ["Was", "braucht", "er", "denn", "hie\u00b7her", "zu", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und unsre Kinderchen zu schm\u00e4hn?", "tokens": ["Und", "uns\u00b7re", "Kin\u00b7der\u00b7chen", "zu", "schm\u00e4hn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "D'rauf griff ein jeder nach dem Knittel,", "tokens": ["D'\u00b7rauf", "griff", "ein", "je\u00b7der", "nach", "dem", "Knit\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Durchklopften weidlich ihm den Kittel,", "tokens": ["Durch\u00b7klopf\u00b7ten", "weid\u00b7lich", "ihm", "den", "Kit\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Da\u00df er, an allen Vieren lahm,", "tokens": ["Da\u00df", "er", ",", "an", "al\u00b7len", "Vie\u00b7ren", "lahm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Zum Fuchse hungrig wieder kam.", "tokens": ["Zum", "Fuch\u00b7se", "hung\u00b7rig", "wie\u00b7der", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Sobald Herr Reineke vernommen,", "tokens": ["So\u00b7bald", "Herr", "Rei\u00b7ne\u00b7ke", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Wie schlecht der Wolf davon gekommen,", "tokens": ["Wie", "schlecht", "der", "Wolf", "da\u00b7von", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NE", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Sprach er: Ihr gebt mir wohl nicht Recht!", "tokens": ["Sprach", "er", ":", "Ihr", "gebt", "mir", "wohl", "nicht", "Recht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Allein Ihr kennt die Welt noch schlecht.", "tokens": ["Al\u00b7lein", "Ihr", "kennt", "die", "Welt", "noch", "schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Gern h\u00e4lt das Ohr dem Schmeichler still;", "tokens": ["Gern", "h\u00e4lt", "das", "Ohr", "dem", "Schmeich\u00b7ler", "still", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Die Wahrheit Niemand h\u00f6ren will.", "tokens": ["Die", "Wahr\u00b7heit", "Nie\u00b7mand", "h\u00f6\u00b7ren", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dies hab' ich, Leser, auch gedacht,", "tokens": ["Dies", "hab'", "ich", ",", "Le\u00b7ser", ",", "auch", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure"