{"dta.poem.5811": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Gesetzen der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein jeder C\u00f6rper hat", "tokens": ["Ein", "je\u00b7der", "C\u00f6r\u00b7per", "hat"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Bewegung und auch Ruh in einem gleichen Grad\u2019:", "tokens": ["Be\u00b7we\u00b7gung", "und", "auch", "Ruh", "in", "ei\u00b7nem", "glei\u00b7chen", "Grad'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So wie ein C\u00f6rper stets der ruht, in Ruhe bleibet,", "tokens": ["So", "wie", "ein", "C\u00f6r\u00b7per", "stets", "der", "ruht", ",", "in", "Ru\u00b7he", "blei\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "ADV", "ART", "VVFIN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo ihn ein andrer nicht aus seiner Ruhe treibet;", "tokens": ["Wo", "ihn", "ein", "an\u00b7drer", "nicht", "aus", "sei\u00b7ner", "Ru\u00b7he", "trei\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So m\u00fcssen C\u00f6rper, die sich regen,", "tokens": ["So", "m\u00fcs\u00b7sen", "C\u00f6r\u00b7per", ",", "die", "sich", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NE", "$,", "PRELS", "PRF", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ohn Unterla\u00df sich auch bewegen,", "tokens": ["Ohn", "Un\u00b7ter\u00b7la\u00df", "sich", "auch", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wo C\u00f6rper ihnen nicht im Lauff entgegen gehn,", "tokens": ["Wo", "C\u00f6r\u00b7per", "ih\u00b7nen", "nicht", "im", "Lauff", "ent\u00b7ge\u00b7gen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "PPER", "PTKNEG", "APPRART", "NN", "PTKVZ", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und ihnen widerstehn.", "tokens": ["Und", "ih\u00b7nen", "wi\u00b7der\u00b7stehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}