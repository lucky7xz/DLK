{"textgrid.poem.58336": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "374. Wie Gott zu finden sei", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Seel', entsink dem eignen Willen,", "tokens": ["O", "Seel'", ",", "ent\u00b7sink", "dem", "eig\u00b7nen", "Wil\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kehr in dich ein und la\u00df dich stillen!", "tokens": ["Kehr", "in", "dich", "ein", "und", "la\u00df", "dich", "stil\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PTKVZ", "KON", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Im Seelengrund ist Gott uns nah,", "tokens": ["Im", "See\u00b7len\u00b7grund", "ist", "Gott", "uns", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer all's verliert, der find't ihn da.", "tokens": ["Wer", "all's", "ver\u00b7liert", ",", "der", "find't", "ihn", "da", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}