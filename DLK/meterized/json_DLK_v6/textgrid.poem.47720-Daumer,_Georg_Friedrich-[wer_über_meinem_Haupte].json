{"textgrid.poem.47720": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wer \u00fcber meinem Haupte]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwer \u00fcber meinem Haupte,", "tokens": ["\u00bb", "wer", "\u00fc\u00b7ber", "mei\u00b7nem", "Haup\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wer weinet und wer schluchzet?", "tokens": ["Wer", "wei\u00b7net", "und", "wer", "schluch\u00b7zet", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "PWS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer sitzet in Verzweiflung,", "tokens": ["Wer", "sit\u00b7zet", "in", "Ver\u00b7zwei\u00b7flung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Ruhe meines Schlummers,", "tokens": ["Die", "Ru\u00b7he", "mei\u00b7nes", "Schlum\u00b7mers", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die tiefe, tiefe st\u00f6rend,", "tokens": ["Die", "tie\u00b7fe", ",", "tie\u00b7fe", "st\u00f6\u00b7rend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auf meinem H\u00fcgel wer?\u00ab \u2013", "tokens": ["Auf", "mei\u00b7nem", "H\u00fc\u00b7gel", "wer", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PWS", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbich, deine Tochter, bin es;", "tokens": ["\u00bb", "ich", ",", "dei\u00b7ne", "Toch\u00b7ter", ",", "bin", "es", ";"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "PPOSAT", "NN", "$,", "VAFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich sitze hier und weine.", "tokens": ["Ich", "sit\u00b7ze", "hier", "und", "wei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie sollt' ich auch nicht weinen?", "tokens": ["Wie", "sollt'", "ich", "auch", "nicht", "wei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich bin allein, alleine,", "tokens": ["Ich", "bin", "al\u00b7lein", ",", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bin ohne Stab und St\u00fctze,", "tokens": ["Bin", "oh\u00b7ne", "Stab", "und", "St\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bin ohne Hab' und Hoffnung,", "tokens": ["Bin", "oh\u00b7ne", "Hab'", "und", "Hoff\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nicht Friede bl\u00fcht und Freude", "tokens": ["Nicht", "Frie\u00b7de", "bl\u00fcht", "und", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "NN", "VVFIN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mir auf der Erde mehr.\u00ab \u2013", "tokens": ["Mir", "auf", "der", "Er\u00b7de", "mehr", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADV", "$.", "$(", "$("], "meter": "++-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbbeschwichte, liebe Tochter,", "tokens": ["\u00bb", "be\u00b7schwich\u00b7te", ",", "lie\u00b7be", "Toch\u00b7ter", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Beschwichte deine Z\u00e4hre", "tokens": ["Be\u00b7schwich\u00b7te", "dei\u00b7ne", "Z\u00e4h\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wandele nach Hause", "tokens": ["Und", "wan\u00b7de\u00b7le", "nach", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit neu belebter Brust!", "tokens": ["Mit", "neu", "be\u00b7leb\u00b7ter", "Brust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es bl\u00fcht ein neues Dasein,", "tokens": ["Es", "bl\u00fcht", "ein", "neu\u00b7es", "Da\u00b7sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dir eine holde Zukunft,", "tokens": ["Dir", "ei\u00b7ne", "hol\u00b7de", "Zu\u00b7kunft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dir s\u00fc\u00dfer Heilbescheere", "tokens": ["Dir", "s\u00fc\u00b7\u00dfer", "Heil\u00b7be\u00b7schee\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Niemals gef\u00fchlte Lust.", "tokens": ["Nie\u00b7mals", "ge\u00b7f\u00fchl\u00b7te", "Lust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "Es harret dein ein Kr\u00e4nzchen,", "tokens": ["Es", "har\u00b7ret", "dein", "ein", "Kr\u00e4nz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Ein br\u00e4utliches, ein stolzes,", "tokens": ["Ein", "br\u00e4ut\u00b7li\u00b7ches", ",", "ein", "stol\u00b7zes", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Dein eine zweite Mutter,", "tokens": ["Dein", "ei\u00b7ne", "zwei\u00b7te", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Die deine Haare str\u00e4hlen", "tokens": ["Die", "dei\u00b7ne", "Haa\u00b7re", "str\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Mit linden H\u00e4nden wird.", "tokens": ["Mit", "lin\u00b7den", "H\u00e4n\u00b7den", "wird", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Es harret dein ein Knabe,", "tokens": ["Es", "har\u00b7ret", "dein", "ein", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Ein lieblicher, ein feiner,", "tokens": ["Ein", "lieb\u00b7li\u00b7cher", ",", "ein", "fei\u00b7ner", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Der zarte Minneworte", "tokens": ["Der", "zar\u00b7te", "Min\u00b7ne\u00b7wor\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Dir in die Seele fl\u00fcstern,", "tokens": ["Dir", "in", "die", "See\u00b7le", "fl\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Der alle deine Klagen", "tokens": ["Der", "al\u00b7le", "dei\u00b7ne", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Holdselig enden wird.", "tokens": ["Hold\u00b7se\u00b7lig", "en\u00b7den", "wird", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VAFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}