{"textgrid.poem.46292": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "An meine dochter F. Elisabeth Trumbull", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Recht sch\u00f6n an geists- und leibsgestalt", "tokens": ["Recht", "sch\u00f6n", "an", "geist\u00b7s", "und", "leibs\u00b7ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPR", "TRUNC", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "bist du ein wunderkind gewesen,", "tokens": ["bist", "du", "ein", "wun\u00b7der\u00b7kind", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "so wol als andre weis und alt,", "tokens": ["so", "wol", "als", "and\u00b7re", "weis", "und", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "PIS", "PTKVZ", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "dreij\u00e4hrig kontest du schon lesen.", "tokens": ["drei\u00b7j\u00e4h\u00b7rig", "kon\u00b7test", "du", "schon", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du hast die sprachen, welche wir", "tokens": ["Du", "hast", "die", "spra\u00b7chen", ",", "wel\u00b7che", "wir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "VVFIN", "$,", "PRELS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mit m\u00fch erlernen, leicht und reichlich,", "tokens": ["mit", "m\u00fch", "er\u00b7ler\u00b7nen", ",", "leicht", "und", "reich\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVINF", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und die ged\u00e4chtnus, welche dir", "tokens": ["und", "die", "ge\u00b7d\u00e4cht\u00b7nus", ",", "wel\u00b7che", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "der himmel gab, ist kaum vergleichlich.", "tokens": ["der", "him\u00b7mel", "gab", ",", "ist", "kaum", "ver\u00b7gleich\u00b7lich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mit gotsforcht, zucht, gehorsam, ehr,", "tokens": ["Mit", "gots\u00b7forcht", ",", "zucht", ",", "ge\u00b7hor\u00b7sam", ",", "ehr", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "$,", "ADJD", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mit flei\u00df und tugend wol gezieret,", "tokens": ["mit", "flei\u00df", "und", "tu\u00b7gend", "wol", "ge\u00b7zie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "bist du der spiegel und die lehr,", "tokens": ["bist", "du", "der", "spie\u00b7gel", "und", "die", "lehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "mit welchem dein geschlecht prachtieret.", "tokens": ["mit", "wel\u00b7chem", "dein", "ge\u00b7schlecht", "prach\u00b7tie\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PPOSAT", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Daher bit ich got, da\u00df die freud,", "tokens": ["Da\u00b7her", "bit", "ich", "got", ",", "da\u00df", "die", "freud", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "ART", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "die deinen eltern du gegeben,", "tokens": ["die", "dei\u00b7nen", "el\u00b7tern", "du", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "du m\u00f6gest ohn verdru\u00df und leid", "tokens": ["du", "m\u00f6\u00b7gest", "ohn", "ver\u00b7dru\u00df", "und", "leid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "an deinen kindern lang erleben!", "tokens": ["an", "dei\u00b7nen", "kin\u00b7dern", "lang", "er\u00b7le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}