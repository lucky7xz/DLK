{"dta.poem.21062": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "An Fanny.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Warum bleichet Kummerbl&#228;sse,               ", "tokens": ["Wa\u00b7rum", "blei\u00b7chet", "Kum\u00b7merbl", "&#228;", "sse", ","], "token_info": ["word", "word", "word", "XML_entity", "word", "punct"], "pos": ["PWAV", "VVFIN", "NE", "$(", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fanny, dein Gesicht?", "tokens": ["Fan\u00b7ny", ",", "dein", "Ge\u00b7sicht", "?"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Warum w\u00f6lket Thr\u00e4nenn\u00e4sse", "tokens": ["Wa\u00b7rum", "w\u00f6l\u00b7ket", "Thr\u00e4\u00b7nen\u00b7n\u00e4s\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deines Auges Licht?", "tokens": ["Dei\u00b7nes", "Au\u00b7ges", "Licht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Um dich l\u00e4rmen laute Freuden.", "tokens": ["Um", "dich", "l\u00e4r\u00b7men", "lau\u00b7te", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber tiefverhohlnes Leiden", "tokens": ["A\u00b7ber", "tief\u00b7ver\u00b7hohl\u00b7nes", "Lei\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nagt an deiner wunden Brust,", "tokens": ["Nagt", "an", "dei\u00b7ner", "wun\u00b7den", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und dir widern Scherz und Lust.", "tokens": ["Und", "dir", "wi\u00b7dern", "Scherz", "und", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Klagst du ob der Guten, Frommen,", "tokens": ["Klagst", "du", "ob", "der", "Gu\u00b7ten", ",", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "ART", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die der Tod dir nahm?", "tokens": ["Die", "der", "Tod", "dir", "nahm", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Klage nicht um sie; entnommen", "tokens": ["Kla\u00b7ge", "nicht", "um", "sie", ";", "ent\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "PTKNEG", "APPR", "PPER", "$.", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist sie jedem Gram.", "tokens": ["Ist", "sie", "je\u00b7dem", "Gram", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Alles Herzeleids entbunden,", "tokens": ["Al\u00b7les", "Her\u00b7ze\u00b7leids", "ent\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jedem Ungemach entwunden,", "tokens": ["Je\u00b7dem", "Un\u00b7ge\u00b7mach", "ent\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ruht sie in der Schwester Arm,", "tokens": ["Ruht", "sie", "in", "der", "Schwes\u00b7ter", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00e4rmt sich nur um deinen Harm.", "tokens": ["H\u00e4rmt", "sich", "nur", "um", "dei\u00b7nen", "Harm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lass sie schlummern ihren Schlummer", "tokens": ["Lass", "sie", "schlum\u00b7mern", "ih\u00b7ren", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach des Tages Last.", "tokens": ["Nach", "des", "Ta\u00b7ges", "Last", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "S\u00fcss behagt nach M\u00fch' und Kummer,", "tokens": ["S\u00fcss", "be\u00b7hagt", "nach", "M\u00fch'", "und", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sanft und s\u00fcss die Rast.", "tokens": ["Sanft", "und", "s\u00fcss", "die", "Rast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wenig waren ihrer Freuden;", "tokens": ["We\u00b7nig", "wa\u00b7ren", "ih\u00b7rer", "Freu\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zahllos waren ihre Leiden.", "tokens": ["Zahl\u00b7los", "wa\u00b7ren", "ih\u00b7re", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schwerermattet sank sie hin,", "tokens": ["Schwe\u00b7rer\u00b7mat\u00b7tet", "sank", "sie", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Eine fromme Dulderin \u2014", "tokens": ["Ei\u00b7ne", "from\u00b7me", "Dul\u00b7de\u00b7rin"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Warum blicken wir so traurig", "tokens": ["Wa\u00b7rum", "bli\u00b7cken", "wir", "so", "trau\u00b7rig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die Nacht hinab?", "tokens": ["In", "die", "Nacht", "hin\u00b7ab", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Warum d\u00fcnkst du uns so schaurig,", "tokens": ["Wa\u00b7rum", "d\u00fcnkst", "du", "uns", "so", "schau\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stilles k\u00fchles Grab?", "tokens": ["Stil\u00b7les", "k\u00fch\u00b7les", "Grab", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Bist du doch die traute Kammer,", "tokens": ["Bist", "du", "doch", "die", "trau\u00b7te", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Welche schwichtigt jeden Jammer,", "tokens": ["Wel\u00b7che", "schwich\u00b7tigt", "je\u00b7den", "Jam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Welche jede Unruh stillt,", "tokens": ["Wel\u00b7che", "je\u00b7de", "Un\u00b7ruh", "stillt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Schonend jede Schuld verh\u00fcllt.", "tokens": ["Scho\u00b7nend", "je\u00b7de", "Schuld", "ver\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Warum macht uns dein Erscheinen,", "tokens": ["Wa\u00b7rum", "macht", "uns", "dein", "Er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tod, so angst und bang?", "tokens": ["Tod", ",", "so", "angst", "und", "bang", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "S\u00e4umst du doch dem Frommen Reinen,", "tokens": ["S\u00e4umst", "du", "doch", "dem", "From\u00b7men", "Rei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Retter, nur zu lang!", "tokens": ["Ret\u00b7ter", ",", "nur", "zu", "lang", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "PTKA", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Heil und Preis dem Abgesandten,", "tokens": ["Heil", "und", "Preis", "dem", "Ab\u00b7ge\u00b7sand\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der den schmachtenden Verbannten", "tokens": ["Der", "den", "schmach\u00b7ten\u00b7den", "Ver\u00b7bann\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ruft aus seines ", "tokens": ["Ruft", "aus", "sei\u00b7nes"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Heim ins s\u00fcsse Vaterland.", "tokens": ["Heim", "ins", "s\u00fcs\u00b7se", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Warum \u00e4ngstigt uns dein Dunkel,", "tokens": ["Wa\u00b7rum", "\u00e4ngs\u00b7tigt", "uns", "dein", "Dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ewigkeit, so sehr!", "tokens": ["E\u00b7wig\u00b7keit", ",", "so", "sehr", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sch\u00f6nrer Welten Sterngefunkel", "tokens": ["Sch\u00f6n\u00b7rer", "Wel\u00b7ten", "Stern\u00b7ge\u00b7fun\u00b7kel"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gl\u00e4nzt durch dich daher!", "tokens": ["Gl\u00e4nzt", "durch", "dich", "da\u00b7her", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "PAV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "B\u00fcrgt der Mond nicht f\u00fcr die Sonne?", "tokens": ["B\u00fcrgt", "der", "Mond", "nicht", "f\u00fcr", "die", "Son\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kr\u00e4nzt nicht Wehmuth sich mit Wonne?", "tokens": ["Kr\u00e4nzt", "nicht", "Weh\u00b7muth", "sich", "mit", "Won\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "NN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Deutet Ahnen nicht auf Schaun,", "tokens": ["Deu\u00b7tet", "Ah\u00b7nen", "nicht", "auf", "Schaun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKNEG", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nicht auf Geister unser Graun?", "tokens": ["Nicht", "auf", "Geis\u00b7ter", "un\u00b7ser", "Graun", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Selig, die im Herrn entschliefen!", "tokens": ["Se\u00b7lig", ",", "die", "im", "Herrn", "ent\u00b7schlie\u00b7fen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Selig, Fanny, sie!", "tokens": ["Se\u00b7lig", ",", "Fan\u00b7ny", ",", "sie", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Die die Engel dir entriefen,", "tokens": ["Die", "die", "En\u00b7gel", "dir", "en\u00b7trie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schau, sie f\u00fchrten sie", "tokens": ["Schau", ",", "sie", "f\u00fchr\u00b7ten", "sie"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Aus den Nebeln in die Klarheit,", "tokens": ["Aus", "den", "Ne\u00b7beln", "in", "die", "Klar\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aus den Tr\u00e4umen in die Wahrheit,", "tokens": ["Aus", "den", "Tr\u00e4u\u00b7men", "in", "die", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Durch das Zweifeln zum Vertraun,", "tokens": ["Durch", "das", "Zwei\u00b7feln", "zum", "Ver\u00b7traun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Durch das Ahnen hin zum Schaun.", "tokens": ["Durch", "das", "Ah\u00b7nen", "hin", "zum", "Schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Darum trockne deine Thr\u00e4nen!", "tokens": ["Da\u00b7rum", "trock\u00b7ne", "dei\u00b7ne", "Thr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heitre deinen Blick!", "tokens": ["Heit\u00b7re", "dei\u00b7nen", "Blick", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Nicht dein Jammern, nicht dein Sehnen", "tokens": ["Nicht", "dein", "Jam\u00b7mern", ",", "nicht", "dein", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "PPOSAT", "NN", "$,", "PTKNEG", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bringt sie dir zur\u00fcck.", "tokens": ["Bringt", "sie", "dir", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Liebe! hoffe! dulde! glaube!", "tokens": ["Lie\u00b7be", "!", "hof\u00b7fe", "!", "dul\u00b7de", "!", "glau\u00b7be", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "$.", "VVFIN", "$.", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und aus Edens sch\u00f6nster Laube", "tokens": ["Und", "aus", "E\u00b7dens", "sch\u00f6ns\u00b7ter", "Lau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schaut sie l\u00e4chelnd deiner Ruh,", "tokens": ["Schaut", "sie", "l\u00e4\u00b7chelnd", "dei\u00b7ner", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Segnend deinen Thaten zu!", "tokens": ["Seg\u00b7nend", "dei\u00b7nen", "Tha\u00b7ten", "zu", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}