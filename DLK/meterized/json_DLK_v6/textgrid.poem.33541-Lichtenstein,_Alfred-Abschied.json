{"textgrid.poem.33541": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1902, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vorm Sterben mache ich noch mein Gedicht.", "tokens": ["Vorm", "Ster\u00b7ben", "ma\u00b7che", "ich", "noch", "mein", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Still, Kameraden, st\u00f6rt mich nicht.", "tokens": ["Still", ",", "Ka\u00b7me\u00b7ra\u00b7den", ",", "st\u00f6rt", "mich", "nicht", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wir ziehn zum Krieg. Der Tod ist unser Kitt.", "tokens": ["Wir", "ziehn", "zum", "Krieg", ".", "Der", "Tod", "ist", "un\u00b7ser", "Kitt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O, heulte mir doch die Geliebte nit.", "tokens": ["O", ",", "heul\u00b7te", "mir", "doch", "die", "Ge\u00b7lieb\u00b7te", "nit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was liegt an mir. Ich gehe gerne ein.", "tokens": ["Was", "liegt", "an", "mir", ".", "Ich", "ge\u00b7he", "ger\u00b7ne", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPER", "$.", "PPER", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Mutter weint. Man mu\u00df aus Eisen sein.", "tokens": ["Die", "Mut\u00b7ter", "weint", ".", "Man", "mu\u00df", "aus", "Ei\u00b7sen", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PIS", "VMFIN", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Sonne f\u00e4llt zum Horizont hinab.", "tokens": ["Die", "Son\u00b7ne", "f\u00e4llt", "zum", "Ho\u00b7ri\u00b7zont", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bald wirft man mich ins milde Massengrab.", "tokens": ["Bald", "wirft", "man", "mich", "ins", "mil\u00b7de", "Mas\u00b7sen\u00b7grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Am Himmel brennt das brave Abendrot.", "tokens": ["Am", "Him\u00b7mel", "brennt", "das", "bra\u00b7ve", "A\u00b7ben\u00b7drot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vielleicht bin ich in dreizehn Tagen tot.", "tokens": ["Viel\u00b7leicht", "bin", "ich", "in", "drei\u00b7zehn", "Ta\u00b7gen", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "CARD", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}