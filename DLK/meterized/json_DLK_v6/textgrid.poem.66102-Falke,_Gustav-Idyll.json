{"textgrid.poem.66102": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Idyll", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Maria, unterm Lindenbaum,", "tokens": ["Ma\u00b7ria", ",", "un\u00b7term", "Lin\u00b7den\u00b7baum", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lullt ihren Sohn in Schlaf und Traum.", "tokens": ["Lullt", "ih\u00b7ren", "Sohn", "in", "Schlaf", "und", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Herr Joseph auch, der wackre Greis,", "tokens": ["Herr", "Jo\u00b7se\u00b7ph", "auch", ",", "der", "wack\u00b7re", "Greis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ist eingenickt und schnarcht ganz leis.", "tokens": ["Ist", "ein\u00b7ge\u00b7nickt", "und", "schnarcht", "ganz", "leis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Vier Englein aber hocken dicht", "tokens": ["Vier", "En\u00b7glein", "a\u00b7ber", "ho\u00b7cken", "dicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "ADV", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf einem Ast und schlafen nicht.", "tokens": ["Auf", "ei\u00b7nem", "Ast", "und", "schla\u00b7fen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie schlafen nicht und singen sacht,", "tokens": ["Sie", "schla\u00b7fen", "nicht", "und", "sin\u00b7gen", "sacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein' Nachtigall es besser macht!", "tokens": ["Kein'", "Nach\u00b7ti\u00b7gall", "es", "bes\u00b7ser", "macht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Gro\u00df \u00fcberm Wald her, Himmelsruh,", "tokens": ["Gro\u00df", "\u00fc\u00b7berm", "Wald", "her", ",", "Him\u00b7mels\u00b7ruh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PTKVZ", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hebt sich der Mond und guckt herzu.", "tokens": ["Hebt", "sich", "der", "Mond", "und", "guckt", "her\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Maria rei\u00dft die Augen auf,", "tokens": ["Ma\u00b7ria", "rei\u00dft", "die", "Au\u00b7gen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihr fiel ein Schlummerk\u00f6rnlein drauf.", "tokens": ["Ihr", "fiel", "ein", "Schlum\u00b7mer\u00b7k\u00f6rn\u00b7lein", "drauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und ist erst in der halben Nacht,", "tokens": ["Und", "ist", "erst", "in", "der", "hal\u00b7ben", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df sie bei ihrem Kind gewacht.", "tokens": ["Da\u00df", "sie", "bei", "ih\u00b7rem", "Kind", "ge\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Sie sieht in all den Silberschein", "tokens": ["Sie", "sieht", "in", "all", "den", "Sil\u00b7ber\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit gro\u00dfen Augen still hinein.", "tokens": ["Mit", "gro\u00b7\u00dfen", "Au\u00b7gen", "still", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "H\u00f6rt kaum das Lied von obenher,", "tokens": ["H\u00f6rt", "kaum", "das", "Lied", "von", "o\u00b7ben\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Herz ist bang, ihr Herz ist schwer.", "tokens": ["Ihr", "Herz", "ist", "bang", ",", "ihr", "Herz", "ist", "schwer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Ein Tr\u00e4nlein f\u00e4llt ihr auf die Hand", "tokens": ["Ein", "Tr\u00e4n\u00b7lein", "f\u00e4llt", "ihr", "auf", "die", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und blitzt im Mond wie ein Demant.", "tokens": ["Und", "blitzt", "im", "Mond", "wie", "ein", "De\u00b7mant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}