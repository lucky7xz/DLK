{"textgrid.poem.67826": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "22. Die Wiese", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ging einst einen Fr\u00fchlingstag,", "tokens": ["Ich", "ging", "einst", "ei\u00b7nen", "Fr\u00fch\u00b7lings\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo alles sch\u00f6n und lustig lag,", "tokens": ["Wo", "al\u00b7les", "sch\u00f6n", "und", "lus\u00b7tig", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kam an ein einsam Sommerhaus,", "tokens": ["Kam", "an", "ein", "ein\u00b7sam", "Som\u00b7mer\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein liebes M\u00e4dchen trat heraus,", "tokens": ["Ein", "lie\u00b7bes", "M\u00e4d\u00b7chen", "trat", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und weint' und ging und sang betr\u00fcbt:", "tokens": ["Und", "weint'", "und", "ging", "und", "sang", "be\u00b7tr\u00fcbt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbach, wer hat je, wie ich, geliebt!\u00ab", "tokens": ["\u00bb", "ach", ",", "wer", "hat", "je", ",", "wie", "ich", ",", "ge\u00b7liebt", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "PWS", "VAFIN", "ADV", "$,", "PWAV", "PPER", "$,", "VVPP", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Sie ging die Wiese still umher,", "tokens": ["Sie", "ging", "die", "Wie\u00b7se", "still", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und rang die Hand und seufzte schwer;", "tokens": ["Und", "rang", "die", "Hand", "und", "seufz\u00b7te", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann pfl\u00fcckte sie ein Bl\u00fcmchen ab,", "tokens": ["Dann", "pfl\u00fcck\u00b7te", "sie", "ein", "Bl\u00fcm\u00b7chen", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie's hie und da die Wiese gab,", "tokens": ["Wie's", "hie", "und", "da", "die", "Wie\u00b7se", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "KON", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Maasliebchen, klein' Vergi\u00df mein nicht,", "tokens": ["Maas\u00b7lieb\u00b7chen", ",", "klein'", "Ver\u00b7gi\u00df", "mein", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "PPOSAT", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und seufzte: \u00bbach er liebt mich nicht!\u00ab", "tokens": ["Und", "seufz\u00b7te", ":", "\u00bb", "ach", "er", "liebt", "mich", "nicht", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "XY", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie band die Blumen in ein Bund,", "tokens": ["Sie", "band", "die", "Blu\u00b7men", "in", "ein", "Bund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weint' noch einmal aus Herzensgrund:", "tokens": ["Weint'", "noch", "ein\u00b7mal", "aus", "Her\u00b7zens\u00b7grund", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbvergi\u00df mein nicht! hier bind ich dich,", "tokens": ["\u00bb", "ver\u00b7gi\u00df", "mein", "nicht", "!", "hier", "bind", "ich", "dich", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPOSAT", "PTKNEG", "$.", "ADV", "VAFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr wen? \u2013 Maasliebchen, schaust auf mich,", "tokens": ["F\u00fcr", "wen", "?", "\u2013", "Maas\u00b7lieb\u00b7chen", ",", "schaust", "auf", "mich", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "$.", "$(", "NN", "$,", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weinst um mich! \u2013 Ja, ich bin betr\u00fcbt;", "tokens": ["Weinst", "um", "mich", "!", "\u2013", "Ja", ",", "ich", "bin", "be\u00b7tr\u00fcbt", ";"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$.", "$(", "PTKANT", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er hat mich nicht, wie ich ihn geliebt.\u00ab", "tokens": ["Er", "hat", "mich", "nicht", ",", "wie", "ich", "ihn", "ge\u00b7liebt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "PPER", "VVPP", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Nun hatt' sie Busen voll und Schoos,", "tokens": ["Nun", "hatt'", "sie", "Bu\u00b7sen", "voll", "und", "Schoos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ach! nun ward ihr Schmerz zu gro\u00df;", "tokens": ["Und", "ach", "!", "nun", "ward", "ihr", "Schmerz", "zu", "gro\u00df", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "ADV", "VAFIN", "PPOSAT", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie go\u00df die liebe B\u00fcrd' hinab;", "tokens": ["Sie", "go\u00df", "die", "lie\u00b7be", "B\u00fcrd'", "hin\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Liegt, sprach sie, seyd mein sanftes Grab!", "tokens": ["Liegt", ",", "sprach", "sie", ",", "seyd", "mein", "sanf\u00b7tes", "Grab", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "$,", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sank dahin \u2013 ein stilles Ach", "tokens": ["Und", "sank", "da\u00b7hin", "\u2013", "ein", "stil\u00b7les", "Ach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PAV", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Voll Lieb' und Leid ihr Herz zerbrach.", "tokens": ["Voll", "Lieb'", "und", "Leid", "ihr", "Herz", "zer\u00b7brach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}