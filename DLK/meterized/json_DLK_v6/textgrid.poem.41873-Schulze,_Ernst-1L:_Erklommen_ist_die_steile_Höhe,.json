{"textgrid.poem.41873": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Erklommen ist die steile H\u00f6he,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erklommen ist die steile H\u00f6he,", "tokens": ["Er\u00b7klom\u00b7men", "ist", "die", "stei\u00b7le", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich schau' hinab in's ferne Land,", "tokens": ["Ich", "schau'", "hin\u00b7ab", "in's", "fer\u00b7ne", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Alles ist, so weit ich sehe,", "tokens": ["Und", "Al\u00b7les", "ist", ",", "so", "weit", "ich", "se\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir hold und heimisch und bekannt.", "tokens": ["Mir", "hold", "und", "hei\u00b7misch", "und", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Fast wollt' ich jeden Berg dir sagen", "tokens": ["Fast", "wollt'", "ich", "je\u00b7den", "Berg", "dir", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und jede Wiese, jedes Thal,", "tokens": ["Und", "je\u00b7de", "Wie\u00b7se", ",", "je\u00b7des", "Thal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo ich mit ihr in fr\u00fchen Tagen", "tokens": ["Wo", "ich", "mit", "ihr", "in", "fr\u00fc\u00b7hen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mich einst erging in Lust und Qual.", "tokens": ["Mich", "einst", "er\u00b7ging", "in", "Lust", "und", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie zog ich doch so manche Stunden", "tokens": ["Wie", "zog", "ich", "doch", "so", "man\u00b7che", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im dichten Haine hin und her", "tokens": ["Im", "dich\u00b7ten", "Hai\u00b7ne", "hin", "und", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und w\u00e4hnte ganz mein Ziel verschwunden,", "tokens": ["Und", "w\u00e4hn\u00b7te", "ganz", "mein", "Ziel", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und traute keinem Pfade mehr!", "tokens": ["Und", "trau\u00b7te", "kei\u00b7nem", "Pfa\u00b7de", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und dennoch hat auf irren Spuren", "tokens": ["Und", "den\u00b7noch", "hat", "auf", "ir\u00b7ren", "Spu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch vielverschlungne Waldesnacht,", "tokens": ["Durch", "viel\u00b7ver\u00b7schlung\u00b7ne", "Wal\u00b7des\u00b7nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So nah den wohlbekannten Fluren,", "tokens": ["So", "nah", "den", "wohl\u00b7be\u00b7kann\u00b7ten", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Pfad zum Ziele mich gebracht.", "tokens": ["Der", "Pfad", "zum", "Zie\u00b7le", "mich", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O s\u00fc\u00dfe Liebe, d\u00fcrft' ich ahnen,", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "Lie\u00b7be", ",", "d\u00fcrft'", "ich", "ah\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df endlich f\u00fcr die lange Treu", "tokens": ["Da\u00df", "end\u00b7lich", "f\u00fcr", "die", "lan\u00b7ge", "Treu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir so nach manchen dunkeln Bahnen", "tokens": ["Mir", "so", "nach", "man\u00b7chen", "dun\u00b7keln", "Bah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein sel'ges Heil beschieden sey,", "tokens": ["Dein", "sel'\u00b7ges", "Heil", "be\u00b7schie\u00b7den", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann wollt' ich ohne Thr\u00e4n' und Klage", "tokens": ["Dann", "wollt'", "ich", "oh\u00b7ne", "Thr\u00e4n'", "und", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch deine rauhen W\u00fcsten gehn", "tokens": ["Durch", "dei\u00b7ne", "rau\u00b7hen", "W\u00fcs\u00b7ten", "gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und muthig in der Nacht dem Tage,", "tokens": ["Und", "mut\u00b7hig", "in", "der", "Nacht", "dem", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Im Sturm der Ruh' entgegensehn.", "tokens": ["Im", "Sturm", "der", "Ruh'", "ent\u00b7ge\u00b7gen\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}