{"dta.poem.8503": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Gastrecht .", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich war in einem sch\u00f6nen Haus", "tokens": ["Ich", "war", "in", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schien darin ein lieber Gast;", "tokens": ["Und", "schien", "da\u00b7rin", "ein", "lie\u00b7ber", "Gast", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Damen sah\u2019n wie Musen fast,", "tokens": ["Die", "Da\u00b7men", "sah'n", "wie", "Mu\u00b7sen", "fast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sogar die Hunde geistreich aus.", "tokens": ["So\u00b7gar", "die", "Hun\u00b7de", "geist\u00b7reich", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Luft, von Ambraduft bewegt,", "tokens": ["Die", "Luft", ",", "von", "Am\u00b7bra\u00b7duft", "be\u00b7wegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schien aufgel\u00f6s\u2019te Phantasie,", "tokens": ["Schien", "auf\u00b7ge\u00b7l\u00f6s'\u00b7te", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wenn ein Vorhang sich geregt,", "tokens": ["Und", "wenn", "ein", "Vor\u00b7hang", "sich", "ge\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dann war sein Fl\u00fcstern Poesie.", "tokens": ["Dann", "war", "sein", "Fl\u00fcs\u00b7tern", "Poe\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zwar trat mir oft ein Schwindel nah,", "tokens": ["Zwar", "trat", "mir", "oft", "ein", "Schwin\u00b7del", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u2014 Ich bin an Aether nicht gew\u00f6hnt, \u2014", "tokens": ["Ich", "bin", "an", "A\u00b7e\u00b7ther", "nicht", "ge\u00b7w\u00f6hnt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "APPR", "NN", "PTKNEG", "VVPP", "$,", "$("], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Doch hat der Zauber mich vers\u00f6hnt", "tokens": ["Doch", "hat", "der", "Zau\u00b7ber", "mich", "ver\u00b7s\u00f6hnt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und reiche Stunden lebt\u2019 ich da.", "tokens": ["Und", "rei\u00b7che", "Stun\u00b7den", "lebt'", "ich", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "All was man sagte war so klar", "tokens": ["All", "was", "man", "sag\u00b7te", "war", "so", "klar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "PWS", "PIS", "VVFIN", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und so vortrefflich durchgef\u00fchrt,", "tokens": ["Und", "so", "vor\u00b7treff\u00b7lich", "durch\u00b7ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df ich mich habe ganz und gar", "tokens": ["Da\u00df", "ich", "mich", "ha\u00b7be", "ganz", "und", "gar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Oft wie ein Erzkameel gesp\u00fcrt.", "tokens": ["Oft", "wie", "ein", "Erz\u00b7ka\u00b7me\u00b7el", "ge\u00b7sp\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Da traf es eines Tags, da\u00df oft", "tokens": ["Da", "traf", "es", "ei\u00b7nes", "Tags", ",", "da\u00df", "oft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "KOUS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man leis von einem Gaste sprach,", "tokens": ["Man", "leis", "von", "ei\u00b7nem", "Gas\u00b7te", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der l\u00e4ngst geladen, hintennach", "tokens": ["Der", "l\u00e4ngst", "ge\u00b7la\u00b7den", ",", "hin\u00b7ten\u00b7nach"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADV", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kam wie die Reue unverhofft.", "tokens": ["Kam", "wie", "die", "Reu\u00b7e", "un\u00b7ver\u00b7hofft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie ward zum Fenster ausgeschaut,", "tokens": ["Wie", "ward", "zum", "Fens\u00b7ter", "aus\u00b7ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein seltsam L\u00e4cheln im Gesicht;", "tokens": ["Ein", "selt\u00b7sam", "L\u00e4\u00b7cheln", "im", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich h\u00e4tte H\u00e4user drauf gebaut,", "tokens": ["Ich", "h\u00e4t\u00b7te", "H\u00e4u\u00b7ser", "drauf", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Gast sei ein Parnassuslicht.", "tokens": ["Der", "Gast", "sei", "ein", "Par\u00b7nas\u00b7sus\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Und als er endlich angelangt,", "tokens": ["Und", "als", "er", "end\u00b7lich", "an\u00b7ge\u00b7langt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stie\u00df jeder, eh zum Gru\u00df er lief,", "tokens": ["Stie\u00df", "je\u00b7der", ",", "eh", "zum", "Gru\u00df", "er", "lief", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erst einen Seufzer lang und tief,", "tokens": ["Erst", "ei\u00b7nen", "Seuf\u00b7zer", "lang", "und", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beweis, wie das Entz\u00fccken bangt;", "tokens": ["Be\u00b7weis", ",", "wie", "das", "Ent\u00b7z\u00fc\u00b7cken", "bangt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Bruder ", "tokens": ["Mein", "Bru\u00b7der"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Schien mir ein schlichter Bursche nur:", "tokens": ["Schien", "mir", "ein", "schlich\u00b7ter", "Bur\u00b7sche", "nur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sein Blick war frank und lebensfroh,", "tokens": ["Sein", "Blick", "war", "frank", "und", "le\u00b7bens\u00b7froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Doch vom Erhabnen keine Spur.", "tokens": ["Doch", "vom", "Er\u00b7hab\u00b7nen", "kei\u00b7ne", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Drei Tage lebten wir so fort", "tokens": ["Drei", "Ta\u00b7ge", "leb\u00b7ten", "wir", "so", "fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PPER", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zusammen wie im Paradies;", "tokens": ["Zu\u00b7sam\u00b7men", "wie", "im", "Pa\u00b7ra\u00b7dies", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man sprach von Wurzeln und Radies,", "tokens": ["Man", "sprach", "von", "Wur\u00b7zeln", "und", "Ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "KON", "NE", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Doch auch manch klar und innig Wort.", "tokens": ["Doch", "auch", "manch", "klar", "und", "in\u00b7nig", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "ADJD", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Fremden Auge hat so frisch", "tokens": ["Des", "Frem\u00b7den", "Au\u00b7ge", "hat", "so", "frisch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und freundlich wie ein Stern geblinkt,", "tokens": ["Und", "freund\u00b7lich", "wie", "ein", "Stern", "ge\u00b7blinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und als er endlich schied nach Tisch,", "tokens": ["Und", "als", "er", "end\u00b7lich", "schied", "nach", "Tisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da ward ihm lange nachgewinkt.", "tokens": ["Da", "ward", "ihm", "lan\u00b7ge", "nach\u00b7ge\u00b7winkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Das hat ger\u00fchrt mich und erg\u00f6tzt,", "tokens": ["Das", "hat", "ge\u00b7r\u00fchrt", "mich", "und", "er\u00b7g\u00f6tzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "PPER", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur war mir etwas wundersam", "tokens": ["Nur", "war", "mir", "et\u00b7was", "wun\u00b7der\u00b7sam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Blick, mit dem sich die Madam", "tokens": ["Der", "Blick", ",", "mit", "dem", "sich", "die", "Ma\u00b7dam"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PRF", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Schnell an die Stickerei gesetzt;", "tokens": ["Schnell", "an", "die", "Sti\u00b7cke\u00b7rei", "ge\u00b7setzt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Zug am Mund, als Claudia", "tokens": ["Der", "Zug", "am", "Mund", ",", "als", "Clau\u00b7dia"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "KOUS", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sacht an den Arm der Schwester griff,", "tokens": ["Sacht", "an", "den", "Arm", "der", "Schwes\u00b7ter", "griff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und da\u00df sich wandte der Papa", "tokens": ["Und", "da\u00df", "sich", "wand\u00b7te", "der", "Pa\u00b7pa"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PRF", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und blinzelnd auf dem Finger pfiff.", "tokens": ["Und", "blin\u00b7zelnd", "auf", "dem", "Fin\u00b7ger", "pfiff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Sie waren Leute sein und tief,", "tokens": ["Sie", "wa\u00b7ren", "Leu\u00b7te", "sein", "und", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "VAINF", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gar noble Leute allzumal;", "tokens": ["Gar", "nob\u00b7le", "Leu\u00b7te", "all\u00b7zu\u00b7mal", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schon sank die D\u00e4mmerung in\u2019s Thal,", "tokens": ["Schon", "sank", "die", "D\u00e4m\u00b7me\u00b7rung", "in's", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bevor ihr Argustakt entschlief,", "tokens": ["Be\u00b7vor", "ihr", "Ar\u00b7gus\u00b7takt", "ent\u00b7schlief", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und hier und dort ein Nadelstich,", "tokens": ["Und", "hier", "und", "dort", "ein", "Na\u00b7del\u00b7stich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und kecker denn ein Messerschnitt,", "tokens": ["Und", "ke\u00b7cker", "denn", "ein", "Mes\u00b7ser\u00b7schnitt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und dann die Sonde s\u00e4uberlich", "tokens": ["Und", "dann", "die", "Son\u00b7de", "s\u00e4u\u00b7ber\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In des Geschiednen Schw\u00e4chen glitt.", "tokens": ["In", "des", "Ge\u00b7schied\u00b7nen", "Schw\u00e4\u00b7chen", "glitt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "O sichre Hand, o fester Arm!", "tokens": ["O", "sich\u00b7re", "Hand", ",", "o", "fes\u00b7ter", "Arm", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Sonde, leuchtend wie der Blitz!", "tokens": ["O", "Son\u00b7de", ",", "leuch\u00b7tend", "wie", "der", "Blitz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVPP", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich lehnte an des Gastes Sitz,", "tokens": ["Ich", "lehn\u00b7te", "an", "des", "Gas\u00b7tes", "Sitz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fchlte sacht ob er noch warm;", "tokens": ["Und", "f\u00fchl\u00b7te", "sacht", "ob", "er", "noch", "warm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "KOUS", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und an das Fenster trat ich dann,", "tokens": ["Und", "an", "das", "Fens\u00b7ter", "trat", "ich", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nahm mir ein allbekanntes Buch,", "tokens": ["Nahm", "mir", "ein", "all\u00b7be\u00b7kann\u00b7tes", "Buch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und las, die Blicke ab und an", "tokens": ["Und", "las", ",", "die", "Bli\u00b7cke", "ab", "und", "an"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "