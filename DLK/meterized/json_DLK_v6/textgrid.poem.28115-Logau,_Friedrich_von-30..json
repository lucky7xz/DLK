{"textgrid.poem.28115": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df mein Buch, sagt mir mein Mut,", "tokens": ["Da\u00df", "mein", "Buch", ",", "sagt", "mir", "mein", "Mut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch gantz b\u00f6se, noch gantz gut.", "tokens": ["Noch", "gantz", "b\u00f6\u00b7se", ",", "noch", "gantz", "gut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kummen dr\u00fcber arge Fliegen,", "tokens": ["Kum\u00b7men", "dr\u00fc\u00b7ber", "ar\u00b7ge", "Flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird gesund es bleiben liegen,", "tokens": ["Wird", "ge\u00b7sund", "es", "blei\u00b7ben", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und das faule leiden an;", "tokens": ["Und", "das", "fau\u00b7le", "lei\u00b7den", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Kummen aber Bienen dran,", "tokens": ["Kum\u00b7men", "a\u00b7ber", "Bie\u00b7nen", "dran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wird das faule seyn vermieden,", "tokens": ["Wird", "das", "fau\u00b7le", "seyn", "ver\u00b7mie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und gesundes recht beschieden.", "tokens": ["Und", "ge\u00b7sun\u00b7des", "recht", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}