{"textgrid.poem.48092": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Nirwana", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df flie\u00dfen alle Wunden!", "tokens": ["La\u00df", "flie\u00b7\u00dfen", "al\u00b7le", "Wun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erst wenn dein Blut zu ebben kommt,", "tokens": ["Erst", "wenn", "dein", "Blut", "zu", "eb\u00b7ben", "kommt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPOSAT", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "wirst du gesunden.", "tokens": ["wirst", "du", "ge\u00b7sun\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "O Wonne, so zu geben,", "tokens": ["O", "Won\u00b7ne", ",", "so", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "was dir aus tiefster Seele quillt:", "tokens": ["was", "dir", "aus", "tiefs\u00b7ter", "See\u00b7le", "quillt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "dein starkes, reiches Leben!", "tokens": ["dein", "star\u00b7kes", ",", "rei\u00b7ches", "Le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O Wonne, so zerflie\u00dfen", "tokens": ["O", "Won\u00b7ne", ",", "so", "zer\u00b7flie\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und aller Schmerzen Glut und Qual", "tokens": ["und", "al\u00b7ler", "Schmer\u00b7zen", "Glut", "und", "Qual"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ins ewige Nichts ergie\u00dfen . . .", "tokens": ["ins", "e\u00b7wi\u00b7ge", "Nichts", "er\u00b7gie\u00b7\u00dfen", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$.", "$.", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}