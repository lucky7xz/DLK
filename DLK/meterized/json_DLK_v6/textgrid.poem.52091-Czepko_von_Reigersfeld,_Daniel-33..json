{"textgrid.poem.52091": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Indem du zu Landeck im warmen Bade bist,", "tokens": ["In\u00b7dem", "du", "zu", "Lan\u00b7deck", "im", "war\u00b7men", "Ba\u00b7de", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "APPRART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+--++-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da nichts als Freud und Lust dir wird zur Seiten stehen,", "tokens": ["Da", "nichts", "als", "Freud", "und", "Lust", "dir", "wird", "zur", "Sei\u00b7ten", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "KOKOM", "NN", "KON", "NN", "PPER", "VAFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sieht mich der ernste Voigt auch allhier m\u00fc\u00dfig gehen,", "tokens": ["Sieht", "mich", "der", "erns\u00b7te", "Voigt", "auch", "all\u00b7hier", "m\u00fc\u00b7\u00dfig", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NE", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der mir von Tag auff Tag sein Rechnungsbuch verlist:", "tokens": ["Der", "mir", "von", "Tag", "auff", "Tag", "sein", "Rech\u00b7nungs\u00b7buch", "ver\u00b7list", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "APPR", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hier ist (das zeigen fast die alten Thier G\u00e4rt an)", "tokens": ["Hier", "ist", "(", "das", "zei\u00b7gen", "fast", "die", "al\u00b7ten", "Thier", "G\u00e4rt", "an", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$(", "PDS", "VVFIN", "ADV", "ART", "ADJA", "NN", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der F\u00fcrstin Agnes Gut und Meyer Hoff gewesen,", "tokens": ["Der", "F\u00fcrs\u00b7tin", "Ag\u00b7nes", "Gut", "und", "Me\u00b7yer", "Hoff", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ADJD", "KON", "NE", "VVFIN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo itzt dein Czepko wohnt, den du dir w\u00fcnschst zu lesen,", "tokens": ["Wo", "itzt", "dein", "Czep\u00b7ko", "wohnt", ",", "den", "du", "dir", "w\u00fcnschst", "zu", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "VVFIN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und mehr von ihm begehrst, als er vermag und kan.", "tokens": ["Und", "mehr", "von", "ihm", "be\u00b7gehrst", ",", "als", "er", "ver\u00b7mag", "und", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "KON", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Nymphen, wo du bist, die la\u00dfen voller Ruh", "tokens": ["Die", "Nym\u00b7phen", ",", "wo", "du", "bist", ",", "die", "la\u00b7\u00dfen", "vol\u00b7ler", "Ruh"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VAFIN", "$,", "PRELS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Selbst die Gesundheit dir aus milden Adern springen,", "tokens": ["Selbst", "die", "Ge\u00b7sund\u00b7heit", "dir", "aus", "mil\u00b7den", "A\u00b7dern", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Mich wil der wilde Mars von sichrer Freude dringen,", "tokens": ["Mich", "wil", "der", "wil\u00b7de", "Mars", "von", "sich\u00b7rer", "Freu\u00b7de", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und l\u00e4\u00dft mir nicht die Helfft' in meiner Wirthschafft zu.", "tokens": ["Und", "l\u00e4\u00dft", "mir", "nicht", "die", "Helfft'", "in", "mei\u00b7ner", "Wirth\u00b7schafft", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und ob die Galle mir zu Zeiten \u00fcbergeht,", "tokens": ["Und", "ob", "die", "Gal\u00b7le", "mir", "zu", "Zei\u00b7ten", "\u00fc\u00b7ber\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich das gr\u00f6ste Theil mu\u00df \u00f6d' und w\u00fcste schauen:", "tokens": ["Da\u00df", "ich", "das", "gr\u00f6s\u00b7te", "Theil", "mu\u00df", "\u00f6d'", "und", "w\u00fcs\u00b7te", "schau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VMFIN", "ADJD", "KON", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch bin ich offte hier: Ich darff mich be\u00dfer trauen,", "tokens": ["Doch", "bin", "ich", "off\u00b7te", "hier", ":", "Ich", "darff", "mich", "be\u00b7\u00dfer", "trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "ADV", "$.", "PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dieweil es von der Stadt nicht eine Stunde steht.", "tokens": ["Die\u00b7weil", "es", "von", "der", "Stadt", "nicht", "ei\u00b7ne", "Stun\u00b7de", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Vor diesem kont' ich ja bald auff das Land hinaus,", "tokens": ["Vor", "die\u00b7sem", "kont'", "ich", "ja", "bald", "auff", "das", "Land", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VMFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "APZR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bald auff die Berge zu: itzt la\u00df ich alles liegen,", "tokens": ["Bald", "auff", "die", "Ber\u00b7ge", "zu", ":", "itzt", "la\u00df", "ich", "al\u00b7les", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKVZ", "$.", "ADV", "VVIMP", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wer wil sich zu Tod auff w\u00fcsten G\u00fcttern pfl\u00fcgen,", "tokens": ["Und", "wer", "wil", "sich", "zu", "Tod", "auff", "w\u00fcs\u00b7ten", "G\u00fct\u00b7tern", "pfl\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "PRF", "APPR", "NN", "APPR", "VVFIN", "NN", "VVINF", "$,"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Genung, erhalt ich das: wo nicht: Die Stadt bleibt Hau\u00df.", "tokens": ["Ge\u00b7nung", ",", "er\u00b7halt", "ich", "das", ":", "wo", "nicht", ":", "Die", "Stadt", "bleibt", "Hau\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PDS", "$.", "PWAV", "PTKNEG", "$.", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}