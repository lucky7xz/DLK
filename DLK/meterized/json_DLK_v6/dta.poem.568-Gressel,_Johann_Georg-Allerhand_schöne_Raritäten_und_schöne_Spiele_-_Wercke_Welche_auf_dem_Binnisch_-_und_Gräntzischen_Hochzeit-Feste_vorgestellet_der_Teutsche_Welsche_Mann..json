{"dta.poem.568": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Allerhand sch\u00f6ne Rarit\u00e4ten und sch\u00f6ne  \n Spiele - Wercke/  \n Welche auf dem  \n Binnisch - und Gr\u00e4ntzischen  \n Hochzeit-Feste vorgestellet  \n der Teutsche Welsche Mann.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Meinen Kasten thut sich auf/ und l\u00e4st was Rares sehn/", "tokens": ["Mei\u00b7nen", "Kas\u00b7ten", "thut", "sich", "auf", "/", "und", "l\u00e4st", "was", "Ra\u00b7res", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "$(", "KON", "VVFIN", "PIS", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Hier kommt ", "tokens": ["Hier", "kommt"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Damit sein grosses Reich m\u00f6g ohne Abbruch stehn/", "tokens": ["Da\u00b7mit", "sein", "gros\u00b7ses", "Reich", "m\u00f6g", "oh\u00b7ne", "Ab\u00b7bruch", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "ADJA", "NN", "VMFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Viel Menschen folgen ihm die seinen Pfeil nicht fliehen.", "tokens": ["Viel", "Men\u00b7schen", "fol\u00b7gen", "ihm", "die", "sei\u00b7nen", "Pfeil", "nicht", "flie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ART", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jhr lieben Leute schaut einmahl", "tokens": ["Ihr", "lie\u00b7ben", "Leu\u00b7te", "schaut", "ein\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie viel von euch in dieser Zahl.", "tokens": ["Wie", "viel", "von", "euch", "in", "die\u00b7ser", "Zahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sch\u00f6ne Rarit\u00e4t/ sch\u00f6ne Spiele-Werckerc.", "tokens": ["Sch\u00f6\u00b7ne", "Ra\u00b7ri\u00b7t\u00e4t", "/", "sch\u00f6\u00b7ne", "Spie\u00b7le\u00b7\u00b7Wer\u00b7ckerc", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADJA", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}}}}