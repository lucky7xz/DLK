{"textgrid.poem.49402": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "Pariser Notturno", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja rolle, rolle, Seine, deine tr\u00e4gen Wogen,", "tokens": ["Ja", "rol\u00b7le", ",", "rol\u00b7le", ",", "Sei\u00b7ne", ",", "dei\u00b7ne", "tr\u00e4\u00b7gen", "Wo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "$,", "VVFIN", "$,", "PPOSAT", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch deine giftumhauchten, finstern Br\u00fcckenbogen", "tokens": ["Durch", "dei\u00b7ne", "gif\u00b7tum\u00b7hauch\u00b7ten", ",", "fins\u00b7tern", "Br\u00fc\u00b7cken\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Schwamm mancher Leichnam schon entstellt, verwesend, fort,", "tokens": ["Schwamm", "man\u00b7cher", "Leich\u00b7nam", "schon", "ent\u00b7stellt", ",", "ver\u00b7we\u00b7send", ",", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "PIAT", "NN", "ADV", "VVPP", "$,", "VVPP", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem Seele, Herz und Sinn l\u00e4ngst in Paris verdorrt.", "tokens": ["Dem", "See\u00b7le", ",", "Herz", "und", "Sinn", "l\u00e4ngst", "in", "Pa\u00b7ris", "ver\u00b7dorrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "ADV", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch sind die Eisflut nie so viel hinabgeschwommen,", "tokens": ["Doch", "sind", "die", "Eis\u00b7flut", "nie", "so", "viel", "hin\u00b7ab\u00b7ge\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie mir bei deinem Anblick jetzt Gedanken kommen.", "tokens": ["Wie", "mir", "bei", "dei\u00b7nem", "An\u00b7blick", "jetzt", "Ge\u00b7dan\u00b7ken", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Tiber hat Ruinen an dem hellen Strand,", "tokens": ["Der", "Ti\u00b7ber", "hat", "Ru\u00b7i\u00b7nen", "an", "dem", "hel\u00b7len", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch die den Wandrer er in ferne Zeiten bannt,", "tokens": ["Durch", "die", "den", "Wand\u00b7rer", "er", "in", "fer\u00b7ne", "Zei\u00b7ten", "bannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "PPER", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Moos und schwarzer Efeu dicht gerankt umweben", "tokens": ["Die", "Moos", "und", "schwar\u00b7zer", "E\u00b7feu", "dicht", "ge\u00b7rankt", "um\u00b7we\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "ADJD", "VVPP", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und die sich grau zerbr\u00f6ckelt aus dem Gr\u00fcn erheben.", "tokens": ["Und", "die", "sich", "grau", "zer\u00b7br\u00f6\u00b7ckelt", "aus", "dem", "Gr\u00fcn", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PRF", "ADJD", "VVPP", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit Goldorangen l\u00e4chelt der Guadalquivir", "tokens": ["Mit", "Gol\u00b7do\u00b7ran\u00b7gen", "l\u00e4\u00b7chelt", "der", "Gua\u00b7dal\u00b7qui\u00b7vir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und spiegelt abends der Boleros bunte Zier.", "tokens": ["Und", "spie\u00b7gelt", "a\u00b7bends", "der", "Bo\u00b7le\u00b7ros", "bun\u00b7te", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NE", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Der Pactolus f\u00fchrt Gold, des Bosporus Gestade", "tokens": ["Der", "Pac\u00b7to\u00b7lus", "f\u00fchrt", "Gold", ",", "des", "Bos\u00b7po\u00b7rus", "Ge\u00b7sta\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Laden die \u00fcpp'ge Odaliske ein zum Bade.", "tokens": ["La\u00b7den", "die", "\u00fcpp'\u00b7ge", "O\u00b7da\u00b7lis\u00b7ke", "ein", "zum", "Ba\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ART", "APPRART", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Ein Burggraf ist der Rhein, ein kecker Troubadour", "tokens": ["Ein", "Burg\u00b7graf", "ist", "der", "Rhein", ",", "ein", "ke\u00b7cker", "Trou\u00b7ba\u00b7dour"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NE", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ist der Lignonfluss und ein Buhler der Adour.", "tokens": ["Ist", "der", "Lig\u00b7non\u00b7fluss", "und", "ein", "Buh\u00b7ler", "der", "A\u00b7dour", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KON", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Des Nilstroms Flut, fl\u00fcsternd wie leiser Kummer,", "tokens": ["Des", "Nil\u00b7stroms", "Flut", ",", "fl\u00fcs\u00b7ternd", "wie", "lei\u00b7ser", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "$,", "VVPP", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Wiegt seine Mumien ein in tr\u00e4umes\u00fcssen Schlummer,", "tokens": ["Wiegt", "sei\u00b7ne", "Mu\u00b7mi\u00b7en", "ein", "in", "tr\u00e4u\u00b7me\u00b7s\u00fcs\u00b7sen", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Und der Meschasc\u00e9b\u00e9, voll heil'gem Schilf, benagt", "tokens": ["Und", "der", "Me\u00b7schasc\u00e9b\u00e9", ",", "voll", "heil'\u00b7gem", "Schilf", ",", "be\u00b7nagt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Der Inseln Fels, der steil aus seinen Wassern ragt.", "tokens": ["Der", "In\u00b7seln", "Fels", ",", "der", "steil", "aus", "sei\u00b7nen", "Was\u00b7sern", "ragt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "PRELS", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Donnernd und leuchtend l\u00e4sst er seine hoheitsvollen", "tokens": ["Don\u00b7nernd", "und", "leuch\u00b7tend", "l\u00e4sst", "er", "sei\u00b7ne", "ho\u00b7heits\u00b7vol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "ADJA"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.16": {"text": "Wogen in breiten Niagaras niederrollen.", "tokens": ["Wo\u00b7gen", "in", "brei\u00b7ten", "Nia\u00b7ga\u00b7ras", "nie\u00b7der\u00b7rol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.17": {"text": "Und der Eurotas, wo die heim'schen Schw\u00e4ne zieh'n,", "tokens": ["Und", "der", "Eu\u00b7ro\u00b7tas", ",", "wo", "die", "heim'\u00b7schen", "Schw\u00e4\u00b7ne", "zieh'n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "$,", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Eint ihren weissen Glanz mit seines Lorbeers Gr\u00fcn.", "tokens": ["Eint", "ih\u00b7ren", "weis\u00b7sen", "Glanz", "mit", "sei\u00b7nes", "Lor\u00b7beers", "Gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "In blauer, klarer Luft, die Geier stolz durchfliegen,", "tokens": ["In", "blau\u00b7er", ",", "kla\u00b7rer", "Luft", ",", "die", "Gei\u00b7er", "stolz", "durch\u00b7flie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Singt einem Dichter gleich der Fluss im Wellenwiegen.", "tokens": ["Singt", "ei\u00b7nem", "Dich\u00b7ter", "gleich", "der", "Fluss", "im", "Wel\u00b7len\u00b7wie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Stark w\u00e4lzt der Ganges durch der Palmen Schauern sich,", "tokens": ["Stark", "w\u00e4lzt", "der", "Gan\u00b7ges", "durch", "der", "Pal\u00b7men", "Schau\u00b7ern", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "PRF", "$,"], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.22": {"text": "Umschwebt von roten Padmas, sanft und feierlich,", "tokens": ["Um\u00b7schwebt", "von", "ro\u00b7ten", "Pad\u00b7mas", ",", "sanft", "und", "fei\u00b7er\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "In k\u00f6niglichem Pomp; an seinem Ufer schwellen", "tokens": ["In", "k\u00f6\u00b7nig\u00b7li\u00b7chem", "Pomp", ";", "an", "sei\u00b7nem", "U\u00b7fer", "schwel\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Um die Moscheen des Volkes l\u00e4rmbewegte Wellen.", "tokens": ["Um", "die", "Mo\u00b7scheen", "des", "Vol\u00b7kes", "l\u00e4rm\u00b7be\u00b7weg\u00b7te", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Das Holz der Cymbals klingt in kriegerischem Ton,", "tokens": ["Das", "Holz", "der", "Cym\u00b7bals", "klingt", "in", "krie\u00b7ge\u00b7ri\u00b7schem", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und in das Rohr geduckt, dumpf klagend, wie Hobo'n,", "tokens": ["Und", "in", "das", "Rohr", "ge\u00b7duckt", ",", "dumpf", "kla\u00b7gend", ",", "wie", "Ho\u00b7bo'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$,", "ADJD", "VVPP", "$,", "PWAV", "NE", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Ob nicht die flinken Antilopen nahen wollen,", "tokens": ["Ob", "nicht", "die", "flin\u00b7ken", "An\u00b7ti\u00b7lo\u00b7pen", "na\u00b7hen", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "ADJA", "NN", "ADJA", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Birgt der gestreifte Tiger sich mit tiefem Grollen.", "tokens": ["Birgt", "der", "ge\u00b7streif\u00b7te", "Ti\u00b7ger", "sich", "mit", "tie\u00b7fem", "Grol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du, Seine, du hast nichts. Zwei Quais und die allein!", "tokens": ["Du", ",", "Sei\u00b7ne", ",", "du", "hast", "nichts", ".", "Zwei", "Qua\u00b7is", "und", "die", "al\u00b7lein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "$,", "PPER", "VAFIN", "PIS", "$.", "CARD", "NN", "KON", "ART", "ADV", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zwei schmier'ge Quais, die rings bes\u00e4t von Schmutzerei'n,", "tokens": ["Zwei", "schmier'\u00b7ge", "Qua\u00b7is", ",", "die", "rings", "be\u00b7s\u00e4t", "von", "Schmut\u00b7ze\u00b7rei'n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Mit schimmelndem Papier, voll von der dichten Menge,", "tokens": ["Mit", "schim\u00b7meln\u00b7dem", "Pa\u00b7pier", ",", "voll", "von", "der", "dich\u00b7ten", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Kreise macht im Fluss und angelt in der Enge.", "tokens": ["Die", "Krei\u00b7se", "macht", "im", "Fluss", "und", "an\u00b7gelt", "in", "der", "En\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "KON", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch wenn es Abend wird, erblickt man ringsumher", "tokens": ["Doch", "wenn", "es", "A\u00b7bend", "wird", ",", "er\u00b7blickt", "man", "rings\u00b7um\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "NN", "VAFIN", "$,", "VVFIN", "PIS", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nur wen'ge Wandrer noch, von Schlaf und Hunger schwer,", "tokens": ["Nur", "wen'\u00b7ge", "Wand\u00b7rer", "noch", ",", "von", "Schlaf", "und", "Hun\u00b7ger", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "$,", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Abendsonne malt die Luft mit roten Flecken,", "tokens": ["Die", "A\u00b7bend\u00b7son\u00b7ne", "malt", "die", "Luft", "mit", "ro\u00b7ten", "Fle\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da kommt der Tr\u00e4umer gern aus finsteren Verstecken,", "tokens": ["Da", "kommt", "der", "Tr\u00e4u\u00b7mer", "gern", "aus", "fins\u00b7te\u00b7ren", "Ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dass auf der Br\u00fccke, unweit Notredame, er sinnt,", "tokens": ["Dass", "auf", "der", "Br\u00fc\u00b7cke", ",", "un\u00b7weit", "Not\u00b7re\u00b7da\u00b7me", ",", "er", "sinnt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "APPR", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Gedanken, Haupt und Herz umspielt vom Abendwind.", "tokens": ["Ge\u00b7dan\u00b7ken", ",", "Haupt", "und", "Herz", "um\u00b7spielt", "vom", "Ab\u00b7end\u00b7wind", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der n\u00e4cht'ge Hauch zerreisst der Wolken grau Gewimmel", "tokens": ["Der", "n\u00e4cht'\u00b7ge", "Hauch", "zer\u00b7reisst", "der", "Wol\u00b7ken", "grau", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und kupfern segeln sie, zerstreut im toten Himmel,", "tokens": ["Und", "kup\u00b7fern", "se\u00b7geln", "sie", ",", "zer\u00b7streut", "im", "to\u00b7ten", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und eines Heil'gen Haupt trifft an dem Domportal", "tokens": ["Und", "ei\u00b7nes", "Heil'\u00b7gen", "Haupt", "trifft", "an", "dem", "Dom\u00b7por\u00b7tal"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mit rotem Kuss der Abendsonne letzter Strahl.", "tokens": ["Mit", "ro\u00b7tem", "Kuss", "der", "A\u00b7bend\u00b7son\u00b7ne", "letz\u00b7ter", "Strahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Beim Nahn des Dunkels fliehn von Furcht erf\u00fcllt die Schwalben,", "tokens": ["Beim", "Nahn", "des", "Dun\u00b7kels", "fliehn", "von", "Furcht", "er\u00b7f\u00fcllt", "die", "Schwal\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und finst're Flederm\u00e4use flattern allenthalben.", "tokens": ["Und", "finst'\u00b7re", "Fle\u00b7der\u00b7m\u00e4u\u00b7se", "flat\u00b7tern", "al\u00b7len\u00b7thal\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und alles schweigt. Ein Summen nur, das dumpf erklingt,", "tokens": ["Und", "al\u00b7les", "schweigt", ".", "Ein", "Sum\u00b7men", "nur", ",", "das", "dumpf", "er\u00b7klingt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$.", "ART", "NN", "ADV", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Erz\u00e4hlt uns von der Stadt, die noch ihr Lied uns singt,", "tokens": ["Er\u00b7z\u00e4hlt", "uns", "von", "der", "Stadt", ",", "die", "noch", "ihr", "Lied", "uns", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Sie, die Tyrannen k\u00fcsst, der Opfer Schar zertr\u00fcmmert \u2013", "tokens": ["Sie", ",", "die", "Ty\u00b7ran\u00b7nen", "k\u00fcsst", ",", "der", "Op\u00b7fer", "Schar", "zer\u00b7tr\u00fcm\u00b7mert", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "VVFIN", "$,", "ART", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Das ist das Morgenrot, das dem Verbrechen schimmert.", "tokens": ["Das", "ist", "das", "Mor\u00b7gen\u00b7rot", ",", "das", "dem", "Ver\u00b7bre\u00b7chen", "schim\u00b7mert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Da pl\u00f6tzlich klingen j\u00e4h, als stiesse ein Tenor", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "klin\u00b7gen", "j\u00e4h", ",", "als", "sties\u00b7se", "ein", "Te\u00b7nor"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ADJD", "$,", "KOUS", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Im Lampendunste seinen grellen Schrei hervor,", "tokens": ["Im", "Lam\u00b7pen\u00b7duns\u00b7te", "sei\u00b7nen", "grel\u00b7len", "Schrei", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Den Schrei, der klagend schwillt in schmerzlichem Erbeben,", "tokens": ["Den", "Schrei", ",", "der", "kla\u00b7gend", "schwillt", "in", "schmerz\u00b7li\u00b7chem", "Er\u00b7be\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Drehorgelt\u00f6ne, die durchs Dunkel zu mir schweben,", "tokens": ["Dre\u00b7hor\u00b7gel\u00b7t\u00f6\u00b7ne", ",", "die", "durchs", "Dun\u00b7kel", "zu", "mir", "schwe\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "APPRART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und wieder h\u00f6rt man jene alten Polkas da,", "tokens": ["Und", "wie\u00b7der", "h\u00f6rt", "man", "je\u00b7ne", "al\u00b7ten", "Pol\u00b7kas", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "PDAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die wir als Kind versucht auf der Harmonika,", "tokens": ["Die", "wir", "als", "Kind", "ver\u00b7sucht", "auf", "der", "Har\u00b7mo\u00b7ni\u00b7ka", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOUS", "NN", "VVPP", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die tr\u00e4ge oder flink, voll Frohsinn oder Trauern,", "tokens": ["Die", "tr\u00e4\u00b7ge", "o\u00b7der", "flink", ",", "voll", "Froh\u00b7sinn", "o\u00b7der", "Trau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJD", "$,", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Frau'n, K\u00fcnstlern und Ge\u00e4chteten das Herz durchschauern.", "tokens": ["Frau'n", ",", "K\u00fcnst\u00b7lern", "und", "Ge\u00e4ch\u00b7te\u00b7ten", "das", "Herz", "durch\u00b7schau\u00b7ern", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Das ist zerst\u00fcckt und rauh, kein einz'ger rechter Ton,", "tokens": ["Das", "ist", "zer\u00b7st\u00fcckt", "und", "rauh", ",", "kein", "einz'\u00b7ger", "rech\u00b7ter", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "KON", "ADJD", "$,", "PIAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Rossini w\u00e4re sicher l\u00e4ngst davon gefloh'n,", "tokens": ["Ros\u00b7si\u00b7ni", "w\u00e4\u00b7re", "si\u00b7cher", "l\u00e4ngst", "da\u00b7von", "ge\u00b7floh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "ADV", "PAV", "VVPP", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.31": {"text": "Das Lachen ist verzerrt, zerrissen sind die Klagen,", "tokens": ["Das", "La\u00b7chen", "ist", "ver\u00b7zerrt", ",", "zer\u00b7ris\u00b7sen", "sind", "die", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Der Klang ist scharf und grell, dass kaum es zu ertragen.", "tokens": ["Der", "Klang", "ist", "scharf", "und", "grell", ",", "dass", "kaum", "es", "zu", "er\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,", "KOUS", "ADV", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "C klingt wie A: Die ganze Tonart ist zerst\u00f6rt \u2013", "tokens": ["C", "klingt", "wie", "A", ":", "Die", "gan\u00b7ze", "To\u00b7nart", "ist", "zer\u00b7st\u00f6rt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KOKOM", "NE", "$.", "ART", "ADJA", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Jedoch was macht es aus! Man weint, wenn man das h\u00f6rt.", "tokens": ["Je\u00b7doch", "was", "macht", "es", "aus", "!", "Man", "weint", ",", "wenn", "man", "das", "h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "PPER", "PTKVZ", "$.", "PIS", "VVFIN", "$,", "KOUS", "PIS", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und unsre Seele scheint im Tr\u00e4umen hinzufliessen", "tokens": ["Und", "uns\u00b7re", "See\u00b7le", "scheint", "im", "Tr\u00e4u\u00b7men", "hin\u00b7zu\u00b7flies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und f\u00fchlt im alten Klang seltsame Kr\u00e4fte spriessen,", "tokens": ["Und", "f\u00fchlt", "im", "al\u00b7ten", "Klang", "selt\u00b7sa\u00b7me", "Kr\u00e4f\u00b7te", "spries\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Das Auge wird uns feucht und Mitleid f\u00fcllt das Herz,", "tokens": ["Das", "Au\u00b7ge", "wird", "uns", "feucht", "und", "Mit\u00b7leid", "f\u00fcllt", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Nach Himmelsfrieden sehnt man sich nach allem Schmerz,", "tokens": ["Nach", "Him\u00b7mels\u00b7frie\u00b7den", "sehnt", "man", "sich", "nach", "al\u00b7lem", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "PRF", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und in phantastischen und fremden Harmonien", "tokens": ["Und", "in", "phan\u00b7tas\u00b7ti\u00b7schen", "und", "frem\u00b7den", "Har\u00b7mo\u00b7ni\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Sieht man im Strom des Klangs tiefs\u00fcsse Bilder ziehn,", "tokens": ["Sieht", "man", "im", "Strom", "des", "Klangs", "tief\u00b7s\u00fcs\u00b7se", "Bil\u00b7der", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "NN", "ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Die unser Herz bestrahlt mit Freude, Licht und Lied,", "tokens": ["Die", "un\u00b7ser", "Herz", "be\u00b7strahlt", "mit", "Freu\u00b7de", ",", "Licht", "und", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Das mit den T\u00f6nen eint die Sonne, die verschied.", "tokens": ["Das", "mit", "den", "T\u00f6\u00b7nen", "eint", "die", "Son\u00b7ne", ",", "die", "ver\u00b7schied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dann ist die Leier fort, und tiefes Schweigen lastet,", "tokens": ["Dann", "ist", "die", "Lei\u00b7er", "fort", ",", "und", "tie\u00b7fes", "Schwei\u00b7gen", "las\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ", "$,", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die tr\u00fcbe Nacht bricht an, die blasse Venus rastet", "tokens": ["Die", "tr\u00fc\u00b7be", "Nacht", "bricht", "an", ",", "die", "blas\u00b7se", "Ve\u00b7nus", "ras\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Fern in der Finsternis auf weichem Wolkenhang,", "tokens": ["Fern", "in", "der", "Fins\u00b7ter\u00b7nis", "auf", "wei\u00b7chem", "Wol\u00b7ken\u00b7hang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Die Gaslaternen gl\u00fchn die H\u00e4userreih'n entlang.", "tokens": ["Die", "Gas\u00b7la\u00b7ter\u00b7nen", "gl\u00fchn", "die", "H\u00e4u\u00b7ser\u00b7reih'n", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und aus dem Strom, der schwarz wie samt'ne Masken, flimmern", "tokens": ["Und", "aus", "dem", "Strom", ",", "der", "schwarz", "wie", "samt'\u00b7ne", "Mas\u00b7ken", ",", "flim\u00b7mern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "KOKOM", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Im Zickzack das Gestirn und all' der Lampen Schimmern.", "tokens": ["Im", "Zick\u00b7zack", "das", "Ge\u00b7stirn", "und", "all'", "der", "Lam\u00b7pen", "Schim\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "PIS", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und der Beschauer am Gel\u00e4nder, das voll Rost", "tokens": ["Und", "der", "Be\u00b7schau\u00b7er", "am", "Ge\u00b7l\u00e4n\u00b7der", ",", "das", "voll", "Rost"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "$,", "PRELS", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "'nem alten Geldst\u00fcck gleich, durch Jahre, Luft und Frost,", "tokens": ["'nem", "al\u00b7ten", "Geld\u00b7st\u00fcck", "gleich", ",", "durch", "Jah\u00b7re", ",", "Luft", "und", "Frost", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Neigt tief zum Abgrund sich, wo finst're Winde schwanken,", "tokens": ["Neigt", "tief", "zum", "Ab\u00b7grund", "sich", ",", "wo", "finst'\u00b7re", "Win\u00b7de", "schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "PRF", "$,", "PWAV", "VVFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und stolzer Ehrgeiz, Hoffnung, heitere Gedanken,", "tokens": ["Und", "stol\u00b7zer", "Ehr\u00b7geiz", ",", "Hoff\u00b7nung", ",", "hei\u00b7te\u00b7re", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Selbst die Erinnerung \u2013 dahin ist alles dies!", "tokens": ["Selbst", "die", "E\u00b7rin\u00b7ne\u00b7rung", "\u2013", "da\u00b7hin", "ist", "al\u00b7les", "dies", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$(", "PAV", "VAFIN", "PIS", "PDS", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.12": {"text": "Und um uns ist die Nacht, das Wasser und Paris!", "tokens": ["Und", "um", "uns", "ist", "die", "Nacht", ",", "das", "Was\u00b7ser", "und", "Pa\u00b7ris", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUI", "PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dreieinigkeit des Fluchs! Grabschl\u00fcnde allzeit offen!", "tokens": ["Drei\u00b7ei\u00b7nig\u00b7keit", "des", "Fluchs", "!", "Grab\u00b7schl\u00fcn\u00b7de", "all\u00b7zeit", "of\u00b7fen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Du Mene-Tekel-Phares f\u00fcr verlor'nes Hoffen!", "tokens": ["Du", "Me\u00b7ne\u00b7Te\u00b7kel\u00b7Pha\u00b7res", "f\u00fcr", "ver\u00b7lor'\u00b7nes", "Hof\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ihr Pforten unsres Grams, ihr drei seid allzumal", "tokens": ["Ihr", "Pfor\u00b7ten", "uns\u00b7res", "Grams", ",", "ihr", "drei", "seid", "all\u00b7zu\u00b7mal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$,", "PPOSAT", "CARD", "VAFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So schrecklich, dass der Mensch, wie trunken von der Qual,", "tokens": ["So", "schreck\u00b7lich", ",", "dass", "der", "Mensch", ",", "wie", "trun\u00b7ken", "von", "der", "Qual", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "ART", "NN", "$,", "PWAV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Mit der ihr ihn durchbohrt und schattenhaft umklammert,", "tokens": ["Mit", "der", "ihr", "ihn", "durch\u00b7bohrt", "und", "schat\u00b7ten\u00b7haft", "um\u00b7klam\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPER", "VVPP", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der Mensch, der wie Orest, doch ohne Schwester jammert,", "tokens": ["Der", "Mensch", ",", "der", "wie", "O\u00b7rest", ",", "doch", "oh\u00b7ne", "Schwes\u00b7ter", "jam\u00b7mert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "KOKOM", "NE", "$,", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Dem schicksalsfinstren Blick nicht l\u00e4nger widersteht", "tokens": ["Dem", "schick\u00b7sals\u00b7finst\u00b7ren", "Blick", "nicht", "l\u00e4n\u00b7ger", "wi\u00b7der\u00b7steht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und machtlos g'radeaus zum graus'gen Abgrund geht.", "tokens": ["Und", "macht\u00b7los", "g'\u00b7ra\u00b7deaus", "zum", "graus'\u00b7gen", "Ab\u00b7grund", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und alle drei wetteifert ihr in eurem W\u00fcten,", "tokens": ["Und", "al\u00b7le", "drei", "wett\u00b7ei\u00b7fert", "ihr", "in", "eu\u00b7rem", "W\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "CARD", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Dem grossen Wurme tote Br\u00e4ute darzubieten,", "tokens": ["Dem", "gros\u00b7sen", "Wur\u00b7me", "to\u00b7te", "Br\u00e4u\u00b7te", "dar\u00b7zu\u00b7bie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Dass man nicht weiss, wer von den Dreien uns verdirbt,", "tokens": ["Dass", "man", "nicht", "weiss", ",", "wer", "von", "den", "Drei\u00b7en", "uns", "ver\u00b7dirbt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "VVFIN", "$,", "PWS", "APPR", "ART", "CARD", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.24": {"text": "Und dass man lieber noch im grausen Dunkel stirbt,", "tokens": ["Und", "dass", "man", "lie\u00b7ber", "noch", "im", "grau\u00b7sen", "Dun\u00b7kel", "stirbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Als in dem tauben Strom, dem eis'gen Flutenbette,", "tokens": ["Als", "in", "dem", "tau\u00b7ben", "Strom", ",", "dem", "eis'\u00b7gen", "Flu\u00b7ten\u00b7bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Oder in deinem Lug, Paris, Herrin der St\u00e4dte!", "tokens": ["O\u00b7der", "in", "dei\u00b7nem", "Lug", ",", "Pa\u00b7ris", ",", "Her\u00b7rin", "der", "St\u00e4d\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$,", "NE", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und ewig, Seine, rollst du weiter, ewig ziehst", "tokens": ["Und", "e\u00b7wig", ",", "Sei\u00b7ne", ",", "rollst", "du", "wei\u00b7ter", ",", "e\u00b7wig", "ziehst"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "$,", "PPOSAT", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Du durch Paris den Strom, der grau und schmutzig fliesst,", "tokens": ["Du", "durch", "Pa\u00b7ris", "den", "Strom", ",", "der", "grau", "und", "schmut\u00b7zig", "fliesst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NE", "ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Hinw\u00e4lzend in der Flut, der tr\u00fcben, schlangengleichen,", "tokens": ["Hin\u00b7w\u00e4l\u00b7zend", "in", "der", "Flut", ",", "der", "tr\u00fc\u00b7ben", ",", "schlan\u00b7gen\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die Ladungen von Holz, von Kohle und von Leichen!", "tokens": ["Die", "La\u00b7dun\u00b7gen", "von", "Holz", ",", "von", "Koh\u00b7le", "und", "von", "Lei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}