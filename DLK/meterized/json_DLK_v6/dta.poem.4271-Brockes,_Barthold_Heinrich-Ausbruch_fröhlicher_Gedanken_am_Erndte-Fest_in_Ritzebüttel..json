{"dta.poem.4271": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ausbruch fr\u00f6hlicher Gedanken  \n am  \n  Erndte-Fest in Ritzeb\u00fcttel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nachdem es in die dreyzehn Wochen ein feuchtes Wetter", "tokens": ["Nach\u00b7dem", "es", "in", "die", "drey\u00b7zehn", "Wo\u00b7chen", "ein", "feuch\u00b7tes", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "CARD", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "war gewesen,", "tokens": ["war", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VAPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und t\u00e4glich fast geregnet hatte, erblickte man der Sonnen", "tokens": ["Und", "t\u00e4g\u00b7lich", "fast", "ge\u00b7reg\u00b7net", "hat\u00b7te", ",", "er\u00b7blick\u00b7te", "man", "der", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "VVPP", "VAFIN", "$,", "VVFIN", "PIS", "ART", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Pracht,", "tokens": ["Pracht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Nach einer stillen, heitern Nacht,", "tokens": ["Nach", "ei\u00b7ner", "stil\u00b7len", ",", "hei\u00b7tern", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Am Tage, der, in unserm Lande, zum frohen Erndte-Fest", "tokens": ["Am", "Ta\u00b7ge", ",", "der", ",", "in", "un\u00b7serm", "Lan\u00b7de", ",", "zum", "fro\u00b7hen", "Ernd\u00b7te\u00b7Fest"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PRELS", "$,", "APPR", "PPOSAT", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "erlesen,", "tokens": ["er\u00b7le\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "In einem ganz entw\u00f6lkten Glanz, der uns um desto mehr", "tokens": ["In", "ei\u00b7nem", "ganz", "ent\u00b7w\u00f6lk\u00b7ten", "Glanz", ",", "der", "uns", "um", "des\u00b7to", "mehr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$,", "PRELS", "PPER", "APPR", "ADV", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.9": {"text": "noch r\u00fchrte,", "tokens": ["noch", "r\u00fchr\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Als man darinn, fast sichtbarlich, den Himmel uns gewogen", "tokens": ["Als", "man", "da\u00b7rinn", ",", "fast", "sicht\u00b7bar\u00b7lich", ",", "den", "Him\u00b7mel", "uns", "ge\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PAV", "$,", "ADV", "ADJD", "$,", "ART", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.11": {"text": "sp\u00fchrte.", "tokens": ["sp\u00fchr\u00b7te", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Es brachte dieses mich zum Andacht- vollen Singen,", "tokens": ["Es", "brach\u00b7te", "die\u00b7ses", "mich", "zum", "An\u00b7dacht", "vol\u00b7len", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "PRF", "APPRART", "TRUNC", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und lie\u00df ich, GOtt zum Dank und Ruhm, voll Freuden,", "tokens": ["Und", "lie\u00df", "ich", ",", "Gott", "zum", "Dank", "und", "Ruhm", ",", "voll", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "NN", "APPRART", "NN", "KON", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "dieses Lied erklingen:", "tokens": ["die\u00b7ses", "Lied", "er\u00b7klin\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Mein Geist, an diesem frohen Fest,", "tokens": ["Mein", "Geist", ",", "an", "die\u00b7sem", "fro\u00b7hen", "Fest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das GOtt, in unsrer Lust, zu Seinem Ruhm, beginnen,", "tokens": ["Das", "Gott", ",", "in", "uns\u00b7rer", "Lust", ",", "zu", "Sei\u00b7nem", "Ruhm", ",", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und, in der Erndt\u2019, uns feyren l\u00e4\u00dft,", "tokens": ["Und", ",", "in", "der", "Erndt'", ",", "uns", "fey\u00b7ren", "l\u00e4\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "$,", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df, inniglich ger\u00fchrt, durch alle Sinnen,", "tokens": ["La\u00df", ",", "in\u00b7nig\u00b7lich", "ge\u00b7r\u00fchrt", ",", "durch", "al\u00b7le", "Sin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADJD", "VVPP", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Inbrunst solchen Grad gewinnen,", "tokens": ["Die", "In\u00b7brunst", "sol\u00b7chen", "Grad", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Damit auch Freuden-Thr\u00e4nen rinnen,", "tokens": ["Da\u00b7mit", "auch", "Freu\u00b7den\u00b7Thr\u00e4\u00b7nen", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Von Lust und Ehrfurcht ausgepre\u00dft!", "tokens": ["Von", "Lust", "und", "Ehr\u00b7furcht", "aus\u00b7ge\u00b7pre\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es f\u00fclle heute nichts die frohe Brust,", "tokens": ["Es", "f\u00fcl\u00b7le", "heu\u00b7te", "nichts", "die", "fro\u00b7he", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Als eine Gott geweihte Lust,", "tokens": ["Als", "ei\u00b7ne", "Gott", "ge\u00b7weih\u00b7te", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Entbrannt von reinen Andachts-Flammen!", "tokens": ["Ent\u00b7brannt", "von", "rei\u00b7nen", "An\u00b7dachts\u00b7Flam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zieh\u2019 deine ganze Kraft zusammen!", "tokens": ["Zieh'", "dei\u00b7ne", "gan\u00b7ze", "Kraft", "zu\u00b7sam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Empfind\u2019 heut anders keine Triebe,", "tokens": ["Emp\u00b7find'", "heut", "an\u00b7ders", "kei\u00b7ne", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als die des grossen Sch\u00f6pfers Liebe,", "tokens": ["Als", "die", "des", "gros\u00b7sen", "Sch\u00f6p\u00b7fers", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In Seinen uns geschenkten Gaben,", "tokens": ["In", "Sei\u00b7nen", "uns", "ge\u00b7schenk\u00b7ten", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zur Absicht und zum Vorwurf haben.", "tokens": ["Zur", "Ab\u00b7sicht", "und", "zum", "Vor\u00b7wurf", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So dacht\u2019 ich, als mich fr\u00fch\u2019 ein\u2019 ungemeine Freude,", "tokens": ["So", "dacht'", "ich", ",", "als", "mich", "fr\u00fch'", "ein'", "un\u00b7ge\u00b7mei\u00b7ne", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die sehr gew\u00fcnscht, und kaum gehoffet war,", "tokens": ["Die", "sehr", "ge\u00b7w\u00fcnscht", ",", "und", "kaum", "ge\u00b7hof\u00b7fet", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "$,", "KON", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durch schnell ver\u00e4nderte erseufzte Witterung,", "tokens": ["Durch", "schnell", "ver\u00b7\u00e4n\u00b7der\u00b7te", "er\u00b7seufz\u00b7te", "Wit\u00b7te\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In unser aller Herzen drung.", "tokens": ["In", "un\u00b7ser", "al\u00b7ler", "Her\u00b7zen", "drung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "PIAT", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nach langem Regen ist nunmehr der Himmel klar,", "tokens": ["Nach", "lan\u00b7gem", "Re\u00b7gen", "ist", "nun\u00b7mehr", "der", "Him\u00b7mel", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und stellet uns die Segen-reiche Welt,", "tokens": ["Und", "stel\u00b7let", "uns", "die", "Se\u00b7gen\u00b7rei\u00b7che", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durch ihren Freuden- reichen Strahl, im angenehmen", "tokens": ["Durch", "ih\u00b7ren", "Freu\u00b7den", "rei\u00b7chen", "Strahl", ",", "im", "an\u00b7ge\u00b7neh\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "TRUNC", "ADJA", "NN", "$,", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schimmer dar,", "tokens": ["Schim\u00b7mer", "dar", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Der uns, indem er neu, um so viel lieber war,", "tokens": ["Der", "uns", ",", "in\u00b7dem", "er", "neu", ",", "um", "so", "viel", "lie\u00b7ber", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "KOUS", "PPER", "ADJD", "$,", "KOUI", "ADV", "ADV", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So mich, da alles uns im hellen Glanz anlachte,", "tokens": ["So", "mich", ",", "da", "al\u00b7les", "uns", "im", "hel\u00b7len", "Glanz", "an\u00b7lach\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "KOUS", "PIS", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn fast so dreiste machte,", "tokens": ["Denn", "fast", "so", "dreis\u00b7te", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Es als ein Zeichen anzuseh\u2019n,", "tokens": ["Es", "als", "ein", "Zei\u00b7chen", "an\u00b7zu\u00b7seh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Als ob das, was von uns, zu GOttes Ehr\u2019,", "tokens": ["Als", "ob", "das", ",", "was", "von", "uns", ",", "zu", "Got\u00b7tes", "Ehr'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PDS", "$,", "PRELS", "APPR", "PPER", "$,", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "An diesem Tag f\u00fcr Ordnungen gescheh\u2019n,", "tokens": ["An", "die\u00b7sem", "Tag", "f\u00fcr", "Ord\u00b7nun\u00b7gen", "ge\u00b7scheh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Dem Sch\u00f6pfer nicht zuwider w\u00e4r\u2019;", "tokens": ["Dem", "Sch\u00f6p\u00b7fer", "nicht", "zu\u00b7wi\u00b7der", "w\u00e4r'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Allein ich zog, wie billig, die Gedanken", "tokens": ["Al\u00b7lein", "ich", "zog", ",", "wie", "bil\u00b7lig", ",", "die", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "$,", "PWAV", "ADJD", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "In Demuhts-volle Schranken,", "tokens": ["In", "Dem\u00b7uhts\u00b7vol\u00b7le", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Aus Furcht, es m\u00f6chte die\u00df aus Eigen-Liebe mehr,", "tokens": ["Aus", "Furcht", ",", "es", "m\u00f6ch\u00b7te", "die\u00df", "aus", "Ei\u00b7gen\u00b7Lie\u00b7be", "mehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPER", "VMFIN", "PDS", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Als aus gegr\u00fcndeter Versicherung, entstehen.", "tokens": ["Als", "aus", "ge\u00b7gr\u00fcn\u00b7de\u00b7ter", "Ver\u00b7si\u00b7che\u00b7rung", ",", "ent\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Doch hoff\u2019 ich wenigstens, es werd\u2019 auch meine Freude,", "tokens": ["Doch", "hoff'", "ich", "we\u00b7nigs\u00b7tens", ",", "es", "werd'", "auch", "mei\u00b7ne", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ob der so sch\u00f6nen Augen-Weide,", "tokens": ["Ob", "der", "so", "sch\u00f6\u00b7nen", "Au\u00b7gen\u00b7Wei\u00b7de", ","], "