{"textgrid.poem.39989": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich sahe j\u00fcngst, mit fast erstaunten Blicken,", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sahe j\u00fcngst, mit fast erstaunten Blicken,", "tokens": ["Ich", "sa\u00b7he", "j\u00fcngst", ",", "mit", "fast", "er\u00b7staun\u00b7ten", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Sonn' im Garten, nach dem Regen,", "tokens": ["Die", "Sonn'", "im", "Gar\u00b7ten", ",", "nach", "dem", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Bluhmen Heer mit heitern Strahlen schm\u00fccken,", "tokens": ["Der", "Bluh\u00b7men", "Heer", "mit", "hei\u00b7tern", "Strah\u00b7len", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ihren reinen Glantz in nasse Bl\u00e4tter pr\u00e4gen.", "tokens": ["Und", "ih\u00b7ren", "rei\u00b7nen", "Glantz", "in", "nas\u00b7se", "Bl\u00e4t\u00b7ter", "pr\u00e4\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Indem mein Auge nun, durch ihre Zahl verwirrt,", "tokens": ["In\u00b7dem", "mein", "Au\u00b7ge", "nun", ",", "durch", "ih\u00b7re", "Zahl", "ver\u00b7wirrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "$,", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch ihren Schmuck entz\u00fcckt, von der zu jener irrt,", "tokens": ["Durch", "ih\u00b7ren", "Schmuck", "ent\u00b7z\u00fcckt", ",", "von", "der", "zu", "je\u00b7ner", "irrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,", "APPR", "ART", "APPR", "PDAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der spielenden Natur gef\u00e4rbtes Kleid betrachtet,", "tokens": ["Der", "spie\u00b7len\u00b7den", "Na\u00b7tur", "ge\u00b7f\u00e4rb\u00b7tes", "Kleid", "be\u00b7trach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bald die, bald jene, h\u00f6her achtet;", "tokens": ["Bald", "die", ",", "bald", "je\u00b7ne", ",", "h\u00f6\u00b7her", "ach\u00b7tet", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "ADV", "PDS", "$,", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sich bald an dieser hier, und bald an der, erg\u00f6tzet;", "tokens": ["Sich", "bald", "an", "die\u00b7ser", "hier", ",", "und", "bald", "an", "der", ",", "er\u00b7g\u00f6t\u00b7zet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "PDAT", "ADV", "$,", "KON", "ADV", "APPR", "ART", "$,", "VVFIN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Bald beyde gleiche sch\u00f6n, bald die noch sch\u00f6ner, sch\u00e4tzet;", "tokens": ["Bald", "bey\u00b7de", "glei\u00b7che", "sch\u00f6n", ",", "bald", "die", "noch", "sch\u00f6\u00b7ner", ",", "sch\u00e4t\u00b7zet", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "ADJD", "$,", "ADV", "ART", "ADV", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Reisst endlich Augen, Hertz und Sinn", "tokens": ["Reisst", "end\u00b7lich", "Au\u00b7gen", ",", "Hertz", "und", "Sinn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Rosen-Busch auf sich nur eintzig hin.", "tokens": ["Ein", "Ro\u00b7sen\u00b7Busch", "auf", "sich", "nur", "eint\u00b7zig", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich seh' ihn kaum aufmercksam in der N\u00e4he;", "tokens": ["Ich", "seh'", "ihn", "kaum", "auf\u00b7merck\u00b7sam", "in", "der", "N\u00e4\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So deucht mich, als ob ich in seiner Zier", "tokens": ["So", "deucht", "mich", ",", "als", "ob", "ich", "in", "sei\u00b7ner", "Zier"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOKOM", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nichts Irdisches, nein, gar aus Edens Lust-Revier", "tokens": ["Nichts", "Ir\u00b7di\u00b7sches", ",", "nein", ",", "gar", "aus", "E\u00b7dens", "Lust\u00b7Re\u00b7vier"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "ADJA", "$,", "PTKANT", "$,", "ADV", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Annoch ein Ueberbleibsel, s\u00e4he.", "tokens": ["An\u00b7noch", "ein", "Ue\u00b7ber\u00b7bleib\u00b7sel", ",", "s\u00e4\u00b7he", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}