{"textgrid.poem.44438": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Bei der Geburt eines Prinzen", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du eines guten Mannes gute Tochter", "tokens": ["Du", "ei\u00b7nes", "gu\u00b7ten", "Man\u00b7nes", "gu\u00b7te", "Toch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und eines frommen Kaisers Schwiegerkind,", "tokens": ["Und", "ei\u00b7nes", "from\u00b7men", "Kai\u00b7sers", "Schwie\u00b7ger\u00b7kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So windest du dich, jammernd um Erbarmen,", "tokens": ["So", "win\u00b7dest", "du", "dich", ",", "jam\u00b7mernd", "um", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und bebt dein Leib von ahnungsvollem Weh?", "tokens": ["Und", "bebt", "dein", "Leib", "von", "ah\u00b7nungs\u00b7vol\u00b7lem", "Weh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sind das denn nicht die Hallen der C\u00e4saren,", "tokens": ["Sind", "das", "denn", "nicht", "die", "Hal\u00b7len", "der", "C\u00e4\u00b7sa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Pol-Stern eines sturmbedr\u00e4ngten Volks,", "tokens": ["Der", "Pol\u00b7S\u00b7tern", "ei\u00b7nes", "sturm\u00b7be\u00b7dr\u00e4ng\u00b7ten", "Volks", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von wo aus donnernd die Geschicke fahren,", "tokens": ["Von", "wo", "aus", "don\u00b7nernd", "die", "Ge\u00b7schi\u00b7cke", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAV", "APPR", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die blind erwartend hinnimmt eine Welt?", "tokens": ["Die", "blind", "er\u00b7war\u00b7tend", "hin\u00b7nimmt", "ei\u00b7ne", "Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und fand der Schmerz in diese Herrschermauern,", "tokens": ["Und", "fand", "der", "Schmerz", "in", "die\u00b7se", "Herr\u00b7scher\u00b7mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In diese Herrscherglieder einen Weg?", "tokens": ["In", "die\u00b7se", "Herr\u00b7scher\u00b7glie\u00b7der", "ei\u00b7nen", "Weg", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und, leichthingleitend ob des Fr\u00f6ners Weibe,", "tokens": ["Und", ",", "leich\u00b7thing\u00b7lei\u00b7tend", "ob", "des", "Fr\u00f6\u00b7ners", "Wei\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "KOUS", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Tritt er dich an und ruft: sei Mensch und leid!", "tokens": ["Tritt", "er", "dich", "an", "und", "ruft", ":", "sei", "Mensch", "und", "leid", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKVZ", "KON", "VVFIN", "$.", "VAFIN", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So widerspricht sich also sehr der Himmel?", "tokens": ["So", "wi\u00b7der\u00b7spricht", "sich", "al\u00b7so", "sehr", "der", "Him\u00b7mel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und die er ausnimmt vom gemeinen Los,", "tokens": ["Und", "die", "er", "aus\u00b7nimmt", "vom", "ge\u00b7mei\u00b7nen", "Los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df sie nicht irren oder doch nicht fehlen,", "tokens": ["Da\u00df", "sie", "nicht", "ir\u00b7ren", "o\u00b7der", "doch", "nicht", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "KON", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wenn auch fehlen, nimmer sich vergehn,", "tokens": ["Und", "wenn", "auch", "feh\u00b7len", ",", "nim\u00b7mer", "sich", "ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "VVINF", "$,", "ADV", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und wenn vergehn, sie selbst kein Tadel richtet,", "tokens": ["Und", "wenn", "ver\u00b7gehn", ",", "sie", "selbst", "kein", "Ta\u00b7del", "rich\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "VVINF", "$,", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bis einst, statt Pairs, als Gleicher \u00fcber Gleichen,", "tokens": ["Bis", "einst", ",", "statt", "Pairs", ",", "als", "Glei\u00b7cher", "\u00fc\u00b7ber", "Glei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "KOUI", "NN", "$,", "KOUS", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein einziger Geschworner sie verdammet: Gott.", "tokens": ["Ein", "ein\u00b7zi\u00b7ger", "Ge\u00b7schwor\u00b7ner", "sie", "ver\u00b7dam\u00b7met", ":", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$.", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und widerspricht sich also sehr der Himmel,", "tokens": ["Und", "wi\u00b7der\u00b7spricht", "sich", "al\u00b7so", "sehr", "der", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df, ob von Ewigkeit und Gottes Gnaden", "tokens": ["Da\u00df", ",", "ob", "von", "E\u00b7wig\u00b7keit", "und", "Got\u00b7tes", "Gna\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "APPR", "NN", "KON", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erkoren, recht zu tun und recht zu haben,", "tokens": ["Er\u00b7ko\u00b7ren", ",", "recht", "zu", "tun", "und", "recht", "zu", "ha\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "PTKZU", "VVINF", "KON", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie doch der Menschheit Los, das irren hei\u00dft und leiden,", "tokens": ["Sie", "doch", "der", "Menschheit", "Los", ",", "das", "ir\u00b7ren", "hei\u00dft", "und", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "NE", "$,", "PDS", "VVINF", "VVFIN", "KON", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Nur halb verschont mit seiner Fl\u00fcche beiden,", "tokens": ["Nur", "halb", "ver\u00b7schont", "mit", "sei\u00b7ner", "Fl\u00fc\u00b7che", "bei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "APPR", "PPOSAT", "NN", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und sie, befreit von Rechenschaft und Wahl,", "tokens": ["Und", "sie", ",", "be\u00b7freit", "von", "Re\u00b7chen\u00b7schaft", "und", "Wahl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Dr\u00e4nger Schmerz heimsucht mit seiner Qual?", "tokens": ["Der", "Dr\u00e4n\u00b7ger", "Schmerz", "heim\u00b7sucht", "mit", "sei\u00b7ner", "Qual", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O schwach und falsch! F\u00fcrwahr, ein festres Merkmal", "tokens": ["O", "schwach", "und", "falsch", "!", "F\u00fcr\u00b7wahr", ",", "ein", "fest\u00b7res", "Merk\u00b7mal"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "KON", "ADJD", "$.", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Tat not, um zu beglaubigen der Welt", "tokens": ["Tat", "not", ",", "um", "zu", "be\u00b7glau\u00b7bi\u00b7gen", "der", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "KOUI", "PTKZU", "VVINF", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die auserkornen Lenker der Geschicke.", "tokens": ["Die", "au\u00b7ser\u00b7kor\u00b7nen", "Len\u00b7ker", "der", "Ge\u00b7schi\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie einst Alcid und jene G\u00f6tters\u00f6hne", "tokens": ["Wie", "einst", "Al\u00b7cid", "und", "je\u00b7ne", "G\u00f6t\u00b7ter\u00b7s\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "NE", "KON", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mu\u00dft ein verkl\u00e4rter Leib im Mark der Kraft", "tokens": ["Mu\u00dft", "ein", "ver\u00b7kl\u00e4r\u00b7ter", "Leib", "im", "Mark", "der", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Umkleiden wie ein Purpur ihr Verm\u00f6gen,", "tokens": ["Um\u00b7klei\u00b7den", "wie", "ein", "Pur\u00b7pur", "ihr", "Ver\u00b7m\u00f6\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "In langen Doppeln\u00e4chten stark erzeugt", "tokens": ["In", "lan\u00b7gen", "Dop\u00b7pel\u00b7n\u00e4ch\u00b7ten", "stark", "er\u00b7zeugt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und freudig an das Licht der Welt geboren,", "tokens": ["Und", "freu\u00b7dig", "an", "das", "Licht", "der", "Welt", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "War eines \u00d6ta Brand, ein Donnerkeil", "tokens": ["War", "ei\u00b7nes", "\u00d6\u00b7ta", "Brand", ",", "ein", "Don\u00b7ner\u00b7keil"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der einzge R\u00fcckweg aufw\u00e4rts zu den Ahnen,", "tokens": ["Der", "einz\u00b7ge", "R\u00fcck\u00b7weg", "auf\u00b7w\u00e4rts", "zu", "den", "Ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Der w\u00fcrdig ihres Laufs und ihres Stamms.", "tokens": ["Der", "w\u00fcr\u00b7dig", "ih\u00b7res", "Laufs", "und", "ih\u00b7res", "Stamms", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und, arme Mutter neurer G\u00f6tters\u00f6hne,", "tokens": ["Und", ",", "ar\u00b7me", "Mut\u00b7ter", "neu\u00b7rer", "G\u00f6t\u00b7ter\u00b7s\u00f6h\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Liegst wimmernd du mit halbzerfleischtem Leibe,", "tokens": ["Liegst", "wim\u00b7mernd", "du", "mit", "halb\u00b7zer\u00b7fleischtem", "Lei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "F\u00fchlst dich, halb sterbend, gleich des letzten Bettlers Weibe?", "tokens": ["F\u00fchlst", "dich", ",", "halb", "ster\u00b7bend", ",", "gleich", "des", "letz\u00b7ten", "Bett\u00b7lers", "Wei\u00b7be", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJD", "VVPP", "$,", "ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und dieses Kind, das deinem Scho\u00df entsprie\u00dfet,", "tokens": ["Und", "die\u00b7ses", "Kind", ",", "das", "dei\u00b7nem", "Scho\u00df", "ent\u00b7sprie\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wird es nicht wimmern und nach Nahrung weinen?", "tokens": ["Wird", "es", "nicht", "wim\u00b7mern", "und", "nach", "Nah\u00b7rung", "wei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "VVINF", "KON", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht spielen? und du wirst sein Fehl bestrafen.", "tokens": ["Nicht", "spie\u00b7len", "?", "und", "du", "wirst", "sein", "Fehl", "be\u00b7stra\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$.", "KON", "PPER", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Selbst an das Ziel der M\u00fcndigkeit gelangt,", "tokens": ["Selbst", "an", "das", "Ziel", "der", "M\u00fcn\u00b7dig\u00b7keit", "ge\u00b7langt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wird er im Unrecht sein, sooft er anders will", "tokens": ["Wird", "er", "im", "Un\u00b7recht", "sein", ",", "sooft", "er", "an\u00b7ders", "will"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VAINF", "$,", "VVFIN", "PPER", "ADV", "VMFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und anders denkt als sein gekr\u00f6nter Vater.", "tokens": ["Und", "an\u00b7ders", "denkt", "als", "sein", "ge\u00b7kr\u00f6n\u00b7ter", "Va\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "KOKOM", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Erst an der Gruft einst dessen, der ihn zeugte,", "tokens": ["E