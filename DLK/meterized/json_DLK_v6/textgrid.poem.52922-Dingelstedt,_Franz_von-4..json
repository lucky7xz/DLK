{"textgrid.poem.52922": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf diesen Felsen m\u00f6cht' ich H\u00fctten baun,", "tokens": ["Auf", "die\u00b7sen", "Fel\u00b7sen", "m\u00f6cht'", "ich", "H\u00fct\u00b7ten", "baun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein treuer Gast dem abgeschiednen Eiland,", "tokens": ["Ein", "treu\u00b7er", "Gast", "dem", "ab\u00b7ge\u00b7schied\u00b7nen", "Ei\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht um nach S\u00fcden, heimatw\u00e4rts, zu schaun,", "tokens": ["Nicht", "um", "nach", "S\u00fc\u00b7den", ",", "hei\u00b7mat\u00b7w\u00e4rts", ",", "zu", "schaun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "KOUI", "APPR", "NN", "$,", "ADV", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So wie gen Ithaka der Dulder weiland,", "tokens": ["So", "wie", "gen", "I\u00b7tha\u00b7ka", "der", "Dul\u00b7der", "wei\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "NE", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nein, um des Festlands d\u00fcrres Einerlei", "tokens": ["Nein", ",", "um", "des", "Fest\u00b7lands", "d\u00fcr\u00b7res", "Ei\u00b7ner\u00b7lei"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUI", "ART", "NN", "ADJA", "NN"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.6": {"text": "Im Meereshauch auf ewig zu vergessen;", "tokens": ["Im", "Mee\u00b7res\u00b7hauch", "auf", "e\u00b7wig", "zu", "ver\u00b7ges\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Hier weht das Banner Albions, und frei", "tokens": ["Hier", "weht", "das", "Ban\u00b7ner", "Al\u00b7bions", ",", "und", "frei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NE", "$,", "KON", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Hat hier von je ein freies Volk gesessen.", "tokens": ["Hat", "hier", "von", "je", "ein", "frei\u00b7es", "Volk", "ge\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "La\u00dft mich willkomm an Eurem Herde sein,", "tokens": ["La\u00dft", "mich", "will\u00b7komm", "an", "Eu\u00b7rem", "Her\u00b7de", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als B\u00fcrger gr\u00fc\u00dfe jeder mich, als Bruder,", "tokens": ["Als", "B\u00fcr\u00b7ger", "gr\u00fc\u00b7\u00dfe", "je\u00b7der", "mich", ",", "als", "Bru\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PIS", "PPER", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Legt in die schwache Rechte mir hinein", "tokens": ["Legt", "in", "die", "schwa\u00b7che", "Rech\u00b7te", "mir", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Statt eines Wanderstabs ein t\u00fcchtig Ruder,", "tokens": ["Statt", "ei\u00b7nes", "Wan\u00b7der\u00b7stabs", "ein", "t\u00fcch\u00b7tig", "Ru\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Lehrt auf den D\u00fcnen mich den Robbenfang", "tokens": ["Lehrt", "auf", "den", "D\u00fc\u00b7nen", "mich", "den", "Rob\u00b7ben\u00b7fang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und andre Kiel' als G\u00e4nsekiele f\u00fchren;", "tokens": ["Und", "and\u00b7re", "Kiel'", "als", "G\u00e4n\u00b7se\u00b7kie\u00b7le", "f\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "M\u00fcd' war ich's, beim Allm\u00e4chtigen, schon lang,", "tokens": ["M\u00fcd'", "war", "ich's", ",", "beim", "All\u00b7m\u00e4ch\u00b7ti\u00b7gen", ",", "schon", "lang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PIS", "$,", "APPRART", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sie t\u00e4glich sonder Ziel und Rast zu r\u00fchren!", "tokens": ["Sie", "t\u00e4g\u00b7lich", "son\u00b7der", "Ziel", "und", "Rast", "zu", "r\u00fch\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gib mir die Hand, du sch\u00f6nes Fischerkind,", "tokens": ["Gib", "mir", "die", "Hand", ",", "du", "sch\u00f6\u00b7nes", "Fi\u00b7scher\u00b7kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sei du mein Weib, mein Engel, meine Muse,", "tokens": ["Sei", "du", "mein", "Weib", ",", "mein", "En\u00b7gel", ",", "mei\u00b7ne", "Mu\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf da\u00df ich werde, was die Deinen sind,", "tokens": ["Auf", "da\u00df", "ich", "wer\u00b7de", ",", "was", "die", "Dei\u00b7nen", "sind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VAFIN", "$,", "PRELS", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein wackrer Lotsen-Mann in blauer Bluse;", "tokens": ["Ein", "wack\u00b7rer", "Lot\u00b7sen\u00b7Mann", "in", "blau\u00b7er", "Blu\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Streich mir die alten Falten von der Stirn", "tokens": ["Streich", "mir", "die", "al\u00b7ten", "Fal\u00b7ten", "von", "der", "Stirn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und die Gedanken-Runzeln aus den Brauen,", "tokens": ["Und", "die", "Ge\u00b7dan\u00b7ken\u00b7Run\u00b7zeln", "aus", "den", "Brau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Fortan soll nur dein Ku\u00df, du schmucke Dirn',", "tokens": ["For\u00b7tan", "soll", "nur", "dein", "Ku\u00df", ",", "du", "schmu\u00b7cke", "Dirn'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADV", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und Arbeitsschwei\u00df auf diesen Schl\u00e4fen tauen.", "tokens": ["Und", "Ar\u00b7beits\u00b7schwei\u00df", "auf", "die\u00b7sen", "Schl\u00e4\u00b7fen", "tau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Hinein ins Bad! des Staubes letzten Rest,", "tokens": ["Hin\u00b7ein", "ins", "Bad", "!", "des", "Stau\u00b7bes", "letz\u00b7ten", "Rest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NE", "$.", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ihn hinweg der Schaum der Welle sp\u00fcle!", "tokens": ["Da\u00df", "ihn", "hin\u00b7weg", "der", "Schaum", "der", "Wel\u00b7le", "sp\u00fc\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie dehnt die Brust, so enge, so gepre\u00dft,", "tokens": ["Wie", "dehnt", "die", "Brust", ",", "so", "en\u00b7ge", ",", "so", "ge\u00b7pre\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "ADV", "ADJA", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sich selig aus in dieses Morgens K\u00fchle!", "tokens": ["Sich", "se\u00b7lig", "aus", "in", "die\u00b7ses", "Mor\u00b7gens", "K\u00fch\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "APPR", "PDAT", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Den alten Adam tauch' ich opfernd ein,", "tokens": ["Den", "al\u00b7ten", "A\u00b7dam", "tauch'", "ich", "op\u00b7fernd", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Du, weihe, Meer, mich selbst zum neuen Lose,", "tokens": ["Du", ",", "wei\u00b7he", ",", "Meer", ",", "mich", "selbst", "zum", "neu\u00b7en", "Lo\u00b7se", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "$,", "NN", "$,", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "La\u00df mich gesund und dein auf ewig sein,", "tokens": ["La\u00df", "mich", "ge\u00b7sund", "und", "dein", "auf", "e\u00b7wig", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "KON", "PPOSAT", "APPR", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wenn ich entsteige deinem Mutterscho\u00dfe!", "tokens": ["Wenn", "ich", "ent\u00b7stei\u00b7ge", "dei\u00b7nem", "Mut\u00b7ter\u00b7scho\u00b7\u00dfe", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}