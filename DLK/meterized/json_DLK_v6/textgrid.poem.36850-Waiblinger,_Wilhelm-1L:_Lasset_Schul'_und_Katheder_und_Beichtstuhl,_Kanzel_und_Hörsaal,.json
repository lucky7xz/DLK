{"textgrid.poem.36850": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lasset Schul' und Katheder und Beichtstuhl, Kanzel und H\u00f6rsaal,", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lasset Schul' und Katheder und Beichtstuhl, Kanzel und H\u00f6rsaal,", "tokens": ["Las\u00b7set", "Schul'", "und", "Ka\u00b7the\u00b7der", "und", "Beicht\u00b7stuhl", ",", "Kan\u00b7zel", "und", "H\u00f6r\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "B\u00fccher und Bibliothek, la\u00dft sie und h\u00f6ret mich an.", "tokens": ["B\u00fc\u00b7cher", "und", "Bib\u00b7lio\u00b7thek", ",", "la\u00dft", "sie", "und", "h\u00f6\u00b7ret", "mich", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "VVIMP", "PPER", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+-++--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Eifrer der Fr\u00f6mmigkeit, euch preis' ich Siciliens Frauen;", "tokens": ["Eif\u00b7rer", "der", "Fr\u00f6m\u00b7mig\u00b7keit", ",", "euch", "preis'", "ich", "Si\u00b7ci\u00b7li\u00b7ens", "Frau\u00b7en", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "NE", "NN", "$."], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Denn ein herrlich Geschlecht schm\u00fccket Trinakrien noch.", "tokens": ["Denn", "ein", "herr\u00b7lich", "Ge\u00b7schlecht", "schm\u00fc\u00b7cket", "Tri\u00b7na\u00b7kri\u00b7en", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "NN", "VVFIN", "NE", "ADV", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "Fremden gef\u00e4llig, von l\u00fcsternem Geist, von feurigen Sinnen,", "tokens": ["Frem\u00b7den", "ge\u00b7f\u00e4l\u00b7lig", ",", "von", "l\u00fcs\u00b7ter\u00b7nem", "Geist", ",", "von", "feu\u00b7ri\u00b7gen", "Sin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Wer verm\u00f6cht' euch darum, T\u00f6chter von Zankle, zu schm\u00e4hn?", "tokens": ["Wer", "ver\u00b7m\u00f6cht'", "euch", "da\u00b7rum", ",", "T\u00f6ch\u00b7ter", "von", "Zank\u00b7le", ",", "zu", "schm\u00e4hn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PAV", "$,", "NN", "APPR", "NE", "$,", "PTKZU", "VVINF", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Zarte Kinder, von blondem Gelock, blaugl\u00e4nzenden Augen", "tokens": ["Zar\u00b7te", "Kin\u00b7der", ",", "von", "blon\u00b7dem", "Ge\u00b7lock", ",", "blau\u00b7gl\u00e4n\u00b7zen\u00b7den", "Au\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Bietet Catania dir, bietet die freundliche dar.", "tokens": ["Bie\u00b7tet", "Ca\u00b7ta\u00b7nia", "dir", ",", "bie\u00b7tet", "die", "freund\u00b7li\u00b7che", "dar", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPER", "$,", "VVFIN", "ART", "ADJA", "PTKVZ", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.9": {"text": "Zwar kaum hatte den goldenen West, den reinen, die erste", "tokens": ["Zwar", "kaum", "hat\u00b7te", "den", "gol\u00b7de\u00b7nen", "West", ",", "den", "rei\u00b7nen", ",", "die", "ers\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,", "ART", "ADJA"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.10": {"text": "Abendr\u00f6the mit Gluth \u00fcber dem Hybla gef\u00e4rbt:", "tokens": ["A\u00b7bend\u00b7r\u00f6\u00b7the", "mit", "Gluth", "\u00fc\u00b7ber", "dem", "Hy\u00b7bla", "ge\u00b7f\u00e4rbt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+--++-+", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Sieh, und es zeigt vom Balkon ein Liebchen mir schon Arethusa,", "tokens": ["Sieh", ",", "und", "es", "zeigt", "vom", "Bal\u00b7kon", "ein", "Lieb\u00b7chen", "mir", "schon", "A\u00b7ret\u00b7hu\u00b7sa", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "PPER", "ADV", "NE", "$,"], "meter": "-+-+-+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "Und Ortygia d\u00fcnkt l\u00e4ngst mir die Heimath zu sein.", "tokens": ["Und", "Or\u00b7ty\u00b7gia", "d\u00fcnkt", "l\u00e4ngst", "mir", "die", "Hei\u00b7math", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ADV", "PPER", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Doch nicht w\u00fc\u00dft' ich darum Syrakusas T\u00f6chter zu preisen,", "tokens": ["Doch", "nicht", "w\u00fc\u00dft'", "ich", "da\u00b7rum", "Sy\u00b7ra\u00b7ku\u00b7sas", "T\u00f6ch\u00b7ter", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVFIN", "PPER", "PAV", "NE", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Denn mit der Quelle gefolgt sind sie dem niederen Dienst.", "tokens": ["Denn", "mit", "der", "Quel\u00b7le", "ge\u00b7folgt", "sind", "sie", "dem", "nie\u00b7de\u00b7ren", "Dienst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Frisches Blut und kr\u00e4ft'ge Natur und edle Gesundheit,", "tokens": ["Fri\u00b7sches", "Blut", "und", "kr\u00e4ft'\u00b7ge", "Na\u00b7tur", "und", "ed\u00b7le", "Ge\u00b7sund\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Unverdorbene Zucht, fern der Verf\u00fchrung der Welt", "tokens": ["Un\u00b7ver\u00b7dor\u00b7be\u00b7ne", "Zucht", ",", "fern", "der", "Ver\u00b7f\u00fch\u00b7rung", "der", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJD", "ART", "NN", "ART", "NN"], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Findest am Aetna du, wo Indiens Feig' und die Rebe,", "tokens": ["Fin\u00b7dest", "am", "A\u00b7et\u00b7na", "du", ",", "wo", "In\u00b7di\u00b7ens", "Feig'", "und", "die", "Re\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NE", "NE", "$,", "PWAV", "NE", "NN", "KON", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+--+-", "measure": "iambic.septa.invert"}, "line.18": {"text": "Wo Orang' und Granat gl\u00fcckliche St\u00e4dte bedeckt.", "tokens": ["Wo", "O\u00b7rang'", "und", "Gra\u00b7nat", "gl\u00fcck\u00b7li\u00b7che", "St\u00e4d\u00b7te", "be\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "KON", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Oder im fernen Buscemi, im pinienumgr\u00fcnten Piazza,", "tokens": ["O\u00b7der", "im", "fer\u00b7nen", "Bus\u00b7ce\u00b7mi", ",", "im", "pi\u00b7ni\u00b7e\u00b7num\u00b7gr\u00fcn\u00b7ten", "Piaz\u00b7za", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+--+", "measure": "iambic.octa.plus.octa.plus.chol"}, "line.20": {"text": "Oder auf luftigem Fels, auf dem gigantischen dort,", "tokens": ["O\u00b7der", "auf", "luf\u00b7ti\u00b7gem", "Fels", ",", "auf", "dem", "gi\u00b7gan\u00b7ti\u00b7schen", "dort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "ADV", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.21": {"text": "Wo mit Dianen einst Proserpina Veilchen gepfl\u00fccket", "tokens": ["Wo", "mit", "Di\u00b7a\u00b7nen", "einst", "Pro\u00b7ser\u00b7pi\u00b7na", "Veil\u00b7chen", "ge\u00b7pfl\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NE", "ADV", "NE", "NE", "VVPP"], "meter": "-+-+--+---+--+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Und Aphrodite selbst heilige Keuschheit bewahrt;", "tokens": ["Und", "A\u00b7phro\u00b7di\u00b7te", "selbst", "hei\u00b7li\u00b7ge", "Keuschheit", "be\u00b7wahrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Oder auf Trapanis Berg, nur da\u00df die bl\u00fchenden Reize", "tokens": ["O\u00b7der", "auf", "Tra\u00b7pa\u00b7nis", "Berg", ",", "nur", "da\u00df", "die", "bl\u00fc\u00b7hen\u00b7den", "Rei\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "NN", "$,", "ADV", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.24": {"text": "Neidisch der Schleier dem Blick hier, der arab'sche, bedeckt.", "tokens": ["Nei\u00b7disch", "der", "Schlei\u00b7er", "dem", "Blick", "hier", ",", "der", "a\u00b7rab'\u00b7sche", ",", "be\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "ADV", "$,", "ART", "ADJA", "$,", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.25": {"text": "Dennoch aber der Preis der hohen K\u00f6nigin sei er,", "tokens": ["Den\u00b7noch", "a\u00b7ber", "der", "Preis", "der", "ho\u00b7hen", "K\u00f6\u00b7ni\u00b7gin", "sei", "er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VAFIN", "PPER", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.26": {"text": "Mutter der sch\u00f6nsten Frau'n, dir, o Palermo, geweiht.", "tokens": ["Mut\u00b7ter", "der", "sch\u00f6ns\u00b7ten", "Frau'n", ",", "dir", ",", "o", "Pa\u00b7ler\u00b7mo", ",", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "PPER", "$,", "FM", "NE", "$,", "VVPP", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.27": {"text": "Denn wie du selbst die erhabenste bist der St\u00e4dte, wie \u00fcppig", "tokens": ["Denn", "wie", "du", "selbst", "die", "er\u00b7ha\u00b7bens\u00b7te", "bist", "der", "St\u00e4d\u00b7te", ",", "wie", "\u00fcp\u00b7pig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "ART", "ADJA", "VAFIN", "ART", "NN", "$,", "PWAV", "ADJD"], "meter": "-+-+---+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.28": {"text": "Berg und H\u00fcgel und Thal Flora mit Bl\u00fcthen bedeckt,", "tokens": ["Berg", "und", "H\u00fc\u00b7gel", "und", "Thal", "Flo\u00b7ra", "mit", "Bl\u00fc\u00b7then", "be\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "NE", "APPR", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.29": {"text": "Hat die Natur, die mit Palmen dich schm\u00fcckt und Aloe, der Menschheit", "tokens": ["Hat", "die", "Na\u00b7tur", ",", "die", "mit", "Pal\u00b7men", "dich", "schm\u00fcckt", "und", "A\u00b7loe", ",", "der", "Menschheit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "NN", "PPER", "VVFIN", "KON", "NN", "$,", "ART", "NN"], "meter": "+--+--+--+-+--+", "measure": "dactylic.tri.plus"}, "line.30": {"text": "Z\u00e4rteste Blumen auch euch, reizende Frauen, geliebt.", "tokens": ["Z\u00e4r\u00b7tes\u00b7te", "Blu\u00b7men", "auch", "euch", ",", "rei\u00b7zen\u00b7de", "Frau\u00b7en", ",", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "PPER", "$,", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.31": {"text": "H\u00f6rt's, o Eifrer, an S\u00fc\u00dfigkeit gleicht Palermos Orangen", "tokens": ["H\u00f6rt's", ",", "o", "Eif\u00b7rer", ",", "an", "S\u00fc\u00b7\u00dfig\u00b7keit", "gleicht", "Pa\u00b7ler\u00b7mos", "O\u00b7ran\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "FM", "NN", "$,", "APPR", "NN", "VVFIN", "NE", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.32": {"text": "Kein', und Palermos Geschlecht gleicht in Trinakrien keins.", "tokens": ["Kein'", ",", "und", "Pa\u00b7ler\u00b7mos", "Ge\u00b7schlecht", "gleicht", "in", "Tri\u00b7na\u00b7kri\u00b7en", "keins", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KON", "NN", "NN", "VVFIN", "APPR", "NE", "NE", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.33": {"text": "Nun zu Schul' und Katheder, zu Beichtstuhl, Kanzel und H\u00f6rsaal,", "tokens": ["Nun", "zu", "Schul'", "und", "Ka\u00b7the\u00b7der", ",", "zu", "Beicht\u00b7stuhl", ",", "Kan\u00b7zel", "und", "H\u00f6r\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "--+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "B\u00fccher und Bibliothek kehret mir wieder zur\u00fcck.", "tokens": ["B\u00fc\u00b7cher", "und", "Bib\u00b7lio\u00b7thek", "keh\u00b7ret", "mir", "wie\u00b7der", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.35": {"text": "Nennet euch tugendhaft und schm\u00e4het mich fort, doch die Strafe", "tokens": ["Nen\u00b7net", "euch", "tu\u00b7gend\u00b7haft", "und", "schm\u00e4\u00b7het", "mich", "fort", ",", "doch", "die", "Stra\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "ART", "NN"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.36": {"text": "Giebt sich selber wer nie menschliche Liebe gef\u00fchlt.", "tokens": ["Giebt", "sich", "sel\u00b7ber", "wer", "nie", "menschli\u00b7che", "Lie\u00b7be", "ge\u00b7f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "PWS", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "+-+---+-+--+", "measure": "iambic.penta.chol"}}}}}