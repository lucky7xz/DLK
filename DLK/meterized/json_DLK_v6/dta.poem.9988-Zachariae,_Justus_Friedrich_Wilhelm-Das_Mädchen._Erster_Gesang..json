{"dta.poem.9988": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Das M\u00e4dchen.  \n   \n Erster Gesang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20674-3", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Muse, begeistert durch dich, sang von dem mensch-", "tokens": ["Mu\u00b7se", ",", "be\u00b7geis\u00b7tert", "durch", "dich", ",", "sang", "von", "dem", "men\u00b7sch"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "APPR", "PPER", "$,", "VVFIN", "APPR", "ART", "TRUNC"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Uns Wertm\u00fcllers gl\u00fcckliche Leyer. Mit r\u00f6mischer", "tokens": ["Uns", "Wert\u00b7m\u00fcl\u00b7lers", "gl\u00fcck\u00b7li\u00b7che", "Le\u00b7yer", ".", "Mit", "r\u00f6\u00b7mi\u00b7scher"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "NN", "ADJA", "NN", "$.", "APPR", "ADJA"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wiederhohlte sein Lied Oltrotschi. Vergassen die", "tokens": ["Wie\u00b7der\u00b7hohl\u00b7te", "sein", "Lied", "Olt\u00b7rot\u00b7schi", ".", "Ver\u00b7gas\u00b7sen", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "NE", "$.", "NN", "ART"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "Ganz, die andre sch\u00f6nere H\u00e4lfte des Menschenge-", "tokens": ["Ganz", ",", "die", "and\u00b7re", "sch\u00f6\u00b7ne\u00b7re", "H\u00e4lf\u00b7te", "des", "Men\u00b7schen\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ART", "ADJA", "ADJA", "NN", "ART", "TRUNC"], "meter": "+-+-+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Singe du sie Germaniens T\u00f6chtern! Sie lieben", "tokens": ["Sin\u00b7ge", "du", "sie", "Ger\u00b7ma\u00b7ni\u00b7ens", "T\u00f6ch\u00b7tern", "!", "Sie", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "NN", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Welche mit lehrendem Reiz die einsamen Stunden", "tokens": ["Wel\u00b7che", "mit", "leh\u00b7ren\u00b7dem", "Reiz", "die", "ein\u00b7sa\u00b7men", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAT", "APPR", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.7": {"text": "Und das f\u00fchlende Herz zur himmlischen Tugend er-", "tokens": ["Und", "das", "f\u00fch\u00b7len\u00b7de", "Herz", "zur", "himm\u00b7li\u00b7schen", "Tu\u00b7gend", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "TRUNC"], "meter": "--+--+-+--+--", "measure": "anapaest.di.plus"}, "line.8": {"text": "Liebliches M\u00e4dchen! nahe dich mir! \u2014 Wie", "tokens": ["Lieb\u00b7li\u00b7ches", "M\u00e4d\u00b7chen", "!", "na\u00b7he", "dich", "mir", "!", "Wie"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word"], "pos": ["ADJA", "NN", "$.", "APPR", "PPER", "PPER", "$.", "$(", "PWAV"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Mit dem feinsten Gesicht! Jhr braunes offenes", "tokens": ["Mit", "dem", "feins\u00b7ten", "Ge\u00b7sicht", "!", "Ihr", "brau\u00b7nes", "of\u00b7fe\u00b7nes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "PPOSAT", "ADJA", "ADJA"], "meter": "--+--+-+-+--", "measure": "anapaest.di.plus"}, "line.10": {"text": "L\u00e4chelt schon Sieg. Schon gl\u00fchen die Lippen in", "tokens": ["L\u00e4\u00b7chelt", "schon", "Sieg", ".", "Schon", "gl\u00fc\u00b7hen", "die", "Lip\u00b7pen", "in"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "APPR"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.11": {"text": "Und zerstreuete Rosen bedecken die z\u00e4rtlichen Wan-", "tokens": ["Und", "zer\u00b7streu\u00b7e\u00b7te", "Ro\u00b7sen", "be\u00b7de\u00b7cken", "die", "z\u00e4rt\u00b7li\u00b7chen", "Wan"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ART", "ADJA", "TRUNC"], "meter": "--+--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.12": {"text": "Aber noch warten des gelblichten Haars sanftwallen-", "tokens": ["A\u00b7ber", "noch", "war\u00b7ten", "des", "gelb\u00b7lich\u00b7ten", "Haars", "sanft\u00b7wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "TRUNC"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.13": {"text": "Auf die siegende Farbe der Nacht, die k\u00fcnftig die", "tokens": ["Auf", "die", "sie\u00b7gen\u00b7de", "Far\u00b7be", "der", "Nacht", ",", "die", "k\u00fcnf\u00b7tig", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "ADJD", "ART"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.14": {"text": "Jhres blendenden Halses erh\u00f6ht. Es flattert im", "tokens": ["Ih\u00b7res", "blen\u00b7den\u00b7den", "Hal\u00b7ses", "er\u00b7h\u00f6ht", ".", "Es", "flat\u00b7tert", "im"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$.", "PPER", "VVFIN", "APPRART"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Wenn sie mit kleinen gefl\u00fcgelten F\u00fcssen die Mutter", "tokens": ["Wenn", "sie", "mit", "klei\u00b7nen", "ge\u00b7fl\u00fc\u00b7gel\u00b7ten", "F\u00fcs\u00b7sen", "die", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "ADJA", "NN", "ART", "NN"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "An das lange Gewand sich h\u00e4ngt, und stammelt,", "tokens": ["An", "das", "lan\u00b7ge", "Ge\u00b7wand", "sich", "h\u00e4ngt", ",", "und", "stam\u00b7melt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Bis ihr die Mutter zur\u00fcckegefolgt. Jetzt setzt sie", "tokens": ["Bis", "ihr", "die", "Mut\u00b7ter", "zu\u00b7r\u00fc\u00b7ck\u00b7e\u00b7ge\u00b7folgt", ".", "Jetzt", "setzt", "sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "VVPP", "$.", "ADV", "VVFIN", "PPER"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Vor den Theetisch, und wartet ihr auf. Mit klei-", "tokens": ["Vor", "den", "Thee\u00b7tisch", ",", "und", "war\u00b7tet", "ihr", "auf", ".", "Mit", "klei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$.", "APPR", "TRUNC"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.19": {"text": "Unterh\u00e4lt sie sie lange, die Antwort erwartend,", "tokens": ["Un\u00b7ter\u00b7h\u00e4lt", "sie", "sie", "lan\u00b7ge", ",", "die", "Ant\u00b7wort", "er\u00b7war\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$,", "ART", "NN", "VVPP", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.20": {"text": "Ueber ihr eigensinniges Schweigen\u2019; sie giebt ihr", "tokens": ["Ue\u00b7ber", "ihr", "ei\u00b7gen\u00b7sin\u00b7ni\u00b7ges", "Schwei\u00b7gen'", ";", "sie", "giebt", "ihr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPER"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.21": {"text": "Welche die Mutter ihr gab, zur\u00fcck. Der Vater", "tokens": ["Wel\u00b7che", "die", "Mut\u00b7ter", "ihr", "gab", ",", "zu\u00b7r\u00fcck", ".", "Der", "Va\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWS", "ART", "NN", "PPER", "VVFIN", "$,", "PTKVZ", "$.", "ART", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.22": {"text": "L\u00e4chelt von seinen B\u00fcchern empor; erinnert sie", "tokens": ["L\u00e4\u00b7chelt", "von", "sei\u00b7nen", "B\u00fc\u00b7chern", "em\u00b7por", ";", "e\u00b7rin\u00b7nert", "sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$.", "VVFIN", "PPER"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.23": {"text": "Da\u00df die Puppe nicht spricht, und tr\u00f6stet die kleine", "tokens": ["Da\u00df", "die", "Pup\u00b7pe", "nicht", "spricht", ",", "und", "tr\u00f6s\u00b7tet", "die", "klei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "VVFIN", "$,", "KON", "VVFIN", "ART", "ADJA"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.24": {"text": "Dann k\u00f6mmt auf dem muthigen Stecken, ihr j\u00fcn-", "tokens": ["Dann", "k\u00f6mmt", "auf", "dem", "mut\u00b7hi\u00b7gen", "Ste\u00b7cken", ",", "ihr", "j\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "PPOSAT", "TRUNC"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.25": {"text": "Ueber den Saal her geritten. Sie sieht mit furcht-", "tokens": ["Ue\u00b7ber", "den", "Saal", "her", "ge\u00b7rit\u00b7ten", ".", "Sie", "sieht", "mit", "furcht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APZR", "VVPP", "$.", "PPER", "VVFIN", "APPR", "TRUNC"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.26": {"text": "Z\u00e4rtlich ihm nach, und warnt ihn; umsonst! der", "tokens": ["Z\u00e4rt\u00b7lich", "ihm", "nach", ",", "und", "warnt", "ihn", ";", "um\u00b7sonst", "!", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NE", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "$.", "ADV", "$.", "ART"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.27": {"text": "Zeigt sich bereits in jeglichem Schritt der kindischen", "tokens": ["Zeigt", "sich", "be\u00b7reits", "in", "jeg\u00b7li\u00b7chem", "Schritt", "der", "kin\u00b7di\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "PIAT", "NN", "ART", "NN"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.28": {"text": "Pferd\u2019 und Wagen ergetzen ihn nur, und der blin-", "tokens": ["Pferd'", "und", "Wa\u00b7gen", "er\u00b7get\u00b7zen", "ihn", "nur", ",", "und", "der", "blin"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "ADV", "$,", "KON", "ART", "TRUNC"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.29": {"text": "Und der m\u00e4nnliche Hut. Er kennet die Furcht nicht,", "tokens": ["Und", "der", "m\u00e4nn\u00b7li\u00b7che", "Hut", ".", "Er", "ken\u00b7net", "die", "Furcht", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.30": {"text": "Wenn die kriegrische Trommel erschallt. Doch weib-", "tokens": ["Wenn", "die", "krie\u00b7gri\u00b7sche", "Trom\u00b7mel", "er\u00b7schallt", ".", "Doch", "weib"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVPP", "$.", "KON", "TRUNC"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.31": {"text": "Herrscht ganz in dem f\u00fchlenden M\u00e4dchen. Jetzt", "tokens": ["Herrscht", "ganz", "in", "dem", "f\u00fch\u00b7len\u00b7den", "M\u00e4d\u00b7chen", ".", "Jetzt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "ADV", "APPR", "ART", "ADJA", "NN", "$.", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.32": {"text": "Mit sich allein, und flehet ihn an, sein Leben zu", "tokens": ["Mit", "sich", "al\u00b7lein", ",", "und", "fle\u00b7het", "ihn", "an", ",", "sein", "Le\u00b7ben", "zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRF", "ADV", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "NN", "PTKZU"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.33": {"text": "Und nicht der wallenden Fahne zu folgen. Der mu-", "tokens": ["Und", "nicht", "der", "wal\u00b7len\u00b7den", "Fah\u00b7ne", "zu", "fol\u00b7gen", ".", "Der", "mu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$.", "ART", "TRUNC"], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Wird von den Thr\u00e4nen erweicht, legt seine l\u00e4rmen-", "tokens": ["Wird", "von", "den", "Thr\u00e4\u00b7nen", "er\u00b7weicht", ",", "legt", "sei\u00b7ne", "l\u00e4r\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$,", "VVFIN", "PPOSAT", "TRUNC"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Und sein blankes Husarenschwerdt ab, und spielt", "tokens": ["Und", "sein", "blan\u00b7kes", "Hu\u00b7sa\u00b7ren\u00b7schwerdt", "ab", ",", "und", "spielt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.36": {"text": "Stillere Spiele; wird Kutscher und Koch, und l\u00e4\u00dft", "tokens": ["Stil\u00b7le\u00b7re", "Spie\u00b7le", ";", "wird", "Kut\u00b7scher", "und", "Koch", ",", "und", "l\u00e4\u00dft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "VAFIN", "NN", "KON", "NE", "$,", "KON", "VVFIN"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.37": {"text": "Zu des M\u00e4dchens Geschmacke herab. Dann folgt", "tokens": ["Zu", "des", "M\u00e4d\u00b7chens", "Ge\u00b7schma\u00b7cke", "her\u00b7ab", ".", "Dann", "folgt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "ADV", "$.", "ADV", "VVFIN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.38": {"text": "H\u00e4u\u00dflichem Schritt, und ahmet ihr nach in kindi-", "tokens": ["H\u00e4u\u00df\u00b7li\u00b7chem", "Schritt", ",", "und", "ah\u00b7met", "ihr", "nach", "in", "kin\u00b7di"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "VVFIN", "PPER", "APPR", "APPR", "TRUNC"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.39": {"text": "Oder ergreift mit zitternder Hand die Nadel der", "tokens": ["O\u00b7der", "er\u00b7greift", "mit", "zit\u00b7tern\u00b7der", "Hand", "die", "Na\u00b7del", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "ART"], "meter": "-+-+-+--+-+--", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "Und glaubt Blumen und Laub in ihren Versuchen", "tokens": ["Und", "glaubt", "Blu\u00b7men", "und", "Laub", "in", "ih\u00b7ren", "Ver\u00b7su\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "APPR", "PPOSAT", "NN"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.41": {"text": "Oftmals nimmt sie der liebende Vater mit zartli-", "tokens": ["Oft\u00b7mals", "nimmt", "sie", "der", "lie\u00b7ben\u00b7de", "Va\u00b7ter", "mit", "zart\u00b7li"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "TRUNC"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.42": {"text": "Auf den schmeichelnden Schoos, und lehrt sie zeitig", "tokens": ["Auf", "den", "schmei\u00b7cheln\u00b7den", "Schoos", ",", "und", "lehrt", "sie", "zei\u00b7tig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "PPER", "ADJD"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.43": {"text": "Von dem g\u00fctigen Sch\u00f6pfer der Welt. Steigt \u00fcber", "tokens": ["Von", "dem", "g\u00fc\u00b7ti\u00b7gen", "Sch\u00f6p\u00b7fer", "de