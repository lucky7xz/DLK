{"textgrid.poem.58740": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "34.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer dem Herrn anhangt, der ist ein Geist mit ihm. 1. Kor. 6, 17", "tokens": ["Wer", "dem", "Herrn", "an\u00b7hangt", ",", "der", "ist", "ein", "Geist", "mit", "ihm", ".", "1.", "Kor", ".", "6", ",", "17"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "ordinal", "word", "punct", "number", "punct", "number"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "PRELS", "VAFIN", "ART", "NN", "APPR", "PPER", "$.", "ADJA", "NN", "$.", "CARD", "$,", "CARD"], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "In Eins, in Eines schmelz, mein Himmelsbr\u00e4utigam,", "tokens": ["In", "Eins", ",", "in", "Ei\u00b7nes", "schmelz", ",", "mein", "Him\u00b7mels\u00b7br\u00e4u\u00b7ti\u00b7gam", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So innig dein Herz und das meine,", "tokens": ["So", "in\u00b7nig", "dein", "Herz", "und", "das", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "KON", "PDS", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da\u00df nichts als dein Herz mehr erscheine,", "tokens": ["Da\u00df", "nichts", "als", "dein", "Herz", "mehr", "er\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "KOKOM", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ei, blase auf die sch\u00f6ne Flamm!", "tokens": ["Ei", ",", "bla\u00b7se", "auf", "die", "sch\u00f6\u00b7ne", "Flamm", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "In deiner Schmiede dreh die Herzen tausendfach,", "tokens": ["In", "dei\u00b7ner", "Schmie\u00b7de", "dreh", "die", "Her\u00b7zen", "tau\u00b7send\u00b7fach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schlag mein Herz, da\u00df dein Ambo\u00df t\u00f6ne,", "tokens": ["Schlag", "mein", "Herz", ",", "da\u00df", "dein", "Am\u00b7bo\u00df", "t\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Mach's gar zunichte nur, wie wird's dann sein so sch\u00f6ne,", "tokens": ["Mach's", "gar", "zu\u00b7nich\u00b7te", "nur", ",", "wie", "wird's", "dann", "sein", "so", "sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "ADV", "$,", "PWAV", "VAFIN", "ADV", "PPOSAT", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erl\u00f6schet einst dein Feu'r, die Glut bald neue mach!", "tokens": ["Er\u00b7l\u00f6\u00b7schet", "einst", "dein", "Feu'r", ",", "die", "Glut", "bald", "neu\u00b7e", "mach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$,", "ART", "NN", "ADV", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ein Herz, wie dein Herz ist, ja, da\u00df nur sei ", "tokens": ["Ein", "Herz", ",", "wie", "dein", "Herz", "ist", ",", "ja", ",", "da\u00df", "nur", "sei"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "PPOSAT", "NN", "VAFIN", "$,", "PTKANT", "$,", "KOUS", "ADV", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wann werd ich dieses Gl\u00fcck genie\u00dfen?", "tokens": ["Wann", "werd", "ich", "die\u00b7ses", "Gl\u00fcck", "ge\u00b7nie\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df in Eins alle Herzen flie\u00dfen", "tokens": ["La\u00df", "in", "Eins", "al\u00b7le", "Her\u00b7zen", "flie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "NN", "PIAT", "NN", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Durch einen angenehmen Schmerz!", "tokens": ["Durch", "ei\u00b7nen", "an\u00b7ge\u00b7neh\u00b7men", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wer deine Glut noch scheut, kennt sie nicht in der Tat,", "tokens": ["Wer", "dei\u00b7ne", "Glut", "noch", "scheut", ",", "kennt", "sie", "nicht", "in", "der", "Tat", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADV", "VVFIN", "$,", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich finde darin mein Vergn\u00fcgen;", "tokens": ["Ich", "fin\u00b7de", "da\u00b7rin", "mein", "Ver\u00b7gn\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist wahrer Friede sonst zu kriegen,", "tokens": ["Ist", "wah\u00b7rer", "Frie\u00b7de", "sonst", "zu", "krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo diese liebe Not man nicht geschmecket hat?", "tokens": ["Wo", "die\u00b7se", "lie\u00b7be", "Not", "man", "nicht", "ge\u00b7schme\u00b7cket", "hat", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "ADJA", "NN", "PIS", "PTKNEG", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}