{"dta.poem.18302": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Einem Gartenlaubendichter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Pust aus dein kleines Dreierlicht!", "tokens": ["Pust", "aus", "dein", "klei\u00b7nes", "Drei\u00b7er\u00b7licht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Denn die schwarzwei\u00dfrothen Gelegenheitscarmen", "tokens": ["Denn", "die", "schwarz\u00b7wei\u00df\u00b7ro\u00b7then", "Ge\u00b7le\u00b7gen\u00b7heits\u00b7car\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Haben wir endlich dick gekriegt.", "tokens": ["Ha\u00b7ben", "wir", "end\u00b7lich", "dick", "ge\u00b7kriegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Du bist und bleibst ein blo\u00dfer Reimer,", "tokens": ["Du", "bist", "und", "bleibst", "ein", "blo\u00b7\u00dfer", "Rei\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein echter Sohn des Vater Rhein,", "tokens": ["Kein", "ech\u00b7ter", "Sohn", "des", "Va\u00b7ter", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schenkst deinen Lesern, statt R\u00fcdesheimer,", "tokens": ["Und", "schenkst", "dei\u00b7nen", "Le\u00b7sern", ",", "statt", "R\u00fc\u00b7des\u00b7hei\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "APPR", "NE", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Nur versificirten Dreim\u00e4nnerwein.", "tokens": ["Nur", "ver\u00b7si\u00b7fi\u00b7cir\u00b7ten", "Drei\u00b7m\u00e4n\u00b7ner\u00b7wein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}