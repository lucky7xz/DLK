{"textgrid.poem.29220": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "35.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geduld hat manchen Sieg,", "tokens": ["Ge\u00b7duld", "hat", "man\u00b7chen", "Sieg", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Macht aber auch viel Krieg;", "tokens": ["Macht", "a\u00b7ber", "auch", "viel", "Krieg", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein ieder wil sich reiben", "tokens": ["Ein", "ie\u00b7der", "wil", "sich", "rei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VMFIN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An den, der sich lie\u00df treiben.", "tokens": ["An", "den", ",", "der", "sich", "lie\u00df", "trei\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PRF", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}