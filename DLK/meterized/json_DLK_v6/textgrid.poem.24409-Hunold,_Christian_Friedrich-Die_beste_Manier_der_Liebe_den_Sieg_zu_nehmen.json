{"textgrid.poem.24409": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die beste Manier der Liebe den Sieg zu nehmen", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem Feinde mu\u00df ein Held/ den Sieg davon zu ziehn", "tokens": ["Dem", "Fein\u00b7de", "mu\u00df", "ein", "Held", "/", "den", "Sieg", "da\u00b7von", "zu", "ziehn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "$(", "ART", "NN", "PAV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Wei\u00df' im Auge sehn/ und auf die Haube f\u00fchlen.", "tokens": ["Das", "Wei\u00df'", "im", "Au\u00b7ge", "sehn", "/", "und", "auf", "die", "Hau\u00b7be", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$(", "KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch wer im Liebes-Feld sich selber nicht verspielen", "tokens": ["Doch", "wer", "im", "Lie\u00b7bes\u00b7Feld", "sich", "sel\u00b7ber", "nicht", "ver\u00b7spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPRART", "NN", "PRF", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und \u00fcberwinden will/ mu\u00df sehn und F\u00fchlen fliehn.", "tokens": ["Und", "\u00fc\u00b7berw\u00b7in\u00b7den", "will", "/", "mu\u00df", "sehn", "und", "F\u00fch\u00b7len", "fliehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["