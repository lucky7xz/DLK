{"textgrid.poem.40581": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein M\u00e4dchen kniet an einem Leichenstein", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein M\u00e4dchen kniet an einem Leichenstein", "tokens": ["Ein", "M\u00e4d\u00b7chen", "kniet", "an", "ei\u00b7nem", "Lei\u00b7chen\u00b7stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und pflanzt daneben ein Pappel ein:", "tokens": ["Und", "pflanzt", "da\u00b7ne\u00b7ben", "ein", "Pap\u00b7pel", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbstreb' auf zum Aether, schlanker Baum,", "tokens": ["\u00bb", "streb'", "auf", "zum", "A\u00b7e\u00b7ther", ",", "schlan\u00b7ker", "Baum", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "APPRART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auch Er flog auf zum Sternenraum.", "tokens": ["Auch", "Er", "flog", "auf", "zum", "Ster\u00b7nen\u00b7raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie meine H\u00e4nde zum Gebet,", "tokens": ["Wie", "mei\u00b7ne", "H\u00e4n\u00b7de", "zum", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Sei aufw\u00e4rts jeder Zweig gedreht;", "tokens": ["Sei", "auf\u00b7w\u00e4rts", "je\u00b7der", "Zweig", "ge\u00b7dreht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie meine Augen sternenw\u00e4rts sp\u00e4hen,", "tokens": ["Wie", "mei\u00b7ne", "Au\u00b7gen", "ster\u00b7nen\u00b7w\u00e4rts", "sp\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Soll jedes Blatt nach oben sehen.", "tokens": ["Soll", "je\u00b7des", "Blatt", "nach", "o\u00b7ben", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Zu ihm, zu ihm! Empor, empor!", "tokens": ["Zu", "ihm", ",", "zu", "ihm", "!", "Em\u00b7por", ",", "em\u00b7por", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "$.", "NE", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Rausch' es aus deinem Laub hervor!", "tokens": ["Rausch'", "es", "aus", "dei\u00b7nem", "Laub", "her\u00b7vor", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "So, Pappel, auf des Grabes H\u00f6hen", "tokens": ["So", ",", "Pap\u00b7pel", ",", "auf", "des", "Gra\u00b7bes", "H\u00f6\u00b7hen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "$,", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Sollst, meiner Trauer Bild du stehen.\u00ab", "tokens": ["Sollst", ",", "mei\u00b7ner", "Trau\u00b7er", "Bild", "du", "ste\u00b7hen", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "$,", "PPOSAT", "NN", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein J\u00fcngling kniet an einem Leichenstein", "tokens": ["Ein", "J\u00fcng\u00b7ling", "kniet", "an", "ei\u00b7nem", "Lei\u00b7chen\u00b7stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und pflanzt daneben eine Weide ein:", "tokens": ["Und", "pflanzt", "da\u00b7ne\u00b7ben", "ei\u00b7ne", "Wei\u00b7de", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbstreb' erdenw\u00e4rts, du Thr\u00e4nenbaum,", "tokens": ["\u00bb", "streb'", "er\u00b7den\u00b7w\u00e4rts", ",", "du", "Thr\u00e4\u00b7nen\u00b7baum", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch Sie sank in der Erde Raum;", "tokens": ["Auch", "Sie", "sank", "in", "der", "Er\u00b7de", "Raum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wie meine Z\u00e4hre auf die\u00df Grab,", "tokens": ["Wie", "mei\u00b7ne", "Z\u00e4h\u00b7re", "auf", "die\u00df", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "PDS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So sch\u00fcttle deinen Thau herab;", "tokens": ["So", "sch\u00fctt\u00b7le", "dei\u00b7nen", "Thau", "her\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie meine Arme abw\u00e4rts ringen", "tokens": ["Wie", "mei\u00b7ne", "Ar\u00b7me", "ab\u00b7w\u00e4rts", "rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und gern den kalten Sarg umfingen,", "tokens": ["Und", "gern", "den", "kal\u00b7ten", "Sarg", "um\u00b7fin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ihr Zweige, so umschlingt die\u00df Grab.", "tokens": ["Ihr", "Zwei\u00b7ge", ",", "so", "um\u00b7schlingt", "die\u00df", "Grab", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VVFIN", "PDS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Zu ihr, zu ihr! Hinab, hinab!", "tokens": ["Zu", "ihr", ",", "zu", "ihr", "!", "Hin\u00b7ab", ",", "hin\u00b7ab", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPOSAT", "$.", "ADV", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "So, Weide, auf des Grabes H\u00f6hen", "tokens": ["So", ",", "Wei\u00b7de", ",", "auf", "des", "Gra\u00b7bes", "H\u00f6\u00b7hen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "$,", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Sollst, meiner Trauer Bild, du stehen.\u00ab", "tokens": ["Sollst", ",", "mei\u00b7ner", "Trau\u00b7er", "Bild", ",", "du", "ste\u00b7hen", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VMFIN", "$,", "PPOSAT", "NN", "NN", "$,", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}