{"textgrid.poem.27815": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo ein gemahlter Brieff und au\u00dfgekauffte Bullen,", "tokens": ["Wo", "ein", "ge\u00b7mahl\u00b7ter", "Brieff", "und", "au\u00df\u00b7ge\u00b7kauff\u00b7te", "Bul\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer Edel noch nicht ist, erst Edel machen sollen,", "tokens": ["Wer", "E\u00b7del", "noch", "nicht", "ist", ",", "erst", "E\u00b7del", "ma\u00b7chen", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADV", "PTKNEG", "VAFIN", "$,", "ADV", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So kan wol eine Maus de\u00df Adels sich vermessen,", "tokens": ["So", "kan", "wol", "ei\u00b7ne", "Maus", "de\u00df", "A\u00b7dels", "sich", "ver\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ART", "NN", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die einen solchen Brieff hat unversehns gefressen.", "tokens": ["Die", "ei\u00b7nen", "sol\u00b7chen", "Brieff", "hat", "un\u00b7ver\u00b7sehns", "ge\u00b7fres\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "PIAT", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}