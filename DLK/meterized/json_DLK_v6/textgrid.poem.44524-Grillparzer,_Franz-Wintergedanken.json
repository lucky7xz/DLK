{"textgrid.poem.44524": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Wintergedanken", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du, Seele, nicht mehr bl\u00fchen,", "tokens": ["Willst", "du", ",", "See\u00b7le", ",", "nicht", "mehr", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "NN", "$,", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da vorbei des Sommers Flucht?", "tokens": ["Da", "vor\u00b7bei", "des", "Som\u00b7mers", "Flucht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder wenn der Herbst erschienen,", "tokens": ["O\u00b7der", "wenn", "der", "Herbst", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Warum gibst du keine Frucht?", "tokens": ["Wa\u00b7rum", "gibst", "du", "kei\u00b7ne", "Frucht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "War vielleicht zu reich dein Bl\u00fchen,", "tokens": ["War", "viel\u00b7leicht", "zu", "reich", "dein", "Bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKA", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War zu bunt der Farben Licht?", "tokens": ["War", "zu", "bunt", "der", "Far\u00b7ben", "Licht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKA", "ADJD", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn die Bl\u00fcten geben Fr\u00fcchte,", "tokens": ["Denn", "die", "Bl\u00fc\u00b7ten", "ge\u00b7ben", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber, ach, die Blumen nicht.", "tokens": ["A\u00b7ber", ",", "ach", ",", "die", "Blu\u00b7men", "nicht", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ITJ", "$,", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}