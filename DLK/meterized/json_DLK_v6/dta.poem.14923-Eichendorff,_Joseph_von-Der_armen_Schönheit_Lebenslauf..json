{"dta.poem.14923": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der armen Sch\u00f6nheit Lebenslauf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewas bist Du f\u00fcr 'ne sch\u00f6ne Leiche!", "tokens": ["\u201e", "was", "bist", "Du", "f\u00fcr", "'ne", "sch\u00f6\u00b7ne", "Lei\u00b7che", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So w\u00fcste ist mir meine Brust,", "tokens": ["So", "w\u00fcs\u00b7te", "ist", "mir", "mei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie bist Du nun so arm, Du Reiche,", "tokens": ["Wie", "bist", "Du", "nun", "so", "arm", ",", "Du", "Rei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,", "PPER", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich hab' an Dir nicht weiter Lust!\u201c", "tokens": ["Ich", "hab'", "an", "Dir", "nicht", "wei\u00b7ter", "Lust", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "PTKNEG", "ADV", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}