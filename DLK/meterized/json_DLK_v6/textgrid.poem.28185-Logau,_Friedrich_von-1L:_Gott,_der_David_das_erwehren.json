{"textgrid.poem.28185": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gott, der David das erwehren", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott, der David das erwehren", "tokens": ["Gott", ",", "der", "Da\u00b7vid", "das", "er\u00b7weh\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ART", "NE", "PDS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gab vom L\u00f6wen und vom Beeren,", "tokens": ["Gab", "vom", "L\u00f6\u00b7wen", "und", "vom", "Bee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gab ihm auch durch einen Stein,", "tokens": ["Gab", "ihm", "auch", "durch", "ei\u00b7nen", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "De\u00df Philisters Mann zu seyn.", "tokens": ["De\u00df", "Phi\u00b7lis\u00b7ters", "Mann", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gott, der uns vom H\u00f6llen-Rachen", "tokens": ["Gott", ",", "der", "uns", "vom", "H\u00f6l\u00b7len\u00b7Ra\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gab das Mittel los zu machen,", "tokens": ["Gab", "das", "Mit\u00b7tel", "los", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Gibt auch wohl, da\u00df Menschen macht", "tokens": ["Gibt", "auch", "wohl", ",", "da\u00df", "Men\u00b7schen", "macht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "$,", "KOUS", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Schafft nichts mehr, als da\u00df man lacht.", "tokens": ["Schafft", "nichts", "mehr", ",", "als", "da\u00df", "man", "lacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$,", "KOKOM", "KOUS", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}