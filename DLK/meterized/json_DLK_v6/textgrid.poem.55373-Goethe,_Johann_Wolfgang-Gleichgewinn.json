{"textgrid.poem.55373": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Gleichgewinn", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geht einer mit dem andern hin", "tokens": ["Geht", "ei\u00b7ner", "mit", "dem", "an\u00b7dern", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "APPR", "ART", "ADJA", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und auch wohl vor dem andern;", "tokens": ["Und", "auch", "wohl", "vor", "dem", "an\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "ART", "ADJA", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Drum la\u00dft uns treu und brav und k\u00fchn", "tokens": ["Drum", "la\u00dft", "uns", "treu", "und", "brav", "und", "k\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Lebenspfade wandern.", "tokens": ["Die", "Le\u00b7ben\u00b7spfa\u00b7de", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es f\u00e4llt ein j\u00fcngerer Soldat", "tokens": ["Es", "f\u00e4llt", "ein", "j\u00fcn\u00b7ge\u00b7rer", "Sol\u00b7dat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wohl in den ersten Schlachten;", "tokens": ["Wohl", "in", "den", "ers\u00b7ten", "Schlach\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der andre mu\u00df ins Alter spat", "tokens": ["Der", "and\u00b7re", "mu\u00df", "ins", "Al\u00b7ter", "spat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VMFIN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Im Biwak \u00fcbernachten.", "tokens": ["Im", "Bi\u00b7wak", "\u00fc\u00b7bern\u00b7ach\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Doch wei\u00df er eifrig seinen Ruhm", "tokens": ["Doch", "wei\u00df", "er", "eif\u00b7rig", "sei\u00b7nen", "Ruhm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und seines Herrn zu mehren,", "tokens": ["Und", "sei\u00b7nes", "Herrn", "zu", "meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "So bleibt sein letztes Eigentum", "tokens": ["So", "bleibt", "sein", "letz\u00b7tes", "Ei\u00b7gen\u00b7tum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Gewi\u00df das Bett der Ehren.", "tokens": ["Ge\u00b7wi\u00df", "das", "Bett", "der", "Eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}