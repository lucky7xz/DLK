{"dta.poem.21309": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput IV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Flamme des Scheiterhaufens hat hier", "tokens": ["Die", "Flam\u00b7me", "des", "Schei\u00b7ter\u00b7hau\u00b7fens", "hat", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADV"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "B\u00fccher und Menschen verschlungen;", "tokens": ["B\u00fc\u00b7cher", "und", "Men\u00b7schen", "ver\u00b7schlun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Die Glocken wurden gel\u00e4utet dabei", "tokens": ["Die", "Glo\u00b7cken", "wur\u00b7den", "ge\u00b7l\u00e4u\u00b7tet", "da\u00b7bei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "PAV"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und Kyrie Eleison gesungen.", "tokens": ["Und", "Ky\u00b7rie", "E\u00b7lei\u00b7son", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dummheit und Bosheit buhlten hier", "tokens": ["Dumm\u00b7heit", "und", "Bos\u00b7heit", "buhl\u00b7ten", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Gleich Hunden auf freier Gasse;", "tokens": ["Gleich", "Hun\u00b7den", "auf", "frei\u00b7er", "Gas\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Enkelbrut erkennt man noch heut", "tokens": ["Die", "En\u00b7kel\u00b7brut", "er\u00b7kennt", "man", "noch", "heut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADV", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "An ihrem Glaubenshasse. \u2013", "tokens": ["An", "ih\u00b7rem", "Glau\u00b7bens\u00b7has\u00b7se", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch siehe! dort im Mondenschein", "tokens": ["Doch", "sie\u00b7he", "!", "dort", "im", "Mon\u00b7den\u00b7schein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "$.", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den kolossalen Gesellen!", "tokens": ["Den", "ko\u00b7los\u00b7sa\u00b7len", "Ge\u00b7sel\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Er ragt verteufelt schwarz empor,", "tokens": ["Er", "ragt", "ver\u00b7teu\u00b7felt", "schwarz", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ist der Dom von C\u00f6llen.", "tokens": ["Das", "ist", "der", "Dom", "von", "C\u00f6l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er sollte des Geistes Bastille sein,", "tokens": ["Er", "soll\u00b7te", "des", "Geis\u00b7tes", "Bas\u00b7til\u00b7le", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "NE", "VAINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und die listigen R\u00f6mlinge dachten:", "tokens": ["Und", "die", "lis\u00b7ti\u00b7gen", "R\u00f6m\u00b7lin\u00b7ge", "dach\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "In diesem Riesenkerker wird", "tokens": ["In", "die\u00b7sem", "Rie\u00b7sen\u00b7ker\u00b7ker", "wird"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die deutsche Vernunft verschmachten!", "tokens": ["Die", "deut\u00b7sche", "Ver\u00b7nunft", "ver\u00b7schmach\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}