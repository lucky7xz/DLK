{"textgrid.poem.55089": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Vor Gericht", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von wem ich es habe, das sag ich euch nicht,", "tokens": ["Von", "wem", "ich", "es", "ha\u00b7be", ",", "das", "sag", "ich", "euch", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "PPER", "PPER", "VAFIN", "$,", "PDS", "VVFIN", "PPER", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Das Kind! in meinem Leib.", "tokens": ["Das", "Kind", "!", "in", "mei\u00b7nem", "Leib", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbpfui!\u00ab speit ihr aus: \u00bbdie Hure da!\u00ab", "tokens": ["\u00bb", "pfui", "!", "\u00ab", "speit", "ihr", "aus", ":", "\u00bb", "die", "Hu\u00b7re", "da", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$.", "$(", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bin doch ein ehrlich Weib.", "tokens": ["Bin", "doch", "ein", "ehr\u00b7lich", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mit wem ich mich traute, das sag ich euch nicht.", "tokens": ["Mit", "wem", "ich", "mich", "trau\u00b7te", ",", "das", "sag", "ich", "euch", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "PPER", "PRF", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "PPER", "PTKNEG", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mein Schatz ist lieb und gut,", "tokens": ["Mein", "Schatz", "ist", "lieb", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Tr\u00e4gt er eine goldene Kett am Hals,", "tokens": ["Tr\u00e4gt", "er", "ei\u00b7ne", "gol\u00b7de\u00b7ne", "Kett", "am", "Hals", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Tr\u00e4gt er einen strohernen Hut.", "tokens": ["Tr\u00e4gt", "er", "ei\u00b7nen", "stro\u00b7her\u00b7nen", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Soll Spott und Hohn getragen sein,", "tokens": ["Soll", "Spott", "und", "Hohn", "ge\u00b7tra\u00b7gen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Trag ich allein den Hohn.", "tokens": ["Trag", "ich", "al\u00b7lein", "den", "Hohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich kenn ihn wohl, er kennt mich wohl,", "tokens": ["Ich", "kenn", "ihn", "wohl", ",", "er", "kennt", "mich", "wohl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Gott wei\u00df auch davon.", "tokens": ["Und", "Gott", "wei\u00df", "auch", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Herr Pfarrer und Herr Amtmann ihr,", "tokens": ["Herr", "Pfar\u00b7rer", "und", "Herr", "Amt\u00b7mann", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "NE", "PPER", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich bitte, la\u00dft mich in Ruh!", "tokens": ["Ich", "bit\u00b7te", ",", "la\u00dft", "mich", "in", "Ruh", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVIMP", "PPER", "APPR", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Es ist mein Kind, es bleibt mein Kind,", "tokens": ["Es", "ist", "mein", "Kind", ",", "es", "bleibt", "mein", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr gebt mir ja nichts dazu.", "tokens": ["Ihr", "gebt", "mir", "ja", "nichts", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PIS", "PAV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}