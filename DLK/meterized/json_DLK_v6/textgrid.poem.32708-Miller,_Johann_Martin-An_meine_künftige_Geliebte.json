{"textgrid.poem.32708": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "An meine k\u00fcnftige Geliebte", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du, das ganz mein Herz erf\u00fcllt,", "tokens": ["O", "du", ",", "das", "ganz", "mein", "Herz", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geliebtes, s\u00fc\u00dfes Schattenbild!", "tokens": ["Ge\u00b7lieb\u00b7tes", ",", "s\u00fc\u00b7\u00dfes", "Schat\u00b7ten\u00b7bild", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O M\u00e4dchen, das einst Harm und Wohl", "tokens": ["O", "M\u00e4d\u00b7chen", ",", "das", "einst", "Harm", "und", "Wohl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit mir auf Erden teilen soll!", "tokens": ["Mit", "mir", "auf", "Er\u00b7den", "tei\u00b7len", "soll", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Im Staube nieder werf' ich mich,", "tokens": ["Im", "Stau\u00b7be", "nie\u00b7der", "wer\u00b7f'", "ich", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "VAFIN", "PPER", "PRF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und fleh' zu Gott empor f\u00fcr dich,", "tokens": ["Und", "fleh'", "zu", "Gott", "em\u00b7por", "f\u00fcr", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PTKVZ", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df er dein Herz mir rein und zart,", "tokens": ["Da\u00df", "er", "dein", "Herz", "mir", "rein", "und", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und fromm und edel aufbewahrt.", "tokens": ["Und", "fromm", "und", "e\u00b7del", "auf\u00b7be\u00b7wahrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da\u00df Flitterstaat, und Prunk und Geld", "tokens": ["Da\u00df", "Flit\u00b7ter\u00b7staat", ",", "und", "Prunk", "und", "Geld"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nie deinen Geist gefangen h\u00e4lt;", "tokens": ["Nie", "dei\u00b7nen", "Geist", "ge\u00b7fan\u00b7gen", "h\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Buhlersang dein Herz emp\u00f6rt;", "tokens": ["Kein", "Buh\u00b7ler\u00b7sang", "dein", "Herz", "em\u00b7p\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch Schmeichelrede dich beth\u00f6rt.", "tokens": ["Noch", "Schmei\u00b7chel\u00b7re\u00b7de", "dich", "be\u00b7th\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dein Engel f\u00fchr' oft dich allein", "tokens": ["Dein", "En\u00b7gel", "f\u00fchr'", "oft", "dich", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am stillen Abend in den Hain,", "tokens": ["Am", "stil\u00b7len", "A\u00b7bend", "in", "den", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und zeige dir auf jeder Flur", "tokens": ["Und", "zei\u00b7ge", "dir", "auf", "je\u00b7der", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den guten Sch\u00f6pfer der Natur!", "tokens": ["Den", "gu\u00b7ten", "Sch\u00f6p\u00b7fer", "der", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nimm, wie der Mittler einst gethan,", "tokens": ["Nimm", ",", "wie", "der", "Mitt\u00b7ler", "einst", "ge\u00b7than", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich jedes armen Bruders an!", "tokens": ["Dich", "je\u00b7des", "ar\u00b7men", "Bru\u00b7ders", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wisch ihm die Thr\u00e4ne vom Gesicht!", "tokens": ["Wisch", "ihm", "die", "Thr\u00e4\u00b7ne", "vom", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Doch meide, selbst zu weinen, nicht!", "tokens": ["Doch", "mei\u00b7de", ",", "selbst", "zu", "wei\u00b7nen", ",", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "PTKZU", "VVINF", "$,", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ein M\u00e4dchen, gut und rein wie du,", "tokens": ["Ein", "M\u00e4d\u00b7chen", ",", "gut", "und", "rein", "wie", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "KOKOM", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Eil deinem Arm, als Freundin, zu!", "tokens": ["Eil", "dei\u00b7nem", "Arm", ",", "als", "Freun\u00b7din", ",", "zu", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "KOUS", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann komme Lieb', und winke dir,", "tokens": ["Dann", "kom\u00b7me", "Lieb'", ",", "und", "win\u00b7ke", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schenke deine Seele mir!", "tokens": ["Und", "schen\u00b7ke", "dei\u00b7ne", "See\u00b7le", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}