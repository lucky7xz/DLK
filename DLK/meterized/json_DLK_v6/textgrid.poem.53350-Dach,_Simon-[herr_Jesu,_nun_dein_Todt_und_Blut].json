{"textgrid.poem.53350": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[herr Jesu, nun dein Todt und Blut]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Jesu, nun dein Todt und Blut", "tokens": ["Herr", "Je\u00b7su", ",", "nun", "dein", "Todt", "und", "Blut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "$,", "ADV", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Trost ist und mein h\u00f6chstes Gut", "tokens": ["Mein", "Trost", "ist", "und", "mein", "h\u00f6chs\u00b7tes", "Gut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Darauff ich sicher mich kan gr\u00fcnden:", "tokens": ["Dar\u00b7auff", "ich", "si\u00b7cher", "mich", "kan", "gr\u00fcn\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADJD", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dadurch ich bin gerecht und frey", "tokens": ["Da\u00b7durch", "ich", "bin", "ge\u00b7recht", "und", "frey"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "VAFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von de\u00df Gesetzes Tyranney", "tokens": ["Von", "de\u00df", "Ge\u00b7set\u00b7zes", "Ty\u00b7ran\u00b7ney"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd lohs von allen meinen S\u00fcnden,", "tokens": ["Vnd", "lohs", "von", "al\u00b7len", "mei\u00b7nen", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So wei\u00df ich durch de\u00df Glaubens Sinn", "tokens": ["So", "wei\u00df", "ich", "durch", "de\u00df", "Glau\u00b7bens", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich mit GOTT vertragen bin,", "tokens": ["Da\u00df", "ich", "mit", "GoTT", "ver\u00b7tra\u00b7gen", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd er mir zuneigt sein Gem\u00fcte:", "tokens": ["Vnd", "er", "mir", "zu\u00b7neigt", "sein", "Ge\u00b7m\u00fc\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mich als ein Vater hertzlich liebt,", "tokens": ["Mich", "als", "ein", "Va\u00b7ter", "hertz\u00b7lich", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mir seinem Kind auch zugang giebt", "tokens": ["Mir", "sei\u00b7nem", "Kind", "auch", "zu\u00b7gang", "giebt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu seiner V\u00e4terlichen G\u00fcte.", "tokens": ["Zu", "sei\u00b7ner", "V\u00e4\u00b7ter\u00b7li\u00b7chen", "G\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Daher ich in de\u00df Todes Bann", "tokens": ["Da\u00b7her", "ich", "in", "de\u00df", "To\u00b7des", "Bann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich auch der Hoffnung r\u00fchmen kan,", "tokens": ["Mich", "auch", "der", "Hoff\u00b7nung", "r\u00fch\u00b7men", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Herrligkeit nach diesem Leben", "tokens": ["Der", "Herr\u00b7lig\u00b7keit", "nach", "die\u00b7sem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Gott, wann wir im Glauben stehn", "tokens": ["Die", "Gott", ",", "wann", "wir", "im", "Glau\u00b7ben", "stehn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd auff der Vnschuld Wegen gehn,", "tokens": ["Vnd", "auff", "der", "Vn\u00b7schuld", "We\u00b7gen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vns sich versprochen hat zu geben.", "tokens": ["Vns", "sich", "ver\u00b7spro\u00b7chen", "hat", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "VVPP", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}}}}}