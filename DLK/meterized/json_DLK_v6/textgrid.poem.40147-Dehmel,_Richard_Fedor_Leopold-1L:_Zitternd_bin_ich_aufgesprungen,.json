{"textgrid.poem.40147": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zitternd bin ich aufgesprungen,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zitternd bin ich aufgesprungen,", "tokens": ["Zit\u00b7ternd", "bin", "ich", "auf\u00b7ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "gl\u00fchend, mit dem Tageslichte,", "tokens": ["gl\u00fc\u00b7hend", ",", "mit", "dem", "Ta\u00b7ges\u00b7lich\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "dir zu singen die Gedichte,", "tokens": ["dir", "zu", "sin\u00b7gen", "die", "Ge\u00b7dich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "die im Traum ich dir gesungen:", "tokens": ["die", "im", "Traum", "ich", "dir", "ge\u00b7sun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "nie ert\u00f6nte Zauberkl\u00e4nge,", "tokens": ["nie", "er\u00b7t\u00f6n\u00b7te", "Zau\u00b7ber\u00b7kl\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wunderzarte, weiche, milde, \u2013", "tokens": ["wun\u00b7derz\u00b7ar\u00b7te", ",", "wei\u00b7che", ",", "mil\u00b7de", ",", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "ADJA", "$,", "ADJD", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "nie erbrauste, hei\u00dfe, wilde,", "tokens": ["nie", "er\u00b7braus\u00b7te", ",", "hei\u00b7\u00dfe", ",", "wil\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "heilig rauschende Ges\u00e4nge;", "tokens": ["hei\u00b7lig", "rau\u00b7schen\u00b7de", "Ge\u00b7s\u00e4n\u00b7ge", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "mu\u00dften alle alle preisen", "tokens": ["mu\u00df\u00b7ten", "al\u00b7le", "al\u00b7le", "prei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PIAT", "PIS", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deinen, immer Deinen Namen,", "tokens": ["Dei\u00b7nen", ",", "im\u00b7mer", "Dei\u00b7nen", "Na\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$,", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "bis der Erde V\u00f6lker kamen,", "tokens": ["bis", "der", "Er\u00b7de", "V\u00f6l\u00b7ker", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "fromm zu lauschen meinen Weisen;", "tokens": ["fromm", "zu", "lau\u00b7schen", "mei\u00b7nen", "Wei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "kamen aus den fernsten Landen,", "tokens": ["ka\u00b7men", "aus", "den", "ferns\u00b7ten", "Lan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "sprachen wohl in ", "tokens": ["spra\u00b7chen", "wohl", "in"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "aber wie sie ", "tokens": ["a\u00b7ber", "wie", "sie"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWAV", "PPER"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "haben Alle mich verstanden.", "tokens": ["ha\u00b7ben", "Al\u00b7le", "mich", "ver\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Eines nur der tausend Lieder,", "tokens": ["Ei\u00b7nes", "nur", "der", "tau\u00b7send", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ART", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eines nur noch Einmal singen!", "tokens": ["Ei\u00b7nes", "nur", "noch", "Ein\u00b7mal", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "ewig w\u00fcrd' es weiterklingen!", "tokens": ["e\u00b7wig", "w\u00fcrd'", "es", "wei\u00b7ter\u00b7klin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, ich finde keines wieder:", "tokens": ["Ach", ",", "ich", "fin\u00b7de", "kei\u00b7nes", "wie\u00b7der", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PIS", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "tief im Herzen welch ein Bangen,", "tokens": ["tief", "im", "Her\u00b7zen", "welch", "ein", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "PWAT", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "welch ein zagendes Beginnen,", "tokens": ["welch", "ein", "za\u00b7gen\u00b7des", "Be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "welch ein bebendes Besinnen,", "tokens": ["welch", "ein", "be\u00b7ben\u00b7des", "Be\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "seit du ", "tokens": ["seit", "du"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "-+", "measure": "iambic.single"}}}}}