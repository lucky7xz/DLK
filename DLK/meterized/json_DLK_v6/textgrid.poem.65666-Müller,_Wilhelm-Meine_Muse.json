{"textgrid.poem.65666": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Meine Muse", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbund willst du, meine Muse, denn gar zur Meg\u00e4ra werden?", "tokens": ["\u00bb", "und", "willst", "du", ",", "mei\u00b7ne", "Mu\u00b7se", ",", "denn", "gar", "zur", "Me\u00b7g\u00e4\u00b7ra", "wer\u00b7den", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VMFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "KON", "ADV", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+-++--+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Du sangst noch j\u00fcngst im stillen Hain den Hirten und den Heerden,", "tokens": ["Du", "sangst", "noch", "j\u00fcngst", "im", "stil\u00b7len", "Hain", "den", "Hir\u00b7ten", "und", "den", "Heer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPRART", "ADJA", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Und nun schwingst eine Gei\u00dfel du laut durch die lauten Gassen,", "tokens": ["Und", "nun", "schwingst", "ei\u00b7ne", "Gei\u00b7\u00dfel", "du", "laut", "durch", "die", "lau\u00b7ten", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--++-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Und spr\u00fchest Flammen um dich her \u2013 Ich wei\u00df dich nicht zu fassen.\u00ab", "tokens": ["Und", "spr\u00fc\u00b7hest", "Flam\u00b7men", "um", "dich", "her", "\u2013", "Ich", "wei\u00df", "dich", "nicht", "zu", "fas\u00b7sen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "PRF", "APZR", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "Du fragst? Siehst du die Hirten nicht nach scharfen Eisen greifen?", "tokens": ["Du", "fragst", "?", "Siehst", "du", "die", "Hir\u00b7ten", "nicht", "nach", "schar\u00b7fen", "Ei\u00b7sen", "grei\u00b7fen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Siehst statt der L\u00e4mmer W\u00f6lfe nicht Arkadien durchstreifen?", "tokens": ["Siehst", "statt", "der", "L\u00e4m\u00b7mer", "W\u00f6l\u00b7fe", "nicht", "Ar\u00b7ka\u00b7di\u00b7en", "durch\u00b7strei\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "PTKNEG", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Siehst in Epirus Felsen nicht die Weiber Schwerter wetzen?", "tokens": ["Siehst", "in", "E\u00b7pi\u00b7rus", "Fel\u00b7sen", "nicht", "die", "Wei\u00b7ber", "Schwer\u00b7ter", "wet\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "NN", "PTKNEG", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Siehst du auf Sparta's Fluren", "tokens": ["Siehst", "du", "auf", "Spar\u00b7ta's", "Flu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Da mu\u00dft' ich Hirtens\u00e4ngerin mein Haferrohr zerbrechen,", "tokens": ["Da", "mu\u00dft'", "ich", "Hir\u00b7ten\u00b7s\u00e4n\u00b7ge\u00b7rin", "mein", "Ha\u00b7fer\u00b7rohr", "zer\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "Und, wie's die scharfe Zeit gebeut, in scharfen T\u00f6nen sprechen.", "tokens": ["Und", ",", "wie's", "die", "schar\u00b7fe", "Zeit", "ge\u00b7beut", ",", "in", "schar\u00b7fen", "T\u00f6\u00b7nen", "spre\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.11": {"text": "Der Freiheit Tuba hab' ich hell durch Stadt und Land geblasen:", "tokens": ["Der", "Frei\u00b7heit", "Tu\u00b7ba", "hab'", "ich", "hell", "durch", "Stadt", "und", "Land", "ge\u00b7bla\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.12": {"text": "La\u00df meine Gei\u00dfel nun um's Haupt der Pharis\u00e4er rasen!", "tokens": ["La\u00df", "mei\u00b7ne", "Gei\u00b7\u00dfel", "nun", "um's", "Haupt", "der", "Pha\u00b7ri\u00b7s\u00e4\u00b7er", "ra\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADV", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}