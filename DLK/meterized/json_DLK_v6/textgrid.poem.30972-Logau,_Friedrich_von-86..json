{"textgrid.poem.30972": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "86.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcrsten m\u00fcgen leben herrlich;", "tokens": ["F\u00fcrs\u00b7ten", "m\u00fc\u00b7gen", "le\u00b7ben", "herr\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "VVINF", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dann sie leben auch beschwerlich.", "tokens": ["Dann", "sie", "le\u00b7ben", "auch", "be\u00b7schwer\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil sie andren Wolfahrt geben,", "tokens": ["Weil", "sie", "an\u00b7dren", "Wol\u00b7fahrt", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00fcgen sie vergn\u00fcgt ja leben.", "tokens": ["M\u00fc\u00b7gen", "sie", "ver\u00b7gn\u00fcgt", "ja", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}