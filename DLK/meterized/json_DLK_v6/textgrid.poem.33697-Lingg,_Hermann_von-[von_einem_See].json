{"textgrid.poem.33697": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "[von einem See]", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von einem See", "tokens": ["Von", "ei\u00b7nem", "See"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Im sch\u00f6nen Hellas ging die Sage:", "tokens": ["Im", "sch\u00f6\u00b7nen", "Hel\u00b7las", "ging", "die", "Sa\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df wer es je", "tokens": ["Da\u00df", "wer", "es", "je"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PWS", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "In diesen See zu schauen wage,", "tokens": ["In", "die\u00b7sen", "See", "zu", "schau\u00b7en", "wa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zum Schattenreich", "tokens": ["Zum", "Schat\u00b7ten\u00b7reich"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Zieh's den hinunter in die Tiefe", "tokens": ["Zieh's", "den", "hin\u00b7un\u00b7ter", "in", "die", "Tie\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "ADV", "APPR", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Und wenn er gleich", "tokens": ["Und", "wenn", "er", "gleich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Um Rettung alle G\u00f6tter riefe.", "tokens": ["Um", "Ret\u00b7tung", "al\u00b7le", "G\u00f6t\u00b7ter", "rie\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ja, so bist du!", "tokens": ["Ja", ",", "so", "bist", "du", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VAFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wem deine dunklen Blicke winken,", "tokens": ["Wem", "dei\u00b7ne", "dunk\u00b7len", "Bli\u00b7cke", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der sehe zu!", "tokens": ["Der", "se\u00b7he", "zu", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Er wird so tief in sie versinken,", "tokens": ["Er", "wird", "so", "tief", "in", "sie", "ver\u00b7sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df keine Macht", "tokens": ["Da\u00df", "kei\u00b7ne", "Macht"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Je mehr erl\u00f6st aus diesen T\u00fccken:", "tokens": ["Je", "mehr", "er\u00b7l\u00f6st", "aus", "die\u00b7sen", "T\u00fc\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ihn wird die Nacht", "tokens": ["Ihn", "wird", "die", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Ans stumme Herz auf ewig dr\u00fccken.", "tokens": ["Ans", "stum\u00b7me", "Herz", "auf", "e\u00b7wig", "dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}