{"textgrid.poem.31644": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "16. Ich traue auf den Herrn / etc.", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Flieg nicht / mein erl\u00f6ste Seel / auf die Hoffart-St\u00fcrzungs-Berge /", "tokens": ["Flieg", "nicht", "/", "mein", "er\u00b7l\u00f6s\u00b7te", "Seel", "/", "auf", "die", "Hoff\u00b7ar\u00b7tSt\u00fcr\u00b7zungs\u00b7Ber\u00b7ge", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$(", "PPOSAT", "ADJA", "NN", "$(", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "noch auff die Sictlisch' H\u00f6he / schn\u00f6der Flamm verdampter Brand.", "tokens": ["noch", "auff", "die", "Sict\u00b7lisch'", "H\u00f6\u00b7he", "/", "schn\u00f6\u00b7der", "Flamm", "ver\u00b7damp\u00b7ter", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$(", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Hoffart / spricht Gott / ist das jenig / welches ich zu meist verherge.", "tokens": ["Hof\u00b7fart", "/", "spricht", "Gott", "/", "ist", "das", "je\u00b7nig", "/", "wel\u00b7ches", "ich", "zu", "meist", "ver\u00b7her\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVFIN", "NN", "$(", "VAFIN", "ART", "ADJD", "$(", "PWS", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wer die Hoheit kan verachten / wird gar billich hoch genandt.", "tokens": ["Wer", "die", "Ho\u00b7heit", "kan", "ver\u00b7ach\u00b7ten", "/", "wird", "gar", "bil\u00b7lich", "hoch", "ge\u00b7nandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VMFIN", "VVFIN", "$(", "VAFIN", "ADV", "ADJD", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Bleibet fort auf euren Bergen! mir beliebt der Demut Thal.", "tokens": ["Blei\u00b7bet", "fort", "auf", "eu\u00b7ren", "Ber\u00b7gen", "!", "mir", "be\u00b7liebt", "der", "De\u00b7mut", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Wird sich alles / wie man sagt / in erwarter Zeit verkehren:", "tokens": ["Wird", "sich", "al\u00b7les", "/", "wie", "man", "sagt", "/", "in", "er\u00b7war\u00b7ter", "Zeit", "ver\u00b7keh\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "PIS", "$(", "PWAV", "PIS", "VVFIN", "$(", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "werd ich so dann seyn versetzt aus dem Staub in Thron der Ehren;", "tokens": ["werd", "ich", "so", "dann", "seyn", "ver\u00b7setzt", "aus", "dem", "Staub", "in", "Thron", "der", "Eh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VAINF", "VVFIN", "APPR", "ART", "NN", "APPR", "NN", "ART", "NN", "$."], "meter": "+---+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "ihr / aus diesem in den Staub / durch der Hoffart Donnerstrahl.", "tokens": ["ihr", "/", "aus", "die\u00b7sem", "in", "den", "Staub", "/", "durch", "der", "Hof\u00b7fart", "Don\u00b7ner\u00b7strahl", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "APPR", "PDAT", "APPR", "ART", "NN", "$(", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}}}}}