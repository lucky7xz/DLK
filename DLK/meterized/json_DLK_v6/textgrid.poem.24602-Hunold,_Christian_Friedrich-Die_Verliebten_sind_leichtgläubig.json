{"textgrid.poem.24602": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Verliebten sind leichtgl\u00e4ubig", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unbestand/ Betrug/ und leicht ist der Sch\u00e4fer sch\u00f6nstes Lieben/", "tokens": ["Un\u00b7be\u00b7stand", "/", "Be\u00b7trug", "/", "und", "leicht", "ist", "der", "Sch\u00e4\u00b7fer", "sch\u00f6ns\u00b7tes", "Lie\u00b7ben", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "KON", "ADJD", "VAFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Und die Sch\u00f6n' ist unbegl\u00fcckt/ die so einem sich verschrieben/", "tokens": ["Und", "die", "Sch\u00f6n'", "ist", "un\u00b7be\u00b7gl\u00fcckt", "/", "die", "so", "ei\u00b7nem", "sich", "ver\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "$(", "ART", "ADV", "ART", "PRF", "VVINF", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Tausend proben mir zum Schmertzen/ machen es zusehr bekandt.", "tokens": ["Tau\u00b7send", "pro\u00b7ben", "mir", "zum", "Schmert\u00b7zen", "/", "ma\u00b7chen", "es", "zu\u00b7sehr", "be\u00b7kandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "PPER", "APPRART", "NN", "$(", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Doch so gut auch diese Regul von der Sch\u00e4fer Unbestand/", "tokens": ["Doch", "so", "gut", "auch", "die\u00b7se", "Re\u00b7gul", "von", "der", "Sch\u00e4\u00b7fer", "Un\u00b7be\u00b7stand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ADV", "PDAT", "NN", "APPR", "ART", "NN", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Ach so glaubt man/ wenn das Hertz kaum die Flammen eingesogen/", "tokens": ["Ach", "so", "glaubt", "man", "/", "wenn", "das", "Hertz", "kaum", "die", "Flam\u00b7men", "ein\u00b7ge\u00b7so\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "PIS", "$(", "KOUS", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Da\u00df ein Sch\u00e4fer uns getreu/ und vollkommen sey gewogen.", "tokens": ["Da\u00df", "ein", "Sch\u00e4\u00b7fer", "uns", "ge\u00b7treu", "/", "und", "voll\u00b7kom\u00b7men", "sey", "ge\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADJD", "$(", "KON", "ADJD", "VAFIN", "VVPP", "$."], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}