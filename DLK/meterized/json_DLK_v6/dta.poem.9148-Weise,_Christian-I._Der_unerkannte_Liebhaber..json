{"dta.poem.9148": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "I.  \n Der unerkannte Liebhaber.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Mein liebgen will es nicht verstehn/", "tokens": ["Mein", "lieb\u00b7gen", "will", "es", "nicht", "ver\u00b7stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "VMFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich in sie verliebet bin/", "tokens": ["Da\u00df", "ich", "in", "sie", "ver\u00b7lie\u00b7bet", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie kan vor mir vor\u00fcber gehn/", "tokens": ["Sie", "kan", "vor", "mir", "vor\u00b7\u00fc\u00b7ber", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als h\u00e4tte mein getreuer sinn/", "tokens": ["Als", "h\u00e4t\u00b7te", "mein", "ge\u00b7treu\u00b7er", "sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der ihre lieb und gunst begehrt/", "tokens": ["Der", "ih\u00b7re", "lieb", "und", "gunst", "be\u00b7gehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJD", "KON", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich nach der gn\u00fcge nicht erkl\u00e4rt.", "tokens": ["Sich", "nach", "der", "gn\u00fc\u00b7ge", "nicht", "er\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Ich kan die aller sch\u00f6nsten blicke", "tokens": ["Ich", "kan", "die", "al\u00b7ler", "sch\u00f6ns\u00b7ten", "bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geniessen als ein guter Freund;", "tokens": ["Ge\u00b7nies\u00b7sen", "als", "ein", "gu\u00b7ter", "Freund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch hab ich nicht das hohe gl\u00fccke/", "tokens": ["Doch", "hab", "ich", "nicht", "das", "ho\u00b7he", "gl\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df sie es in dem hertzen meint/", "tokens": ["Da\u00df", "sie", "es", "in", "dem", "hert\u00b7zen", "meint", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df sie auff mich viel kundschafft legt", "tokens": ["Da\u00df", "sie", "auff", "mich", "viel", "kund\u00b7schafft", "legt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "PIAT", "NN", "VVFIN"], "meter": "--+-++-+", "measure": "anapaest.init"}, "line.6": {"text": "Und ein verlangen nach mir tr\u00e4gt.", "tokens": ["Und", "ein", "ver\u00b7lan\u00b7gen", "nach", "mir", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Wil ich bey ihr die zeit vertreiben", "tokens": ["Wil", "ich", "bey", "ihr", "die", "zeit", "ver\u00b7trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "ART", "NN", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "So l\u00e4st sie mich mit willen ein;", "tokens": ["So", "l\u00e4st", "sie", "mich", "mit", "wil\u00b7len", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jedennoch will ich aussenbleiben/", "tokens": ["Je\u00b7den\u00b7noch", "will", "ich", "aus\u00b7sen\u00b7blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So kan sie auch zufrieden seyn/", "tokens": ["So", "kan", "sie", "auch", "zu\u00b7frie\u00b7den", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie henckt deswegen vor das hau\u00df", "tokens": ["Sie", "henckt", "des\u00b7we\u00b7gen", "vor", "das", "hau\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gar keinen schwartzen flor hinau\u00df.", "tokens": ["Gar", "kei\u00b7nen", "schwart\u00b7zen", "flor", "hin\u00b7au\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Und was mich treflich kan verdriessen/", "tokens": ["Und", "was", "mich", "tref\u00b7lich", "kan", "ver\u00b7dries\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So ist das lose tausend-kind", "tokens": ["So", "ist", "das", "lo\u00b7se", "tau\u00b7sen\u00b7dkind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor allen andern drauff beflissen/", "tokens": ["Vor", "al\u00b7len", "an\u00b7dern", "drauff", "be\u00b7flis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PIS", "PAV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sie mir reden abgewinnt/", "tokens": ["Wie", "sie", "mir", "re\u00b7den", "ab\u00b7ge\u00b7winnt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVINF", "VVPP", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und meine brust hindangesetzt", "tokens": ["Und", "mei\u00b7ne", "brust", "hin\u00b7dan\u00b7ge\u00b7setzt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mich nur mit fremden m\u00e4dgen hetzt.", "tokens": ["Mich", "nur", "mit", "frem\u00b7den", "m\u00e4d\u00b7gen", "hetzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Ich darff dieselben nicht verachten/", "tokens": ["Ich", "darff", "die\u00b7sel\u00b7ben", "nicht", "ver\u00b7ach\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDS", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denn sonsten hie\u00df es alsobald/", "tokens": ["Denn", "sons\u00b7ten", "hie\u00df", "es", "al\u00b7so\u00b7bald", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df wir es nirgend anders machten/", "tokens": ["Da\u00df", "wir", "es", "nir\u00b7gend", "an\u00b7ders", "mach\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und w\u00e4ren weder warm noch kalt:", "tokens": ["Und", "w\u00e4\u00b7ren", "we\u00b7der", "warm", "noch", "kalt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "KON", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und gleichwohl wenn ich freundlich thu", "tokens": ["Und", "gleich\u00b7wohl", "wenn", "ich", "freund\u00b7lich", "thu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOUS", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So heists ich spreche ja darzu.", "tokens": ["So", "heists", "ich", "spre\u00b7che", "ja", "dar\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ADV", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Ich darff ihr zwar die H\u00e4nde dr\u00fccken/", "tokens": ["Ich", "darff", "ihr", "zwar", "die", "H\u00e4n\u00b7de", "dr\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die meinen aber dr\u00fcckt sie nicht/", "tokens": ["Die", "mei\u00b7nen", "a\u00b7ber", "dr\u00fcckt", "sie", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich spiele mit verkehrten blicken/", "tokens": ["Ich", "spie\u00b7le", "mit", "ver\u00b7kehr\u00b7ten", "bli\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sie beh\u00e4lt ihr angesicht/", "tokens": ["Und", "sie", "be\u00b7h\u00e4lt", "ihr", "an\u00b7ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein fu\u00df der st\u00f6sst sie ungefehr/", "tokens": ["Mein", "fu\u00df", "der", "st\u00f6sst", "sie", "un\u00b7ge\u00b7fehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Jedoch sie st\u00f6sst nicht wieder her.", "tokens": ["Je\u00b7doch", "sie", "st\u00f6sst", "nicht", "wie\u00b7der", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "7. Den s\u00fcssen purpur-mund zu k\u00fcssen", "tokens": ["Den", "s\u00fcs\u00b7sen", "pur\u00b7pur\u00b7mund", "zu", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geht mir zu schwer und sauer ein/", "tokens": ["Geht", "mir", "zu", "schwer", "und", "sau\u00b7er", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKA", "ADJD", "KON", "ADJD", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dieweil ich mich bef\u00fcrchten m\u00fcssen", "tokens": ["Die\u00b7weil", "ich", "mich", "be\u00b7f\u00fcrch\u00b7ten", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es m\u00f6cht ihr nicht belieblich seyn/", "tokens": ["Es", "m\u00f6cht", "ihr", "nicht", "be\u00b7lieb\u00b7lich", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wiewohl was hilfft ein kalter ku\u00df/", "tokens": ["Wie\u00b7wohl", "was", "hilfft", "ein", "kal\u00b7ter", "ku\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Indem man sonsten warten mu\u00df.", "tokens": ["In\u00b7dem", "man", "sons\u00b7ten", "war\u00b7ten", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "8. Ich schwatze viel von liebes-sachen", "tokens": ["Ich", "schwat\u00b7ze", "viel", "von", "lie\u00b7bes\u00b7sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie es die leutgen in der welt", "tokens": ["Wie", "es", "die", "leut\u00b7gen", "in", "der", "welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An dem und jenem orte machen/", "tokens": ["An", "dem", "und", "je\u00b7nem", "or\u00b7te", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "KON", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und alles was mir wohl gef\u00e4llt/", "tokens": ["Und", "al\u00b7les", "was", "mir", "wohl", "ge\u00b7f\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PWS", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das macht mein h\u00f6chst-verliebter mund", "tokens": ["Das", "macht", "mein", "h\u00f6chst\u00b7ver\u00b7lieb\u00b7ter", "mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Jhr durch verbl\u00fcmte reden kund.", "tokens": ["Ihr", "durch", "ver\u00b7bl\u00fcm\u00b7te", "re\u00b7den", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "9. Ich kan sie aber nicht erwischen/", "tokens": ["Ich", "kan", "sie", "a\u00b7ber", "nicht", "er\u00b7wi\u00b7schen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie schl\u00e4gt die reden in den wind/", "tokens": ["Sie", "schl\u00e4gt", "die", "re\u00b7den", "in", "den", "wind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "APPR", "ART", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich mu\u00df in einem teiche fischen", "tokens": ["Ich", "mu\u00df", "in", "ei\u00b7nem", "tei\u00b7che", "fi\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da keine fische drinnen sind/", "tokens": ["Da", "kei\u00b7ne", "fi\u00b7sche", "drin\u00b7nen", "sind", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "ADV", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So eilt die junge zeit dahin/", "tokens": ["So", "eilt", "die", "jun\u00b7ge", "zeit", "da\u00b7hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PAV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich stets aus mir selber bin.", "tokens": ["Da\u00df", "ich", "stets", "aus", "mir", "sel\u00b7ber", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "10. Und also seh ich meine freude", "tokens": ["Und", "al\u00b7so", "seh", "ich", "mei\u00b7ne", "freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur zwischen furcht und hoffnung stehn/", "tokens": ["Nur", "zwi\u00b7schen", "furcht", "und", "hoff\u00b7nung", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und kan in meinem hertzen-leide", "tokens": ["Und", "kan", "in", "mei\u00b7nem", "hert\u00b7zen\u00b7lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der wollust nicht entgegen gehn/", "tokens": ["Der", "wol\u00b7lust", "nicht", "ent\u00b7ge\u00b7gen", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PTKNEG", "PTKVZ", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der himmel hat es so gef\u00fcgt/", "tokens": ["Der", "him\u00b7mel", "hat", "es", "so", "ge\u00b7f\u00fcgt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich liebe wol/ doch unvergn\u00fcgt.", "tokens": ["Ich", "lie\u00b7be", "wol", "/", "doch", "un\u00b7ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}