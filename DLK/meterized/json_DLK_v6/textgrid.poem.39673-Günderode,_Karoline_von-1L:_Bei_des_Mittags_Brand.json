{"textgrid.poem.39673": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bei des Mittags Brand", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bei des Mittags Brand", "tokens": ["Bei", "des", "Mit\u00b7tags", "Brand"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wo der W\u00fcste Sand", "tokens": ["Wo", "der", "W\u00fcs\u00b7te", "Sand"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Kein k\u00fchlend L\u00fcftchen erlabet,", "tokens": ["Kein", "k\u00fch\u00b7lend", "L\u00fcft\u00b7chen", "er\u00b7la\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wo hei\u00df, vom Samum nur gek\u00fcsset,", "tokens": ["Wo", "hei\u00df", ",", "vom", "Sa\u00b7mum", "nur", "ge\u00b7k\u00fcs\u00b7set", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein grauer Fels die Wolken gr\u00fc\u00dfet", "tokens": ["Ein", "grau\u00b7er", "Fels", "die", "Wol\u00b7ken", "gr\u00fc\u00b7\u00dfet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da sinket m\u00fcd der Seher hin.", "tokens": ["Da", "sin\u00b7ket", "m\u00fcd", "der", "Se\u00b7her", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vom tr\u00fcgenden Schein", "tokens": ["Vom", "tr\u00fc\u00b7gen\u00b7den", "Schein"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Will der Dinge Seyn", "tokens": ["Will", "der", "Din\u00b7ge", "Seyn"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "PPOSAT"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sein Geist, betrachtend hier, trennen.", "tokens": ["Sein", "Geist", ",", "be\u00b7trach\u00b7tend", "hier", ",", "tren\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVPP", "ADV", "$,", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Zukunft Geist will er beschw\u00f6ren,", "tokens": ["Der", "Zu\u00b7kunft", "Geist", "will", "er", "be\u00b7schw\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Des eignen Herzens Stimme h\u00f6ren,", "tokens": ["Des", "eig\u00b7nen", "Her\u00b7zens", "Stim\u00b7me", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und folgen seiner Eingebung.", "tokens": ["Und", "fol\u00b7gen", "sei\u00b7ner", "Ein\u00b7ge\u00b7bung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Hier flieht die Gottheit,", "tokens": ["Hier", "flieht", "die", "Got\u00b7theit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Die der Wahn ihm leiht,", "tokens": ["Die", "der", "Wahn", "ihm", "leiht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Der eitle Schimmer verstiebet.", "tokens": ["Der", "eit\u00b7le", "Schim\u00b7mer", "ver\u00b7stie\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und ihn, auf den die V\u00f6lker sehen,", "tokens": ["Und", "ihn", ",", "auf", "den", "die", "V\u00f6l\u00b7ker", "se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPR", "ART", "ART", "NN", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Den Siegespalmen nur umwehen,", "tokens": ["Den", "Sie\u00b7ge\u00b7spal\u00b7men", "nur", "um\u00b7we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Umkreist der Sorgen dunkle Nacht.", "tokens": ["Um\u00b7kreist", "der", "Sor\u00b7gen", "dunk\u00b7le", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Des Sehers Traum", "tokens": ["Des", "Se\u00b7hers", "Traum"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Durchflieget den Raum", "tokens": ["Durch\u00b7flie\u00b7get", "den", "Raum"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und all' die k\u00fcnftigen Zeiten,", "tokens": ["Und", "all'", "die", "k\u00fcnf\u00b7ti\u00b7gen", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bald kostet er, in trunknem Wahne,", "tokens": ["Bald", "kos\u00b7tet", "er", ",", "in", "trunk\u00b7nem", "Wah\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Seligkeit gelung'ner Plane,", "tokens": ["Die", "Se\u00b7lig\u00b7keit", "ge\u00b7lung'\u00b7ner", "Pla\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dann sieht er seinen Untergang,", "tokens": ["Dann", "sieht", "er", "sei\u00b7nen", "Un\u00b7ter\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Entsetzen und Wuth,", "tokens": ["Ent\u00b7set\u00b7zen", "und", "Wuth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Mit wechselnder Fluth,", "tokens": ["Mit", "wech\u00b7seln\u00b7der", "Fluth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "K\u00e4mpfen im innersten Leben,", "tokens": ["K\u00e4mp\u00b7fen", "im", "in\u00b7ners\u00b7ten", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von Zweifeln, ruft er, nur umgeben!", "tokens": ["Von", "Zwei\u00b7feln", ",", "ruft", "er", ",", "nur", "um\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "PPER", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Verhauchet der Entschlu\u00df sein Leben!", "tokens": ["Ver\u00b7hau\u00b7chet", "der", "Ent\u00b7schlu\u00df", "sein", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Eh' Reu ihn und Mi\u00dflingen straft.", "tokens": ["Eh'", "Reu", "ihn", "und", "Mi\u00df\u00b7lin\u00b7gen", "straft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPER", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Gottheit Macht,", "tokens": ["Der", "Got\u00b7theit", "Macht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Zerrei\u00dfe die Nacht", "tokens": ["Zer\u00b7rei\u00b7\u00dfe", "die", "Nacht"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Des Schicksals, vor meinen Blicken!", "tokens": ["Des", "Schick\u00b7sals", ",", "vor", "mei\u00b7nen", "Bli\u00b7cken", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie lasse mich die Zukunft sehen,", "tokens": ["Sie", "las\u00b7se", "mich", "die", "Zu\u00b7kunft", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ob meine Fahnen siegreich wehen?", "tokens": ["Ob", "mei\u00b7ne", "Fah\u00b7nen", "sieg\u00b7reich", "we\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ob mein Gesetz die Welt regiert?", "tokens": ["Ob", "mein", "Ge\u00b7setz", "die", "Welt", "re\u00b7giert", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Er sprichts; da bebt", "tokens": ["Er", "sprichts", ";", "da", "bebt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ADV", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die Erde, es hebt", "tokens": ["Die", "Er\u00b7de", ",", "es", "hebt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die See sich auf zu den Wolken,", "tokens": ["Die", "See", "sich", "auf", "zu", "den", "Wol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Flammen entlodern den Felsenkl\u00fcften,", "tokens": ["Flam\u00b7men", "ent\u00b7lo\u00b7dern", "den", "Fel\u00b7sen\u00b7kl\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Die Luft, erf\u00fcllt von Schwefeld\u00fcften,", "tokens": ["Die", "Luft", ",", "er\u00b7f\u00fcllt", "von", "Schwe\u00b7fel\u00b7d\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "L\u00e4\u00dft tr\u00e4g die m\u00fcden Schwingen ruhn.", "tokens": ["L\u00e4\u00dft", "tr\u00e4g", "die", "m\u00fc\u00b7den", "Schwin\u00b7gen", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Im wilden Tanz,", "tokens": ["Im", "wil\u00b7den", "Tanz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Umschlinget der Kranz", "tokens": ["Um\u00b7schlin\u00b7get", "der", "Kranz"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Der irren Sterne, die Himmel;", "tokens": ["Der", "ir\u00b7ren", "Ster\u00b7ne", ",", "die", "Him\u00b7mel", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das Meer erbrau\u00dft in seinen Gr\u00fcnden,", "tokens": ["Das", "Meer", "er\u00b7brau\u00dft", "in", "sei\u00b7nen", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und in der Erde tiefsten Schl\u00fcnden", "tokens": ["Und", "in", "der", "Er\u00b7de", "tiefs\u00b7ten", "Schl\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Streiten die Elemente sich.", "tokens": ["Strei\u00b7ten", "die", "E\u00b7le\u00b7men\u00b7te", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.9": {"line.1": {"text": "Und der Eintracht Band,", "tokens": ["Und", "der", "Ein\u00b7tracht", "Band", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Das m\u00e4chtig umwand", "tokens": ["Das", "m\u00e4ch\u00b7tig", "um\u00b7wand"], "token_info": ["word", "word", "word"], "pos": ["PDS", "ADJD", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die Kr\u00e4fte, es schien gel\u00f6set.", "tokens": ["Die", "Kr\u00e4f\u00b7te", ",", "es", "schien", "ge\u00b7l\u00f6\u00b7set", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Luft entsinkt der Wolken Schleier", "tokens": ["Der", "Luft", "ent\u00b7sinkt", "der", "Wol\u00b7ken", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und aus dem Abgrund steigt das Feuer,", "tokens": ["Und", "aus", "dem", "Ab\u00b7grund", "steigt", "das", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und zehret alles Ird'sche auf.", "tokens": ["Und", "zeh\u00b7ret", "al\u00b7les", "Ird'\u00b7sche", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Mit tr\u00fcberer Fluth", "tokens": ["Mit", "tr\u00fc\u00b7be\u00b7rer", "Fluth"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Steigt erst die Gluth,", "tokens": ["Steigt", "erst", "die", "Gluth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Doch brennt sie stets sich reiner,", "tokens": ["Doch", "brennt", "sie", "stets", "sich", "rei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "PRF", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bis hell ein Lichtmeer ihr entsteiget", "tokens": ["Bis", "hell", "ein", "Licht\u00b7meer", "ihr", "ent\u00b7stei\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das lodernd zu den Sternen reichet", "tokens": ["Das", "lo\u00b7dernd", "zu", "den", "Ster\u00b7nen", "rei\u00b7chet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und rein, und hell, und strahlend wallt.", "tokens": ["Und", "rein", ",", "und", "hell", ",", "und", "strah\u00b7lend", "wallt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KON", "ADJD", "$,", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Der Seher erwacht", "tokens": ["Der", "Se\u00b7her", "er\u00b7wacht"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Wie aus Grabesnacht", "tokens": ["Wie", "aus", "Gra\u00b7bes\u00b7nacht"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und staunend f\u00fchlt er sich leben,", "tokens": ["Und", "stau\u00b7nend", "f\u00fchlt", "er", "sich", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Erwachet aus dem Tod der Schrecken,", "tokens": ["Er\u00b7wa\u00b7chet", "aus", "dem", "Tod", "der", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Harr't zagend er, ob nun erwecken", "tokens": ["Harr't", "za\u00b7gend", "er", ",", "ob", "nun", "er\u00b7we\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJD", "PPER", "$,", "KOUS", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Gott der Wesen Kette wird.", "tokens": ["Ein", "Gott", "der", "We\u00b7sen", "Ket\u00b7te", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Von Sternen herab", "tokens": ["Von", "Ster\u00b7nen", "her\u00b7ab"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Zum Seher hinab", "tokens": ["Zum", "Se\u00b7her", "hin\u00b7ab"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ert\u00f6nt nun eine Stimme:", "tokens": ["Er\u00b7t\u00f6nt", "nun", "ei\u00b7ne", "Stim\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u00bbverk\u00f6rpert hast du hier gesehen", "tokens": ["\u00bb", "ver\u00b7k\u00f6r\u00b7pert", "hast", "du", "hier", "ge\u00b7se\u00b7hen"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVPP", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was allen Dingen wird geschehen", "tokens": ["Was", "al\u00b7len", "Din\u00b7gen", "wird", "ge\u00b7sche\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PIAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Weltgeschichte sahst du hier.", "tokens": ["Die", "Welt\u00b7ge\u00b7schich\u00b7te", "sahst", "du", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Es treibet die Kraft", "tokens": ["Es", "trei\u00b7bet", "die", "Kraft"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Sie wirket und schafft,", "tokens": ["Sie", "wir\u00b7ket", "und", "schafft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "In unaufhaltsamem Regen;", "tokens": ["In", "un\u0