{"dta.poem.16214": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "268.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer nur besch\u00e4ftigt ist, da\u00df er sich selber bilde,", "tokens": ["Wer", "nur", "be\u00b7sch\u00e4f\u00b7tigt", "ist", ",", "da\u00df", "er", "sich", "sel\u00b7ber", "bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "VAFIN", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Besch\u00e4men mag ihn wol die arbeitsame Gilde,", "tokens": ["Be\u00b7sch\u00e4\u00b7men", "mag", "ihn", "wol", "die", "ar\u00b7beit\u00b7sa\u00b7me", "Gil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die nur besch\u00e4ftigt ist zu bilden f\u00fcr die Welt,", "tokens": ["Die", "nur", "be\u00b7sch\u00e4f\u00b7tigt", "ist", "zu", "bil\u00b7den", "f\u00fcr", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "VAFIN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und jeden Tag daf\u00fcr den baaren Lohn erh\u00e4lt.", "tokens": ["Und", "je\u00b7den", "Tag", "da\u00b7f\u00fcr", "den", "baa\u00b7ren", "Lohn", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ja sch\u00e4me dich, die Hand zu legen in den Scho\u00df;", "tokens": ["Ja", "sch\u00e4\u00b7me", "dich", ",", "die", "Hand", "zu", "le\u00b7gen", "in", "den", "Scho\u00df", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "PPER", "$,", "ART", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Lohn, den du dir selbst daf\u00fcr gibst, ist nicht gro\u00df.", "tokens": ["Der", "Lohn", ",", "den", "du", "dir", "selbst", "da\u00b7f\u00fcr", "gibst", ",", "ist", "nicht", "gro\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPER", "ADV", "PAV", "VVFIN", "$,", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und wie du vom Versteck der Abgeschiedenheit", "tokens": ["Und", "wie", "du", "vom", "Ver\u00b7steck", "der", "Ab\u00b7ge\u00b7schie\u00b7den\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hervortritst, schmilzt der Traum der Selbzufriedenheit.", "tokens": ["Her\u00b7vor\u00b7tritst", ",", "schmilzt", "der", "Traum", "der", "Selb\u00b7zu\u00b7frie\u00b7den\u00b7heit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Was hilfts da\u00df du dir sagst, du bildest dich der Welt?", "tokens": ["Was", "hilfts", "da\u00df", "du", "dir", "sagst", ",", "du", "bil\u00b7dest", "dich", "der", "Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KOUS", "PPER", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die doch als Musterbild dich nie vor Augen h\u00e4lt.", "tokens": ["Die", "doch", "als", "Mus\u00b7ter\u00b7bild", "dich", "nie", "vor", "Au\u00b7gen", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "KOUS", "NN", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}