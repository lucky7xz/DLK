{"textgrid.poem.56296": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Der rechte Barbier", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und soll ich nach Philisterart", "tokens": ["Und", "soll", "ich", "nach", "Phi\u00b7lis\u00b7ter\u00b7art"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir Kinn und Wange putzen,", "tokens": ["Mir", "Kinn", "und", "Wan\u00b7ge", "put\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So will ich meinen langen Bart", "tokens": ["So", "will", "ich", "mei\u00b7nen", "lan\u00b7gen", "Bart"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den letzten Tag noch nutzen;", "tokens": ["Den", "letz\u00b7ten", "Tag", "noch", "nut\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ja! \u00e4rgerlich, wie ich nun bin,", "tokens": ["Ja", "!", "\u00e4r\u00b7ger\u00b7lich", ",", "wie", "ich", "nun", "bin", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADJD", "$,", "PWAV", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor meinem Groll, vor meinem Kinn,", "tokens": ["Vor", "mei\u00b7nem", "Groll", ",", "vor", "mei\u00b7nem", "Kinn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Soll mancher noch erzittern.", "tokens": ["Soll", "man\u00b7cher", "noch", "er\u00b7zit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbholla! Herr Wirt, mein Pferd! macht fort!", "tokens": ["\u00bb", "hol\u00b7la", "!", "Herr", "Wirt", ",", "mein", "Pferd", "!", "macht", "fort", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ITJ", "$.", "NN", "NN", "$,", "PPOSAT", "NN", "$.", "VVFIN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ihm wird der Hafer frommen.", "tokens": ["Ihm", "wird", "der", "Ha\u00b7fer", "from\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Habt ihr Barbierer hier im Ort?", "tokens": ["Habt", "ihr", "Bar\u00b7bie\u00b7rer", "hier", "im", "Ort", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00dft gleich den rechten kommen.", "tokens": ["La\u00dft", "gleich", "den", "rech\u00b7ten", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Waldaus, waldein, verfluchtes Land!", "tokens": ["Wald\u00b7aus", ",", "wal\u00b7dein", ",", "ver\u00b7fluch\u00b7tes", "Land", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "$,", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Ich ritt die Kreuz und Quer und fand", "tokens": ["Ich", "ritt", "die", "Kreuz", "und", "Quer", "und", "fand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch nirgends noch den rechten.", "tokens": ["Doch", "nir\u00b7gends", "noch", "den", "rech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Tritt her, Bartputzer, aufgeschaut!", "tokens": ["Tritt", "her", ",", "Bart\u00b7put\u00b7zer", ",", "auf\u00b7ge\u00b7schaut", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "NN", "$,", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Du sollst den Bart mir kratzen;", "tokens": ["Du", "sollst", "den", "Bart", "mir", "krat\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch kitzlich sehr ist meine Haut,", "tokens": ["Doch", "kitz\u00b7lich", "sehr", "ist", "mei\u00b7ne", "Haut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich biete hundert Batzen;", "tokens": ["Ich", "bie\u00b7te", "hun\u00b7dert", "Bat\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nur, machst du nicht die Sache gut,", "tokens": ["Nur", ",", "machst", "du", "nicht", "die", "Sa\u00b7che", "gut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und flie\u00dft ein einz'ges Tr\u00f6pflein Blut, \u2013", "tokens": ["Und", "flie\u00dft", "ein", "einz'\u00b7ges", "Tr\u00f6pf\u00b7lein", "Blut", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00e4hrt dir mein Dolch ins Herze.\u00ab", "tokens": ["F\u00e4hrt", "dir", "mein", "Dolch", "ins", "Her\u00b7ze", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das spitze, kalte Eisen sah", "tokens": ["Das", "spit\u00b7ze", ",", "kal\u00b7te", "Ei\u00b7sen", "sah"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man auf dem Tische blitzen,", "tokens": ["Man", "auf", "dem", "Ti\u00b7sche", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und dem verw\u00fcnschten Ding gar nah", "tokens": ["Und", "dem", "ver\u00b7w\u00fcnschten", "Ding", "gar", "nah"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "ADJD"], "meter": "+--++-+", "measure": "dactylic.init"}, "line.4": {"text": "Auf seinem Schemmel sitzen", "tokens": ["Auf", "sei\u00b7nem", "Schem\u00b7mel", "sit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den grimm'gen schwarzbehaarten Mann", "tokens": ["Den", "grim\u00b7m'\u00b7gen", "schwarz\u00b7be\u00b7haar\u00b7ten", "Mann"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Im schwarzen, kurzen Wams, woran", "tokens": ["Im", "schwar\u00b7zen", ",", "kur\u00b7zen", "Wams", ",", "wo\u00b7ran"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$,", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Noch schw\u00e4rzre Troddeln hingen.", "tokens": ["Noch", "schw\u00e4rz\u00b7re", "Trod\u00b7deln", "hin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dem Meister wird's zu grausig fast,", "tokens": ["Dem", "Meis\u00b7ter", "wird's", "zu", "grau\u00b7sig", "fast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er will die Messer wetzen,", "tokens": ["Er", "will", "die", "Mes\u00b7ser", "wet\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er sieht den Dolch, er sieht den Gast,", "tokens": ["Er", "sieht", "den", "Dolch", ",", "er", "sieht", "den", "Gast", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es packt ihn das Entsetzen;", "tokens": ["Es", "packt", "ihn", "das", "Ent\u00b7set\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er zittert wie das Espenlaub,", "tokens": ["Er", "zit\u00b7tert", "wie", "das", "E\u00b7spen\u00b7laub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er macht sich pl\u00f6tzlich aus dem Staub", "tokens": ["Er", "macht", "sich", "pl\u00f6tz\u00b7lich", "aus", "dem", "Staub"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sendet den Gesellen.", "tokens": ["Und", "sen\u00b7det", "den", "Ge\u00b7sel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "+----+-", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "\u00bbein Hundert Batzen mein Gebot,", "tokens": ["\u00bb", "ein", "Hun\u00b7dert", "Bat\u00b7zen", "mein", "Ge\u00b7bot", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "CARD", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Falls du die Kunst besitzest;", "tokens": ["Falls", "du", "die", "Kunst", "be\u00b7sit\u00b7zest", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch, merk es dir, dich stech ich tot,", "tokens": ["Doch", ",", "merk", "es", "dir", ",", "dich", "stech", "ich", "tot", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So du die Haut mir ritzest.\u00ab", "tokens": ["So", "du", "die", "Haut", "mir", "rit\u00b7zest", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und der Gesell: \u00bbDen Teufel auch!", "tokens": ["Und", "der", "Ge\u00b7sell", ":", "\u00bb", "Den", "Teu\u00b7fel", "auch", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$.", "$(", "ART", "NN", "ADV", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.6": {"text": "Das ist des Landes nicht der Brauch.\u00ab", "tokens": ["Das", "ist", "des", "Lan\u00b7des", "nicht", "der", "Brauch", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PTKNEG", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er l\u00e4uft und schickt den Jungen.", "tokens": ["Er", "l\u00e4uft", "und", "schickt", "den", "Jun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbbist du der rechte, kleiner Molch?", "tokens": ["\u00bb", "bist", "du", "der", "rech\u00b7te", ",", "klei\u00b7ner", "Molch", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Frisch auf! fang an zu schaben;", "tokens": ["Frisch", "auf", "!", "fang", "an", "zu", "scha\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$.", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hier ist das Geld, hier ist der Dolch,", "tokens": ["Hier", "ist", "das", "Geld", ",", "hier", "ist", "der", "Dolch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das beides ist zu haben;", "tokens": ["Das", "bei\u00b7des", "ist", "zu", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und schneidest, ritzest du mich blo\u00df,", "tokens": ["Und", "schnei\u00b7dest", ",", "rit\u00b7zest", "du", "mich", "blo\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So geb ich dir den Gnadensto\u00df;", "tokens": ["So", "geb", "ich", "dir", "den", "Gna\u00b7den\u00b7sto\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du w\u00e4rest nicht der erste.\u00ab", "tokens": ["Du", "w\u00e4\u00b7rest", "nicht", "der", "ers\u00b7te", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ART", "ADJA", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der Junge denkt der Batzen, druckst", "tokens": ["Der", "Jun\u00b7ge", "denkt", "der", "Bat\u00b7zen", ",", "druckst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht lang und ruft verwegen;", "tokens": ["Nicht", "lang", "und", "ruft", "ver\u00b7we\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbnur still gesessen! nicht gemuckst!", "tokens": ["\u00bb", "nur", "still", "ge\u00b7ses\u00b7sen", "!", "nicht", "ge\u00b7muckst", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "VVPP", "$.", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gott geb Euch seinen Segen!\u00ab", "tokens": ["Gott", "geb", "Euch", "sei\u00b7nen", "Se\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er seift ihn ein ganz unverdutzt,", "tokens": ["Er", "seift", "ihn", "ein", "ganz", "un\u00b7ver\u00b7dutzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er wetzt, er stutzt, er kratzt, er putzt:", "tokens": ["Er", "wetzt", ",", "er", "stutzt", ",", "er", "kratzt", ",", "er", "putzt", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbgottlob! nun seid Ihr fertig.\u00ab", "tokens": ["\u00bb", "gott\u00b7lob", "!", "nun", "seid", "Ihr", "fer\u00b7tig", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$.", "ADV", "VAFIN", "PPER", "ADJD", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbnimm, kleiner Knirps, dein Geld nur hin;", "tokens": ["\u00bb", "nimm", ",", "klei\u00b7ner", "Knirps", ",", "dein", "Geld", "nur", "hin", ";"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$,", "ADJA", "NN", "$,", "PPOSAT", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist ein wahrer Teufel!", "tokens": ["Du", "bist", "ein", "wah\u00b7rer", "Teu\u00b7fel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kein andrer mochte den Gewinn,", "tokens": ["Kein", "an\u00b7drer", "moch\u00b7te", "den", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du hegtest keinen Zweifel,", "tokens": ["Du", "heg\u00b7test", "kei\u00b7nen", "Zwei\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es kam das Zittern dich nicht an,", "tokens": ["Es", "kam", "das", "Zit\u00b7tern", "dich", "nicht", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wenn ein Tr\u00f6pflein Blutes rann,", "tokens": ["Und", "wenn", "ein", "Tr\u00f6pf\u00b7lein", "Blu\u00b7tes", "rann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So stach ich doch dich nieder.\u00ab", "tokens": ["So", "stach", "ich", "doch", "dich", "nie\u00b7der", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "\u00bbei! guter Herr, so stand es nicht,", "tokens": ["\u00bb", "ei", "!", "gu\u00b7ter", "Herr", ",", "so", "stand", "es", "nicht", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$.", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich hielt Euch an der Kehle,", "tokens": ["Ich", "hielt", "Euch", "an", "der", "Keh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verzucktet Ihr nur das Gesicht", "tokens": ["Ver\u00b7zuck\u00b7tet", "Ihr", "nur", "das", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ging der Schnitt mir fehle,", "tokens": ["Und", "ging", "der", "Schnitt", "mir", "feh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So lie\u00df ich Euch dazu nicht Zeit,", "tokens": ["So", "lie\u00df", "ich", "Euch", "da\u00b7zu", "nicht", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PAV", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Entschlossen war ich und bereit", "tokens": ["Ent\u00b7schlos\u00b7sen", "war", "ich", "und", "be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Kehl Euch abzuschneiden.\u00ab \u2013", "tokens": ["Die", "Kehl", "Euch", "ab\u00b7zu\u00b7schnei\u00b7den", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "PPER", "VVIZU", "$.", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "\u00bbso so! ein ganz verw\u00fcnschter Spa\u00df!\u00ab", "tokens": ["\u00bb", "so", "so", "!", "ein", "ganz", "ver\u00b7w\u00fcnschter", "Spa\u00df", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADV", "$.", "ART", "ADV", "ADJA", "NN", "$.", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Dem Herrn ward's unbeh\u00e4glich,", "tokens": ["Dem", "Herrn", "ward's", "un\u00b7be\u00b7h\u00e4g\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er wurd auf einmal leichenbla\u00df", "tokens": ["Er", "wurd", "auf", "ein\u00b7mal", "lei\u00b7chen\u00b7bla\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und zitterte nachtr\u00e4glich:", "tokens": ["Und", "zit\u00b7ter\u00b7te", "nach\u00b7tr\u00e4g\u00b7lich", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbso so! das hatt ich nicht bedacht,", "tokens": ["\u00bb", "so", "so", "!", "das", "hatt", "ich", "nicht", "be\u00b7dacht", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "$.", "PDS", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch hat es Gott noch gut gemacht;", "tokens": ["Doch", "hat", "es", "Gott", "noch", "gut", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich will's mir aber merken.\u00ab", "tokens": ["Ich", "will's", "mir", "a\u00b7ber", "mer\u00b7ken", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}