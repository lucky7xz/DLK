word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Aus seinem R\u00fccken ragt ein Felsenst\u00fcck hervor,", "tokens": ["Aus", "sei\u00b7nem", "R\u00fc\u00b7cken", "ragt", "ein", "Fel\u00b7sen\u00b7st\u00fcck", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und droht, zum Sturze reif, der Tiefe Schlund zu f\u00fcllen.", "tokens": ["Und", "droht", ",", "zum", "Stur\u00b7ze", "reif", ",", "der", "Tie\u00b7fe", "Schlund", "zu", "f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPRART", "NN", "ADJD", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Des Thales Raum ist dicht mit Steinen \u00fcbers\u00e4t,", "tokens": ["Des", "Tha\u00b7les", "Raum", "ist", "dicht", "mit", "Stei\u00b7nen", "\u00fc\u00b7bers\u00b7\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Aus denen hie und da ein Distelstrauch entsteht.", "tokens": ["Aus", "de\u00b7nen", "hie", "und", "da", "ein", "Dis\u00b7tel\u00b7strauch", "ent\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "KON", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Es herrschet weit 