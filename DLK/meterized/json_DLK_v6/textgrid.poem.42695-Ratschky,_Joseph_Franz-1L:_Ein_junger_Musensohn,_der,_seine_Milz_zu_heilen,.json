{"textgrid.poem.42695": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein junger Musensohn, der, seine Milz zu heilen,", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein junger Musensohn, der, seine Milz zu heilen,", "tokens": ["Ein", "jun\u00b7ger", "Mu\u00b7sen\u00b7sohn", ",", "der", ",", "sei\u00b7ne", "Milz", "zu", "hei\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun frische Bergluft schnappt, entbeut durch diese Zeilen", "tokens": ["Nun", "fri\u00b7sche", "Berg\u00b7luft", "schnappt", ",", "ent\u00b7beut", "durch", "die\u00b7se", "Zei\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der sch\u00f6nen S\u00e4ngerinn am stolzen Isterfluss", "tokens": ["Der", "sch\u00f6\u00b7nen", "S\u00e4n\u00b7ge\u00b7rinn", "am", "stol\u00b7zen", "Is\u00b7ter\u00b7fluss"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hier vom bescheidnen Inn der Freundschaft trauten Gruss.", "tokens": ["Hier", "vom", "be\u00b7scheid\u00b7nen", "Inn", "der", "Freund\u00b7schaft", "trau\u00b7ten", "Gruss", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Verehrungswerthe Gabriele,", "tokens": ["Ver\u00b7eh\u00b7rungs\u00b7wert\u00b7he", "Ga\u00b7bri\u00b7e\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die du des K\u00f6rpers Reitz durch Bildung deiner Seele", "tokens": ["Die", "du", "des", "K\u00f6r\u00b7pers", "Reitz", "durch", "Bil\u00b7dung", "dei\u00b7ner", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "NE", "APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Versch\u00f6nerst, H\u00e4uslichkeit mit Geist und Witz vereinst,", "tokens": ["Ver\u00b7sch\u00f6\u00b7nerst", ",", "H\u00e4us\u00b7lich\u00b7keit", "mit", "Geist", "und", "Witz", "ver\u00b7einst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wider den Gebrauch der weiblichen Pagoden,", "tokens": ["Und", "wi\u00b7der", "den", "Ge\u00b7brauch", "der", "weib\u00b7li\u00b7chen", "Pa\u00b7go\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "F\u00fcr die das Studium der Moden", "tokens": ["F\u00fcr", "die", "das", "Stu\u00b7di\u00b7um", "der", "Mo\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Als das Talent, das L\u00e4rvchen weiss zu schminken,", "tokens": ["Als", "das", "Ta\u00b7lent", ",", "das", "L\u00e4rv\u00b7chen", "weiss", "zu", "schmin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Von hundert B\u00e4ndern flugs das sch\u00f6nste zu ersp\u00e4hn,", "tokens": ["Von", "hun\u00b7dert", "B\u00e4n\u00b7dern", "flugs", "das", "sch\u00f6ns\u00b7te", "zu", "er\u00b7sp\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADV", "ART", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mit Artigkeit Kafee zu trinken,", "tokens": ["Mit", "Ar\u00b7tig\u00b7keit", "Ka\u00b7fee", "zu", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Auf weichen Sophen sich recht adelich zu bl\u00e4hn,", "tokens": ["Auf", "wei\u00b7chen", "So\u00b7phen", "sich", "recht", "a\u00b7de\u00b7lich", "zu", "bl\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Des Gatten Zuversicht mit schlauer Kunst zu t\u00e4uschen,", "tokens": ["Des", "Gat\u00b7ten", "Zu\u00b7ver\u00b7sicht", "mit", "schlau\u00b7er", "Kunst", "zu", "t\u00e4u\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und wackrer Leute Ruf mit Anstand zu zerfleischen,", "tokens": ["Und", "wack\u00b7rer", "Leu\u00b7te", "Ruf", "mit", "An\u00b7stand", "zu", "zer\u00b7flei\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wenn meine rohe Zeichnung dir", "tokens": ["Wenn", "mei\u00b7ne", "ro\u00b7he", "Zeich\u00b7nung", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Nicht missf\u00e4llt, so verg\u00f6nne mir,", "tokens": ["Nicht", "miss\u00b7f\u00e4llt", ",", "so", "ver\u00b7g\u00f6n\u00b7ne", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "O s\u00fcsse Schwester der Kam\u00f6nen,", "tokens": ["O", "s\u00fcs\u00b7se", "Schwes\u00b7ter", "der", "Ka\u00b7m\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Die mannigfaltigen Schattirungen und Scenen", "tokens": ["Die", "man\u00b7nig\u00b7fal\u00b7ti\u00b7gen", "Schat\u00b7ti\u00b7run\u00b7gen", "und", "Sce\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.17": {"text": "Der unverk\u00fcnstelten Natur,", "tokens": ["Der", "un\u00b7ver\u00b7k\u00fcns\u00b7tel\u00b7ten", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Die hier zu Land mir zu Gesichte kamen,", "tokens": ["Die", "hier", "zu", "Land", "mir", "zu", "Ge\u00b7sich\u00b7te", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Mit unge\u00fcbter Hand, im Schattenrisse nur,", "tokens": ["Mit", "un\u00b7ge\u00b7\u00fcb\u00b7ter", "Hand", ",", "im", "Schat\u00b7ten\u00b7ris\u00b7se", "nur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Auf diesem Blatte nachzuahmen.", "tokens": ["Auf", "die\u00b7sem", "Blat\u00b7te", "nach\u00b7zu\u00b7ah\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So wie man sich, von W\u00e4lschlands Reitzen satt,", "tokens": ["So", "wie", "man", "sich", ",", "von", "W\u00e4l\u00b7schlands", "Reit\u00b7zen", "satt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PIS", "PRF", "$,", "APPR", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von der ber\u00fchmten Vaterstadt", "tokens": ["Von", "der", "be\u00b7r\u00fchm\u00b7ten", "Va\u00b7ter\u00b7stadt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des lockeren Katulls (der hier bey manchem s\u00fcssen", "tokens": ["Des", "lo\u00b7cke\u00b7ren", "Ka\u00b7tulls", "(", "der", "hier", "bey", "man\u00b7chem", "s\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADV", "APPR", "PIAT", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gedichtchen, dem es noch an Politur gebrach,", "tokens": ["Ge\u00b7dicht\u00b7chen", ",", "dem", "es", "noch", "an", "Po\u00b7li\u00b7tur", "ge\u00b7brach", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nach \u00e4ltrer Dichter Art die N\u00e4gel sich zerbissen)", "tokens": ["Nach", "\u00e4lt\u00b7rer", "Dich\u00b7ter", "Art", "die", "N\u00e4\u00b7gel", "sich", "zer\u00b7bis\u00b7sen", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "ART", "NN", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nach Norden kehrt, th\u00fcrmt allgemach", "tokens": ["Nach", "Nor\u00b7den", "kehrt", ",", "th\u00fcrmt", "all\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$,", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Erde sich empor, und wolkenhoch erheben,", "tokens": ["Die", "Er\u00b7de", "sich", "em\u00b7por", ",", "und", "wol\u00b7ken\u00b7hoch", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "PTKVZ", "$,", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Von grauen Nebeln rings umgeben,", "tokens": ["Von", "grau\u00b7en", "Ne\u00b7beln", "rings", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der Berge H\u00e4upter sich, die W\u00e4lschlands milder Luft", "tokens": ["Der", "Ber\u00b7ge", "H\u00e4up\u00b7ter", "sich", ",", "die", "W\u00e4l\u00b7schlands", "mil\u00b7der", "Luft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PRF", "$,", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Den Eintritt in Tyrols verwachsne Th\u00e4ler wehren.", "tokens": ["Den", "Ein\u00b7tritt", "in", "Ty\u00b7rols", "ver\u00b7wachs\u00b7ne", "Th\u00e4\u00b7ler", "weh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es reiht sich Berg an Berg: des Thales enge Kluft,", "tokens": ["Es", "reiht", "sich", "Berg", "an", "Berg", ":", "des", "Tha\u00b7les", "en\u00b7ge", "Kluft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "NN", "APPR", "NN", "$.", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Voll finstrer Tannen, gleicht der Heimath wilder B\u00e4ren.", "tokens": ["Voll", "finst\u00b7rer", "Tan\u00b7nen", ",", "gleicht", "der", "Hei\u00b7math", "wil\u00b7der", "B\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Hier richtet senkrecht sich bis in der Wolken Saum", "