d", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Einst Herzog Ferdinand so manches St\u00fcndchen s\u00e4umte,", "tokens": ["Einst", "Her\u00b7zog", "Fer\u00b7di\u00b7nand", "so", "man\u00b7ches", "St\u00fcnd\u00b7chen", "s\u00e4um\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "NE", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als er, trotz seines Vaters Drohn", "tokens": ["Als", "er", ",", "trotz", "sei\u00b7nes", "Va\u00b7ters", "Drohn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und Philippinens Stand, bey Tag' ihr auf dem Thron", "tokens": ["Und", "Phil\u00b7ip\u00b7pi\u00b7nens", "Stand", ",", "bey", "Tag'", "ihr", "auf", "dem", "Thron"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "NN", "$,", "APPR", "NN", "PPOSAT", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und nachts in seinem Bett ein kleines Pl\u00e4tzchen r\u00e4umte.", "tokens": ["Und", "nachts", "in", "sei\u00b7nem", "Bett", "ein", "klei\u00b7nes", "Pl\u00e4tz\u00b7chen", "r\u00e4um\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "O Freundinn, wenn ein Mann von deutscher Biederart", "tokens": ["O", "Freun\u00b7dinn", ",", "wenn", "ein", "Mann", "von", "deut\u00b7scher", "Bie\u00b7der\