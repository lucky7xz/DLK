{"dta.poem.16704": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "63,  Der taube h\u00f6rt das Wort.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Freund glaub es oder nicht; ich h\u00f6r\u2019 in jedem nu/", "tokens": ["Freund", "glaub", "es", "o\u00b7der", "nicht", ";", "ich", "h\u00f6r'", "in", "je\u00b7dem", "nu", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KON", "PTKNEG", "$.", "PPER", "VVFIN", "APPR", "PIS", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann ich bin taub und Stumm dem ", "tokens": ["Wann", "ich", "bin", "taub", "und", "Stumm", "dem"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VAFIN", "ADJD", "KON", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "zu.", "tokens": ["zu", "."], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "+", "measure": "single.up"}}}}}