{"dta.poem.23498": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Von rc. beeden  \n Fr\u00e4wlin rc. Agnes vnd  \n Anna Hertzoginen zu Wir-  \n temberg rc.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1619", "urn": "urn:nbn:de:kobv:b4-20627-4", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Solt Phaebe selbs mit vollem glantz/", "tokens": ["Solt", "Phae\u00b7be", "selbs", "mit", "vol\u00b7lem", "glantz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wan Sie der sternen klaren dantz", "tokens": ["Wan", "Sie", "der", "ster\u00b7nen", "kla\u00b7ren", "dantz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Den G\u00f6ttern zu ehren auff\u00fchret/", "tokens": ["Den", "G\u00f6t\u00b7tern", "zu", "eh\u00b7ren", "auf\u00b7f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Vnd einig an sch\u00f6nheit prachtieret/", "tokens": ["Vnd", "ei\u00b7nig", "an", "sch\u00f6n\u00b7heit", "prach\u00b7tie\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "VVFIN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Sich in Jhrem geborgten klaid", "tokens": ["Sich", "in", "Ih\u00b7rem", "ge\u00b7borg\u00b7ten", "klaid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Vor Euch/ Agnes/ ruhmhaft erzaigen;", "tokens": ["Vor", "Euch", "/", "Ag\u00b7nes", "/", "ruhm\u00b7haft", "er\u00b7zai\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$(", "NE", "$(", "ADJD", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "W\u00fcrd sie (zwar nicht ohn rew vnd laid)", "tokens": ["W\u00fcrd", "sie", "(", "zwar", "nicht", "ohn", "rew", "vnd", "laid", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$(", "ADV", "PTKNEG", "APPR", "NE", "KON", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vor Euch bald jhren hochmuht naigen.", "tokens": ["Vor", "Euch", "bald", "jhren", "hoch\u00b7muht", "nai\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}