{"textgrid.poem.33523": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "An Frida", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwischen uns sind W\u00e4nde Trennung.", "tokens": ["Zwi\u00b7schen", "uns", "sind", "W\u00e4n\u00b7de", "Tren\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spinnetze Sonderbares.", "tokens": ["Spin\u00b7net\u00b7ze", "Son\u00b7der\u00b7ba\u00b7res", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Doch oft fliege ich schmal in meiner sinkenden", "tokens": ["Doch", "oft", "flie\u00b7ge", "ich", "schmal", "in", "mei\u00b7ner", "sin\u00b7ken\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "H\u00e4nderingenden Stube, ein blutender Piepmatz.", "tokens": ["H\u00e4n\u00b7de\u00b7rin\u00b7gen\u00b7den", "Stu\u00b7be", ",", "ein", "blu\u00b7ten\u00b7der", "Pie\u00b7pmatz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "W\u00e4rst du da.", "tokens": ["W\u00e4rst", "du", "da", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Ich bin so ermordet.", "tokens": ["Ich", "bin", "so", "er\u00b7mor\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Frida.", "tokens": ["Fri\u00b7da", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}