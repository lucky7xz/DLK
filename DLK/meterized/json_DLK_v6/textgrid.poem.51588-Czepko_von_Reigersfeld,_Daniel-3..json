{"textgrid.poem.51588": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwinge die Fl\u00fcgel der Seelen empor!", "tokens": ["Schwin\u00b7ge", "die", "Fl\u00fc\u00b7gel", "der", "See\u00b7len", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Oben die sch\u00f6nste Gestalt zu beginnen,", "tokens": ["O\u00b7ben", "die", "sch\u00f6ns\u00b7te", "Ge\u00b7stalt", "zu", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Schaue, der Glauben, Vernunfft und die Sinnen", "tokens": ["Schau\u00b7e", ",", "der", "Glau\u00b7ben", ",", "Ver\u00b7nunfft", "und", "die", "Sin\u00b7nen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ART", "NN", "$,", "NN", "KON", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Treten wie selber der Engel ihr Chor.", "tokens": ["Tre\u00b7ten", "wie", "sel\u00b7ber", "der", "En\u00b7gel", "ihr", "Chor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Glauben: weil er bey Gott eintzig kommt vor,", "tokens": ["Glau\u00b7ben", ":", "weil", "er", "bey", "Gott", "eint\u00b7zig", "kommt", "vor", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KOUS", "PPER", "APPR", "NN", "ADJD", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die Vernunfft: weil sie gereinigt von innen:", "tokens": ["Die", "Ver\u00b7nunfft", ":", "weil", "sie", "ge\u00b7rei\u00b7nigt", "von", "in\u00b7nen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "KOUS", "PPER", "VVPP", "APPR", "ADV", "$."], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sinnen: dieweil sie sich k\u00f6nnen besinnen,", "tokens": ["Sin\u00b7nen", ":", "die\u00b7weil", "sie", "sich", "k\u00f6n\u00b7nen", "be\u00b7sin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KOUS", "PPER", "PRF", "VMFIN", "VVINF", "$,"], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "\u00d6ffne die Augen, sie \u00f6ffnen das Thor.", "tokens": ["\u00d6ff\u00b7ne", "die", "Au\u00b7gen", ",", "sie", "\u00f6ff\u00b7nen", "das", "Thor", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Schaue, wie sch\u00f6n dich der Glantz wil bewonnen,", "tokens": ["Schau\u00b7e", ",", "wie", "sch\u00f6n", "dich", "der", "Glantz", "wil", "be\u00b7won\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ADJD", "PPER", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Drinnen die Obriste Selbst\u00e4ndigkeit", "tokens": ["Drin\u00b7nen", "die", "O\u00b7bris\u00b7te", "Selb\u00b7st\u00e4n\u00b7dig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "\u00dcberall kommt entgegen geronnen,", "tokens": ["\u00dc\u00b7be\u00b7rall", "kommt", "ent\u00b7ge\u00b7gen", "ge\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "VVPP", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Sie beschleust dich schon, und du bist noch weit!", "tokens": ["Sie", "be\u00b7schleust", "dich", "schon", ",", "und", "du", "bist", "noch", "weit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KON", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Aber halt an, du verbrennst in der Sonnen,", "tokens": ["A\u00b7ber", "halt", "an", ",", "du", "ver\u00b7brennst", "in", "der", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Du bist des Fluges: Sie in dir:, befreyt.", "tokens": ["Du", "bist", "des", "Flu\u00b7ges", ":", "Sie", "in", "dir", ":", ",", "be\u00b7freyt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "PPER", "APPR", "PPER", "$.", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}