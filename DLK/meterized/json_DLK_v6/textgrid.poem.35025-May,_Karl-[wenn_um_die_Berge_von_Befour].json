{"textgrid.poem.35025": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "[wenn um die Berge von Befour]", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwenn um die Berge von Befour", "tokens": ["\u00bb", "wenn", "um", "die", "Ber\u00b7ge", "von", "Be\u00b7four"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Abends erste Schatten wallen,", "tokens": ["Des", "A\u00b7bends", "ers\u00b7te", "Schat\u00b7ten", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann tritt die Mutter der Natur", "tokens": ["Dann", "tritt", "die", "Mut\u00b7ter", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hervor aus unterirdschen Hallen", "tokens": ["Her\u00b7vor", "aus", "un\u00b7ter\u00b7ird\u00b7schen", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und ihres Diadem Azur", "tokens": ["Und", "ih\u00b7res", "Di\u00b7a\u00b7dem", "A\u00b7zur"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ergl\u00e4nzt von funkelnden Krystallen.", "tokens": ["Er\u00b7gl\u00e4nzt", "von", "fun\u00b7keln\u00b7den", "Krys\u00b7tal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "In ihren dunklen Locken bl\u00fchn", "tokens": ["In", "ih\u00b7ren", "dunk\u00b7len", "Lo\u00b7cken", "bl\u00fchn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Erde d\u00fcftereiche Lieder;", "tokens": ["Der", "Er\u00b7de", "d\u00fcf\u00b7te\u00b7rei\u00b7che", "Lie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Aus ungeme\u00dfnen Fernen gl\u00fchn", "tokens": ["Aus", "un\u00b7ge\u00b7me\u00df\u00b7nen", "Fer\u00b7nen", "gl\u00fchn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Des Kreuzes Funken auf sie nieder,", "tokens": ["Des", "Kreu\u00b7zes", "Fun\u00b7ken", "auf", "sie", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und traumbewegte Wogen spr\u00fchn", "tokens": ["Und", "traum\u00b7be\u00b7weg\u00b7te", "Wo\u00b7gen", "spr\u00fchn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der Sterne goldne Opfer wieder.", "tokens": ["Der", "Ster\u00b7ne", "gold\u00b7ne", "Op\u00b7fer", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und bricht der junge Tag heran", "tokens": ["Und", "bricht", "der", "jun\u00b7ge", "Tag", "he\u00b7ran"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Die Tausend\u00e4ugige zu finden,", "tokens": ["Die", "Tau\u00b7sen\u00b7d\u00e4u\u00b7gi\u00b7ge", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "L\u00e4\u00dft sie das leuchtende Gespann", "tokens": ["L\u00e4\u00dft", "sie", "das", "leuch\u00b7ten\u00b7de", "Ge\u00b7spann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Sich durch purpurne Thore winden,", "tokens": ["Sich", "durch", "pur\u00b7pur\u00b7ne", "Tho\u00b7re", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+++-+-+-", "measure": "unknown.measure.penta"}, "line.17": {"text": "Sein Angesicht zu schaun und dann", "tokens": ["Sein", "An\u00b7ge\u00b7sicht", "zu", "schaun", "und", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Im fernen Westen zu verschwinden.\u00ab", "tokens": ["Im", "fer\u00b7nen", "Wes\u00b7ten", "zu", "ver\u00b7schwin\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbwenn um die Berge von Befour", "tokens": ["\u00bb", "wenn", "um", "die", "Ber\u00b7ge", "von", "Be\u00b7four"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Abends dunkle Schatten wallen,", "tokens": ["Des", "A\u00b7bends", "dunk\u00b7le", "Schat\u00b7ten", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann tritt die Mutter der Natur", "tokens": ["Dann", "tritt", "die", "Mut\u00b7ter", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hervor aus unterid'schen Hallen", "tokens": ["Her\u00b7vor", "aus", "un\u00b7te\u00b7ri\u00b7d'\u00b7schen", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und l\u00e4\u00dft auf die versengte Flur", "tokens": ["Und", "l\u00e4\u00dft", "auf", "die", "ver\u00b7seng\u00b7te", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Thaues stille Perlen fallen.", "tokens": ["Des", "Thau\u00b7es", "stil\u00b7le", "Per\u00b7len", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Des Himmels Seraph flieht, verh\u00fcllt", "tokens": ["Des", "Him\u00b7mels", "Se\u00b7raph", "flieht", ",", "ver\u00b7h\u00fcllt"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "$,", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von Wolken, die sich rastlos jagen,", "tokens": ["Von", "Wol\u00b7ken", ",", "die", "sich", "rast\u00b7los", "ja\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die Erde l\u00e4\u00dft, von Schmerz erf\u00fcllt,", "tokens": ["Die", "Er\u00b7de", "l\u00e4\u00dft", ",", "von", "Schmerz", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Den Blumen bittre Thr\u00e4nen tragen,", "tokens": ["Den", "Blu\u00b7men", "bitt\u00b7re", "Thr\u00e4\u00b7nen", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und um verborgne Klippen br\u00fcllt", "tokens": ["Und", "um", "ver\u00b7borg\u00b7ne", "Klip\u00b7pen", "br\u00fcllt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die Brandung ihre wilden Klagen.", "tokens": ["Die", "Bran\u00b7dung", "ih\u00b7re", "wil\u00b7den", "Kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Da bricht des Morgens gl\u00fchend Herz:", "tokens": ["Da", "bricht", "des", "Mor\u00b7gens", "gl\u00fc\u00b7hend", "Herz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Er l\u00e4\u00dft den jungen Tag erscheinen;", "tokens": ["Er", "l\u00e4\u00dft", "den", "jun\u00b7gen", "Tag", "er\u00b7schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Der k\u00fc\u00dft den diamantnen Schmerz", "tokens": ["Der", "k\u00fc\u00dft", "den", "di\u00b7a\u00b7mant\u00b7nen", "Schmerz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Von tropfenden Karfunkelsteinen", "tokens": ["Von", "trop\u00b7fen\u00b7den", "Kar\u00b7fun\u00b7kel\u00b7stei\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Und tr\u00e4gt ihn liebend himmelw\u00e4rts,", "tokens": ["Und", "tr\u00e4gt", "ihn", "lie\u00b7bend", "him\u00b7mel\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Im Aether dort sich auszuweinen.\u00ab", "tokens": ["Im", "A\u00b7e\u00b7ther", "dort", "sich", "aus\u00b7zu\u00b7wei\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ADV", "PRF", "VVIZU", "$.", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}