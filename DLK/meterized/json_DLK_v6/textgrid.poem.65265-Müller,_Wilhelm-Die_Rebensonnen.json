{"textgrid.poem.65265": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Rebensonnen", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drei Sonnen sah ich am Himmel stehn,", "tokens": ["Drei", "Son\u00b7nen", "sah", "ich", "am", "Him\u00b7mel", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hab' lang' und fest sie angesehn;", "tokens": ["Hab'", "lang'", "und", "fest", "sie", "an\u00b7ge\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sie auch standen da so stier,", "tokens": ["Und", "sie", "auch", "stan\u00b7den", "da", "so", "stier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als k\u00f6nnten sie nicht weg von mir.", "tokens": ["Als", "k\u00f6nn\u00b7ten", "sie", "nicht", "weg", "von", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "PTKNEG", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach, ", "tokens": ["Ach", ","], "token_info": ["word", "punct"], "pos": ["ITJ", "$,"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Schaut Andren doch in's Angesicht!", "tokens": ["Schaut", "An\u00b7dren", "doch", "in's", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ja, neulich hatt' ich auch wohl drei:", "tokens": ["Ja", ",", "neu\u00b7lich", "hatt'", "ich", "auch", "wohl", "drei", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VAFIN", "PPER", "ADV", "ADV", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nun sind hinab die besten zwei.", "tokens": ["Nun", "sind", "hin\u00b7ab", "die", "bes\u00b7ten", "zwei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "ADJA", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ging' nur die dritt' erst hinterdrein!", "tokens": ["Ging'", "nur", "die", "dritt'", "erst", "hin\u00b7ter\u00b7drein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Im Dunkel wird mir wohler sein.", "tokens": ["Im", "Dun\u00b7kel", "wird", "mir", "woh\u00b7ler", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}