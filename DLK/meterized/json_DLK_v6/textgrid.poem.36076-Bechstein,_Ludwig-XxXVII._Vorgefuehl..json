{"textgrid.poem.36076": {"metadata": {"author": {"name": "Bechstein, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "XxXVII. Vorgefuehl.", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Herz ist eine Glocke, die schl\u00e4gt manch ernste Stunde;", "tokens": ["Das", "Herz", "ist", "ei\u00b7ne", "Glo\u00b7cke", ",", "die", "schl\u00e4gt", "manch", "erns\u00b7te", "Stun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sie giebt von Freudenfesten, sie giebt von Trauer Kunde.", "tokens": ["Sie", "giebt", "von", "Freu\u00b7den\u00b7fes\u00b7ten", ",", "sie", "giebt", "von", "Trau\u00b7er", "Kun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wenn tief im Busen Aufruhr, wenn die Gef\u00fchle ringen,", "tokens": ["Wenn", "tief", "im", "Bu\u00b7sen", "Auf\u00b7ruhr", ",", "wenn", "die", "Ge\u00b7f\u00fch\u00b7le", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPRART", "NN", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da schl\u00e4gt sie laut und angstvoll, und m\u00f6chte fast zerspringen.", "tokens": ["Da", "schl\u00e4gt", "sie", "laut", "und", "angst\u00b7voll", ",", "und", "m\u00f6ch\u00b7te", "fast", "zer\u00b7sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,", "KON", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Das Herz ist eine Harfe, die Gott besaitet hat,", "tokens": ["Das", "Herz", "ist", "ei\u00b7ne", "Har\u00b7fe", ",", "die", "Gott", "be\u00b7sai\u00b7tet", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der S\u00fcnder nur verstimmt sie durch frevelhafte That.", "tokens": ["Der", "S\u00fcn\u00b7der", "nur", "ver\u00b7stimmt", "sie", "durch", "fre\u00b7vel\u00b7haf\u00b7te", "That", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und will sie dann erklingen, dann wird's ein tr\u00fcber Klang,", "tokens": ["Und", "will", "sie", "dann", "er\u00b7klin\u00b7gen", ",", "dann", "wird's", "ein", "tr\u00fc\u00b7ber", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF", "$,", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Leichhuhnruf \u00fcber Gr\u00e4bern, und heisrer Unkensang.", "tokens": ["Leich\u00b7huhn\u00b7ruf", "\u00fc\u00b7ber", "Gr\u00e4\u00b7bern", ",", "und", "heis\u00b7rer", "Un\u00b7ken\u00b7sang", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Und tief im Herzen wohnet eine Rune wunderbar,", "tokens": ["Und", "tief", "im", "Her\u00b7zen", "woh\u00b7net", "ei\u00b7ne", "Ru\u00b7ne", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Die sagt prophetischdunkel die k\u00fcnftgen Dinge wahr.", "tokens": ["Die", "sagt", "pro\u00b7phe\u00b7tischdun\u00b7kel", "die", "k\u00fcnft\u00b7gen", "Din\u00b7ge", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Sie warnt mit leiser Mahnung, es warnt kein Freund getreuer,", "tokens": ["Sie", "warnt", "mit", "lei\u00b7ser", "Mah\u00b7nung", ",", "es", "warnt", "kein", "Freund", "ge\u00b7treu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,", "PPER", "VVFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und das Gespenst der Ahnung h\u00fcllt sich in ihre Schleier.", "tokens": ["Und", "das", "Ge\u00b7spenst", "der", "Ah\u00b7nung", "h\u00fcllt", "sich", "in", "ih\u00b7re", "Schlei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Um Faustus ist es tr\u00fcbe, recht tr\u00fcbe, still und leer;", "tokens": ["Um", "Faus\u00b7tus", "ist", "es", "tr\u00fc\u00b7be", ",", "recht", "tr\u00fc\u00b7be", ",", "still", "und", "leer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "NE", "VAFIN", "PPER", "VVFIN", "$,", "ADJD", "ADJA", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ihn heitert nicht die Liebe mit sanftem Kosen mehr.", "tokens": ["Ihn", "hei\u00b7tert", "nicht", "die", "Lie\u00b7be", "mit", "sanf\u00b7tem", "Ko\u00b7sen", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "APPR", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Kaum dass es ihn, den Ernsten, zum leisen L\u00e4cheln zwingt,", "tokens": ["Kaum", "dass", "es", "ihn", ",", "den", "Erns\u00b7ten", ",", "zum", "lei\u00b7sen", "L\u00e4\u00b7cheln", "zwingt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPER", "$,", "ART", "NN", "$,", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wenn seine Arme Justus um Vaters Nacken schlingt.", "tokens": ["Wenn", "sei\u00b7ne", "Ar\u00b7me", "Jus\u00b7tus", "um", "Va\u00b7ters", "Na\u00b7cken", "schlingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NE", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+----+-+-+", "measure": "unknown.measure.penta"}}, "stanza.5": {"line.1": {"text": "\u00bbo Leben, rollend Leben, Du Fl\u00fcgelball des Gl\u00fccks,", "tokens": ["\u00bb", "o", "Le\u00b7ben", ",", "rol\u00b7lend", "Le\u00b7ben", ",", "Du", "Fl\u00fc\u00b7gel\u00b7ball", "des", "Gl\u00fccks", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "ADJD", "NN", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Du Spiel der G\u00f6tterlaune, Du Kampfpreis des Geschicks!", "tokens": ["Du", "Spiel", "der", "G\u00f6t\u00b7ter\u00b7lau\u00b7ne", ",", "Du", "Kampf\u00b7preis", "des", "Ge\u00b7schicks", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,", "PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Was fliehst Du mir so fl\u00fcchtig vor\u00fcber, Huldgestalt!", "tokens": ["Was", "fliehst", "Du", "mir", "so", "fl\u00fcch\u00b7tig", "vor\u00b7\u00fc\u00b7ber", ",", "Huld\u00b7ge\u00b7stalt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "PTKVZ", "$,", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Du, f\u00fcr so Viele liebreich, warum f\u00fcr mich so kalt?\u00ab", "tokens": ["Du", ",", "f\u00fcr", "so", "Vie\u00b7le", "lieb\u00b7reich", ",", "wa\u00b7rum", "f\u00fcr", "mich", "so", "kalt", "?", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "$,", "APPR", "ADV", "PIS", "ADJD", "$,", "PWAV", "APPR", "PPER", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbo Leben, s\u00fcsse Buhle, was hab' ich Dir gethan?", "tokens": ["\u00bb", "o", "Le\u00b7ben", ",", "s\u00fcs\u00b7se", "Buh\u00b7le", ",", "was", "hab'", "ich", "Dir", "ge\u00b7than", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "ADJA", "NN", "$,", "PWS", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Was blickst Du mich so finster, so kalt, so lieblos an?", "tokens": ["Was", "blickst", "Du", "mich", "so", "fins\u00b7ter", ",", "so", "kalt", ",", "so", "lieb\u00b7los", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "O lass von mir Dich halten! Komm, k\u00fcsse Faustus warm!", "tokens": ["O", "lass", "von", "mir", "Dich", "hal\u00b7ten", "!", "Komm", ",", "k\u00fcs\u00b7se", "Faus\u00b7tus", "warm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "PRF", "VVINF", "$.", "VVFIN", "$,", "ADJA", "NN", "ADJD", "$."], "meter": "-+--++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Nimm gegen Nachtgewalten mich rettend in den Arm!\u00ab", "tokens": ["Nimm", "ge\u00b7gen", "Nacht\u00b7ge\u00b7wal\u00b7ten", "mich", "ret\u00b7tend", "in", "den", "Arm", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "APPR", "NN", "PPER", "ADJD", "APPR", "ART", "NN", "$.", "$("], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.7": {"line.1": {"text": "\u00bbbald ist der Zauberbecher geleert bis auf den Grund;", "tokens": ["\u00bb", "bald", "ist", "der", "Zau\u00b7ber\u00b7be\u00b7cher", "ge\u00b7leert", "bis", "auf", "den", "Grund", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "ART", "NN", "VVPP", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und der beth\u00f6rte Zecher f\u00fchrt ihn nicht mehr zum Mund.", "tokens": ["Und", "der", "be\u00b7th\u00f6r\u00b7te", "Ze\u00b7cher", "f\u00fchrt", "ihn", "nicht", "mehr", "zum", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Er schleudert ihn zum Abgrund, und st\u00fcrzt sich hinterdrein,", "tokens": ["Er", "schleu\u00b7dert", "ihn", "zum", "Ab\u00b7grund", ",", "und", "st\u00fcrzt", "sich", "hin\u00b7ter\u00b7drein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,", "KON", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und keine liebe Stimme ruft schmerzbewegt: Halt ein!\u00ab \u2013", "tokens": ["Und", "kei\u00b7ne", "lie\u00b7be", "Stim\u00b7me", "ruft", "schmerz\u00b7be\u00b7wegt", ":", "Halt", "ein", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN", "VVFIN", "$.", "VVIMP", "PTKVZ", "$.", "$(", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Er ruft's und schl\u00e4gt die H\u00e4nde wild vor sein Angesicht;", "tokens": ["Er", "ruft's", "und", "schl\u00e4gt", "die", "H\u00e4n\u00b7de", "wild", "vor", "sein", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Er m\u00f6chte gerne weinen, ach, und vermag es nicht.", "tokens": ["Er", "m\u00f6ch\u00b7te", "ger\u00b7ne", "wei\u00b7nen", ",", "ach", ",", "und", "ver\u00b7mag", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$,", "ITJ", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Er hat nicht ", "tokens": ["Er", "hat", "nicht"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Die Pforten sind verschlossen des milden Thr\u00e4nenbornes.", "tokens": ["Die", "Pfor\u00b7ten", "sind", "ver\u00b7schlos\u00b7sen", "des", "mil\u00b7den", "Thr\u00e4\u00b7nen\u00b7bor\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Da naht die Reizumflossne, Lust lebt, wohin sie tritt,", "tokens": ["Da", "naht", "die", "Rei\u00b7zum\u00b7floss\u00b7ne", ",", "Lust", "lebt", ",", "wo\u00b7hin", "sie", "tritt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "NN", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-++-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Die Sch\u00f6nheit\u00fcbergossne, sie bringt den Knaben mit.", "tokens": ["Die", "Sch\u00f6n\u00b7hei\u00b7t\u00fc\u00b7ber\u00b7goss\u00b7ne", ",", "sie", "bringt", "den", "Kna\u00b7ben", "mit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Gleich Himmelslenzen bl\u00fchet ihr Rosenwangenpaar,", "tokens": ["Gleich", "Him\u00b7mels\u00b7len\u00b7zen", "bl\u00fc\u00b7het", "ihr", "Ro\u00b7sen\u00b7wan\u00b7gen\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und sanfte Flammen spr\u00fchet ihr Auge, wunderklar.", "tokens": ["Und", "sanf\u00b7te", "Flam\u00b7men", "spr\u00fc\u00b7het", "ihr", "Au\u00b7ge", ",", "wun\u00b7der\u00b7klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "\u00bbmein Faustus! trauter Gatte bleibt kein Gedank' an mich?", "tokens": ["\u00bb", "mein", "Faus\u00b7tus", "!", "trau\u00b7ter", "Gat\u00b7te", "bleibt", "kein", "Ge\u00b7dank'", "an", "mich", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$.", "ADJA", "NN", "VVFIN", "PIAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und qu\u00e4lest nimmerfr\u00f6hlich doch mit Gedanken Dich?!", "tokens": ["Und", "qu\u00e4\u00b7lest", "nim\u00b7mer\u00b7fr\u00f6h\u00b7lich", "doch", "mit", "Ge\u00b7dan\u00b7ken", "Dich", "?!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPR", "NN", "PPER", "$."], "meter": "-+-+-+-++-+-+", "measure": "unknown.measure.septa"}, "line.3": {"text": "Mein Faustus, lass sie schwinden, die Kinder schwarzer Nacht,", "tokens": ["Mein", "Faus\u00b7tus", ",", "lass", "sie", "schwin\u00b7den", ",", "die", "Kin\u00b7der", "schwar\u00b7zer", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "VVFIN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sieh mich, sieh Deinen Knaben, wie der so freundlich lacht!\u00ab", "tokens": ["Sieh", "mich", ",", "sieh", "Dei\u00b7nen", "Kna\u00b7ben", ",", "wie", "der", "so", "freund\u00b7lich", "lacht", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "$,", "VVIMP", "PPOSAT", "NN", "$,", "PWAV", "ART", "ADV", "ADJD", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "\u203ao Helena! Von Hellas? Nein, von der H\u00f6ll' entf\u00fchrt!", "tokens": ["\u203a", "o", "He\u00b7le\u00b7na", "!", "Von", "Hel\u00b7las", "?", "Nein", ",", "von", "der", "H\u00f6ll'", "ent\u00b7f\u00fchrt", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "NE", "$.", "APPR", "NE", "$.", "PTKANT", "$,", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Weib, das sich meine Thorheit, das sich mein Wahn erk\u00fchrt!", "tokens": ["Weib", ",", "das", "sich", "mei\u00b7ne", "Thor\u00b7heit", ",", "das", "sich", "mein", "Wahn", "er\u00b7k\u00fchrt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "PPOSAT", "NN", "$,", "PRELS", "PRF", "PPOSAT", "NN", "VVPP", "$."], "meter": "---+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Du nicht vermagst zu tr\u00f6sten, Halbding von Mensch und Geist,", "tokens": ["Du", "nicht", "ver\u00b7magst", "zu", "tr\u00f6s\u00b7ten", ",", "Halb\u00b7ding", "von", "Mensch", "und", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$,", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Du nicht vermagst zu f\u00fchlen, was mir das Herz zerreisst!\u2039", "tokens": ["Du", "nicht", "ver\u00b7magst", "zu", "f\u00fch\u00b7len", ",", "was", "mir", "das", "Herz", "zer\u00b7reisst", "!", "\u2039"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$,", "PWS", "PPER", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.12": {"line.1": {"text": "\u00bbwie Du mich kr\u00e4nkest, Faustus! Bin ja Dein treues Weib!", "tokens": ["\u00bb", "wie", "Du", "mich", "kr\u00e4n\u00b7kest", ",", "Faus\u00b7tus", "!", "Bin", "ja", "Dein", "treu\u00b7es", "Weib", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "PRF", "VVFIN", "$,", "NE", "$.", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Bin kein Phantom, kein Trugbild! Nicht Luft ist dieser Leib!", "tokens": ["Bin", "kein", "Phan\u00b7tom", ",", "kein", "Trug\u00b7bild", "!", "Nicht", "Luft", "ist", "die\u00b7ser", "Leib", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$,", "PIAT", "NN", "$.", "PTKNEG", "NN", "VAFIN", "PDAT", "NN", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Hab' ich Dich nicht umschlungen, so liebereich und warm,", "tokens": ["Hab'", "ich", "Dich", "nicht", "um\u00b7schlun\u00b7gen", ",", "so", "lie\u00b7be\u00b7reich", "und", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "PTKNEG", "VVINF", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Und ruhtest, lustdurchdrungen, Du nicht in meinem Arm?\u00ab", "tokens": ["Und", "ruh\u00b7test", ",", "lust\u00b7durch\u00b7drun\u00b7gen", ",", "Du", "nicht", "in", "mei\u00b7nem", "Arm", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "$,", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.13": {"line.1": {"text": "\u00bbund hast Du mich gek\u00fcsst nicht, ach viele tausendmal?", "tokens": ["\u00bb", "und", "hast", "Du", "mich", "ge\u00b7k\u00fcsst", "nicht", ",", "ach", "vie\u00b7le", "tau\u00b7send\u00b7mal", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VAFIN", "PPER", "PRF", "ADJD", "PTKNEG", "$,", "ADV", "PIS", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Hab' ich Dir nicht geboren diess Kind mit Mutterqual?", "tokens": ["Hab'", "ich", "Dir", "nicht", "ge\u00b7bo\u00b7ren", "diess", "Kind", "mit", "Mut\u00b7ter\u00b7qual", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "PTKNEG", "VVPP", "PDAT", "NN", "APPR", "NN", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Nun qu\u00e4lt mich solcher Vorwurf, der mir das Herz zerbricht;", "tokens": ["Nun", "qu\u00e4lt", "mich", "sol\u00b7cher", "Vor\u00b7wurf", ",", "der", "mir", "das", "Herz", "zer\u00b7bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Abl\u00e4ugne Heil und Himmel, nur meine Liebe nicht!\u00ab", "tokens": ["Ab\u00b7l\u00e4ug\u00b7ne", "Heil", "und", "Him\u00b7mel", ",", "nur", "mei\u00b7ne", "Lie\u00b7be", "nicht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,", "ADV", "PPOSAT", "NN", "PTKNEG", "$.", "$("], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.14": {"line.1": {"text": "\u00bbdas ist Dein Fluch, mein Faustus, dass Du Dein Gl\u00fcck nicht glaubst;", "tokens": ["\u00bb", "das", "ist", "Dein", "Fluch", ",", "mein", "Faus\u00b7tus", ",", "dass", "Du", "Dein", "Gl\u00fcck", "nicht", "glaubst", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Dass Du mit ewger Sehnsucht Dir selbst den Frieden raubst!", "tokens": ["Dass", "Du", "mit", "ew\u00b7ger", "Sehn\u00b7sucht", "Dir", "selbst", "den", "Frie\u00b7den", "raubst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Kein Gott vermag zu tr\u00f6sten, Beklagenswerther, Dich!\u00ab", "tokens": ["Kein", "Gott", "ver\u00b7mag", "zu", "tr\u00f6s\u00b7ten", ",", "Be\u00b7kla\u00b7gens\u00b7wert\u00b7her", ",", "Dich", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKZU", "VVINF", "$,", "NN", "$,", "PPER", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So rief sie, herzte weinend den Knaben, und entwich.", "tokens": ["So", "rief", "sie", ",", "herz\u00b7te", "wei\u00b7nend", "den", "Kna\u00b7ben", ",", "und", "ent\u00b7wich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "ADJD", "ART", "NN", "$,", "KON", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.15": {"line.1": {"text": "\u00bbwohl wahr, kein Gott vermag das; Wahrheit hast Du gesagt!", "tokens": ["\u00bb", "wohl", "wahr", ",", "kein", "Gott", "ver\u00b7mag", "das", ";", "Wahr\u00b7heit", "hast", "Du", "ge\u00b7sagt", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "$,", "PIAT", "NN", "VVFIN", "PDS", "$.", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "O wie mir's in der Seele so furchtbar schrecklich tagt!", "tokens": ["O", "wie", "mir's", "in", "der", "See\u00b7le", "so", "furcht\u00b7bar", "schreck\u00b7lich", "tagt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "NE", "APPR", "ART", "NN", "ADV", "ADJD", "ADJD", "VVFIN", "$."], "meter": "---+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die R\u00f6the, die dort aufgl\u00fcht, ist das die Sonne? Nein!", "tokens": ["Die", "R\u00f6\u00b7the", ",", "die", "dort", "auf\u00b7gl\u00fcht", ",", "ist", "das", "die", "Son\u00b7ne", "?", "Nein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$,", "VAFIN", "ART", "ART", "NN", "$.", "PTKANT", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Brand ist es, H\u00f6llenfeuer; mich \u2013 stossen sie hinein!\u00ab", "tokens": ["Brand", "ist", "es", ",", "H\u00f6l\u00b7len\u00b7feu\u00b7er", ";", "mich", "\u2013", "stos\u00b7sen", "sie", "hin\u00b7ein", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "NN", "$.", "PPER", "$(", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.16": {"line.1": {"text": "\u00bbja n\u00e4her, schrecklich n\u00e4her kommt mir der letzte Tag;", "tokens": ["\u00bb", "ja", "n\u00e4\u00b7her", ",", "schreck\u00b7lich", "n\u00e4\u00b7her", "kommt", "mir", "der", "letz\u00b7te", "Tag", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "$,", "ADJD", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ein beutegierger Geier, wie saust sein Fl\u00fcgelschlag!", "tokens": ["Ein", "beu\u00b7te\u00b7gier\u00b7ger", "Gei\u00b7er", ",", "wie", "saust", "sein", "Fl\u00fc\u00b7gel\u00b7schlag", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und ich? Ich bin Prometheus, gefesselt Fuss und Hand,", "tokens": ["Und", "ich", "?", "Ich", "bin", "Pro\u00b7me\u00b7theus", ",", "ge\u00b7fes\u00b7selt", "Fuss", "und", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$.", "PPER", "VAFIN", "NE", "$,", "VVPP", "NN", "KON", "NN", "$,"], "meter": "---+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Steh' nackt ich angeschmiedet an nackter Felsenwand!\u00ab", "tokens": ["Steh'", "nackt", "ich", "an\u00b7ge\u00b7schmie\u00b7det", "an", "nack\u00b7ter", "Fel\u00b7sen\u00b7wand", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVPP", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.17": {"line.1": {"text": "\u00bbich kann nicht vor, nicht r\u00fcckw\u00e4rts! Weh mir \u2013 der Geier beisst!", "tokens": ["\u00bb", "ich", "kann", "nicht", "vor", ",", "nicht", "r\u00fcck\u00b7w\u00e4rts", "!", "Weh", "mir", "\u2013", "der", "Gei\u00b7er", "beisst", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PTKNEG", "PTKVZ", "$,", "PTKNEG", "ADV", "$.", "NN", "PPER", "$(", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Friss, friss, Du wilder Vogel! Den Leib nicht! Friss den Geist!", "tokens": ["Friss", ",", "friss", ",", "Du", "wil\u00b7der", "Vo\u00b7gel", "!", "Den", "Leib", "nicht", "!", "Friss", "den", "Geist", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "PPER", "ADJA", "NN", "$.", "ART", "NN", "PTKNEG", "$.", "NE", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ins Hirn hack' ein, das bietet Dir noch ein leckres Mahl;", "tokens": ["Ins", "Hirn", "hack'", "ein", ",", "das", "bie\u00b7tet", "Dir", "noch", "ein", "leck\u00b7res", "Mahl", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PTKVZ", "$,", "PDS", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wenn's erst der Wahnsinn anbrennt, mein Geier, dann wird's schal!\u00ab", "tokens": ["Wenn's", "erst", "der", "Wahn\u00b7sinn", "an\u00b7brennt", ",", "mein", "Gei\u00b7er", ",", "dann", "wird's", "schal", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "ADJD", "$.", "$("], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}}, "stanza.18": {"line.1": {"text": "Im wilden Schmerz aufheulend rauft Faustus sich das Haar,", "tokens": ["Im", "wil\u00b7den", "Schmerz", "auf\u00b7heu\u00b7lend", "rauft", "Faus\u00b7tus", "sich", "das", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "VVFIN", "NE", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und unter'm Ofen, spottend, heult auch Pr\u00e4stigiar.", "tokens": ["Und", "un\u00b7ter'm", "O\u00b7fen", ",", "spot\u00b7tend", ",", "heult", "auch", "Pr\u00e4s\u00b7ti\u00b7gi\u00b7ar", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "$,", "ADJD", "$,", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Der fletscht die scharfen Z\u00e4hne, rollt wild der Augen Gluth,", "tokens": ["Der", "fletscht", "die", "schar\u00b7fen", "Z\u00e4h\u00b7ne", ",", "rollt", "wild", "der", "Au\u00b7gen", "Gluth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,", "VVFIN", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So heult wohl die Hy\u00e4ne und rollt ihr Aug' nach Blut.", "tokens": ["So", "heult", "wohl", "die", "Hy\u00b7\u00e4\u00b7ne", "und", "rollt", "ihr", "Aug'", "nach", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "KON", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.19": {"line.1": {"text": "\u00bbkommt zu Euch, Herr!\u00ab spricht Wagner, und tritt dem Armen nah:", "tokens": ["\u00bb", "kommt", "zu", "Euch", ",", "Herr", "!", "\u00ab", "spricht", "Wag\u00b7ner", ",", "und", "tritt", "dem", "Ar\u00b7men", "nah", ":"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "PPER", "$,", "NN", "$.", "$(", "VVFIN", "NE", "$,", "KON", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "\u00bbihr tr\u00e4umt in Fieberwahnwitz, die Zeit ist noch nicht da!", "tokens": ["\u00bb", "ihr", "tr\u00e4umt", "in", "Fie\u00b7ber\u00b7wahn\u00b7witz", ",", "die", "Zeit", "ist", "noch", "nicht", "da", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "NE", "$,", "ART", "NN", "VAFIN", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Das Herz ist eine Glocke; wenn Schmerzgef\u00fchle ringen,", "tokens": ["Das", "Herz", "ist", "ei\u00b7ne", "Glo\u00b7cke", ";", "wenn", "Schmerz\u00b7ge\u00b7f\u00fch\u00b7le", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$.", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da schl\u00e4gt sie laut und angstvoll, und m\u00f6chte fast zerspringen.\u00ab", "tokens": ["Da", "schl\u00e4gt", "sie", "laut", "und", "angst\u00b7voll", ",", "und", "m\u00f6ch\u00b7te", "fast", "zer\u00b7sprin\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,", "KON", "VMFIN", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}