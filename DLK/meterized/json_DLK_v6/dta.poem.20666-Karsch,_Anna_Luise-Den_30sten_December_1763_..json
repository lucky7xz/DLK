{"dta.poem.20666": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Den 30sten December 1763 .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst bat ich von dem Sch\u00f6pfer aller T\u00f6ne,                   ", "tokens": ["J\u00fcngst", "bat", "ich", "von", "dem", "Sch\u00f6p\u00b7fer", "al\u00b7ler", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von dem Apoll, das Saitenspiel", "tokens": ["Von", "dem", "A\u00b7poll", ",", "das", "Sai\u00b7ten\u00b7spiel"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Des Sophocles, und rief, o Melpomene!", "tokens": ["Des", "So\u00b7pho\u00b7cles", ",", "und", "rief", ",", "o", "Mel\u00b7po\u00b7me\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "$,", "FM", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dich an mit tragischem Gef\u00fchl.", "tokens": ["Dich", "an", "mit", "tra\u00b7gi\u00b7schem", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dich lud ich ein zum klagenden Gesange,", "tokens": ["Dich", "lud", "ich", "ein", "zum", "kla\u00b7gen\u00b7den", "Ge\u00b7san\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn Preu\u00dfens gro\u00dfer Genius", "tokens": ["Denn", "Preu\u00b7\u00dfens", "gro\u00b7\u00dfer", "Ge\u00b7nius"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Gab mit verh\u00fcllter thr\u00e4nenvoller Wange", "tokens": ["Gab", "mit", "ver\u00b7h\u00fcll\u00b7ter", "thr\u00e4\u00b7nen\u00b7vol\u00b7ler", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem kranken ", "tokens": ["Dem", "kran\u00b7ken"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Und st\u00fcrmte des Olymp-Beherrschers Ohren,", "tokens": ["Und", "st\u00fcrm\u00b7te", "des", "O\u00b7lym\u00b7p\u00b7Be\u00b7herr\u00b7schers", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wenn er f\u00fcr diesen ", "tokens": ["Wenn", "er", "f\u00fcr", "die\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PDAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Der sch\u00f6n ist, wie das Antlitz von Auroren,", "tokens": ["Der", "sch\u00f6n", "ist", ",", "wie", "das", "Ant\u00b7litz", "von", "Au\u00b7ro\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$,", "PWAV", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So sch\u00f6n war nicht Aleibiad;", "tokens": ["So", "sch\u00f6n", "war", "nicht", "A\u00b7lei\u00b7bi\u00b7ad", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PTKNEG", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Den Soerates platonisch feurig liebte,", "tokens": ["Den", "Soe\u00b7ra\u00b7tes", "pla\u00b7to\u00b7nisch", "feu\u00b7rig", "lieb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und k\u00fcssend ihn zur Weisheit ri\u00df;", "tokens": ["Und", "k\u00fcs\u00b7send", "ihn", "zur", "Weis\u00b7heit", "ri\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So sch\u00f6n war nicht Pompejus der Verliebte,", "tokens": ["So", "sch\u00f6n", "war", "nicht", "Pom\u00b7pe\u00b7jus", "der", "Ver\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PTKNEG", "NE", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den in die Lippe Flora bi\u00df.", "tokens": ["Den", "in", "die", "Lip\u00b7pe", "Flo\u00b7ra", "bi\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NE", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O Muse! deine Schwestern senkten alle", "tokens": ["O", "Mu\u00b7se", "!", "dei\u00b7ne", "Schwes\u00b7tern", "senk\u00b7ten", "al\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PPOSAT", "NN", "VVFIN", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit aufgebundnem Haar und Kranz", "tokens": ["Mit", "auf\u00b7ge\u00b7bund\u00b7nem", "Haar", "und", "Kranz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich \u00fcber ihn, und riefen: Wenn er falle;", "tokens": ["Sich", "\u00fc\u00b7ber", "ihn", ",", "und", "rie\u00b7fen", ":", "Wenn", "er", "fal\u00b7le", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "$,", "KON", "VVFIN", "$.", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann schwiegen Saitenspiel und Tanz.", "tokens": ["Dann", "schwie\u00b7gen", "Sai\u00b7ten\u00b7spiel", "und", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dann w\u00fcrde wie bey Kriegesdonnerwetter", "tokens": ["Dann", "w\u00fcr\u00b7de", "wie", "bey", "Krie\u00b7ges\u00b7don\u00b7ner\u00b7wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "KOKOM", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Saal verschlossen, wo die Nacht", "tokens": ["Der", "Saal", "ver\u00b7schlos\u00b7sen", ",", "wo", "die", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den K\u00f6nig sieht, der Siegeslorbeerbl\u00e4tter", "tokens": ["Den", "K\u00f6\u00b7nig", "sieht", ",", "der", "Sie\u00b7ges\u00b7lor\u00b7beer\u00b7bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Versteckt in frischer Myrthen Pracht.", "tokens": ["Ver\u00b7steckt", "in", "fri\u00b7scher", "Myr\u00b7then", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und bey der Symphonien s\u00fc\u00dfem Tone", "tokens": ["Und", "bey", "der", "Sym\u00b7pho\u00b7ni\u00b7en", "s\u00fc\u00b7\u00dfem", "To\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Gr\u00f6\u00dfe seines Ruhms vergi\u00dft,", "tokens": ["Die", "Gr\u00f6\u00b7\u00dfe", "sei\u00b7nes", "Ruhms", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und froher, als auf rund umknieten Throne", "tokens": ["Und", "fro\u00b7her", ",", "als", "auf", "rund", "um\u00b7knie\u00b7ten", "Thro\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "KOUS", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "In seiner Freunde Zirkel ist.", "tokens": ["In", "sei\u00b7ner", "Freun\u00b7de", "Zir\u00b7kel", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und jezt Empfindung l\u00e4chelt in die Scene,", "tokens": ["Und", "jezt", "Emp\u00b7fin\u00b7dung", "l\u00e4\u00b7chelt", "in", "die", "Sce\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn hoch des S\u00e4ngers Busen bebt;", "tokens": ["Wenn", "hoch", "des", "S\u00e4n\u00b7gers", "Bu\u00b7sen", "bebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und lieblich spricht zur preu\u00dfischen Alcmene:", "tokens": ["Und", "lieb\u00b7lich", "spricht", "zur", "preu\u00b7\u00dfi\u00b7schen", "A\u00b7lcme\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df ihr Alcides wieder lebt.", "tokens": ["Da\u00df", "ihr", "Al\u00b7ci\u00b7des", "wie\u00b7der", "lebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Da\u00df Aesculap und die Natur verbunden,", "tokens": ["Da\u00df", "A\u00b7e\u00b7scu\u00b7lap", "und", "die", "Na\u00b7tur", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ihn rissen aus des Charons Kahn,", "tokens": ["Ihn", "ris\u00b7sen", "aus", "des", "Cha\u00b7rons", "Kahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und seinen Blick die Parce selbst empfunden,", "tokens": ["Und", "sei\u00b7nen", "Blick", "die", "Par\u00b7ce", "selbst", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die an dem goldnen Faden spann.", "tokens": ["Die", "an", "dem", "gold\u00b7nen", "Fa\u00b7den", "spann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}