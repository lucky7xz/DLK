{"textgrid.poem.57663": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Sonntag Palmarum", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heute ist Sonntag Palmarum,", "tokens": ["Heu\u00b7te", "ist", "Sonn\u00b7tag", "Pal\u00b7ma\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "NE", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der Wald ist voll Sonnenschein,", "tokens": ["Der", "Wald", "ist", "voll", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich bat dich, du m\u00f6chtest kommen,", "tokens": ["Ich", "bat", "dich", ",", "du", "m\u00f6ch\u00b7test", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Du sagtest, es k\u00f6nne nicht sein.", "tokens": ["Du", "sag\u00b7test", ",", "es", "k\u00f6n\u00b7ne", "nicht", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "PTKNEG", "VAINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Mit goldenen Palmen prangen", "tokens": ["Mit", "gol\u00b7de\u00b7nen", "Pal\u00b7men", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die B\u00fcsche am Waldesrand,", "tokens": ["Die", "B\u00fc\u00b7sche", "am", "Wal\u00b7des\u00b7rand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mit \u00e4ngstlichem Herzen ich wartend,", "tokens": ["Mit", "\u00e4ngst\u00b7li\u00b7chem", "Her\u00b7zen", "ich", "war\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Unter der Saalweide stand.", "tokens": ["Un\u00b7ter", "der", "Saal\u00b7wei\u00b7de", "stand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--++-+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Mir bl\u00fchte nicht die Weide,", "tokens": ["Mir", "bl\u00fch\u00b7te", "nicht", "die", "Wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kein Vogel ein Lied mir sang,", "tokens": ["Kein", "Vo\u00b7gel", "ein", "Lied", "mir", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich sah mit traurigen Augen", "tokens": ["Ich", "sah", "mit", "trau\u00b7ri\u00b7gen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den sonnigen Weg entlang.", "tokens": ["Den", "son\u00b7ni\u00b7gen", "Weg", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Nun bist du doch gekommen,", "tokens": ["Nun", "bist", "du", "doch", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Liebste, ich wu\u00dfte es ja,", "tokens": ["Liebs\u00b7te", ",", "ich", "wu\u00df\u00b7te", "es", "ja", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Goldgelb bl\u00fchen die Weiden,", "tokens": ["Gold\u00b7gelb", "bl\u00fc\u00b7hen", "die", "Wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Sonntag Palmarum ist da.", "tokens": ["Sonn\u00b7tag", "Pal\u00b7ma\u00b7rum", "ist", "da", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}