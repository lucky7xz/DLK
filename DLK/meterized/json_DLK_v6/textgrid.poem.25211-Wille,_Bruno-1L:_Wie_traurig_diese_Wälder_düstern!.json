{"textgrid.poem.25211": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie traurig diese W\u00e4lder d\u00fcstern!", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie traurig diese W\u00e4lder d\u00fcstern!", "tokens": ["Wie", "trau\u00b7rig", "die\u00b7se", "W\u00e4l\u00b7der", "d\u00fcs\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Sonnengold tief innen lacht;", "tokens": ["Kein", "Son\u00b7nen\u00b7gold", "tief", "in\u00b7nen", "lacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das tun die felsengrauen R\u00fcstern,", "tokens": ["Das", "tun", "die", "fel\u00b7sen\u00b7grau\u00b7en", "R\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Laubgeflechten \u00fcberdacht.", "tokens": ["Von", "Laub\u00b7ge\u00b7flech\u00b7ten", "\u00fc\u00b7berd\u00b7acht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auch ich so tr\u00fcb. Der Liebe Gnade", "tokens": ["Auch", "ich", "so", "tr\u00fcb", ".", "Der", "Lie\u00b7be", "Gna\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "ADV", "ADJD", "$.", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Darf strahlen nicht zu meinem Grund.", "tokens": ["Darf", "strah\u00b7len", "nicht", "zu", "mei\u00b7nem", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Sorg umd\u00fcstert meine Pfade,", "tokens": ["Die", "Sorg", "um\u00b7d\u00fcs\u00b7tert", "mei\u00b7ne", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich bin ein \u00f6der Dickichtschlund.", "tokens": ["Ich", "bin", "ein", "\u00f6\u00b7der", "Di\u00b7ckicht\u00b7schlund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch duld ich l\u00e4chelnd, heilge Sonne,", "tokens": ["Doch", "duld", "ich", "l\u00e4\u00b7chelnd", ",", "heil\u00b7ge", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df sich dein Brautku\u00df mir verschlie\u00dft/", "tokens": ["Da\u00df", "sich", "dein", "Braut\u00b7ku\u00df", "mir", "ver\u00b7schlie\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn drau\u00dfen nur die goldne Wonne", "tokens": ["Wenn", "drau\u00b7\u00dfen", "nur", "die", "gold\u00b7ne", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um tausend Sonnenkindlein flie\u00dft.", "tokens": ["Um", "tau\u00b7send", "Son\u00b7nen\u00b7kin\u00b7dlein", "flie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "La\u00df lieben dich mit jener Liebe,", "tokens": ["La\u00df", "lie\u00b7ben", "dich", "mit", "je\u00b7ner", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die nicht Genu\u00df, nur Andacht will.", "tokens": ["Die", "nicht", "Ge\u00b7nu\u00df", ",", "nur", "An\u00b7dacht", "will", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "NN", "$,", "ADV", "NN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ob ich ewig dunkel bliebe/", "tokens": ["Und", "ob", "ich", "e\u00b7wig", "dun\u00b7kel", "blie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von deinem Leuchten tr\u00e4um ich still.", "tokens": ["Von", "dei\u00b7nem", "Leuch\u00b7ten", "tr\u00e4um", "ich", "still", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}