{"textgrid.poem.61102": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Flei\u00dfig bl\u00e4ttr' ich die Alten mir durch, dann sinn' ich auf Lieder,", "tokens": ["Flei\u00b7\u00dfig", "bl\u00e4t\u00b7tr'", "ich", "die", "Al\u00b7ten", "mir", "durch", ",", "dann", "sinn'", "ich", "auf", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Bl\u00e4ttre wieder, und so fliehn mir die Stunden dahin.", "tokens": ["Bl\u00e4tt\u00b7re", "wie\u00b7der", ",", "und", "so", "fliehn", "mir", "die", "Stun\u00b7den", "da\u00b7hin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KON", "ADV", "VVFIN", "PPER", "ART", "NN", "PAV", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Gl\u00fccklicher Doppelgenu\u00df! Kaum wei\u00df ich, ist das Empfangen", "tokens": ["Gl\u00fcck\u00b7li\u00b7cher", "Dop\u00b7pel\u00b7ge\u00b7nu\u00df", "!", "Kaum", "wei\u00df", "ich", ",", "ist", "das", "Emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "$,", "VAFIN", "ART", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "S\u00fc\u00dfer, ist's das Gef\u00fchl, selber ein Dichter zu sein.", "tokens": ["S\u00fc\u00b7\u00dfer", ",", "ist's", "das", "Ge\u00b7f\u00fchl", ",", "sel\u00b7ber", "ein", "Dich\u00b7ter", "zu", "sein", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ART", "NN", "$,", "ADV", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Aber ich flehe zu euch, ihr G\u00f6tter, erhaltet mir gn\u00e4dig", "tokens": ["A\u00b7ber", "ich", "fle\u00b7he", "zu", "euch", ",", "ihr", "G\u00f6t\u00b7ter", ",", "er\u00b7hal\u00b7tet", "mir", "gn\u00e4\u00b7dig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "ADJD"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Jenen beweglichen Sinn, der sich auf beides versteht!", "tokens": ["Je\u00b7nen", "be\u00b7weg\u00b7li\u00b7chen", "Sinn", ",", "der", "sich", "auf", "bei\u00b7des", "ver\u00b7steht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,", "PRELS", "PRF", "APPR", "PIS", "VVFIN", "$."], "meter": "+--+--+--++--+", "measure": "dactylic.tri.plus"}, "line.7": {"text": "La\u00dft wie die Biene mich sein, die bald in der Rose sich festsaugt,", "tokens": ["La\u00dft", "wie", "die", "Bie\u00b7ne", "mich", "sein", ",", "die", "bald", "in", "der", "Ro\u00b7se", "sich", "fes\u00b7tsaugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KOKOM", "ART", "NN", "PPER", "VAINF", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.8": {"text": "Bald den gewonnenen Saft emsig in Honig verkehrt!", "tokens": ["Bald", "den", "ge\u00b7won\u00b7ne\u00b7nen", "Saft", "em\u00b7sig", "in", "Ho\u00b7nig", "ver\u00b7kehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ADJD", "APPR", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}}}}}