{"textgrid.poem.39460": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein fr\u00fches Veilchen, tief versteckt in Moosen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein fr\u00fches Veilchen, tief versteckt in Moosen,", "tokens": ["Ein", "fr\u00fc\u00b7hes", "Veil\u00b7chen", ",", "tief", "ver\u00b7steckt", "in", "Moo\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erblickt' ich; doch als Sommerl\u00fcfte giengen,", "tokens": ["Er\u00b7blickt'", "ich", ";", "doch", "als", "Som\u00b7mer\u00b7l\u00fcf\u00b7te", "gien\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "ADV", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wuchs es zum Strauch, den Dornen starr umfiengen;", "tokens": ["Wuchs", "es", "zum", "Strauch", ",", "den", "Dor\u00b7nen", "starr", "um\u00b7fi\u00b7en\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$,", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Dann wurden alle diese Dornen Rosen.", "tokens": ["Dann", "wur\u00b7den", "al\u00b7le", "die\u00b7se", "Dor\u00b7nen", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Aus ihnen h\u00f6rt' ich Liederstimmen kosen,", "tokens": ["Aus", "ih\u00b7nen", "h\u00f6rt'", "ich", "Lie\u00b7ders\u00b7tim\u00b7men", "ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sah empor sich Nachtigallen schwingen;", "tokens": ["Und", "sah", "em\u00b7por", "sich", "Nach\u00b7ti\u00b7gal\u00b7len", "schwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "PRF", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ich erkannt' in diesen Wunderdingen", "tokens": ["Und", "ich", "er\u00b7kannt'", "in", "die\u00b7sen", "Wun\u00b7der\u00b7din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Liebe liebliche Metamorphosen.", "tokens": ["Der", "Lie\u00b7be", "lieb\u00b7li\u00b7che", "Me\u00b7ta\u00b7mor\u00b7pho\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hab' ich in stiller Sch\u00fcchternheit verg\u00f6ttert,", "tokens": ["Hab'", "ich", "in", "stil\u00b7ler", "Sch\u00fcch\u00b7tern\u00b7heit", "ver\u00b7g\u00f6t\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Litt ich der Zweifel stechendes Gew\u00fchle,", "tokens": ["Litt", "ich", "der", "Zwei\u00b7fel", "ste\u00b7chen\u00b7des", "Ge\u00b7w\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Eh sich Erwiedrung wollt' entgegen neigen:", "tokens": ["Eh", "sich", "Er\u00b7wie\u00b7drung", "wollt'", "ent\u00b7ge\u00b7gen", "nei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NN", "VMFIN", "VVINF", "VVFIN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}, "stanza.4": {"line.1": {"text": "So sei mir nie der Rosen F\u00fcll' entbl\u00e4ttert,", "tokens": ["So", "sei", "mir", "nie", "der", "Ro\u00b7sen", "F\u00fcll'", "ent\u00b7bl\u00e4t\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es bleib' ihr Kelch die Wohnung der Gef\u00fchle,", "tokens": ["Es", "bleib'", "ihr", "Kelch", "die", "Woh\u00b7nung", "der", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die singend zwischen Erd' und Himmel steigen.", "tokens": ["Die", "sin\u00b7gend", "zwi\u00b7schen", "Erd'", "und", "Him\u00b7mel", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}