{"dta.poem.19105": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "LxXXVII.   Auf der Frau von Meu\u00dfbach  \n 81. Geburts-Tag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "So sehr ich sonst Natur und Stand", "tokens": ["So", "sehr", "ich", "sonst", "Na\u00b7tur", "und", "Stand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf immer zu verl\u00e4ugnen trachte;", "tokens": ["Auf", "im\u00b7mer", "zu", "ver\u00b7l\u00e4ug\u00b7nen", "trach\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und was mir irgends anverwandt,", "tokens": ["Und", "was", "mir", "ir\u00b7gends", "an\u00b7ver\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht anders als durch Christum achte:", "tokens": ["Nicht", "an\u00b7ders", "als", "durch", "Chris\u00b7tum", "ach\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KOKOM", "APPR", "NE", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So schleunig wacht die Regung auf", "tokens": ["So", "schleu\u00b7nig", "wacht", "die", "Re\u00b7gung", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Meinigen im HErrn zu segnen,", "tokens": ["Die", "Mei\u00b7ni\u00b7gen", "im", "Herrn", "zu", "seg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.7": {"text": "Wenn ihnen in dem Lebens-Lauf", "tokens": ["Wenn", "ih\u00b7nen", "in", "dem", "Le\u00b7bens\u00b7Lauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Besondre Schickungen begegnen.", "tokens": ["Be\u00b7sond\u00b7re", "Schi\u00b7ckun\u00b7gen", "be\u00b7geg\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Mein Trieb wird aufgebracht, die allgemeine Macht", "tokens": ["Mein", "Trieb", "wird", "auf\u00b7ge\u00b7bracht", ",", "die", "all\u00b7ge\u00b7mei\u00b7ne", "Macht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "In Glaubens Freyheit aufzubieten,", "tokens": ["In", "Glau\u00b7bens", "Frey\u00b7heit", "auf\u00b7zu\u00b7bie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "All eine, ", "tokens": ["All", "ei\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ART", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Wenn ich von ", "tokens": ["Wenn", "ich", "von"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "So, da\u00df es iederman erbau,", "tokens": ["So", ",", "da\u00df", "es", "ie\u00b7der\u00b7man", "er\u00b7bau", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So, da\u00df ich nichts verschweigen wolte,", "tokens": ["So", ",", "da\u00df", "ich", "nichts", "ver\u00b7schwei\u00b7gen", "wol\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie fest die Tugend sonderlich,", "tokens": ["Wie", "fest", "die", "Tu\u00b7gend", "son\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Tugend der Erbarmung stehet,", "tokens": ["Die", "Tu\u00b7gend", "der", "Er\u00b7bar\u00b7mung", "ste\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mit welcher sie so mildiglich,", "tokens": ["Mit", "wel\u00b7cher", "sie", "so", "mil\u00b7dig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und ungew\u00f6hnlich \u00fcbergehet:", "tokens": ["Und", "un\u00b7ge\u00b7w\u00f6hn\u00b7lich", "\u00fc\u00b7berg\u00b7e\u00b7het", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "So find ich zweyerley, das mir im Wege sey;", "tokens": ["So", "find", "ich", "zwey\u00b7er\u00b7ley", ",", "das", "mir", "im", "We\u00b7ge", "sey", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PRELS", "PPER", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Jhr eignes Demuths-volles ", "tokens": ["Ihr", "eig\u00b7nes", "De\u00b7muths\u00b7vol\u00b7les"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Weil, was sie Gutes thut, all ein auf ihr beruht,", "tokens": ["Weil", ",", "was", "sie", "Gu\u00b7tes", "thut", ",", "all", "ein", "auf", "ihr", "be\u00b7ruht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "PPER", "NN", "VVFIN", "$,", "PIAT", "ART", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch dieser Trieb ist nicht so leicht", "tokens": ["Doch", "die\u00b7ser", "Trieb", "ist", "nicht", "so", "leicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bey danckbarn Hertzen zu bez\u00e4hmen.", "tokens": ["Bey", "dan\u00b7ck\u00b7barn", "Hert\u00b7zen", "zu", "be\u00b7z\u00e4h\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der ehemals davon gezeugt:", "tokens": ["Der", "e\u00b7he\u00b7mals", "da\u00b7von", "ge\u00b7zeugt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der hielt die Zungen auch im Zaum,", "tokens": ["Der", "hielt", "die", "Zun\u00b7gen", "auch", "im", "Zaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die seine Thaten wolten sagen;", "tokens": ["Die", "sei\u00b7ne", "Tha\u00b7ten", "wol\u00b7ten", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie machten sich nicht minder Raum,", "tokens": ["Sie", "mach\u00b7ten", "sich", "nicht", "min\u00b7der", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es all enthalben auszutragen.", "tokens": ["Es", "all", "ent\u00b7hal\u00b7ben", "aus\u00b7zu\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Was ihr zu statten k\u00f6mmt, was meine Zunge hemmt,", "tokens": ["Was", "ihr", "zu", "stat\u00b7ten", "k\u00f6mmt", ",", "was", "mei\u00b7ne", "Zun\u00b7ge", "hemmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKZU", "VVINF", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mit dero Lobspruch auszubrechen,", "tokens": ["Mit", "de\u00b7ro", "Lob\u00b7spruch", "aus\u00b7zu\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ist ihrer Wohlthat Hand, und da\u00df ich ihr verwandt:", "tokens": ["Ist", "ih\u00b7rer", "Wohlt\u00b7hat", "Hand", ",", "und", "da\u00df", "ich", "ihr", "ver\u00b7wandt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$,", "KON", "KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So will ich denn, nach meiner Art", "tokens": ["So", "will", "ich", "denn", ",", "nach", "mei\u00b7ner", "Art"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit wenig Worten vieles deuten;", "tokens": ["Mit", "we\u00b7nig", "Wor\u00b7ten", "vie\u00b7les", "deu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich lobe den, der sie bewahrt:", "tokens": ["Ich", "lo\u00b7be", "den", ",", "der", "sie", "be\u00b7wahrt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich denck\u2019 an jene Ewigkeiten.", "tokens": ["Ich", "denck", "an", "je\u00b7ne", "E\u00b7wig\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie \u00fcberlebt schon ", "tokens": ["Sie", "\u00fc\u00b7ber\u00b7lebt", "schon"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Das ist ein Zeichen von der Gnade,", "tokens": ["Das", "ist", "ein", "Zei\u00b7chen", "von", "der", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Wie wenig sind ihr gleich, wie viel in einem Reich?", "tokens": ["Wie", "we\u00b7nig", "sind", "ihr", "gleich", ",", "wie", "viel", "in", "ei\u00b7nem", "Reich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "PPER", "ADV", "$,", "PWAV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wes ist hie Schrift und Bild? Des K\u00f6nigs, der vergilt,", "tokens": ["Wes", "ist", "hie", "Schrift", "und", "Bild", "?", "Des", "K\u00f6\u00b7nigs", ",", "der", "ver\u00b7gilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "NN", "KON", "NN", "$.", "ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.11": {"text": "Bey dem ihr Wohlthun unverloschen.", "tokens": ["Bey", "dem", "ihr", "Wohl\u00b7thun", "un\u00b7ver\u00b7lo\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was ist an einem solchen Fest", "tokens": ["Was", "ist", "an", "ei\u00b7nem", "sol\u00b7chen", "Fest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "APPR", "ART", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vorerst zu sagen und zu singen?", "tokens": ["Vor\u00b7erst", "zu", "sa\u00b7gen", "und", "zu", "sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das uns der HErr erscheinen l\u00e4st,", "tokens": ["Das", "uns", "der", "Herr", "er\u00b7schei\u00b7nen", "l\u00e4st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jhm Prei\u00df und Herrlichkeit zu bringen?", "tokens": ["Jhm", "Prei\u00df", "und", "Herr\u00b7lich\u00b7keit", "zu", "brin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was vor ein Wunsch wird \u00fcberbracht,", "tokens": ["Was", "vor", "ein", "Wunsch", "wird", "\u00fc\u00b7berb\u00b7racht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und h\u00e4tten gerne ausgemacht,", "tokens": ["Und", "h\u00e4t\u00b7ten", "ger\u00b7ne", "aus\u00b7ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Weil ihnen diese Welt beschwerlich?", "tokens": ["Weil", "ih\u00b7nen", "die\u00b7se", "Welt", "be\u00b7schwer\u00b7lich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Weil doch die Ruhe-Zeit ein m\u00fcdes Hertz erfreut,", "tokens": ["Weil", "doch", "die", "Ru\u00b7he\u00b7Zeit", "ein", "m\u00fc\u00b7des", "Hertz", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So sich am Scheitel drehn, am liebsten schmeltzen sehn,", "tokens": ["So", "sich", "am", "Schei\u00b7tel", "drehn", ",", "am", "liebs\u00b7ten", "schmelt\u00b7zen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "APPRART", "NN", "VVINF", "$,", "PTKA", "ADJD", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Damit der Geist in Friede fahre.", "tokens": ["Da\u00b7mit", "der", "Geist", "in", "Frie\u00b7de", "fah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Was w\u00fcnscht man sich? (aus Eigennutz:)", "tokens": ["Was", "w\u00fcnscht", "man", "sich", "?", "(", "aus", "Ei\u00b7gen\u00b7nutz", ":)"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "emoticon"], "pos": ["PWS", "VVFIN", "PIS", "PRF", "$.", "$(", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Wohlthat m\u00e4chtiglich bewege.", "tokens": ["Und", "Wohlt\u00b7hat", "m\u00e4ch\u00b7tig\u00b7lich", "be\u00b7we\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "ia