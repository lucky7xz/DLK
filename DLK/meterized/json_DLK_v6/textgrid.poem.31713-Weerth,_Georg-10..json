{"textgrid.poem.31713": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei still, du sollst nicht traurig sein!", "tokens": ["Sei", "still", ",", "du", "sollst", "nicht", "trau\u00b7rig", "sein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "PPER", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich la\u00df die Saiten klingen,", "tokens": ["Ich", "la\u00df", "die", "Sai\u00b7ten", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich will von Brandeliedelein", "tokens": ["Ich", "will", "von", "Bran\u00b7de\u00b7lie\u00b7de\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Parzival dir singen.", "tokens": ["Und", "Par\u00b7zi\u00b7val", "dir", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich will dir bis um Mitternacht", "tokens": ["Ich", "will", "dir", "bis", "um", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In bunt phantast'schen Bildern", "tokens": ["In", "bunt", "phan\u00b7tast'\u00b7schen", "Bil\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Entfernter L\u00e4nder Lust und Pracht", "tokens": ["Ent\u00b7fern\u00b7ter", "L\u00e4n\u00b7der", "Lust", "und", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und gr\u00fcne Meere schildern.", "tokens": ["Und", "gr\u00fc\u00b7ne", "Mee\u00b7re", "schil\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich f\u00fchre dich durchs H\u00fcgelland", "tokens": ["Ich", "f\u00fch\u00b7re", "dich", "durchs", "H\u00fc\u00b7gel\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hinaus zum blauen Strome,", "tokens": ["Hin\u00b7aus", "zum", "blau\u00b7en", "Stro\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo Burgen ragen \u00fcbern Strand", "tokens": ["Wo", "Bur\u00b7gen", "ra\u00b7gen", "\u00fc\u00b7bern", "Strand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und steingehaune Dome.", "tokens": ["Und", "stein\u00b7ge\u00b7hau\u00b7ne", "Do\u00b7me", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Zur Alpe, wo der Adler kreist,", "tokens": ["Zur", "Al\u00b7pe", ",", "wo", "der", "Ad\u00b7ler", "kreist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Tannenforst entstiegen,", "tokens": ["Dem", "Tan\u00b7nen\u00b7forst", "ent\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zur Stadt, die man Venedig hei\u00dft,", "tokens": ["Zur", "Stadt", ",", "die", "man", "Ve\u00b7ne\u00b7dig", "hei\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PIS", "NE", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Wo pr\u00e4cht'ge Gondeln liegen.", "tokens": ["Wo", "pr\u00e4cht'\u00b7ge", "Gon\u00b7deln", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich zeige dir im Mondenstrahl", "tokens": ["Ich", "zei\u00b7ge", "dir", "im", "Mon\u00b7dens\u00b7trahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Inseln der Hellenen;", "tokens": ["Die", "In\u00b7seln", "der", "Hel\u00b7le\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich will dich mit ins Blumental", "tokens": ["Ich", "will", "dich", "mit", "ins", "Blu\u00b7men\u00b7tal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu frommen V\u00f6lkern nehmen.", "tokens": ["Zu", "from\u00b7men", "V\u00f6l\u00b7kern", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du sollst dich wiegen auf der Flut", "tokens": ["Du", "sollst", "dich", "wie\u00b7gen", "auf", "der", "Flut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit einem sch\u00f6nen Schwane,", "tokens": ["Mit", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Schwa\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du sollst dich sonnen in der Glut", "tokens": ["Du", "sollst", "dich", "son\u00b7nen", "in", "der", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erzitternder Vulkane.", "tokens": ["Er\u00b7zit\u00b7tern\u00b7der", "Vul\u00b7ka\u00b7ne", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich will im d\u00fcstern Lorbeerwald", "tokens": ["Ich", "will", "im", "d\u00fcs\u00b7tern", "Lor\u00b7beer\u00b7wald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Grab der Dichter sprengen,", "tokens": ["Das", "Grab", "der", "Dich\u00b7ter", "spren\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df die Provence widerhallt", "tokens": ["Da\u00df", "die", "Pro\u00b7ven\u00b7ce", "wi\u00b7der\u00b7hallt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von t\u00f6nenden Ges\u00e4ngen.", "tokens": ["Von", "t\u00f6\u00b7nen\u00b7den", "Ge\u00b7s\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Du sollst die ew'ge Roma sehn,", "tokens": ["Du", "sollst", "die", "ew'\u00b7ge", "Ro\u00b7ma", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Tempeln wild zerrissen;", "tokens": ["Mit", "Tem\u00b7peln", "wild", "zer\u00b7ris\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du sollst hoch in den Pyren\u00e4n", "tokens": ["Du", "sollst", "hoch", "in", "den", "Py\u00b7re\u00b7n\u00e4n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein spanisch M\u00e4dchen k\u00fcssen!", "tokens": ["Ein", "spa\u00b7nisch", "M\u00e4d\u00b7chen", "k\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und willst du dennoch traurig sein?", "tokens": ["Und", "willst", "du", "den\u00b7noch", "trau\u00b7rig", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohlan, du deutsch Gem\u00fcte,", "tokens": ["Wo\u00b7hlan", ",", "du", "deutsch", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So nimm doch diesen Becher Wein", "tokens": ["So", "nimm", "doch", "die\u00b7sen", "Be\u00b7cher", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "ADV", "PDAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und diese Rosenbl\u00fcte!", "tokens": ["Und", "die\u00b7se", "Ro\u00b7sen\u00b7bl\u00fc\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}