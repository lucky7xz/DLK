{"textgrid.poem.61951": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Fanatismus und Geld", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf der Kette wohlverschlung'ner Berge", "tokens": ["Auf", "der", "Ket\u00b7te", "wohl\u00b7ver\u00b7schlung'\u00b7ner", "Ber\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Steh'n zwei Gnomen, stolz und m\u00e4chtig gro\u00df,", "tokens": ["Steh'n", "zwei", "Gno\u00b7men", ",", "stolz", "und", "m\u00e4ch\u00b7tig", "gro\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "$,", "ADJD", "KON", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Tausend Riesen knie'n vor jedem Zwerge,", "tokens": ["Tau\u00b7send", "Rie\u00b7sen", "knie'n", "vor", "je\u00b7dem", "Zwer\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ihre Arme m\u00fc\u00dfig bei dem eignen Los;", "tokens": ["Ih\u00b7re", "Ar\u00b7me", "m\u00fc\u00b7\u00dfig", "bei", "dem", "eig\u00b7nen", "Los", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Nur wenn jene Gnomen es gebieten,", "tokens": ["Nur", "wenn", "je\u00b7ne", "Gno\u00b7men", "es", "ge\u00b7bie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PDAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Eilt die Arbeit, da\u00df die Funken spr\u00fchten.", "tokens": ["Eilt", "die", "Ar\u00b7beit", ",", "da\u00df", "die", "Fun\u00b7ken", "spr\u00fch\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Jedem Zwerg ist untertan die Erde,", "tokens": ["Je\u00b7dem", "Zwerg", "ist", "un\u00b7ter\u00b7tan", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Kr\u00fcppelhaft gestaltet sich die Welt,", "tokens": ["Kr\u00fcp\u00b7pel\u00b7haft", "ge\u00b7stal\u00b7tet", "sich", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Riesen wurden eine staub'ge Herde,", "tokens": ["Rie\u00b7sen", "wur\u00b7den", "ei\u00b7ne", "staub'\u00b7ge", "Her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Vor dem Fanatismus und dem Geld, \u2013", "tokens": ["Vor", "dem", "Fa\u00b7na\u00b7tis\u00b7mus", "und", "dem", "Geld", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "$,", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Geist'gen Arme sch\u00fcttelt eure Kette,", "tokens": ["Geist'\u00b7gen", "Ar\u00b7me", "sch\u00fct\u00b7telt", "eu\u00b7re", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und die Gnomen gleichen Brandesst\u00e4tte!", "tokens": ["Und", "die", "Gno\u00b7men", "glei\u00b7chen", "Bran\u00b7des\u00b7st\u00e4t\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}