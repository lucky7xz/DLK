, "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PTKZU", "VAINF", "$.", "ADV", "VAFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.92": {"text": "Dergleichen grosser Ruhm, als ihm zu theile werden.", "tokens": ["Derg\u00b7lei\u00b7chen", "gros\u00b7ser", "Ruhm", ",", "als", "ihm", "zu", "thei\u00b7le", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJA", "NN", "$,", "KOUS", "PPER", "PTKZU", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.93": {"text": "Stimmt nur die Cyther rein, und zieht die Wirbel auf,", "tokens": ["Stimmt", "nur", "die", "Cy\u00b7ther", "rein", ",", "und", "zieht", "die", "Wir\u00b7bel", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADJD", "$,", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.94": {"text": "Und la\u00dft dem starken Zug der Ehrbegierde Lauf,", "tokens": ["Und", "la\u00dft", "dem", "star\u00b7ken", "Zug", "der", "Ehr\u00b7be\u00b7gier\u00b7de", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "ADJA", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.95": {"text": "So mu\u00df der Nachklang auch in Weiser Ohren bleiben,", "tokens": ["So", "mu\u00df", "der", "Nach\u00b7klang", "auch", "in", "Wei\u00b7ser", "Oh\u00b7ren", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "me