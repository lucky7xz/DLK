rd", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "$,", "PTKVZ", "$.", "FM", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.74": {"text": "Aus deiner tiefen Gruft; verla\u00df den Aschentopf,", "tokens": ["Aus", "dei\u00b7ner", "tie\u00b7fen", "Gruft", ";", "ver\u00b7la\u00df", "den", "A\u00b7schen\u00b7topf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "VVIMP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.75": {"text": "Und sieh, wie dieser Freund dich mit Bestand verehret,", "tokens": ["Und", "sieh", ",", "wie", "die\u00b7ser", "Freund", "dich", "mit", "Be\u00b7stand", "ver\u00b7eh\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PDAT", "NN", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.76": {"text": "Und deiner Liederklang im Sarg noch gerne h\u00f6ret.", "tokens": ["Und", "dei\u00b7ner", "Lie\u00b7der\u00b7klang", "im", "Sarg", "noch", "ger\u00b7ne", "h\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.77": {"text": "Man preist dich immer noch in unsrer Musen Chor,", "tokens": ["Man", "preist", "dich", "im\u00b7mer", "noch", "in", "uns\u00b7rer", "Mu\u00b7sen", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.78": {"text": "Und g\u00f6nnet deinem Ton stets ein geneigtes Ohr.", "tokens": ["Und", "g\u00f6n\u00b7net", "dei\u00b7nem", "Ton", "stets", "ein", "ge\u00b7neig\u00b7tes", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.79": {"text": "Dein lautes Seitenspiel und deine sch\u00f6nen Schriften,", "tokens": ["Dein", "lau\u00b7tes", "Sei\u00b7ten\u00b7spiel", "und", "dei\u00b7ne", "sch\u00f6\u00b7nen", "Schrif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.80": {"text": "Die m\u00fcssen dir f\u00fcrwahr ein ewig Denkmal stiften.", "tokens": ["Die", "m\u00fcs\u00b7sen", "dir", "f\u00fcr\u00b7wahr", "ein", "e\u00b7wig", "Denk\u00b7mal", "stif\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.81": {"text": "Kommt, kluge Geister! kommt, die ihr Begierde sp\u00fcrt,", "tokens": ["Kommt", ",", "klu\u00b7ge", "Geis\u00b7ter", "!", "kommt", ",", "die", "ihr", "Be\u00b7gier\u00b7de", "sp\u00fcrt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$.", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.82": {"text": "Von edlem Trieb entflammt, der Musen Fl\u00f6the r\u00fchrt,", "tokens": ["Von", "ed\u00b7lem", "Trieb", "ent\u00b7flammt", ",", "der", "Mu\u00b7sen", "Fl\u00f6\u00b7the", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.83": {"text": "Und euch den Pindus habt zur Wohnung auserkohren,", "tokens": ["Und", "euch", "den", "Pin\u00b7dus", "habt", "zur", "Woh\u00b7nung", "au\u00b7ser\u00b7koh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NE", "VAFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.84": {"text": "Verlacht der Feinde Schwarm; scheut nicht dergleichen Thoren.", "tokens": ["Ver\u00b7lacht", "der", "Fein\u00b7de", "Schwarm", ";", "scheut", "nicht", "derg\u00b7lei\u00b7chen", "Tho\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$.", "VVFIN", "PTKNEG", "PIS", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.85": {"text": "Ihr L\u00e4stern schadet euch bey klugen Leuten nicht,", "tokens": ["Ihr", "L\u00e4s\u00b7tern", "scha\u00b7det", "euch", "bey", "klu\u00b7gen", "Leu\u00b7ten", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.86": {"text": "Weil der so euch recht kennt, von euch geneigter spricht.", "tokens": ["Weil", "der", "so", "euch", "recht", "kennt", ",", "von", "euch", "ge\u00b7neig\u00b7ter", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "PPER", "ADJD", "VVFIN", "$,", "APPR", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.87": {"text": "Verdopplet