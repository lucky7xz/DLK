{"textgrid.poem.54405": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "15. Gedichte", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft ab, und macht euch nicht durch gar zu strengen Zwang", "tokens": ["La\u00dft", "ab", ",", "und", "macht", "euch", "nicht", "durch", "gar", "zu", "stren\u00b7gen", "Zwang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "APPR", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die heissen K\u00f6pfe w\u00fcst; ihr, die ihr durch Gesang", "tokens": ["Die", "heis\u00b7sen", "K\u00f6p\u00b7fe", "w\u00fcst", ";", "ihr", ",", "die", "ihr", "durch", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "PPER", "$,", "PRELS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und euren Fl\u00f6then Thon, bey so verderbten Zeiten", "tokens": ["Und", "eu\u00b7ren", "Fl\u00f6\u00b7then", "Thon", ",", "bey", "so", "ver\u00b7derb\u00b7ten", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN", "$,", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vor andern Ehr und Ruhm wollt mit Gewalt erbeuten.", "tokens": ["Vor", "an\u00b7dern", "Ehr", "und", "Ruhm", "wollt", "mit", "Ge\u00b7walt", "er\u00b7beu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ihr sitzet Tag und Nacht an Aganippens Flu\u00df,", "tokens": ["Ihr", "sit\u00b7zet", "Tag", "und", "Nacht", "an", "A\u00b7gan\u00b7ip\u00b7pens", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und da vergesset ihr auf einmal den Verdru\u00df.", "tokens": ["Und", "da", "ver\u00b7ges\u00b7set", "ihr", "auf", "ein\u00b7mal", "den", "Ver\u00b7dru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ADV", "ART", "NN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Da spielt ihr so entz\u00fcckt und br\u00fcnstig um die Wette,", "tokens": ["Da", "spielt", "ihr", "so", "ent\u00b7z\u00fcckt", "und", "br\u00fcns\u00b7tig", "um", "die", "Wet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVPP", "KON", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als wenn ein jeder viel davon zu hoffen h\u00e4tte.", "tokens": ["Als", "wenn", "ein", "je\u00b7der", "viel", "da\u00b7von", "zu", "hof\u00b7fen", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "PIAT", "PIS", "PAV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "O! werft doch unges\u00e4umt die matte Leyer hin,", "tokens": ["O", "!", "werft", "doch", "un\u00b7ge\u00b7s\u00e4umt", "die", "mat\u00b7te", "Le\u00b7yer", "hin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "ADV", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und st\u00f6ret weiter nicht den sonst so muntern Sinn.", "tokens": ["Und", "st\u00f6\u00b7ret", "wei\u00b7ter", "nicht", "den", "sonst", "so", "mun\u00b7tern", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "ART", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Warum zerbrecht ihr euch mit Vorsatz das Gehirne,", "tokens": ["Wa\u00b7rum", "zer\u00b7brecht", "ihr", "euch", "mit", "Vor\u00b7satz", "das", "Ge\u00b7hir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn ihr zu eurer Schmach der bulerhaften Dirne,", "tokens": ["Wenn", "ihr", "zu", "eu\u00b7rer", "Schmach", "der", "bu\u00b7ler\u00b7haf\u00b7ten", "Dir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Uranien, die man sowohl als euch, verlacht,", "tokens": ["U\u00b7ra\u00b7ni\u00b7en", ",", "die", "man", "so\u00b7wohl", "als", "euch", ",", "ver\u00b7lacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PIS", "ADV", "KOUS", "PPER", "$,", "VVPP", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.14": {"text": "Ein frisches Opfer bringt, und sie bey ihrer Macht,", "tokens": ["Ein", "fri\u00b7sches", "Op\u00b7fer", "bringt", ",", "und", "sie", "bey", "ih\u00b7rer", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KON", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Als eine G\u00f6nnerinn euch zuzuneigen trachtet;", "tokens": ["Als", "ei\u00b7ne", "G\u00f6n\u00b7ne\u00b7rinn", "euch", "zu\u00b7zu\u00b7nei\u00b7gen", "trach\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da man doch ihren Dienst, so wie euch selbst verachtet.", "tokens": ["Da", "man", "doch", "ih\u00b7ren", "Dienst", ",", "so", "wie", "euch", "selbst", "ver\u00b7ach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PPOSAT", "NN", "$,", "ADV", "KOKOM", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wer ist wohl, der antizt bey der verkehrten Welt", "tokens": ["Wer", "ist", "wohl", ",", "der", "an\u00b7tizt", "bey", "der", "ver\u00b7kehr\u00b7ten", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ADV", "$,", "PRELS", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "Auf Ph\u00f6bus edle Kunst und Wissenschaften h\u00e4lt?", "tokens": ["Auf", "Ph\u00f6\u00b7bus", "ed\u00b7le", "Kunst", "und", "Wis\u00b7sen\u00b7schaf\u00b7ten", "h\u00e4lt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Man sch\u00e4tzt sie h\u00f6her nicht, als das Geplerr der Dohlen,", "tokens": ["Man", "sch\u00e4tzt", "sie", "h\u00f6\u00b7her", "nicht", ",", "als", "das", "Ge\u00b7plerr", "der", "Doh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "PTKNEG", "$,", "KOUS", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die t\u00e4glich ihr Geschrey mit Ekel wiederholen.", "tokens": ["Die", "t\u00e4g\u00b7lich", "ihr", "Ge\u00b7schrey", "mit", "E\u00b7kel", "wie\u00b7der\u00b7ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Es ist ihr einerley, ob Opitz m\u00e4nnlich singt,", "tokens": ["Es", "ist", "ihr", "ei\u00b7ner\u00b7ley", ",", "ob", "O\u00b7pitz", "m\u00e4nn\u00b7lich", "singt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "$,", "KOUS", "NE", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und ob ein Sch\u00e4ferknecht, der Zung und Lippen zwingt,", "tokens": ["Und", "ob", "ein", "Sch\u00e4\u00b7fer\u00b7knecht", ",", "der", "Zung", "und", "Lip\u00b7pen", "zwingt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Auf einem d\u00fcnnen Blat, das man ihn sieht ergreifen,", "tokens": ["Auf", "ei\u00b7nem", "d\u00fcn\u00b7nen", "Blat", ",", "das", "man", "ihn", "sieht", "er\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PIS", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Sich selbst vor langer Weil ein Lied sucht vor zupfeifen.", "tokens": ["Sich", "selbst", "vor", "lan\u00b7ger", "Weil", "ein", "Lied", "sucht", "vor", "zu\u00b7pfei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Man hegt den tollen Wahn, als ob der Dichter Kunst,", "tokens": ["Man", "hegt", "den", "tol\u00b7len", "Wahn", ",", "als", "ob", "der", "Dich\u00b7ter", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "$,", "KOKOM", "KOUS", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die doch vom Himmel stammt, nichts als nur lerer Dunst", "tokens": ["Die", "doch", "vom", "Him\u00b7mel", "stammt", ",", "nichts", "als", "nur", "le\u00b7rer", "Dunst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPRART", "NN", "VVFIN", "$,", "PIS", "KOKOM", "ADV", "ADJA", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Und Hirngespinste sey; des kl\u00fcgsten Dichters Lieder", "tokens": ["Und", "Hirn\u00b7ge\u00b7spins\u00b7te", "sey", ";", "des", "kl\u00fcgs\u00b7ten", "Dich\u00b7ters", "Lie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "$.", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Sind ja zu dieser Zeit sehr vielen ganz zu wieder;", "tokens": ["Sind", "ja", "zu", "die\u00b7ser", "Zeit", "sehr", "vie\u00b7len", "ganz", "zu", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PDAT", "NN", "ADV", "PIAT", "ADV", "PTKA", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ja was? da sonst die Welt an K\u00fcnsten sich ergetzt,", "tokens": ["Ja", "was", "?", "da", "sonst", "die", "Welt", "an", "K\u00fcns\u00b7ten", "sich", "er\u00b7getzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PWS", "$.", "ADV", "ADV", "ART", "NN", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und jede Wissenschaft, wie billig, hochgesch\u00e4tzt,", "tokens": ["Und", "je\u00b7de", "Wis\u00b7sen\u00b7schaft", ",", "wie", "bil\u00b7lig", ",", "hoch\u00b7ge\u00b7sch\u00e4tzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PWAV", "ADJD", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So mu\u00df die Dichterkunst allein das Ungl\u00fcck treffen,", "tokens": ["So", "mu\u00df", "die", "Dich\u00b7ter\u00b7kunst", "al\u00b7lein", "das", "Un\u00b7gl\u00fcck", "tref\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da\u00df jeder Thore sie ver\u00e4chtlich sucht zu \u00e4ffen;", "tokens": ["Da\u00df", "je\u00b7der", "Tho\u00b7re", "sie", "ver\u00b7\u00e4cht\u00b7lich", "sucht", "zu", "\u00e4f\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "ADJD", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Der Musen rein Gewand mit Gall und Gift befleckt,", "tokens": ["Der", "Mu\u00b7sen", "rein", "Ge\u00b7wand", "mit", "Gall", "und", "Gift", "be\u00b7fleckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "APPR", "NE", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die Schm\u00e4hsucht gegen sie aus Schlaf und Schlummer weckt,", "tokens": ["Die", "Schm\u00e4h\u00b7sucht", "ge\u00b7gen", "sie", "aus", "Schlaf", "und", "Schlum\u00b7mer", "weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und weil er recht vor Neid und Misgunst schnaubt und rauchet,", "tokens": ["Und", "weil", "er", "recht", "vor", "Neid", "und", "Mis\u00b7gunst", "schnaubt", "und", "rau\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Auf ihrer Lorbern Pracht nach Art der Drachen hauchet.", "tokens": ["Auf", "ih\u00b7rer", "Lor\u00b7bern", "Pracht", "nach", "Art", "der", "Dra\u00b7chen", "hau\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Verschm\u00e4hter Musengott! was n\u00fctzt dein Unterricht?", "tokens": ["Ver\u00b7schm\u00e4h\u00b7ter", "Mu\u00b7sen\u00b7gott", "!", "was", "n\u00fctzt", "dein", "Un\u00b7ter\u00b7richt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Da man von deinem Chor nun so ver\u00e4chtlich spricht,", "tokens": ["Da", "man", "von", "dei\u00b7nem", "Chor", "nun", "so", "ver\u00b7\u00e4cht\u00b7lich", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ihm in das Antlitz speyt, auf seine Lieder fluchet,", "tokens": ["Ihm", "in", "das", "Ant\u00b7litz", "speyt", ",", "auf", "sei\u00b7ne", "Lie\u00b7der", "flu\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und den der ihn verehrt, stets zu verl\u00e4stern suchet;", "tokens": ["Und", "den", "der", "ihn", "ver\u00b7ehrt", ",", "stets", "zu", "ver\u00b7l\u00e4s\u00b7tern", "su\u00b7chet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PRELS", "PPER", "VVPP", "$,", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "O! hemm doch ferner hin der muntern Finger Lauf,", "tokens": ["O", "!", "hemm", "doch", "fer\u00b7ner", "hin", "der", "mun\u00b7tern", "Fin\u00b7ger", "Lauf", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "ADV", "ADV", "ADV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und h\u00e4ng das Seitenspiel an d\u00fcrre Fichten auf,", "tokens": ["Und", "h\u00e4ng", "das", "Sei\u00b7ten\u00b7spiel", "an", "d\u00fcr\u00b7re", "Fich\u00b7ten", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Denn man betritt zu frech des hohen Tempels Schwellen,", "tokens": ["Denn", "man", "be\u00b7tritt", "zu", "frech", "des", "ho\u00b7hen", "Tem\u00b7pels", "Schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PTKA", "ADJD", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Und sucht in ihrem Sitz den Musen nach zu stellen.", "tokens": ["Und", "sucht", "in", "ih\u00b7rem", "Sitz", "den", "Mu\u00b7sen", "nach", "zu", "stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Beth\u00f6rte Feder! halt, und gehe nicht zu weit,", "tokens": ["Be\u00b7th\u00f6r\u00b7te", "Fe\u00b7der", "!", 