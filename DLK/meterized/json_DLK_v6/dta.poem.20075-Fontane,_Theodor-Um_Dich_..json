{"dta.poem.20075": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Um Dich .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Storch und Schwalbe sind gekommen,", "tokens": ["Storch", "und", "Schwal\u00b7be", "sind", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Veilchen auch, die blauen frommen", "tokens": ["Veil\u00b7chen", "auch", ",", "die", "blau\u00b7en", "from\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "ART", "ADJA", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fr\u00fchlingsaugen gr\u00fc\u00dfen mich;", "tokens": ["Fr\u00fch\u00b7lings\u00b7au\u00b7gen", "gr\u00fc\u00b7\u00dfen", "mich", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber hin an Lenz und Leben", "tokens": ["A\u00b7ber", "hin", "an", "Lenz", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zieh in Bangen ich und Beben \u2014", "tokens": ["Zieh", "in", "Ban\u00b7gen", "ich", "und", "Be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PPER", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Um Dich.", "tokens": ["Um", "Dich", "."], "token_info": ["word", "word", "punct"], "pos": ["KOUI", "PPER", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Ach, um Dich! und doch ich f\u00fchle,", "tokens": ["Ach", ",", "um", "Dich", "!", "und", "doch", "ich", "f\u00fch\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUI", "PPER", "$.", "KON", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4te jetzt die Todesk\u00fchle", "tokens": ["Tr\u00e4\u00b7te", "jetzt", "die", "To\u00b7des\u00b7k\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "An mein Herz, und riefe mich,", "tokens": ["An", "mein", "Herz", ",", "und", "rie\u00b7fe", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein Kind dann, unter Jammern", "tokens": ["Wie", "ein", "Kind", "dann", ",", "un\u00b7ter", "Jam\u00b7mern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADV", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "W\u00fcrd\u2019 ich mich an\u2019s Leben klammern \u2014", "tokens": ["W\u00fcrd'", "ich", "mich", "an's", "Le\u00b7ben", "klam\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "APPRART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Um Dich.", "tokens": ["Um", "Dich", "."], "token_info": ["word", "word", "punct"], "pos": ["KOUI", "PPER", "$."], "meter": "-+", "measure": "iambic.single"}}}}}