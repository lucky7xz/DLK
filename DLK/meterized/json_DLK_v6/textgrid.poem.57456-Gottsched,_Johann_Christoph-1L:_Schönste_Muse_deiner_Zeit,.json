, "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Seyd gegr\u00fc\u00dft, ihr sch\u00f6nen Stunden!", "tokens": ["Seyd", "ge\u00b7gr\u00fc\u00dft", ",", "ihr", "sch\u00f6\u00b7nen", "Stun\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "VVPP", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eurer Morgenr\u00f6the Schein", "tokens": ["Eu\u00b7rer", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "Schein"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Soll mein liebster Anblick seyn,", "tokens": ["Soll", "mein", "liebs\u00b7ter", "An\u00b7blick", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Der sich jemals eingefunden:", "tokens": ["Der", "sich", "je\u00b7mals", "ein\u00b7ge\u00b7fun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Kommt noch oft, und stellt sie mir,", "tokens": ["Kommt", "noch", "oft", ",", "und", "stellt", "sie", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$,", "KON", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "So wie j\u00fcngst, im Traume f\u00fcr.", "tokens": ["So", "wie", "j\u00fcngst", ",", "im", "Trau\u00b7me", "f\u00fcr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADV", "$,", "APPRART", "NN", "APPR", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}}, "stanza.9": {"line.1": {"text": "Lies die\u00df Blatt, Victoria,", "tokens": ["Lies", "die\u00df", "Blatt", ",", "Vic\u00b7to\u00b7ria", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PDS", "NN", "$,", "NE", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Als ein treues Ehrfurchtszeichen.", "tokens": ["Als", "ein", "treu\u00b7es", "Ehr\u00b7furchts\u00b7zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O w\u00e4r ich dir itzt so nah!", "tokens": ["O", "w\u00e4r", "ich", "dir", "itzt", "so", "nah", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was k\u00f6nnt mir an Freude gleichen;", "tokens": ["Was", "k\u00f6nnt", "mir", "an", "Freu\u00b7de", "glei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Doch der Himmel kann es f\u00fcgen,", "tokens": ["Doch", "der", "Him\u00b7mel", "kann", "es", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df mein Wunsch sich bald erf\u00fcllt:", "tokens": ["Da\u00df", "mein", "Wunsch", "sich", "bald", "er\u00b7f\u00fcllt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und indessen soll dein Bild", "tokens": ["Und", "in\u00b7des\u00b7sen", "soll", "dein", "Bild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "In Gedanken mich vergn\u00fcgen;", "tokens": ["In", "Ge\u00b7dan\u00b7ken", "mich", "ver\u00b7gn\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Bis ich, (wenns doch bald gesch\u00e4h!)", "tokens": ["Bis", "ich", ",", "(", "wenns", "doch", "bald", "ge\u00b7sch\u00e4h", "!", ")"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "$,", "$(", "KOUS", "ADV", "ADV", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Dich pers\u00f6nlich wieder seh.", "tokens": ["Dich", "per\u00b7s\u00f6n\u00b7lich", "wie\u00b7der", "seh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}