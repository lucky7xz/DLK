{"textgrid.poem.57456": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sch\u00f6nste Muse deiner Zeit,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6nste Muse deiner Zeit,", "tokens": ["Sch\u00f6ns\u00b7te", "Mu\u00b7se", "dei\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unvergleichliche Louise!", "tokens": ["Un\u00b7ver\u00b7gleich\u00b7li\u00b7che", "Lou\u00b7i\u00b7se", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hilf doch meiner Sch\u00fcchternheit,", "tokens": ["Hilf", "doch", "mei\u00b7ner", "Sch\u00fcch\u00b7tern\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die dich itzt so gerne priese,", "tokens": ["Die", "dich", "itzt", "so", "ger\u00b7ne", "prie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lehre du mich selber dichten,", "tokens": ["Leh\u00b7re", "du", "mich", "sel\u00b7ber", "dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hilf mein schlechtes Rohr erh\u00f6hn;", "tokens": ["Hilf", "mein", "schlech\u00b7tes", "Rohr", "er\u00b7h\u00f6hn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn dein Lob so rein und sch\u00f6n,", "tokens": ["Denn", "dein", "Lob", "so", "rein", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Als du singest, einzurichten,", "tokens": ["Als", "du", "sin\u00b7gest", ",", "ein\u00b7zu\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Mu\u00df mein Lied so ungemein,", "tokens": ["Mu\u00df", "mein", "Lied", "so", "un\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Als dein ganzes Wesen seyn.", "tokens": ["Als", "dein", "gan\u00b7zes", "We\u00b7sen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wahrlich! ein so edler Geist", "tokens": ["Wahr\u00b7lich", "!", "ein", "so", "ed\u00b7ler", "Geist"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$.", "ART", "ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird nicht \u00fcberall gefunden,", "tokens": ["Wird", "nicht", "\u00fc\u00b7be\u00b7rall", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der, was Witz und Tugend hei\u00dft,", "tokens": ["Der", ",", "was", "Witz", "und", "Tu\u00b7gend", "hei\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch ein festes Band verbunden.", "tokens": ["Durch", "ein", "fes\u00b7tes", "Band", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Selbst bey M\u00e4nnern sieht man selten", "tokens": ["Selbst", "bey", "M\u00e4n\u00b7nern", "sieht", "man", "sel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VVFIN", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Solcher G\u00fcter Zahl vereint;", "tokens": ["Sol\u00b7cher", "G\u00fc\u00b7ter", "Zahl", "ver\u00b7eint", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Als in deinem Thun erscheint;", "tokens": ["Als", "in", "dei\u00b7nem", "Thun", "er\u00b7scheint", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wo sie wahrlich zwiefach gelten:", "tokens": ["Wo", "sie", "wahr\u00b7lich", "zwie\u00b7fach", "gel\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Weil man niemals mehr Verstand", "tokens": ["Weil", "man", "nie\u00b7mals", "mehr", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Bey so zarter Jugend fand.", "tokens": ["Bey", "so", "zar\u00b7ter", "Ju\u00b7gend", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Kann doch weder Stolz noch Geiz", "tokens": ["Kann", "doch", "we\u00b7der", "Stolz", "noch", "Geiz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "KON", "NN", "ADV", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In dein starkes Herze dringen,", "tokens": ["In", "dein", "star\u00b7kes", "Her\u00b7ze", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch der Eitelkeiten Reiz", "tokens": ["Noch", "der", "Ei\u00b7tel\u00b7kei\u00b7ten", "Reiz"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine gro\u00dfe Seele zwingen!", "tokens": ["Dei\u00b7ne", "gro\u00b7\u00dfe", "See\u00b7le", "zwin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deiner Mutter Witz und Tugend,", "tokens": ["Dei\u00b7ner", "Mut\u00b7ter", "Witz", "und", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Einsicht und Belesenheit", "tokens": ["Ein\u00b7sicht", "und", "Be\u00b7le\u00b7sen\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "F\u00fchrt dich zur Gelehrsamkeit,", "tokens": ["F\u00fchrt", "dich", "zur", "Ge\u00b7lehr\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und verg\u00f6ttert deine Jugend;", "tokens": ["Und", "ver\u00b7g\u00f6t\u00b7tert", "dei\u00b7ne", "Ju\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Welche so schon, wie du bist,", "tokens": ["Wel\u00b7che", "so", "schon", ",", "wie", "du", "bist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Englisch mehr, als menschlich ist.", "tokens": ["Eng\u00b7lisch", "mehr", ",", "als", "menschlich", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KOUS", "ADJD", "VAFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "In der K\u00fcnste Feld gedrungen,", "tokens": ["In", "der", "K\u00fcns\u00b7te", "Feld", "ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als es dir, der Weisheit Kern", "tokens": ["Als", "es", "dir", ",", "der", "Weis\u00b7heit", "Kern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "$,", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gr\u00fcndlich einzusehn, gelungen.", "tokens": ["Gr\u00fcnd\u00b7lich", "ein\u00b7zu\u00b7sehn", ",", "ge\u00b7lun\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So viel Frauenzimmerspiele", "tokens": ["So", "viel", "Frau\u00b7en\u00b7zim\u00b7mer\u00b7spie\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Man bisher bey uns vernahm,", "tokens": ["Man", "bis\u00b7her", "bey", "uns", "ver\u00b7nahm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Klingen schlecht, ja matt und lahm", "tokens": ["Klin\u00b7gen", "schlecht", ",", "ja", "matt", "und", "lahm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "ADV", "ADJD", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gegen deinem Dichterkiele;", "tokens": ["Ge\u00b7gen", "dei\u00b7nem", "Dich\u00b7ter\u00b7kie\u00b7le", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Welcher nicht nur sie verlacht,", "tokens": ["Wel\u00b7cher", "nicht", "nur", "sie", "ver\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "PTKNEG", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Nein! auch M\u00e4nner neidisch macht.", "tokens": ["Nein", "!", "auch", "M\u00e4n\u00b7ner", "nei\u00b7disch", "macht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "K\u00fcnftig darf sich dein Geschlecht", "tokens": ["K\u00fcnf\u00b7tig", "darf", "sich", "dein", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "PRF", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seiner Schwachheit nicht mehr sch\u00e4men;", "tokens": ["Sei\u00b7ner", "Schwach\u00b7heit", "nicht", "mehr", "sch\u00e4\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Dichtkunst Meisterrecht", "tokens": ["Und", "der", "Dicht\u00b7kunst", "Meis\u00b7ter\u00b7recht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gleich den st\u00e4rksten Dichtern nehmen.", "tokens": ["Gleich", "den", "st\u00e4rks\u00b7ten", "Dich\u00b7tern", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Adelgunde wird mit Ruhme", "tokens": ["A\u00b7del\u00b7gun\u00b7de", "wird", "mit", "Ruh\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unsers Preu\u00dfens ", "tokens": ["Un\u00b7sers", "Preu\u00b7\u00dfens"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Ja die\u00df Lob ist dir zu klein,", "tokens": ["Ja", "die\u00df", "Lob", "ist", "dir", "zu", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PDS", "NN", "VAFIN", "PPER", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Deutschland trotzt dem Alterthume;", "tokens": ["Deutschland", "trotzt", "dem", "Al\u00b7ter\u00b7thu\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Denn du f\u00e4ngst viel st\u00e4rker an,", "tokens": ["Denn", "du", "f\u00e4ngst", "viel", "st\u00e4r\u00b7ker", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Als es Sappho enden kann.", "tokens": ["Als", "es", "Sap\u00b7pho", "en\u00b7den", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wird die kluge ", "tokens": ["Wird", "die", "klu\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "N\u00e4chst, durch dich, auch deutsch gelesen,", "tokens": ["N\u00e4chst", ",", "durch", "dich", ",", "auch", "deutsch", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PPER", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6mmt man leichtlich auf die Spur,", "tokens": ["K\u00f6mmt", "man", "leicht\u00b7lich", "auf", "die", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Welch ein Geist dabey gewesen.", "tokens": ["Welch", "ein", "Geist", "da\u00b7bey", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "PAV", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch wer weis, obs jemand glaubet?", "tokens": ["Doch", "wer", "weis", ",", "obs", "je\u00b7mand", "glau\u00b7bet", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKVZ", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der, wenn ihn die Schrift ergetzt,", "tokens": ["Der", ",", "wenn", "ihn", "die", "Schrift", "er\u00b7getzt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dich, die du sie \u00fcbersetzt,", "tokens": ["Dich", ",", "die", "du", "sie", "\u00fc\u00b7bers\u00b7etzt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Des verdienten Ruhms beraubet:", "tokens": ["Des", "ver\u00b7dien\u00b7ten", "Ruhms", "be\u00b7rau\u00b7bet", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Weil er solcher Schreibart Preis", "tokens": ["Weil", "er", "sol\u00b7cher", "Schrei\u00b7bart", "Preis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Noch von keiner Sch\u00f6nen weis.", "tokens": ["Noch", "von", "kei\u00b7ner", "Sch\u00f6\u00b7nen", "weis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Dieses Geistes seltne Pracht,", "tokens": ["Die\u00b7ses", "Geis\u00b7tes", "selt\u00b7ne", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser edlen Seele Gaben,", "tokens": ["Die\u00b7ser", "ed\u00b7len", "See\u00b7le", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcrden mich entz\u00fcckt gemacht,", "tokens": ["W\u00fcr\u00b7den", "mich", "ent\u00b7z\u00fcckt", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fcrden mich bezaubert haben;", "tokens": ["W\u00fcr\u00b7den", "mich", "be\u00b7zau\u00b7bert", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00e4tt ich gleich am Weichselstrande", "tokens": ["H\u00e4tt", "ich", "gleich", "am", "Weich\u00b7sel\u00b7stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deine Sch\u00f6nheit nie erblickt:", "tokens": ["Dei\u00b7ne", "Sch\u00f6n\u00b7heit", "nie", "er\u00b7blickt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn dadurch ist mirs gegl\u00fcckt,", "tokens": ["Denn", "da\u00b7durch", "ist", "mirs", "ge\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "NE", "VVPP", "$,"], "meter": "----+-+", "measure": "unknown.