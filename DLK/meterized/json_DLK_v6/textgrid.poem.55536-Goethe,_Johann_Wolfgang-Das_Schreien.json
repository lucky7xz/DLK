{"textgrid.poem.55536": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Das Schreien", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst schlich ich meinem M\u00e4dchen nach,", "tokens": ["J\u00fcngst", "schlich", "ich", "mei\u00b7nem", "M\u00e4d\u00b7chen", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ohne Hindernis", "tokens": ["Und", "oh\u00b7ne", "Hin\u00b7der\u00b7nis"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Umfa\u00dft ich sie im Hain; sie sprach:", "tokens": ["Um\u00b7fa\u00dft", "ich", "sie", "im", "Hain", ";", "sie", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "NN", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbla\u00df mich, ich schrei gewi\u00df.\u00ab", "tokens": ["\u00bb", "la\u00df", "mich", ",", "ich", "schrei", "ge\u00b7wi\u00df", ".", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "PPER", "$,", "PPER", "ADJD", "ADV", "$.", "$("], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.2": {"line.1": {"text": "Da droht ich trotzig: \u00bbHa, ich will", "tokens": ["Da", "droht", "ich", "trot\u00b7zig", ":", "\u00bb", "Ha", ",", "ich", "will"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$.", "$(", "ITJ", "$,", "PPER", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den t\u00f6ten, der uns st\u00f6rt.\u00ab", "tokens": ["Den", "t\u00f6\u00b7ten", ",", "der", "uns", "st\u00f6rt", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbstill\u00ab, winkt sie lispelnd, \u00bbLiebster, still,", "tokens": ["\u00bb", "still", "\u00ab", ",", "winkt", "sie", "lis\u00b7pelnd", ",", "\u00bb", "Liebs\u00b7ter", ",", "still", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "ADJD", "$(", "$,", "VVFIN", "PPER", "VVPP", "$,", "$(", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Damit dich niemand h\u00f6rt.\u00ab", "tokens": ["Da\u00b7mit", "dich", "nie\u00b7mand", "h\u00f6rt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}