{"textgrid.poem.36912": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein K\u00f6nig ging im Monat Mai", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein K\u00f6nig ging im Monat Mai", "tokens": ["Ein", "K\u00f6\u00b7nig", "ging", "im", "Mo\u00b7nat", "Mai"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An einem gr\u00fcnen Wald vorbei,", "tokens": ["An", "ei\u00b7nem", "gr\u00fc\u00b7nen", "Wald", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kuckuck!", "tokens": ["Ku\u00b7ckuck", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Mit finstrem Aug' und finstrem Sinn,", "tokens": ["Mit", "fins\u00b7trem", "Aug'", "und", "fins\u00b7trem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So br\u00fctete er vor sich hin.", "tokens": ["So", "br\u00fc\u00b7te\u00b7te", "er", "vor", "sich", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kuckuck! Kuckuck!", "tokens": ["Ku\u00b7ckuck", "!", "Ku\u00b7ckuck", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Das Bl\u00fchen all, ihm bl\u00fcht es nicht,", "tokens": ["Das", "Bl\u00fc\u00b7hen", "all", ",", "ihm", "bl\u00fcht", "es", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Winter steht ihm im Gesicht;", "tokens": ["Der", "Win\u00b7ter", "steht", "ihm", "im", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kuckuck!", "tokens": ["Ku\u00b7ckuck", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "\u00bbso viele K\u00f6pf' beherrsche ich,", "tokens": ["\u00bb", "so", "vie\u00b7le", "K\u00f6pf'", "be\u00b7herr\u00b7sche", "ich", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PIAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kein Herz in Liebe schl\u00e4gt f\u00fcr mich!\u00ab", "tokens": ["Kein", "Herz", "in", "Lie\u00b7be", "schl\u00e4gt", "f\u00fcr", "mich", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "VVFIN", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kuckuck! Kuckuck!", "tokens": ["Ku\u00b7ckuck", "!", "Ku\u00b7ckuck", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "\u00bbda h\u00f6r' ich ja den Kuckuck schrein;", "tokens": ["\u00bb", "da", "h\u00f6r'", "ich", "ja", "den", "Ku\u00b7ckuck", "schrein", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das soll ein weiser Vogel sein.\u00ab", "tokens": ["Das", "soll", "ein", "wei\u00b7ser", "Vo\u00b7gel", "sein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VMFIN", "ART", "ADJA", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kuckuck!", "tokens": ["Ku\u00b7ckuck", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "\u00bbdu weiser Vogel sag' mir doch,", "tokens": ["\u00bb", "du", "wei\u00b7ser", "Vo\u00b7gel", "sag'", "mir", "doch", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie lange Jahre herrsch' ich noch?\u00ab", "tokens": ["Wie", "lan\u00b7ge", "Jah\u00b7re", "herr\u00b7sch'", "ich", "noch", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADV", "NN", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Kuckuck! Kuckuck!", "tokens": ["Ku\u00b7ckuck", "!", "Ku\u00b7ckuck", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Der Vogel h\u00fcpft auf gr\u00fcnem Zweig", "tokens": ["Der", "Vo\u00b7gel", "h\u00fcpft", "auf", "gr\u00fc\u00b7nem", "Zweig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ruft durch's freie Fr\u00fchlingsreich:", "tokens": ["Und", "ruft", "durch's", "frei\u00b7e", "Fr\u00fch\u00b7lings\u00b7reich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kuckuck!", "tokens": ["Ku\u00b7ckuck", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Der finstre K\u00f6nig wird vers\u00f6hnt,", "tokens": ["Der", "finst\u00b7re", "K\u00f6\u00b7nig", "wird", "ver\u00b7s\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil's fort und immer wieder t\u00f6nt:", "tokens": ["Weil's", "fort", "und", "im\u00b7mer", "wie\u00b7der", "t\u00f6nt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "KON", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kuckuck! Kuckuck!", "tokens": ["Ku\u00b7ckuck", "!", "Ku\u00b7ckuck", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Drauf kam ein gn\u00e4diger Erla\u00df", "tokens": ["Drauf", "kam", "ein", "gn\u00e4\u00b7di\u00b7ger", "Er\u00b7la\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Staatsminister auf die Ga\u00df!", "tokens": ["Vom", "Staats\u00b7mi\u00b7nis\u00b7ter", "auf", "die", "Ga\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kuckuck!", "tokens": ["Ku\u00b7ckuck", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Das Volk sei aller Sorge bar:", "tokens": ["Das", "Volk", "sei", "al\u00b7ler", "Sor\u00b7ge", "bar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der K\u00f6nig herrscht noch drei\u00dfig Jahr!", "tokens": ["Der", "K\u00f6\u00b7nig", "herrscht", "noch", "drei\u00b7\u00dfig", "Jahr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "CARD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kuckuck! Kuckuck!", "tokens": ["Ku\u00b7ckuck", "!", "Ku\u00b7ckuck", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Der Kuckuck h\u00e4tt' es ihm vertraut;", "tokens": ["Der", "Ku\u00b7ckuck", "h\u00e4tt'", "es", "ihm", "ver\u00b7traut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es sei im ganzen Reiche laut;", "tokens": ["Es", "sei", "im", "gan\u00b7zen", "Rei\u00b7che", "laut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NE", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kuckuck!", "tokens": ["Ku\u00b7ckuck", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Der Vogel sei auch schon gefang'n,", "tokens": ["Der", "Vo\u00b7gel", "sei", "auch", "schon", "ge\u00b7fang'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und ihm ein Orden umgehang'n.", "tokens": ["Und", "ihm", "ein", "Or\u00b7den", "um\u00b7ge\u00b7hang'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "PPER", "ART", "NN", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kuckuck! Kuckuck!", "tokens": ["Ku\u00b7ckuck", "!", "Ku\u00b7ckuck", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "So oft nun Seine Majest\u00e4t", "tokens": ["So", "oft", "nun", "Sei\u00b7ne", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Zorn und Wuth auf's Volk ger\u00e4th \u2013", "tokens": ["In", "Zorn", "und", "Wuth", "auf's", "Volk", "ge\u00b7r\u00e4\u00b7th", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kuckuck!", "tokens": ["Ku\u00b7ckuck", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "H\u00f6rt man im ganzen Lande flehn:", "tokens": ["H\u00f6rt", "man", "im", "gan\u00b7zen", "Lan\u00b7de", "flehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O m\u00f6cht' er doch zum Kuckuck gehn!", "tokens": ["O", "m\u00f6cht'", "er", "doch", "zum", "Ku\u00b7ckuck", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kuckuck! Kuckuck!", "tokens": ["Ku\u00b7ckuck", "!", "Ku\u00b7ckuck", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}