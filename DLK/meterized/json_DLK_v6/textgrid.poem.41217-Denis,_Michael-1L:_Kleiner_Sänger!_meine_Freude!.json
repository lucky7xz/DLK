{"textgrid.poem.41217": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kleiner S\u00e4nger! meine Freude!", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kleiner S\u00e4nger! meine Freude!", "tokens": ["Klei\u00b7ner", "S\u00e4n\u00b7ger", "!", "mei\u00b7ne", "Freu\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zeuge meiner Einsamkeit!", "tokens": ["Zeu\u00b7ge", "mei\u00b7ner", "Ein\u00b7sam\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meiner Ohren Lust und Weide!", "tokens": ["Mei\u00b7ner", "Oh\u00b7ren", "Lust", "und", "Wei\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir sei dieses Lied geweiht.", "tokens": ["Dir", "sei", "die\u00b7ses", "Lied", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Allerliebstes V\u00f6gelchen!", "tokens": ["Al\u00b7ler\u00b7liebs\u00b7tes", "V\u00f6\u00b7gel\u00b7chen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Was dein Herr in dreien Jahren", "tokens": ["Was", "dein", "Herr", "in", "drei\u00b7en", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "R\u00fchmliches an dir erfahren,", "tokens": ["R\u00fchm\u00b7li\u00b7ches", "an", "dir", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Soll die Welt in Reimen seh'n!", "tokens": ["Soll", "die", "Welt", "in", "Rei\u00b7men", "seh'n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "And're m\u00f6gen Menschen loben!", "tokens": ["An\u00b7d'\u00b7re", "m\u00f6\u00b7gen", "Men\u00b7schen", "lo\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Menschen sind des Lob's gewohnt.", "tokens": ["Men\u00b7schen", "sind", "des", "Lob's", "ge\u00b7wohnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihr habt den, und die erhoben,", "tokens": ["Ihr", "habt", "den", ",", "und", "die", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "$,", "KON", "ART", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dichter war't ihr auch belohn't?", "tokens": ["Dich\u00b7ter", "wa\u00b7r't", "ihr", "auch", "be\u00b7lohn't", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "O mir bl\u00fcht ein besser Gl\u00fcck!", "tokens": ["O", "mir", "bl\u00fcht", "ein", "bes\u00b7ser", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was ich singe dir zu Ehren,", "tokens": ["Was", "ich", "sin\u00b7ge", "dir", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dieses singst du mir Homeren", "tokens": ["Die\u00b7ses", "singst", "du", "mir", "Ho\u00b7me\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "PPER", "NE"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.8": {"text": "Zehnfach mein Achill! zur\u00fcck.", "tokens": ["Zehn\u00b7fach", "mein", "A\u00b7chill", "!", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Nun so fang' ich an zu dichten.", "tokens": ["Nun", "so", "fang'", "ich", "an", "zu", "dich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Panegyrisch sei mein Flug! \u2013", "tokens": ["Pa\u00b7ne\u00b7gy\u00b7risch", "sei", "mein", "Flug", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du sollst Menschen unterrichten,", "tokens": ["Du", "sollst", "Men\u00b7schen", "un\u00b7ter\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebes Thier! ist's dir genug?", "tokens": ["Lie\u00b7bes", "Thier", "!", "ist's", "dir", "ge\u00b7nug", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll mir in mein Lobgedicht", "tokens": ["Soll", "mir", "in", "mein", "Lob\u00b7ge\u00b7dicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Manchesmal der Satyr lachen,", "tokens": ["Man\u00b7ches\u00b7mal", "der", "Sa\u00b7tyr", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ha, wie kann ich's anders machen?", "tokens": ["Ha", ",", "wie", "kann", "ich's", "an\u00b7ders", "ma\u00b7chen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Menschen \u2013 sie belohnen nicht!", "tokens": ["Men\u00b7schen", "\u2013", "sie", "be\u00b7loh\u00b7nen", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Bei dem ersten Morgenschimmer", "tokens": ["Bei", "dem", "ers\u00b7ten", "Mor\u00b7gen\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bist du schon, mein Vogel! reg.", "tokens": ["Bist", "du", "schon", ",", "mein", "Vo\u00b7gel", "!", "reg."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "abbreviation"], "pos": ["VAFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$.", "XY"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Deine Kehle f\u00fcllt das Zimmer,", "tokens": ["Dei\u00b7ne", "Keh\u00b7le", "f\u00fcllt", "das", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Singt mir meinen Schlummer weg.", "tokens": ["Singt", "mir", "mei\u00b7nen", "Schlum\u00b7mer", "weg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses kann Dorinde nicht.", "tokens": ["Die\u00b7ses", "kann", "Do\u00b7rin\u00b7de", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "NE", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Soll so fr\u00fch der Schlaf entweichen?", "tokens": ["Soll", "so", "fr\u00fch", "der", "Schlaf", "ent\u00b7wei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Zarten Dingern ihres gleichen", "tokens": ["Zar\u00b7ten", "Din\u00b7gern", "ih\u00b7res", "glei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wird es erst am Mittag licht.", "tokens": ["Wird", "es", "erst", "am", "Mit\u00b7tag", "licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Fern, mein Schwarzkopf! von dem Zwange,", "tokens": ["Fern", ",", "mein", "Schwarz\u00b7kopf", "!", "von", "dem", "Zwan\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$.", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der die F\u00e4higkeit entehrt,", "tokens": ["Der", "die", "F\u00e4\u00b7hig\u00b7keit", "ent\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bleibst du stets bei dem Gesange,", "tokens": ["Bleibst", "du", "stets", "bei", "dem", "Ge\u00b7san\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den dich die Natur gelehrt.", "tokens": ["Den", "dich", "die", "Na\u00b7tur", "ge\u00b7lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses kann Alcindor nicht,", "tokens": ["Die\u00b7ses", "kann", "Al\u00b7cin\u00b7dor", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "NE", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der, f\u00fcrs Fabelreich gebohren,", "tokens": ["Der", ",", "f\u00fcrs", "Fa\u00b7bel\u00b7reich", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Zu der tauben Mitwelt Ohren", "tokens": ["Zu", "der", "tau\u00b7ben", "Mit\u00b7welt", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Im Hexameter nur spricht.", "tokens": ["Im", "He\u00b7xa\u00b7me\u00b7ter", "nur", "spricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Gelbe R\u00fcben, Ameiseier,", "tokens": ["Gel\u00b7be", "R\u00fc\u00b7ben", ",", "A\u00b7mei\u00b7sei\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "N\u00fc\u00dfekerne liebst du nur.", "tokens": ["N\u00fc\u00b7\u00df\u00b7e\u00b7ker\u00b7ne", "liebst", "du", "nur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Deine Speisen sind nicht theuer,", "tokens": ["Dei\u00b7ne", "Spei\u00b7sen", "sind", "nicht", "theu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dein Koch ist die Natur.", "tokens": ["Und", "dein", "Koch", "ist", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NE", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses kann Dermestes nicht.", "tokens": ["Die\u00b7ses", "kann", "Der\u00b7mes\u00b7tes", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PDS", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Speisen, die man deutsch kann nennen,", "tokens": ["Spei\u00b7sen", ",", "die", "man", "deutsch", "kann", "nen\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIS", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Welche nicht das Blut verbrennen,", "tokens": ["Wel\u00b7che", "nicht", "das", "Blut", "ver\u00b7bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nein! die sind nicht sein Gericht!", "tokens": ["Nein", "!", "die", "sind", "nicht", "sein", "Ge\u00b7richt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ART", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wenn dich bei beliebter Mu\u00dfe", "tokens": ["Wenn", "dich", "bei", "be\u00b7lieb\u00b7ter", "Mu\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sonnenstral und Bad erfreut,", "tokens": ["Son\u00b7nen\u00b7stral", "und", "Bad", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stehst du gern auf einem Fu\u00dfe,", "tokens": ["Stehst", "du", "gern", "auf", "ei\u00b7nem", "Fu\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und so stehst du lange Zeit.", "tokens": ["Und", "so", "stehst", "du", "lan\u00b7ge", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses kann Florindo nicht.", "tokens": ["Die\u00b7ses", "kann", "Flo\u00b7rin\u00b7do", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "NE", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sein Beruf ist h\u00fcpfen, flattern,", "tokens": ["Sein", "Be\u00b7ruf", "ist", "h\u00fcp\u00b7fen", ",", "flat\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVINF", "$,", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sein Verbeugen, trillern, schnattern.", "tokens": ["Sein", "Ver\u00b7beu\u00b7gen", ",", "tril\u00b7lern", ",", "schnat\u00b7tern", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gecken! nehmet Unterricht!", "tokens": ["Ge\u00b7cken", "!", "neh\u00b7met", "Un\u00b7ter\u00b7richt", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Dir ist nur ein Kleid beschieden;", "tokens": ["Dir", "ist", "nur", "ein", "Kleid", "be\u00b7schie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "J\u00e4hrlich legst du selbes ab,", "tokens": ["J\u00e4hr\u00b7lich", "legst", "du", "sel\u00b7bes", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADJA", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, 