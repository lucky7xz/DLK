{"dta.poem.8089": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Jungfrau Sieglinde .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Das war Jungfrau Sieglinde,", "tokens": ["Das", "war", "Jung\u00b7frau", "Sieg\u00b7lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die wollte fr\u00fch aufstehn,", "tokens": ["Die", "woll\u00b7te", "fr\u00fch", "auf\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit ihrem Hofgesinde", "tokens": ["Mit", "ih\u00b7rem", "Hof\u00b7ge\u00b7sin\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Frauenm\u00fcnster gehn.", "tokens": ["Zum", "Frau\u00b7en\u00b7m\u00fcns\u00b7ter", "gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie ging in Gold und Seide,", "tokens": ["Sie", "ging", "in", "Gold", "und", "Sei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit Blumen und Geschmeide,", "tokens": ["Mit", "Blu\u00b7men", "und", "Ge\u00b7schmei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das ward zu gro\u00dfem Leide.", "tokens": ["Das", "ward", "zu", "gro\u00b7\u00dfem", "Lei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es stehn drei Lindenb\u00e4ume", "tokens": ["Es", "stehn", "drei", "Lin\u00b7den\u00b7b\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl vor der Kirchenpfort\u2019;", "tokens": ["Wohl", "vor", "der", "Kir\u00b7chen\u00b7pfort'", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da sa\u00df der edle Heime,", "tokens": ["Da", "sa\u00df", "der", "ed\u00b7le", "Hei\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der sprach viel leise Wort\u2019:", "tokens": ["Der", "sprach", "viel", "lei\u00b7se", "Wort'", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201ewas Gold, was Edelsteine!", "tokens": ["\u201e", "was", "Gold", ",", "was", "E\u00b7del\u00b7stei\u00b7ne", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWS", "NN", "$,", "PWS", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "H\u00e4tt\u2019 ich der Blumen eine", "tokens": ["H\u00e4tt'", "ich", "der", "Blu\u00b7men", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Aus deinem Kranz, du Feine!\u201c", "tokens": ["Aus", "dei\u00b7nem", "Kranz", ",", "du", "Fei\u00b7ne", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPER", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So sprach der J\u00fcngling leise,", "tokens": ["So", "sprach", "der", "J\u00fcng\u00b7ling", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da trieb der Wind sein Spiel,", "tokens": ["Da", "trieb", "der", "Wind", "sein", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df aus der Blumen Kreise", "tokens": ["Da\u00df", "aus", "der", "Blu\u00b7men", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die sch\u00f6nste Rose fiel.", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Ro\u00b7se", "fiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Herr Heime th\u00e4t sich b\u00fccken,", "tokens": ["Herr", "Hei\u00b7me", "th\u00e4t", "sich", "b\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Rose wegzupfl\u00fccken,", "tokens": ["Die", "Ro\u00b7se", "weg\u00b7zu\u00b7pfl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Damit wollt\u2019 er sich schm\u00fccken.", "tokens": ["Da\u00b7mit", "wollt'", "er", "sich", "schm\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Da war ein alter Ritter", "tokens": ["Da", "war", "ein", "al\u00b7ter", "Rit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Siegelindens Chor,", "tokens": ["In", "Sie\u00b7ge\u00b7lin\u00b7dens", "Chor", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem war es leid und bitter,", "tokens": ["Dem", "war", "es", "leid", "und", "bit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gar zornig trat er vor:", "tokens": ["Gar", "zor\u00b7nig", "trat", "er", "vor", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201emu\u00df ich dich Hofzucht lehren?", "tokens": ["\u201e", "mu\u00df", "ich", "dich", "Hof\u00b7zucht", "leh\u00b7ren", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "PRF", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Darfst du vom Kranz der Ehren", "tokens": ["Darfst", "du", "vom", "Kranz", "der", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ein L\u00e4ublein nur begehren?\u201c", "tokens": ["Ein", "L\u00e4ub\u00b7lein", "nur", "be\u00b7geh\u00b7ren", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O weh dem Garten immer,", "tokens": ["O", "weh", "dem", "Gar\u00b7ten", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Der solche Rosen bracht\u2019!", "tokens": ["Der", "sol\u00b7che", "Ro\u00b7sen", "bracht'", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Heil den Linden nimmer,", "tokens": ["O", "Heil", "den", "Lin\u00b7den", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NE", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo solcher Streit erwacht!", "tokens": ["Wo", "sol\u00b7cher", "Streit", "er\u00b7wacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie klangen da die Degen,", "tokens": ["Wie", "klan\u00b7gen", "da", "die", "De\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bis unter wilden Schl\u00e4gen", "tokens": ["Bis", "un\u00b7ter", "wil\u00b7den", "Schl\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der J\u00fcngling todt erlegen!", "tokens": ["Der", "J\u00fcng\u00b7ling", "todt", "er\u00b7le\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sieglinde beugt\u2019 sich nieder", "tokens": ["Sieg\u00b7lin\u00b7de", "beugt'", "sich", "nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und nahm die Ros\u2019 empor,", "tokens": ["Und", "nahm", "die", "Ros'", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Steckt\u2019 in den Kranz sie wieder,", "tokens": ["Steckt'", "in", "den", "Kranz", "sie", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "ADV", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Und ging zur Kirche vor.", "tokens": ["Und", "ging", "zur", "Kir\u00b7che", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie ging in Gold und Seide,", "tokens": ["Sie", "ging", "in", "Gold", "und", "Sei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit Blumen und Geschmeide,", "tokens": ["Mit", "Blu\u00b7men", "und", "Ge\u00b7schmei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wer th\u00e4t ihr was zu Leide?", "tokens": ["Wer", "th\u00e4t", "ihr", "was", "zu", "Lei\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Vor Sankt Mariens Bilde", "tokens": ["Vor", "Sankt", "Ma\u00b7ri\u00b7ens", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "NE", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Nahm sie herab die Kron\u2019:", "tokens": ["Nahm", "sie", "her\u00b7ab", "die", "Kron'", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201enimm du sie, Reine, Milde!", "tokens": ["\u201e", "nimm", "du", "sie", ",", "Rei\u00b7ne", ",", "Mil\u00b7de", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "PPER", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein Bl\u00fcmlein kam davon.", "tokens": ["Kein", "Bl\u00fcm\u00b7lein", "kam", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Welt will ich entsagen,", "tokens": ["Der", "Welt", "will", "ich", "ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den heil\u2019gen Schleier tragen", "tokens": ["Den", "heil'\u00b7gen", "Schlei\u00b7er", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und um die Todten klagen.\u201c", "tokens": ["Und", "um", "die", "Tod\u00b7ten", "kla\u00b7gen", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}