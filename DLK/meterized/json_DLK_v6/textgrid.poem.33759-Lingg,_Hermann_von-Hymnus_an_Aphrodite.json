{"textgrid.poem.33759": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Hymnus an Aphrodite", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gaben dir und Opfer bringend,", "tokens": ["Ga\u00b7ben", "dir", "und", "Op\u00b7fer", "brin\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nahn den Stufen deines Throns,", "tokens": ["Nahn", "den", "Stu\u00b7fen", "dei\u00b7nes", "Throns", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hymnen deiner Sch\u00f6nheit singend,", "tokens": ["Hym\u00b7nen", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "sin\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fromme Frauen Sikyons.", "tokens": ["From\u00b7me", "Frau\u00b7en", "Si\u00b7ky\u00b7ons", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "Die du \u00fcber allem waltend,", "tokens": ["Die", "du", "\u00fc\u00b7ber", "al\u00b7lem", "wal\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PIS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles lenkst nach deinem Sinn,", "tokens": ["Al\u00b7les", "lenkst", "nach", "dei\u00b7nem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Throne, Venus, zepterhaltend", "tokens": ["Thro\u00b7ne", ",", "Ve\u00b7nus", ",", "zep\u00b7ter\u00b7hal\u00b7tend"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcber jeder K\u00f6nigin!", "tokens": ["\u00dc\u00b7ber", "je\u00b7der", "K\u00f6\u00b7ni\u00b7gin", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Im Gew\u00e4hren holder Klagen", "tokens": ["Im", "Ge\u00b7w\u00e4h\u00b7ren", "hol\u00b7der", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bist du einzig mild und gro\u00df,", "tokens": ["Bist", "du", "ein\u00b7zig", "mild", "und", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unerbittlich im Versagen,", "tokens": ["Un\u00b7er\u00b7bitt\u00b7lich", "im", "Ver\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo du fliehst, erbarmungslos!", "tokens": ["Wo", "du", "fliehst", ",", "er\u00b7bar\u00b7mungs\u00b7los", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Jeden Wunsch von dir zu stillen,", "tokens": ["Je\u00b7den", "Wunsch", "von", "dir", "zu", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Strengt sich Macht und Reichtum an;", "tokens": ["Strengt", "sich", "Macht", "und", "Reich\u00b7tum", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mut ergl\u00fcht um deinetwillen,", "tokens": ["Mut", "er\u00b7gl\u00fcht", "um", "dei\u00b7net\u00b7wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00e4rke wird dir untertan.", "tokens": ["St\u00e4r\u00b7ke", "wird", "dir", "un\u00b7ter\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wem den Apfel du beschieden,", "tokens": ["Wem", "den", "Ap\u00b7fel", "du", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den du tr\u00e4gst in deiner Hand,", "tokens": ["Den", "du", "tr\u00e4gst", "in", "dei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O der k\u00e4mpft nicht mehr hienieden", "tokens": ["O", "der", "k\u00e4mpft", "nicht", "mehr", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "VVFIN", "PTKNEG", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um ein h\u00f6h'res Siegespfand!", "tokens": ["Um", "ein", "h\u00f6h'\u00b7res", "Sie\u00b7ge\u00b7spfand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Alles f\u00fcrchtet Speer und Bogen,", "tokens": ["Al\u00b7les", "f\u00fcrch\u00b7tet", "Speer", "und", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwert und Pfeile, nur nicht du!", "tokens": ["Schwert", "und", "Pfei\u00b7le", ",", "nur", "nicht", "du", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,", "ADV", "PTKNEG", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Turmhoch w\u00e4lzt der Sturm die Wogen,", "tokens": ["Turm\u00b7hoch", "w\u00e4lzt", "der", "Sturm", "die", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du, du l\u00e4chelst nur dazu.", "tokens": ["Du", ",", "du", "l\u00e4\u00b7chelst", "nur", "da\u00b7zu", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "VVFIN", "ADV", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Mag Neptun das Land berauben,", "tokens": ["Mag", "Nep\u00b7tun", "das", "Land", "be\u00b7rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ART", "NN", "VVINF", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Typhon den Olymp bedrohn:", "tokens": ["Ty\u00b7phon", "den", "O\u00b7lymp", "be\u00b7drohn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dich umflattern deine Tauben,", "tokens": ["Dich", "um\u00b7flat\u00b7tern", "dei\u00b7ne", "Tau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich, o Cypris, sch\u00fctzt dein Sohn!", "tokens": ["Dich", ",", "o", "Cyp\u00b7ris", ",", "sch\u00fctzt", "dein", "Sohn", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "FM", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}