{"textgrid.poem.62280": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[die Englein im Himmel]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Englein im Himmel,", "tokens": ["Die", "En\u00b7glein", "im", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sie singen ein Lied,", "tokens": ["Sie", "sin\u00b7gen", "ein", "Lied", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Nur Rafaels K\u00f6pfchen", "tokens": ["Nur", "Ra\u00b7faels", "K\u00f6pf\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ruft zornig ergl\u00fcht:", "tokens": ["Ruft", "zor\u00b7nig", "er\u00b7gl\u00fcht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Wie k\u00f6nnte ich singen", "tokens": ["Wie", "k\u00f6nn\u00b7te", "ich", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ein fr\u00f6hliches Lied,", "tokens": ["Ein", "fr\u00f6h\u00b7li\u00b7ches", "Lied", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wenn Ha\u00df statt der Liebe", "tokens": ["Wenn", "Ha\u00df", "statt", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Erde umzieht?", "tokens": ["Die", "Er\u00b7de", "um\u00b7zieht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Und auf, da\u00df ihr's wi\u00dft,", "tokens": ["Und", "auf", ",", "da\u00df", "ih\u00b7r's", "wi\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Satan es ist,", "tokens": ["Der", "Sa\u00b7tan", "es", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Im Munde den Zwist \u2013", "tokens": ["Im", "Mun\u00b7de", "den", "Zwist", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Er macht einen Ritt", "tokens": ["Er", "macht", "ei\u00b7nen", "Ritt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Als Antisemit", "tokens": ["Als", "An\u00b7ti\u00b7se\u00b7mit"], "token_info": ["word", "word"], "pos": ["KOUS", "NE"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Die L\u00fcge geht mit!", "tokens": ["Die", "L\u00fc\u00b7ge", "geht", "mit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}