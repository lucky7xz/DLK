{"textgrid.poem.26413": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Venusine in Rom, auf dem Monte Pincio zum", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Monte Pincios Garten", "tokens": ["Mon\u00b7te", "Pin\u00b7cios", "Gar\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NE", "NE", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Knirscht auf allen Wegen,", "tokens": ["Knirscht", "auf", "al\u00b7len", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn zur Korsostunde", "tokens": ["Wenn", "zur", "Kor\u00b7sos\u00b7tun\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Sich die Wagen regen.", "tokens": ["Sich", "die", "Wa\u00b7gen", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Alle Staatskarossen", "tokens": ["Al\u00b7le", "Staats\u00b7ka\u00b7ros\u00b7sen"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Tragen R\u00f6merinnen", "tokens": ["Tra\u00b7gen", "R\u00f6\u00b7me\u00b7rin\u00b7nen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wie aus Gold gegossen.", "tokens": ["Wie", "aus", "Gold", "ge\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Auf dem Petersdome", "tokens": ["Auf", "dem", "Pe\u00b7ters\u00b7do\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Lagen Abendfeuer.", "tokens": ["La\u00b7gen", "A\u00b7bend\u00b7feu\u00b7er", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Abendrosen hingen", "tokens": ["A\u00b7ben\u00b7dro\u00b7sen", "hin\u00b7gen"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "\u00dcber Roms Gem\u00e4uer.", "tokens": ["\u00dc\u00b7ber", "Roms", "Ge\u00b7m\u00e4u\u00b7er", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Venus nahte l\u00e4chelnd,", "tokens": ["Ve\u00b7nus", "nah\u00b7te", "l\u00e4\u00b7chelnd", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Bettlerinnen \u00e4hnlich", "tokens": ["Bett\u00b7le\u00b7rin\u00b7nen", "\u00e4hn\u00b7lich"], "token_info": ["word", "word"], "pos": ["NN", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mit der Sch\u00fcrze f\u00e4chelnd.", "tokens": ["Mit", "der", "Sch\u00fcr\u00b7ze", "f\u00e4\u00b7chelnd", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Stieg zu Pincio's Garten", "tokens": ["Stieg", "zu", "Pin\u00b7cio's", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NE", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Einsam ohne Wagen,", "tokens": ["Ein\u00b7sam", "oh\u00b7ne", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wollte allem Prunke", "tokens": ["Woll\u00b7te", "al\u00b7lem", "Prun\u00b7ke"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PIS", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Einfach heut entsagen.", "tokens": ["Ein\u00b7fach", "heut", "ent\u00b7sa\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Geht im Bauernkleide", "tokens": ["Geht", "im", "Bau\u00b7ern\u00b7klei\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Zwischen den Karossen,", "tokens": ["Zwi\u00b7schen", "den", "Ka\u00b7ros\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wie ein Weib der Heide.", "tokens": ["Wie", "ein", "Weib", "der", "Hei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Palmen und die Blumen,", "tokens": ["Pal\u00b7men", "und", "die", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sie sogleich erkennen,", "tokens": ["Sie", "sog\u00b7leich", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "M\u00f6chten mit Ger\u00fcchen", "tokens": ["M\u00f6ch\u00b7ten", "mit", "Ge\u00b7r\u00fc\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ihren Namen nennen.", "tokens": ["Ih\u00b7ren", "Na\u00b7men", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Venus l\u00e4chelt eigen,", "tokens": ["Ve\u00b7nus", "l\u00e4\u00b7chelt", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und die scheuen Blumen", "tokens": ["Und", "die", "scheu\u00b7en", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schlie\u00dfen sich und schweigen.", "tokens": ["Schlie\u00b7\u00dfen", "sich", "und", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KON", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Auch die Instrumente", "tokens": ["Auch", "die", "Inst\u00b7ru\u00b7men\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Der Musikkapelle", "tokens": ["Der", "Mu\u00b7sik\u00b7ka\u00b7pel\u00b7le"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.3": {"text": "F\u00fchlen Venusine,", "tokens": ["F\u00fch\u00b7len", "Ve\u00b7nu\u00b7si\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$,"], "meter": "+-+---", "measure": "unknown.measure.di"}, "line.4": {"text": "Werden sanft zur Quelle", "tokens": ["Wer\u00b7den", "sanft", "zur", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Hei\u00dfer Harmonien,", "tokens": ["Hei\u00b7\u00dfer", "Har\u00b7mo\u00b7ni\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die nie Noten finden", "tokens": ["Die", "nie", "No\u00b7ten", "fin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und das Blut durchziehen.", "tokens": ["Und", "das", "Blut", "durch\u00b7zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "In dem Volksgedr\u00e4nge", "tokens": ["In", "dem", "Volks\u00b7ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Zwischen Marmorb\u00e4nken", "tokens": ["Zwi\u00b7schen", "Mar\u00b7mor\u00b7b\u00e4n\u00b7ken"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Steht da jung ein J\u00fcngling,", "tokens": ["Steht", "da", "jung", "ein", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Den noch Menschen kr\u00e4nken,", "tokens": ["Den", "noch", "Men\u00b7schen", "kr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "Lehnt da leidversunken", "tokens": ["Lehnt", "da", "leid\u00b7ver\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Am Zypressenbaume,", "tokens": ["Am", "Zyp\u00b7res\u00b7sen\u00b7bau\u00b7me", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "F\u00fchlt sich weltalltrunken.", "tokens": ["F\u00fchlt", "sich", "welt\u00b7all\u00b7trun\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.13": {"line.1": {"text": "Venus riecht das Schwitzen", "tokens": ["Ve\u00b7nus", "riecht", "das", "Schwit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Dieses Enthusiasten,", "tokens": ["Die\u00b7ses", "En\u00b7thu\u00b7si\u00b7as\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Sieht den grauen Kragen", "tokens": ["Sieht", "den", "grau\u00b7en", "Kra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Unterm Sch\u00e4delkasten,", "tokens": ["Un\u00b7term", "Sch\u00e4\u00b7del\u00b7kas\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.14": {"line.1": {"text": "F\u00fchlt auch Todesschauer,", "tokens": ["F\u00fchlt", "auch", "To\u00b7des\u00b7schau\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Als er zu ihr sagte:", "tokens": ["Als", "er", "zu", "ihr", "sag\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "\u00bbich bin ein \u2013 Bildhauer.", "tokens": ["\u00bb", "ich", "bin", "ein", "\u2013", "Bild\u00b7hau\u00b7er", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "$(", "NN", "$."], "meter": "-+-++-", "measure": "unknown.measure.tri"}}, "stanza.15": {"line.1": {"text": "Du mu\u00dft mit mir gehen!", "tokens": ["Du", "mu\u00dft", "mit", "mir", "ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Will dich nicht ber\u00fchren,", "tokens": ["Will", "dich", "nicht", "be\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nur die N\u00e4he meines", "tokens": ["Nur", "die", "N\u00e4\u00b7he", "mei\u00b7nes"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PPOSAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ideales sp\u00fcren.", "tokens": ["I\u00b7dea\u00b7les", "sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.16": {"line.1": {"text": "Hab Dich gleich empfunden \u2013", "tokens": ["Hab", "Dich", "gleich", "emp\u00b7fun\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "VVPP", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Zahl' f\u00fcr Aktmodelle", "tokens": ["Zahl'", "f\u00fcr", "Akt\u00b7mo\u00b7del\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Lire drei die Stunden.\u00ab", "tokens": ["Li\u00b7re", "drei", "die", "Stun\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "CARD", "ART", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.17": {"line.1": {"text": "Venus unumwunden", "tokens": ["Ve\u00b7nus", "un\u00b7um\u00b7wun\u00b7den"], "token_info": ["word", "word"], "pos": ["NE", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sagt's heraus dem Knaben:", "tokens": ["Sagt's", "he\u00b7raus", "dem", "Kna\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APZR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "F\u00fcr Anschauungsstunden", "tokens": ["F\u00fcr", "An\u00b7schau\u00b7ungs\u00b7stun\u00b7den"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Sei sie nicht zu haben.", "tokens": ["Sei", "sie", "nicht", "zu", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PTKZU", "VAINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.18": {"line.1": {"text": "Wer nur in den Augen,", "tokens": ["Wer", "nur", "in", "den", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sonst nicht hei\u00dfer werde,", "tokens": ["Sonst", "nicht", "hei\u00b7\u00dfer", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "K\u00f6nnte ihr nicht taugen.", "tokens": ["K\u00f6nn\u00b7te", "ihr", "nicht", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.19": {"line.1": {"text": "Doch der J\u00fcngling altklug", "tokens": ["Doch", "der", "J\u00fcng\u00b7ling", "alt\u00b7klug"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Sagt: \u00bbIhr feile Holde,", "tokens": ["Sagt", ":", "\u00bb", "Ihr", "fei\u00b7le", "Hol\u00b7de", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "$(", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schielt nur nach den Reichen!", "tokens": ["Schielt", "nur", "nach", "den", "Rei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ungl\u00fcck h\u00e4ngt am Golde.", "tokens": ["Un\u00b7gl\u00fcck", "h\u00e4ngt", "am", "Gol\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.20": {"line.1": {"text": "Gold sollt man begraben,", "tokens": ["Gold", "sollt", "man", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Weil, ach! die Dukaten", "tokens": ["Weil", ",", "ach", "!", "die", "Du\u00b7ka\u00b7ten"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "$,", "ITJ", "$.", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Keine Seele haben.", "tokens": ["Kei\u00b7ne", "See\u00b7le", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.21": {"line.1": {"text": "Reichtum ist Langweile,", "tokens": ["Reich\u00b7tum", "ist", "Lang\u00b7wei\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "K\u00f6nnt mir p\u00fcnktlich glauben!", "tokens": ["K\u00f6nnt", "mir", "p\u00fcnkt\u00b7lich", "glau\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wollte Venus allen", "tokens": ["Woll\u00b7te", "Ve\u00b7nus", "al\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "NN", "PIAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Reichen hier erlauben,", "tokens": ["Rei\u00b7chen", "hier", "er\u00b7lau\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.22": {"line.1": {"text": "Da\u00df mal Alle sollten,", "tokens": ["Da\u00df", "mal", "Al\u00b7le", "soll\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "VMFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die zu Wagen kommen,", "tokens": ["Die", "zu", "Wa\u00b7gen", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Tuen, was sie wollten, \u2013", "tokens": ["Tu\u00b7en", ",", "was", "sie", "woll\u00b7ten", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VMFIN", "$,", "$("], "meter": "+---+-", "measure": "dactylic.init"}}, "stanza.23": {"line.1": {"text": "Ach, wie wenig W\u00fcnsche", "tokens": ["Ach", ",", "wie", "we\u00b7nig", "W\u