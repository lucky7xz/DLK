{"textgrid.poem.35826": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbstaub bist du und kehrst zum Staube,", "genre": "verse", "period": "N.A.", "pub_year": 1820, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbstaub bist du und kehrst zum Staube,", "tokens": ["\u00bb", "staub", "bist", "du", "und", "kehrst", "zum", "Stau\u00b7be", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PPER", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denk', o Mensch, an deinen Tod!\u00ab", "tokens": ["Denk'", ",", "o", "Mensch", ",", "an", "dei\u00b7nen", "Tod", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "$,", "FM", "NN", "$,", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wohl, dies wei\u00df ich, doch mein Glaube", "tokens": ["Wohl", ",", "dies", "wei\u00df", "ich", ",", "doch", "mein", "Glau\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PDS", "VVFIN", "PPER", "$,", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieht ein ew'ges Morgenroth.", "tokens": ["Sieht", "ein", "ew'\u00b7ges", "Mor\u00b7gen\u00b7roth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sieht ein Land, wo Friedenspalmen", "tokens": ["Sieht", "ein", "Land", ",", "wo", "Frie\u00b7dens\u00b7pal\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PWAV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um des Siegers Scheitel wehn,", "tokens": ["Um", "des", "Sie\u00b7gers", "Schei\u00b7tel", "wehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo umrauscht von ihren Psalmen", "tokens": ["Wo", "um\u00b7rauscht", "von", "ih\u00b7ren", "Psal\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wir der Engel Ch\u00f6re sehn.", "tokens": ["Wir", "der", "En\u00b7gel", "Ch\u00f6\u00b7re", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo Maria, die Getreue,", "tokens": ["Wo", "Ma\u00b7ria", ",", "die", "Ge\u00b7treu\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr geliebtes Kind uns zeigt,", "tokens": ["Ihr", "ge\u00b7lieb\u00b7tes", "Kind", "uns", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die Sehnsucht und die Reue", "tokens": ["Wo", "die", "Sehn\u00b7sucht", "und", "die", "Reu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun ihr selig Ziel erreicht.", "tokens": ["Nun", "ihr", "se\u00b7lig", "Ziel", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJD", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wo der Vater mit dem Sohne", "tokens": ["Wo", "der", "Va\u00b7ter", "mit", "dem", "Soh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und dem heil'gen Geist zugleich", "tokens": ["Und", "dem", "heil'\u00b7gen", "Geist", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Thront auf ", "tokens": ["Thront", "auf"], "token_info": ["word", "word"], "pos": ["NN", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Unaussprechlich herrlich, reich.", "tokens": ["Un\u00b7aus\u00b7sprech\u00b7lich", "herr\u00b7lich", ",", "reich", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wo wir Den, der je gewesen,", "tokens": ["Wo", "wir", "Den", ",", "der", "je", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NE", "$,", "PRELS", "ADV", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schauen, wie Er ewig war.", "tokens": ["Schau\u00b7en", ",", "wie", "Er", "e\u00b7wig", "war", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O, dort wird mein Herz genesen!", "tokens": ["O", ",", "dort", "wird", "mein", "Herz", "ge\u00b7ne\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O, dort wird mein Auge klar!", "tokens": ["O", ",", "dort", "wird", "mein", "Au\u00b7ge", "klar", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wo verkl\u00e4rte Seelen streben", "tokens": ["Wo", "ver\u00b7kl\u00e4r\u00b7te", "See\u00b7len", "stre\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihn, den Einz'gen, zu erh\u00f6hn,", "tokens": ["Ihn", ",", "den", "Einz'\u00b7gen", ",", "zu", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die sel'gen M\u00e4rt'rer schweben,", "tokens": ["Wo", "die", "sel'\u00b7gen", "M\u00e4rt'\u00b7rer", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die reinen Jungfrau'n stehn.", "tokens": ["Wo", "die", "rei\u00b7nen", "Jung\u00b7frau'n", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wo die zarte Magdalene", "tokens": ["Wo", "die", "zar\u00b7te", "Mag\u00b7da\u00b7le\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Selig Ihm zu F\u00fc\u00dfen liegt,", "tokens": ["Se\u00b7lig", "Ihm", "zu", "F\u00fc\u00b7\u00dfen", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da der Reue bittre Thr\u00e4ne", "tokens": ["Da", "der", "Reu\u00b7e", "bitt\u00b7re", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Ihr in Wonne l\u00e4ngst versiegt.", "tokens": ["Ihr", "in", "Won\u00b7ne", "l\u00e4ngst", "ver\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wo Johannes, der Geliebte,", "tokens": ["Wo", "Jo\u00b7han\u00b7nes", ",", "der", "Ge\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Innig Ihm am Herzen ruht,", "tokens": ["In\u00b7nig", "Ihm", "am", "Her\u00b7zen", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles Kranke, einst Betr\u00fcbte", "tokens": ["Al\u00b7les", "Kran\u00b7ke", ",", "einst", "Be\u00b7tr\u00fcb\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ruht in Seines Schoo\u00dfes Hut.", "tokens": ["Ruht", "in", "Sei\u00b7nes", "Schoo\u00b7\u00dfes", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wo die heil'gen Schaaren wandeln,", "tokens": ["Wo", "die", "heil'\u00b7gen", "Schaa\u00b7ren", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die das Kreuz den Weg gelehrt,", "tokens": ["Die", "das", "Kreuz", "den", "Weg", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die im Lieben, Dulden, Handeln", "tokens": ["Die", "im", "Lie\u00b7ben", ",", "Dul\u00b7den", ",", "Han\u00b7deln"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "APPRART", "ADJA", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier Sein Bild in sich verkl\u00e4rt. \u2013", "tokens": ["Hier", "Sein", "Bild", "in", "sich", "ver\u00b7kl\u00e4rt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "PRF", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Wohl mir, da\u00df er Staub einst werde,", "tokens": ["Wohl", "mir", ",", "da\u00df", "er", "Staub", "einst", "wer\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "KOUS", "PPER", "NN", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser Leib von Erd' und Staub!", "tokens": ["Die\u00b7ser", "Leib", "von", "Erd'", "und", "Staub", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Seele wird der Erde,", "tokens": ["Mei\u00b7ne", "See\u00b7le", "wird", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird dem Wurme nicht zum Raub.", "tokens": ["Wird", "dem", "Wur\u00b7me", "nicht", "zum", "Raub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Hauch ist sie aus Gottes Munde,", "tokens": ["Hauch", "ist", "sie", "aus", "Got\u00b7tes", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sie kehrt hinauf zum Licht.", "tokens": ["Und", "sie", "kehrt", "hin\u00b7auf", "zum", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sei gesegnet, ernste Stunde,", "tokens": ["Sei", "ge\u00b7seg\u00b7net", ",", "erns\u00b7te", "Stun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die einst meine Fessel bricht!", "tokens": ["Die", "einst", "mei\u00b7ne", "Fes\u00b7sel", "bricht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Sei gesegnet, stiller H\u00fcgel,", "tokens": ["Sei", "ge\u00b7seg\u00b7net", ",", "stil\u00b7ler", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der einst meine Asche deckt,", "tokens": ["Der", "einst", "mei\u00b7ne", "A\u00b7sche", "deckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis das Weh'n der Seraphsfl\u00fcgel", "tokens": ["Bis", "das", "Weh'n", "der", "Se\u00b7raphs\u00b7fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich vom langen Schlaf erweckt!", "tokens": ["Mich", "vom", "lan\u00b7gen", "Schlaf", "er\u00b7weckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "\u00bbdenn du Staub, du kehrst zum Staube", "tokens": ["\u00bb", "denn", "du", "Staub", ",", "du", "kehrst", "zum", "Stau\u00b7be"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "KON", "PPER", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis zum neuen Morgenroth.\u00ab", "tokens": ["Bis", "zum", "neu\u00b7en", "Mor\u00b7gen\u00b7roth", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, ich wei\u00df es, doch mein Glaube", "tokens": ["Ja", ",", "ich", "wei\u00df", "es", ",", "doch", "mein", "Glau\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "$,", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hebt mich \u00fcber Grab und Tod.", "tokens": ["Hebt", "mich", "\u00fc\u00b7ber", "Grab", "und", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}