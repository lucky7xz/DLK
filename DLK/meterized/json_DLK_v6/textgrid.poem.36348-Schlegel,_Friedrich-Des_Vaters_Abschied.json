{"textgrid.poem.36348": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Des Vaters Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Faust ist schwach, das Haar ist grau,", "tokens": ["Die", "Faust", "ist", "schwach", ",", "das", "Haar", "ist", "grau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich mag kein Ro\u00df mehr lenken;", "tokens": ["Ich", "mag", "kein", "Ro\u00df", "mehr", "len\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Herz, ich f\u00fchl' es, Sohn! genau,", "tokens": ["Das", "Herz", ",", "ich", "f\u00fchl'", "es", ",", "Sohn", "!", "ge\u00b7nau", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PPER", "$,", "NN", "$.", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Will sich zur Grube senken.", "tokens": ["Will", "sich", "zur", "Gru\u00b7be", "sen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nimm hier den Panzer, nimm das Schwert,", "tokens": ["Nimm", "hier", "den", "Pan\u00b7zer", ",", "nimm", "das", "Schwert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$,", "VVIMP", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zieh hin und sei des Vaters wert!", "tokens": ["Zieh", "hin", "und", "sei", "des", "Va\u00b7ters", "wert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Grausam h\u00e4lt jetzt der Feind den Sieg,", "tokens": ["Grau\u00b7sam", "h\u00e4lt", "jetzt", "der", "Feind", "den", "Sieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Die Helden sind gestorben;", "tokens": ["Die", "Hel\u00b7den", "sind", "ge\u00b7stor\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schon drei\u00dfig Jahre tobt der Krieg,", "tokens": ["Schon", "drei\u00b7\u00dfig", "Jah\u00b7re", "tobt", "der", "Krieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und noch kein Recht erworben!", "tokens": ["Und", "noch", "kein", "Recht", "er\u00b7wor\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vielleicht dem kommenden Geschlecht", "tokens": ["Viel\u00b7leicht", "dem", "kom\u00b7men\u00b7den", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gr\u00fcnt einmal wieder Fried' und Recht.", "tokens": ["Gr\u00fcnt", "ein\u00b7mal", "wie\u00b7der", "Fried'", "und", "Recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}