{"textgrid.poem.46674": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Unzertrennliches, ach vom Tod getrenntes", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unzertrennliches, ach vom Tod getrenntes,", "tokens": ["Un\u00b7zer\u00b7trenn\u00b7li\u00b7ches", ",", "ach", "vom", "Tod", "ge\u00b7trenn\u00b7tes", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "XY", "APPRART", "NN", "ADJA", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Zartverschwistertes, durch Geburt und Neigung", "tokens": ["Zart\u00b7ver\u00b7schwis\u00b7ter\u00b7tes", ",", "durch", "Ge\u00b7burt", "und", "Nei\u00b7gung"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "NN", "KON", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "F\u00fcr einander geschaffnes Vogelp\u00e4rchen!", "tokens": ["F\u00fcr", "ein\u00b7an\u00b7der", "ge\u00b7schaff\u00b7nes", "Vo\u00b7gel\u00b7p\u00e4r\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADJA", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "O du Br\u00fcderchen, an der Schwester hangend,", "tokens": ["O", "du", "Br\u00fc\u00b7der\u00b7chen", ",", "an", "der", "Schwes\u00b7ter", "han\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "O du Schwesterchen, nicht vom Bruder lassend!", "tokens": ["O", "du", "Schwes\u00b7ter\u00b7chen", ",", "nicht", "vom", "Bru\u00b7der", "las\u00b7send", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$,", "PTKNEG", "APPRART", "NN", "VVPP", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "In dem sonnigen K\u00e4fig, eurem St\u00fcbchen,", "tokens": ["In", "dem", "son\u00b7ni\u00b7gen", "K\u00e4\u00b7fig", ",", "eu\u00b7rem", "St\u00fcb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Auf dem St\u00e4ngelchen, eurem Stuhle, sitzend,", "tokens": ["Auf", "dem", "St\u00e4n\u00b7ge\u00b7lchen", ",", "eu\u00b7rem", "Stuh\u00b7le", ",", "sit\u00b7zend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "VVPP", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Immer nebeneinander, aneinander.", "tokens": ["Im\u00b7mer", "ne\u00b7ben\u00b7ein\u00b7an\u00b7der", ",", "an\u00b7ein\u00b7an\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Sa\u00df das Br\u00fcderchen auf der rechten Seite,", "tokens": ["Sa\u00df", "das", "Br\u00fc\u00b7der\u00b7chen", "auf", "der", "rech\u00b7ten", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Und das Schwesterchen links, so lie\u00df sein K\u00f6pfchen", "tokens": ["Und", "das", "Schwes\u00b7ter\u00b7chen", "links", ",", "so", "lie\u00df", "sein", "K\u00f6pf\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "$,", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "Links das Br\u00fcderchen hangen, und sein K\u00f6pfchen", "tokens": ["Links", "das", "Br\u00fc\u00b7der\u00b7chen", "han\u00b7gen", ",", "und", "sein", "K\u00f6pf\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVINF", "$,", "KON", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Rechts das Schwesterchen, also da\u00df die beiden", "tokens": ["Rechts", "das", "Schwes\u00b7ter\u00b7chen", ",", "al\u00b7so", "da\u00df", "die", "bei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "KOUS", "ART", "PIAT"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "K\u00f6pfchen leise sich ohne Druck ber\u00fchrten.", "tokens": ["K\u00f6pf\u00b7chen", "lei\u00b7se", "sich", "oh\u00b7ne", "Druck", "be\u00b7r\u00fchr\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Sa\u00df das Br\u00fcderchen aber auf der linken,", "tokens": ["Sa\u00df", "das", "Br\u00fc\u00b7der\u00b7chen", "a\u00b7ber", "auf", "der", "lin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPR", "ART", "ADJA", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "Und das Schwesterchen auf der rechten Seite,", "tokens": ["Und", "das", "Schwes\u00b7ter\u00b7chen", "auf", "der", "rech\u00b7ten", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.16": {"text": "Denn sie pflegten in diesem St\u00fcck zu wechseln,", "tokens": ["Denn", "sie", "pfleg\u00b7ten", "in", "die\u00b7sem", "St\u00fcck", "zu", "wech\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PDAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.17": {"text": "War das H\u00e4ngen der K\u00f6pfchen auch gewechselt,", "tokens": ["War", "das", "H\u00e4n\u00b7gen", "der", "K\u00f6pf\u00b7chen", "auch", "ge\u00b7wech\u00b7selt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.18": {"text": "Da\u00df sie gegeneinander wieder neigten.", "tokens": ["Da\u00df", "sie", "ge\u00b7gen\u00b7ein\u00b7an\u00b7der", "wie\u00b7der", "neig\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "So war euere Neigung gegenseitig,", "tokens": ["So", "war", "eu\u00b7e\u00b7re", "Nei\u00b7gung", "ge\u00b7gen\u00b7sei\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "So war euere Liebe wechselwirkend,", "tokens": ["So", "war", "eu\u00b7e\u00b7re", "Lie\u00b7be", "wech\u00b7sel\u00b7wir\u00b7kend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Ohne Wechsel als den der \u00e4u\u00dfern Stellung.", "tokens": ["Oh\u00b7ne", "Wech\u00b7sel", "als", "den", "der", "\u00e4u\u00b7\u00dfern", "Stel\u00b7lung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KOKOM", "ART", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.22": {"text": "Was ihr a\u00dfet, das a\u00dfet ihr zusammen,", "tokens": ["Was", "ihr", "a\u00b7\u00dfet", ",", "das", "a\u00b7\u00dfet", "ihr", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.23": {"text": "Und ihr tranket zusammen, was ihr tranket;", "tokens": ["Und", "ihr", "tran\u00b7ket", "zu\u00b7sam\u00b7men", ",", "was", "ihr", "tran\u00b7ket", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.24": {"text": "Was ihr spieltet und scherztet, sangt und spranget,", "tokens": ["Was", "ihr", "spiel\u00b7tet", "und", "scherz\u00b7tet", ",", "sangt", "und", "spran\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.25": {"text": "Was ihr lebtet, das lebtet ihr zusammen.", "tokens": ["Was", "ihr", "leb\u00b7tet", ",", "das", "leb\u00b7tet", "ihr", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.26": {"text": "Und so seid ihr zusammen auch gestorben.", "tokens": ["Und", "so", "seid", "ihr", "zu\u00b7sam\u00b7men", "auch", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.27": {"text": "Als das Schwesterchen mit zerdr\u00fcckter Brust lag,", "tokens": ["Als", "das", "Schwes\u00b7ter\u00b7chen", "mit", "zer\u00b7dr\u00fcck\u00b7ter", "Brust", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.28": {"text": "Eingedr\u00fcckt von des Geiers ehrnen Krallen,", "tokens": ["Ein\u00b7ge\u00b7dr\u00fcckt", "von", "des", "Gei\u00b7ers", "ehr\u00b7nen", "Kral\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.29": {"text": "Jenes Geiers, vor dem den Lebensvogel", "tokens": ["Je\u00b7nes", "Gei\u00b7ers", ",", "vor", "dem", "den", "Le\u00b7bens\u00b7vo\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "APPR", "ART", "ART", "NN"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.30": {"text": "Kein Goldk\u00e4fig der Liebe kann besch\u00fctzen;", "tokens": ["Kein", "Gold\u00b7k\u00e4\u00b7fig", "der", "Lie\u00b7be", "kann", "be\u00b7sch\u00fct\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Lie\u00df das Br\u00fcderchen auch das K\u00f6pfchen hangen,", "tokens": ["Lie\u00df", "das", "Br\u00fc\u00b7der\u00b7chen", "auch", "das", "K\u00f6pf\u00b7chen", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.32": {"text": "Nicht zur linken und nicht zur rechten Seite,", "tokens": ["Nicht", "zur", "lin\u00b7ken", "und", "nicht", "zur", "rech\u00b7ten", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "ADJA", "KON", "PTKNEG", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.33": {"text": "Sondern grad' auf die Brust, und hob's nicht wieder.", "tokens": ["Son\u00b7dern", "grad'", "auf", "die", "Brust", ",", "und", "hob's", "nicht", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$,", "KON", "NE", "PTKNEG", "ADV", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.34": {"text": "Und nun la\u00dft uns zusammen sie begraben,", "tokens": ["Und", "nun", "la\u00dft", "uns", "zu\u00b7sam\u00b7men", "sie", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Unter Thr\u00e4nen, am Fu\u00df des Lebensbaumes,", "tokens": ["Un\u00b7ter", "Thr\u00e4\u00b7nen", ",", "am", "Fu\u00df", "des", "Le\u00b7bens\u00b7bau\u00b7mes", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.36": {"text": "Wo wir klagen um sie im dunkeln Schatten.", "tokens": ["Wo", "wir", "kla\u00b7gen", "um", "sie", "im", "dun\u00b7keln", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.37": {"text": "Doch sie selber entschwingen sich gefl\u00fcgelt", "tokens": ["Doch", "sie", "sel\u00b7ber", "ent\u00b7schwin\u00b7gen", "sich", "ge\u00b7fl\u00fc\u00b7gelt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "VVFIN", "PRF", "VVPP"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.38": {"text": "Auf die sonnigen Wipfel hin und singen,", "tokens": ["Auf", "die", "son\u00b7ni\u00b7gen", "Wip\u00b7fel", "hin", "und", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.39": {"text": "Goldfrucht pickend und Nektarkelche nippend,", "tokens": ["Gold\u00b7frucht", "pi\u00b7ckend", "und", "Nek\u00b7tar\u00b7kel\u00b7che", "nip\u00b7pend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "NN", "VVPP", "$,"], "meter": "+-+-++-+--+", "measure": "iambic.hexa.chol"}, "line.40": {"text": "Auf den Zweigen des Lebens schaukelnd, eines", "tokens": ["Auf", "den", "Zwei\u00b7gen", "des", "Le\u00b7bens", "schau\u00b7kelnd", ",", "ei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVPP", "$,", "ART"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.41": {"text": "N\u00e4chst am andern, wie einst auf ihrem St\u00e4ngchen;", "tokens": ["N\u00e4chst", "am", "an\u00b7dern", ",", "wie", "einst", "auf", "ih\u00b7rem", "St\u00e4ng\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "$,", "PWAV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.42": {"text": "Hangen lassen sie nur nicht mehr die K\u00f6pfchen,", "tokens": ["Han\u00b7gen", "las\u00b7sen", "sie", "nur", "nicht", "mehr", "die", "K\u00f6pf\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.43": {"text": "Sondern heben sie freudig gen einander.", "tokens": ["Son\u00b7dern", "he\u00b7ben", "sie", "freu\u00b7dig", "gen", "ein\u00b7an\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "PRF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.44": {"text": "Wohl gefall' es dir dort, und wohl ergeh dir's,", "tokens": ["Wohl", "ge\u00b7fall'", "es", "dir", "dort", ",", "und", "wohl", "er\u00b7geh", "dir's", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "$,", "KON", "ADV", "VVFIN", "PIS", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.45": {"text": "Im Gen\u00fcgen der Liebe, nicht vermissend", "tokens": ["Im", "Ge\u00b7n\u00fc\u00b7gen", "der", "Lie\u00b7be", ",", "nicht", "ver\u00b7mis\u00b7send"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PTKNEG", "VVPP"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.46": {"text": "Die dich Missenden hier, mein Vogelp\u00e4rchen,", "tokens": ["Die", "dich", "Mis\u00b7sen\u00b7den", "hier", ",", "mein", "Vo\u00b7gel\u00b7p\u00e4r\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos"