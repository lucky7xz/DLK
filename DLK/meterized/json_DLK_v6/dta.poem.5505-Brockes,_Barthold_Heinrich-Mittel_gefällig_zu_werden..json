{"dta.poem.5505": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mittel gef\u00e4llig zu werden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie viel ein sch\u00f6n Gesicht vermag; und was in wol-\nformirten Z\u00fcgen,", "tokens": ["Wie", "viel", "ein", "sch\u00f6n", "Ge\u00b7sicht", "ver\u00b7mag", ";", "und", "was", "in", "wol", "for\u00b7mir\u00b7ten", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJD", "NN", "VVFIN", "$.", "KON", "PWS", "APPR", "TRUNC", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Nicht f\u00fcr geheime Kr\u00e4fte stecken, wie sie, den Seelen selbst", "tokens": ["Nicht", "f\u00fcr", "ge\u00b7hei\u00b7me", "Kr\u00e4f\u00b7te", "ste\u00b7cken", ",", "wie", "sie", ",", "den", "See\u00b7len", "selbst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$,", "PWAV", "PPER", "$,", "ART", "NN", "ADV"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Und Gunst und Neigung zu erwecken,", "tokens": ["Und", "Gunst", "und", "Nei\u00b7gung", "zu", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geschickt und f\u00e4hig sind; ist klar:", "tokens": ["Ge\u00b7schickt", "und", "f\u00e4\u00b7hig", "sind", ";", "ist", "klar", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "VAFIN", "$.", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun aber ist auch dieses wahr,", "tokens": ["Nun", "a\u00b7ber", "ist", "auch", "die\u00b7ses", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ADV", "PDAT", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df unsre Seelen zu besiegen,", "tokens": ["Da\u00df", "uns\u00b7re", "See\u00b7len", "zu", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "In der beliebten Freundlichkeit", "tokens": ["In", "der", "be\u00b7lieb\u00b7ten", "Freund\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die s\u00fcss- und st\u00e4rcksten Kr\u00e4fte liegen.", "tokens": ["Die", "s\u00fcss", "und", "st\u00e4rcks\u00b7ten", "Kr\u00e4f\u00b7te", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Derselben Sitz nun sind die Augen,", "tokens": ["Der\u00b7sel\u00b7ben", "Sitz", "nun", "sind", "die", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch unser Mund; drey rege Glieder, von denen wir ihr", "tokens": ["Auch", "un\u00b7ser", "Mund", ";", "drey", "re\u00b7ge", "Glie\u00b7der", ",", "von", "de\u00b7nen", "wir", "ihr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "$.", "CARD", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "PPOSAT"], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und ihr uns nimmer wiederspenstig, nie ungehorsames,", "tokens": ["Und", "ihr", "uns", "nim\u00b7mer", "wie\u00b7der\u00b7spens\u00b7tig", ",", "nie", "un\u00b7ge\u00b7hor\u00b7sa\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "PRF", "ADV", "ADJD", "$,", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Zu leiten, zu regieren, taugen.", "tokens": ["Zu", "lei\u00b7ten", ",", "zu", "re\u00b7gie\u00b7ren", ",", "tau\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es sind ja, wie bekannt, die Augen in unsrer Stirne", "tokens": ["Es", "sind", "ja", ",", "wie", "be\u00b7kannt", ",", "die", "Au\u00b7gen", "in", "uns\u00b7rer", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PWAV", "ADJD", "$,", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wodurch die Seelen sich einander am allermeisten sichtbar", "tokens": ["Wo\u00b7durch", "die", "See\u00b7len", "sich", "ein\u00b7an\u00b7der", "am", "al\u00b7ler\u00b7meis\u00b7ten", "sicht\u00b7bar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PRF", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Wodurch, wie unsre gegen sie gesinnt, sie glauben zu", "tokens": ["Wo\u00b7durch", ",", "wie", "uns\u00b7re", "ge\u00b7gen", "sie", "ge\u00b7sinnt", ",", "sie", "glau\u00b7ben", "zu"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "$,", "PWAV", "PPOSAT", "APPR", "PPER", "VVPP", "$,", "PPER", "VVINF", "PTKZU"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Und folglich, durch dergleichen Minen erregt- und aufge-", "tokens": ["Und", "folg\u00b7lich", ",", "durch", "derg\u00b7lei\u00b7chen", "Mi\u00b7nen", "er\u00b7reg\u00b7t", "und", "auf\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "APPR", "PIS", "NN", "TRUNC", "KON", "TRUNC"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Uns eingepflantzter Eigen-Liebe,", "tokens": ["Uns", "ein\u00b7ge\u00b7pflantz\u00b7ter", "Ei\u00b7gen\u00b7Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dergleichen Leidenschaft in ihr so dann erregen und er-", "tokens": ["Derg\u00b7lei\u00b7chen", "Lei\u00b7den\u00b7schaft", "in", "ihr", "so", "dann", "er\u00b7re\u00b7gen", "und", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "NN", "APPR", "PPER", "ADV", "ADV", "VVINF", "KON", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Als wie sie in der andern Seele, durchs Auge, meinen", "tokens": ["Als", "wie", "sie", "in", "der", "an\u00b7dern", "See\u00b7le", ",", "durchs", "Au\u00b7ge", ",", "mei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KOUS", "KOKOM", "PPER", "APPR", "ART", "ADJA", "NN", "$,", "APPRART", "NN", "$,", "PPOSAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Dahero folgt nun \u00fcberzeuglich, wie sehr sie wehrt sind", "tokens": ["Da\u00b7he\u00b7ro", "folgt", "nun", "\u00fc\u00b7berz\u00b7eug\u00b7lich", ",", "wie", "sehr", "sie", "wehrt", "sind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "ADJD", "$,", "PWAV", "ADV", "PPER", "VVFIN", "VAFIN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da\u00df man mit mehrer\u2019 Achtsamkeit, zu unserm Nutzen, sie", "tokens": ["Da\u00df", "man", "mit", "meh\u00b7rer'", "Acht\u00b7sam\u00b7keit", ",", "zu", "un\u00b7serm", "Nut\u00b7zen", ",", "sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PIS", "APPR", "PIAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "PPER"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Und sie, mit mehrer M\u00fch und Sorgfalt, zu diesem End-", "tokens": ["Und", "sie", ",", "mit", "meh\u00b7rer", "M\u00fch", "und", "Sorg\u00b7falt", ",", "zu", "die\u00b7sem", "En\u00b7d"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "APPR", "PIAT", "NN", "KON", "NN", "$,", "APPR", "PDAT", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wir selbst seynd Herrn von unsern Minen.", "tokens": ["Wir", "selbst", "seynd", "Herrn", "von", "un\u00b7sern", "Mi\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wann nun daran so viel gelegen, da wir ja gerne", "tokens": ["Wann", "nun", "da\u00b7ran", "so", "viel", "ge\u00b7le\u00b7gen", ",", "da", "wir", "ja", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PAV", "ADV", "ADV", "VVPP", "$,", "KOUS", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da\u00df andre Seelen unsrer Seele geneigt seyn, und sie lie-", "tokens": ["Da\u00df", "and\u00b7re", "See\u00b7len", "uns\u00b7rer", "See\u00b7le", "ge\u00b7neigt", "seyn", ",", "und", "sie", "lie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "VAINF", "$,", "KON", "PPER", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Da\u00df sie durchs Aug\u2019 in unsrer Seel\u2019 ein\u2019 Achtung gegen", "tokens": ["Da\u00df", "sie", "durchs", "Aug'", "in", "uns\u00b7rer", "Seel'", "ein'", "Ach\u00b7tung", "ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "APPR", "PPOSAT", "NN", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Um sie dadurch zur Gegen-Gunst f\u00fcr uns hinwieder zu", "tokens": ["Um", "sie", "da\u00b7durch", "zur", "Ge\u00b7gen\u00b7Gunst", "f\u00fcr", "uns", "hin\u00b7wie\u00b7der", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "PAV", "APPRART", "NN", "APPR", "PPER", "ADV", "APPR"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "So m\u00fcssen wir, durch Freundlichkeit in unsern Augen,", "tokens": ["So", "m\u00fcs\u00b7sen", "wir", ",", "durch", "Freund\u00b7lich\u00b7keit", "in", "un\u00b7sern", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "APPR", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von unsrer Achtung gegen sie ein Merckmahl ihnen abzu-", "tokens": ["Von", "uns\u00b7rer", "Ach\u00b7tung", "ge\u00b7gen", "sie", "ein", "Merck\u00b7mahl", "ih\u00b7nen", "ab\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPER", "ART", "NN", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Zu diesen Zweck nun zu gelangen, ist leichter als mans", "tokens": ["Zu", "die\u00b7sen", "Zweck", "nun", "zu", "ge\u00b7lan\u00b7gen", ",", "ist", "leich\u00b7ter", "als", "mans"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADV", "PTKZU", "VVINF", "$,", "VAFIN", "ADJD", "KOKOM", "PIS"], "meter": "-+-+-+-+--+-++", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "Wenn man nur so viel Acht auf sich, zum eignem Nutzen,", "tokens": ["Wenn", "man", "nur", "so", "viel", "Acht", "auf", "sich", ",", "zum", "eig\u00b7nem", "Nut\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "ADV", "CARD", "APPR", "PRF", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Da\u00df wir die Z\u00fcge des Gesichts, wovon wir Meister seyn,", "tokens": ["Da\u00df", "wir", "die", "Z\u00fc\u00b7ge", "des", "Ge\u00b7sichts", ",", "wo\u00b7von", "wir", "Meis\u00b7ter", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "$,", "PWAV", "PPER", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "Und Augen-Lieder, Augen-Branen und Lippen so in Ord-", "tokens": ["Und", "Au\u00b7gen\u00b7Lie\u00b7der", ",", "Au\u00b7gen\u00b7Bra\u00b7nen", "und", "Lip\u00b7pen", "so", "in", "Ord"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "Da\u00df wir, an statt ein gr\u00e4mlich, bitter und schwartz Ge-", "tokens": ["Da\u00df", "wir", ",", "an", "statt", "ein", "gr\u00e4m\u00b7lich", ",", "bit\u00b7ter", "und", "schwartz", "Ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "APPR", "ART", "ADJD", "$,", "ADJD", "KON", "ADJD", "TRUNC"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Sie zu der holden Freundlichkeit bem\u00fchet w\u00e4ren oft zu", "tokens": ["Sie", "zu", "der", "hol\u00b7den", "Freund\u00b7lich\u00b7keit", "be\u00b7m\u00fc\u00b7het", "w\u00e4\u00b7ren", "oft", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "VAFIN", "ADV", "PTKZU"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}}}}}