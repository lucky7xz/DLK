{"textgrid.poem.25127": {"metadata": {"author": {"name": "Keats, John", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Herz tut weh, und schl\u00e4friges Erlahmen,", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Herz tut weh, und schl\u00e4friges Erlahmen,", "tokens": ["Ein", "Herz", "tut", "weh", ",", "und", "schl\u00e4f\u00b7ri\u00b7ges", "Er\u00b7lah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als h\u00e4tt ich Gift getrunken, qu\u00e4lt mich sehr.", "tokens": ["Als", "h\u00e4tt", "ich", "Gift", "ge\u00b7trun\u00b7ken", ",", "qu\u00e4lt", "mich", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "NN", "VVPP", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bet\u00e4ubte mich ein Trank aus giftigen Samen?", "tokens": ["Be\u00b7t\u00e4ub\u00b7te", "mich", "ein", "Trank", "aus", "gif\u00b7ti\u00b7gen", "Sa\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Mich h\u00fcllt Vergessenheit, ich wei\u00df nichts mehr.", "tokens": ["Mich", "h\u00fcllt", "Ver\u00b7ges\u00b7sen\u00b7heit", ",", "ich", "wei\u00df", "nichts", "mehr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "PPER", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch ist's nicht Neid auf dein so gl\u00fccklich Los \u2013", "tokens": ["Doch", "ist's", "nicht", "Neid", "auf", "dein", "so", "gl\u00fcck\u00b7lich", "Los", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "NN", "APPR", "PPOSAT", "ADV", "ADJD", "NE", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nur f\u00fcllt so schwer mit Gl\u00fcck dein Gl\u00fcck mich an:", "tokens": ["Nur", "f\u00fcllt", "so", "schwer", "mit", "Gl\u00fcck", "dein", "Gl\u00fcck", "mich", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "APPR", "NN", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df du, des Walds befl\u00fcgelte Dryade,", "tokens": ["Da\u00df", "du", ",", "des", "Walds", "be\u00b7fl\u00fc\u00b7gel\u00b7te", "Dry\u00b7a\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In lieblich k\u00fchlem Scho\u00df,", "tokens": ["In", "lieb\u00b7lich", "k\u00fch\u00b7lem", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Im Schatten, den das Buchengr\u00fcn dir spann,", "tokens": ["Im", "Schat\u00b7ten", ",", "den", "das", "Bu\u00b7chen\u00b7gr\u00fcn", "dir", "spann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der Freiheit jubeln kannst, der Sommergnade.", "tokens": ["Der", "Frei\u00b7heit", "ju\u00b7beln", "kannst", ",", "der", "Som\u00b7mer\u00b7gna\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O Wein jetzt! Jungen Wein, den Erde k\u00fchlte,", "tokens": ["O", "Wein", "jetzt", "!", "Jun\u00b7gen", "Wein", ",", "den", "Er\u00b7de", "k\u00fchl\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "$.", "NN", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den dunkelk\u00fchl ein langes Jahr gereift,", "tokens": ["Den", "dun\u00b7kel\u00b7k\u00fchl", "ein", "lan\u00b7ges", "Jahr", "ge\u00b7reift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der sonngebr\u00e4unten Frohsinn tanzen f\u00fchlte,", "tokens": ["Der", "sonn\u00b7ge\u00b7br\u00e4un\u00b7ten", "Froh\u00b7sinn", "tan\u00b7zen", "f\u00fchl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und der des Proven\u00e7alen Lied begreift;", "tokens": ["Und", "der", "des", "Pro\u00b7ven\u00e7a\u00b7len", "Lied", "be\u00b7greift", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "NN", "VVFIN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "O einen Becher warmen S\u00fcdens jetzt!", "tokens": ["O", "ei\u00b7nen", "Be\u00b7cher", "war\u00b7men", "S\u00fc\u00b7dens", "jetzt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "O Hippokrene, die zum Rande sch\u00e4umt", "tokens": ["O", "Hip\u00b7po\u00b7kre\u00b7ne", ",", "die", "zum", "Ran\u00b7de", "sch\u00e4umt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PRELS", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und gern und gut Begeisterung bereitet", "tokens": ["Und", "gern", "und", "gut", "Be\u00b7geis\u00b7te\u00b7rung", "be\u00b7rei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KON", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit Lippen rot benetzt,", "tokens": ["Mit", "Lip\u00b7pen", "rot", "be\u00b7netzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Dich will ich trinken, da\u00df ich unges\u00e4umt", "tokens": ["Dich", "will", "ich", "trin\u00b7ken", ",", "da\u00df", "ich", "un\u00b7ge\u00b7s\u00e4umt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Zum Wald entschweben kann, von ", "tokens": ["Zum", "Wald", "ent\u00b7schwe\u00b7ben", "kann", ",", "von"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "VVINF", "VMFIN", "$,", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Entschweben, ganz vergehn \u2013 und ganz vergessen,", "tokens": ["Ent\u00b7schwe\u00b7ben", ",", "ganz", "ver\u00b7gehn", "\u2013", "und", "ganz", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVINF", "$(", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was du in deinem Walde nie gekannt:", "tokens": ["Was", "du", "in", "dei\u00b7nem", "Wal\u00b7de", "nie", "ge\u00b7kannt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Menschennot, die M\u00fchen unermessen,", "tokens": ["Die", "Men\u00b7schen\u00b7not", ",", "die", "M\u00fc\u00b7hen", "un\u00b7er\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Sorgenfieber, das die Herzen bannt;", "tokens": ["Das", "Sor\u00b7gen\u00b7fie\u00b7ber", ",", "das", "die", "Her\u00b7zen", "bannt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du wei\u00dft nicht, wie gel\u00e4hmtes Alter st\u00f6hnt,", "tokens": ["Du", "wei\u00dft", "nicht", ",", "wie", "ge\u00b7l\u00e4hm\u00b7tes", "Al\u00b7ter", "st\u00f6hnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie Denken immer nur Sich-h\u00e4rmen hei\u00dft,", "tokens": ["Wie", "Den\u00b7ken", "im\u00b7mer", "nur", "Sich\u00b7h\u00e4r\u00b7men", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie Jugend bleicht und schleicht und siecht und schwindet,", "tokens": ["Wie", "Ju\u00b7gend", "bleicht", "und", "schleicht", "und", "siecht", "und", "schwin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und wie Verzweiflung h\u00f6hnt,", "tokens": ["Und", "wie", "Ver\u00b7zwei\u00b7flung", "h\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Wo Sch\u00f6nheit, wenn ihr Blick das Leben preist,", "tokens": ["Wo", "Sch\u00f6n\u00b7heit", ",", "wenn", "ihr", "Blick", "das", "Le\u00b7ben", "preist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Um Liebe weinen lernt und bald erblindet.", "tokens": ["Um", "Lie\u00b7be", "wei\u00b7nen", "lernt", "und", "bald", "er\u00b7blin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVINF", "VVFIN", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Hinweg! Zu dir! Doch soll nicht Bacchus Wagen", "tokens": ["Hin\u00b7weg", "!", "Zu", "dir", "!", "Doch", "soll", "nicht", "Bac\u00b7chus", "Wa\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "APPR", "PPER", "$.", "KON", "VMFIN", "PTKNEG", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Mit Pantherkraft mich ziehn, nein! Poesie", "tokens": ["Mit", "Pan\u00b7ther\u00b7kraft", "mich", "ziehn", ",", "nein", "!", "Poe\u00b7sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "NN", "PPER", "VVINF", "$,", "PTKANT", "$.", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Soll mich auf unsichtbaren Schwingen tragen,", "tokens": ["Soll", "mich", "auf", "un\u00b7sicht\u00b7ba\u00b7ren", "Schwin\u00b7gen", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Dr\u00fcckt auch dies Hirn noch m\u00fcde Apathie.", "tokens": ["Dr\u00fcckt", "auch", "dies", "Hirn", "noch", "m\u00fc\u00b7de", "A\u00b7pat\u00b7hie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PDS", "NN", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Schon bin ich bei dir! Milde ist die Nacht,", "tokens": ["Schon", "bin", "ich", "bei", "dir", "!", "Mil\u00b7de", "ist", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "$.", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und Luna thront mit l\u00e4chelndem Gesicht", "tokens": ["Und", "Lu\u00b7na", "thront", "mit", "l\u00e4\u00b7cheln\u00b7dem", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Und \u00fcberblickt ihr Sternenvolk voll Gnade,", "tokens": ["Und", "\u00fc\u00b7berb\u00b7lickt", "ihr", "Ster\u00b7nen\u00b7volk", "voll", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Doch hat sie hier nicht Macht:", "tokens": ["Doch", "hat", "sie", "hier", "nicht", "Macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PTKNEG", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Nur manchmal bl\u00e4st ein Windhauch etwas Licht", "tokens": ["Nur", "manch\u00b7mal", "bl\u00e4st", "ein", "Wind\u00b7hauch", "et\u00b7was", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Durch gr\u00fcne D\u00e4mmernis auf moosige Pfade.", "tokens": ["Durch", "gr\u00fc\u00b7ne", "D\u00e4m\u00b7mer\u00b7nis", "auf", "moo\u00b7si\u00b7ge", "Pfa\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich sehe nicht, was bl\u00fcht zu meinen Fu\u00dfen,", "tokens": ["Ich", "se\u00b7he", "nicht", ",", "was", "bl\u00fcht", "zu", "mei\u00b7nen", "Fu\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Welch s\u00fc\u00dfer Balsam rings an Zweigen h\u00e4ngt;", "tokens": ["Welch", "s\u00fc\u00b7\u00dfer", "Bal\u00b7sam", "rings", "an", "Zwei\u00b7gen", "h\u00e4ngt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch auch im Dunkel ahn ich, was an s\u00fc\u00dfen", "tokens": ["Doch", "auch", "im", "Dun\u00b7kel", "ahn", "ich", ",", "was", "an", "s\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "PPER", "$,", "PRELS", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Duftwellen atmend in die Mainacht dr\u00e4ngt", "tokens": ["Duft\u00b7wel\u00b7len", "at\u00b7mend", "in", "die", "Mai\u00b7nacht", "dr\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Aus wildem Beerenbaum und Gras und Strauch:", "tokens": ["Aus", "wil\u00b7dem", "Bee\u00b7ren\u00b7baum", "und", "Gras", "und", "Strauch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich atme Wei\u00dfdornduft und Rosenbl\u00fchn", "tokens": ["Ich", "at\u00b7me", "Wei\u00df\u00b7dorn\u00b7duft", "und", "Ro\u00b7sen\u00b7bl\u00fchn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und Veilchen, die in Bl\u00e4tterbetten sterben,", "tokens": ["Und", "Veil\u00b7chen", ",", "die", "in", "Bl\u00e4t\u00b7ter\u00b7bet\u00b7ten", "ster\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und Moschusrosen auch,", "tokens": ["Und", "Mo\u00b7schus\u00b7ro\u00b7sen", "auch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "In denen morgens bunte Tropfen gl\u00fchn", "tokens": ["In", "de\u00b7nen", "mor\u00b7gens", "bun\u00b7te", "Trop\u00b7fen", "gl\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ADV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und abends Sommerfliegen sich umwerben.", "tokens": ["Und", "a\u00b7bends", "Som\u00b7mer\u00b7flie\u00b7gen", "sich", "um\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Im Dunkel lausche ich; und wie Verlangen", "tokens": ["Im", "Dun\u00b7kel", "lau\u00b7sche", "ich", ";", "und", "wie", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$.", "KON", "PWAV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mich oft schon fa\u00dfte nach dem stillen Grab,", "tokens": ["Mich", "oft", "schon", "fa\u00df\u00b7te", "nach", "dem", "stil\u00b7len", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie ich dem Tod, mich herzlich zu umfangen,", "tokens": ["Wie", "ich", "dem", "Tod", ",", "mich", "herz\u00b7lich", "zu", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "$,", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schon oft in Liedern liebe Namen gab,", "tokens": ["Schon", "oft", 