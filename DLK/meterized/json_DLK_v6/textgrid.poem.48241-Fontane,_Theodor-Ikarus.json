{"textgrid.poem.48241": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Ikarus", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Immer wieder dieselbe Geschichte:", "tokens": ["Im\u00b7mer", "wie\u00b7der", "die\u00b7sel\u00b7be", "Ge\u00b7schich\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PDAT", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Siege, Triumphe, Gottesgerichte.", "tokens": ["Sie\u00b7ge", ",", "Tri\u00b7um\u00b7phe", ",", "Got\u00b7tes\u00b7ge\u00b7rich\u00b7te", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Wem jeder Sprung, auch der k\u00fchnste, gegl\u00fcckt,", "tokens": ["Wem", "je\u00b7der", "Sprung", ",", "auch", "der", "k\u00fchns\u00b7te", ",", "ge\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "$,", "ADV", "ART", "ADJA", "$,", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der f\u00fchlt sich dem Gesetz entr\u00fcckt,", "tokens": ["Der", "f\u00fchlt", "sich", "dem", "Ge\u00b7setz", "ent\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er ist heraus aus dem Alltagstrott,", "tokens": ["Er", "ist", "he\u00b7raus", "aus", "dem", "All\u00b7tags\u00b7trott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Fliegen will er, er ist ein Gott;", "tokens": ["Flie\u00b7gen", "will", "er", ",", "er", "ist", "ein", "Gott", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Er f\u00e4llt dem Sonnengespann in die Z\u00fcgel, \u2013", "tokens": ["Er", "f\u00e4llt", "dem", "Son\u00b7nen\u00b7ge\u00b7spann", "in", "die", "Z\u00fc\u00b7gel", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Da schmelzen dem Ikarus die Fl\u00fcgel,", "tokens": ["Da", "schmel\u00b7zen", "dem", "I\u00b7ka\u00b7rus", "die", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "ART", "NN", "$,"], "meter": "-+--++--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Er flog zu hoch, er st\u00fcrzt, er f\u00e4llt,", "tokens": ["Er", "flog", "zu", "hoch", ",", "er", "st\u00fcrzt", ",", "er", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKA", "ADJD", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein neu Spektakelst\u00fcck hat die Welt,", "tokens": ["Ein", "neu", "Spek\u00b7ta\u00b7kel\u00b7st\u00fcck", "hat", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Eben noch zum Himmel getragen ...", "tokens": ["E\u00b7ben", "noch", "zum", "Him\u00b7mel", "ge\u00b7tra\u00b7gen", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Apollo, z\u00fcrnend, hat ihn erschlagen.", "tokens": ["A\u00b7pol\u00b7lo", ",", "z\u00fcr\u00b7nend", ",", "hat", "ihn", "er\u00b7schla\u00b7gen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVPP", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}}}}