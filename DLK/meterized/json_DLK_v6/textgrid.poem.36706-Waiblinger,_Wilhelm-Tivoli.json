{"textgrid.poem.36706": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Tivoli", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Haine gl\u00e4nzen, es donnern die St\u00fcrze des Anio, es st\u00e4uben", "tokens": ["Hai\u00b7ne", "gl\u00e4n\u00b7zen", ",", "es", "don\u00b7nern", "die", "St\u00fcr\u00b7ze", "des", "A\u00b7nio", ",", "es", "st\u00e4u\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVINF", "$,", "PPER", "ADV", "ART", "NN", "ART", "NE", "$,", "PPER", "VVINF"], "meter": "+-+--+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Kaskatellen, es grau'n Tempel und Villen umher.", "tokens": ["Kas\u00b7ka\u00b7tel\u00b7len", ",", "es", "grau'n", "Tem\u00b7pel", "und", "Vil\u00b7len", "um\u00b7her", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wunder bietet die Vorwelt dir an, und Wunder die Mitwelt,", "tokens": ["Wun\u00b7der", "bie\u00b7tet", "die", "Vor\u00b7welt", "dir", "an", ",", "und", "Wun\u00b7der", "die", "Mit\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,", "KON", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Ueber die sch\u00e4umende Kluft herrscht die Sibylle noch heut.", "tokens": ["Ue\u00b7ber", "die", "sch\u00e4u\u00b7men\u00b7de", "Kluft", "herrscht", "die", "Si\u00b7byl\u00b7le", "noch", "heut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ADV", "ADV", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}}}}}