{"dta.poem.12852": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Er suchet Sylviens gegen-liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der sonne sanfftes aug' ist immer ohne neid,", "tokens": ["Der", "son\u00b7ne", "sanff\u00b7tes", "aug'", "ist", "im\u00b7mer", "oh\u00b7ne", "neid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VAFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie wirfft den hohen glantz auch in die tiefsten th\u00e4ler,", "tokens": ["Sie", "wirfft", "den", "ho\u00b7hen", "glantz", "auch", "in", "die", "tiefs\u00b7ten", "th\u00e4\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drum glaube, Sylvia! es ist kein gro\u00dfer fehler,", "tokens": ["Drum", "glau\u00b7be", ",", "Syl\u00b7via", "!", "es", "ist", "kein", "gro\u00b7\u00dfer", "feh\u00b7ler", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "NE", "$.", "PPER", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wenn deine gunst ihr licht gleich auf mein hertze streut.", "tokens": ["Wenn", "dei\u00b7ne", "gunst", "ihr", "licht", "gleich", "auf", "mein", "hert\u00b7ze", "streut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich gebe gerne zu, da\u00df ich verwegen bin,", "tokens": ["Ich", "ge\u00b7be", "ger\u00b7ne", "zu", ",", "da\u00df", "ich", "ver\u00b7we\u00b7gen", "bin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Indem ich mich zu dir, als einer sonne, wage;", "tokens": ["In\u00b7dem", "ich", "mich", "zu", "dir", ",", "als", "ei\u00b7ner", "son\u00b7ne", ",", "wa\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "$,", "KOUS", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denck aber auch, dein knecht schau niemals freuden-tage,", "tokens": ["Denck", "a\u00b7ber", "auch", ",", "dein", "knecht", "schau", "nie\u00b7mals", "freu\u00b7den\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "$,", "PPOSAT", "NN", "ADJD", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es nehme dann dein stern die finstern nebel hin.", "tokens": ["Es", "neh\u00b7me", "dann", "dein", "stern", "die", "fins\u00b7tern", "ne\u00b7bel", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Erschrick nicht vor der glut, sie f\u00fchret keinen rauch:", "tokens": ["Er\u00b7schrick", "nicht", "vor", "der", "glut", ",", "sie", "f\u00fch\u00b7ret", "kei\u00b7nen", "rauch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wo sie nur stille brennt, da bringt sie viel vergn\u00fcgen.", "tokens": ["Wo", "sie", "nur", "stil\u00b7le", "brennt", ",", "da", "bringt", "sie", "viel", "ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJA", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die dornen, welche dir und mir im wege liegen,", "tokens": ["Die", "dor\u00b7nen", ",", "wel\u00b7che", "dir", "und", "mir", "im", "we\u00b7ge", "lie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "KON", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Verwandeln witz und treu in einen rosen-strauch.", "tokens": ["Ver\u00b7wan\u00b7deln", "witz", "und", "treu", "in", "ei\u00b7nen", "ro\u00b7sen\u00b7strauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sey offentlich so stoltz und ernsthafft als du willst,", "tokens": ["Sey", "of\u00b7fent\u00b7lich", "so", "stoltz", "und", "ernst\u00b7hafft", "als", "du", "willst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "ADV", "ADJD", "KON", "VVFIN", "KOUS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ich will dir offentlich auch nichts als ehrsucht zeigen,", "tokens": ["Ich", "will", "dir", "of\u00b7fent\u00b7lich", "auch", "nichts", "als", "ehr\u00b7sucht", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "PIS", "KOKOM", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wofern du meine glut, die sonst best\u00e4ndig schweigen", "tokens": ["Wo\u00b7fern", "du", "mei\u00b7ne", "glut", ",", "die", "sonst", "be\u00b7st\u00e4n\u00b7dig", "schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "$,", "PRELS", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und deiner schonen wird, nur im verborgnen stillst.", "tokens": ["Und", "dei\u00b7ner", "scho\u00b7nen", "wird", ",", "nur", "im", "ver\u00b7borg\u00b7nen", "stillst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVINF", "VAFIN", "$,", "ADV", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Allein, du trauest nicht, dein argwohn ist zu scharff,", "tokens": ["Al\u00b7lein", ",", "du", "trau\u00b7est", "nicht", ",", "dein", "arg\u00b7wohn", "ist", "zu", "scharff", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "PTKNEG", "$,", "PPOSAT", "NN", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Du denckst, ich w\u00fcrde mir die gr\u00f6ste freyheit geben.", "tokens": ["Du", "denckst", ",", "ich", "w\u00fcr\u00b7de", "mir", "die", "gr\u00f6s\u00b7te", "frey\u00b7heit", "ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich aber schwere dir, durchaus vergn\u00fcgt zu leben,", "tokens": ["Ich", "a\u00b7ber", "schwe\u00b7re", "dir", ",", "durc\u00b7haus", "ver\u00b7gn\u00fcgt", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$,", "ADV", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wenn auge, mund und hand nur mit dir spielen darff.", "tokens": ["Wenn", "au\u00b7ge", ",", "mund", "und", "hand", "nur", "mit", "dir", "spie\u00b7len", "darff", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN", "ADV", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}