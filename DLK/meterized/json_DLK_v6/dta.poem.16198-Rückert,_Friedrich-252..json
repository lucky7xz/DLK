{"dta.poem.16198": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "252.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kein Vorbereiten hilft, das Rechte recht zu thun,", "tokens": ["Kein", "Vor\u00b7be\u00b7rei\u00b7ten", "hilft", ",", "das", "Rech\u00b7te", "recht", "zu", "thun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn anders dachtest du, und anders thust du nun.", "tokens": ["Denn", "an\u00b7ders", "dach\u00b7test", "du", ",", "und", "an\u00b7ders", "thust", "du", "nun", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "KON", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein andrer f\u00fchlst du dich im Thun, als du dich dachtest,", "tokens": ["Ein", "an\u00b7drer", "f\u00fchlst", "du", "dich", "im", "Thun", ",", "als", "du", "dich", "dach\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und findest andres vor, als du in Rechnung brachtest.", "tokens": ["Und", "fin\u00b7dest", "and\u00b7res", "vor", ",", "als", "du", "in", "Rech\u00b7nung", "brach\u00b7test", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PTKVZ", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Drum ist kein Rath, als dich im Ganzen recht zu fassen,", "tokens": ["Drum", "ist", "kein", "Rath", ",", "als", "dich", "im", "Gan\u00b7zen", "recht", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PIAT", "NN", "$,", "KOUS", "PPER", "APPRART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dann das Seinige dem Augenblick zu lassen.", "tokens": ["Und", "dann", "das", "Sei\u00b7ni\u00b7ge", "dem", "Au\u00b7gen\u00b7blick", "zu", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "PPOSS", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}