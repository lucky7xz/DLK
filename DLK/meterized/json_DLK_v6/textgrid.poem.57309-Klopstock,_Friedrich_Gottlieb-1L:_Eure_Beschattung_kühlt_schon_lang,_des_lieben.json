{"textgrid.poem.57309": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eure Beschattung k\u00fchlt schon lang, des lieben", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eure Beschattung k\u00fchlt schon lang, des lieben", "tokens": ["Eu\u00b7re", "Be\u00b7schat\u00b7tung", "k\u00fchlt", "schon", "lang", ",", "des", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "$,", "ART", "ADJA"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "W\u00e4ldchens Eichen, ich habe nicht die Wurzel", "tokens": ["W\u00e4ld\u00b7chens", "Ei\u00b7chen", ",", "ich", "ha\u00b7be", "nicht", "die", "Wur\u00b7zel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "PPER", "VAFIN", "PTKNEG", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Dieser hohen Wipfel gesenkt, ihr wuchset", "tokens": ["Die\u00b7ser", "ho\u00b7hen", "Wip\u00b7fel", "ge\u00b7senkt", ",", "ihr", "wuch\u00b7set"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "VVPP", "$,", "PPER", "VVFIN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Fr\u00fcher als ich, seyd", "tokens": ["Fr\u00fc\u00b7her", "als", "ich", ",", "seyd"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NN", "KOUS", "PPER", "$,", "VAFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "J\u00fcnglinge gleichwol noch, erhebet h\u00f6her", "tokens": ["J\u00fcng\u00b7lin\u00b7ge", "gleich\u00b7wol", "noch", ",", "er\u00b7he\u00b7bet", "h\u00f6\u00b7her"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "ADV", "$,", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Einst die H\u00e4upter, und streckt, wenn sich der Tag neigt,", "tokens": ["Einst", "die", "H\u00e4up\u00b7ter", ",", "und", "streckt", ",", "wenn", "sich", "der", "Tag", "neigt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "KON", "VVFIN", "$,", "KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-++", "measure": "anapaest.di.plus"}, "line.3": {"text": "L\u00e4ngre Schatten. Gr\u00fcnet denn, \u00fcberlebt; ich", "tokens": ["L\u00e4ng\u00b7re", "Schat\u00b7ten", ".", "Gr\u00fc\u00b7net", "denn", ",", "\u00fc\u00b7ber\u00b7lebt", ";", "ich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADJA", "NN", "$.", "VVFIN", "ADV", "$,", "VVPP", "$.", "PPER"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Neid' euch nicht, Eichen.", "tokens": ["Neid'", "euch", "nicht", ",", "Ei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "PPER", "PTKNEG", "$,", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Will mit Gespielen euch, mit Thr\u00e4nenweiden,", "tokens": ["Will", "mit", "Ge\u00b7spie\u00b7len", "euch", ",", "mit", "Thr\u00e4\u00b7nen\u00b7wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "PPER", "$,", "APPR", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Rings umpflanzen, dass einst, wenn nun die Sonne", "tokens": ["Rings", "um\u00b7pflan\u00b7zen", ",", "dass", "einst", ",", "wenn", "nun", "die", "Son\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "$,", "KOUS", "ADV", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Sinkt, in eurer K\u00fchle, durchhaucht von Abend -", "tokens": ["Sinkt", ",", "in", "eu\u00b7rer", "K\u00fch\u00b7le", ",", "durch\u00b7haucht", "von", "A\u00b7bend"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "NN", "$("], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "L\u00fcften, ihr Laub sich", "tokens": ["L\u00fcf\u00b7ten", ",", "ihr", "Laub", "sich"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "PRF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Leise bewege, dann der Liebling sage", "tokens": ["Lei\u00b7se", "be\u00b7we\u00b7ge", ",", "dann", "der", "Lieb\u00b7ling", "sa\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "ADV", "ART", "NN", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Zu dem M\u00e4dchen: \u00bbSie weint ja nicht, sie s\u00e4uselt,", "tokens": ["Zu", "dem", "M\u00e4d\u00b7chen", ":", "\u00bb", "Sie", "weint", "ja", "nicht", ",", "sie", "s\u00e4u\u00b7selt", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "$(", "PPER", "VVFIN", "ADV", "PTKNEG", "$,", "PPER", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Lallt Musik; wie fabelte von der sch\u00f6nen", "tokens": ["Lallt", "Mu\u00b7sik", ";", "wie", "fa\u00b7bel\u00b7te", "von", "der", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "$.", "PWAV", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weide der Vorfahr!\u00ab", "tokens": ["Wei\u00b7de", "der", "Vor\u00b7fahr", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Wenn von dem Sturm nicht mehr die Eich' hier rauschet,", "tokens": ["Wenn", "von", "dem", "Sturm", "nicht", "mehr", "die", "Eich'", "hier", "rau\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PTKNEG", "ADV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Keine Lispel mehr wehn von dieser Weide:", "tokens": ["Kei\u00b7ne", "Lis\u00b7pel", "mehr", "wehn", "von", "die\u00b7ser", "Wei\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Dann sind Lieder noch, die von Herzen kamen,", "tokens": ["Dann", "sind", "Lie\u00b7der", "noch", ",", "die", "von", "Her\u00b7zen", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ADV", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Gingen zu Herzen.", "tokens": ["Gin\u00b7gen", "zu", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}