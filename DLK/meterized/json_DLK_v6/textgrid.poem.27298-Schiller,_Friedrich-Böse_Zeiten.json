{"textgrid.poem.27298": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "B\u00f6se Zeiten", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Philosophen verderben die Sprache, Poeten die Logik,", "tokens": ["Phi\u00b7lo\u00b7so\u00b7phen", "ver\u00b7der\u00b7ben", "die", "Spra\u00b7che", ",", "Po\u00b7et\u00b7en", "die", "Lo\u00b7gik", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+---+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und mit dem Menschenverstand kommt man durchs Leben nicht mehr.", "tokens": ["Und", "mit", "dem", "Men\u00b7schen\u00b7ver\u00b7stand", "kommt", "man", "durchs", "Le\u00b7ben", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PIS", "APPRART", "NN", "PTKNEG", "ADV", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}}}}}