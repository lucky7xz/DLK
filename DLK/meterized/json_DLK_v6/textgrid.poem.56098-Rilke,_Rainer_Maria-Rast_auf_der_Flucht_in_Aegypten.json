{"textgrid.poem.56098": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Rast auf der Flucht in Aegypten", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Diese, die noch eben atemlos", "tokens": ["Die\u00b7se", ",", "die", "noch", "e\u00b7ben", "a\u00b7tem\u00b7los"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "$,", "PRELS", "ADV", "ADV", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "flohen mitten aus dem Kindermorden:", "tokens": ["flo\u00b7hen", "mit\u00b7ten", "aus", "dem", "Kin\u00b7der\u00b7mor\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "o wie waren sie unmerklich gro\u00df", "tokens": ["o", "wie", "wa\u00b7ren", "sie", "un\u00b7mer\u00b7klich", "gro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM", "KOKOM", "VAFIN", "PPER", "ADJD", "ADJD"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "\u00fcber ihrer Wanderschaft geworden.", "tokens": ["\u00fc\u00b7ber", "ih\u00b7rer", "Wan\u00b7der\u00b7schaft", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Kaum noch da\u00df im scheuen R\u00fcckw\u00e4rtsschauen", "tokens": ["Kaum", "noch", "da\u00df", "im", "scheu\u00b7en", "R\u00fcck\u00b7w\u00e4rts\u00b7schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KOUS", "APPRART", "ADJA", "NN"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "ihres Schreckens Not zergangen war,", "tokens": ["ih\u00b7res", "Schre\u00b7ckens", "Not", "zer\u00b7gan\u00b7gen", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "und schon brachten sie auf ihrem grauen", "tokens": ["und", "schon", "brach\u00b7ten", "sie", "auf", "ih\u00b7rem", "grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Maultier ganze St\u00e4dte in Gefahr;", "tokens": ["Maul\u00b7tier", "gan\u00b7ze", "St\u00e4d\u00b7te", "in", "Ge\u00b7fahr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "denn so wie sie, klein im gro\u00dfen Land,", "tokens": ["denn", "so", "wie", "sie", ",", "klein", "im", "gro\u00b7\u00dfen", "Land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "PPER", "$,", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "\u2013 fast ein Nichts \u2013 den starken Tempeln nahten,", "tokens": ["\u2013", "fast", "ein", "Nichts", "\u2013", "den", "star\u00b7ken", "Tem\u00b7peln", "nah\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ART", "PIS", "$(", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "platzten alle G\u00f6tzen wie verraten", "tokens": ["platz\u00b7ten", "al\u00b7le", "G\u00f6t\u00b7zen", "wie", "ver\u00b7ra\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "KOKOM", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und verloren v\u00f6llig den Verstand.", "tokens": ["und", "ver\u00b7lo\u00b7ren", "v\u00f6l\u00b7lig", "den", "Ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Ist es denkbar, da\u00df von ihrem Gange", "tokens": ["Ist", "es", "denk\u00b7bar", ",", "da\u00df", "von", "ih\u00b7rem", "Gan\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "alles so verzweifelt sich erbost?", "tokens": ["al\u00b7les", "so", "ver\u00b7zwei\u00b7felt", "sich", "er\u00b7bost", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVFIN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "und sie wurden vor sich selber bange,", "tokens": ["und", "sie", "wur\u00b7den", "vor", "sich", "sel\u00b7ber", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "PRF", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "nur das Kind war namenlos getrost.", "tokens": ["nur", "das", "Kind", "war", "na\u00b7men\u00b7los", "ge\u00b7trost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Immerhin, sie mu\u00dften sich dar\u00fcber", "tokens": ["Im\u00b7mer\u00b7hin", ",", "sie", "mu\u00df\u00b7ten", "sich", "da\u00b7r\u00fc\u00b7ber"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "VMFIN", "PRF", "PAV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "eine Weile setzen. Doch da ging \u2013", "tokens": ["ei\u00b7ne", "Wei\u00b7le", "set\u00b7zen", ".", "Doch", "da", "ging", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "KON", "ADV", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "sieh: der Baum, der still sie \u00fcberhing,", "tokens": ["sieh", ":", "der", "Baum", ",", "der", "still", "sie", "\u00fc\u00b7ber\u00b7hing", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "wie ein Dienender zu ihnen \u00fcber:", "tokens": ["wie", "ein", "Die\u00b7nen\u00b7der", "zu", "ih\u00b7nen", "\u00fc\u00b7ber", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "PPER", "APPR", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "er verneigte sich. Derselbe Baum,", "tokens": ["er", "ver\u00b7neig\u00b7te", "sich", ".", "Der\u00b7sel\u00b7be", "Baum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$.", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "dessen Kr\u00e4nze toten Pharaonen", "tokens": ["des\u00b7sen", "Kr\u00e4n\u00b7ze", "to\u00b7ten", "Pha\u00b7ra\u00b7o\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PRELAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "f\u00fcr das Ewige die Stirnen schonen,", "tokens": ["f\u00fcr", "das", "E\u00b7wi\u00b7ge", "die", "Stir\u00b7nen", "scho\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "neigte sich. Er f\u00fchlte neue Kronen", "tokens": ["neig\u00b7te", "sich", ".", "Er", "f\u00fchl\u00b7te", "neu\u00b7e", "Kro\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "$.", "PPER", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "bl\u00fchen. Und sie sa\u00dfen wie im Traum.", "tokens": ["bl\u00fc\u00b7hen", ".", "Und", "sie", "sa\u00b7\u00dfen", "wie", "im", "Traum", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "KON", "PPER", "VVFIN", "KOKOM", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}