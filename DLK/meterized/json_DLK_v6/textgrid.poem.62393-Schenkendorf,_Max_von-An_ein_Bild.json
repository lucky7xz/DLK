{"textgrid.poem.62393": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "An ein Bild", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was schaust du mich so freundlich an,", "tokens": ["Was", "schaust", "du", "mich", "so", "freund\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Bild aus weiter Ferne,", "tokens": ["O", "Bild", "aus", "wei\u00b7ter", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und winkest dem verbannten Mann?", "tokens": ["Und", "win\u00b7kest", "dem", "ver\u00b7bann\u00b7ten", "Mann", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er k\u00e4me gar zu gerne.", "tokens": ["Er", "k\u00e4\u00b7me", "gar", "zu", "ger\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKA", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die ganze Jugend thut sich auf,", "tokens": ["Die", "gan\u00b7ze", "Ju\u00b7gend", "thut", "sich", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ich an dich gedenke,", "tokens": ["Wenn", "ich", "an", "dich", "ge\u00b7den\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Alls ob ich noch den alten Lauf", "tokens": ["Alls", "ob", "ich", "noch", "den", "al\u00b7ten", "Lauf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach deinem Hause lenke.", "tokens": ["Nach", "dei\u00b7nem", "Hau\u00b7se", "len\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gleich einem, der ins tiefe Meer", "tokens": ["Gleich", "ei\u00b7nem", ",", "der", "ins", "tie\u00b7fe", "Meer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Blicke l\u00e4\u00dft versinken,", "tokens": ["Die", "Bli\u00b7cke", "l\u00e4\u00dft", "ver\u00b7sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nicht sieht, nicht h\u00f6rt, ob um ihn her", "tokens": ["Nicht", "sieht", ",", "nicht", "h\u00f6rt", ",", "ob", "um", "ihn", "her"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "$,", "PTKNEG", "VVFIN", "$,", "KOUS", "APPR", "PPER", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Viel tausend Sch\u00e4tze winken;", "tokens": ["Viel", "tau\u00b7send", "Sch\u00e4t\u00b7ze", "win\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Gleich einem, der am Firmament", "tokens": ["Gleich", "ei\u00b7nem", ",", "der", "am", "Fir\u00b7ma\u00b7ment"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach fernem Sterne blicket,", "tokens": ["Nach", "fer\u00b7nem", "Ster\u00b7ne", "bli\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nur diesen kennt, nur diesen nennt,", "tokens": ["Nur", "die\u00b7sen", "kennt", ",", "nur", "die\u00b7sen", "nennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "$,", "ADV", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sich an ihm entz\u00fccket:", "tokens": ["Und", "sich", "an", "ihm", "ent\u00b7z\u00fc\u00b7cket", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Ist all mein Sehnen, all mein Muth", "tokens": ["Ist", "all", "mein", "Seh\u00b7nen", ",", "all", "mein", "Muth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "PPOSAT", "NN", "$,", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In dir, o Bild, gegr\u00fcndet,", "tokens": ["In", "dir", ",", "o", "Bild", ",", "ge\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und immer noch von gleicher Gluth,", "tokens": ["Und", "im\u00b7mer", "noch", "von", "glei\u00b7cher", "Gluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von gleicher Lust entz\u00fcndet.", "tokens": ["Von", "glei\u00b7cher", "Lust", "ent\u00b7z\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}