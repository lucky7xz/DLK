{"textgrid.poem.62841": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: In einer strengen Kinderlehre", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einer strengen Kinderlehre", "tokens": ["In", "ei\u00b7ner", "stren\u00b7gen", "Kin\u00b7der\u00b7leh\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ward eine dicke Bauermagd", "tokens": ["Ward", "ei\u00b7ne", "di\u00b7cke", "Bau\u00b7er\u00b7magd"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom alten Dorfkaplan gefragt,", "tokens": ["Vom", "al\u00b7ten", "Dorf\u00b7kap\u00b7lan", "ge\u00b7fragt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer doch der gro\u00dfe Christoph w\u00e4re.", "tokens": ["Wer", "doch", "der", "gro\u00b7\u00dfe", "Chris\u00b7toph", "w\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das Mensch war sch\u00fcchtern oder dumm;", "tokens": ["Das", "Mensch", "war", "sch\u00fcch\u00b7tern", "o\u00b7der", "dumm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es zog die Lippen und blieb stumm.", "tokens": ["Es", "zog", "die", "Lip\u00b7pen", "und", "blieb", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Priester, um es zu besch\u00e4men,", "tokens": ["Der", "Pries\u00b7ter", ",", "um", "es", "zu", "be\u00b7sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ruft ein noch kleines Kind herbey", "tokens": ["Ruft", "ein", "noch", "klei\u00b7nes", "Kind", "her\u00b7bey"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADV", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und will nunmehr von ihm vernehmen,", "tokens": ["Und", "will", "nun\u00b7mehr", "von", "ihm", "ver\u00b7neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wer dieser gro\u00dfe Christoph sey.", "tokens": ["Wer", "die\u00b7ser", "gro\u00b7\u00dfe", "Chris\u00b7toph", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Das M\u00e4dchen l\u00e4chelt, h\u00fcpft und gl\u00fchet", "tokens": ["Das", "M\u00e4d\u00b7chen", "l\u00e4\u00b7chelt", ",", "h\u00fcpft", "und", "gl\u00fc\u00b7het"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und spricht: Es ist ein Korporal,", "tokens": ["Und", "spricht", ":", "Es", "ist", "ein", "Kor\u00b7po\u00b7ral", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Der bey uns wohnt und allemal,", "tokens": ["Der", "bey", "uns", "wohnt", "und", "al\u00b7le\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wenn mein Papa zu Markte ziehet,", "tokens": ["Wenn", "mein", "Pa\u00b7pa", "zu", "Mark\u00b7te", "zie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Im Zimmer der Mama, bey Nacht", "tokens": ["Im", "Zim\u00b7mer", "der", "Ma\u00b7ma", ",", "bey", "Nacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Vor Dieben unser Haus bewacht.", "tokens": ["Vor", "Die\u00b7ben", "un\u00b7ser", "Haus", "be\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}