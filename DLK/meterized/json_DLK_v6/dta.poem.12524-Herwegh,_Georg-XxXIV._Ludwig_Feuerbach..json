{"dta.poem.12524": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XxXIV.  \n  Ludwig Feuerbach.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie mu\u00df des Denkers scharfes Schwert", "tokens": ["Wie", "mu\u00df", "des", "Den\u00b7kers", "schar\u00b7fes", "Schwert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Eure Hasenseelen fahren!", "tokens": ["In", "Eu\u00b7re", "Ha\u00b7sen\u00b7see\u00b7len", "fah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00f6rt doch: \u201eDas Beste ist nicht werth,", "tokens": ["H\u00f6rt", "doch", ":", "\u201e", "Das", "Bes\u00b7te", "ist", "nicht", "werth", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$.", "$(", "ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Ewigkeit es aufzusparen;", "tokens": ["In", "E\u00b7wig\u00b7keit", "es", "auf\u00b7zu\u00b7spa\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was einmal die Natur erschuf,", "tokens": ["Was", "ein\u00b7mal", "die", "Na\u00b7tur", "er\u00b7schuf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kann sie auch noch einmal erschaffen.\u201c", "tokens": ["Kann", "sie", "auch", "noch", "ein\u00b7mal", "er\u00b7schaf\u00b7fen", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Allein vergebens ist Sein Ruf", "tokens": ["Al\u00b7lein", "ver\u00b7ge\u00b7bens", "ist", "Sein", "Ruf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "An Kinder und an Laffen.", "tokens": ["An", "Kin\u00b7der", "und", "an", "Laf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es stellt vergebens ihr Symbol", "tokens": ["Es", "stellt", "ver\u00b7ge\u00b7bens", "ihr", "Sym\u00b7bol"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der k\u00fchne Adler an den Pranger:", "tokens": ["Der", "k\u00fch\u00b7ne", "Ad\u00b7ler", "an", "den", "Pran\u00b7ger", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Jedwede Puppe, noch so hohl,", "tokens": ["Jed\u00b7we\u00b7de", "Pup\u00b7pe", ",", "noch", "so", "hohl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADV", "ADV", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "F\u00fchlt sich mit einem Falter schwanger;", "tokens": ["F\u00fchlt", "sich", "mit", "ei\u00b7nem", "Fal\u00b7ter", "schwan\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vergebens l\u00e4uft der Genius Sturm,", "tokens": ["Ver\u00b7ge\u00b7bens", "l\u00e4uft", "der", "Ge\u00b7nius", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Burg des Unsinns zu bezwingen:", "tokens": ["Die", "Burg", "des", "Un\u00b7sinns", "zu", "be\u00b7zwin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es will's nun einmal jeder Wurm", "tokens": ["Es", "will's", "nun", "ein\u00b7mal", "je\u00b7der", "Wurm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zum Schmetterlinge bringen.", "tokens": ["Zum", "Schmet\u00b7ter\u00b7lin\u00b7ge", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}