{"textgrid.poem.37132": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Spr\u00f6\u00dfling einer Thr\u00e4n' Aurorens,", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Spr\u00f6\u00dfling einer Thr\u00e4n' Aurorens,", "tokens": ["Spr\u00f6\u00df\u00b7ling", "ei\u00b7ner", "Thr\u00e4n'", "Au\u00b7ro\u00b7rens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oder ob dich Amor schuf,", "tokens": ["O\u00b7der", "ob", "dich", "A\u00b7mor", "schuf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O vernimm, geweihte Florens,", "tokens": ["O", "ver\u00b7nimm", ",", "ge\u00b7weih\u00b7te", "Flo\u00b7rens", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eines Herzens leisen Ruf!", "tokens": ["Ei\u00b7nes", "Her\u00b7zens", "lei\u00b7sen", "Ruf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schleu\u00df, o Rose! \u2013 Nein, versag' es", "tokens": ["Schleu\u00df", ",", "o", "Ro\u00b7se", "!", "\u2013", "Nein", ",", "ver\u00b7sag'", "es"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "FM", "FM", "$.", "$(", "PTKANT", "$,", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner Sehnsucht Bitten noch!", "tokens": ["Mei\u00b7ner", "Sehn\u00b7sucht", "Bit\u00b7ten", "noch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Blumen, Kinder eines Tages,", "tokens": ["Blu\u00b7men", ",", "Kin\u00b7der", "ei\u00b7nes", "Ta\u00b7ges", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gl\u00e4nzen frisch und welken doch.", "tokens": ["Gl\u00e4n\u00b7zen", "frisch", "und", "wel\u00b7ken", "doch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sanft err\u00f6thend, wie du gl\u00fchest,", "tokens": ["Sanft", "er\u00b7r\u00f6\u00b7thend", ",", "wie", "du", "gl\u00fc\u00b7hest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird Filinde bald dich sehn.", "tokens": ["Wird", "Fi\u00b7lin\u00b7de", "bald", "dich", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! sie bl\u00fchet, wie du bl\u00fchest,", "tokens": ["Ach", "!", "sie", "bl\u00fc\u00b7het", ",", "wie", "du", "bl\u00fc\u00b7hest", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wie du wird sie vergehen!", "tokens": ["Und", "wie", "du", "wird", "sie", "ver\u00b7ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Hast du meinen Ruf vernommen?", "tokens": ["Hast", "du", "mei\u00b7nen", "Ruf", "ver\u00b7nom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oefnest deine Knospe schon?", "tokens": ["Oef\u00b7nest", "dei\u00b7ne", "Knos\u00b7pe", "schon", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00fc\u00dfe Rose, sei willkommen!", "tokens": ["S\u00fc\u00b7\u00dfe", "Ro\u00b7se", ",", "sei", "will\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner harrt ein s\u00fc\u00dfer Lohn.", "tokens": ["Dei\u00b7ner", "harrt", "ein", "s\u00fc\u00b7\u00dfer", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Die dich pfl\u00fcckte soll dich dr\u00fccken,", "tokens": ["Die", "dich", "pfl\u00fcck\u00b7te", "soll", "dich", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Hand, an ihre Brust.", "tokens": ["Mei\u00b7ne", "Hand", ",", "an", "ih\u00b7re", "Brust", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wi\u00dfe nur da\u00df du sie schm\u00fccken,", "tokens": ["Wi\u00b7\u00dfe", "nur", "da\u00df", "du", "sie", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOUS", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber nicht verbergen mu\u00dft.", "tokens": ["A\u00b7ber", "nicht", "ver\u00b7ber\u00b7gen", "mu\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "An Filindens Busen prangend", "tokens": ["An", "Fi\u00b7lin\u00b7dens", "Bu\u00b7sen", "pran\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stirbt dein Reiz zwar fr\u00fcher ab;", "tokens": ["Stirbt", "dein", "Reiz", "zwar", "fr\u00fc\u00b7her", "ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch den sch\u00f6nen Platz verlangend,", "tokens": ["Doch", "den", "sch\u00f6\u00b7nen", "Platz", "ver\u00b7lan\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Neidet jeder selbst dein Grab.", "tokens": ["Nei\u00b7det", "je\u00b7der", "selbst", "dein", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Duft' ihr D\u00fcfte, zart und linde!", "tokens": ["Duft'", "ihr", "D\u00fcf\u00b7te", ",", "zart", "und", "lin\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch erhalte dir den Dorn,", "tokens": ["Doch", "er\u00b7hal\u00b7te", "dir", "den", "Dorn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wer dir sich naht, empfinde", "tokens": ["Und", "wer", "dir", "sich", "naht", ",", "emp\u00b7fin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PWS", "PPER", "PRF", "VVFIN", "$,", "VVFIN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Deine Rache, meinen Zorn!", "tokens": ["Dei\u00b7ne", "Ra\u00b7che", ",", "mei\u00b7nen", "Zorn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Auch gew\u00e4hren neues Leben", "tokens": ["Auch", "ge\u00b7w\u00e4h\u00b7ren", "neu\u00b7es", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir vielleicht die G\u00f6tter dann,", "tokens": ["Dir", "viel\u00b7leicht", "die", "G\u00f6t\u00b7ter", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seufzer werden dich erheben,", "tokens": ["Seuf\u00b7zer", "wer\u00b7den", "dich", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn Filinde seufzen kann.", "tokens": ["Wenn", "Fi\u00b7lin\u00b7de", "seuf\u00b7zen", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Thr\u00e4nen des Gef\u00fchls vergie\u00dfen", "tokens": ["Thr\u00e4\u00b7nen", "des", "Ge\u00b7f\u00fchls", "ver\u00b7gie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lehre sie bei deinem Tod,", "tokens": ["Leh\u00b7re", "sie", "bei", "dei\u00b7nem", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Jugendzeit genie\u00dfen,", "tokens": ["Und", "der", "Ju\u00b7gend\u00b7zeit", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der ein gleiches Ende droht.", "tokens": ["Der", "ein", "glei\u00b7ches", "En\u00b7de", "droht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}