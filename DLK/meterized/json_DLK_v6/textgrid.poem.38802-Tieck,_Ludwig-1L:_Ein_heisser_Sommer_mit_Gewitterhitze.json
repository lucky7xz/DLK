{"textgrid.poem.38802": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein heisser Sommer mit Gewitterhitze", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein heisser Sommer mit Gewitterhitze", "tokens": ["Ein", "heis\u00b7ser", "Som\u00b7mer", "mit", "Ge\u00b7wit\u00b7ter\u00b7hit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist st\u00fcrmisch \u00fcber uns hinweggeflogen,", "tokens": ["Ist", "st\u00fcr\u00b7misch", "\u00fc\u00b7ber", "uns", "hin\u00b7weg\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch schwarze Wolken zuckten gelbe Blitze,", "tokens": ["Durch", "schwar\u00b7ze", "Wol\u00b7ken", "zuck\u00b7ten", "gel\u00b7be", "Blit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Laut donnerte der runde Himmelsbogen;", "tokens": ["Laut", "don\u00b7ner\u00b7te", "der", "run\u00b7de", "Him\u00b7mels\u00b7bo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Im Zorn der Elemente", "tokens": ["Im", "Zorn", "der", "E\u00b7le\u00b7men\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.6": {"text": "H\u00f6rt' ich die rauhe Stimme die von dir mich trennte.", "tokens": ["H\u00f6rt'", "ich", "die", "rau\u00b7he", "Stim\u00b7me", "die", "von", "dir", "mich", "trenn\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ART", "APPR", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So war ein langer Kampf in unserm Leben,", "tokens": ["So", "war", "ein", "lan\u00b7ger", "Kampf", "in", "un\u00b7serm", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Oft brach das Leid mit brausenden Gewittern", "tokens": ["Oft", "brach", "das", "Leid", "mit", "brau\u00b7sen\u00b7den", "Ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch unser Herz, von Dunkelheit umgeben,", "tokens": ["Durch", "un\u00b7ser", "Herz", ",", "von", "Dun\u00b7kel\u00b7heit", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Oft mu\u00dft' ich bis zum tiefsten Sein erzittern,", "tokens": ["Oft", "mu\u00dft'", "ich", "bis", "zum", "tiefs\u00b7ten", "Sein", "er\u00b7zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In allen Lebenstiefen,", "tokens": ["In", "al\u00b7len", "Le\u00b7bens\u00b7tie\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie Feindeslaut' zum Tode hin von dir mich riefen.", "tokens": ["Wie", "Fein\u00b7des\u00b7laut'", "zum", "To\u00b7de", "hin", "von", "dir", "mich", "rie\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "ADV", "APPR", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dann kam aus Streit und Angst ein s\u00fc\u00dfer Frieden,", "tokens": ["Dann", "kam", "aus", "Streit", "und", "Angst", "ein", "s\u00fc\u00b7\u00dfer", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Sonne brach die schwarzen Wolkengr\u00fcfte,", "tokens": ["Die", "Son\u00b7ne", "brach", "die", "schwar\u00b7zen", "Wol\u00b7ken\u00b7gr\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Welch s\u00fcsse Wonne war uns nun beschieden,", "tokens": ["Welch", "s\u00fcs\u00b7se", "Won\u00b7ne", "war", "uns", "nun", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie spielten um uns her die Blumend\u00fcfte,", "tokens": ["Wie", "spiel\u00b7ten", "um", "uns", "her", "die", "Blu\u00b7men\u00b7d\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPR", "PRF", "APZR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nur sanfte Freudenthr\u00e4nen", "tokens": ["Nur", "sanf\u00b7te", "Freu\u00b7dent\u00b7hr\u00e4\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Erlinderten mit heilger Fluth unendlich Sehnen.", "tokens": ["Er\u00b7lin\u00b7der\u00b7ten", "mit", "heil\u00b7ger", "Fluth", "un\u00b7end\u00b7lich", "Seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ADJD", "NN", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Da kam dein Blick; da lachten rothe Rosen,", "tokens": ["Da", "kam", "dein", "Blick", ";", "da", "lach\u00b7ten", "ro\u00b7the", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da sagtest du: ich bin nur dir gebohren!", "tokens": ["Da", "sag\u00b7test", "du", ":", "ich", "bin", "nur", "dir", "ge\u00b7boh\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Einheimisch gr\u00fc\u00dften wir Muthwill und Kosen;", "tokens": ["Ein\u00b7hei\u00b7misch", "gr\u00fc\u00df\u00b7ten", "wir", "Mut\u00b7hwill", "und", "Ko\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "VMFIN", "KON", "NN", "$."], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Wohin der Gram? Er war uns weit verlohren:", "tokens": ["Wo\u00b7hin", "der", "Gram", "?", "Er", "war", "uns", "weit", "ver\u00b7loh\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$.", "PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O Alma, S\u00fcsse, Holde,", "tokens": ["O", "Al\u00b7ma", ",", "S\u00fcs\u00b7se", ",", "Hol\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie zart umspann ein Liebesnetz uns licht von Golde.", "tokens": ["Wie", "zart", "um\u00b7spann", "ein", "Lie\u00b7bes\u00b7netz", "uns", "licht", "von", "Gol\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "PPER", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Aus Rosen kam zuerst dein s\u00fc\u00dfes Blicken,", "tokens": ["Aus", "Ro\u00b7sen", "kam", "zu\u00b7erst", "dein", "s\u00fc\u00b7\u00dfes", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In ihnen bl\u00fchten meine ersten K\u00fcsse;", "tokens": ["In", "ih\u00b7nen", "bl\u00fch\u00b7ten", "mei\u00b7ne", "ers\u00b7ten", "K\u00fcs\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie sollt' ich nicht dir heute Rosen schicken,", "tokens": ["Wie", "sollt'", "ich", "nicht", "dir", "heu\u00b7te", "Ro\u00b7sen", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PTKNEG", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df ihre R\u00f6the r\u00fchrend dich begr\u00fc\u00dfe:", "tokens": ["Da\u00df", "ih\u00b7re", "R\u00f6\u00b7the", "r\u00fch\u00b7rend", "dich", "be\u00b7gr\u00fc\u00b7\u00dfe", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "\u00bbliebst du noch, freundlich Wesen,", "tokens": ["\u00bb", "liebst", "du", "noch", ",", "freund\u00b7lich", "We\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Magst noch in unserm Scham-Err\u00f6then Sehnsucht lesen?\u00ab", "tokens": ["Magst", "noch", "in", "un\u00b7serm", "Scham\u00b7Er\u00b7r\u00f6\u00b7then", "Sehn\u00b7sucht", "le\u00b7sen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PPOSAT", "NN", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Doch Rosen hegt nicht Feld und Wies' und Garten,", "tokens": ["Doch", "Ro\u00b7sen", "hegt", "nicht", "Feld", "und", "Wies'", "und", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKNEG", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ach! schon verbl\u00fcht sind sie, die holden Zeichen!", "tokens": ["Ach", "!", "schon", "ver\u00b7bl\u00fcht", "sind", "sie", ",", "die", "hol\u00b7den", "Zei\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVPP", "VAFIN", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Blumen, wen von euch soll ich der Zarten", "tokens": ["Ihr", "Blu\u00b7men", ",", "wen", "von", "euch", "soll", "ich", "der", "Zar\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PWS", "APPR", "PPER", "VMFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Holdseeligen mit meinem Kusse reichen?", "tokens": ["Hold\u00b7see\u00b7li\u00b7gen", "mit", "mei\u00b7nem", "Kus\u00b7se", "rei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nein, kein' aus buntem Orden,", "tokens": ["Nein", ",", "kein'", "aus", "bun\u00b7tem", "Or\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIAT", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da uns die Ros', das sanfte Kind, untreu geworden.", "tokens": ["Da", "uns", "die", "Ros'", ",", "das", "sanf\u00b7te", "Kind", ",", "un\u00b7treu", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Nie welkt die Ros', die wir im Herzen tragen,", "tokens": ["Nie", "welkt", "die", "Ros'", ",", "die", "wir", "im", "Her\u00b7zen", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie duftet Lebenshauch in deinem Herzen;", "tokens": ["Sie", "duf\u00b7tet", "Le\u00b7bens\u00b7hauch", "in", "dei\u00b7nem", "Her\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bl\u00fch' auf, du Liebesblum', um ihr zu sagen,", "tokens": ["Bl\u00fch'", "auf", ",", "du", "Lie\u00b7bes\u00b7blum'", ",", "um", "ihr", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "NN", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie sie mein Heiligstes in Lust und Schmerzen,", "tokens": ["Wie", "sie", "mein", "Hei\u00b7ligs\u00b7tes", "in", "Lust", "und", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gie\u00df Thau der Thr\u00e4nen nieder, \u2013", "tokens": ["Gie\u00df", "Thau", "der", "Thr\u00e4\u00b7nen", "nie\u00b7der", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "PTKVZ", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "O schweigt, ihr spielnden T\u00f6n', bl\u00f6d' and\u00e4chtger Lieder.", "tokens": ["O", "schweigt", ",", "ihr", "spieln\u00b7den", "T\u00f6n'", ",", "bl\u00f6d'", "an\u00b7d\u00e4cht\u00b7ger", "Lie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Nur Thr\u00e4nen, Seufzer, Blicke durften sprechen,", "tokens": ["Nur", "Thr\u00e4\u00b7nen", ",", "Seuf\u00b7zer", ",", "Bli\u00b7cke", "durf\u00b7ten", "spre\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "$,", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur Ku\u00df in tiefer Inbrunst kann es wagen,", "tokens": ["Nur", "Ku\u00df", "in", "tie\u00b7fer", "In\u00b7brunst", "kann", "es", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In's Heiligthum der Liebe einzubrechen,", "tokens": ["In's", "Hei\u00b7lig\u00b7thum", "der", "Lie\u00b7be", "ein\u00b7zu\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von dem Geheimni\u00df etwas auszusagen;", "tokens": ["Von", "dem", "Ge\u00b7heim\u00b7ni\u00df", "et\u00b7was", "aus\u00b7zu\u00b7sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dann feiert Liebes-Wille", "tokens": ["Dann", "fei\u00b7ert", "Lie\u00b7bes\u00b7Wil\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["AD