{"textgrid.poem.47202": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Guten,", "tokens": ["Mei\u00b7ne", "Gu\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Meine Lieben,", "tokens": ["Mei\u00b7ne", "Lie\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Auf den Fluten", "tokens": ["Auf", "den", "Flu\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Fortgetrieben,", "tokens": ["Fort\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Auf den dunkeln Fluten fort", "tokens": ["Auf", "den", "dun\u00b7keln", "Flu\u00b7ten", "fort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nach dem Hafen", "tokens": ["Nach", "dem", "Ha\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Aus dem Meere,", "tokens": ["Aus", "dem", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.3": {"text": "Um zu schlafen", "tokens": ["Um", "zu", "schla\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "PTKZU", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Mit dem Heere,", "tokens": ["Mit", "dem", "Hee\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Das schon fand die Ruhe dort!", "tokens": ["Das", "schon", "fand", "die", "Ru\u00b7he", "dort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lasset euern", "tokens": ["Las\u00b7set", "eu\u00b7ern"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPOSAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Stern erwachen,", "tokens": ["Stern", "er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Um zu steuern", "tokens": ["Um", "zu", "steu\u00b7ern"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "PTKZU", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Meinen Nachen", "tokens": ["Mei\u00b7nen", "Na\u00b7chen"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Euerm sanften Glanze nach!", "tokens": ["Eu\u00b7erm", "sanf\u00b7ten", "Glan\u00b7ze", "nach", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie ich schiffe,", "tokens": ["Wie", "ich", "schif\u00b7fe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Seht, ich bebe,", "tokens": ["Seht", ",", "ich", "be\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Durchs Geriffe,", "tokens": ["Durchs", "Ge\u00b7rif\u00b7fe", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Durchs Gewebe", "tokens": ["Durchs", "Ge\u00b7we\u00b7be"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Dieser Klippen tausendfach.", "tokens": ["Die\u00b7ser", "Klip\u00b7pen", "tau\u00b7send\u00b7fach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Meine Lieben,", "tokens": ["Mei\u00b7ne", "Lie\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Meine Guten,", "tokens": ["Mei\u00b7ne", "Gu\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Fortgetrieben", "tokens": ["Fort\u00b7ge\u00b7trie\u00b7ben"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Auf den Fluten,", "tokens": ["Auf", "den", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Aus der Nacht hinaus zum Tag!", "tokens": ["Aus", "der", "Nacht", "hin\u00b7aus", "zum", "Tag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APZR", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Eure Bilder,", "tokens": ["Eu\u00b7re", "Bil\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Die mir schweben", "tokens": ["Die", "mir", "schwe\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Mild und milder", "tokens": ["Mild", "und", "mil\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "\u00dcberm Leben,", "tokens": ["\u00dc\u00b7berm", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Thun's, da\u00df ich noch leben mag.", "tokens": ["Thun's", ",", "da\u00df", "ich", "noch", "le\u00b7ben", "mag."], "token_info": ["word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["NE", "$,", "KOUS", "PPER", "ADV", "VVFIN", "NE"], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}