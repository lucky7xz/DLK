{"dta.poem.2274": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Rechensch\u00fcler.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Vater lehrt mich rechnen,", "tokens": ["Mein", "Va\u00b7ter", "lehrt", "mich", "rech\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er z\u00e4let Pfund und Taler;", "tokens": ["Er", "z\u00e4\u00b7let", "Pfund", "und", "Ta\u00b7ler", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich aber z\u00e4le M\u00e4dchens.", "tokens": ["Ich", "a\u00b7ber", "z\u00e4\u00b7le", "M\u00e4d\u00b7chens", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er sagt: Es sollen zwanzig,", "tokens": ["Er", "sagt", ":", "Es", "sol\u00b7len", "zwan\u00b7zig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VMFIN", "CARD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sich in zwei tausend teilen,", "tokens": ["Sich", "in", "zwei", "tau\u00b7send", "tei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "CARD", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gib iedem seine Winspel;", "tokens": ["Gib", "ie\u00b7dem", "sei\u00b7ne", "Wins\u00b7pel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich aber teile M\u00e4dchens,", "tokens": ["Ich", "a\u00b7ber", "tei\u00b7le", "M\u00e4d\u00b7chens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und gebe iedem hundert.", "tokens": ["Und", "ge\u00b7be", "ie\u00b7dem", "hun\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "CARD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ein Centner gilt zwey Gulden,", "tokens": ["Ein", "Cent\u00b7ner", "gilt", "zwey", "Gul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Er fr\u00e4gt: Was gelten zwanzig?", "tokens": ["Er", "fr\u00e4gt", ":", "Was", "gel\u00b7ten", "zwan\u00b7zig", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWS", "VVFIN", "CARD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Und meinet immer Centner;", "tokens": ["Und", "mei\u00b7net", "im\u00b7mer", "Cent\u00b7ner", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Ich aber meine M\u00e4dchens.", "tokens": ["Ich", "a\u00b7ber", "mei\u00b7ne", "M\u00e4d\u00b7chens", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Er fr\u00e4gt mich: Wenn du zwanzig", "tokens": ["Er", "fr\u00e4gt", "mich", ":", "Wenn", "du", "zwan\u00b7zig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "KOUS", "PPER", "CARD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Mit zwanzigen vermehrest,", "tokens": ["Mit", "zwan\u00b7zi\u00b7gen", "ver\u00b7meh\u00b7rest", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Wie viel betr\u00e4gt die Summe?", "tokens": ["Wie", "viel", "be\u00b7tr\u00e4gt", "die", "Sum\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Und wenn er mich so fr\u00e4get,", "tokens": ["Und", "wenn", "er", "mich", "so", "fr\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "So denk ich ans Vermehren", "tokens": ["So", "denk", "ich", "ans", "Ver\u00b7meh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Der Schwestern und der Br\u00fcder,", "tokens": ["Der", "Schwes\u00b7tern", "und", "der", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Und lache, wenn ich rechne.", "tokens": ["Und", "la\u00b7che", ",", "wenn", "ich", "rech\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}