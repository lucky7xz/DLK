{"textgrid.poem.23979": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Sonnenblicke", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leises Bl\u00e4tterrauschen rings,", "tokens": ["Lei\u00b7ses", "Bl\u00e4t\u00b7ter\u00b7rau\u00b7schen", "rings", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Traumhaft, wie im M\u00e4rchenwalde ...", "tokens": ["Traum\u00b7haft", ",", "wie", "im", "M\u00e4r\u00b7chen\u00b7wal\u00b7de", "..."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "APPRART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vogelsingen von den Zweigen,", "tokens": ["Vo\u00b7gel\u00b7sin\u00b7gen", "von", "den", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schmelzend bald in langgezogenen,", "tokens": ["Schmel\u00b7zend", "bald", "in", "lang\u00b7ge\u00b7zo\u00b7ge\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ADJA", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Schluchzenden T\u00f6nen, bald in lautem,", "tokens": ["Schluch\u00b7zen\u00b7den", "T\u00f6\u00b7nen", ",", "bald", "in", "lau\u00b7tem", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "APPR", "ADJA", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Hochaufschmetternden Jubelruf. \u2013", "tokens": ["Hoch\u00b7auf\u00b7schmet\u00b7tern\u00b7den", "Ju\u00b7bel\u00b7ruf", ".", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Leise der Wind weht ... Leise die D\u00fcfte", "tokens": ["Lei\u00b7se", "der", "Wind", "weht", "...", "Lei\u00b7se", "die", "D\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVFIN", "$(", "NN", "ART", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Ferner Blumen schwanken im Winde.", "tokens": ["Fer\u00b7ner", "Blu\u00b7men", "schwan\u00b7ken", "im", "Win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Schweigend kreisen Bl\u00fcten und Bl\u00e4tter", "tokens": ["Schwei\u00b7gend", "krei\u00b7sen", "Bl\u00fc\u00b7ten", "und", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Langsam nieder \u2013 fr\u00fchgewelkte;", "tokens": ["Lang\u00b7sam", "nie\u00b7der", "\u2013", "fr\u00fch\u00b7ge\u00b7welk\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$(", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Milde blickt mit tausend blauen", "tokens": ["Mil\u00b7de", "blickt", "mit", "tau\u00b7send", "blau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "CARD", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Augen durchs Ge\u00e4st der Himmel ...", "tokens": ["Au\u00b7gen", "durchs", "Ge\u00b7\u00e4st", "der", "Him\u00b7mel", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Blaue, milde, sch\u00f6ne Augen,", "tokens": ["Blau\u00b7e", ",", "mil\u00b7de", ",", "sch\u00f6\u00b7ne", "Au\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Feucht ergl\u00e4nzend in fraulicher G\u00fcte,", "tokens": ["Feucht", "er\u00b7gl\u00e4n\u00b7zend", "in", "frau\u00b7li\u00b7cher", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.15": {"text": "Haben mir tief in die Seele geleuchtet \u2013", "tokens": ["Ha\u00b7ben", "mir", "tief", "in", "die", "See\u00b7le", "ge\u00b7leuch\u00b7tet", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "APPR", "ART", "NN", "VVPP", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.16": {"text": "Sonnenblicke, Sonnenblicke ...", "tokens": ["Son\u00b7nen\u00b7bli\u00b7cke", ",", "Son\u00b7nen\u00b7bli\u00b7cke", "..."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Tr\u00fcb und dumpf, von Qual und Zweifel", "tokens": ["Tr\u00fcb", "und", "dumpf", ",", "von", "Qual", "und", "Zwei\u00b7fel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "$,", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Aufgestachelt und niedergedr\u00fcckt,", "tokens": ["Auf\u00b7ge\u00b7sta\u00b7chelt", "und", "nie\u00b7der\u00b7ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.19": {"text": "Schwankte mein Herz in \u00f6der Leere.", "tokens": ["Schwank\u00b7te", "mein", "Herz", "in", "\u00f6\u00b7der", "Lee\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.20": {"text": "Sehnsucht, Sehnsucht breitete aus,", "tokens": ["Sehn\u00b7sucht", ",", "Sehn\u00b7sucht", "brei\u00b7te\u00b7te", "aus", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.21": {"text": "Schlo\u00df und breitete wiederum", "tokens": ["Schlo\u00df", "und", "brei\u00b7te\u00b7te", "wie\u00b7de\u00b7rum"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.22": {"text": "Ihre d\u00fcrren Arme aus ...", "tokens": ["Ih\u00b7re", "d\u00fcr\u00b7ren", "Ar\u00b7me", "aus", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Tr\u00e4ume, nur Tr\u00e4ume kamen und schauerten", "tokens": ["Tr\u00e4u\u00b7me", ",", "nur", "Tr\u00e4u\u00b7me", "ka\u00b7men", "und", "schau\u00b7er\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "NN", "VVFIN", "KON", "VVFIN"], "meter": "+--+-+--+--", "measure": "iambic.tetra.invert"}, "line.24": {"text": "Holde Bilder in meine Seele,", "tokens": ["Hol\u00b7de", "Bil\u00b7der", "in", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.25": {"text": "Sch\u00f6nheitsvolle gl\u00fcckselige Bilder,", "tokens": ["Sch\u00f6n\u00b7heits\u00b7vol\u00b7le", "gl\u00fcck\u00b7se\u00b7li\u00b7ge", "Bil\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "Buntgestaltige, sch\u00f6n in Liebe, \u2013", "tokens": ["Bunt\u00b7ge\u00b7stal\u00b7ti\u00b7ge", ",", "sch\u00f6n", "in", "Lie\u00b7be", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "ADJD", "APPR", "NN", "$,", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.27": {"text": "Aber mit rauhem Griffe zerri\u00df", "tokens": ["A\u00b7ber", "mit", "rau\u00b7hem", "Grif\u00b7fe", "zer\u00b7ri\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.28": {"text": "Grausam kalt die unerbittliche,", "tokens": ["Grau\u00b7sam", "kalt", "die", "un\u00b7er\u00b7bitt\u00b7li\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "ADJA", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.29": {"text": "Grelle Wirklichkeit die schimmernden,", "tokens": ["Grel\u00b7le", "Wirk\u00b7lich\u00b7keit", "die", "schim\u00b7mern\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.30": {"text": "Und mein thr\u00e4nenloses Auge", "tokens": ["Und", "mein", "thr\u00e4\u00b7nen\u00b7lo\u00b7ses", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Sah in die Welt zu klar, zu klar. \u2013", "tokens": ["Sah", "in", "die", "Welt", "zu", "klar", ",", "zu", "klar", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKA", "ADJD", "$,", "PTKA", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Drinnen, tief im leeren Innern,", "tokens": ["Drin\u00b7nen", ",", "tief", "im", "lee\u00b7ren", "In\u00b7nern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Ewige Nebelnacht der Seele,", "tokens": ["E\u00b7wi\u00b7ge", "Ne\u00b7bel\u00b7nacht", "der", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.34": {"text": "Kalt und schweigend,", "tokens": ["Kalt", "und", "schwei\u00b7gend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.35": {"text": "Einsam,", "tokens": ["Ein\u00b7sam", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.36": {"text": "Todt \u2013:", "tokens": ["Todt", "\u2013", ":"], "token_info": ["word", "punct", "punct"], "pos": ["NN", "$(", "$."], "meter": "+", "measure": "single.up"}, "line.37": {"text": "Unkraut\u00fcberwucherter Friedhof", "tokens": ["Un\u00b7kraut\u00b7\u00fc\u00b7berw\u00b7uc\u00b7her\u00b7ter", "Fried\u00b7hof"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.38": {"text": "Hingestorbener Gef\u00fchle.", "tokens": ["Hin\u00b7ge\u00b7stor\u00b7be\u00b7ner", "Ge\u00b7f\u00fch\u00b7le", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.39": {"text": "Gr\u00e4\u00dfliche Ruhe. Ruhe des Scheintods;", "tokens": ["Gr\u00e4\u00df\u00b7li\u00b7che", "Ru\u00b7he", ".", "Ru\u00b7he", "des", "Schein\u00b7tods", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NN", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.40": {"text": "Stummes Krampfen, j\u00e4h unterbrochen", "tokens": ["Stum\u00b7mes", "Kramp\u00b7fen", ",", "j\u00e4h", "un\u00b7ter\u00b7bro\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJD", "VVPP"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.41": {"text": "Schmerzlich von zuckenden, heulenden St\u00f6\u00dfen", "tokens": ["Schmerz\u00b7lich", "von", "zu\u00b7cken\u00b7den", ",", "heu\u00b7len\u00b7den", "St\u00f6\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "APPR", "VVINF", "$,", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.42": {"text": "W\u00fchlenden Verzweiflungssturms.", "tokens": ["W\u00fch\u00b7len\u00b7den", "Ver\u00b7zwei\u00b7flungs\u00b7sturms", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Milde glanzvoll, feucht erschimmernd,", "tokens": ["Mil\u00b7de", "glanz\u00b7voll", ",", "feucht", "er\u00b7schim\u00b7mernd", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sonnenstrahlenklar und w\u00e4rmend", "tokens": ["Son\u00b7nen\u00b7strah\u00b7len\u00b7klar", "und", "w\u00e4r\u00b7mend"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Drang in dieses stumme Dunkel", "tokens": ["Drang", "in", "die\u00b7ses", "stum\u00b7me", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zweier Augen seliges Licht.", "tokens": ["Zwei\u00b7er", "Au\u00b7gen", "se\u00b7li\u00b7ges", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Helle wards. Und heiter weitete", "tokens": ["Hel\u00b7le", "wards", ".", "Und", "hei\u00b7ter", "wei\u00b7te\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VAFIN", "$.", "KON", "ADJD", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Sich das Herz im freundlichen Schimmer", "tokens": ["Sich", "das", "Herz", "im", "freund\u00b7li\u00b7chen", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Dieser Menschen-Sonnenblicke,", "tokens": ["Die\u00b7ser", "Men\u00b7schen\u00b7Son\u00b7nen\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und es keimte, schwellte, wuchs,", "tokens": ["Und", "es", "ke\u00b7im\u00b7te", ",", "schwell\u00b7te", ",", "wuchs", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Drangvoll, fr\u00fchlingsgl\u00e4ubig, selig", "tokens": ["Drang\u00b7voll", ",", "fr\u00fch\u00b7lings\u00b7gl\u00e4u\u00b7big", ",", "se\u00b7lig"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["ADV", "$,", "ADJD", "$,", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "In dem milden, warmen Lichte", "tokens": ["In", "dem", "mil\u00b7den", ",", "war\u00b7men", "Lich\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Hoch empor die Bl\u00fcte der Liebe.", "tokens": ["Hoch", "em\u00b7por", "die", "Bl\u00fc\u00b7te", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}