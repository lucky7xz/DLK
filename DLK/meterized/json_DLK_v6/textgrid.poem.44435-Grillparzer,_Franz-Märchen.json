{"textgrid.poem.44435": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "M\u00e4rchen", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In eines alten Turmes Schacht", "tokens": ["In", "ei\u00b7nes", "al\u00b7ten", "Tur\u00b7mes", "Schacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Liegt goldenhell ein Schatz,", "tokens": ["Liegt", "gol\u00b7den\u00b7hell", "ein", "Schatz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So reich, da\u00df, wer sein kundig ward,", "tokens": ["So", "reich", ",", "da\u00df", ",", "wer", "sein", "kun\u00b7dig", "ward", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "$,", "PWS", "PPOSAT", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00fcnscht sich des H\u00fcters Platz.", "tokens": ["W\u00fcnscht", "sich", "des", "H\u00fc\u00b7ters", "Platz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der H\u00fcter aber ist ein Drach,", "tokens": ["Der", "H\u00fc\u00b7ter", "a\u00b7ber", "ist", "ein", "Drach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der wahrt das edle Gut;", "tokens": ["Der", "wahrt", "das", "ed\u00b7le", "Gut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Goldgierig, geizig, wie er ist,", "tokens": ["Gold\u00b7gie\u00b7rig", ",", "gei\u00b7zig", ",", "wie", "er", "ist", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00e4lt Tag und Nacht er Hut.", "tokens": ["H\u00e4lt", "Tag", "und", "Nacht", "er", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Schuppen jed' ist ihm ein Aug,", "tokens": ["Der", "Schup\u00b7pen", "jed'", "ist", "ihm", "ein", "Aug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Kralle jedes Glied,", "tokens": ["Und", "Kral\u00b7le", "je\u00b7des", "Glied", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum sieht er, h\u00f6rt, h\u00e4lt ab, was vor,", "tokens": ["Drum", "sieht", "er", ",", "h\u00f6rt", ",", "h\u00e4lt", "ab", ",", "was", "vor", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "VVFIN", "$,", "VVFIN", "PTKVZ", "$,", "PWS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was hinter ihm geschieht.", "tokens": ["Was", "hin\u00b7ter", "ihm", "ge\u00b7schieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein Ritter aber, ohne Rast,", "tokens": ["Ein", "Rit\u00b7ter", "a\u00b7ber", ",", "oh\u00b7ne", "Rast", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Klimmt k\u00fchn den Berg empor.", "tokens": ["Klimmt", "k\u00fchn", "den", "Berg", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Umsonst! denn, wenn es halb gelang,", "tokens": ["Um\u00b7sonst", "!", "denn", ",", "wenn", "es", "halb", "ge\u00b7lang", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "$,", "KOUS", "PPER", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kommt ihm der Drach zuvor.", "tokens": ["Kommt", "ihm", "der", "Drach", "zu\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Schatz nun selber regt sich nicht,", "tokens": ["Der", "Schatz", "nun", "sel\u00b7ber", "regt", "sich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVFIN", "PRF", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie eben Sch\u00e4tze tun.", "tokens": ["Wie", "e\u00b7ben", "Sch\u00e4t\u00b7ze", "tun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Sch\u00f6ne ruht; der h\u00f6chste Preis,", "tokens": ["Das", "Sch\u00f6\u00b7ne", "ruht", ";", "der", "h\u00f6chs\u00b7te", "Preis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gleich ihm, in ihm zu ruhn.", "tokens": ["Gleich", "ihm", ",", "in", "ihm", "zu", "ruhn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.6": {"line.1": {"text": "Die Perle hat doch auch kein Ohr,", "tokens": ["Die", "Per\u00b7le", "hat", "doch", "auch", "kein", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Demant keinen Mund,", "tokens": ["Der", "De\u00b7mant", "kei\u00b7nen", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Blick des Goldes winkend nur", "tokens": ["Der", "Blick", "des", "Gol\u00b7des", "win\u00b7kend", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gibt Wunsch nach Freiheit kund.", "tokens": ["Gibt", "Wunsch", "nach", "Frei\u00b7heit", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So setzen sies schon lange fort,", "tokens": ["So", "set\u00b7zen", "sies", "schon", "lan\u00b7ge", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der H\u00fcter seinen Lauf,", "tokens": ["Der", "H\u00fc\u00b7ter", "sei\u00b7nen", "Lauf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das reiche Gut kommt nicht herab,", "tokens": ["Das", "rei\u00b7che", "Gut", "kommt", "nicht", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Sucher nicht hinauf.", "tokens": ["Der", "Su\u00b7cher", "nicht", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Nur f\u00fcrcht ich, w\u00e4hrt es allzulang,", "tokens": ["Nur", "f\u00fcrcht", "ich", ",", "w\u00e4hrt", "es", "all\u00b7zu\u00b7lang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erlahmt die Phantasie,", "tokens": ["Er\u00b7lahmt", "die", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und streift die bunten Farben ab,", "tokens": ["Und", "streift", "die", "bun\u00b7ten", "Far\u00b7ben", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die ihr das M\u00e4rchen lieh.", "tokens": ["Die", "ihr", "das", "M\u00e4r\u00b7chen", "lieh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der Drache geht dann schuppenlos,", "tokens": ["Der", "Dra\u00b7che", "geht", "dann", "schup\u00b7pen\u00b7los", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Ritter r\u00e4umt den Platz;", "tokens": ["Der", "Rit\u00b7ter", "r\u00e4umt", "den", "Platz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und nichts bleibt, was es fr\u00fcher war,", "tokens": ["Und", "nichts", "bleibt", ",", "was", "es", "fr\u00fc\u00b7her", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "PWS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als eines nur: der Schatz.", "tokens": ["Als", "ei\u00b7nes", "nur", ":", "der", "Schatz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "$.", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}