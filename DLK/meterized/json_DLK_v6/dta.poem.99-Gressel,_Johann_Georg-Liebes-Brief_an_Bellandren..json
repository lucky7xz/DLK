{"dta.poem.99": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Liebes-Brief an  Bellandren.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Darf ein verwehnter Kiehl die Schwelle \u00fcberschreiten?", "tokens": ["Darf", "ein", "ver\u00b7wehn\u00b7ter", "Kiehl", "die", "Schwel\u00b7le", "\u00fc\u00b7bersc\u00b7hrei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Woselbst ", "tokens": ["Wo\u00b7selbst"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Wo man ", "tokens": ["Wo", "man"], "token_info": ["word", "word"], "pos": ["PWAV", "PIS"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Wo des ", "tokens": ["Wo", "des"], "token_info": ["word", "word"], "pos": ["PWAV", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Allwo verbannet sind die s\u00fc\u00dften Schmeichelungen/", "tokens": ["All\u00b7wo", "ver\u00b7ban\u00b7net", "sind", "die", "s\u00fc\u00df\u00b7ten", "Schmei\u00b7che\u00b7lun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wo man verborgen Gifft nicht in ", "tokens": ["Wo", "man", "ver\u00b7bor\u00b7gen", "Gifft", "nicht", "in"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VVPP", "NN", "PTKNEG", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo man wies Hertze spricht/ er\u00f6ffnet durch die Zungen/", "tokens": ["Wo", "man", "wies", "Hert\u00b7ze", "spricht", "/", "er\u00b7\u00f6ff\u00b7net", "durch", "die", "Zun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "VVFIN", "VVFIN", "$(", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wo die ", "tokens": ["Wo", "die"], "token_info": ["word", "word"], "pos": ["PWAV", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Wo nichts denn Keuschheit bl\u00fcht/ und reiner Ehre Liljen/", "tokens": ["Wo", "nichts", "denn", "Keuschheit", "bl\u00fcht", "/", "und", "rei\u00b7ner", "Eh\u00b7re", "Lil\u00b7jen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "KON", "NN", "VVFIN", "$(", "KON", "ADJA", "NN", "NE", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Die/ ob gleich Zeit und Neid sich wie die Kr\u00f6ten bl\u00e4hn/", "tokens": ["Die", "/", "ob", "gleich", "Zeit", "und", "Neid", "sich", "wie", "die", "Kr\u00f6\u00b7ten", "bl\u00e4hn", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "KOUS", "ADV", "NN", "KON", "NN", "PRF", "KOKOM", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Kein gifftger L\u00e4ster-Bi\u00df ist m\u00e4chtig zu vertilgen/", "tokens": ["Kein", "gifft\u00b7ger", "L\u00e4s\u00b7ter\u00b7Bi\u00df", "ist", "m\u00e4ch\u00b7tig", "zu", "ver\u00b7til\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wo an der Tugend-Fels der Neid mu\u00df untergehn.", "tokens": ["Wo", "an", "der", "Tu\u00b7gen\u00b7dFels", "der", "Neid", "mu\u00df", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Denn wie der Monden lacht wenn tolle Hunde rasen/", "tokens": ["Denn", "wie", "der", "Mon\u00b7den", "lacht", "wenn", "tol\u00b7le", "Hun\u00b7de", "ra\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "KOUS", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So lacht die Tugend auch/ ob gleich der Neid sie dreut/", "tokens": ["So", "lacht", "die", "Tu\u00b7gend", "auch", "/", "ob", "gleich", "der", "Neid", "sie", "dreut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$(", "KOUS", "ADV", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sein Nebel wird gar bald durch einen Wind verblasen/", "tokens": ["Sein", "Ne\u00b7bel", "wird", "gar", "bald", "durch", "ei\u00b7nen", "Wind", "ver\u00b7bla\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der seine L\u00e4sterung wie leichtes Spreu zerstreut.", "tokens": ["Der", "sei\u00b7ne", "L\u00e4s\u00b7te\u00b7rung", "wie", "leich\u00b7tes", "Spreu", "zer\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "KOKOM", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Darf/ wo der ", "tokens": ["Darf", "/", "wo", "der"], "token_info": ["word", "punct", "word", "word"], "pos": ["VMFIN", "$(", "PWAV", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.18": {"text": "Das tausend Seuffzer hegt/ so die entflammte Brust", "tokens": ["Das", "tau\u00b7send", "Seuff\u00b7zer", "hegt", "/", "so", "die", "ent\u00b7flamm\u00b7te", "Brust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "CARD", "NN", "VVFIN", "$(", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "L\u00e4st als ein Weyrauch-Feur zu ihrer Gottheit steigen/", "tokens": ["L\u00e4st", "als", "ein", "Wey\u00b7rauch\u00b7Feur", "zu", "ih\u00b7rer", "Got\u00b7theit", "stei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Aus dem die G\u00f6tter ziehn die allergr\u00f6\u00dfte Lust.", "tokens": ["Aus", "dem", "die", "G\u00f6t\u00b7ter", "ziehn", "die", "al\u00b7ler\u00b7gr\u00f6\u00df\u00b7te", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Es bringt nicht Seuffzer blo\u00df/ mein Hertz sich selber stellet", "tokens": ["Es", "bringt", "nicht", "Seuff\u00b7zer", "blo\u00df", "/", "mein", "Hertz", "sich", "sel\u00b7ber", "stel\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "ADV", "$(", "PPOSAT", "NN", "PRF", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Zum Opffer vors Altar/ wo ihre Gottheit sitzt/", "tokens": ["Zum", "Opf\u00b7fer", "vors", "Al\u00b7tar", "/", "wo", "ih\u00b7re", "Got\u00b7theit", "sitzt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$(", "PWAV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wo ihr ", "tokens": ["Wo", "ihr"], "token_info": ["word", "word"], "pos": ["PWAV", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.24": {"text": "Wenn sie nur lieblich strahlt/ als wann die ", "tokens": ["Wenn", "sie", "nur", "lieb\u00b7lich", "strahlt", "/", "als", "wann", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$(", "KON", "PWAV", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Dis zeig\u2019 ich selber an mit eigenem Exempel/", "tokens": ["Dis", "zeig'", "ich", "sel\u00b7ber", "an", "mit", "ei\u00b7ge\u00b7nem", "Ex\u00b7em\u00b7pel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Denn da noch nie kein Mensch mich sonft verliebt gemacht/", "tokens": ["Denn", "da", "noch", "nie", "kein", "Mensch", "mich", "sonft", "ver\u00b7liebt", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "PIAT", "NN", "PPER", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So neig\u2019 ich mich doch hier vor ihren Ehren-Tempel/", "tokens": ["So", "neig'", "ich", "mich", "doch", "hier", "vor", "ih\u00b7ren", "Eh\u00b7ren\u00b7Tem\u00b7pel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Allwo mit Flammen spielt der Augen schwartze Nacht.", "tokens": ["All\u00b7wo", "mit", "Flam\u00b7men", "spielt", "der", "Au\u00b7gen", "schwart\u00b7ze", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "ART", "NN", "VVFIN", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.29": {"text": "Welch Sterblicher kan wol bey ihrer Allmacht pochen?", "tokens": ["Welch", "Sterb\u00b7li\u00b7cher", "kan", "wol", "bey", "ih\u00b7rer", "All\u00b7macht", "po\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der nicht/ wenn sie sich zeigt/ mu\u00df lauter Flammen seyn/", "tokens": ["Der", "nicht", "/", "wenn", "sie", "sich", "zeigt", "/", "mu\u00df", "lau\u00b7ter", "Flam\u00b7men", "seyn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "$(", "KOUS", "PPER", "PRF", "VVFIN", "$(", "VMFIN", "PIAT", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Dem nicht sein Stor-Kopff wird durch ihre Macht gebrochen/", "tokens": ["Dem", "nicht", "sein", "Stor\u00b7Kopff", "wird", "durch", "ih\u00b7re", "Macht", "ge\u00b7bro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "PPOSAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da\u00df er gezwungen mu\u00df ihr Weyrauch-K\u00f6rner sireun.", "tokens": ["Da\u00df", "er", "ge\u00b7zwun\u00b7gen", "mu\u00df", "ihr", "Wey\u00b7rauch\u00b7K\u00f6r\u00b7ner", "si\u00b7reun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.33": {"text": "Kan nun kein Sterblicher vor ihrer Macht bestehen", "tokens": ["Kan", "nun", "kein", "Sterb\u00b7li\u00b7cher", "vor", "ih\u00b7rer", "Macht", "be\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Was Wunder ist es dann/ da\u00df auch mein Hertze brennt/", "tokens": ["Was", "Wun\u00b7der", "ist", "es", "dann", "/", "da\u00df", "auch", "mein", "Hert\u00b7ze", "brennt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "PPER", "ADV", "$(", "KOUS", "ADV", "PPOSAT", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "An dem ja Fleisch und Blut als andern ist zu sehen/", "tokens": ["An", "dem", "ja", "Fleisch", "und", "Blut", "als", "an\u00b7dern", "ist", "zu", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "NN", "KON", "NN", "KOUS", "PIS", "VAFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Das/ z\u00fcrnt sie/ Sch\u00f6nste/ gleich/ sich doch ihr Sclave nennt.", "tokens": ["Das", "/", "z\u00fcrnt", "sie", "/", "Sch\u00f6ns\u00b7te", "/", "gleich", "/", "sich", "doch", "ihr", "Scla\u00b7ve", "nennt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "VVFIN", "PPER", "$(", "NN", "$(", "ADV", "$(", "PRF", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}