{"textgrid.poem.25064": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun aus der Urwelt tr\u00fcben D\u00e4mmerungen,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun aus der Urwelt tr\u00fcben D\u00e4mmerungen,", "tokens": ["Nun", "aus", "der", "Ur\u00b7welt", "tr\u00fc\u00b7ben", "D\u00e4m\u00b7me\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In die vor Menschenblick und Tageslicht", "tokens": ["In", "die", "vor", "Men\u00b7schen\u00b7blick", "und", "Ta\u00b7ges\u00b7licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dich die Jahrtausende hinabgeschlungen,", "tokens": ["Dich", "die", "Jahr\u00b7tau\u00b7sen\u00b7de", "hin\u00b7ab\u00b7ge\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Auf steigst du wieder; nein, du selber nicht \u2013", "tokens": ["Auf", "steigst", "du", "wie\u00b7der", ";", "nein", ",", "du", "sel\u00b7ber", "nicht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "ADV", "$.", "PTKANT", "$,", "PPER", "ADV", "PTKNEG", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Von jenem Troja, das Homer besungen,", "tokens": ["Von", "je\u00b7nem", "Tro\u00b7ja", ",", "das", "Ho\u00b7mer", "be\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NE", "$,", "PRELS", "NE", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Begraben in Ruinen, Schicht auf Schicht,", "tokens": ["Be\u00b7gra\u00b7ben", "in", "Ru\u00b7i\u00b7nen", ",", "Schicht", "auf", "Schicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ist, zu Atomen von der Zeit zerrieben,", "tokens": ["Ist", ",", "zu", "A\u00b7to\u00b7men", "von", "der", "Zeit", "zer\u00b7rie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein Aschenrest allein zur\u00fcckgeblieben.", "tokens": ["Ein", "A\u00b7schen\u00b7rest", "al\u00b7lein", "zu\u00b7r\u00fcck\u00b7ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Get\u00fcrmt, seitdem am rauschenden Skamander", "tokens": ["Ge\u00b7t\u00fcrmt", ",", "seit\u00b7dem", "am", "rau\u00b7schen\u00b7den", "Ska\u00b7man\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Priam stolzer K\u00f6nigsbau geragt,", "tokens": ["Des", "Pri\u00b7am", "stol\u00b7zer", "K\u00f6\u00b7nigs\u00b7bau", "ge\u00b7ragt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hier haben sich die Reiche aufeinander;", "tokens": ["Hier", "ha\u00b7ben", "sich", "die", "Rei\u00b7che", "auf\u00b7ein\u00b7an\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "ART", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das eine br\u00f6ckelte, zu Staub zernagt,", "tokens": ["Das", "ei\u00b7ne", "br\u00f6\u00b7ckel\u00b7te", ",", "zu", "Staub", "zer\u00b7nagt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dem andern nach, und schon als Alexander", "tokens": ["Dem", "an\u00b7dern", "nach", ",", "und", "schon", "als", "A\u00b7lex\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "PTKVZ", "$,", "KON", "ADV", "KOUS", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Am Grabesh\u00fcgel des Achill geklagt,", "tokens": ["Am", "Gra\u00b7bes\u00b7h\u00fc\u00b7gel", "des", "A\u00b7chill", "ge\u00b7klagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Versunken in das tr\u00fcmmer\u00fcbers\u00e4te", "tokens": ["Ver\u00b7sun\u00b7ken", "in", "das", "tr\u00fcm\u00b7mer\u00b7\u00fc\u00b7bers\u00b7\u00e4\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Blachfeld l\u00e4ngst warst du unter andre St\u00e4dte.", "tokens": ["Blach\u00b7feld", "l\u00e4ngst", "warst", "du", "un\u00b7ter", "and\u00b7re", "St\u00e4d\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die schwarze Spur von Qualm und Flammenbrand", "tokens": ["Die", "schwar\u00b7ze", "Spur", "von", "Qualm", "und", "Flam\u00b7men\u00b7brand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nur k\u00fcndet, da\u00df die Sage keine L\u00fcge", "tokens": ["Nur", "k\u00fcn\u00b7det", ",", "da\u00df", "die", "Sa\u00b7ge", "kei\u00b7ne", "L\u00fc\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von dem verheerten Ilion, das hier stand;", "tokens": ["Von", "dem", "ver\u00b7heer\u00b7ten", "I\u00b7li\u00b7on", ",", "das", "hier", "stand", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "---+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Dazwischen liegen Spangen, Thr\u00e4nenkr\u00fcge,", "tokens": ["Da\u00b7zwi\u00b7schen", "lie\u00b7gen", "Span\u00b7gen", ",", "Thr\u00e4\u00b7nen\u00b7kr\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "VVFIN", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Goldreife, die der K\u00f6n'ge Haupt umspannt,", "tokens": ["Gold\u00b7rei\u00b7fe", ",", "die", "der", "K\u00f6n'\u00b7ge", "Haupt", "um\u00b7spannt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zerst\u00fcckt sie all; und halberloschne Z\u00fcge", "tokens": ["Zer\u00b7st\u00fcckt", "sie", "all", ";", "und", "hal\u00b7ber\u00b7lo\u00b7schne", "Z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIAT", "$.", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Auf eh'rnen Opferschalen, die zerbrachen,", "tokens": ["Auf", "eh'r\u00b7nen", "Op\u00b7fer\u00b7scha\u00b7len", ",", "die", "zer\u00b7bra\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Noch stammeln stumm in lang verklungnen Sprachen.", "tokens": ["Noch", "stam\u00b7meln", "stumm", "in", "lang", "ver\u00b7klung\u00b7nen", "Spra\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch unten tiefer, wo sich selbst zum bleichen", "tokens": ["Doch", "un\u00b7ten", "tie\u00b7fer", ",", "wo", "sich", "selbst", "zum", "blei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "$,", "PWAV", "PRF", "ADV", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zwielicht die Nacht empor nicht ringen kann,", "tokens": ["Zwie\u00b7licht", "die", "Nacht", "em\u00b7por", "nicht", "rin\u00b7gen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ahn' ich den Staub von ganzer V\u00f6lker Leichen,", "tokens": ["Ahn'", "ich", "den", "Staub", "von", "gan\u00b7zer", "V\u00f6l\u00b7ker", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wie Verwesungsduft haucht es mich an", "tokens": ["Und", "wie", "Ver\u00b7we\u00b7sungs\u00b7duft", "haucht", "es", "mich", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "NN", "VVFIN", "PPER", "PRF", "APPR"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.5": {"text": "Von K\u00f6nigen, die kein Erinnrungszeichen", "tokens": ["Von", "K\u00f6\u00b7ni\u00b7gen", ",", "die", "kein", "E\u00b7rinn\u00b7rungs\u00b7zei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Auf Erden lie\u00dfen; eh dein Tag begann,", "tokens": ["Auf", "Er\u00b7den", "lie\u00b7\u00dfen", ";", "eh", "dein", "Tag", "be\u00b7gann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$.", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Verklungen war selbst in der Sagen Munde", "tokens": ["Ver\u00b7klun\u00b7gen", "war", "selbst", "in", "der", "Sa\u00b7gen", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Von ihnen und von ihrem Reich die Kunde.", "tokens": ["Von", "ih\u00b7nen", "und", "von", "ih\u00b7rem", "Reich", "die", "Kun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wer mag, wie tief die Gr\u00e4ber reichen, wissen?", "tokens": ["Wer", "mag", ",", "wie", "tief", "die", "Gr\u00e4\u00b7ber", "rei\u00b7chen", ",", "wis\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VMFIN", "$,", "PWAV", "ADJD", "ART", "NN", "VVINF", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "W\u00e4r' uns zu F\u00fc\u00dfen eine Riesenkluft", "tokens": ["W\u00e4r'", "uns", "zu", "F\u00fc\u00b7\u00dfen", "ei\u00b7ne", "Rie\u00b7sen\u00b7kluft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hinab bis in der Erde Herz gerissen,", "tokens": ["Hin\u00b7ab", "bis", "in", "der", "Er\u00b7de", "Herz", "ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wir s\u00e4hen eine ungeheure Gruft,", "tokens": ["Wir", "s\u00e4\u00b7hen", "ei\u00b7ne", "un\u00b7ge\u00b7heu\u00b7re", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und noch bis aus den tiefsten Finsternissen", "tokens": ["Und", "noch", "bis", "aus", "den", "tiefs\u00b7ten", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Entgegen qu\u00f6ll' uns feuchte Grabesluft", "tokens": ["Ent\u00b7ge\u00b7gen", "qu\u00f6ll'", "uns", "feuch\u00b7te", "Gra\u00b7bes\u00b7luft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und Moderdunst der stummen, unz\u00e4hlbaren", "tokens": ["Und", "Mo\u00b7der\u00b7dunst", "der", "stum\u00b7men", ",", "un\u00b7z\u00e4hl\u00b7ba\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "NN", "ART", "ADJA", "$,", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Geschlechter, die vor uns auf Erden waren.", "tokens": ["Ge\u00b7schlech\u00b7ter", ",", "die", "vor", "uns", "auf", "Er\u00b7den", "wa\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "PPER", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Mir ist, als h\u00f6rt' ich durch verschollne Tage,", "tokens": ["Mir", "ist", ",", "als", "h\u00f6rt'", "ich", "durch", "ver\u00b7scholl\u00b7ne", "Ta\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den schwarzen Abgrund namenloser Zeiten,", "tokens": ["Den", "schwar\u00b7zen", "Ab\u00b7grund", "na\u00b7men\u00b7lo\u00b7ser", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die keiner kennt, mit leisem Fl\u00fcgelschlage", "tokens": ["Die", "kei\u00b7ner", "kennt", ",", "mit", "lei\u00b7sem", "Fl\u00fc\u00b7gel\u00b7schla\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Tod hin ob der V\u00f6lker H\u00e4uptern gleiten,", "tokens": ["Den", "Tod", "hin", "ob", "der", "V\u00f6l\u00b7ker", "H\u00e4up\u00b7tern", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KOUS", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Als sch\u00f6ll' ans Ohr mir ihre Sterbeklage,", "tokens": ["Als", "sch\u00f6ll'", "ans", "Ohr", "mir", "ih\u00b7re", "Ster\u00b7be\u00b7kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPRART", "NN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie sie im Trauerzug vor\u00fcberschreiten", "tokens": ["Wie", "sie", "im", "Trau\u00b7er\u00b7zug", "vor\u00b7\u00fc\u00b7bersc\u00b7hrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und in das dunkle Reich, die weiten Hallen,", "tokens": ["Und", "in", "das", "dunk\u00b7le", "Reich", ",", "die", "wei\u00b7ten", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die allen aufgethan, hinunterwallen.", "tokens": ["Die", "al\u00b7len", "auf\u00b7ge\u00b7than", ",", "hin\u00b7un\u00b7ter\u00b7wal\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIS", "VVPP", "$,", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und ob die Zukunft z