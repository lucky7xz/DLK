{"dta.poem.12658": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Lisimene an den Damon,  \n Als er sich in eine andere verliebet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verzeihe, liebster Schatz! dem k\u00fchnen unterfaugen,", "tokens": ["Ver\u00b7zei\u00b7he", ",", "liebs\u00b7ter", "Schatz", "!", "dem", "k\u00fch\u00b7nen", "un\u00b7ter\u00b7fau\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Womit zum andern mahl dich meine feder st\u00f6hrt;", "tokens": ["Wo\u00b7mit", "zum", "an\u00b7dern", "mahl", "dich", "mei\u00b7ne", "fe\u00b7der", "st\u00f6hrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "ADJA", "ADV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "La\u00df die gewi\u00dfheit sie von deiner gunst erlangen,", "tokens": ["La\u00df", "die", "ge\u00b7wi\u00df\u00b7heit", "sie", "von", "dei\u00b7ner", "gunst", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und h\u00f6re gn\u00e4dig an, was deine magd begehrt.", "tokens": ["Und", "h\u00f6\u00b7re", "gn\u00e4\u00b7dig", "an", ",", "was", "dei\u00b7ne", "magd", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du weist, ich liebe dich mit recht getreuen hertzen,", "tokens": ["Du", "weist", ",", "ich", "lie\u00b7be", "dich", "mit", "recht", "ge\u00b7treu\u00b7en", "hert\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PRF", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und bete dein gesicht als meinen abgott an;", "tokens": ["Und", "be\u00b7te", "dein", "ge\u00b7sicht", "als", "mei\u00b7nen", "ab\u00b7gott", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJD", "KOKOM", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich opffre dir allein die flammen meiner kertzen,", "tokens": ["Ich", "opff\u00b7re", "dir", "al\u00b7lein", "die", "flam\u00b7men", "mei\u00b7ner", "kert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die auch kein geiler wind iemals verblasen kan.", "tokens": ["Die", "auch", "kein", "gei\u00b7ler", "wind", "ie\u00b7mals", "ver\u00b7bla\u00b7sen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VAFIN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mein geist wei\u00df sich nach dir als seinem pol zu regen,", "tokens": ["Mein", "geist", "wei\u00df", "sich", "nach", "dir", "als", "sei\u00b7nem", "pol", "zu", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "PPER", "KOUS", "PPOSAT", "NN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Daran die feste treu als am magnete klebt;", "tokens": ["Da\u00b7ran", "die", "fes\u00b7te", "treu", "als", "am", "mag\u00b7ne\u00b7te", "klebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "ADJD", "KOKOM", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Es f\u00fchrt mich deine hand auf meinen liebes-wegen,", "tokens": ["Es", "f\u00fchrt", "mich", "dei\u00b7ne", "hand", "auf", "mei\u00b7nen", "lie\u00b7bes\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wo gar kein irrlicht-schein vor meinen augen schwebt.", "tokens": ["Wo", "gar", "kein", "irr\u00b7licht\u00b7schein", "vor", "mei\u00b7nen", "au\u00b7gen", "schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und dennoch will ein traum mir meine ruhe st\u00f6hren,", "tokens": ["Und", "den\u00b7noch", "will", "ein", "traum", "mir", "mei\u00b7ne", "ru\u00b7he", "st\u00f6h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "ART", "NN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wenn er best\u00e4ndigkeit mit wasser-farben mahlt;", "tokens": ["Wenn", "er", "be\u00b7st\u00e4n\u00b7dig\u00b7keit", "mit", "was\u00b7ser\u00b7fa\u00b7rben", "mahlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es darff fast, wie mich d\u00fcnckt mir dieses sinnbild lehren,", "tokens": ["Es", "darff", "fast", ",", "wie", "mich", "d\u00fcnckt", "mir", "die\u00b7ses", "sinn\u00b7bild", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "PWAV", "PPER", "VVFIN", "PPER", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df \u00fcber lieb und lufft ein gleicher einflu\u00df strahlt.", "tokens": ["Da\u00df", "\u00fc\u00b7ber", "lieb", "und", "lufft", "ein", "glei\u00b7cher", "ein\u00b7flu\u00df", "strahlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJD", "KON", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Mein Engel! z\u00fcrne nicht, was ich dir hier geschrieben,", "tokens": ["Mein", "En\u00b7gel", "!", "z\u00fcr\u00b7ne", "nicht", ",", "was", "ich", "dir", "hier", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Das ist ein hei\u00dfer trieb, den liebe z\u00e4rtlich heist;", "tokens": ["Das", "ist", "ein", "hei\u00b7\u00dfer", "trieb", ",", "den", "lie\u00b7be", "z\u00e4rt\u00b7lich", "heist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "VVFIN", "$,", "ART", "VVFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Du wirst, ich wei\u00df gewi\u00df, nicht deine magd betr\u00fcben,", "tokens": ["Du", "wirst", ",", "ich", "wei\u00df", "ge\u00b7wi\u00df", ",", "nicht", "dei\u00b7ne", "magd", "be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VVFIN", "ADV", "$,", "PTKNEG", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die dir als sclavin stets noch ihre treu erweist.", "tokens": ["Die", "dir", "als", "scla\u00b7vin", "stets", "noch", "ih\u00b7re", "treu", "er\u00b7weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOUS", "NN", "ADV", "ADV", "PPOSAT", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ach Liebster! la\u00df dich nicht von frembder sch\u00f6nheit blenden,", "tokens": ["Ach", "Liebs\u00b7ter", "!", "la\u00df", "dich", "nicht", "von", "fremb\u00b7der", "sch\u00f6n\u00b7heit", "blen\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "VVIMP", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und h\u00f6re nicht so bald nach der syrenen klang;", "tokens": ["Und", "h\u00f6\u00b7re", "nicht", "so", "bald", "nach", "der", "sy\u00b7re\u00b7nen", "klang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "ADV", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Du kommest nimmermehr aus ihren m\u00f6rder-h\u00e4nden", "tokens": ["Du", "kom\u00b7mest", "nim\u00b7mer\u00b7mehr", "aus", "ih\u00b7ren", "m\u00f6r\u00b7der\u00b7h\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wann deine lippen schon gekost den wermuths-tranck.", "tokens": ["Wann", "dei\u00b7ne", "lip\u00b7pen", "schon", "ge\u00b7kost", "den", "wer\u00b7muths\u00b7tranck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Will sie gleich immer dich mit falschem wildpret locken,", "tokens": ["Will", "sie", "gleich", "im\u00b7mer", "dich", "mit", "fal\u00b7schem", "wild\u00b7pret", "lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "PRF", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Stellt ihre listigkeit dir garn und netze auf;", "tokens": ["Stellt", "ih\u00b7re", "lis\u00b7tig\u00b7keit", "dir", "garn", "und", "net\u00b7ze", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So rei\u00dfe dich hindurch behend und unerschrocken,", "tokens": ["So", "rei\u00b7\u00dfe", "dich", "hin\u00b7durch", "be\u00b7hend", "und", "un\u00b7er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PAV", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und folge nur allein der edlen tugend lauff.", "tokens": ["Und", "fol\u00b7ge", "nur", "al\u00b7lein", "der", "ed\u00b7len", "tu\u00b7gend", "lauff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Indessen bleibt mein hertz in keuschen schrancken stehen,", "tokens": ["In\u00b7des\u00b7sen", "bleibt", "mein", "hertz", "in", "keu\u00b7schen", "schran\u00b7cken", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und meine zuversicht schleust dieses zeugni\u00df ein:", "tokens": ["Und", "mei\u00b7ne", "zu\u00b7ver\u00b7sicht", "schleust", "die\u00b7ses", "zeug\u00b7ni\u00df", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Es wird der erden-bau in st\u00fcck und dr\u00fcmmer gehen", "tokens": ["Es", "wird", "der", "er\u00b7den\u00b7bau", "in", "st\u00fcck", "und", "dr\u00fcm\u00b7mer", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "KON", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Als Lisimene dir, mein Damon! untreu seyn.", "tokens": ["Als", "Li\u00b7si\u00b7me\u00b7ne", "dir", ",", "mein", "Da\u00b7mon", "!", "un\u00b7treu", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "$,", "PPOSAT", "NN", "$.", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}