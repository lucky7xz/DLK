{"dta.poem.2377": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Andrer Satz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Dr\u00fcm wolauf du junges Volk/ geh/ eyl zum Tantzen/", "tokens": ["Dr\u00fcm", "wo\u00b7lauf", "du", "jun\u00b7ges", "Volk", "/", "geh", "/", "eyl", "zum", "Tant\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADJA", "NN", "$(", "VVFIN", "$(", "NN", "APPRART", "NN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Sieh dich vor damit du nicht versiehst die Schantzen/", "tokens": ["Sieh", "dich", "vor", "da\u00b7mit", "du", "nicht", "ver\u00b7siehst", "die", "Schant\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "KOUS", "PPER", "PTKNEG", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Brauch in deinem jungen Jahre", "tokens": ["Brauch", "in", "dei\u00b7nem", "jun\u00b7gen", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keusche Liebeskunst/", "tokens": ["Keu\u00b7sche", "Lie\u00b7bes\u00b7kunst", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Kommen dir die grauen Haare/", "tokens": ["Kom\u00b7men", "dir", "die", "grau\u00b7en", "Haa\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dann so ists \u00fcmsonst.", "tokens": ["Dann", "so", "ists", "\u00fcm\u00b7sonst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Hast du wo ein sch\u00f6nes Bild in deinem Hertzen/", "tokens": ["Hast", "du", "wo", "ein", "sch\u00f6\u00b7nes", "Bild", "in", "dei\u00b7nem", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PWAV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Wol! entdekk ihr deine Lieb\u2019 und deine Schmertzen/", "tokens": ["Wol", "!", "ent\u00b7dekk", "ihr", "dei\u00b7ne", "Lieb'", "und", "dei\u00b7ne", "Schmert\u00b7zen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Aber zeume die Gedanken/", "tokens": ["A\u00b7ber", "zeu\u00b7me", "die", "Ge\u00b7dan\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit der Tugendzaum/", "tokens": ["Mit", "der", "Tu\u00b7gend\u00b7zaum", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Gib dem geilen Sinnenwanken/", "tokens": ["Gib", "dem", "gei\u00b7len", "Sin\u00b7nen\u00b7wan\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Keinen Platz und Raum.", "tokens": ["Kei\u00b7nen", "Platz", "und", "Raum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Brauch die Liebesmittel die zum Vortrab taugen/", "tokens": ["Brauch", "die", "Lie\u00b7bes\u00b7mit\u00b7tel", "die", "zum", "Vor\u00b7trab", "tau\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "APPRART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "F\u00fchre Sie fein sanfft her\u00fcm/ red mit den Augen/", "tokens": ["F\u00fch\u00b7re", "Sie", "fein", "sanfft", "he\u00b7r\u00fcm", "/", "red", "mit", "den", "Au\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "VVFIN", "PTKVZ", "$(", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "+--++--+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Giebt Sie dir ein Liebesblikken/", "tokens": ["Giebt", "Sie", "dir", "ein", "Lie\u00b7bes\u00b7blik\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gib ihr zu verstehn", "tokens": ["Gib", "ihr", "zu", "ver\u00b7stehn"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Mit dem Handkuss\u2019- oder Dr\u00fckken", "tokens": ["Mit", "dem", "Hand\u00b7kuss'", "o\u00b7der", "Dr\u00fck\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "TRUNC", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df du\u2019s hast gesehn.", "tokens": ["Da\u00df", "du's", "hast", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VAFIN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "F\u00fchre deinen Tantzgeno\u00df vom Tantzen wieder/", "tokens": ["F\u00fch\u00b7re", "dei\u00b7nen", "Tantz\u00b7ge\u00b7no\u00df", "vom", "Tant\u00b7zen", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPRART", "NN", "ADV", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Nenn Sie dein hertzliebstes Kind/ und setz dich nieder/", "tokens": ["Nenn", "Sie", "dein", "hertz\u00b7liebs\u00b7tes", "Kind", "/", "und", "setz", "dich", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$(", "KON", "VVIMP", "PPER", "PTKVZ", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Offenbar ihr dein Verlangen/", "tokens": ["Of\u00b7fen\u00b7bar", "ihr", "dein", "Ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie der Breutgam that.", "tokens": ["Wie", "der", "Breut\u00b7gam", "that", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NE", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wil dann dieses nichts verfangen/", "tokens": ["Wil", "dann", "die\u00b7ses", "nichts", "ver\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PDAT", "PIS", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wei\u00df ich keinen Raht.", "tokens": ["Wei\u00df", "ich", "kei\u00b7nen", "Raht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}