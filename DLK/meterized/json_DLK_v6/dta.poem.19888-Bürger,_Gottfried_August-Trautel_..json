{"dta.poem.19888": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Trautel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Trautel h\u00e4lt mich f\u00fcr und f\u00fcr               ", "tokens": ["Mein", "Trau\u00b7tel", "h\u00e4lt", "mich", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "KON", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In festen Liebesbanden;", "tokens": ["In", "fes\u00b7ten", "Lie\u00b7bes\u00b7ban\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bin immer um und neben ihr;", "tokens": ["Bin", "im\u00b7mer", "um", "und", "ne\u00b7ben", "ihr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKVZ", "KON", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie l\u00e4st mich nicht abhanden.", "tokens": ["Sie", "l\u00e4st", "mich", "nicht", "ab\u00b7han\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich darf nicht weiter, als das Band,", "tokens": ["Ich", "darf", "nicht", "wei\u00b7ter", ",", "als", "das", "Band", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "PTKVZ", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Woran sie mich gebunden.", "tokens": ["Wo\u00b7ran", "sie", "mich", "ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sie g\u00e4ngelt mich an ihrer Hand", "tokens": ["Sie", "g\u00e4n\u00b7gelt", "mich", "an", "ih\u00b7rer", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wol Tag f\u00fcr Tag zw\u00f6lf Stunden.", "tokens": ["Wol", "Tag", "f\u00fcr", "Tag", "zw\u00f6lf", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mein Trautel h\u00e4lt mich f\u00fcr und f\u00fcr", "tokens": ["Mein", "Trau\u00b7tel", "h\u00e4lt", "mich", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "KON", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In ihrer stillen Klause.", "tokens": ["In", "ih\u00b7rer", "stil\u00b7len", "Klau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Darf nie zum Tanz, als nur mit ihr,", "tokens": ["Darf", "nie", "zum", "Tanz", ",", "als", "nur", "mit", "ihr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPRART", "NN", "$,", "KOUS", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nie ohne sie zum Schmause.", "tokens": ["Nie", "oh\u00b7ne", "sie", "zum", "Schmau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und ich bin gar ein guter Man,", "tokens": ["Und", "ich", "bin", "gar", "ein", "gu\u00b7ter", "Man", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ART", "ADJA", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der sie nur sieht und h\u00f6ret,", "tokens": ["Der", "sie", "nur", "sieht", "und", "h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und aus den Augen lesen kan,", "tokens": ["Und", "aus", "den", "Au\u00b7gen", "le\u00b7sen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Was sie befielt und wehret.", "tokens": ["Was", "sie", "be\u00b7fielt", "und", "weh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich, Trautel, bin wol recht f\u00fcr dich,", "tokens": ["Ich", ",", "Trau\u00b7tel", ",", "bin", "wol", "recht", "f\u00fcr", "dich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VAFIN", "ADV", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und du f\u00fcr mich geboren.", "tokens": ["Und", "du", "f\u00fcr", "mich", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O Trautel, ohne dich und mich,", "tokens": ["O", "Trau\u00b7tel", ",", "oh\u00b7ne", "dich", "und", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUI", "PPER", "KON", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sind ich und du verloren. \u2014", "tokens": ["Sind", "ich", "und", "du", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "KON", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wann einst des Todes Sense klirt,", "tokens": ["Wann", "einst", "des", "To\u00b7des", "Sen\u00b7se", "klirt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und m\u00e4het mich von hinnen,", "tokens": ["Und", "m\u00e4\u00b7het", "mich", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ach! lieber lieber Gott! was wird", "tokens": ["Ach", "!", "lie\u00b7ber", "lie\u00b7ber", "Gott", "!", "was", "wird"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$.", "ADV", "ADV", "NN", "$.", "PWS", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Trautel doch beginnen?", "tokens": ["Mein", "Trau\u00b7tel", "doch", "be\u00b7gin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}