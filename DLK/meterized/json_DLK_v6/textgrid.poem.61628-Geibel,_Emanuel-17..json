{"textgrid.poem.61628": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heute w\u00e4r' ich fast erschrocken", "tokens": ["Heu\u00b7te", "w\u00e4r'", "ich", "fast", "er\u00b7schro\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir zu F\u00fc\u00dfen hingest\u00fcrzt,", "tokens": ["Dir", "zu", "F\u00fc\u00b7\u00dfen", "hin\u00b7ge\u00b7st\u00fcrzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als du pl\u00f6tzlich deiner Locken", "tokens": ["Als", "du", "pl\u00f6tz\u00b7lich", "dei\u00b7ner", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wilden Reichtum losgesch\u00fcrzt.", "tokens": ["Wil\u00b7den", "Reich\u00b7tum", "los\u00b7ge\u00b7sch\u00fcrzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gl\u00e4nzend um die schlanken Glieder", "tokens": ["Gl\u00e4n\u00b7zend", "um", "die", "schlan\u00b7ken", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wallt' ihr fesselloser Schwall", "tokens": ["Wallt'", "ihr", "fes\u00b7sel\u00b7lo\u00b7ser", "Schwall"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf des Teppichs Purpur nieder", "tokens": ["Auf", "des", "Tep\u00b7pichs", "Pur\u00b7pur", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein schwarzer Wasserfall.", "tokens": ["Wie", "ein", "schwar\u00b7zer", "Was\u00b7ser\u00b7fall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, und als du nun die braunen", "tokens": ["Ach", ",", "und", "als", "du", "nun", "die", "brau\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KON", "KOUS", "PPER", "ADV", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "R\u00e4tselaugen aufw\u00e4rts schlugst", "tokens": ["R\u00e4t\u00b7se\u00b7lau\u00b7gen", "auf\u00b7w\u00e4rts", "schlugst"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in reizendem Erstaunen,", "tokens": ["Und", "in", "rei\u00b7zen\u00b7dem", "Er\u00b7stau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was mich so verwirre, frugst,", "tokens": ["Was", "mich", "so", "ver\u00b7wir\u00b7re", ",", "frugst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$,", "ADJD", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}}, "stanza.4": {"line.1": {"text": "Als du dann zum Spiegel h\u00fcpftest", "tokens": ["Als", "du", "dann", "zum", "Spie\u00b7gel", "h\u00fcpf\u00b7test"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Schnur von Perlen dir", "tokens": ["Und", "die", "Schnur", "von", "Per\u00b7len", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "T\u00e4ndelnd um die Stirne kn\u00fcpftest \u2013", "tokens": ["T\u00e4n\u00b7delnd", "um", "die", "Stir\u00b7ne", "kn\u00fcpf\u00b7test", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O wie sch\u00f6n erschienst du mir!", "tokens": ["O", "wie", "sch\u00f6n", "er\u00b7schienst", "du", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Lauschend, keines Wortes m\u00e4chtig", "tokens": ["Lau\u00b7schend", ",", "kei\u00b7nes", "Wor\u00b7tes", "m\u00e4ch\u00b7tig"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "PIAT", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stand ich, atemlos gebannt,", "tokens": ["Stand", "ich", ",", "a\u00b7tem\u00b7los", "ge\u00b7bannt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie verzaubert in ein pr\u00e4chtig", "tokens": ["Wie", "ver\u00b7zau\u00b7bert", "in", "ein", "pr\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "ART", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00e4rchen aus dem Morgenland.", "tokens": ["M\u00e4r\u00b7chen", "aus", "dem", "Mor\u00b7gen\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}