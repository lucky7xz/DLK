", ",", "war", "ein", "Schwert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "$,", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.20": {"line.1": {"text": "Als Pflugscharr soll's fortan durch Schollen ringen,", "tokens": ["Als", "Pflug\u00b7scharr", "soll's", "for\u00b7tan", "durch", "Schol\u00b7len", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VMFIN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Saatkorn nur noch weist's den Weg zur Gruft;", "tokens": ["Dem", "Saat\u00b7korn", "nur", "noch", "weist's", "den", "Weg", "zur", "Gruft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Schwertes neue Heldenthaten singen", "tokens": ["Des", "Schwer\u00b7tes", "neu\u00b7e", "Hel\u00b7den\u00b7tha\u00b7ten", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Lerchen Epopee'n in sonn'ger Luft!", "tokens": ["Der", "Ler\u00b7chen", "E\u00b7po\u00b7pee'n", "in", "sonn'\u00b7ger", "Luft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.21": {"line.1": {"text": "Einst wieder sich's begab, da\u00df, als er pfl\u00fcgte,", "tokens": ["Einst", "wie\u00b7der", "sich's", "be\u00b7gab", ",", "da\u00df", ",", "als", "er", "pfl\u00fcg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct",