{"textgrid.poem.40849": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und Ostern wird es einst, der Herr sieht nieder", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und Ostern wird es einst, der Herr sieht nieder", "tokens": ["Und", "Os\u00b7tern", "wird", "es", "einst", ",", "der", "Herr", "sieht", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "PPER", "ADV", "$,", "ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Oelberg in das Thal, das klingt und bl\u00fcht;", "tokens": ["Vom", "O\u00b7el\u00b7berg", "in", "das", "Thal", ",", "das", "klingt", "und", "bl\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,", "PDS", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Rings Glanz und F\u00fcll' und Wonn' und Wonne wieder,", "tokens": ["Rings", "Glanz", "und", "F\u00fcll'", "und", "Wonn'", "und", "Won\u00b7ne", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So weit sein Aug' \u2013 ein Gottesauge \u2013 sieht!", "tokens": ["So", "weit", "sein", "Aug'", "\u2013", "ein", "Got\u00b7tes\u00b7au\u00b7ge", "\u2013", "sieht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "$(", "ART", "NN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Ostern, wie's der Dichtergeist sieht bl\u00fchen,", "tokens": ["Ein", "Os\u00b7tern", ",", "wie's", "der", "Dich\u00b7ter\u00b7geist", "sieht", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem's schon zu schaun, zu pfl\u00fccken jetzt erlaubt", "tokens": ["Dem's", "schon", "zu", "schaun", ",", "zu", "pfl\u00fc\u00b7cken", "jetzt", "er\u00b7laubt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Bl\u00fcthenkr\u00e4nze, die als Kron' einst gl\u00fchen", "tokens": ["Die", "Bl\u00fc\u00b7then\u00b7kr\u00e4n\u00b7ze", ",", "die", "als", "Kron'", "einst", "gl\u00fc\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "KOUS", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um der noch ungebornen Tage Haupt!", "tokens": ["Um", "der", "noch", "un\u00b7ge\u00b7bor\u00b7nen", "Ta\u00b7ge", "Haupt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADV", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein Ostern, wie's das Dichteraug' sieht tagen,", "tokens": ["Ein", "Os\u00b7tern", ",", "wie's", "das", "Dich\u00b7ter\u00b7aug'", "sieht", "ta\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das \u00fcber'm Nebel, der das Jetzt umzieht,", "tokens": ["Das", "\u00fc\u00b7ber'm", "Ne\u00b7bel", ",", "der", "das", "Jetzt", "um\u00b7zieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "NN", "$,", "PRELS", "ART", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die morgenrothen Gletscherh\u00e4upter ragen", "tokens": ["Die", "mor\u00b7gen\u00b7ro\u00b7then", "Glet\u00b7scher\u00b7h\u00e4up\u00b7ter", "ra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der werdenden Jahrtausende schon sieht!", "tokens": ["Der", "wer\u00b7den\u00b7den", "Jahr\u00b7tau\u00b7sen\u00b7de", "schon", "sieht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Ein Ostern, Auferstehungsfest, das wieder", "tokens": ["Ein", "Os\u00b7tern", ",", "Auf\u00b7er\u00b7ste\u00b7hungs\u00b7fest", ",", "das", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Fr\u00fchlings Hauch auf Blumengr\u00e4ber s\u00e4't;", "tokens": ["Des", "Fr\u00fch\u00b7lings", "Hauch", "auf", "Blu\u00b7men\u00b7gr\u00e4\u00b7ber", "s\u00e4't", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Ostern der Verj\u00fcngung, das hernieder", "tokens": ["Ein", "Os\u00b7tern", "der", "Ver\u00b7j\u00fcn\u00b7gung", ",", "das", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ins Menschenherz der Gottheit Athem weht!", "tokens": ["Ins", "Men\u00b7schen\u00b7herz", "der", "Got\u00b7theit", "A\u00b7them", "weht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sieh, welche Wandlung bl\u00fcht auf Zions Bahnen!", "tokens": ["Sieh", ",", "wel\u00b7che", "Wand\u00b7lung", "bl\u00fcht", "auf", "Zi\u00b7ons", "Bah\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAT", "NN", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "L\u00e4ngst h\u00e4lt ja Lenz sein Siegeslager hier;", "tokens": ["L\u00e4ngst", "h\u00e4lt", "ja", "Lenz", "sein", "Sie\u00b7ges\u00b7la\u00b7ger", "hier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf Bergen wehn der Palmen gr\u00fcne Fahnen,", "tokens": ["Auf", "Ber\u00b7gen", "wehn", "der", "Pal\u00b7men", "gr\u00fc\u00b7ne", "Fah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Thale prangt sein Zelt in Bl\u00fcthenzier!", "tokens": ["Im", "Tha\u00b7le", "prangt", "sein", "Zelt", "in", "Bl\u00fc\u00b7then\u00b7zier", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "L\u00e4ngst wogt ja \u00fcber all' den alten Tr\u00fcmmern", "tokens": ["L\u00e4ngst", "wogt", "ja", "\u00fc\u00b7ber", "all'", "den", "al\u00b7ten", "Tr\u00fcm\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PIS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein weites Saatenmeer in goldner Fluth,", "tokens": ["Ein", "wei\u00b7tes", "Saa\u00b7ten\u00b7meer", "in", "gold\u00b7ner", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie fern im Nord, wo wei\u00dfe Wellen schimmern,", "tokens": ["Wie", "fern", "im", "Nord", ",", "wo", "wei\u00b7\u00dfe", "Wel\u00b7len", "schim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPRART", "NN", "$,", "PWAV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Versunken tief im Meer, Vineta ruht.", "tokens": ["Ver\u00b7sun\u00b7ken", "tief", "im", "Meer", ",", "Vi\u00b7ne\u00b7ta", "ruht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "$,", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "L\u00e4ngst \u00fcber alten Schutt ist unermessen", "tokens": ["L\u00e4ngst", "\u00fc\u00b7ber", "al\u00b7ten", "Schutt", "ist", "un\u00b7er\u00b7mes\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geworfen frischer Triften gr\u00fcnes Kleid,", "tokens": ["Ge\u00b7wor\u00b7fen", "fri\u00b7scher", "Trif\u00b7ten", "gr\u00fc\u00b7nes", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gleichwie ein stilles, freundliches Vergessen", "tokens": ["Gleich\u00b7wie", "ein", "stil\u00b7les", ",", "freund\u00b7li\u00b7ches", "Ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich senkt auf dunkler Tag' uraltes Leid.", "tokens": ["Sich", "senkt", "auf", "dunk\u00b7ler", "Tag'", "ur\u00b7al\u00b7tes", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "L\u00e4ngst stehn die H\u00f6hn umfahn von Rebgewinden,", "tokens": ["L\u00e4ngst", "stehn", "die", "H\u00f6hn", "um\u00b7fahn", "von", "Reb\u00b7ge\u00b7win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "L\u00e4ngst bl\u00fcht ein Rosenhag auf Golgatha.", "tokens": ["L\u00e4ngst", "bl\u00fcht", "ein", "Ro\u00b7sen\u00b7hag", "auf", "Gol\u00b7ga\u00b7tha", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Will jetzt ein Mund den Preis der Rose k\u00fcnden,", "tokens": ["Will", "jetzt", "ein", "Mund", "den", "Preis", "der", "Ro\u00b7se", "k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nennt er gepaart Schiras und Golgatha.", "tokens": ["Nennt", "er", "ge\u00b7paart", "Schi\u00b7ras", "und", "Gol\u00b7ga\u00b7tha", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "NN", "KON", "NE", "$."], "meter": "---++--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "L\u00e4ngst alles Land weitum ein sonn'ger Garten;", "tokens": ["L\u00e4ngst", "al\u00b7les", "Land", "wei\u00b7tum", "ein", "sonn'\u00b7ger", "Gar\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es ragt kein Halbmond mehr, kein Kreuz mehr da!", "tokens": ["Es", "ragt", "kein", "Halb\u00b7mond", "mehr", ",", "kein", "Kreuz", "mehr", "da", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADV", "$,", "PIAT", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was sollten auch des blut'gen Kampf's Standarten?", "tokens": ["Was", "soll\u00b7ten", "auch", "des", "blut'\u00b7gen", "Kampf's", "Stand\u00b7ar\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "L\u00e4ngst ist es Frieden, ew'ger Frieden ja!", "tokens": ["L\u00e4ngst", "ist", "es", "Frie\u00b7den", ",", "ew'\u00b7ger", "Frie\u00b7den", "ja", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Der Kedron blieb. Er quillt vor meinen Blicken,", "tokens": ["Der", "Ke\u00b7dron", "blieb", ".", "Er", "quillt", "vor", "mei\u00b7nen", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ins Bett von gelben Aehren eingeengt,", "tokens": ["Ins", "Bett", "von", "gel\u00b7ben", "A\u00b7eh\u00b7ren", "ein\u00b7ge\u00b7engt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wohl noch als Thr\u00e4ne, doch die dem Entz\u00fccken", "tokens": ["Wohl", "noch", "als", "Thr\u00e4\u00b7ne", ",", "doch", "die", "dem", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KOUS", "NN", "$,", "ADV", "ART", "ART", "NN"], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Sich durch die blonden, goldnen Wimpern dr\u00e4ngt!", "tokens": ["Sich", "durch", "die", "blon\u00b7den", ",", "gold\u00b7nen", "Wim\u00b7pern", "dr\u00e4ngt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Das ist ein Bl\u00fchen rings, ein Duften, Klingen,", "tokens": ["Das", "ist", "ein", "Bl\u00fc\u00b7hen", "rings", ",", "ein", "Duf\u00b7ten", ",", "Klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "$,", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das um die Wette sprie\u00dft und rauscht und keimt,", "tokens": ["Das", "um", "die", "Wet\u00b7te", "sprie\u00dft", "und", "rauscht", "und", "keimt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als g\u00e4lt' es jetzt, gesch\u00e4ftig einzubringen,", "tokens": ["Als", "g\u00e4lt'", "es", "jetzt", ",", "ge\u00b7sch\u00e4f\u00b7tig", "ein\u00b7zu\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "$,", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was starr im Schlaf Jahrtausende vers\u00e4umt.", "tokens": ["Was", "starr", "im", "Schlaf", "Jahr\u00b7tau\u00b7sen\u00b7de", "ver\u00b7s\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPRART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Das ist ein Gl\u00e4nzen rings, ein Funkeln, Schimmern", "tokens": ["Das", "ist", "ein", "Gl\u00e4n\u00b7zen", "rings", ",", "ein", "Fun\u00b7keln", ",", "Schim\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "$,", "ART", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der St\u00e4dt' im Thal, der H\u00e4user auf den H\u00f6hn;", "tokens": ["Der", "St\u00e4dt'", "im", "Thal", ",", "der", "H\u00e4u\u00b7ser", "auf", "den", "H\u00f6hn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "