{"textgrid.poem.45827": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[wenn der Sturm das Meer umschlinget]", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn der Sturm das Meer umschlinget,", "tokens": ["Wenn", "der", "Sturm", "das", "Meer", "um\u00b7schlin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwarze Locken ihn umh\u00fcllen,", "tokens": ["Schwar\u00b7ze", "Lo\u00b7cken", "ihn", "um\u00b7h\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Beut sich k\u00e4mpfend seinem Willen", "tokens": ["Beut", "sich", "k\u00e4mp\u00b7fend", "sei\u00b7nem", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADJD", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die allm\u00e4cht'ge Braut und ringet,", "tokens": ["Die", "all\u00b7m\u00e4cht'\u00b7ge", "Braut", "und", "rin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "K\u00fcsset ihn mit wilden Wellen,", "tokens": ["K\u00fcs\u00b7set", "ihn", "mit", "wil\u00b7den", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blitze blicken seine Augen,", "tokens": ["Blit\u00b7ze", "bli\u00b7cken", "sei\u00b7ne", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Donner seine Seufzer hauchen,", "tokens": ["Don\u00b7ner", "sei\u00b7ne", "Seuf\u00b7zer", "hau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das Schifflein mu\u00df zerschellen.", "tokens": ["Und", "das", "Schif\u00b7flein", "mu\u00df", "zer\u00b7schel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn die Liebe aus den Sternen", "tokens": ["Wenn", "die", "Lie\u00b7be", "aus", "den", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Niederblicket auf die Erde,", "tokens": ["Nie\u00b7der\u00b7bli\u00b7cket", "auf", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dein Liebstes Lieb begehrte,", "tokens": ["Und", "dein", "Liebs\u00b7tes", "Lieb", "be\u00b7gehr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df dein Liebstes sich entfernen.", "tokens": ["Mu\u00df", "dein", "Liebs\u00b7tes", "sich", "ent\u00b7fer\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Denn der Tod k\u00f6mmt still gegangen,", "tokens": ["Denn", "der", "Tod", "k\u00f6mmt", "still", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fcsset sie mit Geisterk\u00fcssen,", "tokens": ["K\u00fcs\u00b7set", "sie", "mit", "Geis\u00b7ter\u00b7k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre Augen dir sich schlie\u00dfen,", "tokens": ["Ih\u00b7re", "Au\u00b7gen", "dir", "sich", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind im Himmel aufgegangen.", "tokens": ["Sind", "im", "Him\u00b7mel", "auf\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Rufe, da\u00df die Felsen beben,", "tokens": ["Ru\u00b7fe", ",", "da\u00df", "die", "Fel\u00b7sen", "be\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weine tausend bittre Z\u00e4hren,", "tokens": ["Wei\u00b7ne", "tau\u00b7send", "bitt\u00b7re", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, sie wird dich nie erh\u00f6ren,", "tokens": ["Ach", ",", "sie", "wird", "dich", "nie", "er\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimmermehr dir Antwort geben.", "tokens": ["Nim\u00b7mer\u00b7mehr", "dir", "Ant\u00b7wort", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Fr\u00fchling darf nur leise hauchen,", "tokens": ["Fr\u00fch\u00b7ling", "darf", "nur", "lei\u00b7se", "hau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stille Tr\u00e4nen niedertauen,", "tokens": ["Stil\u00b7le", "Tr\u00e4\u00b7nen", "nie\u00b7der\u00b7tau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Komme, willst dein Lieb du schauen,", "tokens": ["Kom\u00b7me", ",", "willst", "dein", "Lieb", "du", "schau\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VMFIN", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blumen \u00f6ffnen dir die Augen.", "tokens": ["Blu\u00b7men", "\u00f6ff\u00b7nen", "dir", "die", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "In des Baumes dichten Rinden,", "tokens": ["In", "des", "Bau\u00b7mes", "dich\u00b7ten", "Rin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Blumen Kelch versunken,", "tokens": ["In", "der", "Blu\u00b7men", "Kelch", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlummern helle Lebensfunken,", "tokens": ["Schlum\u00b7mern", "hel\u00b7le", "Le\u00b7bens\u00b7fun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werden bald den Wald entz\u00fcnden.", "tokens": ["Wer\u00b7den", "bald", "den", "Wald", "ent\u00b7z\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "In uns selbst sind wir verloren,", "tokens": ["In", "uns", "selbst", "sind", "wir", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bange Fesseln uns beengen,", "tokens": ["Ban\u00b7ge", "Fes\u00b7seln", "uns", "be\u00b7en\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlo\u00df und Riegel mu\u00df zersprengen,", "tokens": ["Schlo\u00df", "und", "Rie\u00b7gel", "mu\u00df", "zer\u00b7spren\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur im Tode wird geboren.", "tokens": ["Nur", "im", "To\u00b7de", "wird", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "In der N\u00e4chte Finsternissen", "tokens": ["In", "der", "N\u00e4ch\u00b7te", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df der junge Tag ertrinken,", "tokens": ["Mu\u00df", "der", "jun\u00b7ge", "Tag", "er\u00b7trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Abend mu\u00df herniedersinken,", "tokens": ["A\u00b7bend", "mu\u00df", "her\u00b7nie\u00b7der\u00b7sin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll der Morgen dich begr\u00fc\u00dfen.", "tokens": ["Soll", "der", "Mor\u00b7gen", "dich", "be\u00b7gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Wer rufet in die stumme Nacht?", "tokens": ["Wer", "ru\u00b7fet", "in", "die", "stum\u00b7me", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer kann mit Geistern sprechen?", "tokens": ["Wer", "kann", "mit", "Geis\u00b7tern", "spre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer steiget in den dunkeln Schacht,", "tokens": ["Wer", "stei\u00b7get", "in", "den", "dun\u00b7keln", "Schacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Lichtes Blum' zu brechen?", "tokens": ["Des", "Lich\u00b7tes", "Blum'", "zu", "bre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Kein Licht scheint aus der tiefen Gruft,", "tokens": ["Kein", "Licht", "scheint", "aus", "der", "tie\u00b7fen", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kein Ton aus stillen N\u00e4chten ruft.", "tokens": ["Kein", "Ton", "aus", "stil\u00b7len", "N\u00e4ch\u00b7ten", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "An Ufers Ferne wallt ein Licht,", "tokens": ["An", "U\u00b7fers", "Fer\u00b7ne", "wallt", "ein", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du m\u00f6chtest jenseits landen;", "tokens": ["Du", "m\u00f6ch\u00b7test", "jen\u00b7seits", "lan\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch fasse Mut, verzage nicht,", "tokens": ["Doch", "fas\u00b7se", "Mut", ",", "ver\u00b7za\u00b7ge", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du mu\u00dft erst diesseits stranden.", "tokens": ["Du", "mu\u00dft", "erst", "dies\u00b7seits", "stran\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Schau still hinab, in Todes Scho\u00df", "tokens": ["Schau", "still", "hin\u00b7ab", ",", "in", "To\u00b7des", "Scho\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADJD", "PTKVZ", "$,", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bl\u00fcht jedes Ziel, f\u00e4llt dir dein Los.", "tokens": ["Bl\u00fcht", "je\u00b7des", "Ziel", ",", "f\u00e4llt", "dir", "dein", "Los", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "So breche dann, du tote Wand,", "tokens": ["So", "bre\u00b7che", "dann", ",", "du", "to\u00b7te", "Wand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hinab mit allen Binden;", "tokens": ["Hin\u00b7ab", "mit", "al\u00b7len", "Bin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Zweig erbl\u00fche meiner Hand,", "tokens": ["Ein", "Zweig", "er\u00b7bl\u00fc\u00b7he", "mei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Frieden zu verk\u00fcnden.", "tokens": ["Den", "Frie\u00b7den", "zu", "ver\u00b7k\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich will kein Einzelner mehr sein,", "tokens": ["Ich", "will", "kein", "Ein\u00b7zel\u00b7ner", "mehr", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "ADV", "VAINF", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Ich bin der Welt, die Welt ist mein.", "tokens": ["Ich", "bin", "der", "Welt", ",", "die", "Welt", "ist", "mein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Vergangen sei vergangen,", "tokens": ["Ver\u00b7gan\u00b7gen", "sei", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Zukunft ewig fern;", "tokens": ["Und", "Zu\u00b7kunft", "e\u00b7wig", "fern", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In Gegenwart gefangen", "tokens": ["In", "Ge\u00b7gen\u00b7wart", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verweilt die Liebe gern,", "tokens": ["Ver\u00b7weilt", "die", "Lie\u00b7be", "gern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Und reicht nach allen Seiten", "tokens": ["Und", "reicht", "nach", "al\u00b7len", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die ew'gen Arme hin,", "tokens": ["Die", "ew'\u00b7gen", "Ar\u00b7me", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Dasein zu erweiten,", "tokens": ["Mein", "Da\u00b7sein", "zu", "er\u00b7wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Bis ich unendlich bin.", "tokens": ["Bis", "ich", "un\u00b7end\u00b7lich", "bin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "So tausendfach gestaltet,", "tokens": ["So", "tau\u00b7send\u00b7fach", "ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erbl\u00fch' ich \u00fcberall,", "tokens": ["Er\u00b7bl\u00fch'", "ich", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und meine Tugend waltet", "tokens": ["Und", "mei\u00b7ne", "Tu\u00b7gend", "wal\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf Berges H\u00f6h', im Tal.", "tokens": ["Auf", "Ber\u00b7ges", "H\u00f6h'", ",", "im", "Tal", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Mein Wort hallt von den Klippen,", "tokens": ["Mein", "Wort", "hallt", "von", "den", "Klip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Lied vom Himmel weht;", "tokens": ["Mein", "Lied", "vom", "Him\u00b7mel", "weht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es fl\u00fcstern tausend Lippen", "tokens": ["Es", "fl\u00fcs\u00b7tern", "tau\u00b7send", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Haine mein Gebet.", "tokens": ["Im", "Hai\u00b7ne", "mein", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Ich habe allem Leben", "tokens": ["Ich", "ha\u00b7be", "al\u00b7lem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit jedem Abendrot", "tokens": ["Mit", "je\u00b7dem", "A\u00b7ben\u00b7drot"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Abschiedsku\u00df gegeben,", "tokens": ["Den", "Ab\u00b7schieds\u00b7ku\u00df", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und jeder Schlaf ist Tod.", "tokens": ["Und", "je\u00b7der", "Schlaf", "ist", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Es sinkt der Morgen nieder,", "tokens": ["Es", "sinkt", "der", "Mor\u00b7gen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Fittichen so lind,", "tokens": ["Mit", "Fit\u00b7ti\u00b7chen", "so", "lind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weckt mich die Liebe wieder,", "tokens": ["Weckt", "mich", "die", "Lie\u00b7be", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein neugeboren Kind.", "tokens": ["Ein", "neu\u00b7ge\u00b7bo\u00b7ren", "Kind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Und wenn ich einsam weine,", "tokens": ["Und", "wenn", "ich", "ein\u00b7sam", "wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wenn das Herz mir bricht,", "tokens": ["Und", "wenn", "das", "Herz", "mir", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So sieh im Sonnenscheine", "tokens": ["So", "sieh", "im", "Son\u00b7nen\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein l\u00e4chelnd Angesicht.", "tokens": ["Mein", "l\u00e4\u00b7chelnd", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Mu\u00df ich am Stabe wanken,", "tokens": ["Mu\u00df", "ich", "am", "Sta\u00b7be", "wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schwebt Winter um mein Haupt,", "tokens": ["Schwebt", "Win\u00b7ter", "um", "mein", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wird nie doch dem Gedanken", "tokens": ["Wird", "nie", "doch", "dem", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Glut und Eil geraubt.", "tokens": ["Die", "Glut", "und", "Eil", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "Ich sinke ewig unter,", "tokens": ["Ich", "sin\u00b7ke", "e\u00b7wig", "un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und steige ewig auf,", "tokens": ["Und", "stei\u00b7ge", "e\u00b7wig", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und bl\u00fche stets gesunder", "tokens": ["Und", "bl\u00fc\u00b7he", "stets", "ge\u00b7sun\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus Liebes-Scho\u00df herauf.", "tokens": ["Aus", "Lie\u00b7bes\u00b7Scho\u00df", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.22": {"line.1": {"text": "Das Leben nie verschwindet,", "tokens": ["Das", "Le\u00b7ben", "nie", "ver\u00b7schwin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Liebesflamm' und Licht", "tokens": ["Mit", "Lie\u00b7bes\u00b7flamm'", "und", "Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hat Gott sich selbst entz\u00fcndet", "tokens": ["Hat", "Gott", "sich", "selbst", "ent\u00b7z\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "PRF", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In der Natur Gedicht.", "tokens": ["In", "der", "Na\u00b7tur", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.23": {"line.1": {"text": "Das Licht hat mich durchdrungen,", "tokens": ["Das", "Licht", "hat", "mich", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und rei\u00dfet mich hervor;", "tokens": ["Und", "rei\u00b7\u00dfet", "mich", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit tausend Flammenzungen", "tokens": ["Mit", "tau\u00b7send", "Flam\u00b7men\u00b7zun\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gl\u00fch' ich zur Glut empor.", "tokens": ["Gl\u00fch'", "ich", "zur", "Glut", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.24": {"line.1": {"text": "So kann ich nimmer sterben,", "tokens": ["So", "kann", "ich", "nim\u00b7mer", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kann nimmer mir entgehn;", "tokens": ["Kann", "nim\u00b7mer", "mir", "ent\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn um mich zu verderben,", "tokens": ["Denn", "um", "mich", "zu", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "M\u00fc\u00dft' Gott selbst untergehn.", "tokens": ["M\u00fc\u00dft'", "Gott", "selbst", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}