{"dta.poem.20683": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  die verwittwete Madame R* ldt  \n nach Freyenwalde nachgeschrieben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach, es regnet unaufh\u00f6rlich,                 ", "tokens": ["Ach", ",", "es", "reg\u00b7net", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es hat die Nacht gest\u00fcrmt \u2014", "tokens": ["Und", "es", "hat", "die", "Nacht", "ge\u00b7st\u00fcrmt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Manchen Wandrer fromm und ehrlich", "tokens": ["Man\u00b7chen", "Wand\u00b7rer", "fromm", "und", "ehr\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat kein Regenschirm beschirmt", "tokens": ["Hat", "kein", "Re\u00b7gen\u00b7schirm", "be\u00b7schirmt"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf der offnen Post nach Preu\u00dfen,", "tokens": ["Auf", "der", "off\u00b7nen", "Post", "nach", "Preu\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Oder wie in unsrer Welt", "tokens": ["O\u00b7der", "wie", "in", "uns\u00b7rer", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "All die L\u00e4nder m\u00f6gen hei\u00dfen,", "tokens": ["All", "die", "L\u00e4n\u00b7der", "m\u00f6\u00b7gen", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wo es na\u00df vom Himmel f\u00e4llt.", "tokens": ["Wo", "es", "na\u00df", "vom", "Him\u00b7mel", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Ach, es ist doch Jammerschade,", "tokens": ["Ach", ",", "es", "ist", "doch", "Jam\u00b7mer\u00b7scha\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wenn der kalte Wolkenwind", "tokens": ["Wenn", "der", "kal\u00b7te", "Wol\u00b7ken\u00b7wind"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Kranken \u00e4rgert, die zum Bade", "tokens": ["Kran\u00b7ken", "\u00e4r\u00b7gert", ",", "die", "zum", "Ba\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Hoffnungsvoll gereiset sind,", "tokens": ["Hoff\u00b7nungs\u00b7voll", "ge\u00b7rei\u00b7set", "sind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Und nach Aeskulaps Gesetzen", "tokens": ["Und", "nach", "A\u00b7es\u00b7ku\u00b7laps", "Ge\u00b7set\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Ihre schwache Nerven nun", "tokens": ["Ih\u00b7re", "schwa\u00b7che", "Ner\u00b7ven", "nun"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Nicht mit Wasser d\u00fcrfen netzen;", "tokens": ["Nicht", "mit", "Was\u00b7ser", "d\u00fcr\u00b7fen", "net\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Denn das m\u00f6chte Schaden thun,", "tokens": ["Denn", "das", "m\u00f6ch\u00b7te", "Scha\u00b7den", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "M\u00f6chte noch die Gicht vermehren;", "tokens": ["M\u00f6ch\u00b7te", "noch", "die", "Gicht", "ver\u00b7meh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Darum l\u00e4\u00dft der Kranke gern", "tokens": ["Da\u00b7rum", "l\u00e4\u00dft", "der", "Kran\u00b7ke", "gern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN", "ADV"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.19": {"text": "Sich von einem Arzt belehren,", "tokens": ["Sich", "von", "ei\u00b7nem", "Arzt", "be\u00b7leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Wie von seines Willens Herrn \u2014", "tokens": ["Wie", "von", "sei\u00b7nes", "Wil\u00b7lens", "Herrn"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Liebe Freundin, du bist endlich,", "tokens": ["Lie\u00b7be", "Freun\u00b7din", ",", "du", "bist", "end\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "VAFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gott sey Dank, nun hingereist,", "tokens": ["Gott", "sey", "Dank", ",", "nun", "hin\u00b7ge\u00b7reist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil man da so still und l\u00e4ndlich", "tokens": ["Weil", "man", "da", "so", "still", "und", "l\u00e4nd\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter viel Gesellschaft speist;", "tokens": ["Un\u00b7ter", "viel", "Ge\u00b7sell\u00b7schaft", "speist", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Menschen kennen lernt im Gange", "tokens": ["Men\u00b7schen", "ken\u00b7nen", "lernt", "im", "Gan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unter B\u00e4umen hoch und breit,", "tokens": ["Un\u00b7ter", "B\u00e4u\u00b7men", "hoch", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und mit Ihnen sondern Zwange", "tokens": ["Und", "mit", "Ih\u00b7nen", "son\u00b7dern", "Zwan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sich der sch\u00f6nen Gegend freut,", "tokens": ["Sich", "der", "sch\u00f6\u00b7nen", "Ge\u00b7gend", "freut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Die du mir so sch\u00f6n beschrieben", "tokens": ["Die", "du", "mir", "so", "sch\u00f6n", "be\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PPER", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und nun nicht genie\u00dfen wirst.", "tokens": ["Und", "nun", "nicht", "ge\u00b7nie\u00b7\u00dfen", "wirst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Keine Wolke wird vertrieben,", "tokens": ["Kei\u00b7ne", "Wol\u00b7ke", "wird", "ver\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "W\u00e4re gleich der erste F\u00fcrst", "tokens": ["W\u00e4\u00b7re", "gleich", "der", "ers\u00b7te", "F\u00fcrst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Auf der ganzen weiten Erde", "tokens": ["Auf", "der", "gan\u00b7zen", "wei\u00b7ten", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Friedrich Maximus allda,", "tokens": ["Fried\u00b7rich", "Ma\u00b7xi\u00b7mus", "all\u00b7da", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "PAV", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.15": {"text": "Spr\u00e4ch er, wie ein Gott, es werde", "tokens": ["Spr\u00e4ch", "er", ",", "wie", "ein", "Gott", ",", "es", "wer\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "PWAV", "ART", "NN", "$,", "PPER", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Licht am Himmel fern nnd nah \u2014", "tokens": ["Licht", "am", "Him\u00b7mel", "fern", "nnd", "nah"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADJD", "KON", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "W\u00fcrd es dennoch d\u00fcster bleiben", "tokens": ["W\u00fcrd", "es", "den\u00b7noch", "d\u00fcs\u00b7ter", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Und er m\u00fc\u00dfte sich im Schach", "tokens": ["Und", "er", "m\u00fc\u00df\u00b7te", "sich", "im", "Schach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "PRF", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Langer Weile Druck vertreiben", "tokens": ["Lan\u00b7ger", "Wei\u00b7le", "Druck", "ver\u00b7trei\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Unterm Badezimmer-Dach,", "tokens": ["Un\u00b7term", "Ba\u00b7de\u00b7zim\u00b7mer\u00b7Dach", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Oder lie\u00dfe vom gelehrten", "tokens": ["O\u00b7der", "lie\u00b7\u00dfe", "vom", "ge\u00b7lehr\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Marquis Luchesini sich", "tokens": ["Mar\u00b7quis", "Lu\u00b7che\u00b7si\u00b7ni", "sich"], "token_info": ["word", "word", "word"], "pos": ["NE", "NE", "PRF"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.23": {"text": "Lesen, was die R\u00f6mer h\u00f6rten,", "tokens": ["Le\u00b7sen", ",", "was", "die", "R\u00f6\u00b7mer", "h\u00f6r\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Wenn ihr Redner k\u00f6niglich", "tokens": ["Wenn", "ihr", "Red\u00b7ner", "k\u00f6\u00b7nig\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Ueber sie zu herrschen wu\u00dfte,", "tokens": ["Ue\u00b7ber", "sie", "zu", "herr\u00b7schen", "wu\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Da\u00df vor seiner Stimme Ton", "tokens": ["Da\u00df", "vor", "sei\u00b7ner", "Stim\u00b7me", "Ton"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Ihre Seele zittern mu\u00dfte,", "tokens": ["Ih\u00b7re", "See\u00b7le", "zit\u00b7tern", "mu\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Oder schmelzen, oder schon", "tokens": ["O\u00b7der", "schmel\u00b7zen", ",", "o\u00b7der", "schon"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVINF", "$,", "KON", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Wieder lieben, wen sie ha\u00dfte.", "tokens": ["Wie\u00b7der", "lie\u00b7ben", ",", "wen", "sie", "ha\u00df\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Freundin! findet irgendwo", "tokens": ["Freun\u00b7din", "!", "fin\u00b7det", "ir\u00b7gend\u00b7wo"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich in einem Badegaste", "tokens": ["Sich", "in", "ei\u00b7nem", "Ba\u00b7de\u00b7gas\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter Euch ein Cicero,", "tokens": ["Un\u00b7ter", "Euch", "ein", "Ci\u00b7ce\u00b7ro", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie der gro\u00dfe Mann gewesen,", "tokens": ["Wie", "der", "gro\u00b7\u00dfe", "Mann", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Den ein R\u00f6mer umgebracht,", "tokens": ["Den", "ein", "R\u00f6\u00b7mer", "um\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dann braucht keiner vorzulesen,", "tokens": ["Dann", "braucht", "kei\u00b7ner", "vor\u00b7zu\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Was ein C\u00e4sar hat gedacht", "tokens": ["Was", "ein", "C\u00e4\u00b7sar", "hat", "ge\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NE", "VAFIN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und gethan und ausgerichtet,", "tokens": ["Und", "ge\u00b7than", "und", "aus\u00b7ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "KON", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Oder was Horazius", "tokens": ["O\u00b7der", "was", "Ho\u00b7ra\u00b7zi\u00b7us"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWS", "NE"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.10": {"text": "K\u00fcnstlich noch dazu gedichtet", "tokens": ["K\u00fcnst\u00b7lich", "noch", "da\u00b7zu", "ge\u00b7dich\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADV", "PAV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "F\u00fcr den Kayserhuldgenu\u00df \u2014", "tokens": ["F\u00fcr", "den", "Kay\u00b7ser\u00b7huld\u00b7ge\u00b7nu\u00df"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Dieser Cicero, der deutsche,", "tokens": ["Die\u00b7ser", "Ci\u00b7ce\u00b7ro", ",", "der", "deut\u00b7sche", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Machte, da\u00df kein Badegast", "tokens": ["Mach\u00b7te", ",", "da\u00df", "kein", "Ba\u00b7de\u00b7gast"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Traurig w\u00e4re beim Gepeitsche", "tokens": ["Trau\u00b7rig", "w\u00e4\u00b7re", "beim", "Ge\u00b7peit\u00b7sche"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Kalten Windes, der die Last", "tokens": ["Kal\u00b7ten", "Win\u00b7des", ",", "der", "die", "Last"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Armer m\u00fcder Wandersleute", "tokens": ["Ar\u00b7mer", "m\u00fc\u00b7der", "Wan\u00b7ders\u00b7leu\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Schwerer macht, als sie schon ist \u2014", "tokens": ["Schwe\u00b7rer", "macht", ",", "als", "sie", "schon", "ist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Cicero verk\u00fcrzt mir heute", "tokens": ["Ci\u00b7ce\u00b7ro", "ver\u00b7k\u00fcrzt", "mir", "heu\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Da Du nicht vorhanden bist,", "tokens": ["Da", "Du", "nicht", "vor\u00b7han\u00b7den", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Dieses tr\u00fcben Tages L\u00e4nge,", "tokens": ["Die\u00b7ses", "tr\u00fc\u00b7ben", "Ta\u00b7ges", "L\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Giebt mir einen Abendschmaus;", "tokens": ["Giebt", "mir", "ei\u00b7nen", "A\u00b7bendsc\u00b7hmaus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Denn wenn\u2019s sieben schl\u00e4gt, dann g\u00e4nge", "tokens": ["Denn", "wenn's", "sie\u00b7ben", "schl\u00e4gt", ",", "dann", "g\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "ADV", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.23": {"text": "Ich vermuthlich in Dein Haus", "tokens": ["Ich", "ver\u00b7muth\u00b7lich", "in", "Dein", "Haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Unter einem Leinwanddache;", "tokens": ["Un\u00b7ter", "ei\u00b7nem", "Lein\u00b7wand\u00b7da\u00b7che", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Krebse schmaust\u2019 ich da mit Dir.", "tokens": ["Kreb\u00b7se", "schmaust'", "ich", "da", "mit", "Dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Nun bleib ich daheim und mache", "tokens": ["Nun", "bleib", "ich", "da\u00b7heim", "und", "ma\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.27": {"text": "Ein Gedankenschm\u00e4uschen mir.", "tokens": ["Ein", "Ge\u00b7dan\u00b7ken\u00b7schm\u00e4us\u00b7chen", "mir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "\u201ecicero, sprech ich beim Lesen,", "tokens": ["\u201e", "ci\u00b7ce\u00b7ro", ",", "sprech", "ich", "beim", "Le\u00b7sen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$,", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "\u201eeicero, du warst begl\u00fcckt,", "tokens": ["\u201e", "ei\u00b7ce\u00b7ro", ",", "du", "warst", "be\u00b7gl\u00fcckt", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "\u201ew\u00e4rst begl\u00fcckter noch gewesen,", "tokens": ["\u201e", "w\u00e4rst", "be\u00b7gl\u00fcck\u00b7ter", "noch", "ge\u00b7we\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADJD", "ADV", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "\u201eh\u00e4tte dich ein Weib bestrickt,", "tokens": ["\u201e", "h\u00e4t\u00b7te", "dich", "ein", "Weib", "be\u00b7strickt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "\u201edie sich f\u00fcr dein Herz gesc