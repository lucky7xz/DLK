{"textgrid.poem.64896": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gewalt'ger! mit dem Schwerte der Gedanken", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gewalt'ger! mit dem Schwerte der Gedanken", "tokens": ["Ge\u00b7walt'\u00b7ger", "!", "mit", "dem", "Schwer\u00b7te", "der", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Strittst du, ein Held, in jenem gro\u00dfen Streite,", "tokens": ["Strittst", "du", ",", "ein", "Held", ",", "in", "je\u00b7nem", "gro\u00b7\u00dfen", "Strei\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Deutschland von dem fremden Joch befreite,", "tokens": ["Der", "Deutschland", "von", "dem", "frem\u00b7den", "Joch", "be\u00b7frei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und siegreich brach dein Lied durch alle Schranken!", "tokens": ["Und", "sieg\u00b7reich", "brach", "dein", "Lied", "durch", "al\u00b7le", "Schran\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ja! ewig mu\u00df das deutsche Volk dir's danken,", "tokens": ["Ja", "!", "e\u00b7wig", "mu\u00df", "das", "deut\u00b7sche", "Volk", "dir's", "dan\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADJD", "VMFIN", "ART", "ADJA", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df du zur Zeit, wo Spaltung es entzweite", "tokens": ["Da\u00df", "du", "zur", "Zeit", ",", "wo", "Spal\u00b7tung", "es", "ent\u00b7zwei\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "$,", "PWAV", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und niedrer Knechtschaft Zeichen es entweihte,", "tokens": ["Und", "nie\u00b7drer", "Knecht\u00b7schaft", "Zei\u00b7chen", "es", "ent\u00b7weih\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Recht ihm sangst und Freiheit sonder Wanken.", "tokens": ["Von", "Recht", "ihm", "sangst", "und", "Frei\u00b7heit", "son\u00b7der", "Wan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVFIN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch weil nun l\u00e4ngst schon dieser Streit geschlichtet,", "tokens": ["Doch", "weil", "nun", "l\u00e4ngst", "schon", "die\u00b7ser", "Streit", "ge\u00b7schlich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ADV", "ADV", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hast du entsagt der Schlachtenlieder Tosung", "tokens": ["Hast", "du", "ent\u00b7sagt", "der", "Schlach\u00b7ten\u00b7lie\u00b7der", "To\u00b7sung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und stehst als Priestergreis hoch aufgerichtet!", "tokens": ["Und", "stehst", "als", "Pries\u00b7ter\u00b7greis", "hoch", "auf\u00b7ge\u00b7rich\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es lehrt, weil Freiheit stets doch deine Losung,", "tokens": ["Es", "lehrt", ",", "weil", "Frei\u00b7heit", "stets", "doch", "dei\u00b7ne", "Lo\u00b7sung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "NN", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Uns jetzt dein Lied und deiner Harfe Klingen", "tokens": ["Uns", "jetzt", "dein", "Lied", "und", "dei\u00b7ner", "Har\u00b7fe", "Klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Gotteskinder Freiheit zu erringen.", "tokens": ["Der", "Got\u00b7tes\u00b7kin\u00b7der", "Frei\u00b7heit", "zu", "er\u00b7rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}