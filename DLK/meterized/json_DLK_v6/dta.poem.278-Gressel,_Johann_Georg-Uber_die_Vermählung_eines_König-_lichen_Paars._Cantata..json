{"dta.poem.278": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Uber die Verm\u00e4hlung eines K\u00f6nig-  \n lichen Paars.  \n  Cantata.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Brennt/ verliebte Hertzen brennet/", "tokens": ["Brennt", "/", "ver\u00b7lieb\u00b7te", "Hert\u00b7zen", "bren\u00b7net", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVFIN", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In verg\u00f6nnter Liebes-Gluth/", "tokens": ["In", "ver\u00b7g\u00f6nn\u00b7ter", "Lie\u00b7bes\u00b7Gluth", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mehret eure heisse Flammen/", "tokens": ["Meh\u00b7ret", "eu\u00b7re", "heis\u00b7se", "Flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn weil sie vom Himmel stammen/", "tokens": ["Denn", "weil", "sie", "vom", "Him\u00b7mel", "stam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Bleibt er ihnen ewig gut.", "tokens": ["Bleibt", "er", "ih\u00b7nen", "e\u00b7wig", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Brennt/ verliebte Hertzen brennet", "tokens": ["Brennt", "/", "ver\u00b7lieb\u00b7te", "Hert\u00b7zen", "bren\u00b7net"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$(", "VVFIN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In verg\u00f6nnter Liebes-Gluth.", "tokens": ["In", "ver\u00b7g\u00f6nn\u00b7ter", "Lie\u00b7bes\u00b7Gluth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hymen ist mit seinen L\u00fcsten", "tokens": ["Hy\u00b7men", "ist", "mit", "sei\u00b7nen", "L\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Euch zu dienen stets bereit.", "tokens": ["Euch", "zu", "die\u00b7nen", "stets", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tausend Anmuth/ tausend Freuden", "tokens": ["Tau\u00b7send", "An\u00b7muth", "/", "tau\u00b7send", "Freu\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["CARD", "NN", "$(", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werden auf euch aus gestreut/", "tokens": ["Wer\u00b7den", "auf", "euch", "aus", "ge\u00b7streut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "APPR", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn auf den erhobnen Br\u00fcsten", "tokens": ["Wenn", "auf", "den", "er\u00b7hob\u00b7nen", "Br\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Dieser vierten ", "tokens": ["Die\u00b7ser", "vier\u00b7ten"], "token_info": ["word", "word"], "pos": ["PDAT", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Phraataces sich wird weiden/", "tokens": ["Phraa\u00b7ta\u00b7ces", "sich", "wird", "wei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die voll Wollust-Rosen stehn.", "tokens": ["Die", "voll", "Wol\u00b7lust\u00b7Ro\u00b7sen", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "So eile dann vollkommnes F\u00fcrsten-Paar/", "tokens": ["So", "ei\u00b7le", "dann", "voll\u00b7komm\u00b7nes", "F\u00fcrs\u00b7ten\u00b7Paar", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verkn\u00fcpffe Hand/ Gem\u00fchte/ Leib und Seele/", "tokens": ["Ver\u00b7kn\u00fcpf\u00b7fe", "Hand", "/", "Ge\u00b7m\u00fch\u00b7te", "/", "Leib", "und", "See\u00b7le", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$(", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "K\u00fchl deine Gluth entflammter K\u00f6nig ab/", "tokens": ["K\u00fchl", "dei\u00b7ne", "Gluth", "ent\u00b7flamm\u00b7ter", "K\u00f6\u00b7nig", "ab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Jm Wollust-Brunn der sch\u00f6nen ", "tokens": ["Jm", "Wol\u00b7lust\u00b7Brunn", "der", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein holdes Gl\u00fcck will euch zu Bette f\u00fchren/", "tokens": ["Ein", "hol\u00b7des", "Gl\u00fcck", "will", "euch", "zu", "Bet\u00b7te", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Woselbst ihr findt ein angenehmes Grab/", "tokens": ["Wo\u00b7selbst", "ihr", "findt", "ein", "an\u00b7ge\u00b7neh\u00b7mes", "Grab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So eile dann vollkommnes F\u00fcrsten-Paar", "tokens": ["So", "ei\u00b7le", "dann", "voll\u00b7komm\u00b7nes", "F\u00fcrs\u00b7ten\u00b7Paar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Verkn\u00fcpffe Hand/ Gem\u00fchte/ Leib und Seele.", "tokens": ["Ver\u00b7kn\u00fcpf\u00b7fe", "Hand", "/", "Ge\u00b7m\u00fch\u00b7te", "/", "Leib", "und", "See\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$(", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}