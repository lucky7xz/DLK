{"textgrid.poem.36165": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vom Rheinstrom bis zum Weichselstrand,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Rheinstrom bis zum Weichselstrand,", "tokens": ["Vom", "Rhein\u00b7strom", "bis", "zum", "Weich\u00b7sel\u00b7strand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Golf, der Istriens Fluren w\u00e4scht,", "tokens": ["Vom", "Golf", ",", "der", "I\u00b7stri\u00b7ens", "Flu\u00b7ren", "w\u00e4scht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bis wo die Ostsee blau't;", "tokens": ["Bis", "wo", "die", "Ost\u00b7see", "blau't", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Von Gau zu Gau, von Mark zu Mark", "tokens": ["Von", "Gau", "zu", "Gau", ",", "von", "Mark", "zu", "Mark"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "$,", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erschallt der V\u00f6lker Feldgeschrei ...", "tokens": ["Er\u00b7schallt", "der", "V\u00f6l\u00b7ker", "Feld\u00b7ge\u00b7schrei", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und ihr, ihr F\u00fcrsten, schweigt?", "tokens": ["Und", "ihr", ",", "ihr", "F\u00fcrs\u00b7ten", ",", "schweigt", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gewaltiger von Nu zu Nu,", "tokens": ["Ge\u00b7wal\u00b7ti\u00b7ger", "von", "Nu", "zu", "Nu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ergrimmter, w\u00fcthiger, st\u00fcrmischer", "tokens": ["Er\u00b7grimm\u00b7ter", ",", "w\u00fct\u00b7hi\u00b7ger", ",", "st\u00fcr\u00b7mi\u00b7scher"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "ADJD", "$,", "ADJA"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Erschallt der V\u00f6lker Schrei.", "tokens": ["Er\u00b7schallt", "der", "V\u00f6l\u00b7ker", "Schrei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zu ihren Hirten schau'n sie auf.", "tokens": ["Zu", "ih\u00b7ren", "Hir\u00b7ten", "schau'n", "sie", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie bieten Gut und Blut euch an ...", "tokens": ["Sie", "bie\u00b7ten", "Gut", "und", "Blut", "euch", "an", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und ihr, ihr Hirten, schweigt?", "tokens": ["Und", "ihr", ",", "ihr", "Hir\u00b7ten", ",", "schweigt", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Urenkel ", "tokens": ["Ur\u00b7en\u00b7kel"], "token_info": ["word"], "pos": ["NN"], "meter": "---", "measure": "unknown.measure.zero"}, "line.2": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Ihr ", "tokens": ["Ihr"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Ihr ", "tokens": ["Ihr"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Ist's m\u00f6glich, da\u00df ihr schweigt?", "tokens": ["Ist's", "m\u00f6g\u00b7lich", ",", "da\u00df", "ihr", "schweigt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Auf, Heldenenkel, auf zum Kampf!", "tokens": ["Auf", ",", "Hel\u00b7de\u00b7nen\u00b7kel", ",", "auf", "zum", "Kampf", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "NN", "$,", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entstrickt der seid'nen Fessel euch!", "tokens": ["Ent\u00b7strickt", "der", "sei\u00b7d'\u00b7nen", "Fes\u00b7sel", "euch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Zerbrecht des Zwingherrn Joch!", "tokens": ["Zer\u00b7brecht", "des", "Zwing\u00b7herrn", "Joch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Den g\u00fcldnen Reif, der Knechtschaft Pfand,", "tokens": ["Den", "g\u00fcld\u00b7nen", "Reif", ",", "der", "Knecht\u00b7schaft", "Pfand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Tauscht um der gr\u00fcnen Eiche Kranz,", "tokens": ["Tauscht", "um", "der", "gr\u00fc\u00b7nen", "Ei\u00b7che", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Erst Freie, F\u00fcrsten dann!", "tokens": ["Erst", "Frei\u00b7e", ",", "F\u00fcrs\u00b7ten", "dann", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Von eurer V\u00f6lker Geist entflammt,", "tokens": ["Von", "eu\u00b7rer", "V\u00f6l\u00b7ker", "Geist", "ent\u00b7flammt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zieht aus mit dem getreuen Volk,", "tokens": ["Zieht", "aus", "mit", "dem", "ge\u00b7treu\u00b7en", "Volk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zieht aus zum heil'gen Streit!", "tokens": ["Zieht", "aus", "zum", "heil'\u00b7gen", "Streit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und der mit uns bei Teutoburg,", "tokens": ["Und", "der", "mit", "uns", "bei", "Teu\u00b7to\u00b7burg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "PPER", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im Lechfeld und bei H\u00f6chst\u00e4tt war,", "tokens": ["Im", "Lech\u00b7feld", "und", "bei", "H\u00f6chs\u00b7t\u00e4tt", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "NE", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gott, Gott wird mit uns seyn.", "tokens": ["Gott", ",", "Gott", "wird", "mit", "uns", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VAFIN", "APPR", "PPER", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und ist die Rettung nun geschehn,", "tokens": ["Und", "ist", "die", "Ret\u00b7tung", "nun", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So bau't ein neues Reich uns auf,", "tokens": ["So", "bau't", "ein", "neu\u00b7es", "Reich", "uns", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein heil'ges deutsches Reich!", "tokens": ["Ein", "heil'\u00b7ges", "deut\u00b7sches", "Reich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So weit das deutsche Ja und Nein,", "tokens": ["So", "weit", "das", "deut\u00b7sche", "Ja", "und", "Nein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "KON", "PTKANT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So weit das deutsche Lied erschallt,", "tokens": ["So", "weit", "das", "deut\u00b7sche", "Lied", "er\u00b7schallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Soll Deutschlands Banner wehn!", "tokens": ["Soll", "Deutschlands", "Ban\u00b7ner", "wehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "NN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Und Kaiser sey der W\u00fcrdigste!", "tokens": ["Und", "Kai\u00b7ser", "sey", "der", "W\u00fcr\u00b7digs\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und wer dem W\u00fcrdigsten zun\u00e4chst,", "tokens": ["Und", "wer", "dem", "W\u00fcr\u00b7digs\u00b7ten", "zu\u00b7n\u00e4chst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sey Herzog, F\u00fcrst und Graf!", "tokens": ["Sey", "Her\u00b7zog", ",", "F\u00fcrst", "und", "Graf", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "$,", "NN", "KON", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und adlich sey, wer edel nur,", "tokens": ["Und", "ad\u00b7lich", "sey", ",", "wer", "e\u00b7del", "nur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "PWS", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und edel sey, wem hoch das Herz", "tokens": ["Und", "e\u00b7del", "sey", ",", "wem", "hoch", "das", "Herz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "$,", "PWS", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr Deutschlands Rettung schl\u00e4gt!", "tokens": ["F\u00fcr", "Deutschlands", "Ret\u00b7tung", "schl\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}