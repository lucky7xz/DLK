{"textgrid.poem.24933": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbgeht, Pater Ambros! Wenn man Euch gebraucht,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbgeht, Pater Ambros! Wenn man Euch gebraucht,", "tokens": ["\u00bb", "geht", ",", "Pa\u00b7ter", "A\u00b7mbros", "!", "Wenn", "man", "Euch", "ge\u00b7braucht", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "NE", "$.", "KOUS", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So pflegt man Euch vor sich zu laden.\u00ab \u2013", "tokens": ["So", "pflegt", "man", "Euch", "vor", "sich", "zu", "la\u00b7den", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "APPR", "PRF", "PTKZU", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbich mu\u00df Euch sprechen, Euch sprechen, Durchlaucht!", "tokens": ["\u00bb", "ich", "mu\u00df", "Euch", "spre\u00b7chen", ",", "Euch", "spre\u00b7chen", ",", "Durch\u00b7laucht", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPER", "VVINF", "$,", "PPER", "VVINF", "$,", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sonst n\u00e4hme die Seele mir Schaden.", "tokens": ["Sonst", "n\u00e4h\u00b7me", "die", "See\u00b7le", "mir", "Scha\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Geschwiegen hab' ich von Opern, Balletts", "tokens": ["Ge\u00b7schwie\u00b7gen", "hab'", "ich", "von", "O\u00b7pern", ",", "Bal\u00b7letts"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVPP", "VAFIN", "PPER", "APPR", "NN", "$,", "NE"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und allem, was, Euch zu locken ins Netz,", "tokens": ["Und", "al\u00b7lem", ",", "was", ",", "Euch", "zu", "lo\u00b7cken", "ins", "Netz", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "$,", "PPER", "PTKZU", "VVINF", "APPRART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Der Teufel gebraucht als K\u00f6der;", "tokens": ["Der", "Teu\u00b7fel", "ge\u00b7braucht", "als", "K\u00f6\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KOKOM", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Nichts sagt' ich zum Spiele, wie arg Ihr's triebt,", "tokens": ["Nichts", "sagt'", "ich", "zum", "Spie\u00b7le", ",", "wie", "arg", "Ih\u00b7r's", "triebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPRART", "NN", "$,", "PWAV", "ADJD", "PIS", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Nichts zu den Aktricen, die Ihr geliebt;", "tokens": ["Nichts", "zu", "den", "Ak\u00b7tri\u00b7cen", ",", "die", "Ihr", "ge\u00b7liebt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Doch schwieg ich zu dem, was jetzt sich begiebt,", "tokens": ["Doch", "schwieg", "ich", "zu", "dem", ",", "was", "jetzt", "sich", "be\u00b7giebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "$,", "PRELS", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Ein Judas w\u00e4r' ich, ein schn\u00f6der.", "tokens": ["Ein", "Ju\u00b7das", "w\u00e4r'", "ich", ",", "ein", "schn\u00f6\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "ART", "ADJA", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Mit Grausen h\u00f6r' ich, verhandeln wollt", "tokens": ["Mit", "Grau\u00b7sen", "h\u00f6r'", "ich", ",", "ver\u00b7han\u00b7deln", "wollt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,", "VVINF", "VMFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihr selber, der Landesvater,", "tokens": ["Ihr", "sel\u00b7ber", ",", "der", "Lan\u00b7des\u00b7va\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sechstausend Landeskinder f\u00fcr Gold,", "tokens": ["Sech\u00b7stau\u00b7send", "Lan\u00b7des\u00b7kin\u00b7der", "f\u00fcr", "Gold", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Zu bauen ein neues Theater;", "tokens": ["Zu", "bau\u00b7en", "ein", "neu\u00b7es", "The\u00b7a\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Ja derer, die ich zu Christen getauft,", "tokens": ["Ja", "de\u00b7rer", ",", "die", "ich", "zu", "Chris\u00b7ten", "ge\u00b7tauft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PDS", "$,", "PRELS", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Zweihundert habt Ihr bereits verkauft,", "tokens": ["Zwei\u00b7hun\u00b7dert", "habt", "Ihr", "be\u00b7reits", "ver\u00b7kauft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Verkauft an die britischen Werber;", "tokens": ["Ver\u00b7kauft", "an", "die", "bri\u00b7ti\u00b7schen", "Wer\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Sie gehen, verdorben durch Branntewein", "tokens": ["Sie", "ge\u00b7hen", ",", "ver\u00b7dor\u00b7ben", "durch", "Brann\u00b7te\u00b7wein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVINF", "$,", "VVPP", "APPR", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Und Ketzerlehren, zur H\u00f6lle ein,", "tokens": ["Und", "Ket\u00b7zer\u00b7leh\u00b7ren", ",", "zur", "H\u00f6l\u00b7le", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Und die Frucht von all meinen Litanein", "tokens": ["Und", "die", "Frucht", "von", "all", "mei\u00b7nen", "Li\u00b7tan\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Pfl\u00fcckt nun f\u00fcr sich der Verderber.", "tokens": ["Pfl\u00fcckt", "nun", "f\u00fcr", "sich", "der", "Ver\u00b7der\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PRF", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Brecht ab, bei Eurem ewigen Heil,", "tokens": ["Brecht", "ab", ",", "bei", "Eu\u00b7rem", "e\u00b7wi\u00b7gen", "Heil", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Durchlaucht, den greulichen Handel,", "tokens": ["Durch\u00b7laucht", ",", "den", "greu\u00b7li\u00b7chen", "Han\u00b7del", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und Absolution wird Euch zu teil", "tokens": ["Und", "Ab\u00b7so\u00b7lu\u00b7ti\u00b7on", "wird", "Euch", "zu", "teil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr den sonstigen Lebenswandel.\u00ab", "tokens": ["F\u00fcr", "den", "sons\u00b7ti\u00b7gen", "Le\u00b7bens\u00b7wan\u00b7del", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "\u00bbnun? Geht es zu Ende mit Eurem Sermon?\u00ab", "tokens": ["\u00bb", "nun", "?", "Geht", "es", "zu", "En\u00b7de", "mit", "Eu\u00b7rem", "Ser\u00b7mon", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$.", "VVFIN", "PPER", "APPR", "NN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Ruft zornig der Herzog; \u00bbIhr h\u00f6rtet es schon,", "tokens": ["Ruft", "zor\u00b7nig", "der", "Her\u00b7zog", ";", "\u00bb", "Ihr", "h\u00f6r\u00b7tet", "es", "schon", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NE", "$.", "$(", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Nicht l\u00e4\u00dft der Beschlu\u00df sich \u00e4ndern;", "tokens": ["Nicht", "l\u00e4\u00dft", "der", "Be\u00b7schlu\u00df", "sich", "\u00e4n\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Allt\u00e4glich ist solcher Handel ja,", "tokens": ["All\u00b7t\u00e4g\u00b7lich", "ist", "sol\u00b7cher", "Han\u00b7del", "ja", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Auch sehnen die Burschen, sobald sie erst da,", "tokens": ["Auch", "seh\u00b7nen", "die", "Bur\u00b7schen", ",", "so\u00b7bald", "sie", "erst", "da", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "ADV", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Sich nicht mehr zur\u00fcck aus Amerika", "tokens": ["Sich", "nicht", "mehr", "zu\u00b7r\u00fcck", "aus", "A\u00b7me\u00b7ri\u00b7ka"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "PTKNEG", "ADV", "PTKVZ", "APPR", "NE"], "meter": "+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.11": {"text": "Nach den gl\u00fccklichen Vaterl\u00e4ndern.\u00ab", "tokens": ["Nach", "den", "gl\u00fcck\u00b7li\u00b7chen", "Va\u00b7ter\u00b7l\u00e4n\u00b7dern", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Er geht, da ruft noch hinter ihm her", "tokens": ["Er", "geht", ",", "da", "ruft", "noch", "hin\u00b7ter", "ihm", "her"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "ADV", "APPR", "PPER", "APZR"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der wackere Pater entr\u00fcstet:", "tokens": ["Der", "wa\u00b7cke\u00b7re", "Pa\u00b7ter", "ent\u00b7r\u00fcs\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "\u00bbdurchlaucht, das th\u00e4tet Ihr nimmermehr,", "tokens": ["\u00bb", "durch\u00b7laucht", ",", "das", "th\u00e4\u00b7tet", "Ihr", "nim\u00b7mer\u00b7mehr", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "$,", "PDS", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wofern Ihr die Folgen w\u00fc\u00dftet!", "tokens": ["Wo\u00b7fern", "Ihr", "die", "Fol\u00b7gen", "w\u00fc\u00df\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Nicht hab' ich's, bei aller Ehrfurcht, Hehl:", "tokens": ["Nicht", "hab'", "ich's", ",", "bei", "al\u00b7ler", "Ehr\u00b7furcht", ",", "Hehl", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "PIS", "$,", "APPR", "PIAT", "NN", "$,", "NN", "$."], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.6": {"text": "Eh geht durch ein Nadel\u00f6hr ein Kamel,", "tokens": ["Eh", "geht", "durch", "ein", "Na\u00b7de\u00b7l\u00f6hr", "ein", "Ka\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Als da\u00df Ihr k\u00e4mt in den Himmel!", "tokens": ["Als", "da\u00df", "Ihr", "k\u00e4mt", "in", "den", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Die Teufel werden \u00fcber dies Geld", "tokens": ["Die", "Teu\u00b7fel", "wer\u00b7den", "\u00fc\u00b7ber", "dies", "Geld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "PDS", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Mehr jauchzen, als wenn ein Engel f\u00e4llt;", "tokens": ["Mehr", "jauch\u00b7zen", ",", "als", "wenn", "ein", "En\u00b7gel", "f\u00e4llt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "$,", "KOKOM", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Ich h\u00f6re schon, wie die H\u00f6lle gellt", "tokens": ["Ich", "h\u00f6\u00b7re", "schon", ",", "wie", "die", "H\u00f6l\u00b7le", "gellt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "ART", "NN", "VVPP"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Von Jubel und Freudenget\u00fcmmel.\u00ab", "tokens": ["Von", "Ju\u00b7bel", "und", "Freu\u00b7den\u00b7ge\u00b7t\u00fcm\u00b7mel", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Vergebens. Vom Marktplatz Paar an Paar", "tokens": ["Ver\u00b7ge\u00b7bens", ".", "Vom", "Markt\u00b7platz", "Paar", "an", "Paar"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "APPRART", "NN", "NN", "APPR", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Fort ziehen die jungen Soldaten;", "tokens": ["Fort", "zie\u00b7hen", "die", "jun\u00b7gen", "Sol\u00b7da\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "F\u00fcr jeden zahlten die Werber bar", "tokens": ["F\u00fcr", "je\u00b7den", "zahl\u00b7ten", "die", "Wer\u00b7ber", "bar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "ART", "NN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zweihundert Holl\u00e4nder Dukaten.", "tokens": ["Zwei\u00b7hun\u00b7dert", "Hol\u00b7l\u00e4n\u00b7der", "Du\u00b7ka\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "NN", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.5": {"text": "Im Saale neben dem Schlafgemach", "tokens": ["Im", "Saa\u00b7le", "ne\u00b7ben", "dem", "Schlaf\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text":