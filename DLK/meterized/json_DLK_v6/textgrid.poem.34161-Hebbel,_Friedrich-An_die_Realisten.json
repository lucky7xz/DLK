{"textgrid.poem.34161": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An die Realisten", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wahrheit wollt ihr; ich auch! Doch mir gen\u00fcgt es, die Thr\u00e4ne", "tokens": ["Wahr\u00b7heit", "wollt", "ihr", ";", "ich", "auch", "!", "Doch", "mir", "ge\u00b7n\u00fcgt", "es", ",", "die", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "$.", "PPER", "ADV", "$.", "KON", "PPER", "VVFIN", "PPER", "$,", "ART", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Aufzufangen, inde\u00df Boz ihr den Schnupfen gesellt.", "tokens": ["Auf\u00b7zu\u00b7fan\u00b7gen", ",", "in\u00b7de\u00df", "Boz", "ihr", "den", "Schnup\u00b7fen", "ge\u00b7sellt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "NE", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "L\u00e4ugnen l\u00e4\u00dft es sich nicht, er folgt ihr im Leben best\u00e4ndig,", "tokens": ["L\u00e4ug\u00b7nen", "l\u00e4\u00dft", "es", "sich", "nicht", ",", "er", "folgt", "ihr", "im", "Le\u00b7ben", "be\u00b7st\u00e4n\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "PTKNEG", "$,", "PPER", "VVFIN", "PPER", "APPRART", "NN", "ADJD", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Doch ein gebildeter Sinn schaudert vor solcher Natur.", "tokens": ["Doch", "ein", "ge\u00b7bil\u00b7de\u00b7ter", "Sinn", "schau\u00b7dert", "vor", "sol\u00b7cher", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}}}}}