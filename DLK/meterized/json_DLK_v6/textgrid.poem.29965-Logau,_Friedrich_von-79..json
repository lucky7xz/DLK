{"textgrid.poem.29965": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "79.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Marcus macht ein Testament, tr\u00f6st sein Weib mit letztem Willen;", "tokens": ["Mar\u00b7cus", "macht", "ein", "Tes\u00b7ta\u00b7ment", ",", "tr\u00f6st", "sein", "Weib", "mit", "letz\u00b7tem", "Wil\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Sie macht auch ein Testament, ihren erstlich zu erv\u00f6llen.", "tokens": ["Sie", "macht", "auch", "ein", "Tes\u00b7ta\u00b7ment", ",", "ih\u00b7ren", "erst\u00b7lich", "zu", "er\u00b7v\u00f6l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "PPOSAT", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}