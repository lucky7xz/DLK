{"dta.poem.10997": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ermahnung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie bist du doch, an Geistes Kr\u00e4ften, so schwach und un-\nbegreiflich klein,", "tokens": ["Wie", "bist", "du", "doch", ",", "an", "Geis\u00b7tes", "Kr\u00e4f\u00b7ten", ",", "so", "schwach", "und", "un", "be\u00b7greif\u00b7lich", "klein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "$,", "ADV", "ADJD", "KON", "TRUNC", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Und bildest, mit des Geistes Kr\u00e4ften, dir so viel unbegreif-", "tokens": ["Und", "bil\u00b7dest", ",", "mit", "des", "Geis\u00b7tes", "Kr\u00e4f\u00b7ten", ",", "dir", "so", "viel", "un\u00b7be\u00b7greif"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "APPR", "ART", "NN", "NN", "$,", "PPER", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "O Mensch! es will sich ja, von dir, ein kleines K\u00f6rnchen", "tokens": ["O", "Mensch", "!", "es", "will", "sich", "ja", ",", "von", "dir", ",", "ein", "klei\u00b7nes", "K\u00f6rn\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PPER", "VMFIN", "PRF", "ADV", "$,", "APPR", "PPER", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und dennoch soll sich die Natur, von dir, ja Gott fast, fassen", "tokens": ["Und", "den\u00b7noch", "soll", "sich", "die", "Na\u00b7tur", ",", "von", "dir", ",", "ja", "Gott", "fast", ",", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "VMFIN", "PRF", "ART", "NN", "$,", "APPR", "PPER", "$,", "ADV", "NN", "ADV", "$,", "VVINF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "Sey nicht so stolz! besinne dich! und la\u00df die Demuth blo\u00df", "tokens": ["Sey", "nicht", "so", "stolz", "!", "be\u00b7sin\u00b7ne", "dich", "!", "und", "la\u00df", "die", "De\u00b7muth", "blo\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADJD", "$.", "VVFIN", "PPER", "$.", "KON", "VVIMP", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Nebst einem \u00e4msigen Bestreben, in den Bewundrungs-werthen", "tokens": ["Nebst", "ei\u00b7nem", "\u00e4m\u00b7si\u00b7gen", "Be\u00b7stre\u00b7ben", ",", "in", "den", "Be\u00b7wun\u00b7drungs\u00b7wert\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-++-+-+-", "measure": "unknown.measure.octa.plus"}, "line.7": {"text": "Der \u00fcberall vorhandnen Gottheit, des Sch\u00f6pfers, Lieb und", "tokens": ["Der", "\u00fc\u00b7be\u00b7rall", "vor\u00b7hand\u00b7nen", "Got\u00b7theit", ",", "des", "Sch\u00f6p\u00b7fers", ",", "Lieb", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN", "$,", "ART", "NN", "$,", "NN", "KON"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Den Vorwurf deines Gottesdienstes, und deines Lebens", "tokens": ["Den", "Vor\u00b7wurf", "dei\u00b7nes", "Got\u00b7tes\u00b7diens\u00b7tes", ",", "und", "dei\u00b7nes", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}}}}