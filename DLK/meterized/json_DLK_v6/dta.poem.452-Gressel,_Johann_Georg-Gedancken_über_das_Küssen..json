{"dta.poem.452": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Gedancken \u00fcber das K\u00fcssen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mit den Gr\u00f6\u00dften ist dis eins von Jdalis Geheimnissen/", "tokens": ["Mit", "den", "Gr\u00f6\u00df\u00b7ten", "ist", "dis", "eins", "von", "Jda\u00b7lis", "Ge\u00b7heim\u00b7nis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PDS", "PIS", "APPR", "NE", "NN", "$("], "meter": "--+--+--+--+--", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Da\u00df sie Macht\u00df at zu verstecken eine solche S\u00fcssigkeit/", "tokens": ["Da\u00df", "sie", "Macht\u00df", "at", "zu", "ver\u00b7ste\u00b7cken", "ei\u00b7ne", "sol\u00b7che", "S\u00fcs\u00b7sig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "NE", "PTKZU", "VVINF", "ART", "PIAT", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Die so sehr durchdringend ist/ in dem ", "tokens": ["Die", "so", "sehr", "durch\u00b7drin\u00b7gend", "ist", "/", "in", "dem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADV", "ADV", "VVPP", "VAFIN", "$(", "APPR", "ART"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und da\u00df/ sie was mehr als Zucker auf die Purpur Lippen streut.", "tokens": ["Und", "da\u00df", "/", "sie", "was", "mehr", "als", "Zu\u00b7cker", "auf", "die", "Pur\u00b7pur", "Lip\u00b7pen", "streut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$(", "PPER", "PWS", "PIS", "KOKOM", "NN", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Keine holde Wollust kan einen Geist so sehr entz\u00fccken", "tokens": ["Kei\u00b7ne", "hol\u00b7de", "Wol\u00b7lust", "kan", "ei\u00b7nen", "Geist", "so", "sehr", "ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "ART", "NN", "ADV", "ADV", "VVINF"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Und mit s\u00fcsser Lust bezaubern/ als des Munds Corallen-Belt/", "tokens": ["Und", "mit", "s\u00fcs\u00b7ser", "Lust", "be\u00b7zau\u00b7bern", "/", "als", "des", "Munds", "Coral\u00b7len\u00b7Belt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF", "$(", "KOUS", "ART", "NN", "NE", "$("], "meter": "+-+-+-+-+-++-+", "measure": "unknown.measure.octa.plus"}, "line.7": {"text": "Kan sich ein entz\u00fcckter Mund an der Liebsten Lippen dr\u00fccken/", "tokens": ["Kan", "sich", "ein", "ent\u00b7z\u00fcck\u00b7ter", "Mund", "an", "der", "Liebs\u00b7ten", "Lip\u00b7pen", "dr\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Alsdenn kostet er im Geiste jene Lust der andern Welt.", "tokens": ["Als\u00b7denn", "kos\u00b7tet", "er", "im", "Geis\u00b7te", "je\u00b7ne", "Lust", "der", "an\u00b7dern", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "PDAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Denn auf dieser Rosen-Au kommet beyder Geist zusammen/", "tokens": ["Denn", "auf", "die\u00b7ser", "Ro\u00b7sen\u00b7Au", "kom\u00b7met", "bey\u00b7der", "Geist", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "VVFIN", "PIAT", "NN", "PTKVZ", "$("], "meter": "----+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Um sich allda zubesprechen von der rechten Liebes-Lust.", "tokens": ["Um", "sich", "all\u00b7da", "zu\u00b7be\u00b7spre\u00b7chen", "von", "der", "rech\u00b7ten", "Lie\u00b7bes\u00b7Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "PAV", "VVINF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Jene Lufft der Seuffzer k\u00fchlt ihre heisse Liebes-Flammen/", "tokens": ["Je\u00b7ne", "Lufft", "der", "Seuff\u00b7zer", "k\u00fchlt", "ih\u00b7re", "heis\u00b7se", "Lie\u00b7bes\u00b7Flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Und die Rede-Kunst der Seelen labet die getreue Brust.", "tokens": ["Und", "die", "Re\u00b7de\u00b7Kunst", "der", "See\u00b7len", "la\u00b7bet", "die", "ge\u00b7treu\u00b7e", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}