{"dta.poem.7251": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Ungastlich oder nicht?", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Ungastlich hat man dich genannt,", "tokens": ["Un\u00b7gast\u00b7lich", "hat", "man", "dich", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PIS", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will deinen gr\u00fcnsten Kranz dir rauben,", "tokens": ["Will", "dei\u00b7nen", "gr\u00fcns\u00b7ten", "Kranz", "dir", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Volk mit der immer offnen Hand,", "tokens": ["Volk", "mit", "der", "im\u00b7mer", "off\u00b7nen", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit deinem argwohnlosen Glauben;", "tokens": ["Mit", "dei\u00b7nem", "arg\u00b7wohn\u00b7lo\u00b7sen", "Glau\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O rege dich, da\u00df nicht die Schmach", "tokens": ["O", "re\u00b7ge", "dich", ",", "da\u00df", "nicht", "die", "Schmach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "KOUS", "PTKNEG", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Auf deinem frommen Haupte laste,", "tokens": ["Auf", "dei\u00b7nem", "from\u00b7men", "Haup\u00b7te", "las\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und redlich, wie das Herz es sprach,", "tokens": ["Und", "red\u00b7lich", ",", "wie", "das", "Herz", "es", "sprach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So sprich es nach zu deinem Gaste:", "tokens": ["So", "sprich", "es", "nach", "zu", "dei\u00b7nem", "Gas\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201efremdling an meiner Marken Stein,", "tokens": ["\u201e", "fremd\u00b7ling", "an", "mei\u00b7ner", "Mar\u00b7ken", "Stein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mann mit der Stirne tr\u00fcben Falten,", "tokens": ["Mann", "mit", "der", "Stir\u00b7ne", "tr\u00fc\u00b7ben", "Fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "O, greif in deines Busens Schrein,", "tokens": ["O", ",", "greif", "in", "dei\u00b7nes", "Bu\u00b7sens", "Schrein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lass' die eigne Stimme walten.", "tokens": ["Und", "lass'", "die", "eig\u00b7ne", "Stim\u00b7me", "wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht soll bestochner Zeugen Schaar", "tokens": ["Nicht", "soll", "be\u00b7stoch\u00b7ner", "Zeu\u00b7gen", "Schaar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VMFIN", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Uns am bestochnen Worte r\u00e4chen,", "tokens": ["Uns", "am", "be\u00b7stoch\u00b7nen", "Wor\u00b7te", "r\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nein, Zeug' und Richter sollst du klar", "tokens": ["Nein", ",", "Zeug'", "und", "Rich\u00b7ter", "sollst", "du", "klar"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "KON", "NN", "VMFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dir selbst das freie Urtheil sprechen.", "tokens": ["Dir", "selbst", "das", "frei\u00b7e", "Ur\u00b7theil", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "F\u00fchlst du das Herz in dir, nicht hei\u00df", "tokens": ["F\u00fchlst", "du", "das", "Herz", "in", "dir", ",", "nicht", "hei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PPER", "$,", "PTKNEG", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch ehrlich, uns entgegen schlagen,", "tokens": ["Doch", "ehr\u00b7lich", ",", "uns", "ent\u00b7ge\u00b7gen", "schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PPER", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Wort kein falsch und tr\u00fcgend Gleis,", "tokens": ["Dein", "Wort", "kein", "falsch", "und", "tr\u00fc\u00b7gend", "Gleis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PIAT", "ADJD", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Besteckend was die Lippen tragen,", "tokens": ["Be\u00b7ste\u00b7ckend", "was", "die", "Lip\u00b7pen", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fchlst du ein Gast dich wie er lieb", "tokens": ["F\u00fchlst", "du", "ein", "Gast", "dich", "wie", "er", "lieb"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "PPER", "PWAV", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dir an dem eignen Hausaltare,", "tokens": ["Dir", "an", "dem", "eig\u00b7nen", "Haus\u00b7al\u00b7ta\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dann frisch heran \u2014 nicht wie ein Dieb,", "tokens": ["Dann", "frisch", "he\u00b7ran", "nicht", "wie", "ein", "Dieb", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$(", "PTKNEG", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Nein, frisch, mit fr\u00f6hlicher Fanfare!", "tokens": ["Nein", ",", "frisch", ",", "mit", "fr\u00f6h\u00b7li\u00b7cher", "Fan\u00b7fa\u00b7re", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wer unsres Landes Sitte ehrt,", "tokens": ["Wer", "uns\u00b7res", "Lan\u00b7des", "Sit\u00b7te", "ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und auch dem seinen h\u00e4lt die Treue \u2014", "tokens": ["Und", "auch", "dem", "sei\u00b7nen", "h\u00e4lt", "die", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "PPOSAT", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hier ist der Sitz an unserm Heerd!", "tokens": ["Hier", "ist", "der", "Sitz", "an", "un\u00b7serm", "Heerd", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hier unsres Bruderkusses Weihe!", "tokens": ["Hier", "uns\u00b7res", "Bru\u00b7der\u00b7kus\u00b7ses", "Wei\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wer fremden Volkes Herzen stellt", "tokens": ["Wer", "frem\u00b7den", "Vol\u00b7kes", "Her\u00b7zen", "stellt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADJA", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gleich seinem in gerechter Wage \u2014", "tokens": ["Gleich", "sei\u00b7nem", "in", "ge\u00b7rech\u00b7ter", "Wa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Hier unsre Hand, da\u00df er das Zelt", "tokens": ["Hier", "uns\u00b7re", "Hand", ",", "da\u00df", "er", "das", "Zelt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sich auf bei unsern Zelten schlage!", "tokens": ["Sich", "auf", "bei", "un\u00b7sern", "Zel\u00b7ten", "schla\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch sagt ein gl\u00fch' Err\u00f6then dir,", "tokens": ["Doch", "sagt", "ein", "gl\u00fch'", "Er\u00b7r\u00f6\u00b7then", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du g\u00f6nntest lieber einer andern", "tokens": ["Du", "g\u00f6nn\u00b7test", "lie\u00b7ber", "ei\u00b7ner", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als deiner Schwelle gleiche Zier \u2014", "tokens": ["Als", "dei\u00b7ner", "Schwel\u00b7le", "glei\u00b7che", "Zier"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Brich auf, und m\u00f6gest eilends wandern!", "tokens": ["Brich", "auf", ",", "und", "m\u00f6\u00b7gest", "ei\u00b7lends", "wan\u00b7dern", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "KON", "VMFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wir sind ein friedlich still Geschlecht", "tokens": ["Wir", "sind", "ein", "fried\u00b7lich", "still", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit lichtem Blick und blonden Haaren,", "tokens": ["Mit", "lich\u00b7tem", "Blick", "und", "blon\u00b7den", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Doch unsres Heerdes heilig Recht", "tokens": ["Doch", "uns\u00b7res", "Heer\u00b7des", "hei\u00b7lig", "Recht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das wissen kr\u00e4ftig wir zu wahren.", "tokens": ["Das", "wis\u00b7sen", "kr\u00e4f\u00b7tig", "wir", "zu", "wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Luft die unsern Odem regt,", "tokens": ["Die", "Luft", "die", "un\u00b7sern", "O\u00b7dem", "regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Grund wo unsre Gr\u00e4ber bl\u00fchen,", "tokens": ["Der", "Grund", "wo", "uns\u00b7re", "Gr\u00e4\u00b7ber", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Scholle die uns Nahrung tr\u00e4gt,", "tokens": ["Die", "Schol\u00b7le", "die", "uns", "Nah\u00b7rung", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Tempel wo wir gl\u00e4ubig knieen,", "tokens": ["Der", "Tem\u00b7pel", "wo", "wir", "gl\u00e4u\u00b7big", "kni\u00b7e\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PWAV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Die soll kein frevler Spott entweihn,", "tokens": ["Die", "soll", "kein", "frev\u00b7ler", "Spott", "ent\u00b7weihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Feigen Schmach und Schamerr\u00f6then,", "tokens": ["Dem", "Fei\u00b7gen", "Schmach", "und", "Scha\u00b7mer\u00b7r\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der an des Heiligthumes Schrein", "tokens": ["Der", "an", "des", "Hei\u00b7lig\u00b7thu\u00b7mes", "Schrein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "L\u00e4\u00dft eine falsche Sohle treten!", "tokens": ["L\u00e4\u00dft", "ei\u00b7ne", "fal\u00b7sche", "Soh\u00b7le", "tre\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch einem Gru\u00df aus treuem Muth", "tokens": ["Doch", "ei\u00b7nem", "Gru\u00df", "aus", "treu\u00b7em", "Muth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem nicken ehrlich wir entgegen.", "tokens": ["Dem", "ni\u00b7cken", "ehr\u00b7lich", "wir", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hat jeder doch sein eignes Blut,", "tokens": ["Hat", "je\u00b7der", "doch", "sein", "eig\u00b7nes", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und seiner eignen Heimath Segen.", "tokens": ["Und", "sei\u00b7ner", "eig\u00b7nen", "Hei\u00b7math", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn deine Ader k\u00e4lter rinnt,", "tokens": ["Wenn", "dei\u00b7ne", "A\u00b7der", "k\u00e4l\u00b7ter", "rinnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So m\u00fcssen billig wir ermessen:", "tokens": ["So", "m\u00fcs\u00b7sen", "bil\u00b7lig", "wir", "er\u00b7mes\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "pun