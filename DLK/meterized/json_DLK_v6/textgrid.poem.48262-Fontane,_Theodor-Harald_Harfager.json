{"textgrid.poem.48262": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Harald Harfager", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da sprach K\u00f6nig Harald (Harfager zubenannt):", "tokens": ["Da", "sprach", "K\u00f6\u00b7nig", "Ha\u00b7rald", "(", "Har\u00b7fa\u00b7ger", "zu\u00b7be\u00b7nannt", ")", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "NE", "NE", "$(", "NN", "VVFIN", "$(", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "\u00bbwi\u00dft, ich habe Boten an Rynhild ausgesandt,", "tokens": ["\u00bb", "wi\u00dft", ",", "ich", "ha\u00b7be", "Bo\u00b7ten", "an", "Ryn\u00b7hild", "aus\u00b7ge\u00b7sandt", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "PPER", "VAFIN", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "An Rynhild, Blaatands Tochter. Und ziehet sie morgen ein,", "tokens": ["An", "Ryn\u00b7hild", ",", "Blaa\u00b7tands", "Toch\u00b7ter", ".", "Und", "zie\u00b7het", "sie", "mor\u00b7gen", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ADJA", "NN", "$.", "KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Unter meinen Frauen allen soll fortan auch Rynhilde sein.\u00ab", "tokens": ["Un\u00b7ter", "mei\u00b7nen", "Frau\u00b7en", "al\u00b7len", "soll", "for\u00b7tan", "auch", "Ryn\u00b7hil\u00b7de", "sein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PIS", "VMFIN", "ADV", "ADV", "NN", "VAINF", "$.", "$("], "meter": "+-+-+-+--++-+--+", "measure": "trochaic.octa.plus.relaxed"}, "line.5": {"text": "Sprach es K\u00f6nig Harald. Und auf und ab im Saal", "tokens": ["Sprach", "es", "K\u00f6\u00b7nig", "Ha\u00b7rald", ".", "Und", "auf", "und", "ab", "im", "Saal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "NN", "NE", "$.", "KON", "PTKVZ", "KON", "PTKVZ", "APPRART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Im Schlosse zu Drammen sa\u00dfen die Gro\u00dfen allzumal,", "tokens": ["Im", "Schlos\u00b7se", "zu", "Dram\u00b7men", "sa\u00b7\u00dfen", "die", "Gro\u00b7\u00dfen", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Und dazwischen in lachender Jugend und wie Kinder anzuschaun", "tokens": ["Und", "da\u00b7zwi\u00b7schen", "in", "la\u00b7chen\u00b7der", "Ju\u00b7gend", "und", "wie", "Kin\u00b7der", "an\u00b7zu\u00b7schaun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "APPR", "ADJA", "NN", "KON", "PWAV", "NN", "VVIZU"], "meter": "-----+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Sa\u00dfen blond und stolz und gl\u00fccklich Harald Harfagers neunundzwanzig Fraun.", "tokens": ["Sa\u00b7\u00dfen", "blond", "und", "stolz", "und", "gl\u00fcck\u00b7lich", "Ha\u00b7rald", "Har\u00b7fa\u00b7gers", "neun\u00b7und\u00b7zwan\u00b7zig", "Fraun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "KON", "ADJD", "NE", "NE", "CARD", "NN", "$."], "meter": "+-+-+-+-+-+--+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.9": {"text": "Aber pl\u00f6tzlich schwieg das Fl\u00fcstern, Olaf Thureson trat in den Kreis.", "tokens": ["A\u00b7ber", "pl\u00f6tz\u00b7lich", "schwieg", "das", "Fl\u00fcs\u00b7tern", ",", "O\u00b7laf", "Thu\u00b7re\u00b7son", "trat", "in", "den", "Kreis", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,", "NE", "NE", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Olaf Thureson, Haralds Bote. Vor den K\u00f6nig tritt er und spricht:", "tokens": ["O\u00b7laf", "Thu\u00b7re\u00b7son", ",", "Ha\u00b7ralds", "Bo\u00b7te", ".", "Vor", "den", "K\u00f6\u00b7nig", "tritt", "er", "und", "spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NE", "NN", "$.", "APPR", "ART", "NN", "VVFIN", "PPER", "KON", "VVFIN", "$."], "meter": "+-+--+-+-+-+-+--+", "measure": "trochaic.octa.plus.relaxed"}, "line.11": {"text": "\u00bbich bringe Rynhilds Antwort, Rynhilden aber bring' ich ", "tokens": ["\u00bb", "ich", "brin\u00b7ge", "Ryn\u00b7hilds", "Ant\u00b7wort", ",", "Ryn\u00b7hil\u00b7den", "a\u00b7ber", "bring'", "ich"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "NE", "NN", "$,", "NN", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "In der Marmorhalle zu Roskild', meine Botschaft zu h\u00f6ren bereit,", "tokens": ["In", "der", "Mar\u00b7mo\u00b7rhal\u00b7le", "zu", "Ros\u00b7kild'", ",", "mei\u00b7ne", "Bot\u00b7schaft", "zu", "h\u00f6\u00b7ren", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "ADJD", "$,"], "meter": "--+-+--+-+-+--+--+", "measure": "iambic.septa.relaxed"}, "line.13": {"text": "Stand sie, Korallen im Goldhaar, an K\u00f6nig Blaatands Seit',", "tokens": ["Stand", "sie", ",", "Ko\u00b7ral\u00b7len", "im", "Gold\u00b7haar", ",", "an", "K\u00f6\u00b7nig", "Blaa\u00b7tands", "Seit'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "APPRART", "NN", "$,", "APPR", "NN", "NE", "NE", "$,"], "meter": "-+-+--++-+-+-+", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "L\u00e4ngst war ihr Kunde gekommen, um wessentwill' ich kam,", "tokens": ["L\u00e4ngst", "war", "ihr", "Kun\u00b7de", "ge\u00b7kom\u00b7men", ",", "um", "wes\u00b7sent\u00b7will'", "ich", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$,", "KOUI", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Und sie lachte, dieweil sie die Spange wie spielend von ihrem Arme nahm.", "tokens": ["Und", "sie", "lach\u00b7te", ",", "die\u00b7weil", "sie", "die", "Span\u00b7ge", "wie", "spie\u00b7lend", "von", "ih\u00b7rem", "Ar\u00b7me", "nahm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "KOKOM", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "--+-+-+-+--+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.16": {"text": "K\u00f6nig Harald ist Herr \u00fcber Norweg, \u00fcber Norwegs Frauen auch,", "tokens": ["K\u00f6\u00b7nig", "Ha\u00b7rald", "ist", "Herr", "\u00fc\u00b7ber", "Nor\u00b7weg", ",", "\u00fc\u00b7ber", "Nor\u00b7wegs", "Frau\u00b7en", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "NN", "APPR", "NE", "$,", "APPR", "NN", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Aber euer Brauch in Drammen ist nicht in Roskilde Brauch,", "tokens": ["A\u00b7ber", "eu\u00b7er", "Brauch", "in", "Dram\u00b7men", "ist", "nicht", "in", "Ros\u00b7kil\u00b7de", "Brauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "VAFIN", "PTKNEG", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+--+-+--+", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Und liebt' ich ihn, wie ich ihn hasse, meine Liebe h\u00e4tte nicht Eil',", "tokens": ["Und", "liebt'", "ich", "ihn", ",", "wie", "ich", "ihn", "has\u00b7se", ",", "mei\u00b7ne", "Lie\u00b7be", "h\u00e4t\u00b7te", "nicht", "Eil'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-+-+-+--+", "measure": "iambic.octa.plus.octa.plus.chol"}, "line.19": {"text": "Ein ganzes Herz will Rynhilde, nicht eines Herzens drei\u00dfigsten Teil.\u00ab", "tokens": ["Ein", "gan\u00b7zes", "Herz", "will", "Ryn\u00b7hil\u00b7de", ",", "nicht", "ei\u00b7nes", "Her\u00b7zens", "drei\u00b7\u00dfigs\u00b7ten", "Teil", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "NE", "$,", "PTKNEG", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+--+-+--+", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "Olaf Thureson sprach's. Wie Freude zuckt's um die Lippe der Fraun,", "tokens": ["O\u00b7laf", "Thu\u00b7re\u00b7son", "sprach'", "s.", "Wie", "Freu\u00b7de", "zuckt's", "um", "die", "Lip\u00b7pe", "der", "Fraun", ","], "token_info": ["word", "word", "word", "abbreviation", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "VVIMP", "PWAV", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+--+--+", "measure": "trochaic.septa.relaxed"}, "line.21": {"text": "Aber rot auf Haralds Stirne war das Zornesmal zu schaun,", "tokens": ["A\u00b7ber", "rot", "auf", "Ha\u00b7ralds", "Stir\u00b7ne", "war", "das", "Zor\u00b7nes\u00b7mal", "zu", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NE", "NN", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.22": {"text": "Er rief: \u00bbUnd wirken nicht Worte, so wirke denn das Schwert,", "tokens": ["Er", "rief", ":", "\u00bb", "Und", "wir\u00b7ken", "nicht", "Wor\u00b7te", ",", "so", "wir\u00b7ke", "denn", "das", "Schwert", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "KON", "VVFIN", "PTKNEG", "NN", "$,", "ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Noch nie hat K\u00f6nig Harald ein K\u00f6nigskind umsonst begehrt.\u00ab", "tokens": ["Noch", "nie", "hat", "K\u00f6\u00b7nig", "Ha\u00b7rald", "ein", "K\u00f6\u00b7nigs\u00b7kind", "um\u00b7sonst", "be\u00b7gehrt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "VAFIN", "NE", "NE", "ART", "NN", "ADV", "VVPP", "$.", "$("], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.24": {"text": "Und er stieg hinab zum Strande. Segelfertig Schiff um Schiff;", "tokens": ["Und", "er", "stieg", "hin\u00b7ab", "zum", "Stran\u00b7de", ".", "Se\u00b7gel\u00b7fer\u00b7tig", "Schiff", "um", "Schiff", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$.", "ADJD", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.25": {"text": "Am ersten Tage vor\u00fcber an Skagens Horn und Riff,", "tokens": ["Am", "ers\u00b7ten", "Ta\u00b7ge", "vor\u00b7\u00fc\u00b7ber", "an", "Ska\u00b7gens", "Horn", "und", "Riff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "APPR", "NE", "NE", "KON", "NN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Am zweiten: Nebel und Windstill', alle Segel schlaff und matt,", "tokens": ["Am", "zwei\u00b7ten", ":", "Ne\u00b7bel", "und", "Wind\u00b7still'", ",", "al\u00b7le", "Se\u00b7gel", "schlaff", "und", "matt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$.", "NN", "KON", "NN", "$,", "PIAT", "NN", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.27": {"text": "Aber sieh, mit Ruderschlage geht es s\u00fcderw\u00e4rts in das Kattegatt.", "tokens": ["A\u00b7ber", "sieh", ",", "mit", "Ru\u00b7der\u00b7schla\u00b7ge", "geht", "es", "s\u00fc\u00b7der\u00b7w\u00e4rts", "in", "das", "Kat\u00b7te\u00b7gatt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "APPR", "NN", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+--+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.28": {"text": "Und am dritten Tag ein Windsto\u00df und zerrissen die Nebelschlei'r,", "tokens": ["Und", "am", "drit\u00b7ten", "Tag", "ein", "Wind\u00b7sto\u00df", "und", "zer\u00b7ris\u00b7sen", "die", "Ne\u00b7bel\u00b7schlei'r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.29": {"text": "Und am blauen Himmel die Sonne. \u00bbDie Sonne, die schickt uns Frej'r.", "tokens": ["Und", "am", "blau\u00b7en", "Him\u00b7mel", "die", "Son\u00b7ne", ".", "\u00bb", "Die", "Son\u00b7ne", ",", "die", "schickt", "uns", "Fre\u00b7j'", "r."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "APPRART", "ADJA", "NN", "ART", "NN", "$.", "$(", "ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "NN", "NE"], "meter": "--+-+--+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "Und bei Kriegsh\u00f6rnerblasen l\u00e4uft Harald ein in Roskilde-Fjord.", "tokens": ["Und", "bei", "Kriegs\u00b7h\u00f6r\u00b7ner\u00b7bla\u00b7sen", "l\u00e4uft", "Ha\u00b7rald", "ein", "in", "Ros\u00b7kil\u00b7de\u00b7\u00b7F\u00b7jord", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "NE", "ART", "APPR", "NE", "$."], "meter": "--+--+-++-+--+-+-", "measure": "anapaest.di.plus"}, "line.31": {"text": "Hoch oben aber am Saaltor, auf der Treppe von Roskild',", "tokens": ["Hoch", "o\u00b7ben", "a\u00b7ber", "am", "Saal\u00b7tor", ",", "auf", "der", "Trep\u00b7pe", "von", "Ros\u00b7kild'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADV", "APPRART", "NN", "$,", "APPR", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.32": {"text": "In Ruhe steht K\u00f6nig Blaatand und neben ihm Rynhild',", "tokens": ["In", "Ru\u00b7he", "steht", "K\u00f6\u00b7nig", "Blaa\u00b7tand", "und", "ne\u00b7ben", "ihm", "Ryn\u00b7hild'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NN", "NE", "KON", "APPR", "PPER", "NE", "$,"], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "Und sie z\u00e4hlen die Nordlandsschiffe, hundert Segel fast \u2013", "tokens": ["Und", "sie", "z\u00e4h\u00b7len", "die", "Nord\u00b7lands\u00b7schif\u00b7fe", ",", "hun\u00b7dert", "Se\u00b7gel", "fast", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "CARD", "NN", "ADV", "$("], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.34": {"text": "Am Bug steht K\u00f6nig Harald. Kriegswimpel flattern hoch am Mast.", "tokens": ["Am", "Bug", "steht", "K\u00f6\u00b7nig", "Ha\u00b7rald", ".", "Kriegs\u00b7wim\u00b7pel", "flat\u00b7tern", "hoch", "am", "Mast", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NE", "NE", "$.", "NN", "VVINF", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.35": {"text": "\u00bbkrieg bringt er!\u00ab Aber pl\u00f6tzlich ... welch' Wunder, das Wandel schuf,", "tokens": ["\u00bb", "krieg", "bringt", "er", "!", "\u00ab", "A\u00b7ber", "pl\u00f6tz\u00b7lich", "...", "welch'", "Wun\u00b7der", ",", "das", "Wan\u00b7del", "schuf", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "word", "wor