{"textgrid.poem.33730": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie lang durchbl\u00e4tterst noch du diese Rolle,", "tokens": ["Wie", "lang", "durch\u00b7bl\u00e4t\u00b7terst", "noch", "du", "die\u00b7se", "Rol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ADV", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Drauf jedes Unrecht steht, das du erlitten,", "tokens": ["Drauf", "je\u00b7des", "Un\u00b7recht", "steht", ",", "das", "du", "er\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "NN", "VVFIN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das deiner Brust mit Ha\u00df ward eingeschnitten", "tokens": ["Das", "dei\u00b7ner", "Brust", "mit", "Ha\u00df", "ward", "ein\u00b7ge\u00b7schnit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "NN", "APPR", "NN", "VAFIN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und einge\u00e4tzt mit langgen\u00e4hrtem Grolle?", "tokens": ["Und", "ein\u00b7ge\u00b7\u00e4tzt", "mit", "lang\u00b7ge\u00b7n\u00e4hr\u00b7tem", "Grol\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es kommt die Zeit noch, die erf\u00fcllungsvolle,", "tokens": ["Es", "kommt", "die", "Zeit", "noch", ",", "die", "er\u00b7f\u00fcl\u00b7lungs\u00b7vol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie kommt, wo du emporgerichtet mitten", "tokens": ["Sie", "kommt", ",", "wo", "du", "em\u00b7por\u00b7ge\u00b7rich\u00b7tet", "mit\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch deine Feinde gehst mit freien Schritten", "tokens": ["Durch", "dei\u00b7ne", "Fein\u00b7de", "gehst", "mit", "frei\u00b7en", "Schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und fragest, wer dich noch mi\u00dfachten wolle?", "tokens": ["Und", "fra\u00b7gest", ",", "wer", "dich", "noch", "mi\u00df\u00b7ach\u00b7ten", "wol\u00b7le", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dann wirst du jedes Denkmal der Entweihung,", "tokens": ["Dann", "wirst", "du", "je\u00b7des", "Denk\u00b7mal", "der", "Ent\u00b7wei\u00b7hung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wirst Grimm und Staub aus deinem Leben merzen", "tokens": ["Wirst", "Grimm", "und", "Staub", "aus", "dei\u00b7nem", "Le\u00b7ben", "mer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NE", "KON", "NN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und deine Seele tr\u00e4nken mit Befreiung.", "tokens": ["Und", "dei\u00b7ne", "See\u00b7le", "tr\u00e4n\u00b7ken", "mit", "Be\u00b7frei\u00b7ung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Erl\u00f6st von einem gro\u00dfen Menschenherzen,", "tokens": ["Er\u00b7l\u00f6st", "von", "ei\u00b7nem", "gro\u00b7\u00dfen", "Men\u00b7schen\u00b7her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wirst du die Tr\u00e4nen gl\u00fchender Verzeihung", "tokens": ["Wirst", "du", "die", "Tr\u00e4\u00b7nen", "gl\u00fc\u00b7hen\u00b7der", "Ver\u00b7zei\u00b7hung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ausweinen und die lange Nacht verschmerzen.", "tokens": ["Aus\u00b7wei\u00b7nen", "und", "die", "lan\u00b7ge", "Nacht", "ver\u00b7schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}