{"dta.poem.11926": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "87.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df einen Heilversuch dir meines Auges sagen,", "tokens": ["La\u00df", "ei\u00b7nen", "Heil\u00b7ver\u00b7such", "dir", "mei\u00b7nes", "Au\u00b7ges", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des \u00e4u\u00dfern, den du magst aufs Innre \u00fcbertragen.", "tokens": ["Des", "\u00e4u\u00b7\u00dfern", ",", "den", "du", "magst", "aufs", "Inn\u00b7re", "\u00fc\u00b7ber\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PPER", "VMFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mein Auge sah sich selbst von einem Flor umhangen,", "tokens": ["Mein", "Au\u00b7ge", "sah", "sich", "selbst", "von", "ei\u00b7nem", "Flor", "um\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von einem Wirrgeweb aus Punkten, Flecken, Schlangen.", "tokens": ["Von", "ei\u00b7nem", "Wirr\u00b7ge\u00b7web", "aus", "Punk\u00b7ten", ",", "Fle\u00b7cken", ",", "Schlan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein Netz der T\u00e4uschung, das die Sehkraft selbst sich wob,", "tokens": ["Ein", "Netz", "der", "T\u00e4u\u00b7schung", ",", "das", "die", "Seh\u00b7kraft", "selbst", "sich", "wob", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "ART", "NN", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das mit dem Blick sich senkt' und mit dem Blick sich hob.", "tokens": ["Das", "mit", "dem", "Blick", "sich", "senkt'", "und", "mit", "dem", "Blick", "sich", "hob", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "PRF", "VVFIN", "KON", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ein Schatten, welcher nie vom Lichte sich verlor,", "tokens": ["Ein", "Schat\u00b7ten", ",", "wel\u00b7cher", "nie", "vom", "Lich\u00b7te", "sich", "ver\u00b7lor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der, aus dem Aug' erzeugt, schwebt' \u00fcberall ihm vor;", "tokens": ["Der", ",", "aus", "dem", "Aug'", "er\u00b7zeugt", ",", "schwebt'", "\u00fc\u00b7be\u00b7rall", "ihm", "vor", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "NN", "VVPP", "$,", "VVFIN", "ADV", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nur um so n\u00e4chtlicher, als heller war der Tag,", "tokens": ["Nur", "um", "so", "n\u00e4cht\u00b7li\u00b7cher", ",", "als", "hel\u00b7ler", "war", "der", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "ADJA", "$,", "KOUS", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie vor der Unschuld wol die Schuld sich f\u00fchlen mag.", "tokens": ["Wie", "vor", "der", "Un\u00b7schuld", "wol", "die", "Schuld", "sich", "f\u00fch\u00b7len", "mag."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PWAV", "APPR", "ART", "NN", "ADV", "ART", "NN", "PRF", "VVFIN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Mir war davon die Lust an Gottes Welt benommen,", "tokens": ["Mir", "war", "da\u00b7von", "die", "Lust", "an", "Got\u00b7tes", "Welt", "be\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "ART", "NN", "APPR", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df rein ihr Sch\u00f6nes nicht mir sollt' ins Auge kommen;", "tokens": ["Da\u00df", "rein", "ihr", "Sch\u00f6\u00b7nes", "nicht", "mir", "sollt'", "ins", "Au\u00b7ge", "kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPOSAT", "NN", "PTKNEG", "PPER", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Getr\u00fcbt der Glanz der Flur, des Menschen Angesicht,", "tokens": ["Ge\u00b7tr\u00fcbt", "der", "Glanz", "der", "Flur", ",", "des", "Men\u00b7schen", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und jede Schrift, durch die der Geist zum Auge spricht.", "tokens": ["Und", "je\u00b7de", "Schrift", ",", "durch", "die", "der", "Geist", "zum", "Au\u00b7ge", "spricht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "APPR", "PRELS", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Den himmlischen Genu\u00df des Lichtes wollt' ich missen", "tokens": ["Den", "himm\u00b7li\u00b7schen", "Ge\u00b7nu\u00df", "des", "Lich\u00b7tes", "wollt'", "ich", "mis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ehr als ihn haben so versetzt mit Finsternissen.", "tokens": ["Ehr", "als", "ihn", "ha\u00b7ben", "so", "ver\u00b7setzt", "mit", "Fins\u00b7ter\u00b7nis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "VAFIN", "ADV", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Heilwasser heilen nicht, einfache noch zusammen", "tokens": ["Heil\u00b7was\u00b7ser", "hei\u00b7len", "nicht", ",", "ein\u00b7fa\u00b7che", "noch", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVINF", "PTKNEG", "$,", "ADJA", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gesetzte, weil sie rein dem Lichte nicht entstammen.", "tokens": ["Ge\u00b7setz\u00b7te", ",", "weil", "sie", "rein", "dem", "Lich\u00b7te", "nicht", "ent\u00b7stam\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADJD", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Sollt' ich die ird'sche Kunst des Augenarztes brauchen?", "tokens": ["Sollt'", "ich", "die", "ird'\u00b7sche", "Kunst", "des", "Au\u00b7gen\u00b7arz\u00b7tes", "brau\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich will mich in den Quell des Lichtes selber tauchen.", "tokens": ["Ich", "will", "mich", "in", "den", "Quell", "des", "Lich\u00b7tes", "sel\u00b7ber", "tau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Die L\u00fcfte waren blau, die Fluren waren gr\u00fcn,", "tokens": ["Die", "L\u00fcf\u00b7te", "wa\u00b7ren", "blau", ",", "die", "Flu\u00b7ren", "wa\u00b7ren", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und meinen Blick erhob zur Sonn' ich adlerk\u00fchn.", "tokens": ["Und", "mei\u00b7nen", "Blick", "er\u00b7hob", "zur", "Sonn'", "ich", "ad\u00b7ler\u00b7k\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Entweder soll die Welt in dir mir untergehn", "tokens": ["Ent\u00b7we\u00b7der", "soll", "die", "Welt", "in", "dir", "mir", "un\u00b7ter\u00b7gehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "NN", "APPR", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf immer, oder ich will rein wie du sie sehn.", "tokens": ["Auf", "im\u00b7mer", ",", "o\u00b7der", "ich", "will", "rein", "wie", "du", "sie", "sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "KON", "PPER", "VMFIN", "ADJD", "KOKOM", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Die Feuerwirbel lie\u00df ich mir im Auge wallen,", "tokens": ["Die", "Feu\u00b7er\u00b7wir\u00b7bel", "lie\u00df", "ich", "mir", "im", "Au\u00b7ge", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie sie mich blendeten f\u00fchlt' ich mit Wohlgefallen.", "tokens": ["Wie", "sie", "mich", "blen\u00b7de\u00b7ten", "f\u00fchlt'", "ich", "mit", "Wohl\u00b7ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.14": {"line.1": {"text": "Solange duldet' ich den Einstrom, bis zusammen", "tokens": ["So\u00b7lan\u00b7ge", "dul\u00b7det'", "ich", "den", "Ein\u00b7strom", ",", "bis", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$,", "KOUS", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die krausen Schlanggewind' in eine Masse schwammen.", "tokens": ["Die", "krau\u00b7sen", "Schlang\u00b7ge\u00b7wind'", "in", "ei\u00b7ne", "Mas\u00b7se", "schwam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Vom Himmel blickt' ich dann zur\u00fcck zur Erdenflur,", "tokens": ["Vom", "Him\u00b7mel", "blickt'", "ich", "dann", "zu\u00b7r\u00fcck", "zur", "Er\u00b7den\u00b7flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und statt der Schlangen sah ich Sonnenblendung nur.", "tokens": ["Und", "statt", "der", "Schlan\u00b7gen", "sah", "ich", "Son\u00b7nen\u00b7blen\u00b7dung", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Die lichte Finsternis zerflo\u00df dann, und o Gl\u00fcck,", "tokens": ["Die", "lich\u00b7te", "Fins\u00b7ter\u00b7nis", "zer\u00b7flo\u00df", "dann", ",", "und", "o", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$,", "KON", "FM", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Schlangen kehrten nicht, die sie verschlang, zur\u00fcck.", "tokens": ["Die", "Schlan\u00b7gen", "kehr\u00b7ten", "nicht", ",", "die", "sie", "ver\u00b7schlang", ",", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$,", "PRELS", "PPER", "VVFIN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.17": {"line.1": {"text": "Und sollten doch einmal sie mir im Auge kehren,", "tokens": ["Und", "soll\u00b7ten", "doch", "ein\u00b7mal", "sie", "mir", "im", "Au\u00b7ge", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "PPER", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So soll ein neuer Stral der Sonne sie verzehren.", "tokens": ["So", "soll", "ein", "neu\u00b7er", "Stral", "der", "Son\u00b7ne", "sie", "ver\u00b7zeh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}