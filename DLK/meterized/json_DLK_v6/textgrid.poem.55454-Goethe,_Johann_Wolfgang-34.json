{"textgrid.poem.55454": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "34", "genre": "verse", "period": "N.A.", "pub_year": 1823, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Sch\u00fcler Howards, wunderlich", "tokens": ["Du", "Sch\u00fc\u00b7ler", "Ho\u00b7wards", ",", "wun\u00b7der\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "NN", "NE", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Siehst morgens um und \u00fcber dich,", "tokens": ["Siehst", "mor\u00b7gens", "um", "und", "\u00fc\u00b7ber", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "KON", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob Nebel fallen, ob sie steigen,", "tokens": ["Ob", "Ne\u00b7bel", "fal\u00b7len", ",", "ob", "sie", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und was sich f\u00fcr Gew\u00f6lke zeigen.", "tokens": ["Und", "was", "sich", "f\u00fcr", "Ge\u00b7w\u00f6l\u00b7ke", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf Berges Ferne ballt sich auf", "tokens": ["Auf", "Ber\u00b7ges", "Fer\u00b7ne", "ballt", "sich", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PRF", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Alpenheer, beeist zuhauf,", "tokens": ["Ein", "Al\u00b7pen\u00b7heer", ",", "be\u00b7eist", "zu\u00b7hauf", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und oben dr\u00fcber fl\u00fcchtig schweifen", "tokens": ["Und", "o\u00b7ben", "dr\u00fc\u00b7ber", "fl\u00fcch\u00b7tig", "schwei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PAV", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gefiedert wei\u00dfe, luftige Streifen;", "tokens": ["Ge\u00b7fie\u00b7dert", "wei\u00b7\u00dfe", ",", "luf\u00b7ti\u00b7ge", "Strei\u00b7fen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Doch unten senkt sich grau und grauer", "tokens": ["Doch", "un\u00b7ten", "senkt", "sich", "grau", "und", "grau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PRF", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Aus Wolkenschicht ein Regenschauer.", "tokens": ["Aus", "Wol\u00b7ken\u00b7schicht", "ein", "Re\u00b7gen\u00b7schau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wenn bei stillem D\u00e4mmerlicht", "tokens": ["Und", "wenn", "bei", "stil\u00b7lem", "D\u00e4m\u00b7mer\u00b7licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein allerliebstes Treugesicht", "tokens": ["Ein", "al\u00b7ler\u00b7liebs\u00b7tes", "Treu\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf holder Schwelle dir begegnet,", "tokens": ["Auf", "hol\u00b7der", "Schwel\u00b7le", "dir", "be\u00b7geg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wei\u00dft du, ob's heitert? ob es regnet?", "tokens": ["Wei\u00dft", "du", ",", "ob's", "hei\u00b7tert", "?", "ob", "es", "reg\u00b7net", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "NE", "$.", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}