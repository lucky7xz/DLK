{"textgrid.poem.41527": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am dritten Sonntage in der Fasten", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbmein Nam' ist Legion, denn unserer sind viele,\u00ab", "tokens": ["\u00bb", "mein", "Nam'", "ist", "Le\u00b7gi\u00b7on", ",", "denn", "un\u00b7se\u00b7rer", "sind", "vie\u00b7le", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "NN", "$,", "KON", "PPOSAT", "VAFIN", "PIS", "$,", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So spricht der finstre Geist:", "tokens": ["So", "spricht", "der", "finst\u00b7re", "Geist", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Nam' ist Legion, weh mir, da\u00df ich es f\u00fchle!", "tokens": ["Sein", "Nam'", "ist", "Le\u00b7gi\u00b7on", ",", "weh", "mir", ",", "da\u00df", "ich", "es", "f\u00fch\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df es mich zittern hei\u00dft!", "tokens": ["Da\u00df", "es", "mich", "zit\u00b7tern", "hei\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wo kindlich dem Gem\u00fct in Einfalt und Vertrauen", "tokens": ["Wo", "kind\u00b7lich", "dem", "Ge\u00b7m\u00fct", "in", "Ein\u00b7falt", "und", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nichts als sein Jesu kund,", "tokens": ["Nichts", "als", "sein", "Je\u00b7su", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "PPOSAT", "NE", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da kann der Finstre nicht die wirren H\u00f6hlen bauen", "tokens": ["Da", "kann", "der", "Finst\u00b7re", "nicht", "die", "wir\u00b7ren", "H\u00f6h\u00b7len", "bau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "PTKNEG", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Im einfach lichten Grund;", "tokens": ["Im", "ein\u00b7fach", "lich\u00b7ten", "Grund", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch du, mein schuldvoll Herz, in deinem eitlen Wissen,", "tokens": ["Doch", "du", ",", "mein", "schuld\u00b7voll", "Herz", ",", "in", "dei\u00b7nem", "eit\u00b7len", "Wis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "ADJD", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In deinem irren Tun,", "tokens": ["In", "dei\u00b7nem", "ir\u00b7ren", "Tun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie sind dir tausend brand'ge Stellen aufgerissen!", "tokens": ["Wie", "sind", "dir", "tau\u00b7send", "bran\u00b7d'\u00b7ge", "Stel\u00b7len", "auf\u00b7ge\u00b7ris\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "CARD", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Worin die Nacht kann ruhn;", "tokens": ["Wo\u00b7rin", "die", "Nacht", "kann", "ruhn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und raff' ich mich empor, und will ich mich erk\u00fchnen", "tokens": ["Und", "raff'", "ich", "mich", "em\u00b7por", ",", "und", "will", "ich", "mich", "er\u00b7k\u00fch\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PRF", "PTKVZ", "$,", "KON", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu heil'gen Namens Schall,", "tokens": ["Zu", "heil'\u00b7gen", "Na\u00b7mens", "Schall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O, k\u00f6nnte nicht vielleicht mein guter Wille dienen", "tokens": ["O", ",", "k\u00f6nn\u00b7te", "nicht", "viel\u00b7leicht", "mein", "gu\u00b7ter", "Wil\u00b7le", "die\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VMFIN", "PTKNEG", "ADV", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu neuem schwerem Fall!", "tokens": ["Zu", "neu\u00b7em", "schwe\u00b7rem", "Fall", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Denn da\u00df die Welt mich nicht, die Menschen mich nicht kennen,", "tokens": ["Denn", "da\u00df", "die", "Welt", "mich", "nicht", ",", "die", "Men\u00b7schen", "mich", "nicht", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "PTKNEG", "$,", "ART", "NN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die glei\u00dfend wie das Meer,", "tokens": ["Die", "glei\u00b7\u00dfend", "wie", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df sie mich oft sogar noch hell und freudig nennen,", "tokens": ["Da\u00df", "sie", "mich", "oft", "so\u00b7gar", "noch", "hell", "und", "freu\u00b7dig", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "ADV", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das senkt unendlich schwer!", "tokens": ["Das", "senkt", "un\u00b7end\u00b7lich", "schwer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Mich kennen mu\u00df die Welt, ich mu\u00df Verachtung tragen,", "tokens": ["Mich", "ken\u00b7nen", "mu\u00df", "die", "Welt", ",", "ich", "mu\u00df", "Ver\u00b7ach\u00b7tung", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "ART", "NN", "$,", "PPER", "VMFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie ich sie stets verdient,", "tokens": ["Wie", "ich", "sie", "stets", "ver\u00b7dient", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich Wurm, der den, den Engel kaum zu nennen wagen,", "tokens": ["Ich", "Wurm", ",", "der", "den", ",", "den", "En\u00b7gel", "kaum", "zu", "nen\u00b7nen", "wa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "ART", "$,", "ART", "NN", "ADV", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu preisen mich erk\u00fchnt!", "tokens": ["Zu", "prei\u00b7sen", "mich", "er\u00b7k\u00fchnt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "La\u00df in Zerknirschung mich, la\u00df mich in Furcht dich singen,", "tokens": ["La\u00df", "in", "Zer\u00b7knir\u00b7schung", "mich", ",", "la\u00df", "mich", "in", "Furcht", "dich", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "NN", "PPER", "$,", "VVIMP", "PPER", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Heiland und mein Gott!", "tokens": ["Mein", "Hei\u00b7land", "und", "mein", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df nicht mein Lied entrauscht, ein kunstvoll s\u00fcndlich Klingen,", "tokens": ["Da\u00df", "nicht", "mein", "Lied", "ent\u00b7rauscht", ",", "ein", "kunst\u00b7voll", "s\u00fcnd\u00b7lich", "Klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$,", "ART", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Frevel und ein Spott.", "tokens": ["Ein", "Fre\u00b7vel", "und", "ein", "Spott", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ach wer so leer wie ich in Worten und in Werken,", "tokens": ["Ach", "wer", "so", "leer", "wie", "ich", "in", "Wor\u00b7ten", "und", "in", "Wer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "ADV", "ADJD", "KOKOM", "PPER", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An Sinnen so verwirrt,", "tokens": ["An", "Sin\u00b7nen", "so", "ver\u00b7wirrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Lied kann nur des Herrn barmherzig Wunder st\u00e4rken,", "tokens": ["Des", "Lied", "kann", "nur", "des", "Herrn", "barm\u00b7her\u00b7zig", "Wun\u00b7der", "st\u00e4r\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ART", "NN", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df es zum Segen wird.", "tokens": ["Da\u00df", "es", "zum", "Se\u00b7gen", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ist nicht mein ganzer Tag nur eine Reihe S\u00fcnden?", "tokens": ["Ist", "nicht", "mein", "gan\u00b7zer", "Tag", "nur", "ei\u00b7ne", "Rei\u00b7he", "S\u00fcn\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mu\u00df oft in Traumeswahn,", "tokens": ["Mu\u00df", "oft", "in", "Trau\u00b7mes\u00b7wahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Oft wachend die Begier nicht zahllos Wege finden,", "tokens": ["Oft", "wa\u00b7chend", "die", "Be\u00b7gier", "nicht", "zahl\u00b7los", "We\u00b7ge", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "PTKNEG", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur nie die Himmelsbahn!", "tokens": ["Nur", "nie", "die", "Him\u00b7mels\u00b7bahn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "T\u00f6nt nicht der Kampfgesang der Lust von allen Seiten?", "tokens": ["T\u00f6nt", "nicht", "der", "Kampf\u00b7ge\u00b7sang", "der", "Lust", "von", "al\u00b7len", "Sei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bringt er nicht den Sieg?", "tokens": ["Und", "bringt", "er", "nicht", "den", "Sieg", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist nicht mein Leben nur ein fl\u00fcchtig kraftlos Streiten?", "tokens": ["Ist", "nicht", "mein", "Le\u00b7ben", "nur", "ein", "fl\u00fcch\u00b7tig", "kraft\u00b7los", "Strei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "NN", "ADV", "ART", "ADJD", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein schmachbedeckter Krieg!", "tokens": ["Ein", "schmach\u00b7be\u00b7deck\u00b7ter", "Krieg", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Und mein' ich eine Zeit, da\u00df ich den Sieg errungen,", "tokens": ["Und", "mein'", "ich", "ei\u00b7ne", "Zeit", ",", "da\u00df", "ich", "den", "Sieg", "er\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil die Begierde schwand,", "tokens": ["Weil", "die", "Be\u00b7gier\u00b7de", "schwand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da bin ich ausgeschl\u00fcrft, wie von Empusenzungen,", "tokens": ["Da", "bin", "ich", "aus\u00b7ge\u00b7schl\u00fcrft", ",", "wie", "von", "Em\u00b7pu\u00b7sen\u00b7zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie eine tote Hand!", "tokens": ["Wie", "ei\u00b7ne", "to\u00b7te", "Hand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Und ist mir's eine Zeit, als will das Leben ziehen", "tokens": ["Und", "ist", "mir's", "ei\u00b7ne", "Zeit", ",", "als", "will", "das", "Le\u00b7ben", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "NE", "ART", "NN", "$,", "KOUS", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ins Herze gar erstarrt,", "tokens": ["Ins", "Her\u00b7ze", "gar", "er\u00b7starrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da mu\u00df mit ihm zugleich der \u00dcbermut entgl\u00fchen,", "tokens": ["Da", "mu\u00df", "mit", "ihm", "zu\u00b7gleich", "der", "\u00dc\u00b7ber\u00b7mut", "ent\u00b7gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der eines Hauchs geharrt.", "tokens": ["Der", "ei\u00b7nes", "Hauchs", "ge\u00b7harrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Und wird mir's endlich klar, umspr\u00fcht von Leidensfunken,", "tokens": ["Und", "wird", "mir's", "end\u00b7lich", "klar", ",", "um\u00b7spr\u00fcht", "von", "Lei\u00b7dens\u00b7fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NE", "ADV", "ADJD", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie klein, wie nichts ich bin,", "tokens": ["Wie", "klein", ",", "wie", "nichts", "ich", "bin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "PIS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da bin ich ausgebrannt, zu Asche eingesunken,", "tokens": ["Da", "bin", "ich", "aus\u00b7ge\u00b7brannt", ",", "zu", "A\u00b7sche", "ein\u00b7ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,", "APPR", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vergl\u00fcht an Geist und Sinn.", "tokens": ["Ver\u00b7gl\u00fcht", "an", "Geist", "und", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Das hast du selber dir, du schuldvoll Herz, zu danken,", "tokens": ["Das", "hast", "du", "sel\u00b7ber", "dir", ",", "du", "schuld\u00b7voll", "Herz", ",", "zu", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "PPER", "$,", "PPER", "ADJD", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Jesu lieb und traut!", "tokens": ["Mein", "Je\u00b7su", "lieb", "und", "traut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PTKA", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "W\u00e4rst du nur irgend treu, er w\u00fcrde nimmer wanken", "tokens": ["W\u00e4rst", "du", "nur", "ir\u00b7gend", "treu", ",", "er", "w\u00fcr\u00b7de", "nim\u00b7mer", "wan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von der geliebten Braut.", "tokens": ["Von", "der", "ge\u00b7lieb\u00b7ten", "Braut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Doch, da\u00df du schlummernd l\u00e4\u00dft durch alle Tore ziehen", "tokens": ["Doch", ",", "da\u00df", "du", "schlum\u00b7mernd", "l\u00e4\u00dft", "durch", "al\u00b7le", "To\u00b7re", "zie\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "APPR", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den grausen H\u00f6llenbund,", "tokens": ["Den", "grau\u00b7sen", "H\u00f6l\u00b7len\u00b7bund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df \u00fcberall f\u00fcr ihn die Siegeskr\u00e4nze bl\u00fchen", "tokens": ["Da\u00df", "\u00fc\u00b7be\u00b7rall", "f\u00fcr", "ihn", "die", "Sie\u00b7ges\u00b7kr\u00e4n\u00b7ze", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Aus deinem eignen Grund,", "tokens": ["Aus", "dei\u00b7nem", "eig\u00b7nen", "Grund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Da\u00df du dich t\u00f6richt w\u00e4hnst in vollem hellem Laube,", "tokens": ["Da\u00df", "du", "dich", "t\u00f6\u00b7richt", "w\u00e4hnst", "in", "vol\u00b7lem", "hel\u00b7lem", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "ADV", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du armer d\u00fcrrer Zweig!", "tokens": ["Du", "ar\u00b7mer", "d\u00fcr\u00b7rer", "Zweig", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df du, indes der Feind frohlockt in deinem Raube,", "tokens": ["Da\u00df", "du", ",", "in\u00b7des", "der", "Feind", "froh\u00b7lockt", "in", "dei\u00b7nem", "Rau\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dich herrlich w\u00e4hnst, und reich:", "tokens": ["Dich", "herr\u00b7lich", "w\u00e4hnst", ",", "und", "reich", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VAFIN", "$,", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Das ist warum du stirbst, da\u00df du in Wahnes Gluten", "tokens": ["Das", "ist", "wa\u00b7rum", "du", "stirbst", ",", "da\u00df", "du", "in", "Wah\u00b7nes", "Glu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PWAV", "PPER", "VVFIN", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht kennst den eignen Schmerz,", "tokens": ["Nicht", "kennst", "den", "eig\u00b7nen", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O, f\u00fchltest du dich selbst aus allen Adern bluten,", "tokens": ["O", ",", "f\u00fchl\u00b7test", "du", "dich", "selbst", "aus", "al\u00b7len", "A\u00b7dern", "blu\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PRF", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du t\u00f6richt frevelnd Herz!", "tokens": ["Du", "t\u00f6\u00b7richt", "fre\u00b7velnd", "Herz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.18": {"line.1": {"text": "So schaue deine Not! noch fielen nicht die Schranken", "tokens": ["So", "schau\u00b7e", "dei\u00b7ne", "Not", "!", "noch", "fie\u00b7len", "nicht", "die", "Schran\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der dunklen Ewigkeit.", "tokens": ["Der", "dunk\u00b7len", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbsein Nam' ist Legion\u00ab, o fasse den Gedanken!", "tokens": ["\u00bb", "sein", "Nam'", "ist", "Le\u00b7gi\u00b7on", "\u00ab", ",", "o", "fas\u00b7se", "den", "Ge\u00b7dan\u00b7ken", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "NN", "$(", "$,", "FM", "FM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es ist die letzte Zeit!", "tokens": ["Es", "ist", "die", "letz\u00b7te", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}