{"textgrid.poem.33359": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Lied an der Toilette der Geliebten zu singen", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "D\u00fcrft' ich Huldin, dich umfangen,", "tokens": ["D\u00fcrft'", "ich", "Hul\u00b7din", ",", "dich", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NE", "$,", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gleich der Luft, die dich umflie\u00dft,", "tokens": ["Gleich", "der", "Luft", ",", "die", "dich", "um\u00b7flie\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit zitterndem Verlangen", "tokens": ["Und", "mit", "zit\u00b7tern\u00b7dem", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jeden deiner Reize k\u00fc\u00dft!", "tokens": ["Je\u00b7den", "dei\u00b7ner", "Rei\u00b7ze", "k\u00fc\u00dft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schwebt' ich, ach mit Wohlgefallen,", "tokens": ["Schwebt'", "ich", ",", "ach", "mit", "Wohl\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie dein Genius um dich,", "tokens": ["Wie", "dein", "Ge\u00b7nius", "um", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Willig b\u00f6t' ich' dann zu allen", "tokens": ["Wil\u00b7lig", "b\u00f6t'", "ich'", "dann", "zu", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Noch so kleinen Diensten mich.", "tokens": ["Noch", "so", "klei\u00b7nen", "Diens\u00b7ten", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gern hielt ich als Wachspomade", "tokens": ["Gern", "hielt", "ich", "als", "Wach\u00b7spo\u00b7ma\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dir die krausen Locken hier,", "tokens": ["Dir", "die", "krau\u00b7sen", "Lo\u00b7cken", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder steckte gar, o Gnade!", "tokens": ["O\u00b7der", "steck\u00b7te", "gar", ",", "o", "Gna\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "FM", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort im Krepp als Nadel dir.", "tokens": ["Dort", "im", "Krepp", "als", "Na\u00b7del", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "KOUS", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wollte gern bei'm Puderp\u00fcsten", "tokens": ["Woll\u00b7te", "gern", "bei'm", "Pu\u00b7der\u00b7p\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kreiselnd um dein Haar mich dreh'n,", "tokens": ["Krei\u00b7selnd", "um", "dein", "Haar", "mich", "dreh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Oder mit den Kolonisten", "tokens": ["O\u00b7der", "mit", "den", "Ko\u00b7lo\u00b7nis\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Deines Haar's spazieren geh'n!", "tokens": ["Dei\u00b7nes", "Haa\u00b7r's", "spa\u00b7zie\u00b7ren", "geh'n", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Bald erh\u00f6ht' ich dann als Musche", "tokens": ["Bald", "er\u00b7h\u00f6ht'", "ich", "dann", "als", "Mu\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner Stirne blendend Wei\u00df,", "tokens": ["Dei\u00b7ner", "Stir\u00b7ne", "blen\u00b7dend", "Wei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder w\u00f6lbte die Contusche", "tokens": ["O\u00b7der", "w\u00f6lb\u00b7te", "die", "Con\u00b7tu\u00b7sche"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir als ein Pariserstei\u00df;", "tokens": ["Dir", "als", "ein", "Pa\u00b7ri\u00b7ser\u00b7stei\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Prangte dann auf deinem Rocke", "tokens": ["Prang\u00b7te", "dann", "auf", "dei\u00b7nem", "Ro\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bald als B\u00e4ndchen oder Knopf,", "tokens": ["Bald", "als", "B\u00e4nd\u00b7chen", "o\u00b7der", "Knopf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ja, sogar zum Haubenstocke", "tokens": ["Ja", ",", "so\u00b7gar", "zum", "Hau\u00b7ben\u00b7sto\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dient' ich dir mit meinem Kopf.", "tokens": ["Dient'", "ich", "dir", "mit", "mei\u00b7nem", "Kopf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Morgens schlich ich mich, o Liebe!", "tokens": ["Mor\u00b7gens", "schlich", "ich", "mich", ",", "o", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PRF", "$,", "FM", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir als Zwieback in den Mund,", "tokens": ["Dir", "als", "Zwie\u00b7back", "in", "den", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder machte meine Triebe", "tokens": ["O\u00b7der", "mach\u00b7te", "mei\u00b7ne", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Im Kaffee als Milch dir kund;", "tokens": ["Im", "Kaf\u00b7fee", "als", "Milch", "dir", "kund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KOUS", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "F\u00e4rbte dir Mittags als guter", "tokens": ["F\u00e4rb\u00b7te", "dir", "Mit\u00b7tags", "als", "gu\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "KOUS", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Rheinwein deine Wangen roth;", "tokens": ["Rhein\u00b7wein", "dei\u00b7ne", "Wan\u00b7gen", "roth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Oder liesse mich als Butter", "tokens": ["O\u00b7der", "lies\u00b7se", "mich", "als", "But\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Streichen auf dein Vesperbrod.", "tokens": ["Strei\u00b7chen", "auf", "dein", "Ves\u00b7per\u00b7brod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Bald ber\u00fchrt' ich armer Schlucker,", "tokens": ["Bald", "be\u00b7r\u00fchrt'", "ich", "ar\u00b7mer", "Schlu\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine Nase als Flacon,", "tokens": ["Dei\u00b7ne", "Na\u00b7se", "als", "Fla\u00b7con", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KOKOM", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder diente dir als Zucker,", "tokens": ["O\u00b7der", "dien\u00b7te", "dir", "als", "Zu\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KOUS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn du naschest, zum Bonbon!", "tokens": ["Wenn", "du", "na\u00b7schest", ",", "zum", "Bon\u00b7bon", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "APPRART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Spannte dann, gleich Pergamente,", "tokens": ["Spann\u00b7te", "dann", ",", "gleich", "Per\u00b7ga\u00b7men\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADV", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Meine Haut zum Zeichen ein,", "tokens": ["Mei\u00b7ne", "Haut", "zum", "Zei\u00b7chen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, und wenn du maltest, k\u00f6nnte", "tokens": ["Ach", ",", "und", "wenn", "du", "mal\u00b7test", ",", "k\u00f6nn\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ITJ", "$,", "KON", "KOUS", "PPER", "VVFIN", "$,", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ich wohl gar dein Pinsel sein!", "tokens": ["Ich", "wohl", "gar", "dein", "Pin\u00b7sel", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Gern deckt' ich in Assembleen", "tokens": ["Gern", "deckt'", "ich", "in", "As\u00b7sem\u00b7bleen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir den Busen, als Linon,", "tokens": ["Dir", "den", "Bu\u00b7sen", ",", "als", "Li\u00b7non", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "KOUS", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder hing in s\u00fcssen Wehen", "tokens": ["O\u00b7der", "hing", "in", "s\u00fcs\u00b7sen", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir am Hals ", "tokens": ["Dir", "am", "Hals"], "token_info": ["word", "word", "word"], "pos": ["PPER", "APPRART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Doch zu meiner Freuden F\u00fclle,", "tokens": ["Doch", "zu", "mei\u00b7ner", "Freu\u00b7den", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sch\u00f6nste, w\u00fcnscht' ich mir allein", "tokens": ["Sch\u00f6ns\u00b7te", ",", "w\u00fcnscht'", "ich", "mir", "al\u00b7lein"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Unter deines Bettes H\u00fclle", "tokens": ["Un\u00b7ter", "dei\u00b7nes", "Bet\u00b7tes", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eine Nacht \u2013 ein Floh zu sein.", "tokens": ["Ei\u00b7ne", "Nacht", "\u2013", "ein", "Floh", "zu", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}