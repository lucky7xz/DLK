{"textgrid.poem.35784": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du liebst mich, weil durch braunes Haar", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du liebst mich, weil durch braunes Haar", "tokens": ["Du", "liebst", "mich", ",", "weil", "durch", "brau\u00b7nes", "Haar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich schlingt der gr\u00fcne Lebenskranz,", "tokens": ["Sich", "schlingt", "der", "gr\u00fc\u00b7ne", "Le\u00b7bens\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil frisch und voll der Wangen Paar", "tokens": ["Weil", "frisch", "und", "voll", "der", "Wan\u00b7gen", "Paar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "KON", "ADJD", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und leicht der Fu\u00df sich hebt im Tanz.", "tokens": ["Und", "leicht", "der", "Fu\u00df", "sich", "hebt", "im", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "PRF", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O, armer J\u00fcngling! wisse, bald", "tokens": ["O", ",", "ar\u00b7mer", "J\u00fcng\u00b7ling", "!", "wis\u00b7se", ",", "bald"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["NE", "$,", "ADJA", "NN", "$.", "VVFIN", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist all das hin, was du geliebt,", "tokens": ["Ist", "all", "das", "hin", ",", "was", "du", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "PDS", "PTKVZ", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geknickt die bl\u00fchende Gestalt,", "tokens": ["Ge\u00b7knickt", "die", "bl\u00fc\u00b7hen\u00b7de", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die jetzt den Zauber auf dich \u00fcbt.", "tokens": ["Die", "jetzt", "den", "Zau\u00b7ber", "auf", "dich", "\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn eine Blume bin ich nur,", "tokens": ["Denn", "ei\u00b7ne", "Blu\u00b7me", "bin", "ich", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kurz ist alles Erdenbl\u00fchn;", "tokens": ["Und", "kurz", "ist", "al\u00b7les", "Er\u00b7den\u00b7bl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum suche ew'ger Sch\u00f6ne Spur,", "tokens": ["Drum", "su\u00b7che", "ew'\u00b7ger", "Sch\u00f6\u00b7ne", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr weihe deines Herzens Gl\u00fchn.", "tokens": ["Ihr", "wei\u00b7he", "dei\u00b7nes", "Her\u00b7zens", "Gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sieh, wenig Lenze gehn in's Thal", "tokens": ["Sieh", ",", "we\u00b7nig", "Len\u00b7ze", "gehn", "in's", "Thal"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PIAT", "NN", "VVFIN", "APPRART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und hin ist dieser Augen Schein,", "tokens": ["Und", "hin", "ist", "die\u00b7ser", "Au\u00b7gen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gewelkt der Mund, die Wange fahl,", "tokens": ["Ge\u00b7welkt", "der", "Mund", ",", "die", "Wan\u00b7ge", "fahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Man sargt die kalte H\u00fclle ein.", "tokens": ["Man", "sargt", "die", "kal\u00b7te", "H\u00fcl\u00b7le", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und nur das Kreuz am H\u00fcgelrand", "tokens": ["Und", "nur", "das", "Kreuz", "am", "H\u00fc\u00b7gel\u00b7rand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sagt, da\u00df ein Leben hier gebl\u00fcht,", "tokens": ["Sagt", ",", "da\u00df", "ein", "Le\u00b7ben", "hier", "ge\u00b7bl\u00fcht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und betend faltet wohl die Hand", "tokens": ["Und", "be\u00b7tend", "fal\u00b7tet", "wohl", "die", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Pilger, der vor\u00fcberzieht.", "tokens": ["Der", "Pil\u00b7ger", ",", "der", "vor\u00b7\u00fc\u00b7berz\u00b7ieht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}