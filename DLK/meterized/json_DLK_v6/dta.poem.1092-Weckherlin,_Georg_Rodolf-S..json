{"dta.poem.1092": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "S.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als lang ich dir so lieb war/", "tokens": ["Als", "lang", "ich", "dir", "so", "lieb", "war", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "PPER", "ADV", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das meine gelb-krause haar", "tokens": ["Das", "mei\u00b7ne", "gelb\u00b7krau\u00b7se", "haar"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dich allein hielten gefangen;", "tokens": ["Dich", "al\u00b7lein", "hiel\u00b7ten", "ge\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Kont mich meinen s\u00fcssen stand", "tokens": ["Kont", "mich", "mei\u00b7nen", "s\u00fcs\u00b7sen", "stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJA", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht mit der Cron Engelland", "tokens": ["Nicht", "mit", "der", "Cron", "En\u00b7gel\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "NN", "NE"], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Zuvertauschen je verlangen.", "tokens": ["Zu\u00b7ver\u00b7tau\u00b7schen", "je", "ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}