{"textgrid.poem.64687": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Alte darf nicht l\u00e4nger bleiben,", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Alte darf nicht l\u00e4nger bleiben,", "tokens": ["Das", "Al\u00b7te", "darf", "nicht", "l\u00e4n\u00b7ger", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Stets m\u00fcssen sie was Neues schreiben;", "tokens": ["Stets", "m\u00fcs\u00b7sen", "sie", "was", "Neu\u00b7es", "schrei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur kennen sie das Alte nicht,", "tokens": ["Nur", "ken\u00b7nen", "sie", "das", "Al\u00b7te", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schreiben Neues stets, das Neuem widerspricht.", "tokens": ["Und", "schrei\u00b7ben", "Neu\u00b7es", "stets", ",", "das", "Neu\u00b7em", "wi\u00b7der\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}