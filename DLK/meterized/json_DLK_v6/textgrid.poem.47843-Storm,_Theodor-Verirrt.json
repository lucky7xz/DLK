{"textgrid.poem.47843": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Verirrt", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein V\u00f6glein singt so s\u00fc\u00dfe", "tokens": ["Ein", "V\u00f6\u00b7glein", "singt", "so", "s\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor mir von Ort zu Ort;", "tokens": ["Vor", "mir", "von", "Ort", "zu", "Ort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weh, meine wunden F\u00fc\u00dfe!", "tokens": ["Weh", ",", "mei\u00b7ne", "wun\u00b7den", "F\u00fc\u00b7\u00dfe", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das V\u00f6glein singt so s\u00fc\u00dfe,", "tokens": ["Das", "V\u00f6\u00b7glein", "singt", "so", "s\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich wandre immerfort.", "tokens": ["Ich", "wand\u00b7re", "im\u00b7mer\u00b7fort", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wo ist nun hin das Singen?", "tokens": ["Wo", "ist", "nun", "hin", "das", "Sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schon sank das Abendrot;", "tokens": ["Schon", "sank", "das", "A\u00b7ben\u00b7drot", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Nacht hat es verstecket,", "tokens": ["Die", "Nacht", "hat", "es", "ver\u00b7ste\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat alles zugedecket \u2013", "tokens": ["Hat", "al\u00b7les", "zu\u00b7ge\u00b7de\u00b7cket", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wem klag ich meine Not?", "tokens": ["Wem", "klag", "ich", "mei\u00b7ne", "Not", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Kein Sternlein blinkt im Walde,", "tokens": ["Kein", "Stern\u00b7lein", "blinkt", "im", "Wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wei\u00df weder Weg noch Ort;", "tokens": ["Wei\u00df", "we\u00b7der", "Weg", "noch", "Ort", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Blumen an der Halde,", "tokens": ["Die", "Blu\u00b7men", "an", "der", "Hal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Blumen in dem Walde,", "tokens": ["Die", "Blu\u00b7men", "in", "dem", "Wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die bl\u00fchn im Dunkeln fort.", "tokens": ["Die", "bl\u00fchn", "im", "Dun\u00b7keln", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}