{"textgrid.poem.47198": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Entgegen geh' ich nun den tr\u00fcben Tagen,", "tokens": ["Ent\u00b7ge\u00b7gen", "geh'", "ich", "nun", "den", "tr\u00fc\u00b7ben", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der traur'gen Zeit, die mir vom ganzen Jahr", "tokens": ["Der", "traur'\u00b7gen", "Zeit", ",", "die", "mir", "vom", "gan\u00b7zen", "Jahr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die unerfreulichste schon sonst auch war,", "tokens": ["Die", "un\u00b7er\u00b7freu\u00b7lichs\u00b7te", "schon", "sonst", "auch", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "ADV", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Eh' sie so herbe Wunden mir geschlagen;", "tokens": ["Eh'", "sie", "so", "her\u00b7be", "Wun\u00b7den", "mir", "ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Zeit, wo wir um Lichtabnahme klagen", "tokens": ["Die", "Zeit", ",", "wo", "wir", "um", "Licht\u00b7ab\u00b7nah\u00b7me", "kla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sehn die Erde Blumenschmuckes bar,", "tokens": ["Und", "sehn", "die", "Er\u00b7de", "Blu\u00b7men\u00b7schmu\u00b7ckes", "bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dieselbe Zeit hat auch mein sch\u00f6nes Paar", "tokens": ["Die\u00b7sel\u00b7be", "Zeit", "hat", "auch", "mein", "sch\u00f6\u00b7nes", "Paar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie Sonnenschein und Blumen weggetragen.", "tokens": ["Wie", "Son\u00b7nen\u00b7schein", "und", "Blu\u00b7men", "weg\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und wenn in Mitte dieser Finsternisse", "tokens": ["Und", "wenn", "in", "Mit\u00b7te", "die\u00b7ser", "Fins\u00b7ter\u00b7nis\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sonst ein Gestirn des Trosts und Heiles stand", "tokens": ["Sonst", "ein", "Ge\u00b7stirn", "des", "Trosts", "und", "Hei\u00b7les", "stand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "NN", "KON", "NE", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Kinderfest der heiligen Weihnachten;", "tokens": ["Das", "Kin\u00b7der\u00b7fest", "der", "hei\u00b7li\u00b7gen", "Weih\u00b7nach\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O wie ich nun auch dessen Segen misse,", "tokens": ["O", "wie", "ich", "nun", "auch", "des\u00b7sen", "Se\u00b7gen", "mis\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "PPER", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da ihr zu Grabe ginget, in der Hand", "tokens": ["Da", "ihr", "zu", "Gra\u00b7be", "gin\u00b7get", ",", "in", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Gaben haltend, die vom Fest gebrachten!", "tokens": ["Die", "Ga\u00b7ben", "hal\u00b7tend", ",", "die", "vom", "Fest", "ge\u00b7brach\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}