{"textgrid.poem.58480": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "518. Viele Geliebte, ein Liebster", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Euch, Freunde Gottes, lieb' ich sehr,", "tokens": ["Euch", ",", "Freun\u00b7de", "Got\u00b7tes", ",", "lieb'", "ich", "sehr", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "NN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch lieb' ich ", "tokens": ["Doch", "lieb'", "ich"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Wenn der einst recht mit mir allein will gehn,", "tokens": ["Wenn", "der", "einst", "recht", "mit", "mir", "al\u00b7lein", "will", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "ADJD", "APPR", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dann bleibt ihr und das Liebste drau\u00dfen stehn.", "tokens": ["Dann", "bleibt", "ihr", "und", "das", "Liebs\u00b7te", "drau\u00b7\u00dfen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O s\u00fc\u00dfe Einsamkeit, da alles sanft verschwindet,", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "Ein\u00b7sam\u00b7keit", ",", "da", "al\u00b7les", "sanft", "ver\u00b7schwin\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "KOUS", "PIS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da unser Auge nichts mehr findet,", "tokens": ["Da", "un\u00b7ser", "Au\u00b7ge", "nichts", "mehr", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da auch das Herz nichts liebt, als nur", "tokens": ["Da", "auch", "das", "Herz", "nichts", "liebt", ",", "als", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "PIS", "VVFIN", "$,", "KOUS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das h\u00f6chste Gut allein und pur!", "tokens": ["Das", "h\u00f6chs\u00b7te", "Gut", "al\u00b7lein", "und", "pur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}