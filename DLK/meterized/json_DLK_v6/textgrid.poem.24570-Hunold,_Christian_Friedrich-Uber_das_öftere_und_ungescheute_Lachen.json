{"textgrid.poem.24570": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber das \u00f6ftere und ungescheute Lachen", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So unvern\u00fcnftig wird ein Narr zum \u00f6ftern Lachen/", "tokens": ["So", "un\u00b7ver\u00b7n\u00fcnf\u00b7tig", "wird", "ein", "Narr", "zum", "\u00f6f\u00b7tern", "La\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um seinen wei\u00dfen Zahn dadurch bekandt zumachen/", "tokens": ["Um", "sei\u00b7nen", "wei\u00b7\u00dfen", "Zahn", "da\u00b7durch", "be\u00b7kandt", "zu\u00b7ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "PAV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df wenn ein Dieb den Hal\u00df dem Hencker bent/", "tokens": ["Da\u00df", "wenn", "ein", "Dieb", "den", "Hal\u00df", "dem", "Hen\u00b7cker", "bent", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er gleich darzu aus voller Kehle schreyt.", "tokens": ["Er", "gleich", "dar\u00b7zu", "aus", "vol\u00b7ler", "Keh\u00b7le", "schreyt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PAV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und wenn ein Redner so geschickt/", "tokens": ["Und", "wenn", "ein", "Red\u00b7ner", "so", "ge\u00b7schickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Volck zum Weinen zu bewegen/", "tokens": ["Das", "Volck", "zum", "Wei\u00b7nen", "zu", "be\u00b7we\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wird sich ein Feuer wo erregen/", "tokens": ["Wird", "sich", "ein", "Feu\u00b7er", "wo", "er\u00b7re\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "NN", "PWAV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dadurch ein eintzger Sohn erstickt/", "tokens": ["Da\u00b7durch", "ein", "eintz\u00b7ger", "Sohn", "er\u00b7stickt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und dieser Schmertzen will die Mutter sterbend machen:", "tokens": ["Und", "die\u00b7ser", "Schmert\u00b7zen", "will", "die", "Mut\u00b7ter", "ster\u00b7bend", "ma\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VMFIN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So wird er sich doch fast zu tode lachen.", "tokens": ["So", "wird", "er", "sich", "doch", "fast", "zu", "to\u00b7de", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADV", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ja/ sonder Ursach/ Ort und Zeit/", "tokens": ["Ja", "/", "son\u00b7der", "Ur\u00b7sach", "/", "Ort", "und", "Zeit", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "KON", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "So denckt er/ die Vollkommenheit/", "tokens": ["So", "denckt", "er", "/", "die", "Voll\u00b7kom\u00b7men\u00b7heit", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Davon er l\u00e4ngst sein Handwerck hat gemacht/", "tokens": ["Da\u00b7von", "er", "l\u00e4ngst", "sein", "Hand\u00b7werck", "hat", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Besteh' in dem/ wenn er vollkommen lacht.", "tokens": ["Be\u00b7steh'", "in", "dem", "/", "wenn", "er", "voll\u00b7kom\u00b7men", "lacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "$(", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}