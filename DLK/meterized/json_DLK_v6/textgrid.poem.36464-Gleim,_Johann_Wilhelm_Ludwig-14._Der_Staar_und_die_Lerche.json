{"textgrid.poem.36464": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "14. Der Staar und die Lerche", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir reisen, sagten einst, auf ihrer Wanderung, St\u00f6rche", "tokens": ["Wir", "rei\u00b7sen", ",", "sag\u00b7ten", "einst", ",", "auf", "ih\u00b7rer", "Wan\u00b7de\u00b7rung", ",", "St\u00f6r\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zu einem Staar und einer Lerche.", "tokens": ["Zu", "ei\u00b7nem", "Staar", "und", "ei\u00b7ner", "Ler\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wir auch! antwortete die kleine Lerche gleich,", "tokens": ["Wir", "auch", "!", "ant\u00b7wor\u00b7te\u00b7te", "die", "klei\u00b7ne", "Ler\u00b7che", "gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$.", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und wenn ihrs wollt, so reisen wir mit euch.", "tokens": ["Und", "wenn", "ihrs", "wollt", ",", "so", "rei\u00b7sen", "wir", "mit", "euch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VMFIN", "$,", "ADV", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mit nichten! sagte drauf der Staar,", "tokens": ["Mit", "nich\u00b7ten", "!", "sag\u00b7te", "drauf", "der", "Staar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$.", "VVFIN", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der kl\u00fcger als die Lerche war,", "tokens": ["Der", "kl\u00fc\u00b7ger", "als", "die", "Ler\u00b7che", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KOKOM", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit nichten! denn auf euren Reisen,", "tokens": ["Mit", "nich\u00b7ten", "!", "denn", "auf", "eu\u00b7ren", "Rei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$.", "KON", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da liefen wir Gefahr!", "tokens": ["Da", "lie\u00b7fen", "wir", "Ge\u00b7fahr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr k\u00f6nntet uns, wie Fr\u00f6sche, speisen.", "tokens": ["Ihr", "k\u00f6nn\u00b7tet", "uns", ",", "wie", "Fr\u00f6\u00b7sche", ",", "spei\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "PWAV", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}