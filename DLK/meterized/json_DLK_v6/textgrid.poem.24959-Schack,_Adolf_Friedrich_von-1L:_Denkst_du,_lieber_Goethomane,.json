ch"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsrer Zeit, sei hier verbannt!", "tokens": ["Uns\u00b7rer", "Zeit", ",", "sei", "hier", "ver\u00b7bannt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00e4lt vielleicht die Mutter Sarah,", "tokens": ["H\u00e4lt", "viel\u00b7leicht", "die", "Mut\u00b7ter", "Sa\u00b7rah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NE", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Weltschmerz, ihre Niederkunft", "tokens": ["Welt\u00b7schmerz", ",", "ih\u00b7re", "Nie\u00b7der\u00b7kunft"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit Child Harold oder Lara", "tokens": ["Mit", "Child", "Ha\u00b7rold", "o\u00b7der", "La\u00b7ra"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NE", "KON", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Oder Manfreds Unvernunft?\u00ab", "tokens": ["O\u00b7der", "Man\u00b7freds", "Un\u00b7ver\u00b7nunft", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "NE", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Wieder dann, jedoch voll Aerger", "tokens": ["Wie\u00b7der", "dann", ",", "je\u00b7doch", "voll", "A\u00b7er\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "ADV", "ADJD", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Naht' ich eurem Tische mich,", "toke