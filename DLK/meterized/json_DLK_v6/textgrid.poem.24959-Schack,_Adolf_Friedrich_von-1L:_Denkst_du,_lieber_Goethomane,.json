", "tokens": ["Al\u00b7len", "uns", "ein", "Freu\u00b7den\u00b7we\u00b7cker"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ward der Wein, nur Fausten nicht,", "tokens": ["Ward", "der", "Wein", ",", "nur", "Faus\u00b7ten", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "ADV", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und Mephist, der feine Schmecker,", "tokens": ["Und", "Me\u00b7phist", ",", "der", "fei\u00b7ne", "Schme\u00b7cker", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schnitt ein bitteres Gesicht;", "tokens": ["Schnitt", "ein", "bit\u00b7te\u00b7res", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jener seufzte: \u00bbWie der Kranich", "tokens": ["Je\u00b7ner", "seufz\u00b7te", ":", "\u00bb", "Wie", "der", "Kra\u00b7nich"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PDAT", "VVFIN", "$.", "$(", "PWAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "M\u00f6cht' ich ziehen \u00fcbers Meer;\u00ab", "tokens": ["M\u00f6cht'", "ich", "zie\u00b7hen", "\u00fc\u00b7bers", "Meer", ";", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dieser rief: \u00bbEur Wein ist kahnig,", "tokens": ["Die\u00b7ser", "rief", ":", "\u00bb", "Eur", "Wein", "ist", "kah\u00b7nig", ","], "token_info"