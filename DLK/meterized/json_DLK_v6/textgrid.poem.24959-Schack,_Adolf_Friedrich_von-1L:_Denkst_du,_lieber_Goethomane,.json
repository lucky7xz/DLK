{"textgrid.poem.24959": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Denkst du, lieber Goethomane,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denkst du, lieber Goethomane,", "tokens": ["Denkst", "du", ",", "lie\u00b7ber", "Goe\u00b7tho\u00b7ma\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den man oft wie mich geneckt,", "tokens": ["Den", "man", "oft", "wie", "mich", "ge\u00b7neckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "KOKOM", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df wir unsres Wolfgang Fahne", "tokens": ["Da\u00df", "wir", "uns\u00b7res", "Wolf\u00b7gang", "Fah\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Allzu eifrig aufgesteckt,", "tokens": ["All\u00b7zu", "eif\u00b7rig", "auf\u00b7ge\u00b7steckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denkst du noch der Nacht beim Sekte", "tokens": ["Denkst", "du", "noch", "der", "Nacht", "beim", "Sek\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In dem Keller Auerbachs,", "tokens": ["In", "dem", "Kel\u00b7ler", "Au\u00b7er\u00b7bachs", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Als wir sprachen vom Projekte", "tokens": ["Als", "wir", "spra\u00b7chen", "vom", "Pro\u00b7jek\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unsres Goethe-Almanachs?", "tokens": ["Uns\u00b7res", "Goe\u00b7the\u00b7Al\u00b7ma\u00b7nachs", "?"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Im Gew\u00f6lb mit spitzem Giebel,", "tokens": ["Im", "Ge\u00b7w\u00f6lb", "mit", "spit\u00b7zem", "Gie\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo der Ruhm noch nicht erlosch", "tokens": ["Wo", "der", "Ruhm", "noch", "nicht", "er\u00b7losch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADV", "PTKNEG", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Des Gelages, welches Siebel", "tokens": ["Des", "Ge\u00b7la\u00b7ges", ",", "wel\u00b7ches", "Sie\u00b7bel"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PWAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort mit Altmayr hielt und Frosch,", "tokens": ["Dort", "mit", "Alt\u00b7mayr", "hielt", "und", "Frosch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Feierten wir Goethesch\u00fcler", "tokens": ["Fei\u00b7er\u00b7ten", "wir", "Goe\u00b7the\u00b7sch\u00fc\u00b7ler"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "NE"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.6": {"text": "(diesmal nicht am Lesepult,", "tokens": ["(", "dies\u00b7mal", "nicht", "am", "Le\u00b7se\u00b7pult", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PTKNEG", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sondern beim Geriesel k\u00fchler", "tokens": ["Son\u00b7dern", "beim", "Ge\u00b7rie\u00b7sel", "k\u00fch\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Rebens\u00e4fte) unsern Kult.", "tokens": ["Re\u00b7ben\u00b7s\u00e4f\u00b7te", ")", "un\u00b7sern", "Kult", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Vor dem Trinken schon am E\u00dftisch", "tokens": ["Vor", "dem", "Trin\u00b7ken", "schon", "am", "E\u00df\u00b7tisch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Waren wir begeistrungsvoll,", "tokens": ["Wa\u00b7ren", "wir", "be\u00b7geis\u00b7trungs\u00b7voll", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df der Versquell anap\u00e4stisch", "tokens": ["Da\u00df", "der", "Ver\u00b7squell", "a\u00b7na\u00b7p\u00e4s\u00b7tisch"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Von den Lippen niederquoll;", "tokens": ["Von", "den", "Lip\u00b7pen", "nie\u00b7der\u00b7quoll", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und als gar ein Glas Burgunder", "tokens": ["Und", "als", "gar", "ein", "Glas", "Bur\u00b7gun\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Erst zum Munde wir gef\u00fchrt,", "tokens": ["Erst", "zum", "Mun\u00b7de", "wir", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ward der Geist wie trockner Zunder", "tokens": ["Ward", "der", "Geist", "wie", "trock\u00b7ner", "Zun\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "KOKOM", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Uns zur Flamme angesch\u00fcrt.", "tokens": ["Uns", "zur", "Flam\u00b7me", "an\u00b7ge\u00b7sch\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Laut, so da\u00df bei deinem Pathos", "tokens": ["Laut", ",", "so", "da\u00df", "bei", "dei\u00b7nem", "Pa\u00b7thos"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "$,", "ADV", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Fast das Glas vom Tische flog,", "tokens": ["Fast", "das", "Glas", "vom", "Ti\u00b7sche", "flog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deklamiertest du Torquatos", "tokens": ["De\u00b7kla\u00b7mier\u00b7test", "du", "Tor\u00b7qua\u00b7tos"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weltber\u00fchmten Monolog \u2013", "tokens": ["Welt\u00b7be\u00b7r\u00fchm\u00b7ten", "Mo\u00b7no\u00b7log", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Iphigenien in Tauris", "tokens": ["Ip\u00b7hi\u00b7ge\u00b7ni\u00b7en", "in", "Tau\u00b7ris"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPR", "NE"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Pries vor allen Gustav mir,", "tokens": ["Pries", "vor", "al\u00b7len", "Gus\u00b7tav", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Heinrich zeichnete im Bauri\u00df", "tokens": ["Hein\u00b7rich", "zeich\u00b7ne\u00b7te", "im", "Bau\u00b7ri\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Goethes Haus uns aufs Papier.", "tokens": ["Goe\u00b7thes", "Haus", "uns", "aufs", "Pa\u00b7pier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hermann sprach: \u00bbHinweg mit Posa,", "tokens": ["Her\u00b7mann", "sprach", ":", "\u00bb", "Hin\u00b7weg", "mit", "Po\u00b7sa", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "$(", "ADV", "APPR", "NE", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der die Welt verbessert hat,", "tokens": ["Der", "die", "Welt", "ver\u00b7bes\u00b7sert", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch zugleich mit schaler Prosa", "tokens": ["Doch", "zu\u00b7gleich", "mit", "scha\u00b7ler", "Pro\u00b7sa"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den Parna\u00df gew\u00e4ssert hat!", "tokens": ["Den", "Par\u00b7na\u00df", "ge\u00b7w\u00e4s\u00b7sert", "hat", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Jener Dichter, welcher Thekla,", "tokens": ["Je\u00b7ner", "Dich\u00b7ter", ",", "wel\u00b7cher", "The\u00b7kla", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "NE", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Neben dem, der Mignon schuf,", "tokens": ["Ne\u00b7ben", "dem", ",", "der", "Mig\u00b7non", "schuf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "D\u00fcnkt mich eisig, wie der Hekla", "tokens": ["D\u00fcnkt", "mich", "ei\u00b7sig", ",", "wie", "der", "He\u00b7kla"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "PWAV", "ART", "NE"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Neben Aetna und Vesuv.\u00ab", "tokens": ["Ne\u00b7ben", "A\u00b7et\u00b7na", "und", "Ve\u00b7suv", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "KON", "NE", "$.", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Gustav rief indes: \u00bbMit Rheinwein", "tokens": ["Gus\u00b7tav", "rief", "in\u00b7des", ":", "\u00bb", "Mit", "Rhein\u00b7wein"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "$.", "$(", "APPR", "NE"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "La\u00dft uns die H\u00e4retiker", "tokens": ["La\u00dft", "uns", "die", "H\u00e4\u00b7re\u00b7ti\u00b7ker"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In den Goethe-Glauben einweihn,", "tokens": ["In", "den", "Goe\u00b7the\u00b7Glau\u00b7ben", "ein\u00b7weihn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Denn zu Sel'gen macht nur er!\u00ab", "tokens": ["Denn", "zu", "Sel'\u00b7gen", "macht", "nur", "er", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ADV", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Du, berauschter F\u00fcrst von Thule,", "tokens": ["Du", ",", "be\u00b7rauschter", "F\u00fcrst", "von", "Thu\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "APPR", "NE", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.6": {"text": "Warfst den Becher in das Meer;", "tokens": ["Warfst", "den", "Be\u00b7cher", "in", "das", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber, schwankend auf dem Stuhle,", "tokens": ["A\u00b7ber", ",", "schwan\u00b7kend", "auf", "dem", "Stuh\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Fielst du selber hinterher.", "tokens": ["Fielst", "du", "sel\u00b7ber", "hin\u00b7ter\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Schw\u00e4cher brannten schon die Lichter;", "tokens": ["Schw\u00e4\u00b7cher", "brann\u00b7ten", "schon", "die", "Lich\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus den Rahmen am Gew\u00f6lb", "tokens": ["Aus", "den", "Rah\u00b7men", "am", "Ge\u00b7w\u00f6lb"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schauten finstere Gesichter", "tokens": ["Schau\u00b7ten", "fins\u00b7te\u00b7re", "Ge\u00b7sich\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf uns nieder fahl und gelb;", "tokens": ["Auf", "uns", "nie\u00b7der", "fahl", "und", "gelb", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "VVFIN", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Siehe! und in unsre Sitzung", "tokens": ["Sie\u00b7he", "!", "und", "in", "uns\u00b7re", "Sit\u00b7zung"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$.", "KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Drang auf einmal \u2013 war es wahr", "tokens": ["Drang", "auf", "ein\u00b7mal", "\u2013", "war", "es", "wahr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "ADV", "$(", "VAFIN", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Oder Spuk der Weinerhitzung? \u2013", "tokens": ["O\u00b7der", "Spuk", "der", "Wei\u00b7ner\u00b7hit\u00b7zung", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ein erstaunlich fremdes Paar.", "tokens": ["Ein", "er\u00b7staun\u00b7lich", "frem\u00b7des", "Paar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Mit dem Wams von gelbem Leder", "tokens": ["Mit", "dem", "Wams", "von", "gel\u00b7bem", "Le\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "(braungelb wie ein gift'ger Pilz)", "tokens": ["(", "braun\u00b7gelb", "wie", "ein", "gift'\u00b7ger", "Pilz", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und der roten Hahnenfeder", "tokens": ["Und", "der", "ro\u00b7ten", "Hah\u00b7nen\u00b7fe\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Auf dem Hut von grauem Filz,", "tokens": ["Auf", "dem", "Hut", "von", "grau\u00b7em", "Filz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "