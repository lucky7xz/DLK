{"dta.poem.18570": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Betr\u00fcbter Unbestand!", "tokens": ["Be\u00b7tr\u00fcb\u00b7ter", "Un\u00b7be\u00b7stand", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der m", "tokens": ["Der", "m"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Dein falsches Wesen wird mir mehr als wohl bekand/", "tokens": ["Dein", "fal\u00b7sches", "We\u00b7sen", "wird", "mir", "mehr", "als", "wohl", "be\u00b7kand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "PIAT", "KOKOM", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was l\u00e4st der Himmel mich doch nur zum Troste hoffen?", "tokens": ["Was", "l\u00e4st", "der", "Him\u00b7mel", "mich", "doch", "nur", "zum", "Tros\u00b7te", "hof\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PPER", "ADV", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Verliert sein Gnaden-Strahl denn endlich seinen Schein;", "tokens": ["Ver\u00b7liert", "sein", "Gna\u00b7den\u00b7Strahl", "denn", "end\u00b7lich", "sei\u00b7nen", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und will mein Freudenstern nun ein Comete seyn?", "tokens": ["Und", "will", "mein", "Freu\u00b7dens\u00b7tern", "nun", "ein", "Co\u00b7me\u00b7te", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "ADV", "ART", "NN", "VAINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "So \u00e4ndert sich die Zeit/", "tokens": ["So", "\u00e4n\u00b7dert", "sich", "die", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die mich zuvo", "tokens": ["Die", "mich", "zu\u00b7vo"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPER", "NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ich seh' im Geiste schon/ da\u00df mich Vergn\u00fcglichkeit", "tokens": ["Ich", "seh'", "im", "Geis\u00b7te", "schon", "/", "da\u00df", "mich", "Ver\u00b7gn\u00fcg\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "$(", "KOUS", "PPER", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nun vor ein Freuden-Fest heist Marter-Wochen wehlen:", "tokens": ["Nun", "vor", "ein", "Freu\u00b7den\u00b7Fest", "heist", "Mar\u00b7ter\u00b7Wo\u00b7chen", "weh\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es bricht der letzte Tag im Jubel-Jahr herein/", "tokens": ["Es", "bricht", "der", "letz\u00b7te", "Tag", "im", "Ju\u00b7bel\u00b7Jahr", "her\u00b7ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und mein best\u00fcrtzter Fu\u00df tritt in die Fasten ein.", "tokens": ["Und", "mein", "be\u00b7st\u00fcrtz\u00b7ter", "Fu\u00df", "tritt", "in", "die", "Fas\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich strande recht an Port/", "tokens": ["Ich", "stran\u00b7de", "recht", "an", "Port", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NE", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und must zuletzt den h\u00e4rtsten Sturm versp\u00fcren.", "tokens": ["Und", "must", "zu\u00b7letzt", "den", "h\u00e4rts\u00b7ten", "Sturm", "ver\u00b7sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Man wincket mir zwar sehr an den geliebten Ort/", "tokens": ["Man", "win\u00b7cket", "mir", "zwar", "sehr", "an", "den", "ge\u00b7lieb\u00b7ten", "Ort", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ja Lieb und Tugend will mich in den Hafen f\u00fchren;", "tokens": ["Ja", "Lieb", "und", "Tu\u00b7gend", "will", "mich", "in", "den", "Ha\u00b7fen", "f\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "KON", "NN", "VMFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch da mein Schiff nicht f\u00e4hrt mit Tagus gelben Sand/", "tokens": ["Doch", "da", "mein", "Schiff", "nicht", "f\u00e4hrt", "mit", "Ta\u00b7gus", "gel\u00b7ben", "Sand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "PTKNEG", "VVFIN", "APPR", "NE", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So komm ich Armer! auch nicht ins gelobte Land.", "tokens": ["So", "komm", "ich", "Ar\u00b7mer", "!", "auch", "nicht", "ins", "ge\u00b7lob\u00b7te", "Land", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$.", "ADV", "PTKNEG", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So schreckt Egypten mich", "tokens": ["So", "schreckt", "E\u00b7gyp\u00b7ten", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Durch tr\u00fcbe Nacht und Ungl\u00fccks-Finstern\u00fcssen:", "tokens": ["Durch", "tr\u00fc\u00b7be", "Nacht", "und", "Un\u00b7gl\u00fccks\u00b7Fins\u00b7ter\u00b7n\u00fcs\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du Sonne meiner Lust/ warum verbirgst du dich?", "tokens": ["Du", "Son\u00b7ne", "mei\u00b7ner", "Lust", "/", "wa\u00b7rum", "ver\u00b7birgst", "du", "dich", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$(", "PWAV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Soll ich an deiner Statt die duncklen Schatten k\u00fcssen?", "tokens": ["Soll", "ich", "an", "dei\u00b7ner", "Statt", "die", "dunck\u00b7len", "Schat\u00b7ten", "k\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jedoch die Unschuld macht dich aller Schulden frey/", "tokens": ["Je\u00b7doch", "die", "Un\u00b7schuld", "macht", "dich", "al\u00b7ler", "Schul\u00b7den", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "PIAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nur das Verh\u00e4ngni\u00df weist mich in die W\u00fcsteney.", "tokens": ["Nur", "das", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "weist", "mich", "in", "die", "W\u00fcs\u00b7te\u00b7ney", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Dr\u00fcm sey getrost mein Geist/", "tokens": ["Dr\u00fcm", "sey", "ge\u00b7trost", "mein", "Geist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wenn harter Sturm und Ungl\u00fccks-Wetter krachen/", "tokens": ["Wenn", "har\u00b7ter", "Sturm", "und", "Un\u00b7gl\u00fccks\u00b7Wet\u00b7ter", "kra\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vor dem ein schwaches Rohr sich zu der Erden reist/", "tokens": ["Vor", "dem", "ein", "schwa\u00b7ches", "Rohr", "sich", "zu", "der", "Er\u00b7den", "reist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur Gro\u00dfmuht kan allein bey starcken Donner-Lachen.", "tokens": ["Nur", "Gro\u00df\u00b7muht", "kan", "al\u00b7lein", "bey", "star\u00b7cken", "Don\u00b7ner\u00b7La\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VMFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Himmel sieht mich zwar mit rauhen Blicken an/", "tokens": ["Der", "Him\u00b7mel", "sieht", "mich", "zwar", "mit", "rau\u00b7hen", "Bli\u00b7cken", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wer wei\u00df/ ob nicht sein Strahl mich noch erfreuen kan.", "tokens": ["Wer", "wei\u00df", "/", "ob", "nicht", "sein", "Strahl", "mich", "noch", "er\u00b7freu\u00b7en", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "KOUS", "PTKNEG", "PPOSAT", "NN", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Weh't gleich ein Jammer Wind/", "tokens": ["Weh't", "gleich", "ein", "Jam\u00b7mer", "Wind", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So soll Gedult doch in die Seegel blasen:", "tokens": ["So", "soll", "Ge\u00b7dult", "doch", "in", "die", "See\u00b7gel", "bla\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das wandelbare Gl\u00fcck ver\u00e4ndert sich geschwind/", "tokens": ["Das", "wan\u00b7del\u00b7ba\u00b7re", "Gl\u00fcck", "ver\u00b7\u00e4n\u00b7dert", "sich", "ge\u00b7schwind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Zephier k\u00fcsset mich noch wohl nach \u00c6ols Rasen.", "tokens": ["Und", "Ze\u00b7phier", "k\u00fcs\u00b7set", "mich", "noch", "wohl", "nach", "\u00c6ols", "Ra\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADV", "ADV", "APPR", "NE", "NE", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Es anckert mein Gem\u00fcht auf be\u00dfres Wolergehn/", "tokens": ["Es", "an\u00b7ckert", "mein", "Ge\u00b7m\u00fcht", "auf", "be\u00df\u00b7res", "Wo\u00b7ler\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mein Schiff soll \u00fcm das Haupt der frohen Hoffnung stehn.", "tokens": ["Mein", "Schiff", "soll", "\u00fcm", "das", "Haupt", "der", "fro\u00b7hen", "Hoff\u00b7nung", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Geh Ungedult zur Ruh/", "tokens": ["Geh", "Un\u00b7ge\u00b7dult", "zur", "Ruh", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und Hoffnung komm durch den erw\u00fcnschten Morgen/", "tokens": ["Und", "Hoff\u00b7nung", "komm", "durch", "den", "er\u00b7w\u00fcnschten", "Mor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sprich den vergn\u00fcgten Trost doch meiner Seelen zu:", "tokens": ["Sprich", "den", "ver\u00b7gn\u00fcg\u00b7ten", "Trost", "doch", "mei\u00b7ner", "See\u00b7len", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Himmel wird vor dich und dein Gel\u00fccke sorgen:", "tokens": ["Der", "Him\u00b7mel", "wird", "vor", "dich", "und", "dein", "Ge\u00b7l\u00fc\u00b7cke", "sor\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Welt wird nach der Nacht durch Morgenr\u00f6ht erfreut/", "tokens": ["Die", "Welt", "wird", "nach", "der", "Nacht", "durch", "Mor\u00b7gen\u00b7r\u00f6ht", "er\u00b7freut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und auf den Winter folgt die sch\u00f6nste Fr\u00fchlings-Zeit.", "tokens": ["Und", "auf", "den", "Win\u00b7ter", "folgt", "die", "sch\u00f6ns\u00b7te", "Fr\u00fch\u00b7lings\u00b7Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}