{"textgrid.poem.65738": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "12", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Z\u00e4hl' ich die Glocke, die die Zeiten mi\u00dft,", "tokens": ["Z\u00e4hl'", "ich", "die", "Glo\u00b7cke", ",", "die", "die", "Zei\u00b7ten", "mi\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Seh' ich den wackern Tag in Nacht verloren,", "tokens": ["Seh'", "ich", "den", "wa\u00b7ckern", "Tag", "in", "Nacht", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wie des Veilchens Lenz vor\u00fcber ist,", "tokens": ["Und", "wie", "des", "Veil\u00b7chens", "Lenz", "vor\u00b7\u00fc\u00b7ber", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "NN", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie sich mit Silber dunkle Haar' umfloren;", "tokens": ["Wie", "sich", "mit", "Sil\u00b7ber", "dunk\u00b7le", "Haa\u00b7r'", "um\u00b7flo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPR", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Erblick' ich hoher Wipfel d\u00fcrres Laub,", "tokens": ["Er\u00b7blick'", "ich", "ho\u00b7her", "Wip\u00b7fel", "d\u00fcr\u00b7res", "Laub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die erst ein Schattendach der Herde waren,", "tokens": ["Die", "erst", "ein", "Schat\u00b7ten\u00b7dach", "der", "Her\u00b7de", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Gesch\u00fcrzt in Garben gr\u00fcnen Feldesraub", "tokens": ["Ge\u00b7sch\u00fcrzt", "in", "Gar\u00b7ben", "gr\u00fc\u00b7nen", "Fel\u00b7de\u00b7sraub"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wei\u00dfb\u00e4rtig, wie im Sarg, zur Scheuer fahren:", "tokens": ["Wei\u00df\u00b7b\u00e4r\u00b7tig", ",", "wie", "im", "Sarg", ",", "zur", "Scheu\u00b7er", "fah\u00b7ren", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "APPRART", "NN", "$,", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Dann kommt mir deine Sch\u00f6nheit in den Sinn,", "tokens": ["Dann", "kommt", "mir", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", "in", "den", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Da\u00df du der Zeiten Tr\u00fcmmer mu\u00dft vermehren;", "tokens": ["Da\u00df", "du", "der", "Zei\u00b7ten", "Tr\u00fcm\u00b7mer", "mu\u00dft", "ver\u00b7meh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Weil Reiz und Jugendschmuck sich selbst entfliehn,", "tokens": ["Weil", "Reiz", "und", "Ju\u00b7gend\u00b7schmuck", "sich", "selbst", "ent\u00b7fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Sich selbst so schnell als andre bl\u00fchn, zerst\u00f6ren,", "tokens": ["Sich", "selbst", "so", "schnell", "als", "and\u00b7re", "bl\u00fchn", ",", "zer\u00b7st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "ADJD", "KOKOM", "PIS", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und vor dem Sensenhieb der Zeit nichts wahrt", "tokens": ["Und", "vor", "dem", "Sen\u00b7sen\u00b7hieb", "der", "Zeit", "nichts", "wahrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "PIS", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Als, ihm zum Trutz, Fortzeugung deiner Art.", "tokens": ["Als", ",", "ihm", "zum", "Trutz", ",", "Fort\u00b7zeu\u00b7gung", "dei\u00b7ner", "Art", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PPER", "APPRART", "NN", "$,", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}