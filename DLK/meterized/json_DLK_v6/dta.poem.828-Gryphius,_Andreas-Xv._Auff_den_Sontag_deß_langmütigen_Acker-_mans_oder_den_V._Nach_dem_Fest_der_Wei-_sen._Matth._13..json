{"dta.poem.828": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Xv.  Auff den Sontag de\u00df langm\u00fctigen Acker-  \n mans/ oder den  V.  Nach dem Fest der Wei-  \n sen. Matth. 13.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Der Feind strew't aus aufs Land das du erbawet/ ", "tokens": ["Der", "Feind", "stre\u00b7w't", "aus", "aufs", "Land", "das", "du", "er\u00b7ba\u00b7wet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPRART", "NN", "ART", "PPER", "VVFIN", "$("], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sein Vnkraut! HERR! in dem die S\u00fcnden Nacht", "tokens": ["Sein", "Vnkraut", "!", "HeRR", "!", "in", "dem", "die", "S\u00fcn\u00b7den", "Nacht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "NN", "$.", "APPR", "PRELS", "ART", "NN", "NN"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "In tr\u00fcben Schlaff die tr\u00e4gen Menschen bracht", "tokens": ["In", "tr\u00fc\u00b7ben", "Schlaff", "die", "tr\u00e4\u00b7gen", "Men\u00b7schen", "bracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den du die Frucht zu h\u00fctten an vertrawet!", "tokens": ["Den", "du", "die", "Frucht", "zu", "h\u00fct\u00b7ten", "an", "ver\u00b7tra\u00b7wet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "PTKZU", "VVINF", "APPR", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Di\u00df was man nur auff allen \u00e4ckern schawet", "tokens": ["Di\u00df", "was", "man", "nur", "auff", "al\u00b7len", "\u00e4c\u00b7kern", "scha\u00b7wet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PRELS", "PIS", "ADV", "APPR", "PIS", "VVINF", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Ist falsche Lehr vnd Neyd vnd Ketzer pracht", "tokens": ["Ist", "fal\u00b7sche", "Lehr", "vnd", "Neyd", "vnd", "Ket\u00b7zer", "pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJA", "NN", "KON", "NN", "KON", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir schlaffen fest: der Sathan seet vnd wacht", "tokens": ["Wir", "schlaf\u00b7fen", "fest", ":", "der", "Sa\u00b7than", "seet", "vnd", "wacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Sathan dem vor deinem Segen grawet.", "tokens": ["Der", "Sa\u00b7than", "dem", "vor", "dei\u00b7nem", "Se\u00b7gen", "gra\u00b7wet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Ach sihst du nicht wie jene schar vmb l\u00e4ufft", "tokens": ["Ach", "sihst", "du", "nicht", "wie", "je\u00b7ne", "schar", "vmb", "l\u00e4ufft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPER", "PTKNEG", "KOKOM", "PDS", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die dir zu trotz so Korn al\u00df Tresp aus r\u00e4ufft.", "tokens": ["Die", "dir", "zu", "trotz", "so", "Korn", "al\u00df", "Tresp", "aus", "r\u00e4ufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "APPR", "ADV", "NN", "KOUS", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn wirst du dich zu letzter Ernd\u2019 auffmachen?", "tokens": ["Wenn", "wirst", "du", "dich", "zu", "letz\u00b7ter", "Ernd'", "auff\u00b7ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Kom\u2019 es ist zeitt! f\u00fchr alle Garben eyn!", "tokens": ["Kom'", "es", "ist", "zeitt", "!", "f\u00fchr", "al\u00b7le", "Gar\u00b7ben", "eyn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VAFIN", "VVFIN", "$.", "VVFIN", "PIAT", "NN", "VAINF", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "F\u00fchr eyn die Frucht. La\u00df in der Flammen Pein", "tokens": ["F\u00fchr", "eyn", "die", "Frucht", ".", "La\u00df", "in", "der", "Flam\u00b7men", "Pein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "ART", "NN", "$.", "VVIMP", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Sathans Saat/ die nicht mehr taug/ verkrachen.", "tokens": ["Des", "Sa\u00b7thans", "Saat", "/", "die", "nicht", "mehr", "taug", "/", "ver\u00b7kra\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "PRELS", "PTKNEG", "ADV", "VVFIN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}