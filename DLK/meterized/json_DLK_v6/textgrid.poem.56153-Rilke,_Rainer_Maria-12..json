{"textgrid.poem.56153": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wolle die Wandlung. O sei f\u00fcr die Flamme begeistert,", "tokens": ["Wol\u00b7le", "die", "Wand\u00b7lung", ".", "O", "sei", "f\u00fcr", "die", "Flam\u00b7me", "be\u00b7geis\u00b7tert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "$.", "NE", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+---+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "drin sich ein Ding dir entzieht, das mit Verwandlungen prunkt;", "tokens": ["drin", "sich", "ein", "Ding", "dir", "ent\u00b7zieht", ",", "das", "mit", "Ver\u00b7wand\u00b7lun\u00b7gen", "prunkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ART", "NN", "PPER", "VVFIN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "jener entwerfende Geist, welcher das Irdische meistert,", "tokens": ["je\u00b7ner", "ent\u00b7wer\u00b7fen\u00b7de", "Geist", ",", "wel\u00b7cher", "das", "Ir\u00b7di\u00b7sche", "meis\u00b7tert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+---+--+-", "measure": "dactylic.tri.plus"}, "line.4": {"text": "liebt in dem Schwung der Figur nichts wie den wendenden Punkt.", "tokens": ["liebt", "in", "dem", "Schwung", "der", "Fi\u00b7gur", "nichts", "wie", "den", "wen\u00b7den\u00b7den", "Punkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "PIS", "KOKOM", "ART", "PIAT", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Was sich ins Bleiben verschlie\u00dft, schon ", "tokens": ["Was", "sich", "ins", "Blei\u00b7ben", "ver\u00b7schlie\u00dft", ",", "schon"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "PRF", "APPRART", "NN", "VVFIN", "$,", "ADV"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "w\u00e4hnt es sich sicher im Schutz des unscheinbaren Grau's?", "tokens": ["w\u00e4hnt", "es", "sich", "si\u00b7cher", "im", "Schutz", "des", "un\u00b7schein\u00b7ba\u00b7ren", "Grau's", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Warte, ein H\u00e4rtestes warnt aus der Ferne das Harte.", "tokens": ["War\u00b7te", ",", "ein", "H\u00e4r\u00b7tes\u00b7tes", "warnt", "aus", "der", "Fer\u00b7ne", "das", "Har\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.4": {"text": "Wehe \u2013: abwesender Hammer holt aus!", "tokens": ["We\u00b7he", "\u2013", ":", "ab\u00b7we\u00b7sen\u00b7der", "Ham\u00b7mer", "holt", "aus", "!"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "$.", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Wer sich als Quelle ergie\u00dft, den erkennt die Erkennung;", "tokens": ["Wer", "sich", "als", "Quel\u00b7le", "er\u00b7gie\u00dft", ",", "den", "er\u00b7kennt", "die", "Er\u00b7ken\u00b7nung", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "KOUS", "NN", "VVFIN", "$,", "ART", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "und sie fuhrt ihn entz\u00fcckt durch das heiter Geschaffne,", "tokens": ["und", "sie", "fuhrt", "ihn", "ent\u00b7z\u00fcckt", "durch", "das", "hei\u00b7ter", "Ge\u00b7schaff\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "das mit Anfang oft schlie\u00dft und mit Ende beginnt.", "tokens": ["das", "mit", "An\u00b7fang", "oft", "schlie\u00dft", "und", "mit", "En\u00b7de", "be\u00b7ginnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "ADV", "VVFIN", "KON", "APPR", "NN", "VVFIN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.4": {"line.1": {"text": "Jeder gl\u00fcckliche Raum ist Kind oder Enkel von Trennung,", "tokens": ["Je\u00b7der", "gl\u00fcck\u00b7li\u00b7che", "Raum", "ist", "Kind", "o\u00b7der", "En\u00b7kel", "von", "Tren\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "den sie staunend durchgehn. Und die verwandelte Daphne", "tokens": ["den", "sie", "stau\u00b7nend", "durch\u00b7gehn", ".", "Und", "die", "ver\u00b7wan\u00b7del\u00b7te", "Daph\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "VVINF", "$.", "KON", "ART", "ADJA", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "will, seit sie lorbeern f\u00fchlt, da\u00df du dich wandelst in Wind.", "tokens": ["will", ",", "seit", "sie", "lor\u00b7beern", "f\u00fchlt", ",", "da\u00df", "du", "dich", "wan\u00b7delst", "in", "Wind", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "KOUS", "PPER", "VVINF", "VVFIN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}}}}