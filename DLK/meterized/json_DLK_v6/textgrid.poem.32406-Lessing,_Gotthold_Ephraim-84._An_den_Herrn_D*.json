{"textgrid.poem.32406": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "84. An den Herrn D*", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Epigramm, o D*, ist fein!", "tokens": ["Dein", "E\u00b7pig\u00b7ramm", ",", "o", "D", "*", ",", "ist", "fein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "XY", "XY", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es hat mich trefflich durchgezogen;", "tokens": ["Es", "hat", "mich", "treff\u00b7lich", "durch\u00b7ge\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und ist, vollkommen sch\u00f6n zu sein,", "tokens": ["Und", "ist", ",", "voll\u00b7kom\u00b7men", "sch\u00f6n", "zu", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "ADJD", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erstunken und erlogen.", "tokens": ["Er\u00b7stun\u00b7ken", "und", "er\u00b7lo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}