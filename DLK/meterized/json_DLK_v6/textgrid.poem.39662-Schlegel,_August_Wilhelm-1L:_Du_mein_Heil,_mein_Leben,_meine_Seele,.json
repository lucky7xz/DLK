{"textgrid.poem.39662": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du mein Heil, mein Leben, meine Seele,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du mein Heil, mein Leben, meine Seele,", "tokens": ["Du", "mein", "Heil", ",", "mein", "Le\u00b7ben", ",", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "S\u00fc\u00dfes Wesen, von des Himmels Macht", "tokens": ["S\u00fc\u00b7\u00dfes", "We\u00b7sen", ",", "von", "des", "Him\u00b7mels", "Macht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Darum, d\u00fcnkt mir, nur hervorgebracht,", "tokens": ["Da\u00b7rum", ",", "d\u00fcnkt", "mir", ",", "nur", "her\u00b7vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "$,", "VVFIN", "PPER", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df dich Liebe ganz mir anverm\u00e4hle!", "tokens": ["Da\u00df", "dich", "Lie\u00b7be", "ganz", "mir", "an\u00b7ver\u00b7m\u00e4h\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Welcher meiner todeswerthen Fehle", "tokens": ["Wel\u00b7cher", "mei\u00b7ner", "to\u00b7des\u00b7wert\u00b7hen", "Feh\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAT", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bannte mich in diesen Sclavenschacht,", "tokens": ["Bann\u00b7te", "mich", "in", "die\u00b7sen", "Scla\u00b7ven\u00b7schacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo ich fern von dir, in \u00f6der Nacht,", "tokens": ["Wo", "ich", "fern", "von", "dir", ",", "in", "\u00f6\u00b7der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "APPR", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ohne Licht und W\u00e4rme mich zerqu\u00e4le?", "tokens": ["Oh\u00b7ne", "Licht", "und", "W\u00e4r\u00b7me", "mich", "zer\u00b7qu\u00e4\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "O warum entbehret mein Gesicht", "tokens": ["O", "wa\u00b7rum", "ent\u00b7beh\u00b7ret", "mein", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PWAV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Jenen Strahl aus deinem Himmelsauge,", "tokens": ["Je\u00b7nen", "Strahl", "aus", "dei\u00b7nem", "Him\u00b7mel\u00b7sau\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Den ich d\u00fcrftig nur im Geiste sauge?", "tokens": ["Den", "ich", "d\u00fcrf\u00b7tig", "nur", "im", "Geis\u00b7te", "sau\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Und die Lippe, welche singt und spricht,", "tokens": ["Und", "die", "Lip\u00b7pe", ",", "wel\u00b7che", "singt", "und", "spricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df ich kaum ihr nachzulallen tauge,", "tokens": ["Da\u00df", "ich", "kaum", "ihr", "nach\u00b7zu\u00b7lal\u00b7len", "tau\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVIZU", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "O warum erquickt sie mich denn nicht?", "tokens": ["O", "wa\u00b7rum", "er\u00b7quickt", "sie", "mich", "denn", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "VVFIN", "PPER", "PRF", "ADV", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}