{"textgrid.poem.54578": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "67.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was hat des F\u00fcrsten Hof/ was fand die weise Stadt/", "tokens": ["Was", "hat", "des", "F\u00fcrs\u00b7ten", "Hof", "/", "was", "fand", "die", "wei\u00b7se", "Stadt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "NE", "$(", "PWS", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das m\u00e4chtig sey mich zu erfreuen?", "tokens": ["Das", "m\u00e4ch\u00b7tig", "sey", "mich", "zu", "er\u00b7freu\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VAFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich mu\u00df die sch\u00f6ne Zeit bereuen/", "tokens": ["Ich", "mu\u00df", "die", "sch\u00f6\u00b7ne", "Zeit", "be\u00b7reu\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die mein Gem\u00fcth ohn sie/ mein Licht/ verzehret hat.", "tokens": ["Die", "mein", "Ge\u00b7m\u00fcth", "ohn", "sie", "/", "mein", "Licht", "/", "ver\u00b7zeh\u00b7ret", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PPER", "$(", "PPOSAT", "NN", "$(", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bey ihr find ich/ was ich voll Hertzens-Seuffzer bat.", "tokens": ["Bey", "ihr", "find", "ich", "/", "was", "ich", "voll", "Hert\u00b7zens\u00b7Seuff\u00b7zer", "bat", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "$(", "PWS", "PPER", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Saamen in das Land einstreuen", "tokens": ["Die", "Saa\u00b7men", "in", "das", "Land", "ein\u00b7streu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Begehren so nicht das Erneuen", "tokens": ["Be\u00b7geh\u00b7ren", "so", "nicht", "das", "Er\u00b7neu\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PTKNEG", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Des Fr\u00fchlings/ der mit Thau kr\u00f6nt die erfrischte Saat;", "tokens": ["Des", "Fr\u00fch\u00b7lings", "/", "der", "mit", "Thau", "kr\u00f6nt", "die", "er\u00b7frischte", "Saat", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Als mich verlanget sie zu schauen/", "tokens": ["Als", "mich", "ver\u00b7lan\u00b7get", "sie", "zu", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sie meine Lust/ Wonn und Vertrauen/", "tokens": ["Sie", "mei\u00b7ne", "Lust", "/", "Wonn", "und", "Ver\u00b7trau\u00b7en", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$(", "NE", "KON", "NN", "$("], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Die mir der Himmel gab zu enden meine Klagen.", "tokens": ["Die", "mir", "der", "Him\u00b7mel", "gab", "zu", "en\u00b7den", "mei\u00b7ne", "Kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sie kan ich diesen Tag nicht sehn/", "tokens": ["Sie", "kan", "ich", "die\u00b7sen", "Tag", "nicht", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PDAT", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ach Himmel la\u00df es doch geschehn", "tokens": ["Ach", "Him\u00b7mel", "la\u00df", "es", "doch", "ge\u00b7schehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Da\u00df mir m\u00f6g ihr Gesicht die Nacht ein Traum vortragen.", "tokens": ["Da\u00df", "mir", "m\u00f6g", "ihr", "Ge\u00b7sicht", "die", "Nacht", "ein", "Traum", "vor\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}}}}