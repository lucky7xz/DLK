{"textgrid.poem.31580": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr Jesu mache du den Anfang meiner Sachen!", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Jesu mache du den Anfang meiner Sachen!", "tokens": ["Herr", "Je\u00b7su", "ma\u00b7che", "du", "den", "An\u00b7fang", "mei\u00b7ner", "Sa\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir / Dir befihl ich mich / mit allem meinen Thun.", "tokens": ["Dir", "/", "Dir", "be\u00b7fihl", "ich", "mich", "/", "mit", "al\u00b7lem", "mei\u00b7nen", "Thun", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PPER", "VVFIN", "PPER", "PRF", "$(", "APPR", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach da\u00df dein klarer Geist mir aus den Lippen rund' /", "tokens": ["Ach", "da\u00df", "dein", "kla\u00b7rer", "Geist", "mir", "aus", "den", "Lip\u00b7pen", "rund'", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPOSAT", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und gl\u00e4nzt / wie vor im Geist / itzt in dem Mund und Sprachen!", "tokens": ["und", "gl\u00e4nzt", "/", "wie", "vor", "im", "Geist", "/", "itzt", "in", "dem", "Mund", "und", "Spra\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOKOM", "APPR", "APPRART", "NN", "$(", "ADV", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach da\u00df die Flammenfl\u00fc\u00df herz-r\u00fchrend' ausherbrachen /", "tokens": ["Ach", "da\u00df", "die", "Flam\u00b7men\u00b7fl\u00fc\u00df", "her\u00b7zr\u00fch\u00b7rend'", "aus\u00b7her\u00b7bra\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "ART", "NN", "APZR", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "da\u00df meine K\u00e4hle w\u00e4r ein Witz- und Warheit-Brunn!", "tokens": ["da\u00df", "mei\u00b7ne", "K\u00e4h\u00b7le", "w\u00e4r", "ein", "Witz", "und", "Wa\u00b7rheit\u00b7Brunn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "ART", "TRUNC", "KON", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "das Chor ist schon erf\u00fcllt; Mein Herrscher / sag nur / Nun", "tokens": ["das", "Chor", "ist", "schon", "er\u00b7f\u00fcllt", ";", "Mein", "Herr\u00b7scher", "/", "sag", "nur", "/", "Nun"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$.", "PPOSAT", "NN", "$(", "VVIMP", "ADV", "$(", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "so will ich / solt die Seel ausgehn / den Anfang machen.", "tokens": ["so", "will", "ich", "/", "solt", "die", "Seel", "aus\u00b7gehn", "/", "den", "An\u00b7fang", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$(", "VMFIN", "ART", "NN", "VVINF", "$(", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich will / wie Cr\u00f6sus Sohn / vor lauter Lieb und Gier /", "tokens": ["Ich", "will", "/", "wie", "Cr\u00f6\u00b7sus", "Sohn", "/", "vor", "lau\u00b7ter", "Lieb", "und", "Gier", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "KOKOM", "NE", "NN", "$(", "APPR", "PIAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "auch stumme Zung und Mund / dich zu bedienen / finden.", "tokens": ["auch", "stum\u00b7me", "Zung", "und", "Mund", "/", "dich", "zu", "be\u00b7die\u00b7nen", "/", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$(", "PPER", "PTKZU", "VVINF", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mach nur die Rolle auf des lieben Tages schier /", "tokens": ["Mach", "nur", "die", "Rol\u00b7le", "auf", "des", "lie\u00b7ben", "Ta\u00b7ges", "schier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "ART", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "und la\u00df durch Glauben mich unglaublich \u00fcberwinden:", "tokens": ["und", "la\u00df", "durch", "Glau\u00b7ben", "mich", "un\u00b7glaub\u00b7lich", "\u00fc\u00b7berw\u00b7in\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "NN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "das Erd' und Himmel froh erstaunend schauen an /", "tokens": ["das", "Erd'", "und", "Him\u00b7mel", "froh", "er\u00b7stau\u00b7nend", "schau\u00b7en", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADJD", "VVPP", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "wie als durch deine Gnad / der Glaube schafft und kan.", "tokens": ["wie", "als", "durch", "dei\u00b7ne", "Gnad", "/", "der", "Glau\u00b7be", "schafft", "und", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "KOKOM", "APPR", "PPOSAT", "NN", "$(", "ART", "NN", "VVFIN", "KON", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}