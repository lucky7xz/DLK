{"dta.poem.3390": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Die Sommernacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zw\u00f6lfe schlug es. Es wirft &#252;ber die m&#252;de Welt", "tokens": ["Zw\u00f6l\u00b7fe", "schlug", "es", ".", "Es", "wirft", "&#252;", "ber", "die", "m", "&#252;", "de", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "XML_entity", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "$(", "ADV", "ART", "FM", "FM", "FM", "NN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ihren Schleier die Nacht dunkler und schattender.", "tokens": ["Ih\u00b7ren", "Schlei\u00b7er", "die", "Nacht", "dunk\u00b7ler", "und", "schat\u00b7ten\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "ADJA", "KON", "ADJD", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Schon ist ", "tokens": ["Schon", "ist"], "token_info": ["word", "word"], "pos": ["ADV", "VAFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Schon ", "tokens": ["Schon"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Tr\u00fcb' und r\u00f6thlich enttaucht ", "tokens": ["Tr\u00fcb'", "und", "r\u00f6th\u00b7lich", "ent\u00b7taucht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VVPP"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Nacht.", "tokens": ["Nacht", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Aus dem silbernen Duft schauet der Mond hervor,", "tokens": ["Aus", "dem", "sil\u00b7ber\u00b7nen", "Duft", "schau\u00b7et", "der", "Mond", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Kos't die sch\u00fcchterne D\u00e4mmrung,", "tokens": ["Kos't", "die", "sch\u00fcch\u00b7ter\u00b7ne", "D\u00e4mm\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Wie ein J\u00fcngling die bl\u00f6de Braut.", "tokens": ["Wie", "ein", "J\u00fcng\u00b7ling", "die", "bl\u00f6\u00b7de", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Aus dem silbernen Duft flimmre vertraulich, Freund,", "tokens": ["Aus", "dem", "sil\u00b7ber\u00b7nen", "Duft", "flimm\u00b7re", "ver\u00b7trau\u00b7lich", ",", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ADJD", "$,", "NN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Durch das schwankende Laub, das um mein Lager", "tokens": ["Durch", "das", "schwan\u00b7ken\u00b7de", "Laub", ",", "das", "um", "mein", "La\u00b7ger"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "schwirrt,", "tokens": ["schwirrt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Um den duftenden Rasen,", "tokens": ["Um", "den", "duf\u00b7ten\u00b7den", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Den ich mir f\u00fcr die Nacht erkohr.", "tokens": ["Den", "ich", "mir", "f\u00fcr", "die", "Nacht", "er\u00b7kohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wahrlich, sch\u00f6n ist mein Bett. Duftendes weiches", "tokens": ["Wahr\u00b7lich", ",", "sch\u00f6n", "ist", "mein", "Bett", ".", "Duf\u00b7ten\u00b7des", "wei\u00b7ches"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "$.", "NN", "ADJA"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Gras", "tokens": ["Gras"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Ist mein Polster; mein Pf\u00fchl ist der bemooste Stein;", "tokens": ["Ist", "mein", "Pols\u00b7ter", ";", "mein", "Pf\u00fchl", "ist", "der", "be\u00b7moos\u00b7te", "Stein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Meine Kerze der Gl\u00fchwurm,", "tokens": ["Mei\u00b7ne", "Ker\u00b7ze", "der", "Gl\u00fch\u00b7wurm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schilfges\u00e4usel und Wachtelshlag", "tokens": ["Schilf\u00b7ge\u00b7s\u00e4u\u00b7sel", "und", "Wach\u00b7tels\u00b7hlag"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Sind mein Wiegengesang. Aber es wieget heut", "tokens": ["Sind", "mein", "Wie\u00b7gen\u00b7ge\u00b7sang", ".", "A\u00b7ber", "es", "wie\u00b7get", "heut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "$.", "KON", "PPER", "VVFIN", "ADV"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Nicht das S\u00e4useln des Schilfs, nicht das Gel\u00e4ut'", "tokens": ["Nicht", "das", "S\u00e4u\u00b7seln", "des", "Schilfs", ",", "nicht", "das", "Ge\u00b7l\u00e4ut'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "ART", "NN", "$,", "PTKNEG", "ART", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "im Teich", "tokens": ["im", "Teich"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Mich in Schlummer. Es wieget", "tokens": ["Mich", "in", "Schlum\u00b7mer", ".", "Es", "wie\u00b7get"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "APPR", "NN", "$.", "PPER", "VVFIN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Meine Seel' in erhabnen Ernst.", "tokens": ["Mei\u00b7ne", "Seel'", "in", "er\u00b7hab\u00b7nen", "Ernst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Viel zu hehr ist die Nacht, viel zu gedankenvoll", "tokens": ["Viel", "zu", "hehr", "ist", "die", "Nacht", ",", "viel", "zu", "ge\u00b7dan\u00b7ken\u00b7voll"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PTKA", "ADJD", "VAFIN", "ART", "NN", "$,", "ADV", "PTKA", "ADJD"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Schaut vom Himmel der Mond. Anger und Trifft", "tokens": ["Schaut", "vom", "Him\u00b7mel", "der", "Mond", ".", "An\u00b7ger", "und", "Trifft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$.", "NN", "KON", "NN"], "meter": "+-+--++--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "und Flur", "tokens": ["und", "Flur"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Stehn so magisch im Nebel,", "tokens": ["Stehn", "so", "ma\u00b7gisch", "im", "Ne\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Der dem dampfenden Grund entwallt.", "tokens": ["Der", "dem", "damp\u00b7fen\u00b7den", "Grund", "ent\u00b7wallt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Du, der Zeiten des Tags herrlichste, heiligste,", "tokens": ["Du", ",", "der", "Zei\u00b7ten", "des", "Tags", "herr\u00b7lichs\u00b7te", ",", "hei\u00b7ligs\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "ART", "NN", "VVFIN", "$,", "ADJA", "$,"], "meter": "+-+--++--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ahndungschwangere Nacht, Schwester der Ewigkeit", "tokens": ["Ahn\u00b7dung\u00b7schwan\u00b7ge\u00b7re", "Nacht", ",", "Schwes\u00b7ter", "der", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "NN", "ART", "NN"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Traute Freundin des Kummers,", "tokens": ["Trau\u00b7te", "Freun\u00b7din", "des", "Kum\u00b7mers", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "S\u00fcssschwerm\u00fcthige Schw\u00e4rmerin,", "tokens": ["S\u00fcss\u00b7schwer\u00b7m\u00fct\u00b7hi\u00b7ge", "Schw\u00e4r\u00b7me\u00b7rin", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Nacht, dich liebet mein Geist! Deinen erhabnen", "tokens": ["Nacht", ",", "dich", "lie\u00b7bet", "mein", "Geist", "!", "Dei\u00b7nen", "er\u00b7hab\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$.", "PPOSAT", "ADJA"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ernst,", "tokens": ["Ernst", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Deinen schweigenden Gram, deine geweihten", "tokens": ["Dei\u00b7nen", "schwei\u00b7gen\u00b7den", "Gram", ",", "dei\u00b7ne", "ge\u00b7weih\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Graun", "tokens": ["Graun"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Lieb' ich mehr, als des Morgens", "tokens": ["Lieb'", "ich", "mehr", ",", "als", "des", "Mor\u00b7gens"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Aufgang, mehr als das Abendk\u00fchl.", "tokens": ["Auf\u00b7gang", ",", "mehr", "als", "das", "A\u00b7bend\u00b7k\u00fchl", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PIAT", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Nacht und Dunkel umh\u00fcllt unsers Erhabnen", "tokens": ["Nacht", "und", "Dun\u00b7kel", "um\u00b7h\u00fcllt", "un\u00b7sers", "Er\u00b7hab\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Thron;", "tokens": ["Thron", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Und in Dunkel und Nacht f\u00fchl' ich dem Herr-", "tokens": ["Und", "in", "Dun\u00b7kel", "und", "Nacht", "f\u00fchl'", "ich", "dem", "Herr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN", "VVFIN", "PPER", "ART", "TRUNC"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "lichen", "tokens": ["li\u00b7chen"], "token_info": ["word"], "pos": ["ADJA"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Mich viel n\u00e4her. Der Gottheit", "tokens": ["Mich", "viel", "n\u00e4\u00b7her", ".", "Der", "Got\u00b7theit"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "ADJD", "$.", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Leise S\u00e4usel umschauern mich.", "tokens": ["Lei\u00b7se", "S\u00e4u\u00b7sel", "um\u00b7schau\u00b7ern", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.10": {"line.1": {"text": "F\u00fcr das Endliche f\u00fchlt sich der gehobne Geist", "tokens": ["F\u00fcr", "das", "End\u00b7li\u00b7che", "f\u00fchlt", "sich", "der", "ge\u00b7hob\u00b7ne", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Viel zu edel. Ihm schwant h\u00f6here Seligkeit,", "tokens": ["Viel", "zu", "e\u00b7del", ".", "Ihm", "schwant", "h\u00f6\u00b7he\u00b7re", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADJD", "$.", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Als der Taumel der Sinne,", "tokens": ["Als", "der", "Tau\u00b7mel", "der", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Als der Becher der Lust gew\u00e4hrt.", "tokens": ["Als", "der", "Be\u00b7cher", "der", "Lust", "ge\u00b7w\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.11": {"line.1": {"text": "Nach Gen\u00fcssen, die nie ekeln, nach Seligkeit,", "tokens": ["Nach", "Ge\u00b7n\u00fcs\u00b7sen", ",", "die", "nie", "e\u00b7keln", ",", "nach", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "VVINF", "$,", "APPR", "NN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Welche nimmer versiegt, lechzt der Unsterbliche;", "tokens": ["Wel\u00b7che", "nim\u00b7mer", "ver\u00b7siegt", ",", "lechzt", "der", "U\u00b7nsterb\u00b7li\u00b7che", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+--+---", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ach, im Antlitz der Sterne", "tokens": ["Ach", ",", "im", "Ant\u00b7litz", "der", "Ster\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPRART", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Lechzet, schmachtet, verschmachtet er.", "tokens": ["Lech\u00b7zet", ",", "schmach\u00b7tet", ",", "ver\u00b7schmach\u00b7tet", "er", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.12": {"line.1": {"text": "Volle Gn\u00fcge, nach dir brannte der Knabe schon.", "tokens": ["Vol\u00b7le", "Gn\u00fc\u00b7ge", ",", "nach", "dir", "brann\u00b7te", "der", "Kna\u00b7be", "schon", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "PPER", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Doch das tappende Herz w\u00e4hnet', es durstete", "tokens": ["Doch", "das", "tap\u00b7pen\u00b7de", "Herz", "w\u00e4h\u00b7net'", ",", "es", "durs\u00b7te\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Bald nach schmeichelnden Ehren,", "tokens": ["Bald", "nach", "schmei\u00b7cheln\u00b7den", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Bald nach lieblicher M\u00e4dchen Kuss.", "tokens": ["Bald", "nach", "lieb\u00b7li\u00b7cher", "M\u00e4d\u00b7chen", "Kuss", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "NE", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.13": {"line.1": {"text": "Rastlos rang ich nach Ruhm, flocht um den jungen", "tokens": ["Rast\u00b7los", "rang", "ich", "nach", "Ruhm", ",", "flocht", "um", "den", "jun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "NN", "$,", "VVFIN", "APPR", "ART", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Schlaf", "tokens": ["Schlaf"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Frische Lorbeern; wie bald welkte der frische Kranz!", "tokens": ["Fri\u00b7sche", "Lor\u00b7beern", ";", "wie", "bald", "welk\u00b7te", "der", "fri\u00b7sche", "Kranz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PWAV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "St\u00fcrmisch warb ich um Liebe.", "tokens": ["St\u00fcr\u00b7misch", "warb", "ich", "um", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Liebe ward mir. Ich w\u00e4hnte mich", "tokens": ["Lie\u00b7be", "ward", "mir", ".", "Ich", "w\u00e4hn\u00b7te", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$.", "PPER", "VVFIN", "PPER"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.14": {"line.1": {"text": "Nun vollgl\u00fccklich; wie bald seufzte der Gl\u00fcckliche!", "tokens": ["Nun", "voll\u00b7gl\u00fcck\u00b7lich", ";", "wie", "bald", "seufz\u00b7te", "der", "Gl\u00fcck\u00b7li\u00b7che", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "PWAV", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "--+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In des Edleren Arm, an der Geliebten Brust,", "tokens": ["In", "des", "Ed\u00b7le\u00b7ren", "Arm", ",", "an", "der", "Ge\u00b7lieb\u00b7ten", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "In dem Schoss des Entz\u00fcckens", "tokens": ["In", "dem", "Schoss", "des", "Ent\u00b7z\u00fc\u00b7ckens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Seufzte, lechzte, verschmachtet' ich!", "tokens": ["Seufz\u00b7te", ",", "lechz\u00b7te", ",", "ver\u00b7schmach\u00b7tet'", "ich", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}