{"textgrid.poem.46249": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Unendliche liebspein", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich brenn aus lieb und lust, doch kan der brunst verdru\u00df", "tokens": ["Ich", "brenn", "aus", "lieb", "und", "lust", ",", "doch", "kan", "der", "brunst", "ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJD", "KON", "VVFIN", "$,", "ADV", "VMFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "meines haupts feuchtigkeit und thr\u00e4nen nicht verzehren;", "tokens": ["mei\u00b7nes", "haupts", "feuch\u00b7tig\u00b7keit", "und", "thr\u00e4\u00b7nen", "nicht", "ver\u00b7zeh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "VVFIN", "PTKNEG", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "ich wein aus lieb und leid, doch kan mein zeherflu\u00df", "tokens": ["ich", "wein", "aus", "lieb", "und", "leid", ",", "doch", "kan", "mein", "ze\u00b7her\u00b7flu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "ADJD", "KON", "ADJD", "$,", "ADV", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "meiner brust gro\u00dfen brunst und flammen gar nicht wehren.", "tokens": ["mei\u00b7ner", "brust", "gro\u00b7\u00dfen", "brunst", "und", "flam\u00b7men", "gar", "nicht", "weh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "KON", "VVFIN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Ja, vil mehr pfleget stets meiner brunst \u00fcberflu\u00df", "tokens": ["Ja", ",", "vil", "mehr", "pfle\u00b7get", "stets", "mei\u00b7ner", "brunst", "\u00fc\u00b7berf\u00b7lu\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "ADV", "VVFIN", "ADV", "PPOSAT", "NN", "NN"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "den quellen meines leids die nahrung zu bescheren;", "tokens": ["den", "quel\u00b7len", "mei\u00b7nes", "leids", "die", "nah\u00b7rung", "zu", "be\u00b7sche\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "ja, vilmehr pfleget stets meines leids zehergu\u00df", "tokens": ["ja", ",", "vil\u00b7mehr", "pfle\u00b7get", "stets", "mei\u00b7nes", "leids", "ze\u00b7her\u00b7gu\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.8": {"text": "die flammen meiner lieb zu st\u00e4rken und zu n\u00e4hren.", "tokens": ["die", "flam\u00b7men", "mei\u00b7ner", "lieb", "zu", "st\u00e4r\u00b7ken", "und", "zu", "n\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PPOSAT", "ADJD", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Indem mein weinen nu, indem nu meine brunst", "tokens": ["In\u00b7dem", "mein", "wei\u00b7nen", "nu", ",", "in\u00b7dem", "nu", "mei\u00b7ne", "brunst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "ADV", "$,", "KOUS", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "einander ihre hilf zu wechslen nicht verneinen,", "tokens": ["ein\u00b7an\u00b7der", "ih\u00b7re", "hilf", "zu", "wechs\u00b7len", "nicht", "ver\u00b7nei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "so leid ich dise lieb und lieb das leid umsunst,", "tokens": ["so", "leid", "ich", "di\u00b7se", "lieb", "und", "lieb", "das", "leid", "um\u00b7sunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "ADJD", "KON", "ADJD", "ART", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da findend dan im feur, das ewiglich mu\u00df scheinen,", "tokens": ["Da", "fin\u00b7dend", "dan", "im", "feur", ",", "das", "e\u00b7wig\u00b7lich", "mu\u00df", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "APPRART", "NN", "$,", "PRELS", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "und in dem steten flu\u00df der zehern keine gunst,", "tokens": ["und", "in", "dem", "ste\u00b7ten", "flu\u00df", "der", "ze\u00b7hern", "kei\u00b7ne", "gunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "so mu\u00df (o schmerz!) mein herz stets brennen und stets weinen.", "tokens": ["so", "mu\u00df", "(", "o", "schmerz", "!", ")", "mein", "herz", "stets", "bren\u00b7nen", "und", "stets", "wei\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$(", "FM", "ADJD", "$.", "$(", "PPOSAT", "NN", "ADV", "VVINF", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}