{"textgrid.poem.56724": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: D\u00fcstere Wolke,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "D\u00fcstere Wolke,", "tokens": ["D\u00fcs\u00b7te\u00b7re", "Wol\u00b7ke", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "die du, ein Riesenfalter,", "tokens": ["die", "du", ",", "ein", "Rie\u00b7sen\u00b7fal\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "um der abendrotgl\u00fchenden Berge", "tokens": ["um", "der", "a\u00b7ben\u00b7drot\u00b7gl\u00fc\u00b7hen\u00b7den", "Ber\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "starrende Tannen", "tokens": ["star\u00b7ren\u00b7de", "Tan\u00b7nen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "wie um die Staubf\u00e4den", "tokens": ["wie", "um", "die", "Staub\u00b7f\u00e4\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.6": {"text": "blutiger Lilien schwebst:", "tokens": ["blu\u00b7ti\u00b7ger", "Li\u00b7li\u00b7en", "schwebst", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Dein Dunkel redet", "tokens": ["Dein", "Dun\u00b7kel", "re\u00b7det"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "vom Leid der Welt.", "tokens": ["vom", "Leid", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Welchen Tales Tr\u00e4nen", "tokens": ["Wel\u00b7chen", "Ta\u00b7les", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PWAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "hast du gesogen?", "tokens": ["hast", "du", "ge\u00b7so\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wie viel angstvoller Seufzer", "tokens": ["Wie", "viel", "angst\u00b7vol\u00b7ler", "Seuf\u00b7zer"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "hei\u00dfen Hauch", "tokens": ["hei\u00b7\u00dfen", "Hauch"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "trankst du in dich?", "tokens": ["trankst", "du", "in", "dich", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "D\u00fcstere Wolke,", "tokens": ["D\u00fcs\u00b7te\u00b7re", "Wol\u00b7ke", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "wohin", "tokens": ["wo\u00b7hin"], "token_info": ["word"], "pos": ["PWAV"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "sch\u00fcttest die Z\u00e4hren", "tokens": ["sch\u00fct\u00b7test", "die", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "du wieder aus?", "tokens": ["du", "wie\u00b7der", "aus", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Sch\u00fctte sie doch", "tokens": ["Sch\u00fct\u00b7te", "sie", "doch"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "hinaus in die Ewigkeit!", "tokens": ["hin\u00b7aus", "in", "die", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Denn wenn sie wieder", "tokens": ["Denn", "wenn", "sie", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "zur Erde fallen,", "tokens": ["zur", "Er\u00b7de", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "zeugen sie neue", "tokens": ["zeu\u00b7gen", "sie", "neu\u00b7e"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "aus ihrem Samen.", "tokens": ["aus", "ih\u00b7rem", "Sa\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Nie dann", "tokens": ["Nie", "dann"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "bleiben der Sterblichen", "tokens": ["blei\u00b7ben", "der", "Sterb\u00b7li\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.18": {"text": "Augen trocken.", "tokens": ["Au\u00b7gen", "tro\u00b7cken", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Ach! da wirfst du sie schon", "tokens": ["Ach", "!", "da", "wirfst", "du", "sie", "schon"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "VAFIN", "PPER", "PPER", "ADV"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "in den Abgrund ...", "tokens": ["in", "den", "Ab\u00b7grund", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Arme Erde,", "tokens": ["Ar\u00b7me", "Er\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "immer wieder aufs Neue", "tokens": ["im\u00b7mer", "wie\u00b7der", "aufs", "Neu\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "ADJA"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "getauft", "tokens": ["ge\u00b7tauft"], "token_info": ["word"], "pos": ["VVPP"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "in den eigenen Tr\u00e4nen!", "tokens": ["in", "den", "ei\u00b7ge\u00b7nen", "Tr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}}}}