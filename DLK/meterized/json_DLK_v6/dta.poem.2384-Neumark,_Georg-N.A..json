{"dta.poem.2384": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Weg eitler Amadi\u00df/ und ihr dergleichen Schrifften", "tokens": ["Weg", "eit\u00b7ler", "A\u00b7ma\u00b7di\u00df", "/", "und", "ihr", "derg\u00b7lei\u00b7chen", "Schriff\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "$(", "KON", "PPER", "PIS", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ihr ein keusches Hertz pflegt heimlich zu vergiffte\u0303/", "tokens": ["Die", "ihr", "ein", "keu\u00b7sches", "Hertz", "pflegt", "heim\u00b7lich", "zu", "ver\u00b7gifft\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit eurer Liebligkeit/ weg fchn\u00f6de Schreibersahrt/", "tokens": ["Mit", "eu\u00b7rer", "Lieb\u00b7lig\u00b7keit", "/", "weg", "fchn\u00f6\u00b7de", "Schrei\u00b7ber\u00b7sahrt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die/ wenn man sie nur liest/ ein reines Hertze pahrt", "tokens": ["Die", "/", "wenn", "man", "sie", "nur", "liest", "/", "ein", "rei\u00b7nes", "Hert\u00b7ze", "pahrt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$(", "KOUS", "PIS", "PPER", "ADV", "VVFIN", "$(", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit geiler Liebeslust/ die zwar den Sinn ergetzet/", "tokens": ["Mit", "gei\u00b7ler", "Lie\u00b7bes\u00b7lust", "/", "die", "zwar", "den", "Sinn", "er\u00b7get\u00b7zet", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "PRELS", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch solchen gleich wol auch in leichte Wollust setzet.", "tokens": ["Doch", "sol\u00b7chen", "gleich", "wol", "auch", "in", "leich\u00b7te", "Wol\u00b7lust", "set\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADV", "ADV", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Weg bittrer Honigseim! weg au\u00dfgeschminkter Wust!", "tokens": ["Weg", "bit\u00b7trer", "Ho\u00b7ni\u00b7gseim", "!", "weg", "au\u00df\u00b7ge\u00b7schmink\u00b7ter", "Wust", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$.", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weg stilles Schlangengifft! weg Lokkaa\u00df b\u00f6ser Lust!", "tokens": ["Weg", "stil\u00b7les", "Schlan\u00b7gen\u00b7gifft", "!", "weg", "Lok\u00b7ka\u00b7a\u00df", "b\u00f6\u00b7ser", "Lust", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$.", "ADV", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Kommt hieher/ wer ihr seit/ ihr Gottergebne Sinnen/", "tokens": ["Kommt", "hie\u00b7her", "/", "wer", "ihr", "seit", "/", "ihr", "Got\u00b7ter\u00b7geb\u00b7ne", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "$(", "PWS", "PPER", "APPR", "$(", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die ihr bi\u00dfweilen wol begehret zu gewinnen", "tokens": ["Die", "ihr", "bi\u00df\u00b7wei\u00b7len", "wol", "be\u00b7ge\u00b7hret", "zu", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADV", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Ein rechtes Zeitvertreib/ hier/ hier habt ihr ein Werk/", "tokens": ["Ein", "rech\u00b7tes", "Zeit\u00b7ver\u00b7treib", "/", "hier", "/", "hier", "habt", "ihr", "ein", "Werk", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ADV", "$(", "ADV", "VAFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.12": {"text": "Welchs Sinn und Seel\u2019 ergetzt/ ein Werk von grosser", "tokens": ["Welchs", "Sinn", "und", "Seel'", "er\u00b7getzt", "/", "ein", "Werk", "von", "gros\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN", "VVPP", "$(", "ART", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "St\u00e4rk'/", "tokens": ["St\u00e4rk'", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.14": {"text": "Und unvermerkter Krafft; Ein Buch in dem ihr findet", "tokens": ["Und", "un\u00b7ver\u00b7merk\u00b7ter", "Krafft", ";", "Ein", "Buch", "in", "dem", "ihr", "fin\u00b7det"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$.", "ART", "NN", "APPR", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Das heilge Himmelsfeur so euren Geist entz\u00fcndet", "tokens": ["Das", "heil\u00b7ge", "Him\u00b7mels\u00b7feur", "so", "eu\u00b7ren", "Geist", "ent\u00b7z\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "In reiner Gotteslieb\u2019; Hier ist Ergetzligkeit/", "tokens": ["In", "rei\u00b7ner", "Got\u00b7tes\u00b7lieb'", ";", "Hier", "ist", "Er\u00b7getz\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "ADV", "VAFIN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Hier geht mit Nutzen weg die langverdrosne Zeit.", "tokens": ["Hier", "geht", "mit", "Nut\u00b7zen", "weg", "die", "lang\u00b7ver\u00b7dros\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Hier findet ihr nach Lust verliebte sch\u00f6ne Sachen/", "tokens": ["Hier", "fin\u00b7det", "ihr", "nach", "Lust", "ver\u00b7lieb\u00b7te", "sch\u00f6\u00b7ne", "Sa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die euer keusches Hertz in Gott verliebet machen/", "tokens": ["Die", "eu\u00b7er", "keu\u00b7sches", "Hertz", "in", "Gott", "ver\u00b7lie\u00b7bet", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und nicht in diese Welt. Was lob\u2019 ich aber viel?", "tokens": ["Und", "nicht", "in", "die\u00b7se", "Welt", ".", "Was", "lob'", "ich", "a\u00b7ber", "viel", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "PDAT", "NN", "$.", "PWS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der Seelen Seligkeit ist dieses Buches\nZiehl.", "tokens": ["Der", "See\u00b7len", "Se\u00b7lig\u00b7keit", "ist", "die\u00b7ses", "Bu\u00b7ches", "Ziehl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}