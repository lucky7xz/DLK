{"textgrid.poem.46972": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Kur der Undeutschen", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Zwitterdeutsche, tr\u00e4chtig", "tokens": ["Ihr", "Zwit\u00b7ter\u00b7deut\u00b7sche", ",", "tr\u00e4ch\u00b7tig"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von selbstischen Entw\u00fcrfen;", "tokens": ["Von", "selbs\u00b7ti\u00b7schen", "Ent\u00b7w\u00fcr\u00b7fen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr scheint, dem Arzt verd\u00e4chtig,", "tokens": ["Ihr", "scheint", ",", "dem", "Arzt", "ver\u00b7d\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Kur noch zu bed\u00fcrfen:", "tokens": ["Der", "Kur", "noch", "zu", "be\u00b7d\u00fcr\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was ordnen wir euch nur", "tokens": ["Was", "ord\u00b7nen", "wir", "euch", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Geschwind f\u00fcr eine Kur?", "tokens": ["Ge\u00b7schwind", "f\u00fcr", "ei\u00b7ne", "Kur", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich seh's an b\u00f6sen Flecken:", "tokens": ["Ich", "seh's", "an", "b\u00f6\u00b7sen", "Fle\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es sind des Franztums Seuchen,", "tokens": ["Es", "sind", "des", "Franz\u00b7tums", "Seu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die noch im Blut euch stecken.", "tokens": ["Die", "noch", "im", "Blut", "euch", "ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn ihr sie wollt verscheuchen,", "tokens": ["Wenn", "ihr", "sie", "wollt", "ver\u00b7scheu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So braucht zu eurer Kur", "tokens": ["So", "braucht", "zu", "eu\u00b7rer", "Kur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Den rheinischen Merkur.", "tokens": ["Den", "rhei\u00b7ni\u00b7schen", "Mer\u00b7kur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}