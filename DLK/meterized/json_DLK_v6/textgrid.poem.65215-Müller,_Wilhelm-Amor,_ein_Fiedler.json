{"textgrid.poem.65215": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Amor, ein Fiedler", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Amor lernt die Fiedel spielen", "tokens": ["A\u00b7mor", "lernt", "die", "Fie\u00b7del", "spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei dem Gott der Musikanten,", "tokens": ["Bei", "dem", "Gott", "der", "Mu\u00b7si\u00b7kan\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zu diesem Pfingstgelage", "tokens": ["Und", "zu", "die\u00b7sem", "Pfingst\u00b7ge\u00b7la\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will er vor dem Thor der Schenke", "tokens": ["Will", "er", "vor", "dem", "Thor", "der", "Schen\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unter gr\u00fcnem Maienschatten", "tokens": ["Un\u00b7ter", "gr\u00fc\u00b7nem", "Mai\u00b7en\u00b7schat\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich bei uns zum ersten Male", "tokens": ["Sich", "bei", "uns", "zum", "ers\u00b7ten", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Unentgeltlich h\u00f6ren lassen.", "tokens": ["Un\u00b7ent\u00b7gelt\u00b7lich", "h\u00f6\u00b7ren", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kommt, ihr Bursche! Kommt, ihr M\u00e4dchen!", "tokens": ["Kommt", ",", "ihr", "Bur\u00b7sche", "!", "Kommt", ",", "ihr", "M\u00e4d\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$.", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Kommt und tanzt nach seiner Fiedel!", "tokens": ["Kommt", "und", "tanzt", "nach", "sei\u00b7ner", "Fie\u00b7del", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und sie tanzen und sie springen,", "tokens": ["Und", "sie", "tan\u00b7zen", "und", "sie", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "KON", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und die F\u00fc\u00dfe mit den Herzen", "tokens": ["Und", "die", "F\u00fc\u00b7\u00dfe", "mit", "den", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Heben sich in gleichen Takte", "tokens": ["He\u00b7ben", "sich", "in", "glei\u00b7chen", "Tak\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Nach dem Striche seines Bogens.", "tokens": ["Nach", "dem", "Stri\u00b7che", "sei\u00b7nes", "Bo\u00b7gens", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Schneller, schneller, kleiner Fiedler!", "tokens": ["Schnel\u00b7ler", ",", "schnel\u00b7ler", ",", "klei\u00b7ner", "Fied\u00b7ler", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Und er fiedelt nach Verlangen,", "tokens": ["Und", "er", "fie\u00b7delt", "nach", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Da\u00df die Kr\u00e4nze, Str\u00e4u\u00dfe, Flechten,", "tokens": ["Da\u00df", "die", "Kr\u00e4n\u00b7ze", ",", "Str\u00e4u\u00b7\u00dfe", ",", "Flech\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "B\u00e4nder, Sch\u00fcrzen, R\u00f6cke fliegen,", "tokens": ["B\u00e4n\u00b7der", ",", "Sch\u00fcr\u00b7zen", ",", "R\u00f6\u00b7cke", "flie\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Und die T\u00e4nzer enger fassen", "tokens": ["Und", "die", "T\u00e4n\u00b7zer", "en\u00b7ger", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Ihre leichten T\u00e4nzerinnen.", "tokens": ["Ih\u00b7re", "leich\u00b7ten", "T\u00e4n\u00b7ze\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Ei, und dennoch sind so viele", "tokens": ["Ei", ",", "und", "den\u00b7noch", "sind", "so", "vie\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "ADV", "VAFIN", "ADV", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Ausgeglitten, fehlgetreten,", "tokens": ["Aus\u00b7ge\u00b7glit\u00b7ten", ",", "fehl\u00b7ge\u00b7tre\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Gar gestolpert und gefallen", "tokens": ["Gar", "ge\u00b7stol\u00b7pert", "und", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVPP", "KON", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Auf dem glatten Rasenplane!", "tokens": ["Auf", "dem", "glat\u00b7ten", "Ra\u00b7sen\u00b7pla\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Aber, Dank dem weichen Grase,", "tokens": ["A\u00b7ber", ",", "Dank", "dem", "wei\u00b7chen", "Gra\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Weh gethan hat sich nicht Eine.", "tokens": ["Weh", "ge\u00b7than", "hat", "sich", "nicht", "Ei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "PRF", "PTKNEG", "ART", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}