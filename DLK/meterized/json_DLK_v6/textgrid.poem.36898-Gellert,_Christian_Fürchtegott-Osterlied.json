{"textgrid.poem.36898": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Osterlied", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbfreiwillig hab ich's dargebracht,", "tokens": ["\u00bb", "frei\u00b7wil\u00b7lig", "hab", "ich's", "dar\u00b7ge\u00b7bracht", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und niemand nimmt mein Leben.", "tokens": ["Und", "nie\u00b7mand", "nimmt", "mein", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es selbst zu lassen, hab ich Macht,", "tokens": ["Es", "selbst", "zu", "las\u00b7sen", ",", "hab", "ich", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "$,", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Macht, wieder mir's zu geben.", "tokens": ["Macht", ",", "wie\u00b7der", "mir's", "zu", "ge\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und darum liebt mein Vater mich,", "tokens": ["Und", "da\u00b7rum", "liebt", "mein", "Va\u00b7ter", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPOSAT", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich mein Leben la\u00df, und ich", "tokens": ["Da\u00df", "ich", "mein", "Le\u00b7ben", "la\u00df", ",", "und", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,", "KON", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fcr meine Feind es lasse.", "tokens": ["F\u00fcr", "mei\u00b7ne", "Feind", "es", "las\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich bin in meiner Niedrigkeit", "tokens": ["Ich", "bin", "in", "mei\u00b7ner", "Nied\u00b7rig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein \u00c4rgernis auf Erden;", "tokens": ["Ein", "\u00c4r\u00b7ger\u00b7nis", "auf", "Er\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verschm\u00e4ht, gegei\u00dfelt und verspeit,", "tokens": ["Ver\u00b7schm\u00e4ht", ",", "ge\u00b7gei\u00b7\u00dfelt", "und", "ver\u00b7speit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gekreuzigt werd ich werden.", "tokens": ["Ge\u00b7kreu\u00b7zigt", "werd", "ich", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wenn alles dies vollendet ist:", "tokens": ["Wenn", "al\u00b7les", "dies", "voll\u00b7en\u00b7det", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PDS", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So wird des Menschen Sohn, der Christ,", "tokens": ["So", "wird", "des", "Men\u00b7schen", "Sohn", ",", "der", "Christ", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht die Verwesung sehen.", "tokens": ["Nicht", "die", "Ver\u00b7we\u00b7sung", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Weil er sich selbst erniedrigt hat:", "tokens": ["Weil", "er", "sich", "selbst", "er\u00b7nied\u00b7rigt", "hat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wird ihn Gott erh\u00f6hen.", "tokens": ["So", "wird", "ihn", "Gott", "er\u00b7h\u00f6\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich leid und sterb an eurer Statt,", "tokens": ["Ich", "leid", "und", "sterb", "an", "eu\u00b7rer", "Statt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann werd ich auferstehen.", "tokens": ["Dann", "werd", "ich", "auf\u00b7er\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Am dritten Tag geh ich heraus,", "tokens": ["Am", "drit\u00b7ten", "Tag", "geh", "ich", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "L\u00f6sch alle Schmach des Kreuzes aus.", "tokens": ["L\u00f6sch", "al\u00b7le", "Schmach", "des", "Kreu\u00b7zes", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als Gottes Sohn bewiesen.", "tokens": ["Als", "Got\u00b7tes", "Sohn", "be\u00b7wie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich will euch sehn, erfreuet euch,", "tokens": ["Ich", "will", "euch", "sehn", ",", "er\u00b7freu\u00b7et", "euch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Euch siegreich wiedersehen;", "tokens": ["Euch", "sieg\u00b7reich", "wie\u00b7der\u00b7se\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Euch lehren, meines Vaters Reich", "tokens": ["Euch", "leh\u00b7ren", ",", "mei\u00b7nes", "Va\u00b7ters", "Reich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVINF", "$,", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hohen Rat verstehen;", "tokens": ["Und", "ho\u00b7hen", "Rat", "ver\u00b7ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Euch den verhei\u00dfnen Geist verleihn;", "tokens": ["Euch", "den", "ver\u00b7hei\u00df\u00b7nen", "Geist", "ver\u00b7leihn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und ihr sollt meine Zeugen sein,", "tokens": ["Und", "ihr", "sollt", "mei\u00b7ne", "Zeu\u00b7gen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df ich vom Tod erstanden.", "tokens": ["Da\u00df", "ich", "vom", "Tod", "er\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Geht hin und lehret alle Welt;", "tokens": ["Geht", "hin", "und", "leh\u00b7ret", "al\u00b7le", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bin des Weibes Samen,", "tokens": ["Ich", "bin", "des", "Wei\u00b7bes", "Sa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Samen Abrahams, der Held;", "tokens": ["Der", "Sa\u00b7men", "Ab\u00b7ra\u00b7hams", ",", "der", "Held", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und tauft in meinem Namen.", "tokens": ["Und", "tauft", "in", "mei\u00b7nem", "Na\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wer an Gott gl\u00e4ubt, gl\u00e4ubt auch an mich.", "tokens": ["Wer", "an", "Gott", "gl\u00e4ubt", ",", "gl\u00e4ubt", "auch", "an", "mich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VVFIN", "$,", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Tut Wunder, und beweist, da\u00df ich", "tokens": ["Tut", "Wun\u00b7der", ",", "und", "be\u00b7weist", ",", "da\u00df", "ich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$,", "KON", "VVFIN", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zur Rechten Gottes sitze.", "tokens": ["Zur", "Rech\u00b7ten", "Got\u00b7tes", "sit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "K\u00e4mpft f\u00fcr mein Evangelium,", "tokens": ["K\u00e4mpft", "f\u00fcr", "mein", "E\u00b7van\u00b7ge\u00b7li\u00b7um", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Und freuet euch der Leiden.", "tokens": ["Und", "freu\u00b7et", "euch", "der", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kein Engel und kein F\u00fcrstentum,", "tokens": ["Kein", "En\u00b7gel", "und", "kein", "F\u00fcrs\u00b7ten\u00b7tum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nichts soll euch von mir scheiden.", "tokens": ["Nichts", "soll", "euch", "von", "mir", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Man wird euch hassen, und euch schm\u00e4hn,", "tokens": ["Man", "wird", "euch", "has\u00b7sen", ",", "und", "euch", "schm\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "VVINF", "$,", "KON", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Euch t\u00f6ten; dennoch soll's geschehn,", "tokens": ["Euch", "t\u00f6\u00b7ten", ";", "den\u00b7noch", "soll's", "ge\u00b7schehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$.", "ADV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df eure Lehre sieget.\u00ab", "tokens": ["Da\u00df", "eu\u00b7re", "Leh\u00b7re", "sie\u00b7get", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Herr, unser Heil! sie hat gesiegt,", "tokens": ["Herr", ",", "un\u00b7ser", "Heil", "!", "sie", "hat", "ge\u00b7siegt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und siegt in allen Landen,", "tokens": ["Und", "siegt", "in", "al\u00b7len", "Lan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und zeuget, da\u00df dein Wort nicht tr\u00fcgt,", "tokens": ["Und", "zeu\u00b7get", ",", "da\u00df", "dein", "Wort", "nicht", "tr\u00fcgt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und zeugt, du bist erstanden.", "tokens": ["Und", "zeugt", ",", "du", "bist", "er\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dein Kreuz, an das man dich erh\u00f6ht,", "tokens": ["Dein", "Kreuz", ",", "an", "das", "man", "dich", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PRELS", "PIS", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verwandelt sich in Majest\u00e4t;", "tokens": ["Ver\u00b7wan\u00b7delt", "sich", "in", "Ma\u00b7jes\u00b7t\u00e4t", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du gehst aus deinem Grabe.", "tokens": ["Du", "gehst", "aus", "dei\u00b7nem", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Geha\u00dft in deiner Niedrigkeit,", "tokens": ["Ge\u00b7ha\u00dft", "in", "dei\u00b7ner", "Nied\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Warst du ein Ziel des Spottes,", "tokens": ["Warst", "du", "ein", "Ziel", "des", "Spot\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und zeigtest doch zu gleicher Zeit", "tokens": ["Und", "zeig\u00b7test", "doch", "zu", "glei\u00b7cher", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An dir die Hoheit Gottes.", "tokens": ["An", "dir", "die", "Ho\u00b7heit", "Got\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dein Kreuz schien zwar der Welt ein Greul;", "tokens": ["Dein", "Kreuz", "schien", "zwar", "der", "Welt", "ein", "Greul", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch sterben f\u00fcr der Feinde Heil,", "tokens": ["Doch", "ster\u00b7ben", "f\u00fcr", "der", "Fein\u00b7de", "Heil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dies ist die h\u00f6chste Tugend.", "tokens": ["Dies", "ist", "die", "h\u00f6chs\u00b7te", "Tu\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Dein Reich war nicht von dieser Welt,", "tokens": ["Dein", "Reich", "war", "nicht", 