{"dta.poem.15606": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "39.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Denk nicht, da\u00df Gott die Welt lie\u00df eine Zeitlang laufen,", "tokens": ["Denk", "nicht", ",", "da\u00df", "Gott", "die", "Welt", "lie\u00df", "ei\u00b7ne", "Zeit\u00b7lang", "lau\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "$,", "KOUS", "NN", "ART", "NN", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um sich im Irrthum auszutoben, auszuschnaufen,", "tokens": ["Um", "sich", "im", "Irr\u00b7thum", "aus\u00b7zu\u00b7to\u00b7ben", ",", "aus\u00b7zu\u00b7schnau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUI", "PRF", "APPRART", "NN", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und dann erst sei hervor getreten auf einmal,", "tokens": ["Und", "dann", "erst", "sei", "her\u00b7vor", "ge\u00b7tre\u00b7ten", "auf", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VAFIN", "ADV", "VVPP", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu f\u00fchren sie hinfort nach seiner Gnadenwahl.", "tokens": ["Zu", "f\u00fch\u00b7ren", "sie", "hin\u00b7fort", "nach", "sei\u00b7ner", "Gna\u00b7den\u00b7wahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Entweder hat er sie von Anfang m\u00fcssen leiten,", "tokens": ["Ent\u00b7we\u00b7der", "hat", "er", "sie", "von", "An\u00b7fang", "m\u00fcs\u00b7sen", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Oder sie wird noch jetzt auf eignen F\u00fc\u00dfen schreiten.", "tokens": ["O\u00b7der", "sie", "wird", "noch", "jetzt", "auf", "eig\u00b7nen", "F\u00fc\u00b7\u00dfen", "schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und beides dis ist eins; die Welt geht ihren Gang,", "tokens": ["Und", "bei\u00b7des", "dis", "ist", "eins", ";", "die", "Welt", "geht", "ih\u00b7ren", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PDS", "VAFIN", "PIS", "$.", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und da\u00df sie jemals Gott' entgeh', ist mir nicht bang.", "tokens": ["Und", "da\u00df", "sie", "je\u00b7mals", "Gott'", "ent\u00b7geh'", ",", "ist", "mir", "nicht", "bang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "NN", "PTKVZ", "$,", "VAFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}