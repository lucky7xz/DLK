{"textgrid.poem.63183": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Patroklos", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Antilochos flog in das Zelt,", "tokens": ["An\u00b7ti\u00b7lo\u00b7chos", "flog", "in", "das", "Zelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wo der Pelide sass und mit den Sch\u00e4deln", "tokens": ["Wo", "der", "Pe\u00b7li\u00b7de", "sass", "und", "mit", "den", "Sch\u00e4\u00b7deln"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "KON", "APPR", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Der toten Feinde Bocca spielte.", "tokens": ["Der", "to\u00b7ten", "Fein\u00b7de", "Boc\u00b7ca", "spiel\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er warf die Sch\u00e4del in die Ecke", "tokens": ["Er", "warf", "die", "Sch\u00e4\u00b7del", "in", "die", "E\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und warf sich auf sein Lager", "tokens": ["Und", "warf", "sich", "auf", "sein", "La\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von Wirbelknochen Rippen", "tokens": ["Von", "Wir\u00b7bel\u00b7kno\u00b7chen", "Rip\u00b7pen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wie Heu und Streu vor ihn gesch\u00fcttet.", "tokens": ["Wie", "Heu", "und", "Streu", "vor", "ihn", "ge\u00b7sch\u00fct\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Antilochos erhob die Stimme", "tokens": ["An\u00b7ti\u00b7lo\u00b7chos", "er\u00b7hob", "die", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "+----+-+-", "measure": "dactylic.init"}, "line.9": {"text": "Zu einem Schrei.", "tokens": ["Zu", "ei\u00b7nem", "Schrei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Der brach in Scherben,", "tokens": ["Der", "brach", "in", "Scher\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Und die klirrten:", "tokens": ["Und", "die", "klirr\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Unseliger!", "tokens": ["Un\u00b7se\u00b7li\u00b7ger", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+---", "measure": "dactylic.init"}, "line.13": {"text": "Patroklos ist nicht mehr!", "tokens": ["Pat\u00b7ro\u00b7klos", "ist", "nicht", "mehr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und der Pelide stiess den Kopf", "tokens": ["Und", "der", "Pe\u00b7li\u00b7de", "stiess", "den", "Kopf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "Dem Geier gleich ins Licht,", "tokens": ["Dem", "Gei\u00b7er", "gleich", "ins", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und alles Blut und Fleisch", "tokens": ["Und", "al\u00b7les", "Blut", "und", "Fleisch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Schien draus gewichen.", "tokens": ["Schien", "draus", "ge\u00b7wi\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "So sass er,", "tokens": ["So", "sass", "er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Selber ein Skelett,", "tokens": ["Sel\u00b7ber", "ein", "Ske\u00b7lett", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Bis dass die schwesterliche D\u00e4mmerung kam", "tokens": ["Bis", "dass", "die", "schwes\u00b7ter\u00b7li\u00b7che", "D\u00e4m\u00b7me\u00b7rung", "kam"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Und auch der milde Bruder Mond.", "tokens": ["Und", "auch", "der", "mil\u00b7de", "Bru\u00b7der", "Mond", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da fiel er in den Staub", "tokens": ["Da", "fiel", "er", "in", "den", "Staub"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und schlug den Kiefer in die Erde wie der Eber,", "tokens": ["Und", "schlug", "den", "Kie\u00b7fer", "in", "die", "Er\u00b7de", "wie", "der", "E\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Tr\u00fcffeln sucht.", "tokens": ["Der", "Tr\u00fcf\u00b7feln", "sucht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Dann stand er auf", "tokens": ["Dann", "stand", "er", "auf"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Und waffenlos", "tokens": ["Und", "waf\u00b7fen\u00b7los"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Schritt er im Mond durch die trojansche Ebne.", "tokens": ["Schritt", "er", "im", "Mond", "durch", "die", "tro\u00b7jan\u00b7sche", "Eb\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Es wichen", "tokens": ["Es", "wi\u00b7chen"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Entsetzt die W\u00e4chter, die die Bahre bargen.", "tokens": ["Ent\u00b7setzt", "die", "W\u00e4ch\u00b7ter", ",", "die", "die", "Bah\u00b7re", "bar\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er trat hinzu", "tokens": ["Er", "trat", "hin\u00b7zu"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und nahm den Leichnam", "tokens": ["Und", "nahm", "den", "Leich\u00b7nam"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Und trug ihn wie der J\u00e4ger", "tokens": ["Und", "trug", "ihn", "wie", "der", "J\u00e4\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "KOKOM", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein Kitz tr\u00e4gt,", "tokens": ["Ein", "Kitz", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Warf ihn aufs Lager", "tokens": ["Warf", "ihn", "aufs", "La\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN"], "meter": "++-+-", "measure": "iambic.di"}, "line.8": {"text": "Schlief die Nacht mit ihm,", "tokens": ["Schlief", "die", "Nacht", "mit", "ihm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Sein Haupt", "tokens": ["Sein", "Haupt"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "Von toter Locken schwarzer Flut getrieben.", "tokens": ["Von", "to\u00b7ter", "Lo\u00b7cken", "schwar\u00b7zer", "Flut", "ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}