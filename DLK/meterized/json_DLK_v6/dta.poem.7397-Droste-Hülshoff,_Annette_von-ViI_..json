{"dta.poem.7397": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "ViI .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Am Wiesenhang 'ne Linde steht, so lieblich winkend mit den", "tokens": ["Am", "Wie\u00b7sen\u00b7hang", "'ne", "Lin\u00b7de", "steht", ",", "so", "lieb\u00b7lich", "win\u00b7kend", "mit", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NE", "VVFIN", "$,", "ADV", "ADJD", "VVPP", "APPR", "ART"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Zweigen,", "tokens": ["Zwei\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Auf jedem Ast ein Vogelnest, um jede Bl\u00fcth' ein Bienen-", "tokens": ["Auf", "je\u00b7dem", "Ast", "ein", "Vo\u00b7gel\u00b7nest", ",", "um", "je\u00b7de", "Bl\u00fcth'", "ein", "Bie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "$,", "KOUI", "PIAT", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "reigen,", "tokens": ["rei\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Sie scheint den d\u00fcstern F\u00f6hrenwald aus ihren Kelchen an-", "tokens": ["Sie", "scheint", "den", "d\u00fcs\u00b7tern", "F\u00f6h\u00b7ren\u00b7wald", "aus", "ih\u00b7ren", "Kel\u00b7chen", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "zul\u00e4cheln,", "tokens": ["zu\u00b7l\u00e4\u00b7cheln", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Des nahen St\u00e4dtleins Angelus ein s\u00e4uselnd Ave zuzuf\u00e4cheln,", "tokens": ["Des", "na\u00b7hen", "St\u00e4dt\u00b7leins", "An\u00b7ge\u00b7lus", "ein", "s\u00e4u\u00b7selnd", "A\u00b7ve", "zu\u00b7zu\u00b7f\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "ART", "ADJD", "NE", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.8": {"text": "Und f\u00fcr den nahen Friedhof auch", "tokens": ["Und", "f\u00fcr", "den", "na\u00b7hen", "Fried\u00b7hof", "auch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Hat sie vers\u00fc\u00dft des Westes Hauch.", "tokens": ["Hat", "sie", "ver\u00b7s\u00fc\u00dft", "des", "Wes\u00b7tes", "Hauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und Blatt an Blatt vom Bl\u00fcthenzweig verstreut sie auf des", "tokens": ["Und", "Blatt", "an", "Blatt", "vom", "Bl\u00fc\u00b7then\u00b7zweig", "ver\u00b7streut", "sie", "auf", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NN", "APPRART", "NN", "VVFIN", "PPER", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Greises Stirne,", "tokens": ["Grei\u00b7ses", "Stir\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Der in dem Wurzelmoose lehnt sein Haupt mit siedendem", "tokens": ["Der", "in", "dem", "Wur\u00b7zel\u00b7moo\u00b7se", "lehnt", "sein", "Haupt", "mit", "sie\u00b7den\u00b7dem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Gehirne;", "tokens": ["Ge\u00b7hir\u00b7ne", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Zur Seite liegt der Stab, gef\u00fcllt mit Bettelbrode liegt der", "tokens": ["Zur", "Sei\u00b7te", "liegt", "der", "Stab", ",", "ge\u00b7f\u00fcllt", "mit", "Bet\u00b7tel\u00b7bro\u00b7de", "liegt", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "VVPP", "APPR", "NN", "VVFIN", "ART"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Ranzen,", "tokens": ["Ran\u00b7zen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Und Schemen hier und Schemen dort mit Elfenschritten", "tokens": ["Und", "Sche\u00b7men", "hier", "und", "Sche\u00b7men", "dort", "mit", "El\u00b7fen\u00b7schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "KON", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "dr\u00fcber tanzen,", "tokens": ["dr\u00fc\u00b7ber", "tan\u00b7zen", ","], "token_info": ["word", "word", "punct"], "pos": ["PAV", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Wie sie der Brust geheimster Hut", "tokens": ["Wie", "sie", "der", "Brust", "ge\u00b7heims\u00b7ter", "Hut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Entschl\u00fcpfen in des Fiebers Glut.", "tokens": ["Ent\u00b7schl\u00fcp\u00b7fen", "in", "des", "Fie\u00b7bers", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Den Anger seiner Kindheit sieht er in den Lindenzweigen", "tokens": ["Den", "An\u00b7ger", "sei\u00b7ner", "Kind\u00b7heit", "sieht", "er", "in", "den", "Lin\u00b7den\u00b7zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "spielen,", "tokens": ["spie\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Die s\u00fc\u00dfe Heimat, und das Haupt der Eltern auf den Sterbe-", "tokens": ["Die", "s\u00fc\u00b7\u00dfe", "Hei\u00b7mat", ",", "und", "das", "Haupt", "der", "El\u00b7tern", "auf", "den", "Ster\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ART", "NN", "ART", "NN", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "pf\u00fchlen;", "tokens": ["pf\u00fch\u00b7len", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Was er verloren und erstrebt, was er ges\u00fcndet und getragen,", "tokens": ["Was", "er", "ver\u00b7lo\u00b7ren", "und", "er\u00b7strebt", ",", "was", "er", "ge\u00b7s\u00fcn\u00b7det", "und", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "KON", "VVFIN", "$,", "PWS", "PPER", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.6": {"text": "Wie Eine Nacht sein Haar gebleicht, die eignen Knechte ihn", "tokens": ["Wie", "Ei\u00b7ne", "Nacht", "sein", "Haar", "ge\u00b7bleicht", ",", "die", "eig\u00b7nen", "Knech\u00b7te", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,", "ART", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "geschlagen.", "tokens": ["ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "O Nacht, die Ehre, Kr\u00e4fte, Hab'", "tokens": ["O", "Nacht", ",", "die", "Eh\u00b7re", ",", "Kr\u00e4f\u00b7te", ",", "Hab'"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["NE", "NN", "$,", "ART", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zerbrach und ihm die Seele gab!", "tokens": ["Zer\u00b7brach", "und", "ihm", "die", "See\u00b7le", "gab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er sieht sein faltiges Gesicht im Wasserspiegel widerscheinen", "tokens": ["Er", "sieht", "sein", "fal\u00b7ti\u00b7ges", "Ge\u00b7sicht", "im", "Was\u00b7ser\u00b7spie\u00b7gel", "wi\u00b7der\u00b7schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Wie er sich selber nicht erkannt, und kindisch dann begann", "tokens": ["Wie", "er", "sich", "sel\u00b7ber", "nicht", "er\u00b7kannt", ",", "und", "kin\u00b7disch", "dann", "be\u00b7gann"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "ADV", "PTKNEG", "VVPP", "$,", "KON", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "zu weinen;", "tokens": ["zu", "wei\u00b7nen", ";"], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Ach, all die Thr\u00e4nen, so nachher aus tiefrer Quelle sind", "tokens": ["Ach", ",", "all", "die", "Thr\u00e4\u00b7nen", ",", "so", "nach\u00b7her", "aus", "tief\u00b7rer", "Quel\u00b7le", "sind"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PIAT", "ART", "NN", "$,", "ADV", "ADV", "APPR", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "geflossen,", "tokens": ["ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "Ob sie ihn Christi Blut vereint? des Himmels Pforten auf-", "tokens": ["Ob", "sie", "ihn", "Chris\u00b7ti", "Blut", "ver\u00b7eint", "?", "des", "Him\u00b7mels", "Pfor\u00b7ten", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "NE", "NN", "VVPP", "$.", "ART", "NN", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "geschlossen?", "tokens": ["ge\u00b7schlos\u00b7sen", "?"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Wohl Schweres trug er mit Geduld,", "tokens": ["Wohl", "Schwe\u00b7res", "trug", "er", "mit", "Ge\u00b7duld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch willenlos, durch eigne Schuld!", "tokens": ["Doch", "wil\u00b7len\u00b7los", ",", "durch", "eig\u00b7ne", "Schuld", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Mit vierzig Jahren siecher Greis, ist er von Land zu Land", "tokens": ["Mit", "vier\u00b7zig", "Jah\u00b7ren", "sie\u00b7cher", "Greis", ",", "ist", "er", "von", "Land", "zu", "Land"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "ADJA", "NN", "$,", "VAFIN", "PPER", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "geschlichen,", "tokens": ["ge\u00b7schli\u00b7chen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Hat seines Namens Fluch geh\u00f6rt und ist zur Seite scheu", "tokens": ["Hat", "sei\u00b7nes", "Na\u00b7mens", "Fluch", "ge\u00b7h\u00f6rt", "und", "ist", "zur", "Sei\u00b7te", "scheu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "VVFIN", "KON", "VAFIN", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "gewichen,", "tokens": ["ge\u00b7wi\u00b7chen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Aus mancher Hand, die ihm gedient, hat er das Bettelbrod", "tokens": ["Aus", "man\u00b7cher", "Hand", ",", "die", "ihm", "ge\u00b7dient", ",", "hat", "er", "das", "Bet\u00b7tel\u00b7brod"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "gebrochen,", "tokens": ["ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Und ist, ein todeskranker Mann, an dieses H\u00fcgels Bug ge-", "tokens": ["Und", "ist", ",", "ein", "to\u00b7des\u00b7kran\u00b7ker", "Mann", ",", "an", "die\u00b7ses", "H\u00fc\u00b7gels", "Bug", "ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "$,", "ART", "ADJA", "NN", "$,", "APPR", "PDAT", "NN", "NE", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "krochen,", "tokens": ["kro\u00b7chen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "An diesen H\u00fcgel \u2014 ew'ge Macht!", "tokens": ["An", "die\u00b7sen", "H\u00fc\u00b7gel", "ew'\u00b7ge", "Macht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Er schaudert auf; \u2014 Sylvesternacht!", "tokens": ["Er", "schau\u00b7dert", "auf", ";", "Syl\u00b7ves\u00b7ter\u00b7nacht", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "$(", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Der F\u00f6hrenwald \u2014 das \u00f6de Haus \u2014 dort stand der Priester,", "tokens": ["Der", "F\u00f6h\u00b7ren\u00b7wald", "das", "\u00f6\u00b7de", "Haus", "dort", "stand", "der", "Pries\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "NN", "$(", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "dort am Hagen \u2014", "tokens": ["dort", "am", "Ha\u00b7gen"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "O, in der Sterbestunde hat sein irrer Fu\u00df ihn hergetragen,", "tokens": ["O", ",", "in", "der", "Ster\u00b7be\u00b7stun\u00b7de", "hat", "sein", "ir\u00b7rer", "Fu\u00df", "ihn", "her\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ART", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.4": {"text": "Das ist kein Schemen, dieses nicht; dort streckt Sankt Michael", "tokens": ["Das", "ist", "kein", "Sche\u00b7men", ",", "die\u00b7ses", "nicht", ";", "dort", "streckt", "Sankt", "Mic\u00b7hael"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "$,", "PDS", "PTKNEG", "$.", "ADV", "VVFIN", "VVFIN", "NE"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "die Fl\u00fcgel,", "tokens": ["die", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Dort kreucht am Fu\u00dfgestell der Drach' un