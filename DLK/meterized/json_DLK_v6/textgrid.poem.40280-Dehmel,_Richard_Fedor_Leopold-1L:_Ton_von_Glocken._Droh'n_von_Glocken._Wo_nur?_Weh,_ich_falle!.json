{"textgrid.poem.40280": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ton von Glocken. Droh'n von Glocken. Wo nur? Weh, ich falle!", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ton von Glocken. Droh'n von Glocken. Wo nur? Weh, ich falle!", "tokens": ["Ton", "von", "Glo\u00b7cken", ".", "Droh'n", "von", "Glo\u00b7cken", ".", "Wo", "nur", "?", "Weh", ",", "ich", "fal\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$.", "NN", "APPR", "NN", "$.", "PWAV", "ADV", "$.", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Wohin wollten doch die stummen, grauen M\u00f6nche alle? \u2013", "tokens": ["Wo\u00b7hin", "woll\u00b7ten", "doch", "die", "stum\u00b7men", ",", "grau\u00b7en", "M\u00f6n\u00b7che", "al\u00b7le", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "ART", "ADJA", "$,", "ADJA", "NN", "PIS", "$.", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Um mein dunkles Gitter seh ich Nachtgespenster jagen,", "tokens": ["Um", "mein", "dunk\u00b7les", "Git\u00b7ter", "seh", "ich", "Nacht\u00b7ge\u00b7spens\u00b7ter", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "und da vor mir, nur zwei Schritte, rollt ein schwarzer Wagen.", "tokens": ["und", "da", "vor", "mir", ",", "nur", "zwei", "Schrit\u00b7te", ",", "rollt", "ein", "schwar\u00b7zer", "Wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "$,", "ADV", "CARD", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "Ringsum glimmt die Luft von Kreuzen, und die Fackeln bluten,", "tokens": ["Ring\u00b7sum", "glimmt", "die", "Luft", "von", "Kreu\u00b7zen", ",", "und", "die", "Fa\u00b7ckeln", "blu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$,", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "und man f\u00fchrt mich an den Armen \u2013 ach dies weiche Fluten!", "tokens": ["und", "man", "f\u00fchrt", "mich", "an", "den", "Ar\u00b7men", "\u2013", "ach", "dies", "wei\u00b7che", "Flu\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "$(", "XY", "PDS", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "Von alleine gehen meine qualgel\u00e4hmten Beine,", "tokens": ["Von", "al\u00b7lei\u00b7ne", "ge\u00b7hen", "mei\u00b7ne", "qual\u00b7ge\u00b7l\u00e4hm\u00b7ten", "Bei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.8": {"text": "ach so sch\u00f6n gef\u00fchrt; ich kenne keine Stra\u00dfe, keine;", "tokens": ["ach", "so", "sch\u00f6n", "ge\u00b7f\u00fchrt", ";", "ich", "ken\u00b7ne", "kei\u00b7ne", "Stra\u00b7\u00dfe", ",", "kei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVPP", "$.", "PPER", "VVFIN", "PIAT", "NN", "$,", "PIAT", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.9": {"text": "gehe flutend wie im Traume, ohne Sinn und Willen,", "tokens": ["ge\u00b7he", "flu\u00b7tend", "wie", "im", "Trau\u00b7me", ",", "oh\u00b7ne", "Sinn", "und", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KOKOM", "APPRART", "NN", "$,", "KOUI", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.10": {"text": "nur im Kopfe, nur im Herzen f\u00fchl'ich etwas w\u00fchlen.", "tokens": ["nur", "im", "Kop\u00b7fe", ",", "nur", "im", "Her\u00b7zen", "f\u00fchl'\u00b7ich", "et\u00b7was", "w\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "ADV", "APPRART", "NN", "VVFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.11": {"text": "Etwas pr\u00fcft da seine Krallen, scharfe, krumme Krallen,", "tokens": ["Et\u00b7was", "pr\u00fcft", "da", "sei\u00b7ne", "Kral\u00b7len", ",", "schar\u00b7fe", ",", "krum\u00b7me", "Kral\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.12": {"text": "und die Raben klagen dr\u00fcben, und die Glocken hallen.", "tokens": ["und", "die", "Ra\u00b7ben", "kla\u00b7gen", "dr\u00fc\u00b7ben", ",", "und", "die", "Glo\u00b7cken", "hal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "$,", "KON", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.13": {"text": "Ach, ich h\u00f6re ferne Ch\u00f6re \u2013 ei so lieb, so liebe;", "tokens": ["Ach", ",", "ich", "h\u00f6\u00b7re", "fer\u00b7ne", "Ch\u00f6\u00b7re", "\u2013", "ei", "so", "lieb", ",", "so", "lie\u00b7be", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ADJA", "NN", "$(", "ITJ", "ADV", "ADJD", "$,", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.14": {"text": "nur in meinen Augen brennt was, oh so tr\u00fcb, so tr\u00fcbe.", "tokens": ["nur", "in", "mei\u00b7nen", "Au\u00b7gen", "brennt", "was", ",", "oh", "so", "tr\u00fcb", ",", "so", "tr\u00fc\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "PIS", "$,", "KOUI", "ADV", "ADJD", "$,", "ADV", "ADJA", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.15": {"text": "Und es rieselt etwas Kaltes \u00fcber meine Mienen,", "tokens": ["Und", "es", "rie\u00b7selt", "et\u00b7was", "Kal\u00b7tes", "\u00fc\u00b7ber", "mei\u00b7ne", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "ADJA", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.16": {"text": "alle Menschen stieren her, und \u2013 Keiner naht von ihnen;", "tokens": ["al\u00b7le", "Men\u00b7schen", "stie\u00b7ren", "her", ",", "und", "\u2013", "Kei\u00b7ner", "naht", "von", "ih\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$,", "KON", "$(", "PIS", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.17": {"text": "etwas mu\u00df in diesen Mienen herrschen, da\u00df sie graut \u2013 ja!", "tokens": ["et\u00b7was", "mu\u00df", "in", "die\u00b7sen", "Mie\u00b7nen", "herr\u00b7schen", ",", "da\u00df", "sie", "graut", "\u2013", "ja", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "PDAT", "NN", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$(", "PTKANT", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.18": {"text": "und es rieselt etwas Kaltes \u00fcber meine Haut da.", "tokens": ["und", "es", "rie\u00b7selt", "et\u00b7was", "Kal\u00b7tes", "\u00fc\u00b7ber", "mei\u00b7ne", "Haut", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "ADJA", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.19": {"text": "Und vier Rappen ziehn den Wagen, trauerflorumflossen,", "tokens": ["Und", "vier", "Rap\u00b7pen", "ziehn", "den", "Wa\u00b7gen", ",", "trau\u00b7er\u00b7flo\u00b7rum\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVFIN", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.20": {"text": "aber mich zieht eine Hand, die h\u00e4lt mich starr umschlossen.", "tokens": ["a\u00b7ber", "mich", "zieht", "ei\u00b7ne", "Hand", ",", "die", "h\u00e4lt", "mich", "starr", "um\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.21": {"text": "Gro\u00dfer Gott, der Sarg, der Sarg da \u2013 kommt wol auf mich los da?!", "tokens": ["Gro\u00b7\u00dfer", "Gott", ",", "der", "Sarg", ",", "der", "Sarg", "da", "\u2013", "kommt", "wol", "auf", "mich", "los", "da", "?!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "ADV", "$(", "VVFIN", "ADV", "APPR", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+----+", "measure": "unknown.measure.hexa"}, "line.22": {"text": "Da im Sarge, ja im Sarge liegt mein Daseinsloos \u2013 ja ...", "tokens": ["Da", "im", "Sar\u00b7ge", ",", "ja", "im", "Sar\u00b7ge", "liegt", "mein", "Da\u00b7seins\u00b7loos", "\u2013", "ja", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "ADV", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$(", "PTKANT", "$("], "meter": "--+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.23": {"text": "Wof\u00fcr schlugst du mich so t\u00fcckisch, Du, den \u00bbGott\u00ab sie schelten?", "tokens": ["Wo\u00b7f\u00fcr", "schlugst", "du", "mich", "so", "t\u00fc\u00b7ckisch", ",", "Du", ",", "den", "\u00bb", "Gott", "\u00ab", "sie", "schel\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "$,", "PPER", "$,", "ART", "$(", "NN", "$(", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.2": {"line.1": {"text": "wo-f\u00fcr? wo-f\u00fcr? aach!", "tokens": ["wo\u00b7f\u00fcr", "?", "wo\u00b7f\u00fcr", "?", "aach", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "$.", "PWAV", "$.", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Du, der Herr der Kreaturen, Herr der Sternenwelten,", "tokens": ["Du", ",", "der", "Herr", "der", "Kre\u00b7a\u00b7tu\u00b7ren", ",", "Herr", "der", "Ster\u00b7nen\u00b7wel\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Mich zerbrachst du! Schmach!", "tokens": ["Mich", "zer\u00b7brachst", "du", "!", "Schmach", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Du ein Gott? Wo denn, sage?!", "tokens": ["Du", "ein", "Gott", "?", "Wo", "denn", ",", "sa\u00b7ge", "?!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ART", "NN", "$.", "PWAV", "ADV", "$,", "VVFIN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "St\u00f6hnst wol im Gedr\u00f6hn der Glocken, in der Rabenklage?", "tokens": ["St\u00f6hnst", "wol", "im", "Ge\u00b7dr\u00f6hn", "der", "Glo\u00b7cken", ",", "in", "der", "Ra\u00b7ben\u00b7kla\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Tritt doch her hier, grinsend, prahlend, mit dem Hohn im Blicke,", "tokens": ["Tritt", "doch", "her", "hier", ",", "grin\u00b7send", ",", "prah\u00b7lend", ",", "mit", "dem", "Hohn", "im", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "$,", "ADJD", "$,", "VVPP", "$,", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "Du \u2013 Scheusal der Nacht!", "tokens": ["Du", "\u2013", "Scheu\u00b7sal", "der", "Nacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Bin doch gr\u00f6\u00dfer, ich mit meinem gro\u00dfen Gramgeschicke,", "tokens": ["Bin", "doch", "gr\u00f6\u00b7\u00dfer", ",", "ich", "mit", "mei\u00b7nem", "gro\u00b7\u00dfen", "Gram\u00b7ge\u00b7schi\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.9": {"text": "als du Gott der Macht!", "tokens": ["als", "du", "Gott", "der", "Macht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "du Giftgott!", "tokens": ["du", "Gift\u00b7gott", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "du Giftgott! \u2013", "tokens": ["du", "Gift\u00b7gott", "!", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["PPER", "NN", "$.", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Ja: so st\u00f6hnen hohl die Glocken ...", "tokens": ["Ja", ":", "so", "st\u00f6h\u00b7nen", "hohl", "die", "Glo\u00b7cken", "..."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "VVFIN", "ADJD", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Jessus-Maria,", "tokens": ["Jes\u00b7sus\u00b7Ma\u00b7ria", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "thut so drohn, der tote Ton!", "tokens": ["thut", "so", "drohn", ",", "der", "to\u00b7te", "Ton", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "der Ton! \u2013 der Ton! \u2013 \u2013", "tokens": ["der", "Ton", "!", "\u2013", "der", "Ton", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "$.", "$(", "ART", "NN", "$.", "$(", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Heilig in der wei\u00dfen Seide", "tokens": ["Hei\u00b7lig", "in", "der", "wei\u00b7\u00dfen", "Sei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "tr\u00e4umt sie, still und schwer,", "tokens": ["tr\u00e4umt", "sie", ",", "still", "und", "schwer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "bleich gekreuzt die H\u00e4nde beide \u2013", "tokens": ["bleich", "ge\u00b7kreuzt", "die", "H\u00e4n\u00b7de", "bei\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "ART", "NN", "PIS", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "nein, du tr\u00e4umst nicht mehr!", "tokens": ["nein", ",", "du", "tr\u00e4umst", "nicht", "mehr", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "F\u00fchlst nicht mehr den Duft des bittern", "tokens": ["F\u00fchlst", "nicht", "mehr", "den", "Duft", "des", "bit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ADV", "ART", "NN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Myrtenkranzes, nicht,", "tokens": ["Myr\u00b7ten\u00b7kran\u00b7zes", ",", "nicht", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "PTKNEG", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "nie mehr meine Lippen zittern,", "tokens": ["nie", "mehr", "mei\u00b7ne", "Lip\u00b7pen", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "k\u00fcss'ich dein Gesicht.", "tokens": ["k\u00fcss'\u00b7ich", "dein", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "H\u00f6rst mich fahl Gespenst nicht schreiten", "tokens": ["H\u00f6rst", "mich", "fahl", "Ge\u00b7spenst", "nicht", "schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVFIN", "NN", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "durch die hohle Luft,", "tokens": ["durch", "die", "hoh\u00b7le", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "wei\u00dft nicht, da\u00df wir dich begleiten", "tokens": ["wei\u00dft", "nicht", ",", "da\u00df", "wir", "dich", "be\u00b7glei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "in die dumpfe Gruft.", "tokens": ["in", "die", "dump\u00b7fe", "Gruft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "Und dich in den Sarg zu legen,", "tokens": ["Und", "dich", "in", "den", "Sarg", "zu", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "dazu liebt'ich dich?", "tokens": ["da\u00b7zu", "liebt'\u00b7ich", "dich", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.15": {"text": "dazu deiner Mutter Segen?", "tokens": ["da\u00b7zu", "dei\u00b7ner", "Mut\u00b7ter", "Se\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "drum gebar sie dich?!", "tokens": ["drum", "ge\u00b7bar", "sie", "dich", "?!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.17": {"text": "Das mein Brautbett? und ich klage", "tokens": ["Das", "mein", "Braut\u00b7bett", "?", "und", "ich", "kla\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "NN", "$.", "KON", "PPER", "VVFIN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.18": {"text": "und ich lebe noch?", "tokens": ["und", "ich", "le\u00b7be", "noch", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.19": {"text": "Gott, nach solchem, solchem Tage,", "tokens": ["Gott", ",", "nach", "sol\u00b7chem", ",", "sol\u00b7chem", "Ta\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PIAT", "$,", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Gott, was soll ich noch?! \u2013", "tokens": ["Gott", ",", "was", "soll", "ich", "noch", "?!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PWS", "VMFIN", "PPER", "ADV", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Oh wie war sie s\u00fc\u00df und milde:", "tokens": ["Oh", "wie", "war", "sie", "s\u00fc\u00df", "und", "mil\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wie ein Liebeslied,", "tokens": ["wie", "ein", "Lie\u00b7bes\u00b7lied", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "wie durch d\u00e4mmernde Gefilde", "tokens": ["wie", "durch", "d\u00e4m\u00b7mern\u00b7de", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "fern ein Engel zieht.", "tokens": ["fern", "ein", "En\u00b7gel", "zieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Keine S\u00fcnde gab es, keine,", "tokens": ["Kei\u00b7ne", "S\u00fcn\u00b7de", "gab", "es", ",", "kei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$,", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "wenn ich an ihr hing", "tokens": ["wenn", "ich", "an", "ihr", "hing"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "und von ihrem Mund das reine", "tokens": ["und", "von", "ih\u00b7rem", "Mund", "das", "rei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sakrament empfing.", "tokens": ["Sak\u00b7ra\u00b7ment", "emp\u00b7fing", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Ja, sie war mein guter Wille,", "tokens": ["Ja", ",", "sie", "war", "mein", "gu\u00b7ter", "Wil\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "und sie liebte mich!", "tokens": ["und", "sie", "lieb\u00b7te", "mich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Ihrer Seele gro\u00dfe, stille", "tokens": ["Ih\u00b7rer", "See\u00b7le", "gro\u00b7\u00dfe", ",", "stil\u00b7le"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "ADJA", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Flamme f\u00fchrte mich,", "tokens": ["Flam\u00b7me", "f\u00fchr\u00b7te", "mich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "f\u00fchrte mich \u2013 wohin? o Jammer:", "tokens": ["f\u00fchr\u00b7te", "mich", "\u2013", "wo\u00b7hin", "?", "o", "Jam\u00b7mer", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "PWAV", "$.", "FM", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "oh, durch Himmelsluft", "tokens": ["oh", ",", "durch", "Him\u00b7mel\u00b7sluft"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADJD", "$,", "APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.15": {"text": "in den Abgrund, in die Kammer", "tokens": ["in", "den", "Ab\u00b7grund", ",", "in", "die", "Kam\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "deiner kalten Gruft!", "tokens": ["dei\u00b7ner", "kal\u00b7ten", "Gruft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.17": {"text": "Das mein Brautbett? und ich klage", "tokens": ["Das", "mein", "Braut\u00b7bett", "?", "und", "ich", "kla\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "NN", "$.", "KON", "PPER", "VVFIN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.18": {"text": "und ich lebe noch?", "tokens": ["und", "ich", "le\u00b7be", "noch", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.19": {"text": "Gott, nach solchem, solchem Tage,", "tokens": ["Gott", ",", "nach", "sol\u00b7chem", ",", "sol\u00b7chem", "Ta\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PIAT", "$,", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Gott, was soll ich noch?! \u2013 \u2013", "tokens": ["Gott", ",", "was", "soll", "ich", "noch", "?!", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "$,", "PWS", "VMFIN", "PPER", "ADV", "$.", "$(", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Und nun heben sie den Sarg und \u2013 woll'n sie mir wol nehmen?!", "tokens": ["Und", "nun", "he\u00b7ben", "sie", "den", "Sarg", "und", "\u2013", "woll'n", "sie", "mir", "wol", "neh\u00b7men", "?!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "KON", "$(", "PWAV", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Was, Gewalt? sie woll'n mich halten?! Hahahah, ihr Memmen!", "tokens": ["Was", ",", "Ge\u00b7walt", "?", "sie", "woll'n", "mich", "hal\u00b7ten", "?!", "Ha\u00b7ha\u00b7hah", ",", "ihr", "Mem\u00b7men", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "$,", "NN", "$.", "PPER", "VMFIN", "PRF", "VVINF", "$.", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Hahahah, ihr tollen Hunde! La\u00dft mich los! ich rase!", "tokens": ["Ha\u00b7ha\u00b7hah", ",", "ihr", "tol\u00b7len", "Hun\u00b7de", "!", "La\u00dft", "mich", "los", "!", "ich", "ra\u00b7se", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "ADJA", "NN", "$.", "VVIMP", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "ich bin jung! ich habe F\u00e4uste! ich bin stark! Ich blase", "tokens": ["ich", "bin", "jung", "!", "ich", "ha\u00b7be", "F\u00e4us\u00b7te", "!", "ich", "bin", "stark", "!", "Ich", "bla\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$.", "PPER", "VAFIN", "NN", "$.", "PPER", "VAFIN", "ADJD", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "euch zu Staub, ihr m\u00fcrben Spinnen! Weg da, feile Menge:", "tokens": ["euch", "zu", "Staub", ",", "ihr", "m\u00fcr\u00b7ben", "Spin\u00b7nen", "!", "Weg", "da", ",", "fei\u00b7le", "Men\u00b7ge", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "$,", "PPOSAT", "ADJA", "NN", "$.", "NN", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Meinem k\u00f6niglichen Gram ist eure Brust zu enge!", "tokens": ["Mei\u00b7nem", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Gram", "ist", "eu\u00b7re", "Brust", "zu", "en\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "APPR", "ADJA", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "Nur ein K\u00f6nig darf mein Kleinod rei\u00dfen mir vom Herzen!", "tokens": ["Nur", "ein", "K\u00f6\u00b7nig", "darf", "mein", "Klei\u00b7nod", "rei\u00b7\u00dfen", "mir", "vom", "Her\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.8": {"text": "Und sie weichen ... Aber ich, ich gro\u00dfer F\u00fcrst der Schmerzen,", "tokens": ["Und", "si