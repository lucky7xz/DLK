, "punct"], "pos": ["PWS", "PPER", "VAFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Hier mein klopfend Herz dein Kl\u00f6ppel, du die hohle Glocke!", "tokens": ["Hier", "mein", "klop\u00b7fend", "Herz", "dein", "Kl\u00f6p\u00b7pel", ",", "du", "die", "hoh\u00b7le", "Glo\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJD", "NN", "PPOSAT", "NN", "$,", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "Wollt ich's werden? heh, wer hing mich in die hohle Glocke?", "tokens": ["Wollt", "ich's", "wer\u00b7den", "?", "heh", ",", "wer", "hing", "mich", "in", "die", "hoh\u00b7le", "Glo\u00b7cke", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VAINF", "$.", "VVFIN", "$,", "PWS", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Gott, ", "tokens": ["Gott", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Bin in meinen Daseinsketten doch kein Knecht am Blocke?!", "tokens": ["Bin", "in", "mei\u00b7nen", "Da\u00b7sein\u00b7sket\u00b7ten", "doch", "kein", "Knecht", "am", "Blo\u00b7cke", "?!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "ADV", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.8": {"text": "Ich bin frei! ich geh!", "tokens": ["Ich", "bin", "frei", "!", "ich", "geh", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$.", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"te