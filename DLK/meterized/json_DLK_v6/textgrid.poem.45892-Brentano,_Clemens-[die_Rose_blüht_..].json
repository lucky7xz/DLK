{"textgrid.poem.45892": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[die Rose bl\u00fcht ..]", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Rose bl\u00fcht, ich bin die fromme Biene,", "tokens": ["Die", "Ro\u00b7se", "bl\u00fcht", ",", "ich", "bin", "die", "from\u00b7me", "Bie\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die in der Bl\u00e4tter keuschen Busen sinkt,", "tokens": ["Die", "in", "der", "Bl\u00e4t\u00b7ter", "keu\u00b7schen", "Bu\u00b7sen", "sinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und milden Tau und s\u00fc\u00dfen Honig trinkt,", "tokens": ["Und", "mil\u00b7den", "Tau", "und", "s\u00fc\u00b7\u00dfen", "Ho\u00b7nig", "trinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch lebt ihr Glanz und bleibet ewig gr\u00fcne.", "tokens": ["Doch", "lebt", "ihr", "Glanz", "und", "blei\u00b7bet", "e\u00b7wig", "gr\u00fc\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "ADJD", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So singt mein tiefstes Freudenlied,", "tokens": ["So", "singt", "mein", "tiefs\u00b7tes", "Freu\u00b7den\u00b7lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach meine Rose bl\u00fcht!", "tokens": ["Ach", "mei\u00b7ne", "Ro\u00b7se", "bl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Rose bl\u00fcht, o Sonnenschein verziehe,", "tokens": ["Die", "Ro\u00b7se", "bl\u00fcht", ",", "o", "Son\u00b7nen\u00b7schein", "ver\u00b7zie\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "FM", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df lange noch der liebe Sommer w\u00e4hrt,", "tokens": ["Da\u00df", "lan\u00b7ge", "noch", "der", "lie\u00b7be", "Som\u00b7mer", "w\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und mir kein Sturm die s\u00fc\u00dfe Lust versehrt,", "tokens": ["Und", "mir", "kein", "Sturm", "die", "s\u00fc\u00b7\u00dfe", "Lust", "ver\u00b7sehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PIAT", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df all mein Heil aus dieser Rose bl\u00fche,", "tokens": ["Da\u00df", "all", "mein", "Heil", "aus", "die\u00b7ser", "Ro\u00b7se", "bl\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "PPOSAT", "NN", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So freut sich innig mein Gem\u00fct,", "tokens": ["So", "freut", "sich", "in\u00b7nig", "mein", "Ge\u00b7m\u00fct", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil meine Rose bl\u00fcht!", "tokens": ["Weil", "mei\u00b7ne", "Ro\u00b7se", "bl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Rose bl\u00fcht, und lacht vor andern Rosen,", "tokens": ["Die", "Ro\u00b7se", "bl\u00fcht", ",", "und", "lacht", "vor", "an\u00b7dern", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit solcher Huld, und Liebesmildigkeit,", "tokens": ["Mit", "sol\u00b7cher", "Huld", ",", "und", "Lie\u00b7bes\u00b7mil\u00b7dig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df gern mein Sinn sich zu der Pflicht erbeut,", "tokens": ["Da\u00df", "gern", "mein", "Sinn", "sich", "zu", "der", "Pflicht", "er\u00b7beut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Weil alle Liebe, die ergl\u00fcht,", "tokens": ["Weil", "al\u00b7le", "Lie\u00b7be", ",", "die", "er\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aus ", "tokens": ["Aus"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}}}}}