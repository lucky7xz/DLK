{"textgrid.poem.38007": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Ueberl\u00e4ufer", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In den Garten wollen wir gehen,", "tokens": ["In", "den", "Gar\u00b7ten", "wol\u00b7len", "wir", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wo die sch\u00f6nen Rosen stehen,", "tokens": ["Wo", "die", "sch\u00f6\u00b7nen", "Ro\u00b7sen", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da stehen der Rosen gar zu viel,", "tokens": ["Da", "ste\u00b7hen", "der", "Ro\u00b7sen", "gar", "zu", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PTKA", "PIS", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Brech ich mir eine, wo ich will.", "tokens": ["Brech", "ich", "mir", "ei\u00b7ne", ",", "wo", "ich", "will", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPER", "ART", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wir haben gar \u00f6fters beysammen gesessen,", "tokens": ["Wir", "ha\u00b7ben", "gar", "\u00f6f\u00b7ters", "bey\u00b7sam\u00b7men", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVIZU", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie ist mir mein Schatz so treu gewesen,", "tokens": ["Wie", "ist", "mir", "mein", "Schatz", "so", "treu", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "VAPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Das hat ich mir nicht gebildet ein,", "tokens": ["Das", "hat", "ich", "mir", "nicht", "ge\u00b7bil\u00b7det", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PPER", "PTKNEG", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da\u00df mein Schatz so falsch k\u00f6nnt seyn.", "tokens": ["Da\u00df", "mein", "Schatz", "so", "falsch", "k\u00f6nnt", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "H\u00f6rt ihr nicht den J\u00e4ger blasen,", "tokens": ["H\u00f6rt", "ihr", "nicht", "den", "J\u00e4\u00b7ger", "bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem Wald auf gr\u00fcnem Rasen?", "tokens": ["In", "dem", "Wald", "auf", "gr\u00fc\u00b7nem", "Ra\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Den J\u00e4ger mit dem gr\u00fcnen Huth,", "tokens": ["Den", "J\u00e4\u00b7ger", "mit", "dem", "gr\u00fc\u00b7nen", "Huth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der meinen Schatz verf\u00fchren thut?", "tokens": ["Der", "mei\u00b7nen", "Schatz", "ver\u00b7f\u00fch\u00b7ren", "thut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "H\u00f6rt ihr nicht den Trompeter blasen,", "tokens": ["H\u00f6rt", "ihr", "nicht", "den", "Trom\u00b7pe\u00b7ter", "bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "In der Stadt auf der Parade?", "tokens": ["In", "der", "Stadt", "auf", "der", "Pa\u00b7ra\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Der Trompeter mit dem Federbusch,", "tokens": ["Der", "Trom\u00b7pe\u00b7ter", "mit", "dem", "Fe\u00b7der\u00b7busch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der mir meinen Schatz verrathen thut.", "tokens": ["Der", "mir", "mei\u00b7nen", "Schatz", "ver\u00b7ra\u00b7then", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}