{"textgrid.poem.47232": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ersch\u00f6pft von langen winterlichen Wegen,", "tokens": ["Er\u00b7sch\u00f6pft", "von", "lan\u00b7gen", "win\u00b7ter\u00b7li\u00b7chen", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf meines Lebens allertr\u00fcbster Reise,", "tokens": ["Auf", "mei\u00b7nes", "Le\u00b7bens", "al\u00b7ler\u00b7tr\u00fcbs\u00b7ter", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kam ich hieher, in froher Hoffnung, leise", "tokens": ["Kam", "ich", "hie\u00b7her", ",", "in", "fro\u00b7her", "Hoff\u00b7nung", ",", "lei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "PPER", "PAV", "$,", "APPR", "ADJA", "NN", "$,", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Herz an eines Freundes Herz zu legen.", "tokens": ["Mein", "Herz", "an", "ei\u00b7nes", "Freun\u00b7des", "Herz", "zu", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da starrte mir das Freundesherz entgegen", "tokens": ["Da", "starr\u00b7te", "mir", "das", "Freun\u00b7des\u00b7herz", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch starrer als vom starrsten Wintereise;", "tokens": ["Noch", "star\u00b7rer", "als", "vom", "starrs\u00b7ten", "Win\u00b7ter\u00b7ei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da klang das Freundeswort in kalter Weise,", "tokens": ["Da", "klang", "das", "Freun\u00b7des\u00b7wort", "in", "kal\u00b7ter", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch k\u00e4lter als der k\u00e4ltste Winterregen.", "tokens": ["Noch", "k\u00e4l\u00b7ter", "als", "der", "k\u00e4lts\u00b7te", "Win\u00b7ter\u00b7re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Weh mir! Natur, wohin soll ich mich wenden?", "tokens": ["Weh", "mir", "!", "Na\u00b7tur", ",", "wo\u00b7hin", "soll", "ich", "mich", "wen\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "NN", "$,", "PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn drau\u00dfen du stehst mit den Winterschauern", "tokens": ["Wenn", "drau\u00b7\u00dfen", "du", "stehst", "mit", "den", "Win\u00b7ter\u00b7schau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und hier die Herzen mit den Winterfr\u00f6sten?", "tokens": ["Und", "hier", "die", "Her\u00b7zen", "mit", "den", "Win\u00b7ter\u00b7fr\u00f6s\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich bitte dich, la\u00df deinen Winter enden", "tokens": ["Ich", "bit\u00b7te", "dich", ",", "la\u00df", "dei\u00b7nen", "Win\u00b7ter", "en\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "VVIMP", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und werde Lenz! Mag Menschenfrost dann dauern;", "tokens": ["Und", "wer\u00b7de", "Lenz", "!", "Mag", "Men\u00b7schen\u00b7frost", "dann", "dau\u00b7ern", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "$.", "VMFIN", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An deiner Fr\u00fchlingsbrust will ich mich tr\u00f6sten.", "tokens": ["An", "dei\u00b7ner", "Fr\u00fch\u00b7lings\u00b7brust", "will", "ich", "mich", "tr\u00f6s\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}