{"textgrid.poem.64150": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Siegeslied der Deutschen beim Einzug in Mailand unter Barbarossa", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun lasset die Posaunen t\u00f6nen, nun breitet froh die Fahnen aus,", "tokens": ["Nun", "las\u00b7set", "die", "Po\u00b7sau\u00b7nen", "t\u00f6\u00b7nen", ",", "nun", "brei\u00b7tet", "froh", "die", "Fah\u00b7nen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVINF", "$,", "ADV", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "La\u00dft durch Lombardenl\u00fcfte dr\u00f6hnen des deutschen Sieges Jubelbraus:", "tokens": ["La\u00dft", "durch", "Lom\u00b7bar\u00b7den\u00b7l\u00fcf\u00b7te", "dr\u00f6h\u00b7nen", "des", "deut\u00b7schen", "Sie\u00b7ges", "Ju\u00b7bel\u00b7braus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "--+--+-+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Denn unser Kaiser Barbarossa, der Held, tat einen gro\u00dfen Schlag: \u2013", "tokens": ["Denn", "un\u00b7ser", "Kai\u00b7ser", "Bar\u00b7ba\u00b7ros\u00b7sa", ",", "der", "Held", ",", "tat", "ei\u00b7nen", "gro\u00b7\u00dfen", "Schlag", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "NE", "$,", "ART", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Seit jener Nacht in Schlo\u00df Kanossa ist dies der erste deutsche Tag.", "tokens": ["Seit", "je\u00b7ner", "Nacht", "in", "Schlo\u00df", "Ka\u00b7nos\u00b7sa", "ist", "dies", "der", "ers\u00b7te", "deut\u00b7sche", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "NN", "NE", "VAFIN", "PDS", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Das Lied soll durch die Alpen klingen bis Deutschland wie ein Lustorkan", "tokens": ["Das", "Lied", "soll", "durch", "die", "Al\u00b7pen", "klin\u00b7gen", "bis", "Deutschland", "wie", "ein", "Lus\u00b7tor\u00b7kan"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "APPR", "ART", "NN", "VVFIN", "APPR", "NE", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+--+--+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Und drohend an das Ohr soll's dringen dem Bischof dort im Lateran.", "tokens": ["Und", "dro\u00b7hend", "an", "das", "Ohr", "soll's", "drin\u00b7gen", "dem", "Bi\u00b7schof", "dort", "im", "La\u00b7te\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "NE", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Nun auf, des welschen Lorbeers Reiser frohlockend schlingt um Helm und Speer", "tokens": ["Nun", "auf", ",", "des", "wel\u00b7schen", "Lor\u00b7beers", "Rei\u00b7ser", "froh\u00b7lo\u00b7ckend", "schlingt", "um", "Helm", "und", "Speer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "$,", "ART", "ADJA", "NN", "NN", "ADJD", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Und jauchzend folgt dem gro\u00dfen Kaiser im Schritte des Triumphs das Heer.", "tokens": ["Und", "jauch\u00b7zend", "folgt", "dem", "gro\u00b7\u00dfen", "Kai\u00b7ser", "im", "Schrit\u00b7te", "des", "Tri\u00b7umphs", "das", "Heer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Das Schwert gez\u00fcckt, die Faust zur Seite, durch Staub und Blut, durch Schutt und Stein,", "tokens": ["Das", "Schwert", "ge\u00b7z\u00fcckt", ",", "die", "Faust", "zur", "Sei\u00b7te", ",", "durch", "Staub", "und", "Blut", ",", "durch", "Schutt", "und", "Stein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "APPRART", "NN", "$,", "APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Stolz, in des Hasses Prachtgeleite, so reiten wir in Mailand ein.", "tokens": ["Stolz", ",", "in", "des", "Has\u00b7ses", "Pracht\u00b7ge\u00b7lei\u00b7te", ",", "so", "rei\u00b7ten", "wir", "in", "Mai\u00b7land", "ein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "NE", "PTKVZ", "$."], "meter": "+--+-+-+--+-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.3": {"text": "Zu lange lie\u00df't den Herrn du pochen am Tor, du Stadt voll Widerstand:", "tokens": ["Zu", "lan\u00b7ge", "lie\u00df't", "den", "Herrn", "du", "po\u00b7chen", "am", "Tor", ",", "du", "Stadt", "voll", "Wi\u00b7der\u00b7stand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ART", "NN", "PPER", "VVFIN", "APPRART", "NN", "$,", "PPER", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Da hat in Tr\u00fcmmer dich zerbrochen die zorn'ge, kaiserliche Hand.", "tokens": ["Da", "hat", "in", "Tr\u00fcm\u00b7mer", "dich", "zer\u00b7bro\u00b7chen", "die", "zorn'\u00b7ge", ",", "kai\u00b7ser\u00b7li\u00b7che", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "PPER", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "War dir dein B\u00fcndnis nun zum Frommen mit hundert St\u00e4dten stark und treu?", "tokens": ["War", "dir", "dein", "B\u00fcnd\u00b7nis", "nun", "zum", "From\u00b7men", "mit", "hun\u00b7dert", "St\u00e4d\u00b7ten", "stark", "und", "treu", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "ADV", "APPRART", "NN", "APPR", "CARD", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Wie Sturmwind ist der Kaiser kommen und auseinander stob die Spreu!", "tokens": ["Wie", "Sturm\u00b7wind", "ist", "der", "Kai\u00b7ser", "kom\u00b7men", "und", "aus\u00b7ein\u00b7an\u00b7der", "stob", "die", "Spreu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VAFIN", "ART", "NN", "VVINF", "KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Was half's nun, da\u00df der Papst uns bannte? Sein Bannstrahl machte uns nicht schlaff:", "tokens": ["Was", "ha\u00b7lf's", "nun", ",", "da\u00df", "der", "Papst", "uns", "bann\u00b7te", "?", "Sein", "Bann\u00b7strahl", "mach\u00b7te", "uns", "nicht", "schlaff", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+-+--+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.4": {"text": "Der Sturmbock, der dein Tor berannte, traf besser als der grimme Pfaff.", "tokens": ["Der", "Sturm\u00b7bock", ",", "der", "dein", "Tor", "be\u00b7rann\u00b7te", ",", "traf", "bes\u00b7ser", "als", "der", "grim\u00b7me", "Pfaff", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "ADJD", "KOKOM", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.5": {"line.1": {"text": "All' deine Besten sind gefallen und deiner Frauen Sch\u00f6ne weint,", "tokens": ["All'", "dei\u00b7ne", "Bes\u00b7ten", "sind", "ge\u00b7fal\u00b7len", "und", "dei\u00b7ner", "Frau\u00b7en", "Sch\u00f6\u00b7ne", "weint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VAFIN", "VVPP", "KON", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Durch die gebroch'nen S\u00e4ulenhallen mit Siegesliedern zieht dein Feind:", "tokens": ["Durch", "die", "ge\u00b7bro\u00b7ch'\u00b7nen", "S\u00e4u\u00b7len\u00b7hal\u00b7len", "mit", "Sie\u00b7ges\u00b7lie\u00b7dern", "zieht", "dein", "Feind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+--+-+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Nun ist dein gro\u00dfer Trotz zerschlagen, nun ist dir alle Kraft geraubt,", "tokens": ["Nun", "ist", "dein", "gro\u00b7\u00dfer", "Trotz", "zer\u00b7schla\u00b7gen", ",", "nun", "ist", "dir", "al\u00b7le", "Kraft", "ge\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$,", "ADV", "VAFIN", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Das Joch der Knechtschaft mu\u00dft du tragen, im Staube liegt dein stolzes Haupt.", "tokens": ["Das", "Joch", "der", "Knecht\u00b7schaft", "mu\u00dft", "du", "tra\u00b7gen", ",", "im", "Stau\u00b7be", "liegt", "dein", "stol\u00b7zes", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,", "APPRART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.6": {"line.1": {"text": "Gebrochen sind die festen Mauern und Turm und Schanzen abgedeckt,", "tokens": ["Ge\u00b7bro\u00b7chen", "sind", "die", "fes\u00b7ten", "Mau\u00b7ern", "und", "Turm", "und", "Schan\u00b7zen", "ab\u00b7ge\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "KON", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Des Kaisers Feinde sei'n mit Schauern von deinem Anblick eingeschreckt:", "tokens": ["Des", "Kai\u00b7sers", "Fein\u00b7de", "sei'n", "mit", "Schau\u00b7ern", "von", "dei\u00b7nem", "An\u00b7blick", "ein\u00b7ge\u00b7schreckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPOSAT", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Denn laut und herrlich warst du weiland, nun aber bist du totenstill: \u2013", "tokens": ["Denn", "laut", "und", "herr\u00b7lich", "warst", "du", "wei\u00b7land", ",", "nun", "a\u00b7ber", "bist", "du", "to\u00b7tens\u00b7till", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VAFIN", "PPER", "VVFIN", "$,", "ADV", "ADV", "VAFIN", "PPER", "ADJD", "$.", "$("], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Darum gedenken soll an Mailand, wer Barbarossa trotzen will!", "tokens": ["Da\u00b7rum", "ge\u00b7den\u00b7ken", "soll", "an", "Mai\u00b7land", ",", "wer", "Bar\u00b7ba\u00b7ros\u00b7sa", "trot\u00b7zen", "will", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVINF", "VMFIN", "APPR", "NE", "$,", "PWS", "NE", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}}}}