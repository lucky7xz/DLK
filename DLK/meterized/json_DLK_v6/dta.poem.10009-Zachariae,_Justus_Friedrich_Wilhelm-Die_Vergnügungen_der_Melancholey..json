{"dta.poem.10009": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Vergn\u00fcgungen  \n  der Melancholey.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20674-3", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Obgleich Popens Gesang die sanftesten Gratien", "tokens": ["Ob\u00b7gleich", "Po\u00b7pens", "Ge\u00b7sang", "die", "sanf\u00b7tes\u00b7ten", "Gra\u00b7ti\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und die gl\u00fccklichste Kunst die attischen Bl\u00e4tter geschm\u00fccker:", "tokens": ["Und", "die", "gl\u00fcck\u00b7lichs\u00b7te", "Kunst", "die", "at\u00b7ti\u00b7schen", "Bl\u00e4t\u00b7ter", "ge\u00b7schm\u00fc\u00b7cker", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Dennoch gl\u00fcht mein ernstes Gem\u00fcth in s\u00fc\u00dferm Ent-", "tokens": ["Den\u00b7noch", "gl\u00fcht", "mein", "erns\u00b7tes", "Ge\u00b7m\u00fcth", "in", "s\u00fc\u00b7\u00dferm", "Ent"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "TRUNC"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Wenn ich manchmal, gelehnt an einen moosigten Eich-", "tokens": ["Wenn", "ich", "manch\u00b7mal", ",", "ge\u00b7lehnt", "an", "ei\u00b7nen", "moo\u00b7sig\u00b7ten", "Eich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "$,", "VVPP", "APPR", "ART", "ADJA", "TRUNC"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "In dem wildanmuthgen Gesang des zaubrischen Spen-", "tokens": ["In", "dem", "wil\u00b7dan\u00b7muth\u00b7gen", "Ge\u00b7sang", "des", "zaub\u00b7ri\u00b7schen", "Spen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "TRUNC"], "meter": "--+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Zitternd der Una irrenden Fu\u00df in schrecklichen W\u00fcsten", "tokens": ["Zit\u00b7ternd", "der", "U\u00b7na", "ir\u00b7ren\u00b7den", "Fu\u00df", "in", "schreck\u00b7li\u00b7chen", "W\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "ART", "NN", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.7": {"text": "Durch die Einsamkeit wandern gesehn; ganz matt und", "tokens": ["Durch", "die", "Ein\u00b7sam\u00b7keit", "wan\u00b7dern", "ge\u00b7sehn", ";", "ganz", "matt", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "VVPP", "$.", "ADV", "ADJD", "KON"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Mehr, als wenn auf schimmerndem Busen der silber-", "tokens": ["Mehr", ",", "als", "wenn", "auf", "schim\u00b7mern\u00b7dem", "Bu\u00b7sen", "der", "sil\u00b7ber"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "$,", "KOKOM", "KOUS", "APPR", "ADJA", "NN", "ART", "TRUNC"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.9": {"text": "Die in ihr Ungl\u00fcck eilende Sch\u00f6ne ", "tokens": ["Die", "in", "ihr", "Un\u00b7gl\u00fcck", "ei\u00b7len\u00b7de", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "In dem blendenden Stral der lachenden Sonne daher-", "tokens": ["In", "dem", "blen\u00b7den\u00b7den", "Stral", "der", "la\u00b7chen\u00b7den", "Son\u00b7ne", "da\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "TRUNC"], "meter": "--+--+-+--+--+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Zarter Empfindung wird bald das muntre Gem\u00e4lde zum", "tokens": ["Zar\u00b7ter", "Emp\u00b7fin\u00b7dung", "wird", "bald", "das", "mun\u00b7tre", "Ge\u00b7m\u00e4l\u00b7de", "zum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "APPRART"], "meter": "+--+-+--+--+-+", "measure": "iambic.hexa.invert"}, "line.12": {"text": "Und trift nur das kalte Gem\u00fcth mit schwachem Ver-", "tokens": ["Und", "trift", "nur", "das", "kal\u00b7te", "Ge\u00b7m\u00fcth", "mit", "schwa\u00b7chem", "Ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "APPR", "ADJA", "TRUNC"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}}}}