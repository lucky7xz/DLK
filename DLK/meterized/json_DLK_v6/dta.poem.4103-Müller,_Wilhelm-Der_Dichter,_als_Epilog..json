{"dta.poem.4103": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Dichter, als Epilog.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Tret' ich noch einmal in den vollen Saal,", "tokens": ["Tret'", "ich", "noch", "ein\u00b7mal", "in", "den", "vol\u00b7len", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als letztes, f\u00fcnf und zwanzigstes Gedicht,", "tokens": ["Als", "letz\u00b7tes", ",", "f\u00fcnf", "und", "zwan\u00b7zigs\u00b7tes", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "$,", "CARD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als Epilog, der gern das Kl\u00fcgste spricht.", "tokens": ["Als", "E\u00b7pi\u00b7log", ",", "der", "gern", "das", "Kl\u00fcgs\u00b7te", "spricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch pfuschte mir der Bach in's Handwerk schon", "tokens": ["Doch", "pfuschte", "mir", "der", "Bach", "in's", "Hand\u00b7werk", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Mit seiner Leichenred' im nassen Ton.", "tokens": ["Mit", "sei\u00b7ner", "Lei\u00b7chen\u00b7red'", "im", "nas\u00b7sen", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Aus solchem hohlen Wasserorgelschall", "tokens": ["Aus", "sol\u00b7chem", "hoh\u00b7len", "Was\u00b7ser\u00b7or\u00b7gel\u00b7schall"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Zieht Jeder selbst sich besser die Moral;", "tokens": ["Zieht", "Je\u00b7der", "selbst", "sich", "bes\u00b7ser", "die", "Mo\u00b7ral", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PRF", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ich geb' es auf, und lasse diesen Zwist,", "tokens": ["Ich", "geb'", "es", "auf", ",", "und", "las\u00b7se", "die\u00b7sen", "Zwist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Weil Widerspruch nicht meines Amtes ist.", "tokens": ["Weil", "Wi\u00b7der\u00b7spruch", "nicht", "mei\u00b7nes", "Am\u00b7tes", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKNEG", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So hab' ich denn nichts lieber hier zu thun,", "tokens": ["So", "hab'", "ich", "denn", "nichts", "lie\u00b7ber", "hier", "zu", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PIS", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als euch zum Schlu\u00df zu w\u00fcnschen, wohl zu ruhn.", "tokens": ["Als", "euch", "zum", "Schlu\u00df", "zu", "w\u00fcn\u00b7schen", ",", "wohl", "zu", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$,", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir blasen unsre Sonn' und Sternlein aus \u2014", "tokens": ["Wir", "bla\u00b7sen", "uns\u00b7re", "Sonn'", "und", "Stern\u00b7lein", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "NN", "APPR", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nun findet euch im Dunkel gut nach Haus,", "tokens": ["Nun", "fin\u00b7det", "euch", "im", "Dun\u00b7kel", "gut", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und wollt ihr tr\u00e4umen einen leichten Traum,", "tokens": ["Und", "wollt", "ihr", "tr\u00e4u\u00b7men", "ei\u00b7nen", "leich\u00b7ten", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So denkt an M\u00fchlenrad und Wasserschaum,", "tokens": ["So", "denkt", "an", "M\u00fch\u00b7len\u00b7rad", "und", "Was\u00b7ser\u00b7schaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wenn ihr die Augen schlie\u00dft zu langer Nacht,", "tokens": ["Wenn", "ihr", "die", "Au\u00b7gen", "schlie\u00dft", "zu", "lan\u00b7ger", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Bis es den Kopf zum Drehen euch gebracht.", "tokens": ["Bis", "es", "den", "Kopf", "zum", "Dre\u00b7hen", "euch", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und wer ein M\u00e4dchen f\u00fchrt an seiner Hand,", "tokens": ["Und", "wer", "ein", "M\u00e4d\u00b7chen", "f\u00fchrt", "an", "sei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der bitte scheidend um ein Liebespfand,", "tokens": ["Der", "bit\u00b7te", "schei\u00b7dend", "um", "ein", "Lie\u00b7be\u00b7spfand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und giebt sie heute, was sie oft versagt,", "tokens": ["Und", "giebt", "sie", "heu\u00b7te", ",", "was", "sie", "oft", "ver\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "So sei des treuen M\u00fcllers treu gedacht", "tokens": ["So", "sei", "des", "treu\u00b7en", "M\u00fcl\u00b7lers", "treu", "ge\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Bei jedem H\u00e4ndedruck, bei jedem Ku\u00df,", "tokens": ["Bei", "je\u00b7dem", "H\u00e4n\u00b7de\u00b7druck", ",", "bei", "je\u00b7dem", "Ku\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Bei jedem hei\u00dfen Herzens\u00fcberflu\u00df:", "tokens": ["Bei", "je\u00b7dem", "hei\u00b7\u00dfen", "Her\u00b7zen\u00b7s\u00fc\u00b7berf\u00b7lu\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Geb' ihm die Liebe f\u00fcr sein kurzes Leid", "tokens": ["Geb'", "ihm", "die", "Lie\u00b7be", "f\u00fcr", "sein", "kur\u00b7zes", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "In eurem Busen lange Seligkeit!", "tokens": ["In", "eu\u00b7rem", "Bu\u00b7sen", "lan\u00b7ge", "Se\u00b7lig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}