{"textgrid.poem.55772": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: W\u00e4r's nicht besser,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r's nicht besser,", "tokens": ["W\u00e4r's", "nicht", "bes\u00b7ser", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Tausendmal besser,", "tokens": ["Tau\u00b7send\u00b7mal", "bes\u00b7ser", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Statt langsam hinzusiechen,", "tokens": ["Statt", "lang\u00b7sam", "hin\u00b7zu\u00b7sie\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Abzusterben Glied f\u00fcr Glied \u2013", "tokens": ["Ab\u00b7zu\u00b7ster\u00b7ben", "Glied", "f\u00fcr", "Glied", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses verschleuderte,", "tokens": ["Die\u00b7ses", "ver\u00b7schleu\u00b7der\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "S\u00fcndenbesudelte,", "tokens": ["S\u00fcn\u00b7den\u00b7be\u00b7su\u00b7del\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.7": {"text": "Elende Dasein", "tokens": ["E\u00b7len\u00b7de", "Da\u00b7sein"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Schnell zu enden?", "tokens": ["Schnell", "zu", "en\u00b7den", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Ich bin nichts und wei\u00df nichts und kann nichts!", "tokens": ["Ich", "bin", "nichts", "und", "wei\u00df", "nichts", "und", "kann", "nichts", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "KON", "VVFIN", "PIS", "KON", "VMFIN", "PIS", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Mein Leben war Wollust", "tokens": ["Mein", "Le\u00b7ben", "war", "Wol\u00b7lust"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Oder hie und da", "tokens": ["O\u00b7der", "hie", "und", "da"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "KON", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "Halbersticktes Geflacker", "tokens": ["Hal\u00b7ber\u00b7stick\u00b7tes", "Ge\u00b7fla\u00b7cker"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.13": {"text": "Kaum empfundner,", "tokens": ["Kaum", "emp\u00b7fund\u00b7ner", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "Schnell verrauchter Liebe.", "tokens": ["Schnell", "ver\u00b7rauch\u00b7ter", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "Wem zu Gefallen", "tokens": ["Wem", "zu", "Ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PWS", "APPR", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Weiterschleppen", "tokens": ["Wei\u00b7ter\u00b7schlep\u00b7pen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Mein verkommenes,", "tokens": ["Mein", "ver\u00b7kom\u00b7me\u00b7nes", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.18": {"text": "Verfehltes Dasein?", "tokens": ["Ver\u00b7fehl\u00b7tes", "Da\u00b7sein", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "Ja, wenn mein Herz und mein Hirn", "tokens": ["Ja", ",", "wenn", "mein", "Herz", "und", "mein", "Hirn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.20": {"text": "Zu schweigen lernten,", "tokens": ["Zu", "schwei\u00b7gen", "lern\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.21": {"text": "Oder vergessen k\u00f6nnten,", "tokens": ["O\u00b7der", "ver\u00b7ges\u00b7sen", "k\u00f6nn\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.22": {"text": "Wenn das ungest\u00fcme Wollen", "tokens": ["Wenn", "das", "un\u00b7ge\u00b7st\u00fc\u00b7me", "Wol\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Meiner Seele", "tokens": ["Mei\u00b7ner", "See\u00b7le"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.24": {"text": "Zu nichts verfl\u00f6ge,", "tokens": ["Zu", "nichts", "ver\u00b7fl\u00f6\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.25": {"text": "Wenn ich verm\u00f6chte", "tokens": ["Wenn", "ich", "ver\u00b7m\u00f6ch\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.26": {"text": "Still, gen\u00fcgsam, bescheiden zu werden,", "tokens": ["Still", ",", "ge\u00b7n\u00fcg\u00b7sam", ",", "be\u00b7schei\u00b7den", "zu", "wer\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.27": {"text": "Ja, dann vielleicht! \u2013", "tokens": ["Ja", ",", "dann", "viel\u00b7leicht", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "punct"], "pos": ["PTKANT", "$,", "ADV", "ADV", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.28": {"text": "Aber so \u2013", "tokens": ["A\u00b7ber", "so", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADV", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.29": {"text": "Mit Riesenwollen", "tokens": ["Mit", "Rie\u00b7sen\u00b7wol\u00b7len"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.30": {"text": "Und Zwergenk\u00f6nnen,", "tokens": ["Und", "Zwer\u00b7gen\u00b7k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.31": {"text": "Mit l\u00e4chelnden Lippen", "tokens": ["Mit", "l\u00e4\u00b7cheln\u00b7den", "Lip\u00b7pen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.32": {"text": "Und brechendem Herzen.", "tokens": ["Und", "bre\u00b7chen\u00b7dem", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.33": {"text": "Nein, o nein!", "tokens": ["Nein", ",", "o", "nein", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "FM", "PTKANT", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.34": {"text": "Hinaus mit mir,", "tokens": ["Hin\u00b7aus", "mit", "mir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.35": {"text": "Schnell, schnell dorthin,", "tokens": ["Schnell", ",", "schnell", "dor\u00b7thin", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.36": {"text": "Wo die gefallenen \u00c4ser", "tokens": ["Wo", "die", "ge\u00b7fal\u00b7le\u00b7nen", "\u00c4\u00b7ser"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.37": {"text": "Still vergraben werden ...", "tokens": ["Still", "ver\u00b7gra\u00b7ben", "wer\u00b7den", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}