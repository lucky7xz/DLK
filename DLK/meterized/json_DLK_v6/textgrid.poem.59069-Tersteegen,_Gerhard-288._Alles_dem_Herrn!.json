{"textgrid.poem.59069": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "288. Alles dem Herrn!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tu dein Gesch\u00e4fte treu und gern,", "tokens": ["Tu", "dein", "Ge\u00b7sch\u00e4f\u00b7te", "treu", "und", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So tatst du's dir, so tu's dem Herrn!", "tokens": ["So", "tatst", "du's", "dir", ",", "so", "tu's", "dem", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "$,", "ADV", "NE", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann mag's gelingen oder fehlen,", "tokens": ["Dann", "mag's", "ge\u00b7lin\u00b7gen", "o\u00b7der", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer Gott vergn\u00fcgt, darf sich nicht qu\u00e4len.", "tokens": ["Wer", "Gott", "ver\u00b7gn\u00fcgt", ",", "darf", "sich", "nicht", "qu\u00e4\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVPP", "$,", "VMFIN", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}