{"textgrid.poem.32282": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Die Planetenbewohner", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit s\u00fc\u00dfen Grillen sich erg\u00f6tzen,", "tokens": ["Mit", "s\u00fc\u00b7\u00dfen", "Gril\u00b7len", "sich", "er\u00b7g\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Einwohner in Planeten setzen,", "tokens": ["Ein\u00b7woh\u00b7ner", "in", "Pla\u00b7ne\u00b7ten", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Eh man aus sichern Gr\u00fcnden schlie\u00dft,", "tokens": ["Eh", "man", "aus", "si\u00b7chern", "Gr\u00fcn\u00b7den", "schlie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Da\u00df Wein in den Planeten ist:", "tokens": ["Da\u00df", "Wein", "in", "den", "Pla\u00b7ne\u00b7ten", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Das hei\u00dft zu fr\u00fch bev\u00f6lkern.", "tokens": ["Das", "hei\u00dft", "zu", "fr\u00fch", "be\u00b7v\u00f6l\u00b7kern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKA", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Freund, bringe nur zuerst aufs reine,", "tokens": ["Freund", ",", "brin\u00b7ge", "nur", "zu\u00b7erst", "aufs", "rei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "ADV", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df in den neuen Welten Weine,", "tokens": ["Da\u00df", "in", "den", "neu\u00b7en", "Wel\u00b7ten", "Wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie in der, die wir kennen, sind:", "tokens": ["Wie", "in", "der", ",", "die", "wir", "ken\u00b7nen", ",", "sind", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "$,", "PRELS", "PPER", "VVINF", "$,", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und glaube mir, dann kann ein Kind", "tokens": ["Und", "glau\u00b7be", "mir", ",", "dann", "kann", "ein", "Kind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADV", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf seine Trinker schlie\u00dfen.", "tokens": ["Auf", "sei\u00b7ne", "Trin\u00b7ker", "schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}