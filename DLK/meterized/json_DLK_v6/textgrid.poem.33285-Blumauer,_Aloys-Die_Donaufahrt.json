os": ["KON", "VVFIN", "NN", "KON", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kein Wunder der Liebe war je so gro\u00df,", "tokens": ["Kein", "Wun\u00b7der", "der", "Lie\u00b7be", "war", "je", "so", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Das nicht aus dem Urborn der Traube flo\u00df.", "tokens": ["Das", "nicht", "aus", "dem", "Ur\u00b7born", "der", "Trau\u00b7be", "flo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.24": {"line.1": {"text": "Heil uns, Heil uns, du Mutterland,", "tokens": ["Heil", "uns", ",", "Heil", "uns", ",", "du", "Mut\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "PPER", "$,", "PPER", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Da\u00df du zu Kindern uns ernannt!", "tokens": ["Da\u00df", "du", "zu", "Kin\u00b7dern", "uns", "er\u00b7nannt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Antlitz schm\u00fccket hohe Zier,", "tokens": ["Dein", "Ant\u00b7litz", "schm\u00fc\u00b7cket", "ho\u00b7he", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Segensf\u00fclle wohnt in dir:", "tokens": ["Und", "Se\u00b7gens\u00b7f\u00fcl\u00b7le", "wohnt", "in", "dir", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "De\u00df' freuen wir Kinder uns dankbarlich,", "tokens": ["De\u00df'", "freu\u00b7en", "wir", "Kin\u00b7der", "uns", "dank\u00b7bar\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "NN", "PPER", "ADJD", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und lieben, und ehren und preisen dich!", "tokens": ["Und", "lie\u00b7ben", ",", "und", "eh\u00b7ren", "und", "prei\u00b7sen", "dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VVINF", "KON", "VVFIN", "PPER", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}