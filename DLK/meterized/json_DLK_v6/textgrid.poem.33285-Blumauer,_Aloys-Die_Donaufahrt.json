{"textgrid.poem.33285": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Die Donaufahrt", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sag' an, mein Lied! wo fern und nah'", "tokens": ["Sag'", "an", ",", "mein", "Lied", "!", "wo", "fern", "und", "nah'"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "PWAV", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich Gottes hohe Wunder sah,", "tokens": ["Ich", "Got\u00b7tes", "ho\u00b7he", "Wun\u00b7der", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo ich die Erde, sch\u00f6n geschm\u00fcckt,", "tokens": ["Wo", "ich", "die", "Er\u00b7de", ",", "sch\u00f6n", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In ihrem Feyerkleid' erblickt:", "tokens": ["In", "ih\u00b7rem", "Feyer\u00b7kleid'", "er\u00b7blickt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Du Donau, du zeigtest die Holde mir,", "tokens": ["Du", "Do\u00b7nau", ",", "du", "zeig\u00b7test", "die", "Hol\u00b7de", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$,", "PPER", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "De\u00df' preiset mein Sang dich, und danket dir!", "tokens": ["De\u00df'", "prei\u00b7set", "mein", "Sang", "dich", ",", "und", "dan\u00b7ket", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "PPER", "$,", "KON", "VVFIN", "PPER", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Das Osterland auf deinem Lauf", "tokens": ["Das", "Os\u00b7ter\u00b7land", "auf", "dei\u00b7nem", "Lauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "That all' mir meinen Brautschatz auf,", "tokens": ["That", "all'", "mir", "mei\u00b7nen", "Braut\u00b7schatz", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und rief mir zu: schau' auf, und sieh", "tokens": ["Und", "rief", "mir", "zu", ":", "schau'", "auf", ",", "und", "sieh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$,", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des hohen Sch\u00f6pfers Gallerie!", "tokens": ["Des", "ho\u00b7hen", "Sch\u00f6p\u00b7fers", "Gal\u00b7le\u00b7rie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Bilder auf Bilder in bunter Reih'", "tokens": ["Und", "Bil\u00b7der", "auf", "Bil\u00b7der", "in", "bun\u00b7ter", "Reih'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Entstanden, und eilten vor mir vorbei.", "tokens": ["Ent\u00b7stan\u00b7den", ",", "und", "eil\u00b7ten", "vor", "mir", "vor\u00b7bei", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Bald vor mir hin ein reiches Feld,", "tokens": ["Bald", "vor", "mir", "hin", "ein", "rei\u00b7ches", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Gottes Segen wohlbestellt,", "tokens": ["Mit", "Got\u00b7tes", "Se\u00b7gen", "wohl\u00b7be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und weiter hin auf H\u00fcgeln gro\u00df", "tokens": ["Und", "wei\u00b7ter", "hin", "auf", "H\u00fc\u00b7geln", "gro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hoch aufgeth\u00fcrmt ein m\u00e4chtig Schlo\u00df:", "tokens": ["Hoch", "auf\u00b7ge\u00b7th\u00fcrmt", "ein", "m\u00e4ch\u00b7tig", "Schlo\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und d'r\u00fcber hin, h\u00f6her im fernen Blau,", "tokens": ["Und", "d'\u00b7r\u00fcber", "hin", ",", "h\u00f6\u00b7her", "im", "fer\u00b7nen", "Blau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "PTKVZ", "$,", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Der Berge sich th\u00fcrmender Wolkenbau.", "tokens": ["Der", "Ber\u00b7ge", "sich", "th\u00fcr\u00b7men\u00b7der", "Wol\u00b7ken\u00b7bau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Bald engumgr\u00e4nzt ein l\u00e4ndlich Bild,", "tokens": ["Bald", "en\u00b7gum\u00b7gr\u00e4nzt", "ein", "l\u00e4nd\u00b7lich", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In tausendfaches Gr\u00fcn geh\u00fcllt:", "tokens": ["In", "tau\u00b7send\u00b7fa\u00b7ches", "Gr\u00fcn", "ge\u00b7h\u00fcllt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier Gras, da Quell die Wies' entlang,", "tokens": ["Hier", "Gras", ",", "da", "Quell", "die", "Wies'", "ent\u00b7lang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "KOUS", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der frohen Heerde Speis' und Trank;", "tokens": ["Der", "fro\u00b7hen", "Heer\u00b7de", "Speis'", "und", "Trank", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Mahder singende Schnitter viel,", "tokens": ["Und", "Mah\u00b7der", "sin\u00b7gen\u00b7de", "Schnit\u00b7ter", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Daneben der Dorfjugend Schaukelspiel.", "tokens": ["Da\u00b7ne\u00b7ben", "der", "Dorf\u00b7ju\u00b7gend", "Schau\u00b7kel\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Bald sinkt in Eb'nen Berg und Baum,", "tokens": ["Bald", "sinkt", "in", "Eb'\u00b7nen", "Berg", "und", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Auge sucht und schaut sie kaum:", "tokens": ["Das", "Au\u00b7ge", "sucht", "und", "schaut", "sie", "kaum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sieh W\u00e4lder nun, wie Stauden gro\u00df,", "tokens": ["Sieh", "W\u00e4l\u00b7der", "nun", ",", "wie", "Stau\u00b7den", "gro\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "$,", "PWAV", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich bergen in der Erde Schoos:", "tokens": ["Sich", "ber\u00b7gen", "in", "der", "Er\u00b7de", "Schoos", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die h\u00f6chsten Gebirge schlie\u00dft winzigklein", "tokens": ["Die", "h\u00f6chs\u00b7ten", "Ge\u00b7bir\u00b7ge", "schlie\u00dft", "win\u00b7zig\u00b7klein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Der w\u00f6lbende Bogen des Himmels ein.", "tokens": ["Der", "w\u00f6l\u00b7ben\u00b7de", "Bo\u00b7gen", "des", "Him\u00b7mels", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "In weiten Betten gro\u00df und hehr,", "tokens": ["In", "wei\u00b7ten", "Bet\u00b7ten", "gro\u00df", "und", "hehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tritt hier der sch\u00f6ne Strom einher:", "tokens": ["Tritt", "hier", "der", "sch\u00f6\u00b7ne", "Strom", "ein\u00b7her", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sieh, wie er Haide, Wies' und Feld", "tokens": ["Sieh", ",", "wie", "er", "Hai\u00b7de", ",", "Wies'", "und", "Feld"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "PPER", "NE", "$,", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In hundert m\u00e4cht'gen Armen h\u00e4lt.", "tokens": ["In", "hun\u00b7dert", "m\u00e4cht'\u00b7gen", "Ar\u00b7men", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie freu'n, ihn am Busen, die Inseln sich,", "tokens": ["Wie", "freu'n", ",", "ihn", "am", "Bu\u00b7sen", ",", "die", "In\u00b7seln", "sich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PPER", "APPRART", "NN", "$,", "ART", "NN", "PRF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Wie tr\u00e4nkt er und pflegt er sie m\u00fctterlich!", "tokens": ["Wie", "tr\u00e4nkt", "er", "und", "pflegt", "er", "sie", "m\u00fct\u00b7ter\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Und weiter hin ein endlos Thal", "tokens": ["Und", "wei\u00b7ter", "hin", "ein", "end\u00b7los", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erwartet seine Wasser all':", "tokens": ["Er\u00b7war\u00b7tet", "sei\u00b7ne", "Was\u00b7ser", "all'", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sieh, wie der Strom die Arme schlie\u00dft,", "tokens": ["Sieh", ",", "wie", "der", "Strom", "die", "Ar\u00b7me", "schlie\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und seines Abgrunds H\u00e4lfte mi\u00dft;", "tokens": ["Und", "sei\u00b7nes", "Ab\u00b7grunds", "H\u00e4lf\u00b7te", "mi\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch m\u00e4chtiger leiten und g\u00e4ngeln ihn", "tokens": ["Doch", "m\u00e4ch\u00b7ti\u00b7ger", "lei\u00b7ten", "und", "g\u00e4n\u00b7geln", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "VVINF", "KON", "VVFIN", "PPER"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Die D\u00e4mme der Berge bald her, bald hin.", "tokens": ["Die", "D\u00e4m\u00b7me", "der", "Ber\u00b7ge", "bald", "her", ",", "bald", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "PTKVZ", "$,", "ADV", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.8": {"line.1": {"text": "Ringsum im Kreise th\u00fcrmen sich", "tokens": ["Ring\u00b7sum", "im", "Krei\u00b7se", "th\u00fcr\u00b7men", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PRF"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Hier Berg' auf Berge schauerlich,", "tokens": ["Hier", "Ber\u00b7g'", "auf", "Ber\u00b7ge", "schau\u00b7er\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sieh hier von hoher Felsenwand", "tokens": ["Sieh", "hier", "von", "ho\u00b7her", "Fel\u00b7sen\u00b7wand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Tages H\u00e4lfte weggebannt,", "tokens": ["Die", "Ta\u00b7ges", "H\u00e4lf\u00b7te", "weg\u00b7ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Umhangen die Berge in stiller Pracht", "tokens": ["Um\u00b7han\u00b7gen", "die", "Ber\u00b7ge", "in", "stil\u00b7ler", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Mit f\u00fcrchterlich finsterer Tannennacht.", "tokens": ["Mit", "f\u00fcrch\u00b7ter\u00b7lich", "fins\u00b7te\u00b7rer", "Tan\u00b7nen\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.9": {"line.1": {"text": "Sieh Pyramiden, gr\u00fcn umschirmt,", "tokens": ["Sieh", "Py\u00b7ra\u00b7mi\u00b7den", ",", "gr\u00fcn", "um\u00b7schirmt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Gottes Finger aufgeth\u00fcrmt,", "tokens": ["Von", "Got\u00b7tes", "Fin\u00b7ger", "auf\u00b7ge\u00b7th\u00fcrmt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Spitzen reichen wolkenan,", "tokens": ["Die", "Spit\u00b7zen", "rei\u00b7chen", "wol\u00b7ke\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So weit das Auge reichen kann;", "tokens": ["So", "weit", "das", "Au\u00b7ge", "rei\u00b7chen", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und hoch auf den Spitzen, den Augen graut,", "tokens": ["Und", "hoch", "auf", "den", "Spit\u00b7zen", ",", "den", "Au\u00b7gen", "graut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", 