{"textgrid.poem.50650": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Uneigenn\u00fctzige Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die schlagen in den Wind der Liebe Widerschein.", "tokens": ["Die", "schla\u00b7gen", "in", "den", "Wind", "der", "Lie\u00b7be", "Wi\u00b7der\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du kannst, sei, wie du willst, geliebt, ungl\u00fccklich sein,", "tokens": ["Du", "kannst", ",", "sei", ",", "wie", "du", "willst", ",", "ge\u00b7liebt", ",", "un\u00b7gl\u00fcck\u00b7lich", "sein", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "VAFIN", "$,", "PWAV", "PPER", "VMFIN", "$,", "VVPP", "$,", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sei, wie du willst, ungl\u00fccklich, dennoch lieben.", "tokens": ["Sei", ",", "wie", "du", "willst", ",", "un\u00b7gl\u00fcck\u00b7lich", ",", "den\u00b7noch", "lie\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWAV", "PPER", "VMFIN", "$,", "ADJD", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}