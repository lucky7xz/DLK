{"dta.poem.425": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An  Albanien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Albanie dein Glantz die Sterne schaam-roht macht/", "tokens": ["Al\u00b7ba\u00b7nie", "dein", "Glantz", "die", "Ster\u00b7ne", "schaam\u00b7roht", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ART", "NN", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "In deinem Angesicht die ", "tokens": ["In", "dei\u00b7nem", "An\u00b7ge\u00b7sicht", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Cupido streut da aus Jesmin und Anemonen", "tokens": ["Cu\u00b7pi\u00b7do", "streut", "da", "aus", "Jes\u00b7min", "und", "A\u00b7ne\u00b7mo\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "APPR", "NE", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Lippen sind der Thron wo Zucht und Sch\u00f6nheit wohnen.", "tokens": ["Die", "Lip\u00b7pen", "sind", "der", "Thron", "wo", "Zucht", "und", "Sch\u00f6n\u00b7heit", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "PWAV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der wohlgezierte Mund kein unrein Wort verspillt", "tokens": ["Der", "wohl\u00b7ge\u00b7zier\u00b7te", 