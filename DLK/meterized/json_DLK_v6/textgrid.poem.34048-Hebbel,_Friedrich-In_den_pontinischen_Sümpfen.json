{"textgrid.poem.34048": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "In den pontinischen S\u00fcmpfen", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lachen erwartete ich, was fand ich? Strotzende Wiesen,", "tokens": ["La\u00b7chen", "er\u00b7war\u00b7te\u00b7te", "ich", ",", "was", "fand", "ich", "?", "Strot\u00b7zen\u00b7de", "Wie\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PWS", "VVFIN", "PPER", "$.", "NN", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Selten wuchernden Schilf, kaum noch die Spuren von Sumpf,", "tokens": ["Sel\u00b7ten", "wu\u00b7chern\u00b7den", "Schilf", ",", "kaum", "noch", "die", "Spu\u00b7ren", "von", "Sumpf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADV", "ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Aber kr\u00e4ftige B\u00e4ume, des Erdreichs Mark mir bezeugend,", "tokens": ["A\u00b7ber", "kr\u00e4f\u00b7ti\u00b7ge", "B\u00e4u\u00b7me", ",", "des", "Er\u00b7dreichs", "Mark", "mir", "be\u00b7zeu\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "ART", "NN", "NN", "PPER", "VVPP", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Korn auch, freilich nur da, wo man gepfl\u00fcgt und ges\u00e4't.", "tokens": ["Korn", "auch", ",", "frei\u00b7lich", "nur", "da", ",", "wo", "man", "ge\u00b7pfl\u00fcgt", "und", "ge\u00b7s\u00e4'", "t."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["NN", "ADV", "$,", "ADV", "ADV", "ADV", "$,", "PWAV", "PIS", "VVPP", "KON", "XY", "XY"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}}}}