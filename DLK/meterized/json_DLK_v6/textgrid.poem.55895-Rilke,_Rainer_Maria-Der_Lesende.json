{"textgrid.poem.55895": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Der Lesende", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich las schon lang. Seit dieser Nachmittag,", "tokens": ["Ich", "las", "schon", "lang", ".", "Seit", "die\u00b7ser", "Nach\u00b7mit\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$.", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "mit Regen rauschend, an den Fenstern lag.", "tokens": ["mit", "Re\u00b7gen", "rau\u00b7schend", ",", "an", "den", "Fens\u00b7tern", "lag", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vom Winde drau\u00dfen h\u00f6rte ich nichts mehr:", "tokens": ["Vom", "Win\u00b7de", "drau\u00b7\u00dfen", "h\u00f6r\u00b7te", "ich", "nichts", "mehr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PPER", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "mein Buch war schwer.", "tokens": ["mein", "Buch", "war", "schwer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ich sah ihm in die Bl\u00e4tter wie in Mienen,", "tokens": ["Ich", "sah", "ihm", "in", "die", "Bl\u00e4t\u00b7ter", "wie", "in", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "KOKOM", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "die dunkel werden von Nachdenklichkeit,", "tokens": ["die", "dun\u00b7kel", "wer\u00b7den", "von", "Nach\u00b7denk\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "und um mein Lesen staute sich die Zeit. \u2013", "tokens": ["und", "um", "mein", "Le\u00b7sen", "stau\u00b7te", "sich", "die", "Zeit", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "PRF", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Auf einmal sind die Seiten \u00fcberschienen,", "tokens": ["Auf", "ein\u00b7mal", "sind", "die", "Sei\u00b7ten", "\u00fc\u00b7bersc\u00b7hie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "und statt der bangen Wortverworrenheit", "tokens": ["und", "statt", "der", "ban\u00b7gen", "Wort\u00b7ver\u00b7wor\u00b7ren\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "steht: Abend, Abend... \u00fcberall auf ihnen.", "tokens": ["steht", ":", "A\u00b7bend", ",", "A\u00b7bend", "...", "\u00fc\u00b7be\u00b7rall", "auf", "ih\u00b7nen", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "NN", "$,", "NN", "$(", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ich schau noch nicht hinaus, und doch zerrei\u00dfen", "tokens": ["Ich", "schau", "noch", "nicht", "hin\u00b7aus", ",", "und", "doch", "zer\u00b7rei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "PTKVZ", "$,", "KON", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "die langen Zeilen, und die Worte rollen", "tokens": ["die", "lan\u00b7gen", "Zei\u00b7len", ",", "und", "die", "Wor\u00b7te", "rol\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "von ihren F\u00e4den fort, wohin sie wollen...", "tokens": ["von", "ih\u00b7ren", "F\u00e4\u00b7den", "fort", ",", "wo\u00b7hin", "sie", "wol\u00b7len", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,", "PWAV", "PPER", "VMFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Da wei\u00df ich es: \u00fcber den \u00fcbervollen", "tokens": ["Da", "wei\u00df", "ich", "es", ":", "\u00fc\u00b7ber", "den", "\u00fc\u00b7ber\u00b7vol\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$.", "APPR", "ART", "ADJA"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "gl\u00e4nzenden G\u00e4rten sind die Himmel weit;", "tokens": ["gl\u00e4n\u00b7zen\u00b7den", "G\u00e4r\u00b7ten", "sind", "die", "Him\u00b7mel", "weit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.16": {"text": "die Sonne hat noch einmal kommen sollen. \u2013", "tokens": ["die", "Son\u00b7ne", "hat", "noch", "ein\u00b7mal", "kom\u00b7men", "sol\u00b7len", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Und jetzt wird Sommernacht, soweit man sieht:", "tokens": ["Und", "jetzt", "wird", "Som\u00b7mer\u00b7nacht", ",", "so\u00b7weit", "man", "sieht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "zu wenig Gruppen stellt sich das Verstreute,", "tokens": ["zu", "we\u00b7nig", "Grup\u00b7pen", "stellt", "sich", "das", "Ver\u00b7streu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "dunkel, auf langen Wegen, gehn die Leute,", "tokens": ["dun\u00b7kel", ",", "auf", "lan\u00b7gen", "We\u00b7gen", ",", "gehn", "die", "Leu\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "und seltsam weit, als ob es mehr bedeute,", "tokens": ["und", "selt\u00b7sam", "weit", ",", "als", "ob", "es", "mehr", "be\u00b7deu\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "$,", "KOKOM", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "h\u00f6rt man das Wenige, das noch geschieht.", "tokens": ["h\u00f6rt", "man", "das", "We\u00b7ni\u00b7ge", ",", "das", "noch", "ge\u00b7schieht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "PIAT", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Und wenn ich jetzt vom Buch die Augen hebe,", "tokens": ["Und", "wenn", "ich", "jetzt", "vom", "Buch", "die", "Au\u00b7gen", "he\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "wird nichts befremdlich sein und alles gro\u00df.", "tokens": ["wird", "nichts", "be\u00b7fremd\u00b7lich", "sein", "und", "al\u00b7les", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "VAINF", "KON", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dort drau\u00dfen ist, was ich hier drinnen lebe,", "tokens": ["Dort", "drau\u00b7\u00dfen", "ist", ",", "was", "ich", "hier", "drin\u00b7nen", "le\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "$,", "PWS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und hier und dort ist alles grenzenlos;", "tokens": ["und", "hier", "und", "dort", "ist", "al\u00b7les", "gren\u00b7zen\u00b7los", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "ADV", "VAFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "nur da\u00df ich mich noch mehr damit verwebe,", "tokens": ["nur", "da\u00df", "ich", "mich", "noch", "mehr", "da\u00b7mit", "ver\u00b7we\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PRF", "ADV", "ADV", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "wenn meine Blicke an die Dinge passen", "tokens": ["wenn", "mei\u00b7ne", "Bli\u00b7cke", "an", "die", "Din\u00b7ge", "pas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "und an die ernste Einfachheit der Massen, \u2013", "tokens": ["und", "an", "die", "erns\u00b7te", "Ein\u00b7fach\u00b7heit", "der", "Mas\u00b7sen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "da w\u00e4chst die Erde \u00fcber sich hinaus.", "tokens": ["da", "w\u00e4chst", "die", "Er\u00b7de", "\u00fc\u00b7ber", "sich", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Den ganzen Himmel scheint sie zu umfassen:", "tokens": ["Den", "gan\u00b7zen", "Him\u00b7mel", "scheint", "sie", "zu", "um\u00b7fas\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "der erste Stern ist wie das letzte Haus.", "tokens": ["der", "ers\u00b7te", "Stern", "ist", "wie", "das", "letz\u00b7te", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}