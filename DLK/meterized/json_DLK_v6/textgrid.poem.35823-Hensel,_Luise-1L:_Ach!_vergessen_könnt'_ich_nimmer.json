{"textgrid.poem.35823": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach! vergessen k\u00f6nnt' ich nimmer", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach! vergessen k\u00f6nnt' ich nimmer", "tokens": ["Ach", "!", "ver\u00b7ges\u00b7sen", "k\u00f6nnt'", "ich", "nim\u00b7mer"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVINF", "VMFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seiner Liebe, Seiner Pein.", "tokens": ["Sei\u00b7ner", "Lie\u00b7be", ",", "Sei\u00b7ner", "Pein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sein gedenken will ich immer;", "tokens": ["Sein", "ge\u00b7den\u00b7ken", "will", "ich", "im\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "VMFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Garten, Feld und Wald und Zimmer", "tokens": ["Gar\u00b7ten", ",", "Feld", "und", "Wald", "und", "Zim\u00b7mer"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll mir stets ein Bethaus sein.", "tokens": ["Soll", "mir", "stets", "ein", "Be\u00b7thaus", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VAINF", "$."], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "St\u00fcndlich will ich Ihn begr\u00fc\u00dfen:", "tokens": ["St\u00fcnd\u00b7lich", "will", "ich", "Ihn", "be\u00b7gr\u00fc\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbsei gepriesen, s\u00fc\u00dfer Freund!\u00ab", "tokens": ["\u00bb", "sei", "ge\u00b7prie\u00b7sen", ",", "s\u00fc\u00b7\u00dfer", "Freund", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "VVPP", "$,", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Blumen, die da sprie\u00dfen,", "tokens": ["Al\u00b7le", "Blu\u00b7men", ",", "die", "da", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Will ich mit dem Thau begie\u00dfen,", "tokens": ["Will", "ich", "mit", "dem", "Thau", "be\u00b7gie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Den um Ihn mein Auge weint. \u2013", "tokens": ["Den", "um", "Ihn", "mein", "Au\u00b7ge", "weint", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "APPR", "PPER", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Keine Gaben, keine Freuden,", "tokens": ["Kei\u00b7ne", "Ga\u00b7ben", ",", "kei\u00b7ne", "Freu\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Keine Krone dieser Welt!", "tokens": ["Kei\u00b7ne", "Kro\u00b7ne", "die\u00b7ser", "Welt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freudig will ich Alles meiden,", "tokens": ["Freu\u00b7dig", "will", "ich", "Al\u00b7les", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will nur Er von mir nicht scheiden,", "tokens": ["Will", "nur", "Er", "von", "mir", "nicht", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "APPR", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Den mein Herz so innig h\u00e4lt.", "tokens": ["Den", "mein", "Herz", "so", "in\u00b7nig", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wer ein Tr\u00f6pflein durfte trinken,", "tokens": ["Wer", "ein", "Tr\u00f6pf\u00b7lein", "durf\u00b7te", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herr! aus Deinem Gnadenmeer,", "tokens": ["Herr", "!", "aus", "Dei\u00b7nem", "Gna\u00b7den\u00b7meer", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der mu\u00df ganz darin versinken,", "tokens": ["Der", "mu\u00df", "ganz", "da\u00b7rin", "ver\u00b7sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "PAV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Folgen einzig Deinen Winken", "tokens": ["Fol\u00b7gen", "ein\u00b7zig", "Dei\u00b7nen", "Win\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ohne Sinne, ohn' Geh\u00f6r.", "tokens": ["Oh\u00b7ne", "Sin\u00b7ne", ",", "ohn'", "Ge\u00b7h\u00f6r", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und ich kann Dich nimmer lassen,", "tokens": ["Und", "ich", "kann", "Dich", "nim\u00b7mer", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ewig halt' ich Dich umarmt.", "tokens": ["E\u00b7wig", "halt'", "ich", "Dich", "um\u00b7armt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wolltest Du mich fliehn und hassen,", "tokens": ["Woll\u00b7test", "Du", "mich", "fliehn", "und", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00fc\u00dft' ich noch Dein Kreuz umfassen,", "tokens": ["M\u00fc\u00dft'", "ich", "noch", "Dein", "Kreuz", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis es jeden Stein erbarmt.", "tokens": ["Bis", "es", "je\u00b7den", "Stein", "er\u00b7barmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "W\u00e4rst Du nicht im Himmel drinnen,", "tokens": ["W\u00e4rst", "Du", "nicht", "im", "Him\u00b7mel", "drin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "APPRART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimmer sehnt' ich mich hinein. \u2013", "tokens": ["Nim\u00b7mer", "sehnt'", "ich", "mich", "hin\u00b7ein", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "All mein Wissen, all mein Sinnen,", "tokens": ["All", "mein", "Wis\u00b7sen", ",", "all", "mein", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Herr! la\u00df ganz in Dich verrinnen,", "tokens": ["Herr", "!", "la\u00df", "ganz", "in", "Dich", "ver\u00b7rin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVIMP", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ganz in Dir verloren sein.", "tokens": ["Ganz", "in", "Dir", "ver\u00b7lo\u00b7ren", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}