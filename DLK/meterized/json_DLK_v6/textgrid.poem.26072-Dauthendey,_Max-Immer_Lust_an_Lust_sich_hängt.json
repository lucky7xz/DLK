{"textgrid.poem.26072": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Immer Lust an Lust sich h\u00e4ngt", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alle Dinge k\u00f6nnen sehen. Sag nicht, da\u00df sie blind dastehen.", "tokens": ["Al\u00b7le", "Din\u00b7ge", "k\u00f6n\u00b7nen", "se\u00b7hen", ".", "Sag", "nicht", ",", "da\u00df", "sie", "blind", "das\u00b7te\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "VVINF", "$.", "NN", "PTKNEG", "$,", "KOUS", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Sag nicht, da\u00df sie dunkel gehen. H\u00e4user, B\u00e4ume, Wege, Wind,", "tokens": ["Sag", "nicht", ",", "da\u00df", "sie", "dun\u00b7kel", "ge\u00b7hen", ".", "H\u00e4u\u00b7ser", ",", "B\u00e4u\u00b7me", ",", "We\u00b7ge", ",", "Wind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "KOUS", "PPER", "ADJD", "VVINF", "$.", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "St\u00fchle, Tische, Bett und Spind, alle Dinge sehend sind.", "tokens": ["St\u00fch\u00b7le", ",", "Ti\u00b7sche", ",", "Bett", "und", "Spind", ",", "al\u00b7le", "Din\u00b7ge", "se\u00b7hend", "sind", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,", "PIAT", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Alle Dinge k\u00f6nnen denken. Nicht nur Stirnen Geist dir schenken,", "tokens": ["Al\u00b7le", "Din\u00b7ge", "k\u00f6n\u00b7nen", "den\u00b7ken", ".", "Nicht", "nur", "Stir\u00b7nen", "Geist", "dir", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "VVINF", "$.", "PTKNEG", "ADV", "NN", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Alle Dinge Geister lenken. Kleiner M\u00fccken grauer Zug,", "tokens": ["Al\u00b7le", "Din\u00b7ge", "Geis\u00b7ter", "len\u00b7ken", ".", "Klei\u00b7ner", "M\u00fc\u00b7cken", "grau\u00b7er", "Zug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVINF", "$.", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Spinnwebfaden leis im Flug; jeder Grashalm denk genug.", "tokens": ["Spinn\u00b7web\u00b7fa\u00b7den", "leis", "im", "Flug", ";", "je\u00b7der", "Gras\u00b7halm", "denk", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "$.", "PIAT", "NN", "VVFIN", "ADV", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Und es lieben alle Dinge. Wie die V\u00f6gel mit Gesinge", "tokens": ["Und", "es", "lie\u00b7ben", "al\u00b7le", "Din\u00b7ge", ".", "Wie", "die", "V\u00f6\u00b7gel", "mit", "Ge\u00b7sin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "$.", "PWAV", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Liebt sich alle Welt im Ringe. Eines hin zum andern dr\u00e4ngt,", "tokens": ["Liebt", "sich", "al\u00b7le", "Welt", "im", "Rin\u00b7ge", ".", "Ei\u00b7nes", "hin", "zum", "an\u00b7dern", "dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIAT", "NN", "APPRART", "NN", "$.", "PIS", "ADV", "APPRART", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Jedes seine Lust sich f\u00e4ngt. Immer Lust an Lust sich h\u00e4ngt.", "tokens": ["Je\u00b7des", "sei\u00b7ne", "Lust", "sich", "f\u00e4ngt", ".", "Im\u00b7mer", "Lust", "an", "Lust", "sich", "h\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "PRF", "VVFIN", "$.", "ADV", "NN", "APPR", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}}}}}