{"textgrid.poem.53356": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "4. Klagelied", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Indessen geht bey Nacht die Geistligkeit zu Raht,", "tokens": ["In\u00b7des\u00b7sen", "geht", "bey", "Nacht", "die", "Geist\u00b7lig\u00b7keit", "zu", "Raht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ihre Z\u00e4hne wetzt auff diese b\u00f6se That,", "tokens": ["Die", "ih\u00b7re", "Z\u00e4h\u00b7ne", "wetzt", "auff", "die\u00b7se", "b\u00f6\u00b7se", "That", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie horchen zu, gleich wie, der V\u00f6gel sucht zu fangen,", "tokens": ["Sie", "hor\u00b7chen", "zu", ",", "gleich", "wie", ",", "der", "V\u00f6\u00b7gel", "sucht", "zu", "fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ADV", "KOKOM", "$,", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Beym Garn sehr wachsam liegt und lauschet mit verlangen.", "tokens": ["Beym", "Garn", "sehr", "wach\u00b7sam", "liegt", "und", "lau\u00b7schet", "mit", "ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "VVFIN", "KON", "VVFIN", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Offt schelten sie die Knecht' als w\u00e4ren sie zu faul,", "tokens": ["Offt", "schel\u00b7ten", "sie", "die", "Knecht'", "als", "w\u00e4\u00b7ren", "sie", "zu", "faul", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "KOKOM", "VAFIN", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Offt reisst die lange weil ihr m\u00f6rderisches Maul,", "tokens": ["Offt", "reisst", "die", "lan\u00b7ge", "weil", "ihr", "m\u00f6r\u00b7de\u00b7ri\u00b7sches", "Maul", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADV", "KOUS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch eilen diese sehr, der Herr wird fortgerissen,", "tokens": ["Doch", "ei\u00b7len", "die\u00b7se", "sehr", ",", "der", "Herr", "wird", "fort\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "ADV", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vnd folgt, so fern die Band' ihn lassen, die ihn schliessen,", "tokens": ["Vnd", "folgt", ",", "so", "fern", "die", "Band'", "ihn", "las\u00b7sen", ",", "die", "ihn", "schlies\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "ADJD", "ART", "NN", "PPER", "VVINF", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er geht gehorsam mit, es mag der Landsknecht ihn", "tokens": ["Er", "geht", "ge\u00b7hor\u00b7sam", "mit", ",", "es", "mag", "der", "Lands\u00b7knecht", "ihn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$,", "PPER", "VMFIN", "ART", "NN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Fort \u00fcber Hals und Kopff gleich reissen oder ziehn.", "tokens": ["Fort", "\u00fc\u00b7ber", "Hals", "und", "Kopff", "gleich", "reis\u00b7sen", "o\u00b7der", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "ADV", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das gantze Haus mu\u00df schon beym Caiphas erschallen,", "tokens": ["Das", "gant\u00b7ze", "Haus", "mu\u00df", "schon", "beym", "Cai\u00b7phas", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Der Priesterschafft beginnt das Hertz erfrewt zu wallen.", "tokens": ["Der", "Pries\u00b7ter\u00b7schafft", "be\u00b7ginnt", "das", "Hertz", "er\u00b7frewt", "zu", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Auff eines Vntergang gehn, die sonst uneins seyn,", "tokens": ["Auff", "ei\u00b7nes", "Vn\u00b7ter\u00b7gang", "gehn", ",", "die", "sonst", "un\u00b7eins", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,", "PRELS", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mit Sinnen und mit Rath zusammen Freundschafft ein.", "tokens": ["Mit", "Sin\u00b7nen", "und", "mit", "Rath", "zu\u00b7sam\u00b7men", "Freund\u00b7schafft", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Des Todes Vrtheil wird von allen ihm gesprochen,", "tokens": ["Des", "To\u00b7des", "Vrtheil", "wird", "von", "al\u00b7len", "ihm", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "APPR", "PIS", "PPER", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Er wird gef\u00fchrt, der Stab ist \u00fcber ihn gebrochen.", "tokens": ["Er", "wird", "ge\u00b7f\u00fchrt", ",", "der", "Stab", "ist", "\u00fc\u00b7ber", "ihn", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ART", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Er stehet mitten ein, der hohen Sch\u00f6nheit Liecht", "tokens": ["Er", "ste\u00b7het", "mit\u00b7ten", "ein", ",", "der", "ho\u00b7hen", "Sch\u00f6n\u00b7heit", "Liecht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Vnd alle Zieraht mahlt sein heiligs Angesicht,", "tokens": ["Vnd", "al\u00b7le", "Zier\u00b7aht", "mahlt", "sein", "hei\u00b7ligs", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Viel Zeugen sind erkaufft, die f\u00e4lschlich ihn beliegen,", "tokens": ["Viel", "Zeu\u00b7gen", "sind", "er\u00b7kaufft", ",", "die", "f\u00e4lschlich", "ihn", "be\u00b7lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,", "PRELS", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Ihr Zeugni\u00df aber wil sich nicht zusammen f\u00fcgen.", "tokens": ["Ihr", "Zeug\u00b7ni\u00df", "a\u00b7ber", "wil", "sich", "nicht", "zu\u00b7sam\u00b7men", "f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VMFIN", "PRF", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Daher sie Christus auch mit schweigen wiederlegt,", "tokens": ["Da\u00b7her", "sie", "Chris\u00b7tus", "auch", "mit", "schwei\u00b7gen", "wie\u00b7der\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "NE", "ADV", "APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da\u00df sich ein jeder selbst mit seinen Worten schl\u00e4gt.", "tokens": ["Da\u00df", "sich", "ein", "je\u00b7der", "selbst", "mit", "sei\u00b7nen", "Wor\u00b7ten", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "PIS", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Da fragt ihn Caiphas, und schwert bey Gottes Leben,", "tokens": ["Da", "fragt", "ihn", "Cai\u00b7phas", ",", "und", "schwert", "bey", "Got\u00b7tes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "$,", "KON", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Du, spricht er: solst Bescheid und Antwort von dir geben,", "tokens": ["Du", ",", "spricht", "er", ":", "solst", "Be\u00b7scheid", "und", "Ant\u00b7wort", "von", "dir", "ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "PPER", "$.", "VMFIN", "NN", "KON", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Sag, bist du Gottes Sohn, des grossen Vaters Raht,", "tokens": ["Sag", ",", "bist", "du", "Got\u00b7tes", "Sohn", ",", "des", "gros\u00b7sen", "Va\u00b7ters", "Raht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "NN", "NN", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wie der gemeine Mann den falschen Glauben hat?", "tokens": ["Wie", "der", "ge\u00b7mei\u00b7ne", "Mann", "den", "fal\u00b7schen", "Glau\u00b7ben", "hat", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ja, spricht er. Caiphas hat da sein Kleid zerrissen.", "tokens": ["Ja", ",", "spricht", "er", ".", "Cai\u00b7phas", "hat", "da", "sein", "Kleid", "zer\u00b7ris\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "$.", "NE", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Weg, schreyen sie, mit ihm! sein Hal\u00df sol solches b\u00fcssen.", "tokens": ["Weg", ",", "schre\u00b7yen", "sie", ",", "mit", "ihm", "!", "sein", "Hal\u00df", "sol", "sol\u00b7ches", "b\u00fcs\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "APPR", "PPER", "$.", "PPOSAT", "NN", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der Raht steht eilends auff und redet Mord und Blut,", "tokens": ["Der", "Raht", "steht", "ei\u00b7lends", "auff", "und", "re\u00b7det", "Mord", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "KON", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Da man denn Gott in Schmach und Bande wieder thut.", "tokens": ["Da", "man", "denn", "Gott", "in", "Schmach", "und", "Ban\u00b7de", "wie\u00b7der", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "NN", "APPR", "NN", "KON", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wie wird mir, Christe, doch die Grausamkeit beschrieben,", "tokens": ["Wie", "wird", "mir", ",", "Chris\u00b7te", ",", "doch", "die", "Grau\u00b7sam\u00b7keit", "be\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "ADJA", "$,", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die Schl\u00e4g' und was man dir f\u00fcr Frevel zugetrieben,", "tokens": ["Die", "Schl\u00e4g'", "und", "was", "man", "dir", "f\u00fcr", "Fre\u00b7vel", "zu\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PWS", "PIS", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Als das Gesindchen blo\u00df mit dir die gantze Nacht,", "tokens": ["Als", "das", "Ge\u00b7sind\u00b7chen", "blo\u00df", "mit", "dir", "die", "gant\u00b7ze", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "In Hohn und Vbermuth aus Kurtzweil zugebracht;", "tokens": ["In", "Hohn", "und", "Vber\u00b7muth", "aus", "Kurt\u00b7zweil", "zu\u00b7ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Als du so manchen Streich auff deine zarte Wangen,", "tokens": ["Als", "du", "so", "man\u00b7chen", "Streich", "auff", "dei\u00b7ne", "zar\u00b7te", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Von der geharnschten Hand der KriegesKnecht empfangen?", "tokens": ["Von", "der", "ge\u00b7harnschten", "Hand", "der", "Krie\u00b7ges", "Knecht", "emp\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "NN", "VVPP", "$."], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.37": {"text": "Als man die Augen dir verh\u00fcllt' und sprach: errath,", "tokens": ["Als", "man", "die", "Au\u00b7gen", "dir", "ver\u00b7h\u00fcllt'", "und", "sprach", ":", "er\u00b7rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "PPER", "VVFIN", "KON", "VVFIN", "$.", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wer ist es von der Schaar der dich geschlagen hat?", "tokens": ["Wer", "ist", "es", "von", "der", "Schaar", "der", "dich", "ge\u00b7schla\u00b7gen", "hat", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "ART", "NN", "ART", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Vnd als ein Lotterbub frech, garstig, ungeschewet", "tokens": ["Vnd", "als", "ein", "Lot\u00b7ter\u00b7bub", "frech", ",", "gars\u00b7tig", ",", "un\u00b7ge\u00b7sche\u00b7wet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "$,", "ADJD", "$,", "ADJD"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.40": {"text": "Dir deinen Bart geraufft, dein Angesicht verspeyet?", "tokens": ["Dir", "dei\u00b7nen", "Bart", "ger\u00b7aufft", ",", "dein", "An\u00b7ge\u00b7sicht", "ver\u00b7spe\u00b7yet", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVPP", "$,", "PPOSAT", "NN", "VVFIN",