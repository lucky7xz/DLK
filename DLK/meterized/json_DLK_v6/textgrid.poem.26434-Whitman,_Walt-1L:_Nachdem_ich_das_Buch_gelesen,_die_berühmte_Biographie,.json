{"textgrid.poem.26434": {"metadata": {"author": {"name": "Whitman, Walt", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nachdem ich das Buch gelesen, die ber\u00fchmte Biographie,", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nachdem ich das Buch gelesen, die ber\u00fchmte Biographie,", "tokens": ["Nach\u00b7dem", "ich", "das", "Buch", "ge\u00b7le\u00b7sen", ",", "die", "be\u00b7r\u00fchm\u00b7te", "Bio\u00b7gra\u00b7phie", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Fragte ich mich: Also das ist es, was der Autor das Leben eines Menschen nennt?", "tokens": ["Frag\u00b7te", "ich", "mich", ":", "Al\u00b7so", "das", "ist", "es", ",", "was", "der", "Au\u00b7tor", "das", "Le\u00b7ben", "ei\u00b7nes", "Men\u00b7schen", "nennt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$.", "ADV", "PDS", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Und so wird vielleicht, wenn ich tot bin, dereinst einer \u00fcber mein Leben schreiben?", "tokens": ["Und", "so", "wird", "viel\u00b7leicht", ",", "wenn", "ich", "tot", "bin", ",", "de\u00b7reinst", "ei\u00b7ner", "\u00fc\u00b7ber", "mein", "Le\u00b7ben", "schrei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "ADV", "ART", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+--++-+--+--+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "(als ob irgend jemand irgend etwas von meinem Leben wirklich w\u00fc\u00dfte!)", "tokens": ["(", "als", "ob", "ir\u00b7gend", "je\u00b7mand", "ir\u00b7gend", "et\u00b7was", "von", "mei\u00b7nem", "Le\u00b7ben", "wirk\u00b7lich", "w\u00fc\u00df\u00b7te", "!", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOKOM", "KOUS", "ADV", "PIS", "ADV", "PIS", "APPR", "PPOSAT", "NN", "ADJD", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+--+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.5": {"text": "Wei\u00df ich doch selber, so scheint mir's oft, wenig oder nichts von meinem wirklichen Leben;", "tokens": ["Wei\u00df", "ich", "doch", "sel\u00b7ber", ",", "so", "scheint", "mir's", "oft", ",", "we\u00b7nig", "o\u00b7der", "nichts", "von", "mei\u00b7nem", "wirk\u00b7li\u00b7chen", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "ADV", "VVFIN", "NE", "ADV", "$,", "PIS", "KON", "PIS", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+-+-+-+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "Nur ein paar Fingerzeige, einige zerstreute schwache Anhaltspunkte auf weiten Umwegen", "tokens": ["Nur", "ein", "paar", "Fin\u00b7ger\u00b7zei\u00b7ge", ",", "ei\u00b7ni\u00b7ge", "zer\u00b7streu\u00b7te", "schwa\u00b7che", "An\u00b7halts\u00b7punk\u00b7te", "auf", "wei\u00b7ten", "Um\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "PIAT", "NN", "$,", "PIAT", "ADJA", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-+-+--+--+-", "measure": "iambic.octa.plus"}, "line.7": {"text": "Suche ich hier f\u00fcr mich selbst zu entdecken.", "tokens": ["Su\u00b7che", "ich", "hier", "f\u00fcr", "mich", "selbst", "zu", "ent\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}}}}