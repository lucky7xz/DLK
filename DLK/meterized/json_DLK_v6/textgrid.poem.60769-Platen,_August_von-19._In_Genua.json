{"textgrid.poem.60769": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "19. In Genua", "genre": "verse", "period": "N.A.", "pub_year": 1828, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, wer wiese zur\u00fcck, wie entw\u00f6hnt die Brust auch", "tokens": ["Ach", ",", "wer", "wie\u00b7se", "zu\u00b7r\u00fcck", ",", "wie", "ent\u00b7w\u00f6hnt", "die", "Brust", "auch"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWS", "VVFIN", "PTKVZ", "$,", "PWAV", "VVPP", "ART", "NN", "ADV"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sei durch ewigen Gram und der Welt Entt\u00e4uschung,", "tokens": ["Sei", "durch", "e\u00b7wi\u00b7gen", "Gram", "und", "der", "Welt", "Ent\u00b7t\u00e4u\u00b7schung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Wer allm\u00e4chtige Sehnsucht,", "tokens": ["Wer", "all\u00b7m\u00e4ch\u00b7ti\u00b7ge", "Sehn\u00b7sucht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "S\u00fc\u00dfe Begierde zur\u00fcck?", "tokens": ["S\u00fc\u00b7\u00dfe", "Be\u00b7gier\u00b7de", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn voll magischer Kraft, in dem Land der Sch\u00f6nheit,", "tokens": ["Wenn", "voll", "ma\u00b7gi\u00b7scher", "Kraft", ",", "in", "dem", "Land", "der", "Sch\u00f6n\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADJA", "NN", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Unausweichlicher Schmerz dem Gef\u00fchl sich aufdringt,", "tokens": ["Un\u00b7aus\u00b7weich\u00b7li\u00b7cher", "Schmerz", "dem", "Ge\u00b7f\u00fchl", "sich", "auf\u00b7dringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "+-+--+--+---", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ach, wer wiese die Liebe,", "tokens": ["Ach", ",", "wer", "wie\u00b7se", "die", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Hielte die Klage zur\u00fcck?", "tokens": ["Hiel\u00b7te", "die", "Kla\u00b7ge", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Doch kein Bleiben verg\u00f6nnt des Geschicks Beschlu\u00df mir:", "tokens": ["Doch", "kein", "Blei\u00b7ben", "ver\u00b7g\u00f6nnt", "des", "Ge\u00b7schicks", "Be\u00b7schlu\u00df", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ART", "NN", "NN", "PPER", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Zwar freiwillig und doch ein Gezwungener mu\u00df ich,", "tokens": ["Zwar", "frei\u00b7wil\u00b7lig", "und", "doch", "ein", "Ge\u00b7zwun\u00b7ge\u00b7ner", "mu\u00df", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ART", "NN", "VMFIN", "PPER", "$,"], "meter": "-++--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mu\u00df dich wieder verlassen,", "tokens": ["Mu\u00df", "dich", "wie\u00b7der", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Genua, bl\u00fchende Stadt!", "tokens": ["Ge\u00b7nua", ",", "bl\u00fc\u00b7hen\u00b7de", "Stadt", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Dich, dein rauschendes Meer und den sch\u00f6nen Strandweg,", "tokens": ["Dich", ",", "dein", "rau\u00b7schen\u00b7des", "Meer", "und", "den", "sch\u00f6\u00b7nen", "Strand\u00b7weg", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ja, was reizender ist! Ich erblickte kaum noch", "tokens": ["Ja", ",", "was", "rei\u00b7zen\u00b7der", "ist", "!", "Ich", "er\u00b7blick\u00b7te", "kaum", "noch"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PRELS", "ADJD", "VAFIN", "$.", "PPER", "VVFIN", "ADV", "ADV"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Je mich selbst in geliebtern", "tokens": ["Je", "mich", "selbst", "in", "ge\u00b7lieb\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Augen und liebenderen.", "tokens": ["Au\u00b7gen", "und", "lie\u00b7ben\u00b7de\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Doch wer Liebe versteht, er bekennt, wie sehr auch", "tokens": ["Doch", "wer", "Lie\u00b7be", "ver\u00b7steht", ",", "er", "be\u00b7kennt", ",", "wie", "sehr", "auch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWS", "NN", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PWAV", "ADV", "ADV"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Freudvoll sei der Besitz, es gew\u00e4hrt Besitz uns", "tokens": ["Freud\u00b7voll", "sei", "der", "Be\u00b7sitz", ",", "es", "ge\u00b7w\u00e4hrt", "Be\u00b7sitz", "uns"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "PPER", "VVPP", "NN", "PPER"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Nie dich, sanftere Wehmut,", "tokens": ["Nie", "dich", ",", "sanf\u00b7te\u00b7re", "Weh\u00b7mut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Selige Tr\u00e4ne der Huld!", "tokens": ["Se\u00b7li\u00b7ge", "Tr\u00e4\u00b7ne", "der", "Huld", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}