{"dta.poem.20659": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Ermahnung  \n an einen jungen Freund .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngling, bla\u00df sind Deine Wangen,                 ", "tokens": ["J\u00fcng\u00b7ling", ",", "bla\u00df", "sind", "Dei\u00b7ne", "Wan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leichenbla\u00df,", "tokens": ["Lei\u00b7chen\u00b7bla\u00df", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Ach, Du bist voll Tugendha\u00df", "tokens": ["Ach", ",", "Du", "bist", "voll", "Tu\u00b7gend\u00b7ha\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Oft der Wollust nachgegangen.", "tokens": ["Oft", "der", "Wol\u00b7lust", "nach\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kehre wieder, kehre wieder,", "tokens": ["Keh\u00b7re", "wie\u00b7der", ",", "keh\u00b7re", "wie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wirf in Dir", "tokens": ["Wirf", "in", "Dir"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPR", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Das ern\u00e4hrte wilde Thier", "tokens": ["Das", "er\u00b7n\u00e4hr\u00b7te", "wil\u00b7de", "Thier"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der verdorbnen Neigung nieder.", "tokens": ["Der", "ver\u00b7dorb\u00b7nen", "Nei\u00b7gung", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "M\u00e4\u00dfigkeit und reine Sitten", "tokens": ["M\u00e4\u00b7\u00dfig\u00b7keit", "und", "rei\u00b7ne", "Sit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fchren schon", "tokens": ["F\u00fch\u00b7ren", "schon"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "In sich selber ihren Lohn,", "tokens": ["In", "sich", "sel\u00b7ber", "ih\u00b7ren", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn Du keinen Arzt darfst bitten.", "tokens": ["Wenn", "Du", "kei\u00b7nen", "Arzt", "darfst", "bit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "PAV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Eine fromm geno\u00dfne Jugend,", "tokens": ["Ei\u00b7ne", "fromm", "ge\u00b7no\u00df\u00b7ne", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fr\u00f6hlichkeit", "tokens": ["Fr\u00f6h\u00b7lich\u00b7keit"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Die dem Tritte Rosen streut,", "tokens": ["Die", "dem", "Trit\u00b7te", "Ro\u00b7sen", "streut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit Bewilligung der Tugend:", "tokens": ["Mit", "Be\u00b7wil\u00b7li\u00b7gung", "der", "Tu\u00b7gend", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ein Gewissen ohne Wunden,", "tokens": ["Ein", "Ge\u00b7wis\u00b7sen", "oh\u00b7ne", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ein Blick,", "tokens": ["Und", "ein", "Blick", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Ohne Schaam und Reu zur\u00fcck,", "tokens": ["Oh\u00b7ne", "Schaam", "und", "Reu", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf des Lebens Mittagsstunden;", "tokens": ["Auf", "des", "Le\u00b7bens", "Mit\u00b7tags\u00b7stun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "O das ist ein Gl\u00fcck auf Erden,", "tokens": ["O", "das", "ist", "ein", "Gl\u00fcck", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wird hier,", "tokens": ["Und", "wird", "hier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Wenn Du Greishaar tr\u00e4gest, Dir", "tokens": ["Wenn", "Du", "Greis\u00b7haar", "tr\u00e4\u00b7gest", ",", "Dir"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$,", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schon zum Freudenhimmel werden!", "tokens": ["Schon", "zum", "Freu\u00b7den\u00b7him\u00b7mel", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}