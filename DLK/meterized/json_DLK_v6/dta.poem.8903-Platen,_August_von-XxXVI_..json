{"dta.poem.8903": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxXVI .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Den unsre Zeit, die feile Modedirne,", "tokens": ["Den", "uns\u00b7re", "Zeit", ",", "die", "fei\u00b7le", "Mo\u00b7de\u00b7dir\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gesch\u00e4ftig flicht f\u00fcr jede flache Stirne,", "tokens": ["Ge\u00b7sch\u00e4f\u00b7tig", "flicht", "f\u00fcr", "je\u00b7de", "fla\u00b7che", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus Blumen flicht, die zwo Sekunden bl\u00fchen?", "tokens": ["Aus", "Blu\u00b7men", "flicht", ",", "die", "zwo", "Se\u00b7kun\u00b7den", "bl\u00fc\u00b7hen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "PRELS", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wer wollte noch f\u00fcr das Vollkommne gl\u00fchen,", "tokens": ["Wer", "woll\u00b7te", "noch", "f\u00fcr", "das", "Voll\u00b7komm\u00b7ne", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo man willkommen ist mit leerem Hirne?", "tokens": ["Wo", "man", "will\u00b7kom\u00b7men", "ist", "mit", "lee\u00b7rem", "Hir\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer wollte fliegen gegen die Gestirne,", "tokens": ["Wer", "woll\u00b7te", "flie\u00b7gen", "ge\u00b7gen", "die", "Ge\u00b7stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo Funken blos aus faulem Holze spr\u00fchen?", "tokens": ["Wo", "Fun\u00b7ken", "blos", "aus", "fau\u00b7lem", "Hol\u00b7ze", "spr\u00fc\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gereimten Aberwitzes Propaganden,", "tokens": ["Ge\u00b7reim\u00b7ten", "A\u00b7ber\u00b7wit\u00b7zes", "Pro\u00b7pa\u00b7gan\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fahrt ruhig fort, euch wechselseits zu preisen,", "tokens": ["Fahrt", "ru\u00b7hig", "fort", ",", "euch", "wech\u00b7sel\u00b7seits", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und stellt euch nur, als w\u00e4r' ich nicht vorhanden!", "tokens": ["Und", "stellt", "euch", "nur", ",", "als", "w\u00e4r'", "ich", "nicht", "vor\u00b7han\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "KOKOM", "VAFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein Zeitungsblatt ist leider nicht von Eisen,", "tokens": ["Ein", "Zei\u00b7tungs\u00b7blatt", "ist", "lei\u00b7der", "nicht", "von", "Ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wenn posaunt ihr seyd in allen Landen,", "tokens": ["Und", "wenn", "po\u00b7saunt", "ihr", "seyd", "in", "al\u00b7len", "Lan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADJD", "PPER", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Eins fehlt euch doch \u2014 es ist das Lob der Weisen.", "tokens": ["Eins", "fehlt", "euch", "doch", "es", "ist", "das", "Lob", "der", "Wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$(", "PPER", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}}}}