{"textgrid.poem.48023": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Auf dem Ball", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut in der rauschenden Festespracht", "tokens": ["Heut", "in", "der", "rau\u00b7schen\u00b7den", "Fes\u00b7te\u00b7spracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "hab ich, mein Schatz, an dich gedacht, \u2013", "tokens": ["hab", "ich", ",", "mein", "Schatz", ",", "an", "dich", "ge\u00b7dacht", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "APPR", "PPER", "VVPP", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "an prunkender Tafel, in schimmernden Reihn", "tokens": ["an", "prun\u00b7ken\u00b7der", "Ta\u00b7fel", ",", "in", "schim\u00b7mern\u00b7den", "Reihn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "war meine Seele mit dir allein.", "tokens": ["war", "mei\u00b7ne", "See\u00b7le", "mit", "dir", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "PPER", "ADV", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Mit Blumen, die deine Hand gepfl\u00fcckt,", "tokens": ["Mit", "Blu\u00b7men", ",", "die", "dei\u00b7ne", "Hand", "ge\u00b7pfl\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "hatt ich mir Haare und Brust geschm\u00fcckt;", "tokens": ["hatt", "ich", "mir", "Haa\u00b7re", "und", "Brust", "ge\u00b7schm\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "NN", "KON", "NN", "VVPP", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "als am vollsten der Becher der Lust gesch\u00e4umt,", "tokens": ["als", "am", "volls\u00b7ten", "der", "Be\u00b7cher", "der", "Lust", "ge\u00b7sch\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "ADJA", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "hab ich vergangene Lust getr\u00e4umt.", "tokens": ["hab", "ich", "ver\u00b7gan\u00b7ge\u00b7ne", "Lust", "ge\u00b7tr\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Die Weise schwoll und der Tanz begann \u2013", "tokens": ["Die", "Wei\u00b7se", "schwoll", "und", "der", "Tanz", "be\u00b7gann", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ART", "NN", "VVFIN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "mich r\u00fchrte der Odem Gottes an;", "tokens": ["mich", "r\u00fchr\u00b7te", "der", "O\u00b7dem", "Got\u00b7tes", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "aus der Seele hallte der Klang zur\u00fcck:", "tokens": ["aus", "der", "See\u00b7le", "hall\u00b7te", "der", "Klang", "zu\u00b7r\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "ein Lied von k\u00fcnftigem gro\u00dfen Gl\u00fcck.", "tokens": ["ein", "Lied", "von", "k\u00fcnf\u00b7ti\u00b7gem", "gro\u00b7\u00dfen", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}