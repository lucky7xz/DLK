{"textgrid.poem.42802": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Ballade", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tief im Innersten von Sachsen", "tokens": ["Tief", "im", "In\u00b7ners\u00b7ten", "von", "Sach\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "APPR", "NE"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "\u00dcberfielen eines Abends zwei", "tokens": ["\u00dc\u00b7berf\u00b7ie\u00b7len", "ei\u00b7nes", "A\u00b7bends", "zwei"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "CARD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Halbw\u00fcchsige Knorpel von Schweinshaxen", "tokens": ["Halb\u00b7w\u00fcch\u00b7si\u00b7ge", "Knor\u00b7pel", "von", "Schweins\u00b7ha\u00b7xen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Eine Bulldogge aus der Walachei.", "tokens": ["Ei\u00b7ne", "Bull\u00b7dog\u00b7ge", "aus", "der", "Wa\u00b7la\u00b7chei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Sie umzingelten den alten Hund.", "tokens": ["Sie", "um\u00b7zin\u00b7gel\u00b7ten", "den", "al\u00b7ten", "Hund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-++---+-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Hinterlistig wollten sie das matte", "tokens": ["Hin\u00b7ter\u00b7lis\u00b7tig", "woll\u00b7ten", "sie", "das", "mat\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "PPER", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Tier, das keine Z\u00e4hne mehr im Mund", "tokens": ["Tier", ",", "das", "kei\u00b7ne", "Z\u00e4h\u00b7ne", "mehr", "im", "Mund"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "PRELS", "PIAT", "NN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und auch keine Haare darauf hatte,", "tokens": ["Und", "auch", "kei\u00b7ne", "Haa\u00b7re", "da\u00b7rauf", "hat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "PAV", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "An den Augen treffen, hinterher", "tokens": ["An", "den", "Au\u00b7gen", "tref\u00b7fen", ",", "hin\u00b7ter\u00b7her"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "VVINF", "$,", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ihm die Zunge schlitzen und durch Zwicken", "tokens": ["Ihm", "die", "Zun\u00b7ge", "schlit\u00b7zen", "und", "durch", "Zwi\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "VVINF", "KON", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Seinen Gaumen reizen und noch mehr,", "tokens": ["Sei\u00b7nen", "Gau\u00b7men", "rei\u00b7zen", "und", "noch", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "KON", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Um zuletzt ihn pl\u00f6tzlich zu ersticken.", "tokens": ["Um", "zu\u00b7letzt", "ihn", "pl\u00f6tz\u00b7lich", "zu", "er\u00b7sti\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Wollten so. Jedoch es kam nicht so.", "tokens": ["Woll\u00b7ten", "so", ".", "Je\u00b7doch", "es", "kam", "nicht", "so", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "$.", "ADV", "PPER", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Denn die Dogge, ohne sich zu wehren,", "tokens": ["Denn", "die", "Dog\u00b7ge", ",", "oh\u00b7ne", "sich", "zu", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "KOUI", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Zog den Schwanz ein, heulte laut und floh", "tokens": ["Zog", "den", "Schwanz", "ein", ",", "heul\u00b7te", "laut", "und", "floh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,", "VVFIN", "ADJD", "KON", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und begann sofort sich zu vermehren.", "tokens": ["Und", "be\u00b7gann", "so\u00b7fort", "sich", "zu", "ver\u00b7meh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PRF", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Und die neuen jungen Hunde knurrten", "tokens": ["Und", "die", "neu\u00b7en", "jun\u00b7gen", "Hun\u00b7de", "knurr\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schon am selben Tag, als man sie warf,", "tokens": ["Schon", "am", "sel\u00b7ben", "Tag", ",", "als", "man", "sie", "warf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hatten spitze Z\u00e4hne, und sie wurden", "tokens": ["Hat\u00b7ten", "spit\u00b7ze", "Z\u00e4h\u00b7ne", ",", "und", "sie", "wur\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADJA", "NN", "$,", "KON", "PPER", "VAFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ganz speziell auf Haxenknochen scharf.", "tokens": ["Ganz", "spe\u00b7zi\u00b7ell", "auf", "Ha\u00b7xen\u00b7kno\u00b7chen", "scharf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und die Enkelhunde bissen sp\u00e4ter", "tokens": ["Und", "die", "En\u00b7kel\u00b7hun\u00b7de", "bis\u00b7sen", "sp\u00e4\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Jede Haxe ohne Unterschied.", "tokens": ["Je\u00b7de", "Ha\u00b7xe", "oh\u00b7ne", "Un\u00b7ter\u00b7schied", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und so r\u00e4cht die S\u00fcnde sich der V\u00e4ter", "tokens": ["Und", "so", "r\u00e4cht", "die", "S\u00fcn\u00b7de", "sich", "der", "V\u00e4\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PRF", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bis ins tausendste und letzte Glied.", "tokens": ["Bis", "ins", "tau\u00b7sends\u00b7te", "und", "letz\u00b7te", "Glied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "+-+---+-+", "measure": "unknown.measure.tetra"}}}}}