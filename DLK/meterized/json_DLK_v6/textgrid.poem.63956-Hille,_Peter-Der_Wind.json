{"textgrid.poem.63956": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "Der Wind", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sank vor ihm das Schiff zu Grund,", "tokens": ["Es", "sank", "vor", "ihm", "das", "Schiff", "zu", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als er \u00fcberbrauste das Meer,", "tokens": ["Als", "er", "\u00fc\u00b7berb\u00b7raus\u00b7te", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Mit dem Feuer schlo\u00df er schnell einen Bund,", "tokens": ["Mit", "dem", "Feu\u00b7er", "schlo\u00df", "er", "schnell", "ei\u00b7nen", "Bund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wie sausten die Flammen einher!", "tokens": ["Wie", "saus\u00b7ten", "die", "Flam\u00b7men", "ein\u00b7her", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Mitinnen der W\u00fcste, der Karawane", "tokens": ["Mi\u00b7tin\u00b7nen", "der", "W\u00fcs\u00b7te", ",", "der", "Ka\u00b7ra\u00b7wa\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Verweht er die wankende, wogende Bahn.", "tokens": ["Ver\u00b7weht", "er", "die", "wan\u00b7ken\u00b7de", ",", "wo\u00b7gen\u00b7de", "Bahn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Im sonnengoldenen Buchengr\u00fcn", "tokens": ["Im", "son\u00b7nen\u00b7gol\u00b7de\u00b7nen", "Bu\u00b7chen\u00b7gr\u00fcn"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da hebt er neckisch den Flor,", "tokens": ["Da", "hebt", "er", "ne\u00b7ckisch", "den", "Flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der reizenden Maid, die Wangen ergl\u00fchn, \u2013", "tokens": ["Der", "rei\u00b7zen\u00b7den", "Maid", ",", "die", "Wan\u00b7gen", "er\u00b7gl\u00fchn", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVINF", "$,", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "O Wind, was hattest du vor?", "tokens": ["O", "Wind", ",", "was", "hat\u00b7test", "du", "vor", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWS", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Die gek\u00fc\u00dfte Lippe, sieh, wie sie schmollt!", "tokens": ["Die", "ge\u00b7k\u00fc\u00df\u00b7te", "Lip\u00b7pe", ",", "sieh", ",", "wie", "sie", "schmollt", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Der Liebste, ob er dem Winde wohl grollt?", "tokens": ["Der", "Liebs\u00b7te", ",", "ob", "er", "dem", "Win\u00b7de", "wohl", "grollt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}