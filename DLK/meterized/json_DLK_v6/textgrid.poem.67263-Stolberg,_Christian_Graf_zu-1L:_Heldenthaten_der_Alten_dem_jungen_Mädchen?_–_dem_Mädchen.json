{"textgrid.poem.67263": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Heldenthaten der Alten dem jungen M\u00e4dchen? \u2013 dem M\u00e4dchen", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heldenthaten der Alten dem jungen M\u00e4dchen? \u2013 dem M\u00e4dchen", "tokens": ["Hel\u00b7den\u00b7tha\u00b7ten", "der", "Al\u00b7ten", "dem", "jun\u00b7gen", "M\u00e4d\u00b7chen", "?", "\u2013", "dem", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "$.", "$(", "ART", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Der bei dem Guten die Wang' und bei dem Sch\u00f6nen ergl\u00fcht.", "tokens": ["Der", "bei", "dem", "Gu\u00b7ten", "die", "Wang'", "und", "bei", "dem", "Sch\u00f6\u00b7nen", "er\u00b7gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "KON", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Jung auch waren Jene. In Jugendbl\u00fcthe des Geistes", "tokens": ["Jung", "auch", "wa\u00b7ren", "Je\u00b7ne", ".", "In", "Ju\u00b7gend\u00b7bl\u00fc\u00b7the", "des", "Geis\u00b7tes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VAFIN", "PDAT", "$.", "APPR", "NN", "ART", "NN"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Strahlte des Griechen Aug', hub sich die r\u00f6mische Brust.", "tokens": ["Strahl\u00b7te", "des", "Grie\u00b7chen", "Aug'", ",", "hub", "sich", "die", "r\u00f6\u00b7mi\u00b7sche", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Liebe sie immer nur, mein T\u00f6chterchen! Zur Amazone", "tokens": ["Lie\u00b7be", "sie", "im\u00b7mer", "nur", ",", "mein", "T\u00f6ch\u00b7ter\u00b7chen", "!", "Zur", "A\u00b7ma\u00b7zo\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "ADV", "ADV", "$,", "PPOSAT", "NN", "$.", "APPRART", "NN"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.6": {"text": "Wirst Du nicht. \u2013 O es ist einzig, untheilbar ist sie,", "tokens": ["Wirst", "Du", "nicht", ".", "\u2013", "O", "es", "ist", "ein\u00b7zig", ",", "un\u00b7theil\u00b7bar", "ist", "sie", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "$.", "$(", "NE", "PPER", "VAFIN", "ADJD", "$,", "ADJD", "VAFIN", "PPER", "$,"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Unsere menschliche Tugend; der Eine g\u00f6ttliche Odem", "tokens": ["Un\u00b7se\u00b7re", "menschli\u00b7che", "Tu\u00b7gend", ";", "der", "Ei\u00b7ne", "g\u00f6tt\u00b7li\u00b7che", "O\u00b7dem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "ART", "ART", "ADJA", "NN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "St\u00e4hlet dem Helden den Arm, feuchtet der Jungfrau den Blick.", "tokens": ["St\u00e4h\u00b7let", "dem", "Hel\u00b7den", "den", "Arm", ",", "feuch\u00b7tet", "der", "Jung\u00b7frau", "den", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--++-+-+-+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Wohl dem Helden, der Fremdling nicht ist den Gef\u00fchlen der Jungfrau;", "tokens": ["Wohl", "dem", "Hel\u00b7den", ",", "der", "Fremd\u00b7ling", "nicht", "ist", "den", "Ge\u00b7f\u00fch\u00b7len", "der", "Jung\u00b7frau", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "PTKNEG", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Wohl ihr, t\u00f6nt' ihr die Sait' auch in Cornelia's Ton!", "tokens": ["Wohl", "ihr", ",", "t\u00f6nt'", "ihr", "die", "Sait'", "auch", "in", "Cor\u00b7ne\u00b7lia's", "Ton", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "VVFIN", "PPER", "ART", "NN", "ADV", "APPR", "NE", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}}}}