{"textgrid.poem.57490": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr sch\u00f6nsten Augen! z\u00fcrnt nur nicht,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr sch\u00f6nsten Augen! z\u00fcrnt nur nicht,", "tokens": ["Ihr", "sch\u00f6ns\u00b7ten", "Au\u00b7gen", "!", "z\u00fcrnt", "nur", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "VVFIN", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich euch lechzend angesehen.", "tokens": ["Da\u00df", "ich", "euch", "lech\u00b7zend", "an\u00b7ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es prallten nur die eignen Blicke,", "tokens": ["Es", "prall\u00b7ten", "nur", "die", "eig\u00b7nen", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die selbst von euch nach mir geschehen,", "tokens": ["Die", "selbst", "von", "euch", "nach", "mir", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Als meiner Seelen Sonnenlicht,", "tokens": ["Als", "mei\u00b7ner", "See\u00b7len", "Son\u00b7nen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch meiner Augen Stral zur\u00fccke.", "tokens": ["Durch", "mei\u00b7ner", "Au\u00b7gen", "Stral", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr sch\u00f6nsten Augen! z\u00fcrnt nur nicht etc.", "tokens": ["Ihr", "sch\u00f6ns\u00b7ten", "Au\u00b7gen", "!", "z\u00fcrnt", "nur", "nicht", "et\u00b7c."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "VVFIN", "ADV", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So sang der zarte Filamor,", "tokens": ["So", "sang", "der", "zar\u00b7te", "Fi\u00b7la\u00b7mor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als Phyllis ihm den Fehler vorger\u00fccket,", "tokens": ["Als", "Phyl\u00b7lis", "ihm", "den", "Feh\u00b7ler", "vor\u00b7ge\u00b7r\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er h\u00e4tte sie zu oft, zu heftig angeblicket.", "tokens": ["Er", "h\u00e4t\u00b7te", "sie", "zu", "oft", ",", "zu", "hef\u00b7tig", "an\u00b7ge\u00b7bli\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKA", "ADV", "$,", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was kann ich, sprach er, denn davor,", "tokens": ["Was", "kann", "ich", ",", "sprach", "er", ",", "denn", "da\u00b7vor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "KON", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df du so reizend bist?", "tokens": ["Da\u00df", "du", "so", "rei\u00b7zend", "bist", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df sich mein Aug auf deinen Lilgenwangen,", "tokens": ["Da\u00df", "sich", "mein", "Aug", "auf", "dei\u00b7nen", "Lil\u00b7gen\u00b7wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die schon so manches Herz gefangen,", "tokens": ["Die", "schon", "so", "man\u00b7ches", "Herz", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Verirrt, vertiefet und vergi\u00dft?", "tokens": ["Ver\u00b7irrt", ",", "ver\u00b7tie\u00b7fet", "und", "ver\u00b7gi\u00dft", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Klage dich nur selber an!", "tokens": ["Kla\u00b7ge", "dich", "nur", "sel\u00b7ber", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ich dir zu viel gethan.", "tokens": ["Wenn", "ich", "dir", "zu", "viel", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "PIS", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deiner Augen Zauberkerzen", "tokens": ["Dei\u00b7ner", "Au\u00b7gen", "Zau\u00b7ber\u00b7ker\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zwingen hundert zarte Herzen,", "tokens": ["Zwin\u00b7gen", "hun\u00b7dert", "zar\u00b7te", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df dich keines hassen kann.", "tokens": ["Da\u00df", "dich", "kei\u00b7nes", "has\u00b7sen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Klage dich nur selber an! etc.", "tokens": ["Kla\u00b7ge", "dich", "nur", "sel\u00b7ber", "an", "!", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "punct", "abbreviation"], "pos": ["NN", "PPER", "ADV", "ADV", "PTKVZ", "$.", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Das Unrecht schien ihm allzugro\u00df,", "tokens": ["Das", "Un\u00b7recht", "schien", "ihm", "all\u00b7zu\u00b7gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Phyllis hier begangen.", "tokens": ["Das", "Phyl\u00b7lis", "hier", "be\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Drum schwieg er etwas still; doch endlich brach er los:", "tokens": ["Drum", "schwieg", "er", "et\u00b7was", "still", ";", "doch", "end\u00b7lich", "brach", "er", "los", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADJD", "$.", "ADV", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie! steht es frey, des Himmels Prangen", "tokens": ["Wie", "!", "steht", "es", "frey", ",", "des", "Him\u00b7mels", "Pran\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "ADJD", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit starren Augen anzusehn?", "tokens": ["Mit", "star\u00b7ren", "Au\u00b7gen", "an\u00b7zu\u00b7sehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Phyllis will von mir verlangen,", "tokens": ["Und", "Phyl\u00b7lis", "will", "von", "mir", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VMFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es soll kein Blick nach ihr geschehn?", "tokens": ["Es", "soll", "kein", "Blick", "nach", "ihr", "ge\u00b7schehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nein! ihr Verboth kann mich nicht r\u00fchren,", "tokens": ["Nein", "!", "ihr", "Ver\u00b7both", "kann", "mich", "nicht", "r\u00fch\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPOSAT", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Nein! sie wird nichts dadurch von ihrer Pracht verlieren.", "tokens": ["Nein", "!", "sie", "wird", "nichts", "da\u00b7durch", "von", "ih\u00b7rer", "Pracht", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VAFIN", "PIS", "PAV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Soll ich mein Verbrechen b\u00fcssen,", "tokens": ["Soll", "ich", "mein", "Ver\u00b7bre\u00b7chen", "b\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Strenge Phyllis! strafe mich!", "tokens": ["Stren\u00b7ge", "Phyl\u00b7lis", "!", "stra\u00b7fe", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sage nur, ich solle dich", "tokens": ["Sa\u00b7ge", "nur", ",", "ich", "sol\u00b7le", "dich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "PPER", "VMFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit verbundnen Augen k\u00fcssen.", "tokens": ["Mit", "ver\u00b7bund\u00b7nen", "Au\u00b7gen", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ungesehn,", "tokens": ["Un\u00b7ge\u00b7sehn", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Wird mir da recht weh geschehn.", "tokens": ["Wird", "mir", "da", "recht", "weh", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch will ich aus Mund und Augen", "tokens": ["Doch", "will", "ich", "aus", "Mund", "und", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Meiner Seelen Nectar saugen.", "tokens": ["Mei\u00b7ner", "See\u00b7len", "Nec\u00b7tar", "sau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}