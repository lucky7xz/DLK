{"textgrid.poem.57816": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Liebesklage", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weidenbaum, dir will ich's sagen,", "tokens": ["Wei\u00b7den\u00b7baum", ",", "dir", "will", "ich's", "sa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weidenbaum, dir will ich's klagen,", "tokens": ["Wei\u00b7den\u00b7baum", ",", "dir", "will", "ich's", "kla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lieblich ist die Maienzeit,", "tokens": ["Lieb\u00b7lich", "ist", "die", "Mai\u00b7en\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch ich trage Herzeleid.", "tokens": ["Doch", "ich", "tra\u00b7ge", "Her\u00b7ze\u00b7leid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Weidenbaum, du sollst es h\u00f6ren,", "tokens": ["Wei\u00b7den\u00b7baum", ",", "du", "sollst", "es", "h\u00f6\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df er nie wird wiederkehren,", "tokens": ["Da\u00df", "er", "nie", "wird", "wie\u00b7der\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6n und lustig ist der Mai,", "tokens": ["Sch\u00f6n", "und", "lus\u00b7tig", "ist", "der", "Mai", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch mein Gl\u00fcck das ist vorbei.", "tokens": ["Doch", "mein", "Gl\u00fcck", "das", "ist", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PDS", "VAFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Weidenbaum, du sollst es wissen,", "tokens": ["Wei\u00b7den\u00b7baum", ",", "du", "sollst", "es", "wis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nie wird er mich wieder k\u00fcssen,", "tokens": ["Nie", "wird", "er", "mich", "wie\u00b7der", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wieder kehrt die Maienzeit,", "tokens": ["Wie\u00b7der", "kehrt", "die", "Mai\u00b7en\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch sie bringt mir neues Leid.", "tokens": ["Doch", "sie", "bringt", "mir", "neu\u00b7es", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Weidenbaum, wenn sie dich fragen,", "tokens": ["Wei\u00b7den\u00b7baum", ",", "wenn", "sie", "dich", "fra\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weidenbaum, dann sollst du sagen,", "tokens": ["Wei\u00b7den\u00b7baum", ",", "dann", "sollst", "du", "sa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wen betrogen hat der Mai,", "tokens": ["Wen", "be\u00b7tro\u00b7gen", "hat", "der", "Mai", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dessen Fr\u00fchling ist vorbei.", "tokens": ["Des\u00b7sen", "Fr\u00fch\u00b7ling", "ist", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}