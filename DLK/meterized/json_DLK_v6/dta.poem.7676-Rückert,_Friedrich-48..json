{"dta.poem.7676": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den einen ehr' ich, der nach Idealem ringt;", "tokens": ["Den", "ei\u00b7nen", "ehr'", "ich", ",", "der", "nach", "I\u00b7dea\u00b7lem", "ringt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "$,", "PRELS", "APPR", "NE", "VVFIN", "$."], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den andern acht' ich auch, dem Wirkliches gelingt.", "tokens": ["Den", "an\u00b7dern", "acht'", "ich", "auch", ",", "dem", "Wirk\u00b7li\u00b7ches", "ge\u00b7lingt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Den aber lieb' ich, der nicht dis noch jenes w\u00e4hlt,", "tokens": ["Den", "a\u00b7ber", "lieb'", "ich", ",", "der", "nicht", "dis", "noch", "je\u00b7nes", "w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PPER", "$,", "PRELS", "PTKNEG", "PDS", "ADV", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der h\u00f6chstes Ideal der Wirklichstheit verm\u00e4hlt.", "tokens": ["Der", "h\u00f6chs\u00b7tes", "I\u00b7deal", "der", "Wirk\u00b7lichst\u00b7heit", "ver\u00b7m\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+", "measure