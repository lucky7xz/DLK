{"textgrid.poem.49554": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr Nymphen, euer Hirt, dem Pan im Singen gleich,", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Nymphen, euer Hirt, dem Pan im Singen gleich,", "tokens": ["Ihr", "Nym\u00b7phen", ",", "eu\u00b7er", "Hirt", ",", "dem", "Pan", "im", "Sin\u00b7gen", "gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Liegt, da der Mittag gl\u00fcht, betrunken im Gestr\u00e4uch,", "tokens": ["Liegt", ",", "da", "der", "Mit\u00b7tag", "gl\u00fcht", ",", "be\u00b7trun\u00b7ken", "im", "Ge\u00b7str\u00e4uch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In einem unerlaubten Schlafe.", "tokens": ["In", "ei\u00b7nem", "un\u00b7er\u00b7laub\u00b7ten", "Schla\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Cytherens Sohn bewacht inde\u00dfen seine Schaafe", "tokens": ["Cy\u00b7the\u00b7rens", "Sohn", "be\u00b7wacht", "in\u00b7de\u00b7\u00dfen", "sei\u00b7ne", "Schaa\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Und tr\u00e4gt den Stab, den er dem Schlummernden entwand,", "tokens": ["Und", "tr\u00e4gt", "den", "Stab", ",", "den", "er", "dem", "Schlum\u00b7mern\u00b7den", "ent\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Stolz, wie ein Sch\u00e4fer, in der Hand.", "tokens": ["Stolz", ",", "wie", "ein", "Sch\u00e4\u00b7fer", ",", "in", "der", "Hand", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Soll nun kein wildes Thier den kleinen Gott verschlingen,", "tokens": ["Soll", "nun", "kein", "wil\u00b7des", "Thier", "den", "klei\u00b7nen", "Gott", "ver\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So eilt, o eilt den Mann aus seinem Schlaf zu bringen!", "tokens": ["So", "eilt", ",", "o", "eilt", "den", "Mann", "aus", "sei\u00b7nem", "Schlaf", "zu", "brin\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "FM", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}