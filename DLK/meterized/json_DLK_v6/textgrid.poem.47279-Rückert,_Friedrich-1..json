{"textgrid.poem.47279": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ging ein Mann im Syrerland,", "tokens": ["Es", "ging", "ein", "Mann", "im", "Sy\u00b7rer\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fchrt' ein Kamel am Halfterband.", "tokens": ["F\u00fchrt'", "ein", "Ka\u00b7mel", "am", "Half\u00b7ter\u00b7band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Das Tier mit grimmigen Geb\u00e4rden", "tokens": ["Das", "Tier", "mit", "grim\u00b7mi\u00b7gen", "Ge\u00b7b\u00e4r\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Urpl\u00f6tzlich anfing scheu zu werden", "tokens": ["Ur\u00b7pl\u00f6tz\u00b7lich", "an\u00b7fing", "scheu", "zu", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADJD", "PTKZU", "VAINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und that so ganz entsetzlich schnaufen,", "tokens": ["Und", "that", "so", "ganz", "ent\u00b7setz\u00b7lich", "schnau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der F\u00fchrer vor ihm mu\u00dft' entlaufen.", "tokens": ["Der", "F\u00fch\u00b7rer", "vor", "ihm", "mu\u00dft'", "ent\u00b7lau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Er lief und einen Brunnen sah", "tokens": ["Er", "lief", "und", "ei\u00b7nen", "Brun\u00b7nen", "sah"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von ungef\u00e4hr am Wege da.", "tokens": ["Von", "un\u00b7ge\u00b7f\u00e4hr", "am", "We\u00b7ge", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Das Tier h\u00f6rt' er im R\u00fccken schnauben,", "tokens": ["Das", "Tier", "h\u00f6rt'", "er", "im", "R\u00fc\u00b7cken", "schnau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Das mu\u00dft' ihm die Besinnung rauben.", "tokens": ["Das", "mu\u00dft'", "ihm", "die", "Be\u00b7sin\u00b7nung", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Er in den Schacht des Brunnens kroch,", "tokens": ["Er", "in", "den", "Schacht", "des", "Brun\u00b7nens", "kroch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Er st\u00fcrzte nicht, er schwebte noch.", "tokens": ["Er", "st\u00fcrz\u00b7te", "nicht", ",", "er", "schweb\u00b7te", "noch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Gewachsen war ein Brombeerstrauch", "tokens": ["Ge\u00b7wach\u00b7sen", "war", "ein", "Brom\u00b7beer\u00b7strauch"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Aus des geborstnen Brunnens Bauch;", "tokens": ["Aus", "des", "ge\u00b7borst\u00b7nen", "Brun\u00b7nens", "Bauch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Daran der Mann sich fest that klammern", "tokens": ["Da\u00b7ran", "der", "Mann", "sich", "fest", "that", "klam\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "PRF", "ADJD", "VVFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und seinen Zustand drauf bejammern.", "tokens": ["Und", "sei\u00b7nen", "Zu\u00b7stand", "drauf", "be\u00b7jam\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Er blickte in die H\u00f6h' und sah", "tokens": ["Er", "blick\u00b7te", "in", "die", "H\u00f6h'", "und", "sah"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Dort das Kamelhaupt furchtbar nah',", "tokens": ["Dort", "das", "Ka\u00b7mel\u00b7haupt", "furcht\u00b7bar", "nah'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-++-+", "measure": "unknown.measure.penta"}, "line.19": {"text": "Das ihn wollt' oben fassen wieder.", "tokens": ["Das", "ihn", "wollt'", "o\u00b7ben", "fas\u00b7sen", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VMFIN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Dann blickt' er in den Brunnen nieder;", "tokens": ["Dann", "blickt'", "er", "in", "den", "Brun\u00b7nen", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Da sah am Grund er einen Drachen", "tokens": ["Da", "sah", "am", "Grund", "er", "ei\u00b7nen", "Dra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Aufg\u00e4hnen mit entsperrtem Rachen,", "tokens": ["Auf\u00b7g\u00e4h\u00b7nen", "mit", "ent\u00b7sperr\u00b7tem", "Ra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Der drunten ihn verschlingen wollte,", "tokens": ["Der", "drun\u00b7ten", "ihn", "ver\u00b7schlin\u00b7gen", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Wenn er hinunter fallen sollte.", "tokens": ["Wenn", "er", "hin\u00b7un\u00b7ter", "fal\u00b7len", "soll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "So schwebend in der beiden Mitte,", "tokens": ["So", "schwe\u00b7bend", "in", "der", "bei\u00b7den", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Da sah der Arme noch das Dritte.", "tokens": ["Da", "sah", "der", "Ar\u00b7me", "noch", "das", "Drit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Wo in die Mauerspalte ging", "tokens": ["Wo", "in", "die", "Mau\u00b7ers\u00b7pal\u00b7te", "ging"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Des Str\u00e4uchleins Wurzel, dran er hing,", "tokens": ["Des", "Str\u00e4uch\u00b7leins", "Wur\u00b7zel", ",", "dran", "er", "hing", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Da sah er still ein M\u00e4usepaar,", "tokens": ["Da", "sah", "er", "still", "ein", "M\u00e4u\u00b7se\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Schwarz eine, wei\u00df die andre war.", "tokens": ["Schwarz", "ei\u00b7ne", ",", "wei\u00df", "die", "and\u00b7re", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "$,", "VVFIN", "ART", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Er sah die schwarze mit der wei\u00dfen", "tokens": ["Er", "sah", "die", "schwar\u00b7ze", "mit", "der", "wei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Abwechselnd an der Wurzel bei\u00dfen.", "tokens": ["Ab\u00b7wech\u00b7selnd", "an", "der", "Wur\u00b7zel", "bei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Sie nagten, zausten, gruben, w\u00fchlten,", "tokens": ["Sie", "nag\u00b7ten", ",", "zaus\u00b7ten", ",", "gru\u00b7ben", ",", "w\u00fchl\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Die Erd' ab von der Wurzel sp\u00fclten;", "tokens": ["Die", "Erd'", "ab", "von", "der", "Wur\u00b7zel", "sp\u00fcl\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.35": {"text": "Und wie sie rieselnd niederrann,", "tokens": ["Und", "wie", "sie", "rie\u00b7selnd", "nie\u00b7der\u00b7rann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Der Drach' im Grund aufblickte dann,", "tokens": ["Der", "Drach'", "im", "Grund", "auf\u00b7blick\u00b7te", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Zu sehn, wie bald mit seiner B\u00fcrde", "tokens": ["Zu", "sehn", ",", "wie", "bald", "mit", "sei\u00b7ner", "B\u00fcr\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "PWAV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Der Strauch entwurzelt fallen w\u00fcrde.", "tokens": ["Der", "Strauch", "ent\u00b7wur\u00b7zelt", "fal\u00b7len", "w\u00fcr\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Der Mann in Angst und Furcht und Not,", "tokens": ["Der", "Mann", "in", "Angst", "und", "Furcht", "und", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Umstellt, umlagert und umdroht,", "tokens": ["Um\u00b7stellt", ",", "um\u00b7la\u00b7gert", "und", "um\u00b7droht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Im Stand des jammerhaften Schwebens,", "tokens": ["Im", "Stand", "des", "jam\u00b7mer\u00b7haf\u00b7ten", "Schwe\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Sah sich nach Rettung um vergebens.", "tokens": ["Sah", "sich", "nach", "Ret\u00b7tung", "um", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "APPR", "ADV", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.43": {"text": "Und da er also um sich blickte,", "tokens": ["Und", "da", "er", "al\u00b7so", "um", "sich", "blick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Sah er ein Zweiglein, welches nickte", "tokens": ["Sah", "er", "ein", "Zwei\u00b7glein", ",", "wel\u00b7ches", "nick\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Vom Brombeerstrauch mit reifen Beeren;", "tokens": ["Vom", "Brom\u00b7beer\u00b7strauch", "mit", "rei\u00b7fen", "Bee\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Da konnt' er doch der Lust nicht wehren.", "tokens": ["Da", "konnt'", "er", "doch", "der", "Lust", "nicht", "weh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Er sah nicht des Kameles Wut", "tokens": ["Er", "sah", "nicht", "des", "Ka\u00b7me\u00b7les", "Wut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Und nicht den Drachen in der Flut", "tokens": ["Und", "nicht", "den", "Dra\u00b7chen", "in", "der", "Flut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Und nicht der M\u00e4use T\u00fcckespiel,", "tokens": ["Und", "nicht", "der", "M\u00e4u\u00b7se", "T\u00fc\u00b7cke\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Als ihm die Beer' ins Auge fiel.", "tokens": ["Als", "ihm", "die", "Beer'", "ins", "Au\u00b7ge", "fiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Er lie\u00df das Tier von oben rauschen", "tokens": ["Er", "lie\u00df", "das", "Tier", "von", "o\u00b7ben", "rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Und unter sich den Drachen lauschen", "tokens": ["Und", "un\u00b7ter", "sich", "den", "Dra\u00b7chen", "lau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PRF", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "Und neben sich die M\u00e4use nagen,", "tokens": ["Und", "ne\u00b7ben", "sich", "die", "M\u00e4u\u00b7se", "na\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "Griff nach den Beerlein mit Behagen,", "tokens": ["Griff", "nach", "den", "Be\u00b7er\u00b7lein", "mit", "Be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}, "line.55": {"text": "Sie deuchten ihm zu es