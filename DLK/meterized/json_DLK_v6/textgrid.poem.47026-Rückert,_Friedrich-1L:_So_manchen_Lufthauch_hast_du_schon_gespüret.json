{"textgrid.poem.47026": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: So manchen Lufthauch hast du schon gesp\u00fcret", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So manchen Lufthauch hast du schon gesp\u00fcret", "tokens": ["So", "man\u00b7chen", "Luft\u00b7hauch", "hast", "du", "schon", "ge\u00b7sp\u00fc\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im offnen Feld um Stirn und Brust und Wange,", "tokens": ["Im", "off\u00b7nen", "Feld", "um", "Stirn", "und", "Brust", "und", "Wan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df nun kein Seufzerhauch, wie er auch bange", "tokens": ["Da\u00df", "nun", "kein", "Seuf\u00b7zer\u00b7hauch", ",", "wie", "er", "auch", "ban\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PIAT", "NN", "$,", "PWAV", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mag hauchen, Unempfindliche, dich r\u00fchret.", "tokens": ["Mag", "hau\u00b7chen", ",", "Un\u00b7emp\u00b7find\u00b7li\u00b7che", ",", "dich", "r\u00fch\u00b7ret", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "ADJA", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So mancher Stoff hat deine Hand ber\u00fchret", "tokens": ["So", "man\u00b7cher", "Stoff", "hat", "dei\u00b7ne", "Hand", "be\u00b7r\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit rauhem Druck in deiner Wirtschaft Gange,", "tokens": ["Mit", "rau\u00b7hem", "Druck", "in", "dei\u00b7ner", "Wirt\u00b7schaft", "Gan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df nun die Hand der Liebe Druck schon lange,", "tokens": ["Da\u00df", "nun", "die", "Hand", "der", "Lie\u00b7be", "Druck", "schon", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie sanft er auch mag dr\u00fccken, nicht mehr sp\u00fcret.", "tokens": ["Wie", "sanft", "er", "auch", "mag", "dr\u00fc\u00b7cken", ",", "nicht", "mehr", "sp\u00fc\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "VMFIN", "VVINF", "$,", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So manches T\u00e4ubchen hast du sonder Leide", "tokens": ["So", "man\u00b7ches", "T\u00e4ub\u00b7chen", "hast", "du", "son\u00b7der", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schon abgew\u00fcrgt, da\u00df du nun mit Erg\u00f6tzen", "tokens": ["Schon", "ab\u00b7ge\u00b7w\u00fcrgt", ",", "da\u00df", "du", "nun", "mit", "Er\u00b7g\u00f6t\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "$,", "KOUS", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Taubengirren h\u00f6rst, ohn' es zu f\u00fchlen;", "tokens": ["Mein", "Tau\u00b7ben\u00b7gir\u00b7ren", "h\u00f6rst", ",", "ohn'", "es", "zu", "f\u00fch\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So manches H\u00e4lmchen mit der Sichel Schneide", "tokens": ["So", "man\u00b7ches", "H\u00e4lm\u00b7chen", "mit", "der", "Si\u00b7chel", "Schnei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gem\u00e4het schon, da\u00df du auch ohn' Entsetzen", "tokens": ["Ge\u00b7m\u00e4\u00b7het", "schon", ",", "da\u00df", "du", "auch", "ohn'", "Ent\u00b7set\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Stahl des Todes in mein Herz kannst w\u00fchlen.", "tokens": ["Den", "Stahl", "des", "To\u00b7des", "in", "mein", "Herz", "kannst", "w\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}