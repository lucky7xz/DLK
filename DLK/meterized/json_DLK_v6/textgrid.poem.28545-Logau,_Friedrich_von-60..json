{"textgrid.poem.28545": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "60.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel geust nicht leicht auff einen alle Gaben;", "tokens": ["Der", "Him\u00b7mel", "geust", "nicht", "leicht", "auff", "ei\u00b7nen", "al\u00b7le", "Ga\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADJD", "APPR", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df derer dann so viel Melampsychus kan haben?", "tokens": ["Da\u00df", "de\u00b7rer", "dann", "so", "viel", "Me\u00b7lamp\u00b7sy\u00b7chus", "kan", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADV", "ADV", "ADV", "NE", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch Kirche, Schule, Hof, durch Rent- und Cantzeley,", "tokens": ["Durch", "Kir\u00b7che", ",", "Schu\u00b7le", ",", "Hof", ",", "durch", "Rent", "und", "Cant\u00b7ze\u00b7ley", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,", "APPR", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Land, Wirthschafft, Rathhaus, Stadt geht sein verordnen frey.", "tokens": ["Land", ",", "Wirth\u00b7schafft", ",", "Rath\u00b7haus", ",", "Stadt", "geht", "sein", "ver\u00b7ord\u00b7nen", "frey", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mich d\u00fcnckt, (man mercke drauff!) es wil mir also scheinen,", "tokens": ["Mich", "d\u00fcnckt", ",", "(", "man", "mer\u00b7cke", "drauff", "!", ")", "es", "wil", "mir", "al\u00b7so", "schei\u00b7nen", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "$(", "PIS", "VVFIN", "PTKVZ", "$.", "$(", "PPER", "VMFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Melampsychus sein Weib und Kinder werden weinen.", "tokens": ["Me\u00b7lamp\u00b7sy\u00b7chus", "sein", "Weib", "und", "Kin\u00b7der", "wer\u00b7den", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "KON", "NN", "VAFIN", "VVINF", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}}}}