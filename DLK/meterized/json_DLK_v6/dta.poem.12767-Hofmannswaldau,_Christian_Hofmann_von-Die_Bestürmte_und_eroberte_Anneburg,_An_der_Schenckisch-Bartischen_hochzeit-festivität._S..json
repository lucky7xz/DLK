{"dta.poem.12767": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n Best\u00fcrmte und eroberte Anneburg,  \n  An der Schenckisch-Bartischen  \n hochzeit-festivit\u00e4t.  \n S.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die liebe zieht ins feld!", "tokens": ["Die", "lie\u00b7be", "zieht", "ins", "feld", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sie will heut approschieren,", "tokens": ["Sie", "will", "heut", "ap\u00b7pro\u00b7schie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Best\u00fcrmen und miniren,", "tokens": ["Be\u00b7st\u00fcr\u00b7men", "und", "mi\u00b7ni\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df sie den platz beh\u00e4lt.", "tokens": ["Da\u00df", "sie", "den", "platz", "be\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drum blase man lermen und r\u00fchre die spiele,", "tokens": ["Drum", "bla\u00b7se", "man", "ler\u00b7men", "und", "r\u00fch\u00b7re", "die", "spie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "VVINF", "KON", "VVFIN", "ART", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Da\u00df alles auf eine eroberung ziele!", "tokens": ["Da\u00df", "al\u00b7les", "auf", "ei\u00b7ne", "er\u00b7o\u00b7be\u00b7rung", "zie\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Das nackte fl\u00fcgel-kind", "tokens": ["Das", "nack\u00b7te", "fl\u00fc\u00b7gel\u00b7kind"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Beschwinget die pasteyen,", "tokens": ["Be\u00b7schwin\u00b7get", "die", "pas\u00b7te\u00b7yen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und pfleget blut zu streuen;", "tokens": ["Und", "pfle\u00b7get", "blut", "zu", "streu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So, wie der blitz geschwind", "tokens": ["So", ",", "wie", "der", "blitz", "ge\u00b7schwind"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die trefflichste vestung mit grimme best\u00fcrmet,", "tokens": ["Die", "treff\u00b7lichs\u00b7te", "ves\u00b7tung", "mit", "grim\u00b7me", "be\u00b7st\u00fcr\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Da\u00df m\u00e4chtiges feuer sich reichlich aufth\u00fcrmet.", "tokens": ["Da\u00df", "m\u00e4ch\u00b7ti\u00b7ges", "feu\u00b7er", "sich", "reich\u00b7lich", "auft\u00b7h\u00fcr\u00b7met", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PRF", "ADJD", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Zwar wird ein liebes kind", "tokens": ["Zwar", "wird", "ein", "lie\u00b7bes", "kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Amazonen verglichen,", "tokens": ["A\u00b7maz\u00b7o\u00b7nen", "ver\u00b7gli\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Sie ist noch nie gewichen,", "tokens": ["Sie", "ist", "noch", "nie", "ge\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn auch gleich fturm und wind", "tokens": ["Wenn", "auch", "gleich", "fturm", "und", "wind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ADJD", "KON", "XY"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die wercke der vestung hat wollen durchl\u00f6chern,", "tokens": ["Die", "wer\u00b7cke", "der", "ves\u00b7tung", "hat", "wol\u00b7len", "durch\u00b7l\u00f6\u00b7chern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "VMFIN", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "So fehlt es ihr weder an pfeilen noch k\u00f6chern.", "tokens": ["So", "fehlt", "es", "ihr", "we\u00b7der", "an", "pfei\u00b7len", "noch", "k\u00f6\u00b7chern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "KON", "APPR", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Der augen helles paar", "tokens": ["Der", "au\u00b7gen", "hel\u00b7les", "paar"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wirfft leichte feuer-ballen,", "tokens": ["Wirfft", "leich\u00b7te", "feu\u00b7er\u00b7bal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So aus der vestung fallen,", "tokens": ["So", "aus", "der", "ves\u00b7tung", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit mercklicher gefahr,", "tokens": ["Mit", "merck\u00b7li\u00b7cher", "ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die ihren belagerer hefftig bestricken,", "tokens": ["Die", "ih\u00b7ren", "be\u00b7la\u00b7ge\u00b7rer", "heff\u00b7tig", "be\u00b7stri\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "ADJD", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und ihm so viel blitze als blicke zuschicken.", "tokens": ["Und", "ihm", "so", "viel", "blit\u00b7ze", "als", "bli\u00b7cke", "zu\u00b7schi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADV", "VVFIN", "KOKOM", "VVFIN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Der br\u00fcste hoher wall", "tokens": ["Der", "br\u00fcs\u00b7te", "ho\u00b7her", "wall"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dient statt der batterien,", "tokens": ["Dient", "statt", "der", "bat\u00b7te\u00b7ri\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf welchen kugeln gl\u00fchen;", "tokens": ["Auf", "wel\u00b7chen", "ku\u00b7geln", "gl\u00fc\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Trotz! sprach sie, da\u00df ein schall", "tokens": ["Trotz", "!", "sprach", "sie", ",", "da\u00df", "ein", "schall"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der donnernden m\u00f6rsel und krachenden st\u00fccke", "tokens": ["Der", "don\u00b7nern\u00b7den", "m\u00f6r\u00b7sel", "und", "kra\u00b7chen\u00b7den", "st\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "An meine bew\u00e4hrte pasteyen anr\u00fccke.", "tokens": ["An", "mei\u00b7ne", "be\u00b7w\u00e4hr\u00b7te", "pas\u00b7te\u00b7yen", "an\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Doch wei\u00df der schlauhe feind", "tokens": ["Doch", "wei\u00df", "der", "schlau\u00b7he", "feind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die burg zu attaqviren,", "tokens": ["Die", "burg", "zu", "at\u00b7ta\u00b7qvi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und da zu avaneiren,", "tokens": ["Und", "da", "zu", "a\u00b7va\u00b7nei\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo man es nicht gemeynt.", "tokens": ["Wo", "man", "es", "nicht", "ge\u00b7meynt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die \u00e4usersten posten sind schleunig erobert,", "tokens": ["Die", "\u00e4u\u00b7sers\u00b7ten", "pos\u00b7ten", "sind", "schleu\u00b7nig", "er\u00b7o\u00b7bert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Es wird die besatzung zur\u00fccke gekobert.", "tokens": ["Es", "wird", "die", "be\u00b7sat\u00b7zung", "zu\u00b7r\u00fc\u00b7cke", "ge\u00b7ko\u00b7bert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Und gleichwol war ihr sinn", "tokens": ["Und", "gleich\u00b7wol", "war", "ihr", "sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zur g\u00fcte nicht zu bringen,", "tokens": ["Zur", "g\u00fc\u00b7te", "nicht", "zu", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie sprach: ich will noch ringen,", "tokens": ["Sie", "sprach", ":", "ich", "will", "noch", "rin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VMFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So lang ich munter bin:", "tokens": ["So", "lang", "ich", "mun\u00b7ter", "bin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich liebe raison, und verachte die memmen,", "tokens": ["Ich", "lie\u00b7be", "rai\u00b7son", ",", "und", "ver\u00b7ach\u00b7te", "die", "mem\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Die lieber in milch als in blute sich schwemmen.", "tokens": ["Die", "lie\u00b7ber", "in", "milch", "als", "in", "blu\u00b7te", "sich", "schwem\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PIAT", "KOKOM", "APPR", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.8": {"line.1": {"text": "Dem feinde wuchs der grimm,", "tokens": ["Dem", "fein\u00b7de", "wuchs", "der", "grimm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Er eilt\u2019 in vollem wurme,", "tokens": ["Er", "eilt'", "in", "vol\u00b7lem", "wur\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und h\u00e4uffte sturm mit sturme,", "tokens": ["Und", "h\u00e4uff\u00b7te", "sturm", "mit", "stur\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nun stund es ziemlich schlimm:", "tokens": ["Nun", "stund", "es", "ziem\u00b7lich", "schlimm", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er trieb die belagerte ziemlich zu chore,", "tokens": ["Er", "trieb", "die", "be\u00b7la\u00b7ger\u00b7te", "ziem\u00b7lich", "zu", "cho\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADV", "APPR", "NE", "$,"], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und schraubte pedarden an schl\u00f6sser und thore.", "tokens": ["Und", "schraub\u00b7te", "pe\u00b7dar\u00b7den", "an", "schl\u00f6s\u00b7ser", "und", "tho\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ADJA", "KON", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.9": {"line.1": {"text": "Da bat sie um accord,", "tokens": ["Da", "bat", "sie", "um", "ac\u00b7cord", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Lie\u00df die chamade schlagen,", "tokens": ["Lie\u00df", "die", "cha\u00b7ma\u00b7de", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wolte nichts mehr wagen:", "tokens": ["Und", "wol\u00b7te", "nichts", "mehr", "wa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es war nur um ein wort,", "tokens": ["Es", "war", "nur", "um", "ein", "wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "So wurde der sieger zur g\u00fcte bewogen,", "tokens": ["So", "wur\u00b7de", "der", "sie\u00b7ger", "zur", "g\u00fc\u00b7te", "be\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und kam durch die presche mit piqven gezogen.", "tokens": ["Und", "kam", "durch", "die", "pre\u00b7sche", "mit", "piq\u00b7ven", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "APPR", "NE", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.10": {"line.1": {"text": "Es war schon sp\u00e4te nacht,", "tokens": ["Es", "war", "schon", "sp\u00e4\u00b7te", "nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Als es das gl\u00fccke f\u00fcgte,", "tokens": ["Als", "es", "das", "gl\u00fc\u00b7cke", "f\u00fcg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df er die burg besiegte,", "tokens": ["Da\u00df", "er", "die", "burg", "be\u00b7sieg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da ward sie ausgelacht.", "tokens": ["Da", "ward", "sie", "aus\u00b7ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Indem sie sich schmeichelte v\u00f6llig zu siegen;", "tokens": ["In\u00b7dem", "sie", "sich", "schmei\u00b7chel\u00b7te", "v\u00f6l\u00b7lig", "zu", "sie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "So hie\u00df es: beqveme dich unten zu liegen.", "tokens": ["So", "hie\u00df", "es", ":", "be\u00b7qve\u00b7me", "dich", "un\u00b7ten", "zu", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.11": {"line.1": {"text": "Die strafe gieng noch hin,", "tokens": ["Die", "stra\u00b7fe", "gieng", "noch", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So ihr der Mars dietirte,", "tokens": ["So", "ihr", "der", "Mars", "die\u00b7tir\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Damit er auch verirte", "tokens": ["Da\u00b7mit", "er", "auch", "ver\u00b7ir\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den schlechten helden-sinn:", "tokens": ["Den", "schlech\u00b7ten", "hel\u00b7den\u00b7sinn", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie solte nach wenigen monden und tagen", "tokens": ["Sie", "sol\u00b7te", "nach", "we\u00b7ni\u00b7gen", "mon\u00b7den", "und", "ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN", "KON", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Die trommel anhengen und \u00fcberall tragen.", "tokens": ["Die", "trom\u00b7mel", "an\u00b7hen\u00b7gen", "und", "\u00fc\u00b7be\u00b7rall", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}