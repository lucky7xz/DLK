{"textgrid.poem.32566": {"metadata": {"author": {"name": "Wieland, Christoph Martin", "birth": "N.A.", "death": "N.A."}, "title": "Ode an Schinz", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn du Daphnen umarmst, und ihr geliebtes Aug", "tokens": ["Wenn", "du", "Daph\u00b7nen", "um\u00b7armst", ",", "und", "ihr", "ge\u00b7lieb\u00b7tes", "Aug"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Alles, was Sie empfindet, sagt,", "tokens": ["Al\u00b7les", ",", "was", "Sie", "emp\u00b7fin\u00b7det", ",", "sagt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "$,", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und vor himmlischer Lust, Freund, dein gef\u00fchlvoll Herz", "tokens": ["Und", "vor", "himm\u00b7li\u00b7scher", "Lust", ",", "Freund", ",", "dein", "ge\u00b7f\u00fchl\u00b7voll", "Herz"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "$,", "NN", "$,", "PPOSAT", "ADJD", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "An dem Herzen der Freundin bebt,", "tokens": ["An", "dem", "Her\u00b7zen", "der", "Freun\u00b7din", "bebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wenn dein Blick itzt an ihr voller Entz\u00fcckung h\u00e4ngt,", "tokens": ["Wenn", "dein", "Blick", "itzt", "an", "ihr", "vol\u00b7ler", "Ent\u00b7z\u00fc\u00b7ckung", "h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Sieht, wie Unschuld und Z\u00e4rtlichkeit", "tokens": ["Sieht", ",", "wie", "Un\u00b7schuld", "und", "Z\u00e4rt\u00b7lich\u00b7keit"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ADJD", "KON", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Jede Miene belebt, wenn Du, in ihrem Ku\u00df", "tokens": ["Je\u00b7de", "Mie\u00b7ne", "be\u00b7lebt", ",", "wenn", "Du", ",", "in", "ih\u00b7rem", "Ku\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "VVPP", "$,", "KOUS", "PPER", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Ganz ges\u00e4ttigt, zu gro\u00df dich f\u00fchlst,", "tokens": ["Ganz", "ge\u00b7s\u00e4t\u00b7tigt", ",", "zu", "gro\u00df", "dich", "f\u00fchlst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "PTKA", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.9": {"text": "Goldne W\u00fcnsche zu tun; sprich mein Geliebtester,", "tokens": ["Gold\u00b7ne", "W\u00fcn\u00b7sche", "zu", "tun", ";", "sprich", "mein", "Ge\u00b7lieb\u00b7tes\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$.", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Wenn, von Daphnen geliebt zu sein,", "tokens": ["Wenn", ",", "von", "Daph\u00b7nen", "ge\u00b7liebt", "zu", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "NN", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Wenn der gro\u00dfe Gedank ganz deine Seele f\u00fcllt,", "tokens": ["Wenn", "der", "gro\u00b7\u00dfe", "Ge\u00b7dank", "ganz", "dei\u00b7ne", "See\u00b7le", "f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "Und kein Trieb ist, den Daphne nicht", "tokens": ["Und", "kein", "Trieb", "ist", ",", "den", "Daph\u00b7ne", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VAFIN", "$,", "ART", "NN", "PTKNEG"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Ganz beruhiget hat, f\u00fchlst du, o Schinz, dann nicht", "tokens": ["Ganz", "be\u00b7ru\u00b7hi\u00b7get", "hat", ",", "f\u00fchlst", "du", ",", "o", "Schinz", ",", "dann", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "VAFIN", "$,", "VVFIN", "PPER", "$,", "FM", "NN", "$,", "ADV", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Diesen einzigen Wunsch in dir:", "tokens": ["Die\u00b7sen", "ein\u00b7zi\u00b7gen", "Wunsch", "in", "dir", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "\u00bbm\u00f6chtest Du auch hier sein, der du mich ferne liebst,", "tokens": ["\u00bb", "m\u00f6ch\u00b7test", "Du", "auch", "hier", "sein", ",", "der", "du", "mich", "fer\u00b7ne", "liebst", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "ADV", "ADV", "VAINF", "$,", "PRELS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Der du fern von Sophiens Arm", "tokens": ["Der", "du", "fern", "von", "So\u00b7phi\u00b7ens", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "APPR", "NE", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Dein Verh\u00e4ngnis beweinst, und noch die Tr\u00e4nen mehr,", "tokens": ["Dein", "Ver\u00b7h\u00e4ng\u00b7nis", "be\u00b7weinst", ",", "und", "noch", "die", "Tr\u00e4\u00b7nen", "mehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KON", "ADV", "ART", "NN", "ADV", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Die die himmlische Freundin weint,", "tokens": ["Die", "die", "himm\u00b7li\u00b7sche", "Freun\u00b7din", "weint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "M\u00f6chtest du auch hier sein! W\u00e4rst Du der Seligkeit", "tokens": ["M\u00f6ch\u00b7test", "du", "auch", "hier", "sein", "!", "W\u00e4rst", "Du", "der", "Se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VAINF", "$.", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zeuge, die itzt mein Leben kr\u00f6nt,", "tokens": ["Zeu\u00b7ge", ",", "die", "itzt", "mein", "Le\u00b7ben", "kr\u00f6nt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Jener, deren Gestalt sich vor dein w\u00fcnschend Herz", "tokens": ["Je\u00b7ner", ",", "de\u00b7ren", "Ge\u00b7stalt", "sich", "vor", "dein", "w\u00fcn\u00b7schend", "Herz"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "$,", "PRELAT", "NN", "PRF", "APPR", "PPOSAT", "ADJD", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Stellte, da du Balsoren sangst!", "tokens": ["Stell\u00b7te", ",", "da", "du", "Bal\u00b7so\u00b7ren", "sangst", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "+--+---+", "measure": "dactylic.di.plus"}, "line.9": {"text": "W\u00e4re die auch bei uns, die du so z\u00e4rtlich liebst,", "tokens": ["W\u00e4\u00b7re", "die", "auch", "bei", "uns", ",", "die", "du", "so", "z\u00e4rt\u00b7lich", "liebst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADV", "APPR", "PPER", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.10": {"text": "Die so himmlisch dich wiederliebt!", "tokens": ["Die", "so", "himm\u00b7lisch", "dich", "wie\u00b7der\u00b7liebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "O, was f\u00fchlten wir dann, Wieland, was f\u00fchlten wir!", "tokens": ["O", ",", "was", "f\u00fchl\u00b7ten", "wir", "dann", ",", "Wie\u00b7land", ",", "was", "f\u00fchl\u00b7ten", "wir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VVFIN", "PPER", "ADV", "$,", "NN", "$,", "PWS", "VVFIN", "PPER", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "O, wie z\u00e4rtlich umarmten sich", "tokens": ["O", ",", "wie", "z\u00e4rt\u00b7lich", "um\u00b7arm\u00b7ten", "sich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ADJD", "VVFIN", "PRF"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.13": {"text": "Unsre Freundinnen dann! o, wie umarmten wir", "tokens": ["Uns\u00b7re", "Freun\u00b7din\u00b7nen", "dann", "!", "o", ",", "wie", "um\u00b7arm\u00b7ten", "wir"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "$.", "FM", "$,", "PWAV", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Uns bei ihren Umarmungen!", "tokens": ["Uns", "bei", "ih\u00b7ren", "Um\u00b7ar\u00b7mun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Auch der segnete dann unsrer Empfindung zu,", "tokens": ["Auch", "der", "seg\u00b7ne\u00b7te", "dann", "uns\u00b7rer", "Emp\u00b7fin\u00b7dung", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Dessen Nam uns zur Tugend weckt,", "tokens": ["Des\u00b7sen", "Nam", "uns", "zur", "Tu\u00b7gend", "weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.17": {"text": "Mit Sokratischem Blick l\u00e4chelte Bodmer oft", "tokens": ["Mit", "Sok\u00b7ra\u00b7ti\u00b7schem", "Blick", "l\u00e4\u00b7chel\u00b7te", "Bod\u00b7mer", "oft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Unsrer edleren Liebe zu.", "tokens": ["Uns\u00b7rer", "ed\u00b7le\u00b7ren", "Lie\u00b7be", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.19": {"text": "O dann f\u00e4nd uns die Ruh mit der \u00e4therschen Lust", "tokens": ["O", "dann", "f\u00e4nd", "uns", "die", "Ruh", "mit", "der", "\u00e4t\u00b7her\u00b7schen", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.20": {"text": "In gesangvollen Hainen gehn,", "tokens": ["In", "ge\u00b7sang\u00b7vol\u00b7len", "Hai\u00b7nen", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.21": {"text": "Unter Lauben, wo gern, weil sie die Einfalt liebt,", "tokens": ["Un\u00b7ter", "Lau\u00b7ben", ",", "wo", "gern", ",", "weil", "sie", "die", "Ein\u00b7falt", "liebt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "Sich die Weisheit zur Freundschaft findt.", "tokens": ["Sich", "die", "Weis\u00b7heit", "zur", "Freund\u00b7schaft", "findt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.23": {"text": "O dann w\u00e4ren wir, Freund, seliger, als voreinst", "tokens": ["O", "dann", "w\u00e4\u00b7ren", "wir", ",", "Freund", ",", "se\u00b7li\u00b7ger", ",", "als", "vor\u00b7e\u00b7inst"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["ITJ", "ADV", "VAFIN", "PPER", "$,", "NN", "$,", "ADJD", "$,", "KOUS", "NN"], "meter": "+-+--++-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "Die Bewohner Arkadiens,", "tokens": ["Die", "Be\u00b7woh\u00b7ner", "Ar\u00b7ka\u00b7di\u00b7ens", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.25": {"text": "Wo die Unschuld und Lust l\u00e4chelnder Nymphen Reihn", "tokens": ["Wo", "die", "Un\u00b7schuld", "und", "Lust", "l\u00e4\u00b7cheln\u00b7der", "Nym\u00b7phen", "Reihn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "KON", "NN", "ADJA", "NN", "NE"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.26": {"text": "Zu harmonischen T\u00e4nzen rief.\u00ab", "tokens": ["Zu", "har\u00b7mo\u00b7ni\u00b7schen", "T\u00e4n\u00b7zen", "rief", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "W\u00fcnscht dein Herz nicht so, wenn Du in Daphnens Arm", "tokens": ["W\u00fcnscht", "dein", "Herz", "nicht", "so", ",", "wenn", "Du", "in", "Daph\u00b7nens", "Arm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "PTKNEG", "ADV", "$,", "KOUS", "PPER", "APPR", "NE", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Mehr die Triebe nach Freunden f\u00fchlst?", "tokens": ["Mehr", "die", "Trie\u00b7be", "nach", "Freun\u00b7den", "f\u00fchlst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Ja, so w\u00fcnschet mein Schinz! ach! warum h\u00f6rest Du", "tokens": ["Ja", ",", "so", "w\u00fcn\u00b7schet", "mein", "Schinz", "!", "ach", "!", "wa\u00b7rum", "h\u00f6\u00b7rest", "Du"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$.", "XY", "$.", "PWAV", "VVFIN", "PPER"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Unsre weiseste W\u00fcnsche nicht,", "tokens": ["Uns\u00b7re", "wei\u00b7ses\u00b7te", "W\u00fcn\u00b7sche", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKNEG", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Der du niemals geh\u00f6rt, da\u00df ein gemeiner Wunsch", "tokens": ["Der", "du", "nie\u00b7mals", "ge\u00b7h\u00f6rt", ",", "da\u00df", "ein", "ge\u00b7mei\u00b7ner", "Wunsch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Mein erhabneres Herz entweiht!", "tokens": ["Mein", "er\u00b7hab\u00b7ne\u00b7res", "Herz", "ent\u00b7weiht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "O! wie w\u00e4ren wir dann gl\u00fccklich! dann w\u00fcnschten wir", "tokens": ["O", "!", "wie", "w\u00e4\u00b7ren", "wir", "dann", "gl\u00fcck\u00b7lich", "!", "dann", "w\u00fcnschten", "wir"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "PWAV", "VAFIN", "PPER", "ADV", "ADJD", "$.", "ADV", "VVFIN", "PPER"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Nimmer! heitre Zufriedenheit,", "tokens": ["Nim\u00b7mer", "!", "heit\u00b7re", "Zu\u00b7frie\u00b7den\u00b7heit", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$.", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.9": {"text": "Wie die Liebe sie schenkt, breitete dann um uns", "tokens": ["Wie", "die", "Lie\u00b7be", "sie", "schenkt", ",", "brei\u00b7te\u00b7te", "dann", "um", "uns"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "APPR", "PPER"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Ihre Schwanengefieder aus,", "tokens": ["Ih\u00b7re", "Schwa\u00b7nen\u00b7ge\u00b7fie\u00b7der", "aus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.11": {"text": "Jede Stunde, die wir lebten, der g\u00e4best Du,", "tokens": ["Je\u00b7de", "Stun\u00b7de", ",", "die", "wir", "leb\u00b7ten", ",", "der", "g\u00e4\u00b7best", "Du", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "Weisheit, neuen verschiedenen Reiz;", "tokens": ["Weis\u00b7heit", ",", "neu\u00b7en", "ver\u00b7schie\u00b7de\u00b7nen", "Reiz", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "O! wir lebten dann so, wie man der Ewigkeit", "tokens": ["O", "!", "wir", "leb\u00b7ten", "dann", "so", ",", "wie", "man", "der", "E\u00b7wig\u00b7keit"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "PPER", "VVFIN", "ADV", "ADV", "$,", "PWAV", "PIS", "ART", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "Und der n\u00e4hern Gottheit lebt!", "tokens": ["Und", "der", "n\u00e4\u00b7hern", "Got\u00b7theit", "lebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}