{"textgrid.poem.43829": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Winter und die Spatzen", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie zwitscherten und sangen,", "tokens": ["Sie", "zwit\u00b7scher\u00b7ten", "und", "san\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Man h\u00f6rte kaum sein Wort:", "tokens": ["Man", "h\u00f6r\u00b7te", "kaum", "sein", "Wort", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Winter ist gegangen", "tokens": ["Der", "Win\u00b7ter", "ist", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und alles Leid ist fort! \u2013", "tokens": ["Und", "al\u00b7les", "Leid", "ist", "fort", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ei, wartet nur, ihr Spatzen!", "tokens": ["Ei", ",", "war\u00b7tet", "nur", ",", "ihr", "Spat\u00b7zen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sollt mich schon wieder seh'n.", "tokens": ["Sollt", "mich", "schon", "wie\u00b7der", "seh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Zwitschern und das Schwatzen,", "tokens": ["Das", "Zwit\u00b7schern", "und", "das", "Schwat\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das soll euch bald vergeh'n! \u2013", "tokens": ["Das", "soll", "euch", "bald", "ver\u00b7geh'n", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da kam der Winter wieder,", "tokens": ["Da", "kam", "der", "Win\u00b7ter", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er brachte K\u00e4lt' und Schnee;", "tokens": ["Er", "brach\u00b7te", "K\u00e4lt'", "und", "Schnee", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da gab es keine Lieder,", "tokens": ["Da", "gab", "es", "kei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein fr\u00f6hliches Juchhe.", "tokens": ["Kein", "fr\u00f6h\u00b7li\u00b7ches", "Juch\u00b7he", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Spatzen aber sa\u00dfen", "tokens": ["Die", "Spat\u00b7zen", "a\u00b7ber", "sa\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vergn\u00fcgt in Stall und Haus:", "tokens": ["Ver\u00b7gn\u00fcgt", "in", "Stall", "und", "Haus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Winter, la\u00df das Spa\u00dfen!", "tokens": ["O", "Win\u00b7ter", ",", "la\u00df", "das", "Spa\u00b7\u00dfen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVIMP", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wir lachen dich doch aus. \u2013", "tokens": ["Wir", "la\u00b7chen", "dich", "doch", "aus", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So ist es auch ergangen:", "tokens": ["So", "ist", "es", "auch", "er\u00b7gan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kaum war der Winter fort,", "tokens": ["Kaum", "war", "der", "Win\u00b7ter", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Spatzen fr\u00f6hlich sangen,", "tokens": ["Die", "Spat\u00b7zen", "fr\u00f6h\u00b7lich", "san\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Man h\u00f6rte kaum sein Wort.", "tokens": ["Man", "h\u00f6r\u00b7te", "kaum", "sein", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}