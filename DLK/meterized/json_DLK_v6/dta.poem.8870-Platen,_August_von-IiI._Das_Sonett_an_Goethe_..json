{"dta.poem.8870": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "IiI.  \n  Das Sonett an Goethe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Mein tiefes Wesen witzig sah verneinen,", "tokens": ["Mein", "tie\u00b7fes", "We\u00b7sen", "wit\u00b7zig", "sah", "ver\u00b7nei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dich selbst nun z\u00e4hl' ich heute zu den Meinen,", "tokens": ["Dich", "selbst", "nun", "z\u00e4hl'", "ich", "heu\u00b7te", "zu", "den", "Mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu Denen, welche meine Gunst erfahren.", "tokens": ["Zu", "De\u00b7nen", ",", "wel\u00b7che", "mei\u00b7ne", "Gunst", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn wer durchdrungen ist vom innig Wahren,", "tokens": ["Denn", "wer", "durch\u00b7drun\u00b7gen", "ist", "vom", "in\u00b7nig", "Wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVPP", "VAFIN", "APPRART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem mu\u00df die Form sich unbewu\u00dft vereinen,", "tokens": ["Dem", "mu\u00df", "die", "Form", "sich", "un\u00b7be\u00b7wu\u00dft", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und was dem St\u00fcmper mag gef\u00e4hrlich scheinen,", "tokens": ["Und", "was", "dem", "St\u00fcm\u00b7per", "mag", "ge\u00b7f\u00e4hr\u00b7lich", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VMFIN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das mu\u00df den Meister g\u00f6ttlich offenbaren.", "tokens": ["Das", "mu\u00df", "den", "Meis\u00b7ter", "g\u00f6tt\u00b7lich", "of\u00b7fen\u00b7ba\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wem Kraft und F\u00fclle tief im Busen keimen,", "tokens": ["Wem", "Kraft", "und", "F\u00fcl\u00b7le", "tief", "im", "Bu\u00b7sen", "kei\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Wort beherrscht er mit gerechtem Stolze,", "tokens": ["Das", "Wort", "be\u00b7herrscht", "er", "mit", "ge\u00b7rech\u00b7tem", "Stol\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bewegt sich leicht, wenn auch in schweren Reimen.", "tokens": ["Be\u00b7wegt", "sich", "leicht", ",", "wenn", "auch", "in", "schwe\u00b7ren", "Rei\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "$,", "KOUS", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er schneidet sich des Liedes fl\u00fccht'ge Bolze", "tokens": ["Er", "schnei\u00b7det", "sich", "des", "Lie\u00b7des", "fl\u00fccht'\u00b7ge", "Bol\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gewandt und sicher, ohne je zu leimen,", "tokens": ["Ge\u00b7wandt", "und", "si\u00b7cher", ",", "oh\u00b7ne", "je", "zu", "lei\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "$,", "KOUI", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und was er fertigt, ist aus ganzem Holze.", "tokens": ["Und", "was", "er", "fer\u00b7tigt", ",", "ist", "aus", "gan\u00b7zem", "Hol\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVPP", "$,", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}