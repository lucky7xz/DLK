{"dta.poem.17936": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "179.  Der Geitzigen und Weisen wirkung", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Der Geitzhals mu\u00df darvon/ l\u00e4st anderen sein Geld;", "tokens": ["Der", "Geitz\u00b7hals", "mu\u00df", "dar\u00b7von", "/", "l\u00e4st", "an\u00b7de\u00b7ren", "sein", "Geld", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PAV", "$(", "VVFIN", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Weise schickts f\u00fcr sich voran in jene Welt.", "tokens": ["Der", "Wei\u00b7se", "schickts", "f\u00fcr", "sich", "vo\u00b7ran", "in", "je\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PRF", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}