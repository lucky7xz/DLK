{"dta.poem.7988": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die V\u00e4tergruft .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Es ging wohl \u00fcber die Haide", "tokens": ["Es", "ging", "wohl", "\u00fc\u00b7ber", "die", "Hai\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zur alten Kapell\u2019 empor", "tokens": ["Zur", "al\u00b7ten", "Ka\u00b7pell'", "em\u00b7por"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein Greis im Waffengeschmeide,", "tokens": ["Ein", "Greis", "im", "Waf\u00b7fen\u00b7ge\u00b7schmei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und trat in den dunkeln Chor.", "tokens": ["Und", "trat", "in", "den", "dun\u00b7keln", "Chor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die S\u00e4rge seiner Ahnen", "tokens": ["Die", "S\u00e4r\u00b7ge", "sei\u00b7ner", "Ah\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Standen die Hall\u2019 entlang,", "tokens": ["Stan\u00b7den", "die", "Hall'", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Aus der Tiefe th\u00e4t ihn mahnen", "tokens": ["Aus", "der", "Tie\u00b7fe", "th\u00e4t", "ihn", "mah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein wunderbarer Gesang.", "tokens": ["Ein", "wun\u00b7der\u00b7ba\u00b7rer", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "\u201ewohl hab\u2019 ich euer Gr\u00fc\u00dfen,", "tokens": ["\u201e", "wohl", "hab'", "ich", "eu\u00b7er", "Gr\u00fc\u00b7\u00dfen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Heldengeister! geh\u00f6rt.", "tokens": ["Ihr", "Hel\u00b7den\u00b7geis\u00b7ter", "!", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Eure Reihe soll ich schlie\u00dfen:", "tokens": ["Eu\u00b7re", "Rei\u00b7he", "soll", "ich", "schlie\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heil mir! ich bin es werth.\u201c", "tokens": ["Heil", "mir", "!", "ich", "bin", "es", "werth", ".", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "$.", "PPER", "VAFIN", "PPER", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es stand an k\u00fchler St\u00e4tte", "tokens": ["Es", "stand", "an", "k\u00fch\u00b7ler", "St\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Sarg noch ungef\u00fcllt,", "tokens": ["Ein", "Sarg", "noch", "un\u00b7ge\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den nahm er zum Ruhebette,", "tokens": ["Den", "nahm", "er", "zum", "Ru\u00b7he\u00b7bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zum Pf\u00fchle nahm er den Schild.", "tokens": ["Zum", "Pf\u00fch\u00b7le", "nahm", "er", "den", "Schild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Die H\u00e4nde th\u00e4t er falten", "tokens": ["Die", "H\u00e4n\u00b7de", "th\u00e4t", "er", "fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf\u2019s Schwerdt, und schlummert\u2019 ein.", "tokens": ["Auf's", "Schwerdt", ",", "und", "schlum\u00b7mert'", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Geisterlaute verhallten;", "tokens": ["Die", "Geis\u00b7ter\u00b7lau\u00b7te", "ver\u00b7hall\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da mocht\u2019 es gar stille seyn.", "tokens": ["Da", "mocht'", "es", "gar", "stil\u00b7le", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJA", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}