{"textgrid.poem.42833": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Der Komiker", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Komiker von erstem Rang", "tokens": ["Ein", "Ko\u00b7mi\u00b7ker", "von", "ers\u00b7tem", "Rang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "Ging eine Stra\u00dfe links entlang.", "tokens": ["Ging", "ei\u00b7ne", "Stra\u00b7\u00dfe", "links", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Leute sagten rings umher", "tokens": ["Die", "Leu\u00b7te", "sag\u00b7ten", "rings", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hindeutend: \u00bbDas ist der und der!\u00ab", "tokens": ["Hin\u00b7deu\u00b7tend", ":", "\u00bb", "Das", "ist", "der", "und", "der", "!", "\u00ab"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "$.", "$(", "PDS", "VAFIN", "ART", "KON", "ART", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der Komiker fuhr aus der Haut", "tokens": ["Der", "Ko\u00b7mi\u00b7ker", "fuhr", "aus", "der", "Haut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nach Haus und w\u00fcrgte seine Braut.", "tokens": ["Nach", "Haus", "und", "w\u00fcrg\u00b7te", "sei\u00b7ne", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht etwa wie von ungef\u00e4hr,", "tokens": ["Nicht", "et\u00b7wa", "wie", "von", "un\u00b7ge\u00b7f\u00e4hr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KOKOM", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nein ernst, als ob das komisch w\u00e4r.", "tokens": ["Nein", "ernst", ",", "als", "ob", "das", "ko\u00b7misch", "w\u00e4r", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "$,", "KOKOM", "KOUS", "ART", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}