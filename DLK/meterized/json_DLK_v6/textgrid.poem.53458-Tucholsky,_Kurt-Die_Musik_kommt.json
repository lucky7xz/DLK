{"textgrid.poem.53458": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Die Musik kommt", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun zw\u00e4ngt, die sonst Musik die T\u00f6chter lehrte,", "tokens": ["Nun", "zw\u00e4ngt", ",", "die", "sonst", "Mu\u00b7sik", "die", "T\u00f6ch\u00b7ter", "lehr\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "ADV", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "sich ins Schwarzseidene mit dem Krachkorsett;", "tokens": ["sich", "ins", "Schwarz\u00b7sei\u00b7de\u00b7ne", "mit", "dem", "Krach\u00b7kor\u00b7sett", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "und da\u00df man Haydn, Bach und Koschat ehrte,", "tokens": ["und", "da\u00df", "man", "Haydn", ",", "Bach", "und", "Ko\u00b7schat", "ehr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "NN", "$,", "NE", "KON", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "beweist man durch Gesang und am Spinett.", "tokens": ["be\u00b7weist", "man", "durch", "Ge\u00b7sang", "und", "am", "Spi\u00b7nett", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+++", "measure": "zehnsilber"}}, "stanza.2": {"line.1": {"text": "Nun schlagen wieder l\u00f6wenm\u00e4hnige Meister", "tokens": ["Nun", "schla\u00b7gen", "wie\u00b7der", "l\u00f6\u00b7wen\u00b7m\u00e4h\u00b7ni\u00b7ge", "Meis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "mit ihren Pranken auf die Fl\u00fcgel ein,", "tokens": ["mit", "ih\u00b7ren", "Pran\u00b7ken", "auf", "die", "Fl\u00fc\u00b7gel", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und fiedelt jemand Violin, dann hei\u00dft er", "tokens": ["und", "fie\u00b7delt", "je\u00b7mand", "Vi\u00b7o\u00b7lin", ",", "dann", "hei\u00dft", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "NN", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mischka und soll erst sieben Jahre sein.", "tokens": ["Mischka", "und", "soll", "erst", "sie\u00b7ben", "Jah\u00b7re", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VMFIN", "ADV", "CARD", "NN", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Du siehst mich l\u00e4chelnd an, Eleonore \u2013", "tokens": ["Du", "siehst", "mich", "l\u00e4\u00b7chelnd", "an", ",", "E\u00b7leo\u00b7no\u00b7re", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "NE", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "auch du, Geliebte, seist ein Singtalent?", "tokens": ["auch", "du", ",", "Ge\u00b7lieb\u00b7te", ",", "seist", "ein", "Sing\u00b7ta\u00b7lent", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "NN", "$,", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch jach entfleucht durch meinem rechten Ohre,", "tokens": ["Doch", "jach", "ent\u00b7fleucht", "durch", "mei\u00b7nem", "rech\u00b7ten", "Oh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "was dein Sopran mir in das linke flennt.", "tokens": ["was", "dein", "Sop\u00b7ran", "mir", "in", "das", "lin\u00b7ke", "flennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PPER", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ach ja, der Herbst! Die Bl\u00e4tter werden gelber,", "tokens": ["Ach", "ja", ",", "der", "Herbst", "!", "Die", "Bl\u00e4t\u00b7ter", "wer\u00b7den", "gel\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$,", "ART", "NN", "$.", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und jedes M\u00e4dchen kriegt ein hohes C,", "tokens": ["und", "je\u00b7des", "M\u00e4d\u00b7chen", "kriegt", "ein", "ho\u00b7hes", "C", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und auch der Muhsikp\u00e4dagoge selber", "tokens": ["und", "auch", "der", "Muh\u00b7sik\u00b7p\u00e4\u00b7da\u00b7go\u00b7ge", "sel\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "stund auf und tremolieretee . . .", "tokens": ["stund", "auf", "und", "tre\u00b7mo\u00b7lie\u00b7re\u00b7tee", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "VVFIN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du Stadt der Lieder, bist du nicht verwundert?", "tokens": ["Du", "Stadt", "der", "Lie\u00b7der", ",", "bist", "du", "nicht", "ver\u00b7wun\u00b7dert", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,", "VAFIN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So jedes Jahr hast du um den Advent", "tokens": ["So", "je\u00b7des", "Jahr", "hast", "du", "um", "den", "Ad\u00b7vent"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Musikkonzerte St\u00fccker achtzehnhundert \u2013", "tokens": ["Mu\u00b7sik\u00b7kon\u00b7zer\u00b7te", "St\u00fc\u00b7cker", "acht\u00b7zehn\u00b7hun\u00b7dert", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "CARD", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "doch mit Gewinn: nur sechseinhalb Prozent.", "tokens": ["doch", "mit", "Ge\u00b7winn", ":", "nur", "sec\u00b7hsein\u00b7halb", "Pro\u00b7zent", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$.", "ADV", "CARD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}