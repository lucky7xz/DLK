{"textgrid.poem.39401": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zwei Musen stritten sich um den Besitz", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei Musen stritten sich um den Besitz", "tokens": ["Zwei", "Mu\u00b7sen", "strit\u00b7ten", "sich", "um", "den", "Be\u00b7sitz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Charlottens, und Thalia sprach zur Schwester:", "tokens": ["Char\u00b7lot\u00b7tens", ",", "und", "Tha\u00b7lia", "sprach", "zur", "Schwes\u00b7ter", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "NE", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Was gleicht an Anmuth ihrem heitern Witz?", "tokens": ["Was", "gleicht", "an", "An\u00b7muth", "ih\u00b7rem", "hei\u00b7tern", "Witz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein ist sie; und ich halte sie nur fester,", "tokens": ["Mein", "ist", "sie", ";", "und", "ich", "hal\u00b7te", "sie", "nur", "fes\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PPER", "$.", "KON", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Je mehr du sie verlockst zu deinem Thronensitz. \u2013", "tokens": ["Je", "mehr", "du", "sie", "ver\u00b7lockst", "zu", "dei\u00b7nem", "Thro\u00b7nen\u00b7sitz", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "PPER", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Melpomene begann: Ich ruf' herbei, als Richter,", "tokens": ["Mel\u00b7po\u00b7me\u00b7ne", "be\u00b7gann", ":", "Ich", "ruf'", "her\u00b7bei", ",", "als", "Rich\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "PPER", "VVFIN", "PTKVZ", "$,", "KOUS", "NN", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Den gro\u00dfen Britten, alle hohen Dichter.", "tokens": ["Den", "gro\u00b7\u00dfen", "Brit\u00b7ten", ",", "al\u00b7le", "ho\u00b7hen", "Dich\u00b7ter."], "token_info": ["word", "word", "word", "punct", "word", "word", "abbreviation"], "pos": ["ART", "ADJA", "NN", "$,", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wer sah nicht in der herrlichen Gestalt", "tokens": ["Wer", "sah", "nicht", "in", "der", "herr\u00b7li\u00b7chen", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der Leidenschaften Sturm, des Schicksals Allgewalt?", "tokens": ["Der", "Lei\u00b7den\u00b7schaf\u00b7ten", "Sturm", ",", "des", "Schick\u00b7sals", "All\u00b7ge\u00b7walt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wer weiht nicht, wenn sie klagt, ihr gern den Thau der Thr\u00e4nen,", "tokens": ["Wer", "weiht", "nicht", ",", "wenn", "sie", "klagt", ",", "ihr", "gern", "den", "Thau", "der", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "VVFIN", "$,", "PPER", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und f\u00fchlet s\u00fc\u00dfen Gram und ein unnennbar Sehnen? \u2013", "tokens": ["Und", "f\u00fch\u00b7let", "s\u00fc\u00b7\u00dfen", "Gram", "und", "ein", "un\u00b7nenn\u00b7bar", "Seh\u00b7nen", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "KON", "ART", "ADJD", "NN", "$.", "$("], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}, "line.12": {"text": "Die Freundschaft trat hinzu, und fl\u00fcsterte zu ihr:", "tokens": ["Die", "Freund\u00b7schaft", "trat", "hin\u00b7zu", ",", "und", "fl\u00fcs\u00b7ter\u00b7te", "zu", "ihr", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Charlotte, h\u00f6r' auch mich! Ich weile still bei dir.", "tokens": ["Char\u00b7lot\u00b7te", ",", "h\u00f6r'", "auch", "mich", "!", "Ich", "wei\u00b7le", "still", "bei", "dir", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "PPER", "$.", "PPER", "VVFIN", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ja! deinem Genius wird jeder Zauber gl\u00fccken;", "tokens": ["Ja", "!", "dei\u00b7nem", "Ge\u00b7nius", "wird", "je\u00b7der", "Zau\u00b7ber", "gl\u00fc\u00b7cken", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPOSAT", "NN", "VAFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Doch sei du selbst! du wirst mich mehr entz\u00fccken.", "tokens": ["Doch", "sei", "du", "selbst", "!", "du", "wirst", "mich", "mehr", "ent\u00b7z\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}