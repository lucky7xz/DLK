{"textgrid.poem.26572": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Blick' ich dich an, du Hohe, Schlanke,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Blick' ich dich an, du Hohe, Schlanke,", "tokens": ["Blick'", "ich", "dich", "an", ",", "du", "Ho\u00b7he", ",", "Schlan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKVZ", "$,", "PPER", "NE", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So wei\u00df ich nicht, wie mir geschieht;", "tokens": ["So", "wei\u00df", "ich", "nicht", ",", "wie", "mir", "ge\u00b7schieht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schon lang umkreis't dich mein Gedanke,", "tokens": ["Schon", "lang", "um\u00b7kreis't", "dich", "mein", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der immer wieder scheu entflieht.", "tokens": ["Der", "im\u00b7mer", "wie\u00b7der", "scheu", "ent\u00b7flieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Noch hab' ich dich nicht ganz empfunden,", "tokens": ["Noch", "hab'", "ich", "dich", "nicht", "ganz", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch hab' ich dich nicht ganz erkannt;", "tokens": ["Noch", "hab'", "ich", "dich", "nicht", "ganz", "er\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch nicht den rechten Ton gefunden,", "tokens": ["Noch", "nicht", "den", "rech\u00b7ten", "Ton", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der dich in meine Lieder bannt.", "tokens": ["Der", "dich", "in", "mei\u00b7ne", "Lie\u00b7der", "bannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Von M\u00e4dchenw\u00fcnschen hold umwoben,", "tokens": ["Von", "M\u00e4d\u00b7chen\u00b7w\u00fcn\u00b7schen", "hold", "um\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Scheinst du oft tr\u00e4um'risch, wandelbar \u2013", "tokens": ["Scheinst", "du", "oft", "tr\u00e4um'\u00b7risch", ",", "wan\u00b7del\u00b7bar", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$,", "PWAV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann wieder, stolz das Haupt erhoben,", "tokens": ["Dann", "wie\u00b7der", ",", "stolz", "das", "Haupt", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So selbstbewu\u00dft, so k\u00fchl und klar.", "tokens": ["So", "selbst\u00b7be\u00b7wu\u00dft", ",", "so", "k\u00fchl", "und", "klar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Giebt's eine Liebe, dich zu lieben \u2013", "tokens": ["Giebt's", "ei\u00b7ne", "Lie\u00b7be", ",", "dich", "zu", "lie\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und die auch ", "tokens": ["Und", "die", "auch"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Wohl Mancher ist dir fern geblieben,", "tokens": ["Wohl", "Man\u00b7cher", "ist", "dir", "fern", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von solchem Zweifel leis' erschreckt.", "tokens": ["Von", "sol\u00b7chem", "Zwei\u00b7fel", "leis'", "er\u00b7schreckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Oft ist es mir, als sollt' ich nahen,", "tokens": ["Oft", "ist", "es", "mir", ",", "als", "sollt'", "ich", "na\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,", "KOUS", "VMFIN", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als sollt' ich fassen deine Hand,", "tokens": ["Als", "sollt'", "ich", "fas\u00b7sen", "dei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den zarten Leib dir sanft umfahen", "tokens": ["Den", "zar\u00b7ten", "Leib", "dir", "sanft", "um\u00b7fa\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und k\u00fcssen deiner Lippe Rand.", "tokens": ["Und", "k\u00fcs\u00b7sen", "dei\u00b7ner", "Lip\u00b7pe", "Rand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch folg' ich nicht dem Drang der Gluthen,", "tokens": ["Doch", "fol\u00b7g'", "ich", "nicht", "dem", "Drang", "der", "Glu\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der scheu aus meinem Innern bricht \u2013", "tokens": ["Der", "scheu", "aus", "mei\u00b7nem", "In\u00b7nern", "bricht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und m\u00f6chte nichts, als stumm verbluten", "tokens": ["Und", "m\u00f6ch\u00b7te", "nichts", ",", "als", "stumm", "ver\u00b7blu\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VMFIN", "PIS", "$,", "KOUS", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor deinem hellen Angesicht.", "tokens": ["Vor", "dei\u00b7nem", "hel\u00b7len", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}