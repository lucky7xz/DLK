{"dta.poem.9354": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "An seinen  \n  Martin Segern/ der Rechten Ge-  \n fliessenen/  \n nach der Melodey/ Frau Nachtigal/ etc.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wohlauf! mein Sinn/", "tokens": ["Wohl\u00b7auf", "!", "mein", "Sinn", "/"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$.", "PPOSAT", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "wirf alles hin", "tokens": ["wirf", "al\u00b7les", "hin"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PIS", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "was traurig ist/", "tokens": ["was", "trau\u00b7rig", "ist", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "her Lautenist/", "tokens": ["her", "Lau\u00b7te\u00b7nist", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "greiff an das s\u00fc\u00dfe Seitenspiel/", "tokens": ["greiff", "an", "das", "s\u00fc\u00b7\u00dfe", "Sei\u00b7ten\u00b7spiel", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "was wiltu noch verzihen viel.", "tokens": ["was", "wil\u00b7tu", "noch", "ver\u00b7zi\u00b7hen", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der m\u00fcde tag", "tokens": ["Der", "m\u00fc\u00b7de", "tag"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "nicht warten mag/", "tokens": ["nicht", "war\u00b7ten", "mag", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "e\u00df fliht die zeit", "tokens": ["e\u00df", "fliht", "die", "zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "die uns erfreut/", "tokens": ["die", "uns", "er\u00b7freut", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "der tag an dem ich spielen mu\u00df", "tokens": ["der", "tag", "an", "dem", "ich", "spie\u00b7len", "mu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PRELS", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "den s\u00fc\u00dfen toon mit \u00fcberflu\u00df.", "tokens": ["den", "s\u00fc\u00b7\u00dfen", "toon", "mit", "\u00fc\u00b7berf\u00b7lu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wohlauff mein Freund/", "tokens": ["Wohl\u00b7auff", "mein", "Freund", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "bi\u00df Luna scheint/", "tokens": ["bi\u00df", "Lu\u00b7na", "scheint", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "wann Wein und Nacht", "tokens": ["wann", "Wein", "und", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "uns lustig macht/", "tokens": ["uns", "lus\u00b7tig", "macht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "soll recht begehn Dein Nahmens-fest", "tokens": ["soll", "recht", "be\u00b7gehn", "Dein", "Nah\u00b7mens\u00b7fest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "der angeflam\u0303ten Sinnen-rest.", "tokens": ["der", "an\u00b7ge\u00b7flam\u0303ten", "Sin\u00b7nen\u00b7rest", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Heut ist Martin/", "tokens": ["Heut", "ist", "Mar\u00b7tin", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "nehmt Wintergr\u00fcn/", "tokens": ["nehmt", "Win\u00b7ter\u00b7gr\u00fcn", "/"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "macht Kr\u00e4ntze draus/", "tokens": ["macht", "Kr\u00e4nt\u00b7ze", "draus", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PAV", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Er gibt den schmau\u00df/", "tokens": ["Er", "gibt", "den", "schmau\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "hey! fr\u00f6lich durch die gantze nacht/", "tokens": ["hey", "!", "fr\u00f6\u00b7lich", "durch", "die", "gant\u00b7ze", "nacht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$.", "ADJD", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und seyd auf nichts als Lust bedacht.", "tokens": ["und", "seyd", "auf", "nichts", "als", "Lust", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PIS", "KOKOM", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Di\u00df sing ich dier/", "tokens": ["Di\u00df", "sing", "ich", "dier", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "mein Freund/ allhier/", "tokens": ["mein", "Freund", "/", "all\u00b7hier", "/"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ADV", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "sey freuden-voll/", "tokens": ["sey", "freu\u00b7den\u00b7voll", "/"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "der Himmel soll", "tokens": ["der", "Him\u00b7mel", "soll"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VMFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Dier fristen deine Lebens-zeit/", "tokens": ["Dier", "fris\u00b7ten", "dei\u00b7ne", "Le\u00b7bens\u00b7zeit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "nun leb\u2019 in lauter fr\u00f6liakeit!", "tokens": ["nun", "leb'", "in", "lau\u00b7ter", "fr\u00b7\u00f6\u00b7li\u00b7a\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}