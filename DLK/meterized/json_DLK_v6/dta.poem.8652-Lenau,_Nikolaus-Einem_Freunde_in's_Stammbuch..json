{"dta.poem.8652": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Einem Freunde in's Stammbuch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "R\u00fcstig wandelst du fort die Alpenpfade der Edlen,", "tokens": ["R\u00fcs\u00b7tig", "wan\u00b7delst", "du", "fort", "die", "Al\u00b7pen\u00b7pfa\u00b7de", "der", "Ed\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Wo die reinere Luft Busen und Stirne bek\u00fchlt,", "tokens": ["Wo", "die", "rei\u00b7ne\u00b7re", "Luft", "Bu\u00b7sen", "und", "Stir\u00b7ne", "be\u00b7k\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Pfl\u00fcckest vom Felsengeklipp', vom schmalen Rande des", "tokens": ["Pfl\u00fc\u00b7ckest", "vom", "Fel\u00b7sen\u00b7ge\u00b7klipp'", ",", "vom", "schma\u00b7len", "Ran\u00b7de", "des"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "ART"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Abgrunds", "tokens": ["Ab\u00b7grunds"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Duftende Blumen und schlingst sie zum harmonischen", "tokens": ["Duf\u00b7ten\u00b7de", "Blu\u00b7men", "und", "schlingst", "sie", "zum", "har\u00b7mo\u00b7ni\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "VVFIN", "PPER", "APPRART", "ADJA"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.6": {"text": "Kranz,", "tokens": ["Kranz", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Ihn zu tragen, ein Opfer, zum Hochaltare der Menschheit,", "tokens": ["Ihn", "zu", "tra\u00b7gen", ",", "ein", "Op\u00b7fer", ",", "zum", "Hoc\u00b7hal\u00b7ta\u00b7re", "der", "Menschheit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "ART", "NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "--+--+--+-+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Ach, um welchen es stets stiller und einsamer wird;", "tokens": ["Ach", ",", "um", "wel\u00b7chen", "es", "stets", "stil\u00b7ler", "und", "ein\u00b7sa\u00b7mer", "wird", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUI", "PWAT", "PPER", "ADV", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Traurig fl\u00fcstern auf ihm die Kr\u00e4nze der wenigen Edlen,", "tokens": ["Trau\u00b7rig", "fl\u00fcs\u00b7tern", "auf", "ihm", "die", "Kr\u00e4n\u00b7ze", "der", "we\u00b7ni\u00b7gen", "Ed\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "PPER", "ART", "NN", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Todtenkr\u00e4nze nunmehr sch\u00f6ner, verblichener Zeit.", "tokens": ["Tod\u00b7ten\u00b7kr\u00e4n\u00b7ze", "nun\u00b7mehr", "sch\u00f6\u00b7ner", ",", "ver\u00b7bli\u00b7che\u00b7ner", "Zeit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Aber du wandle hinan getrost, und w\u00e4re dein Leben", "tokens": ["A\u00b7ber", "du", "wand\u00b7le", "hi\u00b7nan", "ge\u00b7trost", ",", "und", "w\u00e4\u00b7re", "dein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "VVPP", "$,", "KON", "VAFIN", "PPOSAT", "NN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Auch nur Feier des Tod's sch\u00f6ner, verblichener Zeit.", "tokens": ["Auch", "nur", "Fei\u00b7er", "des", "To\u00b7d's", "sch\u00f6\u00b7ner", ",", "ver\u00b7bli\u00b7che\u00b7ner", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "ART", "NN", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "--+--+-+--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Kommt auf deinen Pfaden dir einst der Donner entgegen,", "tokens": ["Kommt", "auf", "dei\u00b7nen", "Pfa\u00b7den", "dir", "einst", "der", "Don\u00b7ner", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Dr\u00e4uend im n\u00e4chtlichen Flug, fahren Orkane dich an:", "tokens": ["Dr\u00e4u\u00b7end", "im", "n\u00e4cht\u00b7li\u00b7chen", "Flug", ",", "fah\u00b7ren", "Or\u00b7ka\u00b7ne", "dich", "an", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$,", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Freund, dann flattre dies Blatt vor deinen Blicken im", "tokens": ["Freund", ",", "dann", "flatt\u00b7re", "dies", "Blatt", "vor", "dei\u00b7nen", "Bli\u00b7cken", "im"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VVFIN", "PDS", "NN", "APPR", "PPOSAT", "NN", "APPRART"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Sturme,", "tokens": ["Stur\u00b7me", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Und es rausche dir zu: \u201edenke des liebenden Freunds!\u201c", "tokens": ["Und", "es", "rau\u00b7sche", "dir", "zu", ":", "\u201e", "den\u00b7ke", "des", "lie\u00b7ben\u00b7den", "Freunds", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}