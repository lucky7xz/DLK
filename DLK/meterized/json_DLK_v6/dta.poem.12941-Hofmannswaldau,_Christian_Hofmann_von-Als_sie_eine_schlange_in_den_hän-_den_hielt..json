{"dta.poem.12941": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie eine schlange in den h\u00e4n-  \n den hielt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das, was du mir in deinen h\u00e4nden weisest,", "tokens": ["Das", ",", "was", "du", "mir", "in", "dei\u00b7nen", "h\u00e4n\u00b7den", "wei\u00b7sest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "PRF", "APPR", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist keine wahre schlange nicht:", "tokens": ["Ist", "kei\u00b7ne", "wah\u00b7re", "schlan\u00b7ge", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es will nur deine grausamkeit,", "tokens": ["Es", "will", "nur", "dei\u00b7ne", "grau\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Damit du meine liebe speisest,", "tokens": ["Da\u00b7mit", "du", "mei\u00b7ne", "lie\u00b7be", "spei\u00b7sest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Unter dieses ungeziefers schein", "tokens": ["Un\u00b7ter", "die\u00b7ses", "un\u00b7ge\u00b7zie\u00b7fers", "schein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Uns sichtbar und handgreifflich seyn.", "tokens": ["Uns", "sicht\u00b7bar", "und", "hand\u00b7greif\u00b7flich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Zwar weil dein angesicht", "tokens": ["Zwar", "weil", "dein", "an\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ein feld voll rosen heissen kan,", "tokens": ["Ein", "feld", "voll", "ro\u00b7sen", "heis\u00b7sen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und deine weisse faust nur lilgen von sich streut,", "tokens": ["Und", "dei\u00b7ne", "weis\u00b7se", "faust", "nur", "lil\u00b7gen", "von", "sich", "streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ADV", "VVFIN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So geht es endlich an,", "tokens": ["So", "geht", "es", "end\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Da\u00df unter dieser blumen-decke", "tokens": ["Da\u00df", "un\u00b7ter", "die\u00b7ser", "blu\u00b7men\u00b7de\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Auch eine wahre schlange stecke.", "tokens": ["Auch", "ei\u00b7ne", "wah\u00b7re", "schlan\u00b7ge", "ste\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}