{"textgrid.poem.55332": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Antike", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Homer ist lange mit Ehren genannt,", "tokens": ["Ho\u00b7mer", "ist", "lan\u00b7ge", "mit", "Eh\u00b7ren", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Jetzt ward euch Phidias bekannt;", "tokens": ["Jetzt", "ward", "euch", "Phi\u00b7di\u00b7as", "be\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun h\u00e4lt nichts gegen beide Stich,", "tokens": ["Nun", "h\u00e4lt", "nichts", "ge\u00b7gen", "bei\u00b7de", "Stich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Darob ereifre niemand sich.", "tokens": ["Da\u00b7rob", "er\u00b7ei\u00b7fre", "nie\u00b7mand", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Seid willkommen, edle G\u00e4ste,", "tokens": ["Seid", "will\u00b7kom\u00b7men", ",", "ed\u00b7le", "G\u00e4s\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jedem echten deutschen Sinn;", "tokens": ["Je\u00b7dem", "ech\u00b7ten", "deut\u00b7schen", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn das Herrlichste, das Beste", "tokens": ["Denn", "das", "Herr\u00b7lichs\u00b7te", ",", "das", "Bes\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Bringt allein dem Geist Gewinn.", "tokens": ["Bringt", "al\u00b7lein", "dem", "Geist", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}