{"textgrid.poem.40547": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du Geist der Ungeduld, mein Foltergeist,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Geist der Ungeduld, mein Foltergeist,", "tokens": ["Du", "Geist", "der", "Un\u00b7ge\u00b7duld", ",", "mein", "Fol\u00b7ter\u00b7geist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der mich zur schleun'gen Flucht kopf\u00fcber rei\u00dft,", "tokens": ["Der", "mich", "zur", "schleun'\u00b7gen", "Flucht", "kopf\u00b7\u00fc\u00b7ber", "rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn auf die Wahlstatt des Salons zur Schlacht", "tokens": ["Wenn", "auf", "die", "Wahl\u00b7statt", "des", "Sa\u00b7lons", "zur", "Schlacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Gro\u00dfmacht Langeweil' ihr Heer gebracht,", "tokens": ["Die", "Gro\u00df\u00b7macht", "Lan\u00b7ge\u00b7weil'", "ihr", "Heer", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und mich des W\u00f6rterschwalles Katarakt", "tokens": ["Und", "mich", "des", "W\u00f6r\u00b7ter\u00b7schwal\u00b7les", "Ka\u00b7ta\u00b7rakt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie Wassersturz und Strudel wirbelnd packt,", "tokens": ["Wie", "Was\u00b7ser\u00b7sturz", "und", "Stru\u00b7del", "wir\u00b7belnd", "packt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mit mir zur Felsschlucht komm, unholder Gast,", "tokens": ["Mit", "mir", "zur", "Fels\u00b7schlucht", "komm", ",", "un\u00b7hol\u00b7der", "Gast", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sieh hin, dann hebe dich von mir in Hast!", "tokens": ["Sieh", "hin", ",", "dann", "he\u00b7be", "dich", "von", "mir", "in", "Hast", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "ADV", "VVFIN", "PRF", "APPR", "PPER", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Auch hier ein wasserreicher Katarakt,", "tokens": ["Auch", "hier", "ein", "was\u00b7ser\u00b7rei\u00b7cher", "Ka\u00b7ta\u00b7rakt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der, niedertosend, mich mit Schwindel packt", "tokens": ["Der", ",", "nie\u00b7der\u00b7to\u00b7send", ",", "mich", "mit", "Schwin\u00b7del", "packt"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "VVPP", "$,", "PRF", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und sinnbet\u00e4ubend braust und dr\u00f6hnt und zischt!", "tokens": ["Und", "sinn\u00b7be\u00b7t\u00e4u\u00b7bend", "braust", "und", "dr\u00f6hnt", "und", "zischt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Doch unterm Fluthgebraus schleicht unvermischt", "tokens": ["Doch", "un\u00b7term", "Fluth\u00b7ge\u00b7braus", "schleicht", "un\u00b7ver\u00b7mischt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Im eh'rnen Rohr ein Heilquell warm und mild,", "tokens": ["Im", "eh'r\u00b7nen", "Rohr", "ein", "Heil\u00b7quell", "warm", "und", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Uns sichtbar kaum, der Schmerz und Leiden stillt", "tokens": ["Uns", "sicht\u00b7bar", "kaum", ",", "der", "Schmerz", "und", "Lei\u00b7den", "stillt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "PRF", "ADV", "$,", "ART", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Der sieche Leiber fromm zu kr\u00e4ft'gen eilt", "tokens": ["Der", "sie\u00b7che", "Lei\u00b7ber", "fromm", "zu", "kr\u00e4ft'\u00b7gen", "eilt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und jetzt, ein Seelenarzt, mein Herz geheilt.", "tokens": ["Und", "jetzt", ",", "ein", "See\u00b7len\u00b7arzt", ",", "mein", "Herz", "ge\u00b7heilt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ART", "NN", "$,", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Ich ahn' es, traun, im Wortgesprudelstrom", "tokens": ["Ich", "ahn'", "es", ",", "traun", ",", "im", "Wort\u00b7ge\u00b7spru\u00b7del\u00b7strom"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "VVINF", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Flie\u00dft dort auch manch ein Heilborn einsam fromm,", "tokens": ["Flie\u00dft", "dort", "auch", "manch", "ein", "Heil\u00b7born", "ein\u00b7sam", "fromm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PIAT", "ART", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Manch Wort, das welke Herzen wieder j\u00fcngt,", "tokens": ["Manch", "Wort", ",", "das", "wel\u00b7ke", "Her\u00b7zen", "wie\u00b7der", "j\u00fcngt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Manch Wort, das m\u00fcde Seelen frei beschwingt,", "tokens": ["Manch", "Wort", ",", "das", "m\u00fc\u00b7de", "See\u00b7len", "frei", "be\u00b7schwingt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Manch Wort heilkr\u00e4ft'gen Geists, liebvoller Huld:", "tokens": ["Manch", "Wort", "heil\u00b7kr\u00e4ft'\u00b7gen", "Geists", ",", "lieb\u00b7vol\u00b7ler", "Huld", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "O lehre finden mich's, Geist der Geduld!", "tokens": ["O", "leh\u00b7re", "fin\u00b7den", "mich's", ",", "Geist", "der", "Ge\u00b7duld", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PIS", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}