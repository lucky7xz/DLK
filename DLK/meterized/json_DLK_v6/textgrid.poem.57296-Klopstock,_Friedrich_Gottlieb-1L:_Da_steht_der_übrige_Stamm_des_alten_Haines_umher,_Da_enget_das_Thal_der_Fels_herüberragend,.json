{"textgrid.poem.57296": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da steht der \u00fcbrige Stamm des alten Haines umher, Da enget das Thal der Fels her\u00fcberragend,", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da steht der \u00fcbrige Stamm des alten Haines umher, Da enget das Thal der Fels her\u00fcberragend,", "tokens": ["Da", "steht", "der", "\u00fcb\u00b7ri\u00b7ge", "Stamm", "des", "al\u00b7ten", "Hai\u00b7nes", "um\u00b7her", ",", "Da", "en\u00b7get", "das", "Thal", "der", "Fels", "her\u00b7\u00fc\u00b7berr\u00b7a\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+--++---+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Auf dem das einzige Maal der Urjahrhunderte Deutschlands", "tokens": ["Auf", "dem", "das", "ein\u00b7zi\u00b7ge", "Maal", "der", "Ur\u00b7jahr\u00b7hun\u00b7der\u00b7te", "Deutschlands"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "ART", "NN", "NE"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Der pfadverlierende Wanderer sieht.", "tokens": ["Der", "pfad\u00b7ver\u00b7lie\u00b7ren\u00b7de", "Wan\u00b7de\u00b7rer", "sieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Weidner fabelt ihm her: Ein Riesenross", "tokens": ["Der", "Weid\u00b7ner", "fa\u00b7belt", "ihm", "her", ":", "Ein", "Rie\u00b7sen\u00b7ross"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein hoher Ritter darauf, sprang \u00fcber das Thal", "tokens": ["Ein", "ho\u00b7her", "Rit\u00b7ter", "da\u00b7rauf", ",", "sprang", "\u00fc\u00b7ber", "das", "Thal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PAV", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der sch\u00f6nen fliehenden Riesin nach!", "tokens": ["Der", "sch\u00f6\u00b7nen", "flie\u00b7hen\u00b7den", "Rie\u00b7sin", "nach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Oben auf der Klippe liess den Fusstritt das Riesenross.", "tokens": ["O\u00b7ben", "auf", "der", "Klip\u00b7pe", "liess", "den", "Fus\u00b7stritt", "das", "Rie\u00b7sen\u00b7ross", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "Druiden haben und Barden, mit erobertem", "tokens": ["Dru\u00b7i\u00b7den", "ha\u00b7ben", "und", "Bar\u00b7den", ",", "mit", "er\u00b7o\u00b7ber\u00b7tem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "KON", "NN", "$,", "APPR", "ADJA"], "meter": "-+-+--+-+-+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Eisen, in den Felsen gehaun das einzige Maal", "tokens": ["Ei\u00b7sen", ",", "in", "den", "Fel\u00b7sen", "ge\u00b7haun", "das", "ein\u00b7zi\u00b7ge", "Maal"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Der Urjahrhunderte Deutschlands,", "tokens": ["Der", "Ur\u00b7jahr\u00b7hun\u00b7der\u00b7te", "Deutschlands", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Den Huf des heiligen weissen Rosses,", "tokens": ["Den", "Huf", "des", "hei\u00b7li\u00b7gen", "weis\u00b7sen", "Ros\u00b7ses", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Mit dem Flammenblick, mit der dichten", "tokens": ["Mit", "dem", "Flam\u00b7men\u00b7blick", ",", "mit", "der", "dich\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Niederstr\u00f6menden M\u00e4hne, dem Sturme selbst", "tokens": ["Nie\u00b7der\u00b7str\u00f6\u00b7men\u00b7den", "M\u00e4h\u00b7ne", ",", "dem", "Stur\u00b7me", "selbst"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "ADV"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Zu heben schwer, mit der schmetternden, (es stampfte dann,", "tokens": ["Zu", "he\u00b7ben", "schwer", ",", "mit", "der", "schmet\u00b7tern\u00b7den", ",", "(", "es", "stampf\u00b7te", "dann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "$,", "APPR", "ART", "ADJA", "$,", "$(", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dass die Erde scholl) mit der zukunftwiehernden Stimme.", "tokens": ["Dass", "die", "Er\u00b7de", "scholl", ")", "mit", "der", "zu\u00b7kunft\u00b7wie\u00b7hern\u00b7den", "Stim\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "$(", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+---+-+-+-", "measure": "unknown.measure.hexa"}}, "stanza.5": {"line.1": {"text": "Der begeisterte Barde trat in den Umkreis", "tokens": ["Der", "be\u00b7geis\u00b7ter\u00b7te", "Bar\u00b7de", "trat", "in", "den", "Um\u00b7kreis"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Des nachgebildeten Hufes, und so durch die Weihe", "tokens": ["Des", "nach\u00b7ge\u00b7bil\u00b7de\u00b7ten", "Hu\u00b7fes", ",", "und", "so", "durch", "die", "Wei\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der G\u00f6tter geweiht, weissagt' er, aus des st\u00fcrzenden Bachs", "tokens": ["Der", "G\u00f6t\u00b7ter", "ge\u00b7weiht", ",", "weis\u00b7sagt'", "er", ",", "aus", "des", "st\u00fcr\u00b7zen\u00b7den", "Bachs"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "VVFIN", "PPER", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Mannichfalten Welle, die Wechsel der feinen Tage;", "tokens": ["Man\u00b7nich\u00b7fal\u00b7ten", "Wel\u00b7le", ",", "die", "Wech\u00b7sel", "der", "fei\u00b7nen", "Ta\u00b7ge", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}}, "stanza.6": {"line.1": {"text": "Oft blutige: Dass in Winfeld Hermann sich einst", "tokens": ["Oft", "blu\u00b7ti\u00b7ge", ":", "Dass", "in", "Win\u00b7feld", "Her\u00b7mann", "sich", "einst"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "$.", "KOUS", "APPR", "NN", "NE", "PRF", "ADV"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein Maal erbaut' aus Legionengebein!", "tokens": ["Ein", "Maal", "er\u00b7baut'", "aus", "Le\u00b7gi\u00b7o\u00b7nen\u00b7ge\u00b7bein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Dass Bojokal, der zu treue Deutsche,", "tokens": ["Dass", "Bo\u00b7jo\u00b7kal", ",", "der", "zu", "treu\u00b7e", "Deut\u00b7sche", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "(er weigert' es Hermann, am Maal mitzubaun;", "tokens": ["(", "er", "wei\u00b7gert'", "es", "Her\u00b7mann", ",", "am", "Maal", "mit\u00b7zu\u00b7baun", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "NE", "$,", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Und der verbot ihm gr\u00f6ssere Treu durch die Fessel)", "tokens": ["Und", "der", "ver\u00b7bot", "ihm", "gr\u00f6s\u00b7se\u00b7re", "Treu", "durch", "die", "Fes\u00b7sel", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PPER", "ADJA", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bojokal einst, belohnt von den Welttyrannen, weinete:", "tokens": ["Bo\u00b7jo\u00b7kal", "einst", ",", "be\u00b7lohnt", "von", "den", "Welt\u00b7ty\u00b7ran\u00b7nen", ",", "wei\u00b7ne\u00b7te", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ADV", "$,", "VVPP", "APPR", "ART", "NN", "$,", "VVFIN", "$."], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "O Wodan! Und Mana! und all' ihr G\u00f6tter! fehlt zu der H\u00fctt' uns", "tokens": ["O", "Wo\u00b7dan", "!", "Und", "Ma\u00b7na", "!", "und", "all'", "ihr", "G\u00f6t\u00b7ter", "!", "fehlt", "zu", "der", "H\u00fctt'", "uns"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$.", "KON", "NE", "$.", "KON", "PIS", "PPOSAT", "NN", "$.", "VVFIN", "APPR", "ART", "NN", "PPER"], "meter": "+-+-+--+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Erde; so soll doch Erde zum Grab' uns nicht fehlen!", "tokens": ["Er\u00b7de", ";", "so", "soll", "doch", "Er\u00b7de", "zum", "Grab'", "uns", "nicht", "feh\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VMFIN", "ADV", "NN", "APPRART", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Ein Barde weissagt's. O Zukunftwisser!", "tokens": ["Ein", "Bar\u00b7de", "weis\u00b7sagt'", "s.", "O", "Zu\u00b7kunft\u00b7wis\u00b7ser", "!"], "token_info": ["word", "word", "word", "abbreviation", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "NE", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bach in dem Hain,", "tokens": ["Bach", "in", "dem", "Hain", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Dess \u00fcbriger Stamm", "tokens": ["Dess", "\u00fcb\u00b7ri\u00b7ger", "Stamm"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Dem weihenden Hufe sch\u00fcttet,", "tokens": ["Dem", "wei\u00b7hen\u00b7den", "Hu\u00b7fe", "sch\u00fct\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "An dir, o du der sch\u00f6nen \u00d6de Bach,", "tokens": ["An", "dir", ",", "o", "du", "der", "sch\u00f6\u00b7nen", "\u00d6\u00b7de", "Bach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "PPER", "ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ging oft mein Cramer, wo du", "tokens": ["Ging", "oft", "mein", "Cra\u00b7mer", ",", "wo", "du"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$,", "PWAV", "PPER"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Entflohen ihr warest, nicht mehr bergunter rauschetest,", "tokens": ["Ent\u00b7flo\u00b7hen", "ihr", "wa\u00b7rest", ",", "nicht", "mehr", "ber\u00b7gun\u00b7ter", "rau\u00b7sche\u00b7test", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VAFIN", "$,", "PTKNEG", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ging mein Giseke, ging mein Resewiz dem Haine zu.", "tokens": ["Ging", "mein", "Gi\u00b7se\u00b7ke", ",", "ging", "mein", "Re\u00b7se\u00b7wiz", "dem", "Hai\u00b7ne", "zu", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.10": {"line.1": {"text": "Geboren wurde nicht fern von dir mein Gleim;", "tokens": ["Ge\u00b7bo\u00b7ren", "wur\u00b7de", "nicht", "fern", "von", "dir", "mein", "Gleim", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PTKNEG", "ADJD", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich ward an dir geboren. Die Tage nach mir", "tokens": ["Ich", "ward", "an", "dir", "ge\u00b7bo\u00b7ren", ".", "Die", "Ta\u00b7ge", "nach", "mir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "$.", "ART", "NN", "APPR", "PPER"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sollen entscheiden, ob aus dir, o m\u00fctterlicher Bach,", "tokens": ["Sol\u00b7len", "ent\u00b7schei\u00b7den", ",", "ob", "aus", "dir", ",", "o", "m\u00fct\u00b7ter\u00b7li\u00b7cher", "Bach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "KOUS", "APPR", "PPER", "$,", "FM", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.4": {"text": "Auch ich geweissagt habe.", "tokens": ["Auch", "ich", "ge\u00b7weis\u00b7sagt", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Was s\u00e4umst du? fang an, ich sehe den Schaum,", "tokens": ["Was", "s\u00e4umst", "du", "?", "fang", "an", ",", "ich", "se\u00b7he", "den", "Schaum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bardiet, fang an, des st\u00fcrzenden Bachs!", "tokens": ["Bar\u00b7diet", ",", "fang", "an", ",", "des", "st\u00fcr\u00b7zen\u00b7den", "Bachs", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PTKVZ", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Vernehme, wie in der Felskluft!", "tokens": ["Ver\u00b7neh\u00b7me", ",", "wie", "in", "der", "Fels\u00b7kluft", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das Rauschen der redenden Wog' ert\u00f6nt!", "tokens": ["Das", "Rau\u00b7schen", "der", "re\u00b7den\u00b7den", "Wo\u00b7g'", "er\u00b7t\u00f6nt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.12": {"line.1": {"text": "Sein Name lebt, welche Thaten er auch gethan hat,", "tokens": ["Sein", "Na\u00b7me", "lebt", ",", "wel\u00b7che", "Tha\u00b7ten", "er", "auch", "ge\u00b7than", "hat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PWAT", "NN