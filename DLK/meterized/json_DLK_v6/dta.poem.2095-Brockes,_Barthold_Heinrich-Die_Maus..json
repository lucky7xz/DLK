{"dta.poem.2095": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Maus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dieses ist ein kleines, zierlichs, aber ein sehr sch\u00e4d-", "tokens": ["Die\u00b7ses", "ist", "ein", "klei\u00b7nes", ",", "zier\u00b7lichs", ",", "a\u00b7ber", "ein", "sehr", "sch\u00e4d"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "$,", "VVFIN", "$,", "ADV", "ART", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "lichs Thier", "tokens": ["lichs", "Thier"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "So im Haus\u2019 als auf dem Felde. Es verursacht dort", "tokens": ["So", "im", "Haus'", "als", "auf", "dem", "Fel\u00b7de", ".", "Es", "ver\u00b7ur\u00b7sacht", "dort"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "KOKOM", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "und hier", "tokens": ["und", "hier"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Oefters ungemeinen Schaden, so da\u00df sie, mehr uns zu", "tokens": ["Oef\u00b7ters", "un\u00b7ge\u00b7mei\u00b7nen", "Scha\u00b7den", ",", "so", "da\u00df", "sie", ",", "mehr", "uns", "zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "ADV", "KOUS", "PPER", "$,", "ADV", "PPER", "PTKZU"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "strafen,", "tokens": ["stra\u00b7fen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Als uns Nutzen zu verschaffen,", "tokens": ["Als", "uns", "Nut\u00b7zen", "zu", "ver\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mehrentheils erzeuget scheinen; sonderlich, wenn sie sich", "tokens": ["Meh\u00b7ren\u00b7theils", "er\u00b7zeu\u00b7get", "schei\u00b7nen", ";", "son\u00b7der\u00b7lich", ",", "wenn", "sie", "sich"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "VVFIN", "$.", "ADJD", "$,", "KOUS", "PPER", "PRF"], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "mehren,", "tokens": ["meh\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Wie sie denn besonders fruchtbar, da sie leicht die Saat", "tokens": ["Wie", "sie", "denn", "be\u00b7son\u00b7ders", "frucht\u00b7bar", ",", "da", "sie", "leicht", "die", "Saat"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ADJD", "$,", "KOUS", "PPER", "ADJD", "ART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.11": {"text": "verheeren", "tokens": ["ver\u00b7hee\u00b7ren"], "token_info": ["word"], "pos": ["VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Und des Landmanns ganze Hoffnung oft betr\u00fcbt genug", "tokens": ["Und", "des", "Land\u00b7manns", "gan\u00b7ze", "Hoff\u00b7nung", "oft", "be\u00b7tr\u00fcbt", "ge\u00b7nug"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "ADV", "ADJD", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.13": {"text": "verzehren.", "tokens": ["ver\u00b7zeh\u00b7ren", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Doch wird eine weise Vorsicht auch hiebey mit Recht er-", "tokens": ["Doch", "wird", "ei\u00b7ne", "wei\u00b7se", "Vor\u00b7sicht", "auch", "hie\u00b7bey", "mit", "Recht", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "ADV", "ADV", "APPR", "NN", "TRUNC"], "meter": "--+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.15": {"text": "kannt,", "tokens": ["kannt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.16": {"text": "Da sie, welches wir nicht fassen, von sich selber aufzu-", "tokens": ["Da", "sie", ",", "wel\u00b7ches", "wir", "nicht", "fas\u00b7sen", ",", "von", "sich", "sel\u00b7ber", "auf\u00b7zu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "$,", "APPR", "PRF", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.17": {"text": "h\u00f6ren", "tokens": ["h\u00f6\u00b7ren"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Und als zu verschwinden scheinen, da gewi\u00df ein ganzes", "tokens": ["Und", "als", "zu", "ver\u00b7schwin\u00b7den", "schei\u00b7nen", ",", "da", "ge\u00b7wi\u00df", "ein", "gan\u00b7zes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PTKZU", "VVINF", "VVFIN", "$,", "KOUS", "ADV", "ART", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.19": {"text": "Land", "tokens": ["Land"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.20": {"text": "Sonst, wenn dieses nicht gesch\u00e4he, nur durch M\u00e4use", "tokens": ["Sonst", ",", "wenn", "die\u00b7ses", "nicht", "ge\u00b7sch\u00e4\u00b7he", ",", "nur", "durch", "M\u00e4u\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PDS", "PTKNEG", "VVFIN", "$,", "ADV", "APPR", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.21": {"text": "blo\u00df, zur W\u00fcste", "tokens": ["blo\u00df", ",", "zur", "W\u00fcs\u00b7te"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$,", "APPRART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.22": {"text": "Werden m\u00fc\u00dfte.", "tokens": ["Wer\u00b7den", "m\u00fc\u00df\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VMFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.23": {"text": "Denn es w\u00e4re zu erweisen, da\u00df, von einer Maus", "tokens": ["Denn", "es", "w\u00e4\u00b7re", "zu", "er\u00b7wei\u00b7sen", ",", "da\u00df", ",", "von", "ei\u00b7ner", "Maus"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PTKZU", "VVINF", "$,", "KOUS", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.24": {"text": "allein,", "tokens": ["al\u00b7lein", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.25": {"text": "Auf die Art, wie sie sich mehren, \u00fcber hundert und noch", "tokens": ["Auf", "die", "Art", ",", "wie", "sie", "sich", "meh\u00b7ren", ",", "\u00fc\u00b7ber", "hun\u00b7dert", "und", "noch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "PPER", "PRF", "VVINF", "$,", "APPR", "CARD", "KON", "ADV"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.26": {"text": "mehr", "tokens": ["mehr"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}, "line.27": {"text": "Blo\u00df in einer Jahreszeit w\u00fcrde ausgehecket seyn.", "tokens": ["Blo\u00df", "in", "ei\u00b7ner", "Jah\u00b7res\u00b7zeit", "w\u00fcr\u00b7de", "aus\u00b7ge\u00b7he\u00b7cket", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN", "VVPP", "VAINF", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.28": {"text": "Welche schreckliche Vermehrung! Welch ein ungez\u00e4hltes", "tokens": ["Wel\u00b7che", "schreck\u00b7li\u00b7che", "Ver\u00b7meh\u00b7rung", "!", "Welch", "ein", "un\u00b7ge\u00b7z\u00e4hl\u00b7tes"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAT", "ADJA", "NN", "$.", "PIAT", "ART", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.29": {"text": "Heer,", "tokens": ["Heer", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}}}}}