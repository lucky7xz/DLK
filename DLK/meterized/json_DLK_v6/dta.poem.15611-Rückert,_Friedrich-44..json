{"dta.poem.15611": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "44.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Auseinander hier im Raum, dort in der Zeit", "tokens": ["Das", "Aus\u00b7ein\u00b7an\u00b7der", "hier", "im", "Raum", ",", "dort", "in", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Nacheinander, ist zwiesp\u00e4lt'ge Ewigkeit.", "tokens": ["Das", "Na\u00b7ch\u00b7ein\u00b7an\u00b7der", ",", "ist", "zwie\u00b7sp\u00e4lt'\u00b7ge", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "ADJA", "NN", "$."], "meter": "--+-+-+++-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Die Zwiespalt, ob in dir, ob in der Welt sie sei,", "tokens": ["Die", "Zwies\u00b7palt", ",", "ob", "in", "dir", ",", "ob", "in", "der", "Welt", "sie", "sei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "APPR", "PPER", "$,", "KOUS", "APPR", "ART", "NN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Genug, dein Anschaun wird nie von der Zwiespalt frei.", "tokens": ["Ge\u00b7nug", ",", "dein", "An\u00b7schaun", "wird", "nie", "von", "der", "Zwies\u00b7palt", "frei", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Das Werden in der Zeit, das Daseyn in dem Raum,", "tokens": ["Das", "Wer\u00b7den", "in", "der", "Zeit", ",", "das", "Da\u00b7seyn", "in", "dem", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ART", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Hebt kein Bewu\u00dftseyn auf, nur unbewu\u00dfter Traum.", "tokens": ["Hebt", "kein", "Be\u00b7wu\u00df\u00b7tseyn", "auf", ",", "nur", "un\u00b7be\u00b7wu\u00df\u00b7ter", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKVZ", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Es aufzuheben mit Bewu\u00dftseyn, diese Kraft", "tokens": ["Es", "auf\u00b7zu\u00b7he\u00b7ben", "mit", "Be\u00b7wu\u00df\u00b7tseyn", ",", "die\u00b7se", "Kraft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Legt durch ihr Denken nur sich bei die Wissenschaft;", "tokens": ["Legt", "durch", "ihr", "Den\u00b7ken", "nur", "sich", "bei", "die", "Wis\u00b7sen\u00b7schaft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ADV", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die das als Werdendes, Gewordenes Getrennte", "tokens": ["Die", "das", "als", "Wer\u00b7den\u00b7des", ",", "Ge\u00b7wor\u00b7de\u00b7nes", "Ge\u00b7trenn\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PDS", "KOUS", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu einer Ganzheit macht geordneter Momente;", "tokens": ["Zu", "ei\u00b7ner", "Ganz\u00b7heit", "macht", "ge\u00b7ord\u00b7ne\u00b7ter", "Mo\u00b7men\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Zur todten Ganzheit doch, dem Kunstsaal zu vergleichen,", "tokens": ["Zur", "tod\u00b7ten", "Ganz\u00b7heit", "doch", ",", "dem", "Kunst\u00b7saal", "zu", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo Bilder lebende geworden sind zu Leichen,", "tokens": ["Wo", "Bil\u00b7der", "le\u00b7ben\u00b7de", "ge\u00b7wor\u00b7den", "sind", "zu", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJA", "VAPP", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}}, "stanza.7": {"line.1": {"text": "Weil ihre Schranke fehlt, worin sie Leben hatten;", "tokens": ["Weil", "ih\u00b7re", "Schran\u00b7ke", "fehlt", ",", "wo\u00b7rin", "sie", "Le\u00b7ben", "hat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$,", "PWAV", "PPER", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wird, aus Zeit und Raum ger\u00fcckt, die Welt zum Schatten.", "tokens": ["So", "wird", ",", "aus", "Zeit", "und", "Raum", "ge\u00b7r\u00fcckt", ",", "die", "Welt", "zum", "Schat\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "APPR", "NN", "KON", "NN", "VVPP", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Drum, willst du dich erfreun der Mannichfaltigkeit", "tokens": ["Drum", ",", "willst", "du", "dich", "er\u00b7freun", "der", "Man\u00b7nich\u00b7fal\u00b7tig\u00b7keit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "$,", "VMFIN", "PPER", "PRF", "VVINF", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des bunten Lebens, la\u00df ihm die Zwiespaltigkeit;", "tokens": ["Des", "bun\u00b7ten", "Le\u00b7bens", ",", "la\u00df", "ihm", "die", "Zwies\u00b7pal\u00b7tig\u00b7keit", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVIMP", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Und nimm mit Dank von Gott die Augenblicke hin,", "tokens": ["Und", "nimm", "mit", "Dank", "von", "Gott", "die", "Au\u00b7gen\u00b7bli\u00b7cke", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "NN", "APPR", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo selbst in Raum und Zeit ahnt Ewiges dein Sinn.", "tokens": ["Wo", "selbst", "in", "Raum", "und", "Zeit", "ahnt", "E\u00b7wi\u00b7ges", "dein", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}