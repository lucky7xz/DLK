{"dta.poem.19973": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "An die K\u00f6nigin Katharina mit meiner  \n Beschreibung des Wildbads .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In altem Tannenhaine", "tokens": ["In", "al\u00b7tem", "Tan\u00b7nen\u00b7hai\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Tief aus krystall'nem Grund,", "tokens": ["Tief", "aus", "kry\u00b7stall'\u00b7nem", "Grund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Giebt ", "tokens": ["Giebt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Sich uns durch Wohlthun kund.", "tokens": ["Sich", "uns", "durch", "Wohl\u00b7thun", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PRF", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es gie\u00dft die Himmelsklare", "tokens": ["Es", "gie\u00dft", "die", "Him\u00b7mels\u00b7kla\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus ihrem Felsenhaus,", "tokens": ["Aus", "ih\u00b7rem", "Fel\u00b7sen\u00b7haus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schon viele hundert Jahre", "tokens": ["Schon", "vie\u00b7le", "hun\u00b7dert", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PIAT", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur Lieb' und Segen aus.", "tokens": ["Nur", "Lieb'", "und", "Se\u00b7gen", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nie ist ihr Auge tr\u00fcbe,", "tokens": ["Nie", "ist", "ihr", "Au\u00b7ge", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nie ist ihr Herze kalt,", "tokens": ["Nie", "ist", "ihr", "Her\u00b7ze", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Stets bleibt sie jung an Liebe,", "tokens": ["Stets", "bleibt", "sie", "jung", "an", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Stets jung auch an Gestalt.", "tokens": ["Stets", "jung", "auch", "an", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Nymphe ist's \u2014 die helle,", "tokens": ["Die", "Nym\u00b7phe", "ist's", "die", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$(", "ART", "ADJA", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die sonnenwarme Fluth,", "tokens": ["Die", "son\u00b7nen\u00b7war\u00b7me", "Fluth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des ", "tokens": ["Des"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Die tausend Wunder thut.", "tokens": ["Die", "tau\u00b7send", "Wun\u00b7der", "thut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ja! Tausend k\u00f6nnen nennen", "tokens": ["Ja", "!", "Tau\u00b7send", "k\u00f6n\u00b7nen", "nen\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$.", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Heil'gen Lieb' und Treu,", "tokens": ["Der", "Heil'\u00b7gen", "Lieb'", "und", "Treu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und m\u00fcssen all bekennen,", "tokens": ["Und", "m\u00fcs\u00b7sen", "all", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df sie verwandt ", "tokens": ["Da\u00df", "sie", "ver\u00b7wandt"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Da\u00df sie, wie ", "tokens": ["Da\u00df", "sie", ",", "wie"], "token_info": ["word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "$,", "PWAV"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Tr\u00e4gt mit der Menschen Schmerz,", "tokens": ["Tr\u00e4gt", "mit", "der", "Men\u00b7schen", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df sie, wie ", "tokens": ["Da\u00df", "sie", ",", "wie"], "token_info": ["word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "$,", "PWAV"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Macht manch' erstarrtes Herz.", "tokens": ["Macht", "man\u00b7ch'", "er\u00b7starr\u00b7tes", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und weil ", "tokens": ["Und", "weil"], "token_info": ["word", "word"], "pos": ["KON", "KOUS"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "An Wohlthun ganz ihr gleich,", "tokens": ["An", "Wohl\u00b7thun", "ganz", "ihr", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur Leben und nur Bl\u00fcthe", "tokens": ["Nur", "Le\u00b7ben", "und", "nur", "Bl\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ausgie\u00dfen m\u00f6cht'st im Reich,", "tokens": ["Aus\u00b7gie\u00b7\u00dfen", "m\u00f6cht'st", "im", "Reich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "L\u00e4\u00dft ", "tokens": ["L\u00e4\u00dft"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Die Heil'ge liebewarm,", "tokens": ["Die", "Heil'\u00b7ge", "lie\u00b7be\u00b7warm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sehnt sich, ", "tokens": ["Und", "sehnt", "sich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Als Schwester in den Arm.", "tokens": ["Als", "Schwes\u00b7ter", "in", "den", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}