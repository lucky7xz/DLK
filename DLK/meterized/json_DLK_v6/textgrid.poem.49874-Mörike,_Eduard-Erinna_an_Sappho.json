{"textgrid.poem.49874": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Erinna an Sappho", "genre": "verse", "period": "N.A.", "pub_year": 1863, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbvielfach sind zum Hades die Pfade\u00ab, hei\u00dft ein", "tokens": ["\u00bb", "viel\u00b7fach", "sind", "zum", "Ha\u00b7des", "die", "Pfa\u00b7de", "\u00ab", ",", "hei\u00dft", "ein"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["$(", "ADV", "VAFIN", "APPRART", "NN", "ART", "NN", "$(", "$,", "VVFIN", "ART"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Altes Liedchen \u2013 \u00bbund einen gehst du selber,", "tokens": ["Al\u00b7tes", "Lied\u00b7chen", "\u2013", "\u00bb", "und", "ei\u00b7nen", "gehst", "du", "sel\u00b7ber", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "$(", "KON", "PIS", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Zweifle nicht!\u00ab Wer, s\u00fc\u00dfeste Sappho, zweifelt?", "tokens": ["Zweif\u00b7le", "nicht", "!", "\u00ab", "Wer", ",", "s\u00fc\u00b7\u00dfes\u00b7te", "Sap\u00b7pho", ",", "zwei\u00b7felt", "?"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PTKNEG", "$.", "$(", "PWS", "$,", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Sagt es nicht jeglicher Tag?", "tokens": ["Sagt", "es", "nicht", "jeg\u00b7li\u00b7cher", "Tag", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "PIAT", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Doch den Lebenden haftet nur leicht im Busen", "tokens": ["Doch", "den", "Le\u00b7ben\u00b7den", "haf\u00b7tet", "nur", "leicht", "im", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ADJD", "APPRART", "NN"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Solch ein Wort, und dem Meer anwohnend ein Fischer von Kind auf", "tokens": ["Solch", "ein", "Wort", ",", "und", "dem", "Meer", "an\u00b7woh\u00b7nend", "ein", "Fi\u00b7scher", "von", "Kind", "auf"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "ART", "NN", "$,", "KON", "ART", "NN", "ADJD", "ART", "NN", "APPR", "NN", "APPR"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.3": {"text": "H\u00f6rt im stumpferen Ohr der Wogen Ger\u00e4usch nicht mehr.", "tokens": ["H\u00f6rt", "im", "stump\u00b7fe\u00b7ren", "Ohr", "der", "Wo\u00b7gen", "Ge\u00b7r\u00e4usch", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "NN", "PTKNEG", "ADV", "$."], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "\u2013 Wundersam aber erschrak mir heute das Herz. Vernimm!", "tokens": ["\u2013", "Wun\u00b7der\u00b7sam", "a\u00b7ber", "er\u00b7schrak", "mir", "heu\u00b7te", "das", "Herz", ".", "Ver\u00b7nimm", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "NN", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$.", "NN", "$."], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Sonniger Morgenglanz im Garten,", "tokens": ["Son\u00b7ni\u00b7ger", "Mor\u00b7gen\u00b7glanz", "im", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ergossen um der B\u00e4ume Wipfel,", "tokens": ["Er\u00b7gos\u00b7sen", "um", "der", "B\u00e4u\u00b7me", "Wip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Lockte die Langschl\u00e4ferin (denn so schaltest du j\u00fcngst Erinna!)", "tokens": ["Lock\u00b7te", "die", "Lang\u00b7schl\u00e4\u00b7fe\u00b7rin", "(", "denn", "so", "schal\u00b7test", "du", "j\u00fcngst", "E\u00b7rin\u00b7na", "!", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "KON", "ADV", "VVFIN", "PPER", "ADV", "NE", "$.", "$("], "meter": "+--+--+--+--+--+", "measure": "dactylic.tetra.plus"}, "line.4": {"text": "Fr\u00fch vom schw\u00fcligen Lager hinweg.", "tokens": ["Fr\u00fch", "vom", "schw\u00fc\u00b7li\u00b7gen", "La\u00b7ger", "hin\u00b7weg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Stille war mein Gem\u00fct; in den Adern aber", "tokens": ["Stil\u00b7le", "war", "mein", "Ge\u00b7m\u00fct", ";", "in", "den", "A\u00b7dern", "a\u00b7ber"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$.", "APPR", "ART", "NN", "ADV"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Unstet klopfte das Blut bei der Wangen Bl\u00e4sse.", "tokens": ["Un\u00b7stet", "klopf\u00b7te", "das", "Blut", "bei", "der", "Wan\u00b7gen", "Bl\u00e4s\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Als ich am Putztisch jetzo die Flechten l\u00f6ste,", "tokens": ["Als", "ich", "am", "Putz\u00b7tisch", "jet\u00b7zo", "die", "Flech\u00b7ten", "l\u00f6s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dann mit nardeduftendem Kamm vor der Stirn den Haar\u2013", "tokens": ["Dann", "mit", "nar\u00b7de\u00b7duf\u00b7ten\u00b7dem", "Kamm", "vor", "der", "Stirn", "den", "Haar", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "APPR", "ART", "NN", "ART", "NN", "$("], "meter": "+-+-+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Schleier teilte \u2013 seltsam betraf mich im Spiegel Blick in Blick.", "tokens": ["Schlei\u00b7er", "teil\u00b7te", "\u2013", "selt\u00b7sam", "be\u00b7traf", "mich", "im", "Spie\u00b7gel", "Blick", "in", "Blick", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "$(", "ADJD", "VVFIN", "PPER", "APPRART", "NN", "NN", "APPR", "NN", "$."], "meter": "+-+-+---+-+-+-+", "measure": "unknown.measure.septa"}, "line.4": {"text": "Augen, sagt ich, ihr Augen, was wollt ihr?", "tokens": ["Au\u00b7gen", ",", "sagt", "ich", ",", "ihr", "Au\u00b7gen", ",", "was", "wollt", "ihr", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PWS", "VMFIN", "PPER", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Du, mein Geist, heute noch sicher behaust da drinne,", "tokens": ["Du", ",", "mein", "Geist", ",", "heu\u00b7te", "noch", "si\u00b7cher", "be\u00b7haust", "da", "drin\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "ADV", "ADJD", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "+-+---+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Lebendigen Sinnen traulich verm\u00e4hlt,", "tokens": ["Le\u00b7ben\u00b7di\u00b7gen", "Sin\u00b7nen", "trau\u00b7lich", "ver\u00b7m\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Wie mit fremdendem Ernst, l\u00e4chelnd halb, ein D\u00e4mon,", "tokens": ["Wie", "mit", "frem\u00b7den\u00b7dem", "Ernst", ",", "l\u00e4\u00b7chelnd", "halb", ",", "ein", "D\u00e4\u00b7mon", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$,", "ADJD", "ADJD", "$,", "ART", "NN", "$,"], "meter": "+-+--++-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Nickst du mich an, Tod weissagend!", "tokens": ["Nickst", "du", "mich", "an", ",", "Tod", "weis\u00b7sa\u00b7gend", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKVZ", "$,", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "\u2013 \u2013 Ha, da mit eins durchzuckt' es mich", "tokens": ["\u2013", "\u2013", "Ha", ",", "da", "mit", "eins", "durch\u00b7zuckt'", "es", "mich"], "token_info": ["punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "$(", "ITJ", "$,", "KOUS", "APPR", "PIS", "VVFIN", "PPER", "PRF"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.10": {"text": "Wie Wetterschein! wie wenn schwarzgefiedert ein t\u00f6dlicher Pfeil", "tokens": ["Wie", "Wet\u00b7ter\u00b7schein", "!", "wie", "wenn", "schwarz\u00b7ge\u00b7fie\u00b7dert", "ein", "t\u00f6d\u00b7li\u00b7cher", "Pfeil"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "$.", "KOKOM", "KOUS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Streifte die Schl\u00e4fe hart vorbei,", "tokens": ["Streif\u00b7te", "die", "Schl\u00e4\u00b7fe", "hart", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Da\u00df ich, die H\u00e4nde gedeckt aufs Antlitz, lange", "tokens": ["Da\u00df", "ich", ",", "die", "H\u00e4n\u00b7de", "ge\u00b7deckt", "aufs", "Ant\u00b7litz", ",", "lan\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "$,", "ART", "NN", "VVPP", "APPRART", "NN", "$,", "ADV"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Staunend blieb, in die nachtschaurige Kluft schwindelnd hinab.", "tokens": ["Stau\u00b7nend", "blieb", ",", "in", "die", "nacht\u00b7schau\u00b7ri\u00b7ge", "Kluft", "schwin\u00b7delnd", "hin\u00b7ab", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.5": {"line.1": {"text": "Und das eigene Todesgeschick erwog ich;", "tokens": ["Und", "das", "ei\u00b7ge\u00b7ne", "To\u00b7des\u00b7ge\u00b7schick", "er\u00b7wog", "ich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Trockenen Augs noch erst,", "tokens": ["Tro\u00b7cke\u00b7nen", "Augs", "noch", "erst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Bis da ich dein, o Sappho, dachte,", "tokens": ["Bis", "da", "ich", "dein", ",", "o", "Sap\u00b7pho", ",", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PPOSAT", "$,", "FM", "NE", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und der Freundinnen all,", "tokens": ["Und", "der", "Freun\u00b7din\u00b7nen", "all", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PIAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und anmutiger Musenkunst,", "tokens": ["Und", "an\u00b7mu\u00b7ti\u00b7ger", "Mu\u00b7sen\u00b7kunst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Gleich da quollen die Tr\u00e4nen mir.", "tokens": ["Gleich", "da", "quol\u00b7len", "die", "Tr\u00e4\u00b7nen", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Und dort blinkte vom Tisch das sch\u00f6ne Kopfnetz, dein Geschenk,", "tokens": ["Und", "dort", "blink\u00b7te", "vom", "Tisch", "das", "sch\u00f6\u00b7ne", "Kopf\u00b7netz", ",", "dein", "Ge\u00b7schenk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-++--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "K\u00f6stliches Byssosgeweb, von goldnen Bienlein schw\u00e4rmend.", "tokens": ["K\u00f6st\u00b7li\u00b7ches", "Bys\u00b7sos\u00b7ge\u00b7web", ",", "von", "gold\u00b7nen", "Bien\u00b7lein", "schw\u00e4r\u00b7mend", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.3": {"text": "Dieses, wenn wir demn\u00e4chst das blumige Fest", "tokens": ["Die\u00b7ses", ",", "wenn", "wir", "dem\u00b7n\u00e4chst", "das", "blu\u00b7mi\u00b7ge", "Fest"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "$,", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Feiern der herrlichen Tochter Demeters,", "tokens": ["Fei\u00b7ern", "der", "herr\u00b7li\u00b7chen", "Toch\u00b7ter", "De\u00b7me\u00b7ters", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "NE", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "M\u00f6cht ich ", "tokens": ["M\u00f6cht", "ich"], "token_info": ["word", "word"], "pos": ["VMFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Da\u00df sie hold uns bleibe (denn viel vermag sie),", "tokens": ["Da\u00df", "sie", "hold", "uns", "blei\u00b7be", "(", "denn", "viel", "ver\u00b7mag", "sie", ")", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "VVFIN", "$(", "KON", "ADV", "VVFIN", "PPER", "$(", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Da\u00df du zu fr\u00fch dir nicht die braune Locke m\u00f6gest", "tokens": ["Da\u00df", "du", "zu", "fr\u00fch", "dir", "nicht", "die", "brau\u00b7ne", "Lo\u00b7cke", "m\u00f6\u00b7gest"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKA", "ADJD", "PPER", "PTKNEG", "ART", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "F\u00fcr Erinna vom lieben Haupte trennen.", "tokens": ["F\u00fcr", "E\u00b7rin\u00b7na", "vom", "lie\u00b7ben", "Haup\u00b7te", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}