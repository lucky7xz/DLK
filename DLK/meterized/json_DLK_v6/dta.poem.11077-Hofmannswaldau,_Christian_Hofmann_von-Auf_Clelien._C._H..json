{"dta.poem.11077": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf Clelien.  \n  C. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der sch\u00f6nen Clelie stehn alle s\u00fcnden frey:", "tokens": ["Der", "sch\u00f6\u00b7nen", "Cle\u00b7lie", "stehn", "al\u00b7le", "s\u00fcn\u00b7den", "frey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das stehlen, denn sie hat mein hertze mir genommen;", "tokens": ["Das", "steh\u00b7len", ",", "denn", "sie", "hat", "mein", "hert\u00b7ze", "mir", "ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "$,", "KON", "PPER", "VAFIN", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das l\u00fcgen, denn sie spricht: da\u00df dem nicht also sey;", "tokens": ["Das", "l\u00fc\u00b7gen", ",", "denn", "sie", "spricht", ":", "da\u00df", "dem", "nicht", "al\u00b7so", "sey", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$.", "KOUS", "ART", "PTKNEG", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das morden, ich bin fast durch sie ums leben kommen;", "tokens": ["Das", "mor\u00b7den", ",", "ich", "bin", "fast", "durch", "sie", "ums", "le\u00b7ben", "kom\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PPER", "VAFIN", "ADV", "APPR", "PPER", "APPRART", "VVINF", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Die hoffart, denn sie sieht mich offtermahls kaum an;", "tokens": ["Die", "hof\u00b7fart", ",", "denn", "sie", "sieht", "mich", "off\u00b7ter\u00b7mahls", "kaum", "an", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KON", "PPER", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die unbarmhertzigkeit, sie achtet nichts mein klagen;", "tokens": ["Die", "un\u00b7barm\u00b7hert\u00b7zig\u00b7keit", ",", "sie", "ach\u00b7tet", "nichts", "mein", "kla\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PIS", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ja was vor s\u00fcnden man nur immer nennen kan,", "tokens": ["Ja", "was", "vor", "s\u00fcn\u00b7den", "man", "nur", "im\u00b7mer", "nen\u00b7nen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PRELS", "APPR", "ADJA", "PIS", "ADV", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Die kan ich insgesamt von dieser sch\u00f6nen sagen.", "tokens": ["Die", "kan", "ich", "ins\u00b7ge\u00b7samt", "von", "die\u00b7ser", "sch\u00f6\u00b7nen", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "APPR", "PDAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}