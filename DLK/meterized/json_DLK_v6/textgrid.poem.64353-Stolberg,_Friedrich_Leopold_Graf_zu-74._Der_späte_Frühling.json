{"textgrid.poem.64353": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "74. Der sp\u00e4te Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Fr\u00fchjahr ist kommen, der Fr\u00fchling noch nicht:", "tokens": ["Das", "Fr\u00fch\u00b7jahr", "ist", "kom\u00b7men", ",", "der", "Fr\u00fch\u00b7ling", "noch", "nicht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$,", "ART", "NN", "ADV", "PTKNEG", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Noch macht die Natur uns ein saures Gesicht,", "tokens": ["Noch", "macht", "die", "Na\u00b7tur", "uns", "ein", "sau\u00b7res", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Noch dr\u00e4uen die Wolken uns Schlo\u00dfen und Schnee,", "tokens": ["Noch", "dr\u00e4u\u00b7en", "die", "Wol\u00b7ken", "uns", "Schlo\u00b7\u00dfen", "und", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Noch spiegelt sich d\u00fcrstend im Eise das Reh.", "tokens": ["Noch", "spie\u00b7gelt", "sich", "d\u00fcrs\u00b7tend", "im", "Ei\u00b7se", "das", "Reh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Der Frost hat das niedrige Veilchen erstickt,", "tokens": ["Der", "Frost", "hat", "das", "nied\u00b7ri\u00b7ge", "Veil\u00b7chen", "er\u00b7stickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und die Knospe der hohen Kastanie geknickt:", "tokens": ["Und", "die", "Knos\u00b7pe", "der", "ho\u00b7hen", "Kas\u00b7ta\u00b7nie", "ge\u00b7knickt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Es starb in der Knospe die Bl\u00fcte vom Wind,", "tokens": ["Es", "starb", "in", "der", "Knos\u00b7pe", "die", "Bl\u00fc\u00b7te", "vom", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "So stirbt in der sterbenden Mutter ihr Kind.", "tokens": ["So", "stirbt", "in", "der", "ster\u00b7ben\u00b7den", "Mut\u00b7ter", "ihr", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Die Pfl\u00e4nzchen im Garten sind gelb und erstarrt;", "tokens": ["Die", "Pfl\u00e4nz\u00b7chen", "im", "Gar\u00b7ten", "sind", "gelb", "und", "er\u00b7starrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Es seufzet der Stier, denn der Boden ist hart;", "tokens": ["Es", "seuf\u00b7zet", "der", "Stier", ",", "denn", "der", "Bo\u00b7den", "ist", "hart", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die Schwalbe verbirgt sich; die Glucke verl\u00e4\u00dft", "tokens": ["Die", "Schwal\u00b7be", "ver\u00b7birgt", "sich", ";", "die", "Glu\u00b7cke", "ver\u00b7l\u00e4\u00dft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "$.", "ART", "NN", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die sterbenden K\u00fcchlein, erstarret im Nest.", "tokens": ["Die", "ster\u00b7ben\u00b7den", "K\u00fcch\u00b7lein", ",", "er\u00b7star\u00b7ret", "im", "Nest", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Doch hat sich der Storch auf dem Kirchturm gezeigt;", "tokens": ["Doch", "hat", "sich", "der", "Storch", "auf", "dem", "Kirch\u00b7turm", "ge\u00b7zeigt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Auch sah ich die Nachtigall, aber sie schweigt.", "tokens": ["Auch", "sah", "ich", "die", "Nach\u00b7ti\u00b7gall", ",", "a\u00b7ber", "sie", "schweigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "O Nachtigall, Nachtigall, nimm dich in acht:", "tokens": ["O", "Nach\u00b7ti\u00b7gall", ",", "Nach\u00b7ti\u00b7gall", ",", "nimm", "dich", "in", "acht", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "VVIMP", "PPER", "APPR", "CARD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die Stauden sind nacket, und kalt ist die Nacht!", "tokens": ["Die", "Stau\u00b7den", "sind", "na\u00b7cket", ",", "und", "kalt", "ist", "die", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "KON", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Langbeinigter K\u00fcster, du klapperst zu fr\u00fch", "tokens": ["Lang\u00b7bei\u00b7nig\u00b7ter", "K\u00fcs\u00b7ter", ",", "du", "klap\u00b7perst", "zu", "fr\u00fch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "PTKA", "ADJD"], "meter": "+-+-+-+++-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Im sausigen Kirchturm; kein Fr\u00fchling ist hie!", "tokens": ["Im", "sau\u00b7si\u00b7gen", "Kirch\u00b7turm", ";", "kein", "Fr\u00fch\u00b7ling", "ist", "hie", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "PIAT", "NN", "VAFIN", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "O sei mir in Zukunft kein L\u00fcgenprophet,", "tokens": ["O", "sei", "mir", "in", "Zu\u00b7kunft", "kein", "L\u00fc\u00b7gen\u00b7pro\u00b7phet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "APPR", "NN", "PIAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wenn mein Weibchen zuerst auf dem Turm dich ersp\u00e4ht!", "tokens": ["Wenn", "mein", "Weib\u00b7chen", "zu\u00b7erst", "auf", "dem", "Turm", "dich", "er\u00b7sp\u00e4ht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Dann t\u00f6n' in den Erlen das B\u00e4chlein entlang", "tokens": ["Dann", "t\u00f6n'", "in", "den", "Er\u00b7len", "das", "B\u00e4ch\u00b7lein", "ent\u00b7lang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "APPO"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Dem br\u00fctenden Weibchen der Nachtigall Sang!", "tokens": ["Dem", "br\u00fc\u00b7ten\u00b7den", "Weib\u00b7chen", "der", "Nach\u00b7ti\u00b7gall", "Sang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Es lausche mein br\u00fctendes Weibchen zugleich!", "tokens": ["Es", "lau\u00b7sche", "mein", "br\u00fc\u00b7ten\u00b7des", "Weib\u00b7chen", "zu\u00b7gleich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Du aber, o Kuckuck, ich warne dich, schweig!", "tokens": ["Du", "a\u00b7ber", ",", "o", "Ku\u00b7ckuck", ",", "ich", "war\u00b7ne", "dich", ",", "schweig", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "$,", "FM", "NN", "$,", "PPER", "VVFIN", "PPER", "$,", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}