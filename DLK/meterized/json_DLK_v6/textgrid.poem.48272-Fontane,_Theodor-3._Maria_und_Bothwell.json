{"textgrid.poem.48272": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "3. Maria und Bothwell", "genre": "verse", "period": "N.A.", "pub_year": 1851, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nig Darnley liegt erschlagen,", "tokens": ["K\u00f6\u00b7nig", "Darn\u00b7ley", "liegt", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Graf Bothwell hat es getan;", "tokens": ["Graf", "Both\u00b7well", "hat", "es", "ge\u00b7tan", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sechs Lords von Schottland tragen", "tokens": ["Sechs", "Lords", "von", "Schott\u00b7land", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Leiche nach Sankt Alban,", "tokens": ["Die", "Lei\u00b7che", "nach", "Sankt", "Al\u00b7ban", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "VVFIN", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sie stellen bei Fackelscheine", "tokens": ["Sie", "stel\u00b7len", "bei", "Fa\u00b7ckel\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Den Sarg an den Altar hin \u2013", "tokens": ["Den", "Sarg", "an", "den", "Al\u00b7tar", "hin", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Von Trauernden fehlt nur ", "tokens": ["Von", "Trau\u00b7ern\u00b7den", "fehlt", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ADV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Maria, die K\u00f6nigin.", "tokens": ["Ma\u00b7ria", ",", "die", "K\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die sitzet daheim im Schlosse,", "tokens": ["Die", "sit\u00b7zet", "da\u00b7heim", "im", "Schlos\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In funkelnder Nische des Saals,", "tokens": ["In", "fun\u00b7keln\u00b7der", "Ni\u00b7sche", "des", "Saals", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Auf dem Sammetpf\u00fchl ihr Genosse", "tokens": ["Auf", "dem", "Sam\u00b7met\u00b7pf\u00fchl", "ihr", "Ge\u00b7nos\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ist der M\u00f6rder ihres Gemahls;", "tokens": ["Ist", "der", "M\u00f6r\u00b7der", "ih\u00b7res", "Ge\u00b7mahls", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dem Lande kleidet die Trauer,", "tokens": ["Dem", "Lan\u00b7de", "klei\u00b7det", "die", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Der K\u00f6nigin kleidet die Lust,", "tokens": ["Der", "K\u00f6\u00b7ni\u00b7gin", "klei\u00b7det", "die", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Kalt-hei\u00dfe Wonneschauer", "tokens": ["Kal\u00b7thei\u00b7\u00dfe", "Won\u00b7ne\u00b7schau\u00b7er"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Durchrieseln ihre Brust.", "tokens": ["Durc\u00b7hrie\u00b7seln", "ih\u00b7re", "Brust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie spricht verlockenden Schalles:", "tokens": ["Sie", "spricht", "ver\u00b7lo\u00b7cken\u00b7den", "Schal\u00b7les", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "\u00bbnun komm und k\u00fcsse dich rot,", "tokens": ["\u00bb", "nun", "komm", "und", "k\u00fcs\u00b7se", "dich", "rot", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich danke dir alles, alles,", "tokens": ["Ich", "dan\u00b7ke", "dir", "al\u00b7les", ",", "al\u00b7les", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "$,", "PIS", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mein Leben und \u2013 ", "tokens": ["Mein", "Le\u00b7ben", "und", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "O schau nicht so fragend und bange,", "tokens": ["O", "schau", "nicht", "so", "fra\u00b7gend", "und", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "++--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Schau lieber wie sonst mich an,", "tokens": ["Schau", "lie\u00b7ber", "wie", "sonst", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KOKOM", "ADV", "PPER", "PTKVZ", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.7": {"text": "Leg ab die blasse Wange \u2013", "tokens": ["Leg", "ab", "die", "blas\u00b7se", "Wan\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Getan ist, was getan.\u00ab", "tokens": ["Ge\u00b7tan", "ist", ",", "was", "ge\u00b7tan", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVPP", "VAFIN", "$,", "PRELS", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Kerzen brennen wie l\u00fcstern", "tokens": ["Die", "Ker\u00b7zen", "bren\u00b7nen", "wie", "l\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "KOKOM", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und geben schw\u00fclen Hauch,", "tokens": ["Und", "ge\u00b7ben", "schw\u00fc\u00b7len", "Hauch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Immer leiser wird das Fl\u00fcstern,", "tokens": ["Im\u00b7mer", "lei\u00b7ser", "wird", "das", "Fl\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun schweigt das Fl\u00fcstern auch,", "tokens": ["Nun", "schweigt", "das", "Fl\u00fcs\u00b7tern", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr Atem lodert zusammen,", "tokens": ["Ihr", "A\u00b7tem", "lo\u00b7dert", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wie Glut und Glut sich mischt,", "tokens": ["Wie", "Glut", "und", "Glut", "sich", "mischt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bis m\u00e4hlich in Flackerflammen", "tokens": ["Bis", "m\u00e4h\u00b7lich", "in", "Fla\u00b7cker\u00b7flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "So Lust wie Licht erlischt.", "tokens": ["So", "Lust", "wie", "Licht", "er\u00b7lischt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOKOM", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Still wird's; nur Mondeslichter", "tokens": ["Still", "wird's", ";", "nur", "Mon\u00b7des\u00b7lich\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJD", "VAFIN", "$.", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durchhuschen noch bleich den Saal,", "tokens": ["Durch\u00b7hu\u00b7schen", "noch", "bleich", "den", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es schlummern, wie Totengesichter,", "tokens": ["Es", "schlum\u00b7mern", ",", "wie", "To\u00b7ten\u00b7ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Graf Bothwell und sein Gemahl.", "tokens": ["Graf", "Both\u00b7well", "und", "sein", "Ge\u00b7mahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie schlummern; des Windes Weise", "tokens": ["Sie", "schlum\u00b7mern", ";", "des", "Win\u00b7des", "Wei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Erstirbt im hohen Kamin,", "tokens": ["Er\u00b7stirbt", "im", "ho\u00b7hen", "Ka\u00b7min", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "An den W\u00e4nden, hastig-leise,", "tokens": ["An", "den", "W\u00e4n\u00b7den", ",", "has\u00b7tig\u00b7lei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schatten vor\u00fcberfliehn.", "tokens": ["Schat\u00b7ten", "vor\u00b7\u00fc\u00b7berf\u00b7liehn", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Und hastiger wird ihr Treiben,", "tokens": ["Und", "has\u00b7ti\u00b7ger", "wird", "ihr", "Trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schon graut und d\u00e4mmert der Tag,", "tokens": ["Schon", "graut", "und", "d\u00e4m\u00b7mert", "der", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da schl\u00e4gt's an die klirrenden Scheiben", "tokens": ["Da", "schl\u00e4gt's", "an", "die", "klir\u00b7ren\u00b7den", "Schei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wie flatternder Fl\u00fcgelschlag;", "tokens": ["Wie", "flat\u00b7tern\u00b7der", "Fl\u00fc\u00b7gel\u00b7schlag", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Auf fahren die zwei vom Kissen,", "tokens": ["Auf", "fah\u00b7ren", "die", "zwei", "vom", "Kis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "CARD", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Verst\u00f6rt an Haar und Sinn;", "tokens": ["Ver\u00b7st\u00f6rt", "an", "Haar", "und", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Im Traume ward wach ihr Gewissen,", "tokens": ["Im", "Trau\u00b7me", "ward", "wach", "ihr", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Und es murmelt die K\u00f6nigin:", "tokens": ["Und", "es", "mur\u00b7melt", "die", "K\u00f6\u00b7ni\u00b7gin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "\u00bbhilf, Himmel, ich sah die Meinen", "tokens": ["\u00bb", "hilf", ",", "Him\u00b7mel", ",", "ich", "sah", "die", "Mei\u00b7nen"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVIMP", "$,", "NN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Landfl\u00fcchtig, der Z\u00fcgel beraubt,", "tokens": ["Land\u00b7fl\u00fcch\u00b7tig", ",", "der", "Z\u00fc\u00b7gel", "be\u00b7raubt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "NN", "VVPP", "$,"], "meter": "++--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Der fallenden Krone des einen", "tokens": ["Der", "fal\u00b7len\u00b7den", "Kro\u00b7ne", "des", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ART"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Nach rollte sein fallendes Haupt,", "tokens": ["Nach", "roll\u00b7te", "sein", "fal\u00b7len\u00b7des", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Und wie Donner durch meine Seele", "tokens": ["Und", "wie", "Don\u00b7ner", "durch", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "NN", "APPR", "PPOSAT", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Ging z\u00fcrnend das alte Lied:", "tokens": ["Ging", "z\u00fcr\u00b7nend", "das", "al\u00b7te", "Lied", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Maria hat es gesprochen,", "tokens": ["Ma\u00b7ria", "hat", "es", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Graf Bothwell h\u00f6rt es kaum,", "tokens": ["Graf", "Both\u00b7well", "h\u00f6rt", "es", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Seine Schl\u00e4fe pulsen und pochen,", "tokens": ["Sei\u00b7ne", "Schl\u00e4\u00b7fe", "pul\u00b7sen", "und", "po\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Er denkt an den eigenen Traum,", "tokens": ["Er", "denkt", "an", "den", "ei\u00b7ge\u00b7nen", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Er spricht unter Starren und Stocken:", "tokens": ["Er", "spricht", "un\u00b7ter", "Star\u00b7ren", "und", "Sto\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6":