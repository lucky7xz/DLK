{"textgrid.poem.59130": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Leb im einf\u00e4ltigen Gottmeinen!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leb im einf\u00e4ltigen Gottmeinen!", "tokens": ["Leb", "im", "ein\u00b7f\u00e4l\u00b7ti\u00b7gen", "Gott\u00b7mei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Hochmut l\u00e4\u00dft oft Demut scheinen,", "tokens": ["Der", "Hoch\u00b7mut", "l\u00e4\u00dft", "oft", "De\u00b7mut", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn man sein Elend selbst anklagt,", "tokens": ["Wenn", "man", "sein", "E\u00b7lend", "selbst", "an\u00b7klagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wehrt sich, wenn's ein andrer sagt.", "tokens": ["Und", "wehrt", "sich", ",", "wenn's", "ein", "an\u00b7drer", "sagt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$,", "KOUS", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}