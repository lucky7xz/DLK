{"textgrid.poem.57306": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Laut erscholl's seit grauer Zeit, gebot Folgerung,", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Laut erscholl's seit grauer Zeit, gebot Folgerung,", "tokens": ["Laut", "er\u00b7scholl's", "seit", "grau\u00b7er", "Zeit", ",", "ge\u00b7bot", "Fol\u00b7ge\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPR", "ADJA", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "In den Kl\u00fcften allen, und allen den Labirinthen.", "tokens": ["In", "den", "Kl\u00fcf\u00b7ten", "al\u00b7len", ",", "und", "al\u00b7len", "den", "La\u00b7bi\u00b7rin\u00b7then", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "$,", "KON", "PIS", "ART", "NN", "$."], "meter": "--+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Weisheit, die Urzustand gr\u00fcbelt:", "tokens": ["Der", "Weis\u00b7heit", ",", "die", "Ur\u00b7zu\u00b7stand", "gr\u00fc\u00b7belt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+++", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Nichts ist ohne Ursach.", "tokens": ["Nichts", "ist", "oh\u00b7ne", "Ur\u00b7sach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Nichts? Ist es denn nicht Gott?", "tokens": ["Nichts", "?", "Ist", "es", "denn", "nicht", "Gott", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$.", "VAFIN", "PPER", "ADV", "PTKNEG", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da schreyen sie, ungef\u00fchrt von dem Faden", "tokens": ["Da", "schre\u00b7yen", "sie", ",", "un\u00b7ge\u00b7f\u00fchrt", "von", "dem", "Fa\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "APPR", "ART", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Des Labirinths: Gott hat seine Ursach in sich selbst!", "tokens": ["Des", "La\u00b7bi\u00b7rinths", ":", "Gott", "hat", "sei\u00b7ne", "Ur\u00b7sach", "in", "sich", "selbst", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NN", "VAFIN", "PPOSAT", "NN", "APPR", "PRF", "ADV", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Mir widert zu, nennen, was sie schreyn.", "tokens": ["Mir", "wi\u00b7dert", "zu", ",", "nen\u00b7nen", ",", "was", "sie", "schreyn", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "VVINF", "$,", "PRELS", "PPER", "VAINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Er, (wie stammeln wir Ihn) der Unaussprechliche,", "tokens": ["Er", ",", "(", "wie", "stam\u00b7meln", "wir", "Ihn", ")", "der", "Un\u00b7aus\u00b7sprech\u00b7li\u00b7che", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "$(", "PWAV", "VVFIN", "PPER", "PPER", "$(", "ART", "NN", "$,"], "meter": "--+--+-+-+--", "measure": "anapaest.di.plus"}, "line.2": {"text": "Er, das Wesen der Wesen ist ohn' Ursach.", "tokens": ["Er", ",", "das", "We\u00b7sen", "der", "We\u00b7sen", "ist", "ohn'", "Ur\u00b7sach", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "ART", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "--+--+--+--", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Aber schau auf, schau nieder, umher: da halten, durch Ihn,", "tokens": ["A\u00b7ber", "schau", "auf", ",", "schau", "nie\u00b7der", ",", "um\u00b7her", ":", "da", "hal\u00b7ten", ",", "durch", "Ihn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKVZ", "$,", "ADJD", "PTKVZ", "$,", "PTKVZ", "$.", "ADV", "VVINF", "$,", "APPR", "PPER", "$,"], "meter": "+-+--+--+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Ursachen, Wirkungen unabsehnlichen Reihntanz.", "tokens": ["Ur\u00b7sa\u00b7chen", ",", "Wir\u00b7kun\u00b7gen", "un\u00b7ab\u00b7sehn\u00b7li\u00b7chen", "Reihn\u00b7tanz", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ADJA", "NN", "$."], "meter": "+---+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Der Geschaffenen, denen Seele ward,", "tokens": ["Der", "Ge\u00b7schaf\u00b7fe\u00b7nen", ",", "de\u00b7nen", "See\u00b7le", "ward", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "NN", "VAFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Verborgenste Kraft, des Willens Freyheit", "tokens": ["Ver\u00b7bor\u00b7gens\u00b7te", "Kraft", ",", "des", "Wil\u00b7lens", "Frey\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ist das h\u00f6chste von allem, was Gott schuf,", "tokens": ["Ist", "das", "h\u00f6chs\u00b7te", "von", "al\u00b7lem", ",", "was", "Gott", "schuf", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "APPR", "PIS", "$,", "PRELS", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Ist es, die unschuldig vor Ihm, oder schuldig macht:", "tokens": ["Ist", "es", ",", "die", "un\u00b7schul\u00b7dig", "vor", "Ihm", ",", "o\u00b7der", "schul\u00b7dig", "macht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PRELS", "ADJD", "APPR", "PPER", "$,", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.5": {"line.1": {"text": "Vor Ihm!", "tokens": ["Vor", "Ihm", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PPER", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Wir endlichen Geister", "tokens": ["Wir", "end\u00b7li\u00b7chen", "Geis\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Halten \u00fcber uns selbst", "tokens": ["Hal\u00b7ten", "\u00fc\u00b7ber", "uns", "selbst"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "ADV"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Blindes Gericht.", "tokens": ["Blin\u00b7des", "Ge\u00b7richt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Verschieden ist die Denkungskraft der Unsterblichen;", "tokens": ["Ver\u00b7schie\u00b7den", "ist", "die", "Den\u00b7kungs\u00b7kraft", "der", "U\u00b7nsterb\u00b7li\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.2": {"text": "Auf Stufen stehen sie, h\u00f6heren, tieferen:", "tokens": ["Auf", "Stu\u00b7fen", "ste\u00b7hen", "sie", ",", "h\u00f6\u00b7he\u00b7ren", ",", "tie\u00b7fe\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,", "VVFIN", "$,", "ADJA", "$."], "meter": "-+-+--+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So der Unsterblichen Freyheitskraft; sie haben auch hier Genie,", "tokens": ["So", "der", "U\u00b7nsterb\u00b7li\u00b7chen", "Frey\u00b7heits\u00b7kraft", ";", "sie", "ha\u00b7ben", "auch", "hier", "Ge\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$.", "PPER", "VAFIN", "ADV", "ADV", "NN", "$,"], "meter": "--+---+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Oder sie stehen auf dieser Stufe nicht.", "tokens": ["O\u00b7der", "sie", "ste\u00b7hen", "auf", "die\u00b7ser", "Stu\u00b7fe", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PDAT", "NN", "PTKNEG", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Ursach wird die Freyheit von Handlungen,", "tokens": ["Ur\u00b7sach", "wird", "die", "Frey\u00b7heit", "von", "Hand\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+--++-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Die der Allwissende selbst nicht vorher mit Gewissheit sieht:", "tokens": ["Die", "der", "All\u00b7wis\u00b7sen\u00b7de", "selbst", "nicht", "vor\u00b7her", "mit", "Ge\u00b7wiss\u00b7heit", "sieht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "PTKNEG", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-++--+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Aber Er, der Immerwirkende, leitet sie", "tokens": ["A\u00b7ber", "Er", ",", "der", "Im\u00b7mer\u00b7wir\u00b7ken\u00b7de", ",", "lei\u00b7tet", "sie"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "$,", "ART", "NN", "$,", "VVFIN", "PPER"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Zu der Sch\u00f6pfung letztem Zweck, der Seligkeit Aller!", "tokens": ["Zu", "der", "Sch\u00f6p\u00b7fung", "letz\u00b7tem", "Zweck", ",", "der", "Se\u00b7lig\u00b7keit", "Al\u00b7ler", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-++-", "measure": "unknown.measure.septa"}}, "stanza.8": {"line.1": {"text": "Anbetung Ihm, der nicht ruhend anschaut,", "tokens": ["An\u00b7be\u00b7tung", "Ihm", ",", "der", "nicht", "ru\u00b7hend", "an\u00b7schaut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PRELS", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Der, auch durch ewiges Wirken, selig ist?", "tokens": ["Der", ",", "auch", "durch", "e\u00b7wi\u00b7ges", "Wir\u00b7ken", ",", "se\u00b7lig", "ist", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "APPR", "ADJA", "NN", "$,", "ADJD", "VAFIN", "$."], "meter": "-+-+------+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Anbetung, dass aus dem tiefen Urquell, wie Er es leitet,", "tokens": ["An\u00b7be\u00b7tung", ",", "dass", "aus", "dem", "tie\u00b7fen", "Ur\u00b7quell", ",", "wie", "Er", "es", "lei\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "APPR", "ART", "ADJA", "NN", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-----+-+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Der sittlichen Handlungen Ozean her\u00fcberstr\u00f6mt.", "tokens": ["Der", "sitt\u00b7li\u00b7chen", "Hand\u00b7lun\u00b7gen", "O\u00b7ze\u00b7an", "her\u00b7\u00fc\u00b7ber\u00b7str\u00f6mt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "VVPP", "$."], "meter": "-+--++-+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.9": {"line.1": {"text": "Grenzloser Ozean, wie brausest,", "tokens": ["Grenz\u00b7lo\u00b7ser", "O\u00b7ze\u00b7an", ",", "wie", "brau\u00b7sest", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWAV", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Donnerst du in allen Welten! Wie wandelt auf dir,", "tokens": ["Don\u00b7nerst", "du", "in", "al\u00b7len", "Wel\u00b7ten", "!", "Wie", "wan\u00b7delt", "auf", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "$.", "PWAV", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Der dir himmelsteigende Wogen gebeut,", "tokens": ["Der", "dir", "him\u00b7mel\u00b7stei\u00b7gen\u00b7de", "Wo\u00b7gen", "ge\u00b7beut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und ebne Stille.", "tokens": ["Und", "eb\u00b7ne", "Stil\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Anbetung dem Vater der Unsterblichen,", "tokens": ["An\u00b7be\u00b7tung", "dem", "Va\u00b7ter", "der", "U\u00b7nsterb\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+---", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Auch f\u00fcr meine Freyheit!", "tokens": ["Auch", "f\u00fcr", "mei\u00b7ne", "Frey\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Aber selber sie, was w\u00e4re sie mir;", "tokens": ["A\u00b7ber", "sel\u00b7ber", "sie", ",", "was", "w\u00e4\u00b7re", "sie", "mir", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "$,", "PWS", "VAFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "K\u00f6nt' ich nicht auch Gott denken, so gar Gott lieben.", "tokens": ["K\u00f6nt'", "ich", "nicht", "auch", "Gott", "den\u00b7ken", ",", "so", "gar", "Gott", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "NN", "VVINF", "$,", "ADV", "ADV", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}