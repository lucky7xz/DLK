{"textgrid.poem.26448": {"metadata": {"author": {"name": "Whitman, Walt", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich bin's, der da leidet vor br\u00fcnstiger Liebe.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin's, der da leidet vor br\u00fcnstiger Liebe.", "tokens": ["Ich", "bin's", ",", "der", "da", "lei\u00b7det", "vor", "br\u00fcns\u00b7ti\u00b7ger", "Lie\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Zieht die Erde nicht an? Ziehen nicht alle Stoffe schmerzvoll alle andern Stoffe an?", "tokens": ["Zieht", "die", "Er\u00b7de", "nicht", "an", "?", "Zie\u00b7hen", "nicht", "al\u00b7le", "Stof\u00b7fe", "schmerz\u00b7voll", "al\u00b7le", "an\u00b7dern", "Stof\u00b7fe", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "PTKVZ", "$.", "VVFIN", "PTKNEG", "PIAT", "NN", "ADJD", "PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+--+-+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "So zieht auch mein K\u00f6rper alle andern, die mir begegnen, an.", "tokens": ["So", "zieht", "auch", "mein", "K\u00f6r\u00b7per", "al\u00b7le", "an\u00b7dern", ",", "die", "mir", "be\u00b7geg\u00b7nen", ",", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "PIAT", "ADJA", "$,", "PRELS", "PPER", "VVINF", "$,", "PTKVZ", "$."], "meter": "-+--+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}}}}