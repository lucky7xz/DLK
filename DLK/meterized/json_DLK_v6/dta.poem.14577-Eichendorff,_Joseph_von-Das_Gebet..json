{"dta.poem.14577": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Das Gebet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Hinauszuschlagen in die T\u00f6ne,", "tokens": ["Hin\u00b7aus\u00b7zu\u00b7schla\u00b7gen", "in", "die", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Lockt Dich Natur mit wilder Lust,", "tokens": ["Lockt", "Dich", "Na\u00b7tur", "mit", "wil\u00b7der", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zieht Minne, holde Frauensch\u00f6ne", "tokens": ["Zieht", "Min\u00b7ne", ",", "hol\u00b7de", "Frau\u00b7en\u00b7sch\u00f6\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVIMP", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Abgrund s\u00fc\u00df die seel'ge Brust;", "tokens": ["Zum", "Ab\u00b7grund", "s\u00fc\u00df", "die", "seel'\u00b7ge", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Tod siehst Du verh\u00fcllet gehen", "tokens": ["Den", "Tod", "siehst", "Du", "ver\u00b7h\u00fcl\u00b7let", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch Lieb' und Leben himmelw\u00e4rts,", "tokens": ["Durch", "Lieb'", "und", "Le\u00b7ben", "him\u00b7mel\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.7": {"text": "Ein einzig Wunder nur bleibt stehen", "tokens": ["Ein", "ein\u00b7zig", "Wun\u00b7der", "nur", "bleibt", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "ADV", "VVFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Einsam \u00fcber dem \u00f6den Schmerz. \u2014", "tokens": ["Ein\u00b7sam", "\u00fc\u00b7ber", "dem", "\u00f6\u00b7den", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}