{"textgrid.poem.46671": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[o Weihnachtsbaum]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Weihnachtsbaum,", "tokens": ["O", "Weih\u00b7nachts\u00b7baum", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "O Weihnachtstraum!", "tokens": ["O", "Weih\u00b7nachts\u00b7traum", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wie erloschen ist dein Glanz,", "tokens": ["Wie", "er\u00b7lo\u00b7schen", "ist", "dein", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie zerstoben ist der Kranz,", "tokens": ["Wie", "zer\u00b7sto\u00b7ben", "ist", "der", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der um dich den Freudentanz", "tokens": ["Der", "um", "dich", "den", "Freu\u00b7den\u00b7tanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PRF", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schlang zur Weihnachtsfeier.", "tokens": ["Schlang", "zur", "Weih\u00b7nachts\u00b7fei\u00b7er", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "O Weihnachtsbaum,", "tokens": ["O", "Weih\u00b7nachts\u00b7baum", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "O Weihnachtstraum!", "tokens": ["O", "Weih\u00b7nachts\u00b7traum", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der du noch an jedem Ast", "tokens": ["Der", "du", "noch", "an", "je\u00b7dem", "Ast"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Halbverbrannte Kerzen hast;", "tokens": ["Halb\u00b7ver\u00b7brann\u00b7te", "Ker\u00b7zen", "hast", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn wir l\u00f6schten sie mit Hast", "tokens": ["Denn", "wir", "l\u00f6schten", "sie", "mit", "Hast"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Mitten in der Feier.", "tokens": ["Mit\u00b7ten", "in", "der", "Fei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "O Weihnachtsbaum,", "tokens": ["O", "Weih\u00b7nachts\u00b7baum", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "O Weihnachtstraum!", "tokens": ["O", "Weih\u00b7nachts\u00b7traum", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Jeder Zweig ist noch beschwert,", "tokens": ["Je\u00b7der", "Zweig", "ist", "noch", "be\u00b7schwert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und kein Naschwerk abgeleert.", "tokens": ["Und", "kein", "Naschwerk", "ab\u00b7ge\u00b7leert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach, da\u00df du so unverheert", "tokens": ["Ach", ",", "da\u00df", "du", "so", "un\u00b7ver\u00b7heert"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ueberstandst die Feier.", "tokens": ["Ue\u00b7ber\u00b7standst", "die", "Fei\u00b7er", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "O Weihnachtsbaum,", "tokens": ["O", "Weih\u00b7nachts\u00b7baum", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "O Weihnachtstraum!", "tokens": ["O", "Weih\u00b7nachts\u00b7traum", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mit den Fr\u00fcchten unverzehrt,", "tokens": ["Mit", "den", "Fr\u00fcch\u00b7ten", "un\u00b7ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit den Kerzen unversehrt,", "tokens": ["Mit", "den", "Ker\u00b7zen", "un\u00b7ver\u00b7sehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Steh', bis Weihnacht wiederkehrt,", "tokens": ["Steh'", ",", "bis", "Weih\u00b7nacht", "wie\u00b7der\u00b7kehrt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Steh' zur Todtenfeier.", "tokens": ["Steh'", "zur", "Tod\u00b7ten\u00b7fei\u00b7er", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "O Weihnachtsbaum,", "tokens": ["O", "Weih\u00b7nachts\u00b7baum", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "O Weihnachtstraum!", "tokens": ["O", "Weih\u00b7nachts\u00b7traum", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wenn wir neu dich z\u00fcnden an,", "tokens": ["Wenn", "wir", "neu", "dich", "z\u00fcn\u00b7den", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kaufen wir kein Englein d'ran;", "tokens": ["Kau\u00b7fen", "wir", "kein", "En\u00b7glein", "d'\u00b7ran", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Unsre beiden Englein nahn", "tokens": ["Uns\u00b7re", "bei\u00b7den", "En\u00b7glein", "nahn"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "PIAT", "NN", "ADJA"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Drobenher zur Feier.", "tokens": ["Dro\u00b7ben\u00b7her", "zur", "Fei\u00b7er", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}