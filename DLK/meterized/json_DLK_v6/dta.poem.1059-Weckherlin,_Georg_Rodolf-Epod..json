{"dta.poem.1059": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Epod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So w\u00f6llet nu gn\u00e4diglich", "tokens": ["So", "w\u00f6l\u00b7let", "nu", "gn\u00e4\u00b7di\u00b7glich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mich vo\u0303 forcht vn\u0303 sorge\u0303 freihe\u0303/", "tokens": ["Mich", "v\u00f5", "forcht", "v\u00f1", "sor\u00b7g\u1ebd", "frei\u00b7h\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd dan auch freygebiglich", "tokens": ["Vnd", "dan", "auch", "frey\u00b7ge\u00b7big\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ehr vnd g\u00fcter mir verleyhen/", "tokens": ["Ehr", "vnd", "g\u00fc\u00b7ter", "mir", "ver\u00b7ley\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dan die tugent vnd das gut", "tokens": ["Dan", "die", "tu\u00b7gent", "vnd", "das", "gut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "KON", "ART", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zusamen gr\u00f6ssen den muht:", "tokens": ["Zu\u00b7sa\u00b7men", "gr\u00f6s\u00b7sen", "den", "muht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Alsdan d\u00e4mpfend mein begehren", "tokens": ["Als\u00b7dan", "d\u00e4mp\u00b7fend", "mein", "be\u00b7geh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPOSAT", "VVINF"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Mit reich vnd f\u00fcrstlicher hand/", "tokens": ["Mit", "reich", "vnd", "f\u00fcrst\u00b7li\u00b7cher", "hand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "ADJA", "NN", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.9": {"text": "Soll ewer nam vnd ruhm wehren", "tokens": ["Soll", "e\u00b7wer", "nam", "vnd", "ruhm", "weh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "KON", "NN", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.10": {"text": "Alslang man in dem Teutschland", "tokens": ["Als\u00b7lang", "man", "in", "dem", "Teutschland"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Wirt das Volck teutsch reden h\u00f6ren.", "tokens": ["Wirt", "das", "Volck", "teutsch", "re\u00b7den", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}