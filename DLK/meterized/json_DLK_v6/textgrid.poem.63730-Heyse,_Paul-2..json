{"textgrid.poem.63730": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was nur denk' ich davon? Mich lockte die F\u00fclle des Mondes", "tokens": ["Was", "nur", "denk'", "ich", "da\u00b7von", "?", "Mich", "lock\u00b7te", "die", "F\u00fcl\u00b7le", "des", "Mon\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "PAV", "$.", "PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wieder hinaus. Nun stand k\u00fchl an den Bergen die Nacht.", "tokens": ["Wie\u00b7der", "hin\u00b7aus", ".", "Nun", "stand", "k\u00fchl", "an", "den", "Ber\u00b7gen", "die", "Nacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "ADV", "VVFIN", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Noch summt tief in den Gassen ged\u00e4mpfteren Klanges die Freude,", "tokens": ["Noch", "summt", "tief", "in", "den", "Gas\u00b7sen", "ge\u00b7d\u00e4mpf\u00b7te\u00b7ren", "Klan\u00b7ges", "die", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "APPR", "ART", "NN", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+---+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Aber die D\u00e4cher, wie still ruhn sie, ver\u00f6det und wei\u00df.", "tokens": ["A\u00b7ber", "die", "D\u00e4\u00b7cher", ",", "wie", "still", "ruhn", "sie", ",", "ver\u00b7\u00f6\u00b7det", "und", "wei\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "ADJD", "VVFIN", "PPER", "$,", "VVPP", "KON", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Und so blick' ich hin\u00fcber zum Nachbarsdach; in der besten", "tokens": ["Und", "so", "blick'", "ich", "hin\u00b7\u00fc\u00b7ber", "zum", "Nach\u00b7bars\u00b7dach", ";", "in", "der", "bes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$.", "APPR", "ART", "ADJA"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Unschuld wahrlich, und was h\u00e4tt' ich zu suchen gehabt?", "tokens": ["Un\u00b7schuld", "wahr\u00b7lich", ",", "und", "was", "h\u00e4tt'", "ich", "zu", "su\u00b7chen", "ge\u00b7habt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "KON", "PWS", "VAFIN", "PPER", "PTKZU", "VVINF", "VAPP", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Aber \u2013 da raunt mir ein L\u00fcftchen ins Ohr: Wo blieb die Orange,", "tokens": ["A\u00b7ber", "\u2013", "da", "raunt", "mir", "ein", "L\u00fcft\u00b7chen", "ins", "Ohr", ":", "Wo", "blieb", "die", "O\u00b7ran\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "ADV", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$.", "PWAV", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+--+-+-+--", "measure": "elegiambus"}, "line.8": {"text": "Die du geworfen? Zuvor lag sie doch sichtbar genug,", "tokens": ["Die", "du", "ge\u00b7wor\u00b7fen", "?", "Zu\u00b7vor", "lag", "sie", "doch", "sicht\u00b7bar", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$.", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Dort, noch wei\u00df ich den Fleck, an der Br\u00fcstung, wo sich in T\u00f6pfen", "tokens": ["Dort", ",", "noch", "wei\u00df", "ich", "den", "Fleck", ",", "an", "der", "Br\u00fcs\u00b7tung", ",", "wo", "sich", "in", "T\u00f6p\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,", "APPR", "ART", "NN", "$,", "PWAV", "PRF", "APPR", "NN"], "meter": "--+--+--+-++-+-", "measure": "anapaest.tri.plus"}, "line.10": {"text": "Angiolina die buntbl\u00fchenden Nelken erzieht.", "tokens": ["An\u00b7gi\u00b7o\u00b7li\u00b7na", "die", "bunt\u00b7bl\u00fc\u00b7hen\u00b7den", "Nel\u00b7ken", "er\u00b7zieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+--+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Hat wohl die sie gefunden? \u2013 Gewi\u00df nicht. Nimmer besucht sie", "tokens": ["Hat", "wohl", "die", "sie", "ge\u00b7fun\u00b7den", "?", "\u2013", "Ge\u00b7wi\u00df", "nicht", ".", "Nim\u00b7mer", "be\u00b7sucht", "sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PRELS", "PPER", "VVPP", "$.", "$(", "ADV", "PTKNEG", "$.", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Abends die Blumen; so h\u00f6rt gleich sie ein jeder im Haus,", "tokens": ["A\u00b7bends", "die", "Blu\u00b7men", ";", "so", "h\u00f6rt", "gleich", "sie", "ein", "je\u00b7der", "im", "Haus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$.", "ADV", "VVFIN", "ADV", "PPER", "ART", "PIS", "APPRART", "NN", "$,"], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.13": {"text": "Weil aus Robert dem Teufel sie j\u00fcngst die Romanze gelernt hat.", "tokens": ["Weil", "aus", "Ro\u00b7bert", "dem", "Teu\u00b7fel", "sie", "j\u00fcngst", "die", "Ro\u00b7man\u00b7ze", "ge\u00b7lernt", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NE", "ART", "NN", "PPER", "ADV", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "--+--+--+-+---+-", "measure": "anapaest.tri.plus"}, "line.14": {"text": "Nun, heut war sie vielleicht heiser; sie fand sie gewi\u00df.", "tokens": ["Nun", ",", "heut", "war", "sie", "viel\u00b7leicht", "hei\u00b7ser", ";", "sie", "fand", "sie", "ge\u00b7wi\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$.", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Oder die Magd \u2013 nichts ist wahrscheinlicher! Oder das H\u00fcndchen", "tokens": ["O\u00b7der", "die", "Magd", "\u2013", "nichts", "ist", "wahr\u00b7schein\u00b7li\u00b7cher", "!", "O\u00b7der", "das", "H\u00fcnd\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$(", "PIS", "VAFIN", "ADJD", "$.", "KON", "ART", "NN"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Fiffi spielte mit ihr, rollte sie weiter. \u2013 Und sind", "tokens": ["Fif\u00b7fi", "spiel\u00b7te", "mit", "ihr", ",", "roll\u00b7te", "sie", "wei\u00b7ter", ".", "\u2013", "Und", "sind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPER", "$,", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "KON", "VAFIN"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Nicht auch Katzen genug in Sorrent? Wahrhaftig, die Katzen", "tokens": ["Nicht", "auch", "Kat\u00b7zen", "ge\u00b7nug", "in", "Sor\u00b7rent", "?", "Wahr\u00b7haf\u00b7tig", ",", "die", "Kat\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PTKNEG", "ADV", "NN", "ADV", "APPR", "NN", "$.", "ADJD", "$,", "ART", "NN"], "meter": "--+--+-+-+---+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Holten sie. Fiffi ist schon viel zu erwachsen zum Spiel.", "tokens": ["Hol\u00b7ten", "sie", ".", "Fif\u00b7fi", "ist", "schon", "viel", "zu", "er\u00b7wach\u00b7sen", "zum", "Spiel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "NE", "VAFIN", "ADV", "ADV", "PTKZU", "VVINF", "APPRART", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.19": {"text": "Oder \u2013 wie w\u00e4r's? Auch diese Vermutung eitel \u2013 so bliebe", "tokens": ["O\u00b7der", "\u2013", "wie", "w\u00e4r's", "?", "Auch", "die\u00b7se", "Ver\u00b7mu\u00b7tung", "ei\u00b7tel", "\u2013", "so", "blie\u00b7be"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$(", "KOKOM", "VAFIN", "$.", "ADV", "PDAT", "NN", "ADJD", "$(", "ADV", "VVFIN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.20": {"text": "Freilich die andere noch, da\u00df sie die Rechte geholt.", "tokens": ["Frei\u00b7lich", "die", "an\u00b7de\u00b7re", "noch", ",", "da\u00df", "sie", "die", "Rech\u00b7te", "ge\u00b7holt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.21": {"text": "Zwar nichts Eitleres kann sich ein Mensch aussinnen, als dieses.", "tokens": ["Zwar", "nichts", "Eit\u00b7le\u00b7res", "kann", "sich", "ein", "Mensch", "aus\u00b7sin\u00b7nen", ",", "als", "die\u00b7ses", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIS", "PIS", "VMFIN", "PRF", "ART", "NN", "VVINF", "$,", "KOUS", "PDAT", "$."], "meter": "--+-++--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Possen! das trotzige Ding, das die Gekr\u00e4nkte gespielt?", "tokens": ["Pos\u00b7sen", "!", "das", "trot\u00b7zi\u00b7ge", "Ding", ",", "das", "die", "Ge\u00b7kr\u00e4nk\u00b7te", "ge\u00b7spielt", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.23": {"text": "Hast du den zornigen Mund \u2013 und welch ein M\u00fcndchen \u2013! vergessen,", "tokens": ["Hast", "du", "den", "zor\u00b7ni\u00b7gen", "Mund", "\u2013", "und", "welch", "ein", "M\u00fcnd\u00b7chen", "\u2013", "!", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$(", "KON", "PWAT", "ART", "NN", "$(", "$.", "VVPP", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Und dies N\u00e4schen \u2013 und welch N\u00e4schen! \u2013 und wie sie es r\u00fcmpft',", "tokens": ["Und", "dies", "N\u00e4\u00b7schen", "\u2013", "und", "welch", "N\u00e4\u00b7schen", "!", "\u2013", "und", "wie", "sie", "es", "r\u00fcmpft'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "$(", "KON", "PWAT", "NN", "$.", "$(", "KON", "PWAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.25": {"text": "Und nun gar den ver\u00e4chtlichen Blick? \u2013 Das wei\u00df ich nun freilich", "tokens": ["Und", "nun", "gar", "den", "ver\u00b7\u00e4cht\u00b7li\u00b7chen", "Blick", "?", "\u2013", "Das", "wei\u00df", "ich", "nun", "frei\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ART", "ADJA", "NN", "$.", "$(", "PDS", "VVFIN", "PPER", "ADV", "ADV"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.26": {"text": "Noch auswendig. \u2013 Du siehst also, sie kann es nicht sein. \u2013", "tokens": ["Noch", "aus\u00b7wen\u00b7dig", ".", "\u2013", "Du", "siehst", "al\u00b7so", ",", "sie", "kann", "es", "nicht", "sein", ".", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "$.", "$(", "PPER", "VVFIN", "ADV", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VAINF", "$.", "$("], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "All das seh' ich; und doch, obwohl so schlagend bewiesen,", "tokens": ["All", "das", "seh'", "ich", ";", "und", "doch", ",", "ob\u00b7wohl", "so", "schla\u00b7gend", "be\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PDS", "VVFIN", "PPER", "$.", "KON", "ADV", "$,", "KOUS", "ADV", "ADJD", "VVPP", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Da\u00df sie es nicht sein kann, qu\u00e4lt es mich, ob sie es war.", "tokens": ["Da\u00df", "sie", "es", "nicht", "sein", "kann", ",", "qu\u00e4lt", "es", "mich", ",", "ob", "sie", "es", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VAINF", "VMFIN", "$,", "VVFIN", "PPER", "PRF", "$,", "KOUS", "PPER", "PPER", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}}}}