{"textgrid.poem.64706": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "333. Warum ich nichts von dem jetzigen Aufenthalte des Verfassers des Museum wei\u00df?", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf das, was L ... sprach, hab' ich gar nicht geh\u00f6rt:", "tokens": ["Auf", "das", ",", "was", "L", "...", "sprach", ",", "hab'", "ich", "gar", "nicht", "ge\u00b7h\u00f6rt", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "NE", "$(", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Die Mahlzeit, die er gab, war die Geduld nicht werth.", "tokens": ["Die", "Mahl\u00b7zeit", ",", "die", "er", "gab", ",", "war", "die", "Ge\u00b7duld", "nicht", "werth", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}