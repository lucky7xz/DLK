{"textgrid.poem.59331": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Neue Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herz, mein Herz, warum so fr\u00f6hlich,", "tokens": ["Herz", ",", "mein", "Herz", ",", "wa\u00b7rum", "so", "fr\u00f6h\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "PWAV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So voll Unruh und zerstreut,", "tokens": ["So", "voll", "Un\u00b7ruh", "und", "zer\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "KON", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als k\u00e4m \u00fcber Berge selig", "tokens": ["Als", "k\u00e4m", "\u00fc\u00b7ber", "Ber\u00b7ge", "se\u00b7lig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schon die sch\u00f6ne Fr\u00fchlingszeit?", "tokens": ["Schon", "die", "sch\u00f6\u00b7ne", "Fr\u00fch\u00b7lings\u00b7zeit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Weil ein liebes M\u00e4dchen wieder", "tokens": ["Weil", "ein", "lie\u00b7bes", "M\u00e4d\u00b7chen", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herzlich an dein Herz sich dr\u00fcckt,", "tokens": ["Herz\u00b7lich", "an", "dein", "Herz", "sich", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaust du fr\u00f6hlich auf und nieder,", "tokens": ["Schaust", "du", "fr\u00f6h\u00b7lich", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Erd und Himmel dich erquickt.", "tokens": ["Erd", "und", "Him\u00b7mel", "dich", "er\u00b7quickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ich hab die Fenster offen,", "tokens": ["Und", "ich", "hab", "die", "Fens\u00b7ter", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Neu zieh in die Welt hinein", "tokens": ["Neu", "zieh", "in", "die", "Welt", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "APZR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Altes Bangen, altes Hoffen!", "tokens": ["Al\u00b7tes", "Ban\u00b7gen", ",", "al\u00b7tes", "Hof\u00b7fen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fr\u00fchling, Fr\u00fchling soll es sein!", "tokens": ["Fr\u00fch\u00b7ling", ",", "Fr\u00fch\u00b7ling", "soll", "es", "sein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Still kann ich hier nicht mehr bleiben,", "tokens": ["Still", "kann", "ich", "hier", "nicht", "mehr", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "ADV", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die Brust ein Singen irrt,", "tokens": ["Durch", "die", "Brust", "ein", "Sin\u00b7gen", "irrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch zu licht ist's mir zum Schreiben,", "tokens": ["Doch", "zu", "licht", "ist's", "mir", "zum", "Schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich bin so froh verwirrt.", "tokens": ["Und", "ich", "bin", "so", "froh", "ver\u00b7wirrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Also schlendr ich durch die Gassen,", "tokens": ["Al\u00b7so", "schlendr", "ich", "durch", "die", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Menschen gehen her und hin,", "tokens": ["Men\u00b7schen", "ge\u00b7hen", "her", "und", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wei\u00df nicht, was ich tu und lasse,", "tokens": ["Wei\u00df", "nicht", ",", "was", "ich", "tu", "und", "las\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWS", "PPER", "NE", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur, da\u00df ich so gl\u00fccklich bin.", "tokens": ["Nur", ",", "da\u00df", "ich", "so", "gl\u00fcck\u00b7lich", "bin", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}