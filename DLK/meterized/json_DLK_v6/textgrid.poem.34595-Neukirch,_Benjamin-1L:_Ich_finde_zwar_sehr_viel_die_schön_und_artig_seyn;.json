{"textgrid.poem.34595": {"metadata": {"author": {"name": "Neukirch, Benjamin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich finde zwar sehr viel/ die sch\u00f6n und artig seyn;", "genre": "verse", "period": "N.A.", "pub_year": 1697, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich finde zwar sehr viel/ die sch\u00f6n und artig seyn;", "tokens": ["Ich", "fin\u00b7de", "zwar", "sehr", "viel", "/", "die", "sch\u00f6n", "und", "ar\u00b7tig", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "$(", "ART", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann eine r\u00fchret uns durch ihrer augen schein/", "tokens": ["Dann", "ei\u00b7ne", "r\u00fch\u00b7ret", "uns", "durch", "ih\u00b7rer", "au\u00b7gen", "schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die andre lacht und prangt mit lippen von corallen/", "tokens": ["Die", "and\u00b7re", "lacht", "und", "prangt", "mit", "lip\u00b7pen", "von", "co\u00b7ral\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "KON", "VVFIN", "APPR", "NN", "APPR", "NE", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An vielen pflegen uns die haare zu gefallen/", "tokens": ["An", "vie\u00b7len", "pfle\u00b7gen", "uns", "die", "haa\u00b7re", "zu", "ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die hat ein kleines kinn/ und eine steiffe brust/", "tokens": ["Die", "hat", "ein", "klei\u00b7nes", "kinn", "/", "und", "ei\u00b7ne", "steif\u00b7fe", "brust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$(", "KON", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die macht durch ihren gang uns zu der liebe lust/", "tokens": ["Die", "macht", "durch", "ih\u00b7ren", "gang", "uns", "zu", "der", "lie\u00b7be", "lust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die f\u00fchrt/ ich wei\u00df nicht was f\u00fcr anmuth in den lenden/", "tokens": ["Die", "f\u00fchrt", "/", "ich", "wei\u00df", "nicht", "was", "f\u00fcr", "an\u00b7muth", "in", "den", "len\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "PPER", "VVFIN", "PTKNEG", "PRELS", "APPR", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-++-+-+", "measure": "unknown.measure.septa"}, "line.8": {"text": "Und andre fesseln uns mit ihren marmel-h\u00e4nden;", "tokens": ["Und", "and\u00b7re", "fes\u00b7seln", "uns", "mit", "ih\u00b7ren", "mar\u00b7mel\u00b7h\u00e4n\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du aber/ Sylvia/ hast alles di\u00df allein:", "tokens": ["Du", "a\u00b7ber", "/", "Syl\u00b7via", "/", "hast", "al\u00b7les", "di\u00df", "al\u00b7lein", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$(", "NE", "$(", "VAFIN", "PIS", "PDS", "ADV", "$."], "meter": "-+----+-+-+", "measure": "dactylic.init"}, "line.10": {"text": "Dann iedes glied an dir kan eine kette seyn.", "tokens": ["Dann", "ie\u00b7des", "glied", "an", "dir", "kan", "ei\u00b7ne", "ket\u00b7te", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "PPER", "VMFIN", "PIS", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie soll mich \u00e4rmsten dann nicht deine pracht entz\u00fcnden/", "tokens": ["Wie", "soll", "mich", "\u00e4rms\u00b7ten", "dann", "nicht", "dei\u00b7ne", "pracht", "ent\u00b7z\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "ADV", "PTKNEG", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die/ wann man sie zertheilt/ kan ihrer sieben binden?", "tokens": ["Die", "/", "wann", "man", "sie", "zer\u00b7theilt", "/", "kan", "ih\u00b7rer", "sie\u00b7ben", "bin\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWAV", "PIS", "PPER", "VVPP", "$(", "VMFIN", "PPOSAT", "CARD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}