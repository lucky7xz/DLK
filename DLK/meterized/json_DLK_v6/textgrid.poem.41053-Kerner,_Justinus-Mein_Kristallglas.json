{"textgrid.poem.41053": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Mein Kristallglas", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Glas, das ist mein Lieben;", "tokens": ["Ein", "Glas", ",", "das", "ist", "mein", "Lie\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VAFIN", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schon sind es zehen Jahr,", "tokens": ["Schon", "sind", "es", "ze\u00b7hen", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "CARD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df es mir treu geblieben", "tokens": ["Da\u00df", "es", "mir", "treu", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Voll Scharten, dennoch klar:", "tokens": ["Voll", "Schar\u00b7ten", ",", "den\u00b7noch", "klar", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Viel Risse, Ehrenzeichen,", "tokens": ["Viel", "Ris\u00b7se", ",", "Eh\u00b7ren\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Fahne zeigt im Wind,", "tokens": ["Die", "Fah\u00b7ne", "zeigt", "im", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Des Glases Scharten sind.", "tokens": ["Des", "Gla\u00b7ses", "Schar\u00b7ten", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Oft ward es angesto\u00dfen", "tokens": ["Oft", "ward", "es", "an\u00b7ge\u00b7sto\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Sang und Klang die Rund',", "tokens": ["Mit", "Sang", "und", "Klang", "die", "Rund'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df spritzte, rot wie Rosen,", "tokens": ["Da\u00df", "spritz\u00b7te", ",", "rot", "wie", "Ro\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Wein aus seinem Grund,", "tokens": ["Der", "Wein", "aus", "sei\u00b7nem", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drob ist es nicht zersprungen,", "tokens": ["Drob", "ist", "es", "nicht", "zer\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es schlie\u00dft in sich noch gut", "tokens": ["Es", "schlie\u00dft", "in", "sich", "noch", "gut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den Alten und den Jungen,", "tokens": ["Den", "Al\u00b7ten", "und", "den", "Jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gleich wie ein Herz das Blut.", "tokens": ["Gleich", "wie", "ein", "Herz", "das", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Treu wie mein lichtes Lieben", "tokens": ["Treu", "wie", "mein", "lich\u00b7tes", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Ist selbst die Sonne nicht,", "tokens": ["Ist", "selbst", "die", "Son\u00b7ne", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Winter noch, dem tr\u00fcben,", "tokens": ["Im", "Win\u00b7ter", "noch", ",", "dem", "tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gibt's W\u00e4rme mir und Licht.", "tokens": ["Gibt's", "W\u00e4r\u00b7me", "mir", "und", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPER", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Im Winter wie im Lenze", "tokens": ["Im", "Win\u00b7ter", "wie", "im", "Len\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fcllt sich's mit goldnem Wein", "tokens": ["F\u00fcllt", "sich's", "mit", "gold\u00b7nem", "Wein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Und h\u00fcllt in Rosenkr\u00e4nze", "tokens": ["Und", "h\u00fcllt", "in", "Ro\u00b7sen\u00b7kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den Schmerz des Trinkers ein.", "tokens": ["Den", "Schmerz", "des", "Trin\u00b7kers", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Seh' ich in seine Tiefe,", "tokens": ["Seh'", "ich", "in", "sei\u00b7ne", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wird es gar seltsam mir,", "tokens": ["Wird", "es", "gar", "selt\u00b7sam", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als ob ein Freund mir riefe:", "tokens": ["Als", "ob", "ein", "Freund", "mir", "rie\u00b7fe", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Herz! Herz! ich bin bei dir!", "tokens": ["Herz", "!", "Herz", "!", "ich", "bin", "bei", "dir", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "PPER", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dies Glas hat mir gegeben", "tokens": ["Dies", "Glas", "hat", "mir", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein Freund im Trennungsschmerz,", "tokens": ["Ein", "Freund", "im", "Tren\u00b7nungs\u00b7schmerz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Zerspringt's mit meinem Leben,", "tokens": ["Zer\u00b7springt's", "mit", "mei\u00b7nem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Legt mir's im Sarg aufs Herz.", "tokens": ["Legt", "mir's", "im", "Sarg", "aufs", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}