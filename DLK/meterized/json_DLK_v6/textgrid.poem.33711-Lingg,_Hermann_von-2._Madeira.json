{"textgrid.poem.33711": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "2. Madeira", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Madeira blaut, vom Ozean umschrieben,", "tokens": ["Ma\u00b7dei\u00b7ra", "blaut", ",", "vom", "O\u00b7ze\u00b7an", "um\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "FM", "$,", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zuerst entdeckt von einem Liebespaare,", "tokens": ["Zu\u00b7erst", "ent\u00b7deckt", "von", "ei\u00b7nem", "Lie\u00b7be\u00b7spaa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Vaterfluch vom heimischen Altare", "tokens": ["Das", "Va\u00b7ter\u00b7fluch", "vom", "hei\u00b7mi\u00b7schen", "Al\u00b7ta\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf leichtem Kahn durchs wilde Meer getrieben.", "tokens": ["Auf", "leich\u00b7tem", "Kahn", "durchs", "wil\u00b7de", "Meer", "ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hier starben sie; die sch\u00f6nen Leichen blieben", "tokens": ["Hier", "star\u00b7ben", "sie", ";", "die", "sch\u00f6\u00b7nen", "Lei\u00b7chen", "blie\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bewacht von Elfen auf umbl\u00fchter Bahre,", "tokens": ["Be\u00b7wacht", "von", "El\u00b7fen", "auf", "um\u00b7bl\u00fch\u00b7ter", "Bah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis neue Kolonieen sp\u00e4trer Jahre", "tokens": ["Bis", "neu\u00b7e", "Ko\u00b7lo\u00b7ni\u00b7e\u00b7en", "sp\u00e4t\u00b7rer", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Den Hain der Liebenden in Tr\u00fcmmer hieben.", "tokens": ["Den", "Hain", "der", "Lie\u00b7ben\u00b7den", "in", "Tr\u00fcm\u00b7mer", "hie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Erz\u00fcrnt erhob ein Waldbrand seine Fl\u00fcgel,", "tokens": ["Er\u00b7z\u00fcrnt", "er\u00b7hob", "ein", "Wald\u00b7brand", "sei\u00b7ne", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ganze Insel ward zum Aschenh\u00fcgel,", "tokens": ["Die", "gan\u00b7ze", "In\u00b7sel", "ward", "zum", "A\u00b7schen\u00b7h\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und aus der Asche wieder spro\u00dften Reben.", "tokens": ["Und", "aus", "der", "A\u00b7sche", "wie\u00b7der", "spro\u00df\u00b7ten", "Re\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So ward ein Becher jetzt das Felsgesteine;", "tokens": ["So", "ward", "ein", "Be\u00b7cher", "jetzt", "das", "Fels\u00b7ge\u00b7stei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Madeira ward ein Becher edler Weine,", "tokens": ["Ma\u00b7dei\u00b7ra", "ward", "ein", "Be\u00b7cher", "ed\u00b7ler", "Wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Worin noch jener Liebe K\u00fcsse beben.", "tokens": ["Wo\u00b7rin", "noch", "je\u00b7ner", "Lie\u00b7be", "K\u00fcs\u00b7se", "be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PDAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}