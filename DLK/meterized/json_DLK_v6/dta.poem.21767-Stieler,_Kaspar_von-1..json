{"dta.poem.21767": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Der du mich \u00fcm mein Lieben schiltst", "tokens": ["Der", "du", "mich", "\u00fcm", "mein", "Lie\u00b7ben", "schiltst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PRF", "ADJD", "PPOSAT", "ADJA", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und meinen Vers nicht achten wiltst/", "tokens": ["und", "mei\u00b7nen", "Vers", "nicht", "ach\u00b7ten", "wiltst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "weil ich ihn habe weich geschrieben:", "tokens": ["weil", "ich", "ihn", "ha\u00b7be", "weich", "ge\u00b7schrie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6r an/ was mich darzu getrieben.", "tokens": ["H\u00f6r", "an", "/", "was", "mich", "dar\u00b7zu", "ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$(", "PWS", "PPER", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich bildte mir auch erstlich ein/", "tokens": ["Ich", "bild\u00b7te", "mir", "auch", "erst\u00b7lich", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ich wolt\u2019 als du tuhst/ ernstlich sein:", "tokens": ["ich", "wolt'", "als", "du", "tuhst", "/", "ernst\u00b7lich", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KOUS", "PPER", "VVFIN", "$(", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ich hatte mich der Lieb\u2019 entzogen/", "tokens": ["ich", "hat\u00b7te", "mich", "der", "Lieb'", "ent\u00b7zo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "indehm hat Amor mich betrogen.", "tokens": ["in\u00b7dehm", "hat", "A\u00b7mor", "mich", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "NE", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er stellte mir die G\u00f6tter-Zier", "tokens": ["Er", "stell\u00b7te", "mir", "die", "G\u00f6t\u00b7ter\u00b7Zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "der himmlischen Dorinden f\u00fcr:", "tokens": ["der", "himm\u00b7li\u00b7schen", "Do\u00b7rin\u00b7den", "f\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Milch-blat der Zinnober-Wangen", "tokens": ["Das", "Milch\u00b7blat", "der", "Zin\u00b7no\u00b7ber\u00b7Wan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "hat meinen wilden Geist gefangen.", "tokens": ["hat", "mei\u00b7nen", "wil\u00b7den", "Geist", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich glaube nicht/ da\u00df Jupiter", "tokens": ["Ich", "glau\u00b7be", "nicht", "/", "da\u00df", "Ju\u00b7pi\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$(", "KOUS", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "noch iezund in dem Himmel wer\u2019", "tokens": ["noch", "ie\u00b7zund", "in", "dem", "Him\u00b7mel", "wer'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "im fall\u2019 er ihrer Gaben Wesen", "tokens": ["im", "fall'", "er", "ih\u00b7rer", "Ga\u00b7ben", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "VVFIN", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "aus meinem Herzen k\u00f6nnte lesen.", "tokens": ["aus", "mei\u00b7nem", "Her\u00b7zen", "k\u00f6nn\u00b7te", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sollt\u2019 iezt ein g\u00f6ldner Apfel sein/", "tokens": ["Sollt'", "iezt", "ein", "g\u00f6ld\u00b7ner", "Ap\u00b7fel", "sein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du/ Troja/ h\u00e4ttest nicht zuklagen/", "tokens": ["Du", "/", "Tro\u00b7ja", "/", "h\u00e4t\u00b7test", "nicht", "zu\u00b7kla\u00b7gen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "NE", "$(", "VAFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "werstu um dieses Bild zerschlagen.", "tokens": ["wers\u00b7tu", "um", "die\u00b7ses", "Bild", "zer\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich wei\u00df es/ Leipzig/ was du bist/", "tokens": ["Ich", "wei\u00df", "es", "/", "Leip\u00b7zig", "/", "was", "du", "bist", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "NE", "$(", "PWS", "PPER", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "da\u00df in dir manche G\u00f6ttin ist:", "tokens": ["da\u00df", "in", "dir", "man\u00b7che", "G\u00f6t\u00b7tin", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch keine kan Dorinden gleichen/", "tokens": ["Noch", "kei\u00b7ne", "kan", "Do\u00b7rin\u00b7den", "glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "VMFIN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "noch keiner darf Dorinde weichen.", "tokens": ["noch", "kei\u00b7ner", "darf", "Do\u00b7rin\u00b7de", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VMFIN", "NE", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Willtu ein Meister st\u00fckkchen tuhn/", "tokens": ["Will\u00b7tu", "ein", "Meis\u00b7ter", "st\u00fckk\u00b7chen", "tuhn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "komm her/ Apelles/ mahle nun/", "tokens": ["komm", "her", "/", "A\u00b7pel\u00b7les", "/", "mah\u00b7le", "nun", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$(", "NE", "$(", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "du darffst dem Gr\u00e4zien nicht trauen.", "tokens": ["du", "darffst", "dem", "Gr\u00e4\u00b7zi\u00b7en", "nicht", "trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hier kanstu Venus gleichen schauen.", "tokens": ["Hier", "kans\u00b7tu", "Ve\u00b7nus", "glei\u00b7chen", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch was? dein Pinsel ist zu schlecht/", "tokens": ["Doch", "was", "?", "dein", "Pin\u00b7sel", "ist", "zu", "schlecht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "$.", "PPOSAT", "NN", "VAFIN", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "gib dich nur an f\u00fcrmeinen Knecht/", "tokens": ["gib", "dich", "nur", "an", "f\u00fcr\u00b7mei\u00b7nen", "Knecht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "wo man dir soll dein k\u00fcnstlich mahlen", "tokens": ["wo", "man", "dir", "soll", "dein", "k\u00fcnst\u00b7lich", "mah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "PPER", "VMFIN", "PPOSAT", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Die Tugend/ den beqveemen Geist/", "tokens": ["Die", "Tu\u00b7gend", "/", "den", "be\u00b7qvee\u00b7men", "Geist", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "den sie in ihrem Wesen weist/", "tokens": ["den", "sie", "in", "ih\u00b7rem", "We\u00b7sen", "weist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "kan keine Mahlerey nicht treiben:", "tokens": ["kan", "kei\u00b7ne", "Mah\u00b7le\u00b7rey", "nicht", "trei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "De\u00df Geistes Kiel mu\u00df sie beschreiben.", "tokens": ["De\u00df", "Geis\u00b7tes", "Kiel", "mu\u00df", "sie", "be\u00b7schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VMFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Di\u00df ist mir so ins Herz gelegt/", "tokens": ["Di\u00df", "ist", "mir", "so", "ins", "Herz", "ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "di\u00df ist mir so ins Herz gepregt/", "tokens": ["di\u00df", "ist", "mir", "so", "ins", "Herz", "ge\u00b7pregt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "da\u00df ich viel lieber wolt\u2019 erblassen/", "tokens": ["da\u00df", "ich", "viel", "lie\u00b7ber", "wolt'", "er\u00b7blas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "als ab-von ihrem Ruhme-lassen.", "tokens": ["als", "ab\u00b7von", "ih\u00b7rem", "Ruh\u00b7me\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ich achte keiner Lorber-Kron\u2019", "tokens": ["Ich", "ach\u00b7te", "kei\u00b7ner", "Lor\u00b7ber\u00b7Kron'"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "im fall ich nicht der Myrten Lohn", "tokens": ["im", "fall", "ich", "nicht", "der", "Myr\u00b7ten", "Lohn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPER", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "aus Jhren H\u00e4nden solt\u2019 empfangen.", "tokens": ["aus", "Ih\u00b7ren", "H\u00e4n\u00b7den", "solt'", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Nu bin ich/ F\u00f6bus/ wieder dich.", "tokens": ["Nu", "bin", "ich", "/", "F\u00f6\u00b7bus", "/", "wie\u00b7der", "dich", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$(", "NE", "$(", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kupido/ du sollst kr\u00f6nen mich:", "tokens": ["Ku\u00b7pi\u00b7do", "/", "du", "sollst", "kr\u00f6\u00b7nen", "mich", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPER", "VMFIN", "VVINF", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wei\u00df es wird mich um Pyrenen", "tokens": ["Ich", "wei\u00df", "es", "wird", "mich", "um", "Py\u00b7re\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}}}}}