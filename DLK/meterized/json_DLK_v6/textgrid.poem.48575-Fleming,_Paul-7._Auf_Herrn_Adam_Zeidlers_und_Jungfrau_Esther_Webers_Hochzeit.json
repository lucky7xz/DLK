{"textgrid.poem.48575": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "7. Auf Herrn Adam Zeidlers und Jungfrau Esther Webers Hochzeit", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh sie an, die ", "tokens": ["Sieh", "sie", "an", ",", "die"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "PRELS"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "fromme Zynthie, und h\u00f6re!", "tokens": ["from\u00b7me", "Zynt\u00b7hie", ",", "und", "h\u00f6\u00b7re", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Du auch, z\u00fcchtige Zythere,", "tokens": ["Du", "auch", ",", "z\u00fcch\u00b7ti\u00b7ge", "Zy\u00b7the\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "unsrer N\u00e4chte Heroldin,", "tokens": ["uns\u00b7rer", "N\u00e4ch\u00b7te", "He\u00b7rol\u00b7din", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "trit herf\u00fcr und gieb dein Zeichen,", "tokens": ["trit", "her\u00b7f\u00fcr", "und", "gieb", "dein", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "VVIMP", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "da\u00df die k\u00fchlen Schatten streichen!", "tokens": ["da\u00df", "die", "k\u00fch\u00b7len", "Schat\u00b7ten", "strei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ja, ihr tuts. Der sch\u00f6ne Tag", "tokens": ["Ja", ",", "ihr", "tuts", ".", "Der", "sch\u00f6\u00b7ne", "Tag"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "ist der sch\u00f6nen Nacht gewichen.", "tokens": ["ist", "der", "sch\u00f6\u00b7nen", "Nacht", "ge\u00b7wi\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ists, das dort kommt geschlichen,", "tokens": ["Was", "ists", ",", "das", "dort", "kommt", "ge\u00b7schli\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "ADV", "VVFIN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "was ists, da\u00df ich fragen mag,", "tokens": ["was", "ists", ",", "da\u00df", "ich", "fra\u00b7gen", "mag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "das aus jenen H\u00f6rnern lachet", "tokens": ["das", "aus", "je\u00b7nen", "H\u00f6r\u00b7nern", "la\u00b7chet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "PDAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und der Braut solch Sehnen machet?", "tokens": ["und", "der", "Braut", "solch", "Seh\u00b7nen", "ma\u00b7chet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lune ists, der Tag der Nacht,", "tokens": ["Lu\u00b7ne", "ists", ",", "der", "Tag", "der", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "und die Herrscherin der Flammen,", "tokens": ["und", "die", "Herr\u00b7sche\u00b7rin", "der", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "so die Lieben f\u00fcgt zusammen,", "tokens": ["so", "die", "Lie\u00b7ben", "f\u00fcgt", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "hat sich neben sie gemacht,", "tokens": ["hat", "sich", "ne\u00b7ben", "sie", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "neben sie, dieweil sie beide", "tokens": ["ne\u00b7ben", "sie", ",", "die\u00b7weil", "sie", "bei\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "KOUS", "PPER", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gleiches tun bei solcher Freude.", "tokens": ["Glei\u00b7ches", "tun", "bei", "sol\u00b7cher", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie stehn nun und sehn mit Lust", "tokens": ["Sie", "stehn", "nun", "und", "sehn", "mit", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVFIN", "APPR", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "ihre g\u00fcldnen Ritter ringen,", "tokens": ["ih\u00b7re", "g\u00fcld\u00b7nen", "Rit\u00b7ter", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df die s\u00fc\u00dfen Bahnen klingen", "tokens": ["da\u00df", "die", "s\u00fc\u00b7\u00dfen", "Bah\u00b7nen", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "einen Ton uns unbewu\u00dft.", "tokens": ["ei\u00b7nen", "Ton", "uns", "un\u00b7be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was sie mit dem Winken wollen,", "tokens": ["Was", "sie", "mit", "dem", "Win\u00b7ken", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "ist, da\u00df wir nun schlafen sollen.", "tokens": ["ist", ",", "da\u00df", "wir", "nun", "schla\u00b7fen", "sol\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Die verj\u00fcngte Fr\u00fclingswelt", "tokens": ["Die", "ver\u00b7j\u00fcng\u00b7te", "Fr\u00fc\u00b7lings\u00b7welt"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "eilet, was sie kan, zu Bette.", "tokens": ["ei\u00b7let", ",", "was", "sie", "kan", ",", "zu", "Bet\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PPER", "VMFIN", "$,", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles eilet um die Wette", "tokens": ["Al\u00b7les", "ei\u00b7let", "um", "die", "Wet\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und verbirgt sich in sein Zelt.", "tokens": ["und", "ver\u00b7birgt", "sich", "in", "sein", "Zelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles hat sich schon geleget,", "tokens": ["Al\u00b7les", "hat", "sich", "schon", "ge\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "was Wald, Luft und Wasser heget.", "tokens": ["was", "Wald", ",", "Luft", "und", "Was\u00b7ser", "he\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}}, "stanza.6": {"line.1": {"text": "Alles braucht sich seiner Ruh'.", "tokens": ["Al\u00b7les", "braucht", "sich", "sei\u00b7ner", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sehet, wie die Saat sich b\u00fccket,", "tokens": ["Se\u00b7het", ",", "wie", "die", "Saat", "sich", "b\u00fc\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "die verwachte Rose nicket", "tokens": ["die", "ver\u00b7wach\u00b7te", "Ro\u00b7se", "ni\u00b7cket"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und tut wie ihr Auge zu,", "tokens": ["und", "tut", "wie", "ihr", "Au\u00b7ge", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "und die taumelnden Zypressen", "tokens": ["und", "die", "tau\u00b7meln\u00b7den", "Zyp\u00b7res\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.6": {"text": "haben ihrer selbst vergessen!", "tokens": ["ha\u00b7ben", "ih\u00b7rer", "selbst", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Die gek\u00fchlte Luft schleicht aus", "tokens": ["Die", "ge\u00b7k\u00fchl\u00b7te", "Luft", "schleicht", "aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "und haucht auf die trucknen Matten", "tokens": ["und", "haucht", "auf", "die", "truck\u00b7nen", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "tauende gesunde Schatten,", "tokens": ["tau\u00b7en\u00b7de", "ge\u00b7sun\u00b7de", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "und das frohe Sternenhaus", "tokens": ["und", "das", "fro\u00b7he", "Ster\u00b7nen\u00b7haus"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "geu\u00dft den schlummernden Gew\u00e4chsen", "tokens": ["geu\u00dft", "den", "schlum\u00b7mern\u00b7den", "Ge\u00b7w\u00e4ch\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "neue Kraft in ihre Flechsen.", "tokens": ["neu\u00b7e", "Kraft", "in", "ih\u00b7re", "Flech\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Was ist mehr froh als die Braut,", "tokens": ["Was", "ist", "mehr", "froh", "als", "die", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "da\u00df der Lauf der hohen Sonnen", "tokens": ["da\u00df", "der", "Lauf", "der", "ho\u00b7hen", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "nun hat seinen Zweck gewonnen,", "tokens": ["nun", "hat", "sei\u00b7nen", "Zweck", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "da\u00df nun Alles, was sie schaut,", "tokens": ["da\u00df", "nun", "Al\u00b7les", ",", "was", "sie", "schaut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "ist mit dicker Nacht begossen", "tokens": ["ist", "mit", "di\u00b7cker", "Nacht", "be\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und die Augen zugeschlossen?", "tokens": ["und", "die", "Au\u00b7gen", "zu\u00b7ge\u00b7schlos\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Mahl und Tanz sind gleich nun aus.", "tokens": ["Mahl", "und", "Tanz", "sind", "gleich", "nun", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die bezechten G\u00e4ste wandern", "tokens": ["Die", "be\u00b7zech\u00b7ten", "G\u00e4s\u00b7te", "wan\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "immer einer nach dem andern;", "tokens": ["im\u00b7mer", "ei\u00b7ner", "nach", "dem", "an\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "APPR", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Iederman der sucht sein Haus.", "tokens": ["Ie\u00b7der\u00b7man", "der", "sucht", "sein", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nun vermeint sie sich zu laben,", "tokens": ["Nun", "ver\u00b7meint", "sie", "sich", "zu", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "erst recht satten Fug zu haben.", "tokens": ["erst", "recht", "sat\u00b7ten", "Fug", "zu", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Sie umf\u00e4ngt des Liebsten Leib.", "tokens": ["Sie", "um\u00b7f\u00e4ngt", "des", "Liebs\u00b7ten", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Wahr ists, da\u00df in s\u00fc\u00dfem Zagen", "tokens": ["Wahr", "ists", ",", "da\u00df", "in", "s\u00fc\u00b7\u00dfem", "Za\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "sie einandern selbst verklagen,", "tokens": ["sie", "ein\u00b7an\u00b7dern", "selbst", "ver\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "sie ihn Man und er sie Weib,", "tokens": ["sie", "ihn", "Man", "und", "er", "sie", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "PIS", "KON", "PPER", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "da\u00df mit halbgeweintem Lachen", "tokens": ["da\u00df", "mit", "halb\u00b7ge\u00b7wein\u00b7tem", "La\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "sie so neue Namen machen.", "tokens": ["sie", "so", "neu\u00b7e", "Na\u00b7men", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Lieben, redet nicht zu viel!", "tokens": ["Lie\u00b7ben", ",", "re\u00b7det", "nicht", "zu", "viel", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "VVFIN", "PTKNEG", "PTKA", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn die abgef\u00fchrten Sternen,", "tokens": ["Denn", "die", "ab\u00b7ge\u00b7f\u00fchr\u00b7ten", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "die behorchen euch vom Fernen", "tokens": ["die", "be\u00b7hor\u00b7chen", "euch", "vom", "Fer\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und verraten dieses Spiel.", "tokens": ["und", "ver\u00b7ra\u00b7ten", "die\u00b7ses", "Spiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle G\u00f6tter wissen morgen,", "tokens": ["Al\u00b7le", "G\u00f6t\u00b7ter", "wis\u00b7sen", "mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "was ihr meintet ganz verborgen.", "tokens": ["was", "ihr", "mein\u00b7tet", "ganz", "ver\u00b7bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Was ihr sonsten tut, das tut!", "tokens": ["Was", "ihr", "sons\u00b7ten", "tut", ",", "das", "tut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$,", "PRELS", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nehmt und gebet, gebt und nehmet,", "tokens": ["Nehmt", "und", "ge\u00b7bet", ",", "gebt", "und", "neh\u00b7met", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "dessen sich kein Herze sch\u00e4met,", "tokens": ["des\u00b7sen", "sich", "kein", "Her\u00b7ze", "sch\u00e4\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "dem es wird wie euch so gut!", "tokens": ["dem", "es", "wird", "wie", "euch", "so", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VAFIN", "KOKOM", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schaffets, da\u00df sich selbsten m\u00fcssen", "tokens": ["Schaf\u00b7fets", ",", "da\u00df", "sich", "selbs\u00b7ten", "m\u00fcs\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PRF", "VVINF", "VMINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "die gek\u00fc\u00dften K\u00fcsse k\u00fcssen!", "tokens": ["die", "ge\u00b7k\u00fc\u00df\u00b7ten", "K\u00fcs\u00b7se", "k\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Dieser angen\u00e4hme Streit,", "tokens": ["Die\u00b7ser", "an\u00b7ge\u00b7n\u00e4h\u00b7me", "Streit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "der aus Einigkeit entspringet,", "tokens": ["der", "aus", "Ei\u00b7nig\u00b7keit", "ent\u00b7sprin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "bringt euch, was er Allen bringet,", "tokens": ["bringt", "euch", ",", "was", "er", "Al\u00b7len", "brin\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "PPER", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "die so sein, als wie ihr seid.", "tokens": ["die", "so", "sein", ",", "als", "wie", "ihr", "seid", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAINF", "$,", "KOUS", "KOKOM", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was durch Streiten wird verloren,", "tokens": ["Was", "durch", "Strei\u00b7ten", "wird", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "wird durch Streiten auch geboren.", "tokens": ["wird", "durch", "Strei\u00b7ten", "auch", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}