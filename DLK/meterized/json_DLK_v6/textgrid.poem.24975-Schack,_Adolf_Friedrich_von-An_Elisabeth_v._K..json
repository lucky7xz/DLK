{"textgrid.poem.24975": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An Elisabeth v. K.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In deiner Seele mildem Lichte", "tokens": ["In", "dei\u00b7ner", "See\u00b7le", "mil\u00b7dem", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist mir der Fr\u00fchling aufgebl\u00fcht;", "tokens": ["Ist", "mir", "der", "Fr\u00fch\u00b7ling", "auf\u00b7ge\u00b7bl\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gereift sind meine ersten Fr\u00fcchte,", "tokens": ["Ge\u00b7reift", "sind", "mei\u00b7ne", "ers\u00b7ten", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Allein von ihrem Strahl durchgl\u00fcht.", "tokens": ["Al\u00b7lein", "von", "ih\u00b7rem", "Strahl", "durch\u00b7gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Als, sich vom Staub empor zu ringen,", "tokens": ["Als", ",", "sich", "vom", "Staub", "em\u00b7por", "zu", "rin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRF", "APPRART", "NN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Geist noch matt die Fl\u00fcgel schlug,", "tokens": ["Mein", "Geist", "noch", "matt", "die", "Fl\u00fc\u00b7gel", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Liehst du ihm, Freundin, Kraft der Schwingen", "tokens": ["Liehst", "du", "ihm", ",", "Freun\u00b7din", ",", "Kraft", "der", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "$,", "NN", "$,", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sporntest ihn zu k\u00fchnem Flug.", "tokens": ["Und", "sporn\u00b7test", "ihn", "zu", "k\u00fch\u00b7nem", "Flug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Sehnsucht, die zu lichtern R\u00e4umen", "tokens": ["Die", "Sehn\u00b7sucht", ",", "die", "zu", "lich\u00b7tern", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich aufschwingt aus dem dunklen Hier,", "tokens": ["Sich", "auf\u00b7schwingt", "aus", "dem", "dunk\u00b7len", "Hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Der Seele Rausch in hohen Tr\u00e4umen", "tokens": ["Der", "See\u00b7le", "Rausch", "in", "ho\u00b7hen", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als Lebensmitgift gabst du mir.", "tokens": ["Als", "Le\u00b7bens\u00b7mit\u00b7gift", "gabst", "du", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Mit mir auf allen meinen Wegen", "tokens": ["Mit", "mir", "auf", "al\u00b7len", "mei\u00b7nen", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zogst du als Schutzgeist ungesehn,", "tokens": ["Zogst", "du", "als", "Schutz\u00b7geist", "un\u00b7ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und deiner Lippen milden Segen", "tokens": ["Und", "dei\u00b7ner", "Lip\u00b7pen", "mil\u00b7den", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fchlt' ich um meine Stirne wehn.", "tokens": ["F\u00fchlt'", "ich", "um", "mei\u00b7ne", "Stir\u00b7ne", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Bei Nacht zu meinen Augenliden", "tokens": ["Bei", "Nacht", "zu", "mei\u00b7nen", "Au\u00b7gen\u00b7li\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hat sich im Traum dein Bild gesenkt,", "tokens": ["Hat", "sich", "im", "Traum", "dein", "Bild", "ge\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "APPRART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis es das Herz mit stillem Frieden", "tokens": ["Bis", "es", "das", "Herz", "mit", "stil\u00b7lem", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Ueberflie\u00dfen mir getr\u00e4nkt.", "tokens": ["Zum", "Ue\u00b7ber\u00b7flie\u00b7\u00dfen", "mir", "ge\u00b7tr\u00e4nkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "F\u00fcr alles, was du mir gegeben,", "tokens": ["F\u00fcr", "al\u00b7les", ",", "was", "du", "mir", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PWS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo w\u00e4r' ein Dank, der nicht zu klein?", "tokens": ["Wo", "w\u00e4r'", "ein", "Dank", ",", "der", "nicht", "zu", "klein", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "PRELS", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von einem vollen, ganzen Leben", "tokens": ["Von", "ei\u00b7nem", "vol\u00b7len", ",", "gan\u00b7zen", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Ernte dacht' ich dir zu weihn.", "tokens": ["Die", "Ern\u00b7te", "dacht'", "ich", "dir", "zu", "weihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nun, da du sankst zum fr\u00fchen Grabe,", "tokens": ["Nun", ",", "da", "du", "sankst", "zum", "fr\u00fc\u00b7hen", "Gra\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Am kalten Marmor hingekniet", "tokens": ["Am", "kal\u00b7ten", "Mar\u00b7mor", "hin\u00b7ge\u00b7kniet"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hab' ich f\u00fcr dich nicht andre Gabe", "tokens": ["Hab'", "ich", "f\u00fcr", "dich", "nicht", "and\u00b7re", "Ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "PPER", "PTKNEG", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als Thr\u00e4nen und dies arme Lied.", "tokens": ["Als", "Thr\u00e4\u00b7nen", "und", "dies", "ar\u00b7me", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "PDS", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}