{"textgrid.poem.62478": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Als die Kaiserin Elisabeth Baden verlie\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zeuch denn fort aus diesen Fluren,", "tokens": ["Zeuch", "denn", "fort", "aus", "die\u00b7sen", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "PTKVZ", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der V\u00e4ter altem Haus,", "tokens": ["Von", "der", "V\u00e4\u00b7ter", "al\u00b7tem", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von den theuren Heldenspuren", "tokens": ["Von", "den", "theu\u00b7ren", "Hel\u00b7den\u00b7spu\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In die Ferne zeuch hinaus.", "tokens": ["In", "die", "Fer\u00b7ne", "zeuch", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sieh noch einmal diese H\u00f6hen,", "tokens": ["Sieh", "noch", "ein\u00b7mal", "die\u00b7se", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einmal noch dies holde Thal,", "tokens": ["Ein\u00b7mal", "noch", "dies", "hol\u00b7de", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PDS", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das du nie in Schmach gesehen,", "tokens": ["Das", "du", "nie", "in", "Schmach", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem hell'sten Sonnenstrahl.", "tokens": ["In", "dem", "hell'\u00b7sten", "Son\u00b7nen\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Diese Bilder la\u00df begleiten", "tokens": ["Die\u00b7se", "Bil\u00b7der", "la\u00df", "be\u00b7glei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich ins kaiserliche Zelt.", "tokens": ["Dich", "ins", "kai\u00b7ser\u00b7li\u00b7che", "Zelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch des Nordens Hallen schreiten", "tokens": ["Durch", "des", "Nor\u00b7dens", "Hal\u00b7len", "schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df die deutsche Heldenwelt.", "tokens": ["La\u00df", "die", "deut\u00b7sche", "Hel\u00b7den\u00b7welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "La\u00df, wie dieses Thales Quellen,", "tokens": ["La\u00df", ",", "wie", "die\u00b7ses", "Tha\u00b7les", "Quel\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWAV", "PDAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch dein k\u00fchnes frommes Herz", "tokens": ["Auch", "dein", "k\u00fch\u00b7nes", "from\u00b7mes", "Herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oft in Lieb' und Sehnsucht schwellen", "tokens": ["Oft", "in", "Lieb'", "und", "Sehn\u00b7sucht", "schwel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach der Heimat Lust und Schmerz.", "tokens": ["Nach", "der", "Hei\u00b7mat", "Lust", "und", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "W\u00e4rm're Quellen, tief're Tiefen", "tokens": ["W\u00e4r\u00b7m'\u00b7re", "Quel\u00b7len", ",", "tie\u00b7f'\u00b7re", "Tie\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VAFIN", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hegt die gotterf\u00fcllte Brust,", "tokens": ["Hegt", "die", "got\u00b7ter\u00b7f\u00fcll\u00b7te", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tausend Geister, welche schliefen,", "tokens": ["Tau\u00b7send", "Geis\u00b7ter", ",", "wel\u00b7che", "schlie\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hast zu wecken du gewu\u00dft.", "tokens": ["Hast", "zu", "we\u00b7cken", "du", "ge\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKZU", "VVINF", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Hohes Amt hat aufgetragen", "tokens": ["Ho\u00b7hes", "Amt", "hat", "auf\u00b7ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir dein deutsches Vaterland,", "tokens": ["Dir", "dein", "deut\u00b7sches", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sel'ges Loos von fernen Tagen,", "tokens": ["Sel'\u00b7ges", "Loos", "von", "fer\u00b7nen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcrstin, liegt in deiner Hand.", "tokens": ["F\u00fcrs\u00b7tin", ",", "liegt", "in", "dei\u00b7ner", "Hand", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wenn dein Ritter, dem in Kriegen", "tokens": ["Wenn", "dein", "Rit\u00b7ter", ",", "dem", "in", "Krie\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie im Rath ein Lorbeer sprie\u00dft,", "tokens": ["Wie", "im", "Rath", "ein", "Lor\u00b7beer", "sprie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach den fernen langen Z\u00fcgen", "tokens": ["Nach", "den", "fer\u00b7nen", "lan\u00b7gen", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Sch\u00f6nheit fr\u00f6hlich gr\u00fc\u00dft,", "tokens": ["Dei\u00b7ne", "Sch\u00f6n\u00b7heit", "fr\u00f6h\u00b7lich", "gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Lab' ihn dann mit neuer St\u00e4rke,", "tokens": ["Lab'", "ihn", "dann", "mit", "neu\u00b7er", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schenk' ihm s\u00fc\u00dfen Heldenwein,", "tokens": ["Schenk'", "ihm", "s\u00fc\u00b7\u00dfen", "Hel\u00b7den\u00b7wein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df noch viel der Gotteswerke", "tokens": ["Da\u00df", "noch", "viel", "der", "Got\u00b7tes\u00b7wer\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch sein gutes Schwert gedeihn.", "tokens": ["Durch", "sein", "gu\u00b7tes", "Schwert", "ge\u00b7deihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "La\u00df ihn Deutschlands Kraft und Milde,", "tokens": ["La\u00df", "ihn", "Deutschlands", "Kraft", "und", "Mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NE", "NN", "KON", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Deutschlands Glauben, Deutschlands Treu'", "tokens": ["Deutschlands", "Glau\u00b7ben", ",", "Deutschlands", "Treu'"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Schauen in dem sch\u00f6nsten Bilde", "tokens": ["Schau\u00b7en", "in", "dem", "sch\u00f6ns\u00b7ten", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich zur Seite, jung und neu.", "tokens": ["Sich", "zur", "Sei\u00b7te", ",", "jung", "und", "neu", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Sei die Botin von dem Lande,", "tokens": ["Sei", "die", "Bo\u00b7tin", "von", "dem", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das nach ihm schaut unverwandt,", "tokens": ["Das", "nach", "ihm", "schaut", "un\u00b7ver\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Das ihm fr\u00fch zum Unterpfande", "tokens": ["Das", "ihm", "fr\u00fch", "zum", "Un\u00b7ter\u00b7pfan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich, du liebstes Gut, gesandt.", "tokens": ["Dich", ",", "du", "liebs\u00b7tes", "Gut", ",", "ge\u00b7sandt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "PPER", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Von den H\u00fcgeln, aus den Reben", "tokens": ["Von", "den", "H\u00fc\u00b7geln", ",", "aus", "den", "Re\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Quillt hier alter Grafenwein,", "tokens": ["Quillt", "hier", "al\u00b7ter", "Gra\u00b7fen\u00b7wein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und so soll dein holdes Leben", "tokens": ["Und", "so", "soll", "dein", "hol\u00b7des", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie dein Vaterland gedeihn!", "tokens": ["Wie", "dein", "Va\u00b7ter\u00b7land", "ge\u00b7deihn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}