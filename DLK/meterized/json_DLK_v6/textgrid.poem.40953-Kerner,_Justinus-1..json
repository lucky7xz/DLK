{"textgrid.poem.40953": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herz! \u2013 wie bist du inniglich", "tokens": ["Herz", "!", "\u2013", "wie", "bist", "du", "in\u00b7nig\u00b7lich"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "$(", "KOKOM", "VAFIN", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem Auge doch verbunden!", "tokens": ["Mit", "dem", "Au\u00b7ge", "doch", "ver\u00b7bun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schl\u00e4gt die Welt dir blut'ge Wunden,", "tokens": ["Schl\u00e4gt", "die", "Welt", "dir", "blut'\u00b7ge", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zeigt im Aug' die Tr\u00e4ne sich.", "tokens": ["Zeigt", "im", "Aug'", "die", "Tr\u00e4\u00b7ne", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber wird dir Wonne, Herz!", "tokens": ["A\u00b7ber", "wird", "dir", "Won\u00b7ne", ",", "Herz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sonnig dann das Auge funkelt!", "tokens": ["Son\u00b7nig", "dann", "das", "Au\u00b7ge", "fun\u00b7kelt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So wie's wieder sich verdunkelt,", "tokens": ["So", "wie's", "wie\u00b7der", "sich", "ver\u00b7dun\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kehrt in dich zur\u00fcck der Schmerz.", "tokens": ["Kehrt", "in", "dich", "zu\u00b7r\u00fcck", "der", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Bist du, Herz, in Weh und N\u00f6ten;", "tokens": ["Bist", "du", ",", "Herz", ",", "in", "Weh", "und", "N\u00f6\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schneller als der Menschen Reden", "tokens": ["Schnel\u00b7ler", "als", "der", "Men\u00b7schen", "Re\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Heilt dich 's ", "tokens": ["Heilt", "dich", "'s"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER"], "meter": "+-+", "measure": "trochaic.di"}}}}}