{"dta.poem.2846": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "47.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hier geb' ich dir, mein Sohn, Gl\u00fcck m\u00f6ge sie dir schlagen,", "tokens": ["Hier", "geb'", "ich", "dir", ",", "mein", "Sohn", ",", "Gl\u00fcck", "m\u00f6\u00b7ge", "sie", "dir", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "PPOSAT", "NN", "$,", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die dein Gro\u00dfvater einst, dein Vater dann getragen,", "tokens": ["Die", "dein", "Gro\u00df\u00b7va\u00b7ter", "einst", ",", "dein", "Va\u00b7ter", "dann", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "$,", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Uhr, nun trag du sie, und m\u00f6ge sie dein eigen", "tokens": ["Die", "Uhr", ",", "nun", "trag", "du", "sie", ",", "und", "m\u00f6\u00b7ge", "sie", "dein", "ei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "$,", "KON", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch sch\u00f6nre Stunden dir als deinen V\u00e4tern zeigen!", "tokens": ["Noch", "sch\u00f6n\u00b7re", "Stun\u00b7den", "dir", "als", "dei\u00b7nen", "V\u00e4\u00b7tern", "zei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PPER", "KOUS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ob ernstbesch\u00e4ftigte, ob heiter aufger\u00e4umte,", "tokens": ["Ob", "ernst\u00b7be\u00b7sch\u00e4f\u00b7tig\u00b7te", ",", "ob", "hei\u00b7ter", "auf\u00b7ge\u00b7r\u00e4um\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "$,", "KOUS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie zeige dir nur nie die Stunde, die vers\u00e4umte!", "tokens": ["Sie", "zei\u00b7ge", "dir", "nur", "nie", "die", "Stun\u00b7de", ",", "die", "ver\u00b7s\u00e4um\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Denn niemals, ob die Uhr du stellen magst zur\u00fcck,", "tokens": ["Denn", "nie\u00b7mals", ",", "ob", "die", "Uhr", "du", "stel\u00b7len", "magst", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "ART", "NN", "PPER", "VVINF", "VMFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kehrt die vers\u00e4umte Zeit und ein vertr\u00e4umtes Gl\u00fcck.", "tokens": ["Kehrt", "die", "ver\u00b7s\u00e4um\u00b7te", "Zeit", "und", "ein", "ver\u00b7tr\u00e4um\u00b7tes", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ein Bild des Lebens ists, was dir dein Vater gab:", "tokens": ["Ein", "Bild", "des", "Le\u00b7bens", "ists", ",", "was", "dir", "dein", "Va\u00b7ter", "gab", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "$,", "PWS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Leben wie die Uhr l\u00e4uft unaufhaltsam ab.", "tokens": ["Das", "Le\u00b7ben", "wie", "die", "Uhr", "l\u00e4uft", "un\u00b7auf\u00b7halt\u00b7sam", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die abgelaufne Uhr l\u00e4\u00dft wieder auf sich ziehn;", "tokens": ["Die", "ab\u00b7ge\u00b7lauf\u00b7ne", "Uhr", "l\u00e4\u00dft", "wie\u00b7der", "auf", "sich", "ziehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr die des Lebens ist kein Schl\u00fcssel uns verliehn.", "tokens": ["F\u00fcr", "die", "des", "Le\u00b7bens", "ist", "kein", "Schl\u00fcs\u00b7sel", "uns", "ver\u00b7liehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VAFIN", "PIAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}