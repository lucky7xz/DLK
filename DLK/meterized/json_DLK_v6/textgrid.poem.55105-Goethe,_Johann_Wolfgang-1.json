{"textgrid.poem.55105": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Strom entrauscht umw\u00f6lktem Felsensaale,", "tokens": ["Ein", "Strom", "ent\u00b7rauscht", "um\u00b7w\u00f6lk\u00b7tem", "Fel\u00b7sen\u00b7saa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Ozean sich eilig zu verbinden;", "tokens": ["Dem", "O\u00b7ze\u00b7an", "sich", "ei\u00b7lig", "zu", "ver\u00b7bin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was auch sich spiegeln mag von Grund zu Gr\u00fcnden,", "tokens": ["Was", "auch", "sich", "spie\u00b7geln", "mag", "von", "Grund", "zu", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PRF", "VVINF", "VMFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er wandelt unaufhaltsam fort zu Tale.", "tokens": ["Er", "wan\u00b7delt", "un\u00b7auf\u00b7halt\u00b7sam", "fort", "zu", "Ta\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "D\u00e4monisch aber st\u00fcrzt mit einem Male \u2013", "tokens": ["D\u00e4\u00b7mo\u00b7nisch", "a\u00b7ber", "st\u00fcrzt", "mit", "ei\u00b7nem", "Ma\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr folgen Berg und Wald in Wirbelwinden \u2013", "tokens": ["Ihr", "fol\u00b7gen", "Berg", "und", "Wald", "in", "Wir\u00b7bel\u00b7win\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich Oreas, Behagen dort zu finden,", "tokens": ["Sich", "O\u00b7re\u00b7as", ",", "Be\u00b7ha\u00b7gen", "dort", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NE", "$,", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und hemmt den Lauf, begrenzt die weite Schale.", "tokens": ["Und", "hemmt", "den", "Lauf", ",", "be\u00b7grenzt", "die", "wei\u00b7te", "Scha\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Welle spr\u00fcht, und staunt zur\u00fcck und weichet,", "tokens": ["Die", "Wel\u00b7le", "spr\u00fcht", ",", "und", "staunt", "zu\u00b7r\u00fcck", "und", "wei\u00b7chet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "VVFIN", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und schwillt bergan, sich immer selbst zu trinken;", "tokens": ["Und", "schwillt", "ber\u00b7gan", ",", "sich", "im\u00b7mer", "selbst", "zu", "trin\u00b7ken", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "PRF", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gehemmt ist nun zum Vater hin das Streben.", "tokens": ["Ge\u00b7hemmt", "ist", "nun", "zum", "Va\u00b7ter", "hin", "das", "Stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "APPRART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie schwankt und ruht, zum See zur\u00fcckgedeichet;", "tokens": ["Sie", "schwankt", "und", "ruht", ",", "zum", "See", "zu\u00b7r\u00fcck\u00b7ge\u00b7dei\u00b7chet", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gestirne, spiegelnd sich, beschaun das Blinken", "tokens": ["Ge\u00b7stir\u00b7ne", ",", "spie\u00b7gelnd", "sich", ",", "be\u00b7schaun", "das", "Blin\u00b7ken"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "PRF", "$,", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Wellenschlags am Fels, ein neues Leben.", "tokens": ["Des", "Wel\u00b7len\u00b7schlags", "am", "Fels", ",", "ein", "neu\u00b7es", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}