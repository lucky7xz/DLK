{"dta.poem.18833": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Vom Himmel sank, in wilder Meere Schauer,", "tokens": ["Vom", "Him\u00b7mel", "sank", ",", "in", "wil\u00b7der", "Mee\u00b7re", "Schau\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Tropfe bangend, gr\u00e4sslich schlug die Flut,", "tokens": ["Ein", "Trop\u00b7fe", "ban\u00b7gend", ",", "gr\u00e4ss\u00b7lich", "schlug", "die", "Flut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch lohnte Gott bescheidnen Glaubensmuth", "tokens": ["Doch", "lohn\u00b7te", "Gott", "be\u00b7scheid\u00b7nen", "Glau\u00b7bens\u00b7muth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und gab dem Tropfen Kraft und Dauer.", "tokens": ["Und", "gab", "dem", "Trop\u00b7fen", "Kraft", "und", "Dau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihn schloss die stille Muschel ein", "tokens": ["Ihn", "schloss", "die", "stil\u00b7le", "Mu\u00b7schel", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und nun, zu ew\u2019gem Ruhm und Lohne,", "tokens": ["Und", "nun", ",", "zu", "ew'\u00b7gem", "Ruhm", "und", "Loh\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Perle gl\u00e4nzt an unsers Kaisers Krone", "tokens": ["Die", "Per\u00b7le", "gl\u00e4nzt", "an", "un\u00b7sers", "Kai\u00b7sers", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit holdem Blick und mildem Schein.", "tokens": ["Mit", "hol\u00b7dem", "Blick", "und", "mil\u00b7dem", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}