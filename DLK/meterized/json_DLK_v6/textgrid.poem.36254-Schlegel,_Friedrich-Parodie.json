{"textgrid.poem.36254": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Parodie", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn sich neue Liebe regt,", "tokens": ["Wenn", "sich", "neu\u00b7e", "Lie\u00b7be", "regt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Pflegen sie es leicht zu wagen,", "tokens": ["Pfle\u00b7gen", "sie", "es", "leicht", "zu", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was sie f\u00fchlend doch nicht sagen,", "tokens": ["Was", "sie", "f\u00fch\u00b7lend", "doch", "nicht", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie das Herz sich auch bewegt.", "tokens": ["Wie", "das", "Herz", "sich", "auch", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn es noch so leise schl\u00e4gt,", "tokens": ["Wenn", "es", "noch", "so", "lei\u00b7se", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "H\u00f6rt es doch die eine Sie,", "tokens": ["H\u00f6rt", "es", "doch", "die", "ei\u00b7ne", "Sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "ART", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die ihm gern das ihre lieh';", "tokens": ["Die", "ihm", "gern", "das", "ih\u00b7re", "lieh'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ART", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und so oder so geht's allen.", "tokens": ["Und", "so", "o\u00b7der", "so", "geht's", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "ADV", "VVFIN", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wollt' ich durch den Scherz mi\u00dffallen,", "tokens": ["Wollt'", "ich", "durch", "den", "Scherz", "mi\u00df\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sagt' ich Wer und Wo und Wie.", "tokens": ["Sagt'", "ich", "Wer", "und", "Wo", "und", "Wie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "PWAV", "KON", "PWAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn die Nachtigallen schlagen", "tokens": ["Wenn", "die", "Nach\u00b7ti\u00b7gal\u00b7len", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und im dichtbelaubten Hain", "tokens": ["Und", "im", "dicht\u00b7be\u00b7laub\u00b7ten", "Hain"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit der Liebsten du allein,", "tokens": ["Mit", "der", "Liebs\u00b7ten", "du", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Magst du's f\u00fchlen, doch nicht sagen.", "tokens": ["Magst", "du's", "f\u00fch\u00b7len", ",", "doch", "nicht", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "$,", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In so wundersamen Lagen", "tokens": ["In", "so", "wun\u00b7der\u00b7sa\u00b7men", "La\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist ein zu bescheidner Knabe", "tokens": ["Ist", "ein", "zu", "be\u00b7scheid\u00b7ner", "Kna\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "PTKZU", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Selten wie ein wei\u00dfer Rabe;", "tokens": ["Sel\u00b7ten", "wie", "ein", "wei\u00b7\u00dfer", "Ra\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und so oft sie das bestritten,", "tokens": ["Und", "so", "oft", "sie", "das", "be\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PPER", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Hat es jede noch gelitten,", "tokens": ["Hat", "es", "je\u00b7de", "noch", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wenn ich sie zum besten habe.", "tokens": ["Wenn", "ich", "sie", "zum", "bes\u00b7ten", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPRART", "ADJA", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wer es je im Herzen wagte,", "tokens": ["Wer", "es", "je", "im", "Her\u00b7zen", "wag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich den Himmel zu erringen,", "tokens": ["Sich", "den", "Him\u00b7mel", "zu", "er\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fand oft solch ein sch\u00f6n Gelingen,", "tokens": ["Fand", "oft", "solch", "ein", "sch\u00f6n", "Ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "ART", "ADJD", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da\u00df er endlich sich beklagte.", "tokens": ["Da\u00df", "er", "end\u00b7lich", "sich", "be\u00b7klag\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn ich allzu k\u00fchn es sagte,", "tokens": ["Wenn", "ich", "all\u00b7zu", "k\u00fchn", "es", "sag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tadelt nicht des Scherzes Gabe,", "tokens": ["Ta\u00b7delt", "nicht", "des", "Scher\u00b7zes", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nehmt zufrieden, was ich habe.", "tokens": ["Nehmt", "zu\u00b7frie\u00b7den", ",", "was", "ich", "ha\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Liebe sollte Lust erwecken,", "tokens": ["Lie\u00b7be", "soll\u00b7te", "Lust", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Jeder gern die M\u00e4dchen necken,", "tokens": ["Je\u00b7der", "gern", "die", "M\u00e4d\u00b7chen", "ne\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Gleich verteilend solche Gabe.", "tokens": ["Gleich", "ver\u00b7tei\u00b7lend", "sol\u00b7che", "Ga\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wenn sich s\u00fc\u00df Musik ergossen,", "tokens": ["Wenn", "sich", "s\u00fc\u00df", "Mu\u00b7sik", "er\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADJD", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "D\u00fcrfen doch die Augen fragen,", "tokens": ["D\u00fcr\u00b7fen", "doch", "die", "Au\u00b7gen", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was im Grunde nichts will sagen,", "tokens": ["Was", "im", "Grun\u00b7de", "nichts", "will", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "PIS", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bleibt es bei so leichten Possen.", "tokens": ["Bleibt", "es", "bei", "so", "leich\u00b7ten", "Pos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was man einmal recht genossen,", "tokens": ["Was", "man", "ein\u00b7mal", "recht", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebt man immer oder nie,", "tokens": ["Liebt", "man", "im\u00b7mer", "o\u00b7der", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "KON", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis die s\u00fc\u00dfe Frucht gedieh.", "tokens": ["Bis", "die", "s\u00fc\u00b7\u00dfe", "Frucht", "ge\u00b7dieh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn es nur bei Scherzen bliebe,", "tokens": ["Wenn", "es", "nur", "bei", "Scher\u00b7zen", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ohne vollen Ernst der Liebe,", "tokens": ["Oh\u00b7ne", "vol\u00b7len", "Ernst", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "G\u00e4b' es keine Ironie.", "tokens": ["G\u00e4b'", "es", "kei\u00b7ne", "I\u00b7ro\u00b7nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}