{"textgrid.poem.43277": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dort in des Thurmes Glockenstube,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort in des Thurmes Glockenstube,", "tokens": ["Dort", "in", "des", "Thur\u00b7mes", "Glo\u00b7cken\u00b7stu\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dort t\u00e4nzelt auf dem Fensterbrett", "tokens": ["Dort", "t\u00e4n\u00b7zelt", "auf", "dem", "Fens\u00b7ter\u00b7brett"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein blondgelockter kecker Bube", "tokens": ["Ein", "blond\u00b7ge\u00b7lock\u00b7ter", "ke\u00b7cker", "Bu\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So leicht als ob er Fl\u00fcgel h\u00e4tt'.", "tokens": ["So", "leicht", "als", "ob", "er", "Fl\u00fc\u00b7gel", "h\u00e4tt'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "KOUS", "PPER", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er lacht und horcht dem klaren Singen", "tokens": ["Er", "lacht", "und", "horcht", "dem", "kla\u00b7ren", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der tiefgestimmten Glocken zu,", "tokens": ["Der", "tief\u00b7ge\u00b7stimm\u00b7ten", "Glo\u00b7cken", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hebt seine Arme hoch wie Schwingen", "tokens": ["Hebt", "sei\u00b7ne", "Ar\u00b7me", "hoch", "wie", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "KOKOM", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und st\u00f6rt der Tauben Mittagsruh.", "tokens": ["Und", "st\u00f6rt", "der", "Tau\u00b7ben", "Mit\u00b7tags\u00b7ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und jetzt erblickt er mich her\u00fcben,", "tokens": ["Und", "jetzt", "er\u00b7blickt", "er", "mich", "her\u00b7\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Winkt mit der M\u00fctze frohen Gru\u00df,", "tokens": ["Winkt", "mit", "der", "M\u00fct\u00b7ze", "fro\u00b7hen", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Zeigt, da\u00df auf lose Steine dr\u00fcben", "tokens": ["Zeigt", ",", "da\u00df", "auf", "lo\u00b7se", "Stei\u00b7ne", "dr\u00fc\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "APPR", "ADJA", "NN", "ADV"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Sich st\u00fctzt sein unbeschuhter Fu\u00df.", "tokens": ["Sich", "st\u00fctzt", "sein", "un\u00b7be\u00b7schuh\u00b7ter", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ob Spielgenossen ihn erwischen,", "tokens": ["Ob", "Spiel\u00b7ge\u00b7nos\u00b7sen", "ihn", "er\u00b7wi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ob ihn der Lehrer nicht entdeckt,", "tokens": ["Ob", "ihn", "der", "Leh\u00b7rer", "nicht", "ent\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Belauert sorglich er dazwischen,", "tokens": ["Be\u00b7lau\u00b7ert", "sorg\u00b7lich", "er", "da\u00b7zwi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "PAV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Auge fragt mich oft erschreckt.", "tokens": ["Sein", "Au\u00b7ge", "fragt", "mich", "oft", "er\u00b7schreckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ahnt er in mir auch den Gef\u00e4hrten,", "tokens": ["Ahnt", "er", "in", "mir", "auch", "den", "Ge\u00b7f\u00e4hr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Der zwischen Erd' und Himmel schwebt", "tokens": ["Der", "zwi\u00b7schen", "Erd'", "und", "Him\u00b7mel", "schwebt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nicht vor dem zerschmettert werden,", "tokens": ["Und", "nicht", "vor", "dem", "zer\u00b7schmet\u00b7tert", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "ART", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch vor des Lehrers Ruthe bebt?", "tokens": ["Doch", "vor", "des", "Leh\u00b7rers", "Ru\u00b7the", "bebt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}