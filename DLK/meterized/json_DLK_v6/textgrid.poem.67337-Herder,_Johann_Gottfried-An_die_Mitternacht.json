{"textgrid.poem.67337": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An die Mitternacht", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzt, in der Mitternacht,", "tokens": ["Jetzt", ",", "in", "der", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die mich erzeugte, reifte und gebar,", "tokens": ["Die", "mich", "er\u00b7zeug\u00b7te", ",", "reif\u00b7te", "und", "ge\u00b7bar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Will ich mich fragen, wer ich war.", "tokens": ["Will", "ich", "mich", "fra\u00b7gen", ",", "wer", "ich", "war", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf meiner Stirn ist Nacht! \u2013", "tokens": ["Auf", "mei\u00b7ner", "Stirn", "ist", "Nacht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ist's Wasser denn, was mir in Adern fleu\u00dft?", "tokens": ["Ist's", "Was\u00b7ser", "denn", ",", "was", "mir", "in", "A\u00b7dern", "fleu\u00dft", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "$,", "PWS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist Fleisch mein Herz, und Staub mein Geist?", "tokens": ["Ist", "Fleisch", "mein", "Herz", ",", "und", "Staub", "mein", "Geist", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "NN", "$,", "KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, Du (o weh Dir Nacht!)", "tokens": ["Ach", ",", "Du", "(", "o", "weh", "Dir", "Nacht", "!", ")"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "PPER", "$(", "FM", "ADV", "PPER", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Schriebst meinen Nam', wo goldne Namen gl\u00fchn,", "tokens": ["Schriebst", "mei\u00b7nen", "Nam'", ",", "wo", "gold\u00b7ne", "Na\u00b7men", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit Lethe's schwarzen Tropfen hin.", "tokens": ["Mit", "Lethe's", "schwar\u00b7zen", "Trop\u00b7fen", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schwarz ist mein Loos wie Du!", "tokens": ["Schwarz", "ist", "mein", "Loos", "wie", "Du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "KOKOM", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Mein B\u00fccherkreis nur eine Milbensph\u00e4r',", "tokens": ["Mein", "B\u00fc\u00b7cher\u00b7kreis", "nur", "ei\u00b7ne", "Mil\u00b7ben\u00b7sph\u00e4r'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Feinde gl\u00e4nzen um mich her.", "tokens": ["Und", "Fein\u00b7de", "gl\u00e4n\u00b7zen", "um", "mich", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "KOUI", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nur meine Knospe sinkt \u2013", "tokens": ["Nur", "mei\u00b7ne", "Knos\u00b7pe", "sinkt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sie, kaum geweckt vom fr\u00fchsten Morgenstrahl,", "tokens": ["Sie", ",", "kaum", "ge\u00b7weckt", "vom", "fr\u00fchs\u00b7ten", "Mor\u00b7gen\u00b7strahl", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADV", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kaum zweener Freunde Reiz dreimal,", "tokens": ["Kaum", "zwee\u00b7ner", "Freun\u00b7de", "Reiz", "drei\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sinkt, stirbt, verwest! O Nacht,", "tokens": ["Sinkt", ",", "stirbt", ",", "ver\u00b7west", "!", "O", "Nacht", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$.", "NE", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sprich, wo noch Geist in ihrer Asche gl\u00fcht,", "tokens": ["Sprich", ",", "wo", "noch", "Geist", "in", "ih\u00b7rer", "A\u00b7sche", "gl\u00fcht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWAV", "ADV", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df sie zu Deiner Blum' aufbl\u00fcht,", "tokens": ["Da\u00df", "sie", "zu", "Dei\u00b7ner", "Blum'", "auf\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die stillen Fr\u00fchlingsthau", "tokens": ["Die", "stil\u00b7len", "Fr\u00fch\u00b7lingst\u00b7hau"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zum Ambra f\u00fcr den matten Wandrer trinkt,", "tokens": ["Zum", "A\u00b7mbra", "f\u00fcr", "den", "mat\u00b7ten", "Wand\u00b7rer", "trinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn Philomele hoch ihm singt.", "tokens": ["Wenn", "Phi\u00b7lo\u00b7me\u00b7le", "hoch", "ihm", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}