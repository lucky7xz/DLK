{"dta.poem.15742": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Im Guten nicht allein, im Wahren auch und Sch\u00f6nen", "tokens": ["Im", "Gu\u00b7ten", "nicht", "al\u00b7lein", ",", "im", "Wah\u00b7ren", "auch", "und", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKNEG", "ADV", "$,", "APPRART", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Spricht eine Stimme laut, die nichts kann \u00fcbert\u00f6nen.", "tokens": ["Spricht", "ei\u00b7ne", "Stim\u00b7me", "laut", ",", "die", "nichts", "kann", "\u00fc\u00b7ber\u00b7t\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "$,", "PRELS", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie sie dir saget, ob du etwas recht gethan,", "tokens": ["Wie", "sie", "dir", "sa\u00b7get", ",", "ob", "du", "et\u00b7was", "recht", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht abgewichen bist von des Gesetzes Bahn;", "tokens": ["Nicht", "ab\u00b7ge\u00b7wi\u00b7chen", "bist", "von", "des", "Ge\u00b7set\u00b7zes", "Bahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "VAFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So sagt sie dir auch, ob du etwas recht erkannt,", "tokens": ["So", "sagt", "sie", "dir", "auch", ",", "ob", "du", "et\u00b7was", "recht", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "$,", "KOUS", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht im verschlungnen Pfad des Irrthums dich verrannt;", "tokens": ["Nicht", "im", "ver\u00b7schlung\u00b7nen", "Pfad", "des", "Irr\u00b7thums", "dich", "ver\u00b7rannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "ADJA", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sie sagt dir auch, ob du der rechten Kunst gewaltet,", "tokens": ["Sie", "sagt", "dir", "auch", ",", "ob", "du", "der", "rech\u00b7ten", "Kunst", "ge\u00b7wal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Gutes, Wahres klar in sch\u00f6ner Form gestaltet.", "tokens": ["Ein", "Gu\u00b7tes", ",", "Wah\u00b7res", "klar", "in", "sch\u00f6\u00b7ner", "Form", "ge\u00b7stal\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "ADJD", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Den h\u00f6chsten Beifall, den du deinem Handeln, Wissen", "tokens": ["Den", "h\u00f6chs\u00b7ten", "Bei\u00b7fall", ",", "den", "du", "dei\u00b7nem", "Han\u00b7deln", ",", "Wis\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Bilden selber gibst, nie m\u00f6gest du ihn missen.", "tokens": ["Und", "Bil\u00b7den", "sel\u00b7ber", "gibst", ",", "nie", "m\u00f6\u00b7gest", "du", "ihn", "mis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVFIN", "$,", "ADV", "VMFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}