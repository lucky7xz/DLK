{"dta.poem.7706": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "78.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Stets sah ich einen Mann, nicht wu\u00dft' ich wie er hie\u00df;", "tokens": ["Stets", "sah", "ich", "ei\u00b7nen", "Mann", ",", "nicht", "wu\u00dft'", "ich", "wie", "er", "hie\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "PTKNEG", "VVFIN", "PPER", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ich erfragen wollt', und immer unterlie\u00df.", "tokens": ["Was", "ich", "er\u00b7fra\u00b7gen", "wollt'", ",", "und", "im\u00b7mer", "un\u00b7ter\u00b7lie\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "VMFIN", "$,", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Auch einen Namen h\u00f6rt' ich nennen oft genug,", "tokens": ["Auch", "ei\u00b7nen", "Na\u00b7men", "h\u00f6rt'", "ich", "nen\u00b7nen", "oft", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und konnte nie den Mann erblicken, der ihn trug.", "tokens": ["Und", "konn\u00b7te", "nie", "den", "Mann", "er\u00b7bli\u00b7cken", ",", "der", "ihn", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Neugierig war ich doch, wie ausseh der Genannte,", "tokens": ["Neu\u00b7gie\u00b7rig", "war", "ich", "doch", ",", "wie", "aus\u00b7seh", "der", "Ge\u00b7nann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "$,", "PWAV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Neugierig, wie genannt sei der vom Sehn Bekannte.", "tokens": ["Neu\u00b7gie\u00b7rig", ",", "wie", "ge\u00b7nannt", "sei", "der", "vom", "Sehn", "Be\u00b7kann\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "VVPP", "VAFIN", "ART", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nun find' ich, jener Nam' ist eben dieser Mann,", "tokens": ["Nun", "find'", "ich", ",", "je\u00b7ner", "Nam'", "ist", "e\u00b7ben", "die\u00b7ser", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PDAT", "NN", "VAFIN", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und alle beide gehn mich weiter nichts mehr an.", "tokens": ["Und", "al\u00b7le", "bei\u00b7de", "gehn", "mich", "wei\u00b7ter", "nichts", "mehr", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PIS", "VVFIN", "PPER", "ADV", "PIS", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}