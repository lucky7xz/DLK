{"textgrid.poem.39153": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deines Nackens stolze Beugung", "tokens": ["Dei\u00b7nes", "Na\u00b7ckens", "stol\u00b7ze", "Beu\u00b7gung"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seh' ich wei\u00df, als Fels sich dehnen,", "tokens": ["Seh'", "ich", "wei\u00df", ",", "als", "Fels", "sich", "deh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$,", "KOUS", "NN", "PRF", "PDS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dr\u00fcber hin mit Gru\u00df und Neigung", "tokens": ["Dr\u00fc\u00b7ber", "hin", "mit", "Gru\u00df", "und", "Nei\u00b7gung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spielend h\u00fcpfen die Sirenen,", "tokens": ["Spie\u00b7lend", "h\u00fcp\u00b7fen", "die", "Si\u00b7re\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deine Locken, Deine n\u00e4cht'gen,", "tokens": ["Dei\u00b7ne", "Lo\u00b7cken", ",", "Dei\u00b7ne", "n\u00e4cht'\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie sie tanzen, wie sie flattern,", "tokens": ["Wie", "sie", "tan\u00b7zen", ",", "wie", "sie", "flat\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Um in ihren zauberm\u00e4cht'gen", "tokens": ["Um", "in", "ih\u00b7ren", "zau\u00b7ber\u00b7m\u00e4cht'\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ringen Herzen zu ergattern.", "tokens": ["Rin\u00b7gen", "Her\u00b7zen", "zu", "er\u00b7gat\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ob ich an den Mast mich binde,", "tokens": ["Ob", "ich", "an", "den", "Mast", "mich", "bin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie der edle Laertide,", "tokens": ["Wie", "der", "ed\u00b7le", "Laer\u00b7ti\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Es zerrei\u00dft das Taugewinde", "tokens": ["Es", "zer\u00b7rei\u00dft", "das", "Tau\u00b7ge\u00b7win\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sehnsucht mir, die Eumenide;", "tokens": ["Sehn\u00b7sucht", "mir", ",", "die", "Eu\u00b7me\u00b7ni\u00b7de", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob ich Aug' und Ohr vergittert,", "tokens": ["Ob", "ich", "Aug'", "und", "Ohr", "ver\u00b7git\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tobend will sie sich emp\u00f6ren,", "tokens": ["To\u00b7bend", "will", "sie", "sich", "em\u00b7p\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bis das Band ich selbst zersplittert,", "tokens": ["Bis", "das", "Band", "ich", "selbst", "zer\u00b7split\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lauschen mu\u00df den Feench\u00f6ren.", "tokens": ["Lau\u00b7schen", "mu\u00df", "den", "Feenc\u00b7h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Und die Klippe zu umranken,", "tokens": ["Und", "die", "Klip\u00b7pe", "zu", "um\u00b7ran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie zu k\u00fcssen, hei\u00df zu pressen,", "tokens": ["Sie", "zu", "k\u00fcs\u00b7sen", ",", "hei\u00df", "zu", "pres\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rei\u00dft mich's auf mit Glutgedanken,", "tokens": ["Rei\u00dft", "mich's", "auf", "mit", "Glut\u00b7ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wollustatmend Tod vergessen,", "tokens": ["Wol\u00b7lus\u00b7tat\u00b7mend", "Tod", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis in qu\u00e4lender Verkettung", "tokens": ["Bis", "in", "qu\u00e4\u00b7len\u00b7der", "Ver\u00b7ket\u00b7tung"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mich umklammert die Sirene", "tokens": ["Mich", "um\u00b7klam\u00b7mert", "die", "Si\u00b7re\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und auf harter Felsenbettung", "tokens": ["Und", "auf", "har\u00b7ter", "Fel\u00b7sen\u00b7bet\u00b7tung"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "\u00c4chzt der Sch\u00fctzling der Athene.", "tokens": ["\u00c4chzt", "der", "Sch\u00fctz\u00b7ling", "der", "A\u00b7the\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}