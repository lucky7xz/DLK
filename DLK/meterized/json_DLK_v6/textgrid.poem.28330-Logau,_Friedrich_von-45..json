{"textgrid.poem.28330": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "45.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kitzligunda Jungferschafft wolt ihr einmal dampffig werden,", "tokens": ["Kitz\u00b7li\u00b7gun\u00b7da", "Jung\u00b7fer\u00b7schafft", "wolt", "ihr", "ein\u00b7mal", "dampf\u00b7fig", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Weil sie nie kam in die Lufft, blieb nur immer bey der Erden;", "tokens": ["Weil", "sie", "nie", "kam", "in", "die", "Lufft", ",", "blieb", "nur", "im\u00b7mer", "bey", "der", "Er\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Drum so hat an einen Nagel sie sie neulich auffgehenckt,", "tokens": ["Drum", "so", "hat", "an", "ei\u00b7nen", "Na\u00b7gel", "sie", "sie", "neu\u00b7lich", "auff\u00b7ge\u00b7henckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VAFIN", "APPR", "ART", "NE", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Klagt nur, da\u00df so viel sie N\u00e4gel nicht kan haben, als sie denckt.", "tokens": ["Klagt", "nur", ",", "da\u00df", "so", "viel", "sie", "N\u00e4\u00b7gel", "nicht", "kan", "ha\u00b7ben", ",", "als", "sie", "denckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "ADV", "ADV", "PPER", "NN", "PTKNEG", "VMFIN", "VAINF", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}