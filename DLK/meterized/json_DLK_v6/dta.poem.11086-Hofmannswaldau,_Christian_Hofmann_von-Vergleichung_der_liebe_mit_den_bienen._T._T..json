{"dta.poem.11086": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vergleichung der liebe mit den bienen.  \n  T. T.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die bienen sind zwar klein;", "tokens": ["Die", "bie\u00b7nen", "sind", "zwar", "klein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Jhr kleiner bi\u00df macht dennoch grosse pein:", "tokens": ["Ihr", "klei\u00b7ner", "bi\u00df", "macht", "den\u00b7noch", "gros\u00b7se", "pein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "KON", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.3": {"text": "Und was ist kleiner als die liebe?", "tokens": ["Und", "was", "ist", "klei\u00b7ner", "als", "die", "lie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "ADJD", "KOKOM", "ART", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie geht durch enge strassen ein,", "tokens": ["Sie", "geht", "durch", "en\u00b7ge", "stras\u00b7sen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und kan in engem auch verborgen seyn.", "tokens": ["Und", "kan", "in", "en\u00b7gem", "auch", "ver\u00b7bor\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "ADJA", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sehr offt verhalten sich derselben scharffe triebe", "tokens": ["Sehr", "offt", "ver\u00b7hal\u00b7ten", "sich", "der\u00b7sel\u00b7ben", "scharf\u00b7fe", "trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "PDAT", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Jm schatten derer augenlieder,", "tokens": ["Jm", "schat\u00b7ten", "de\u00b7rer", "au\u00b7gen\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVFIN", "PDS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bald in den sch\u00f6nen haaren wieder,", "tokens": ["Bald", "in", "den", "sch\u00f6\u00b7nen", "haa\u00b7ren", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Bald in den gr\u00fcbchen, die das wange macht,", "tokens": ["Bald", "in", "den", "gr\u00fcb\u00b7chen", ",", "die", "das", "wan\u00b7ge", "macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wenn dieses freundlich lacht;", "tokens": ["Wenn", "die\u00b7ses", "freund\u00b7lich", "lacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Und dennoch macht sie auch so gro\u00df\u2019 und arge wunden,", "tokens": ["Und", "den\u00b7noch", "macht", "sie", "auch", "so", "gro\u00df'", "und", "ar\u00b7ge", "wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "KON", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Vor die kein mensch noch nicht ein mittel ausgefunden.", "tokens": ["Vor", "die", "kein", "mensch", "noch", "nicht", "ein", "mit\u00b7tel", "aus\u00b7ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "ADV", "PTKNEG", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}