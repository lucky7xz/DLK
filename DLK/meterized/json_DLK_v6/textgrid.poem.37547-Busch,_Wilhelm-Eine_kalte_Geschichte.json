{"textgrid.poem.37547": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Eine kalte Geschichte", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Wind der weht, die Nacht ist k\u00fchl.", "tokens": ["Der", "Wind", "der", "weht", ",", "die", "Nacht", "ist", "k\u00fchl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach Hause wandelt Meister Zwiel.", "tokens": ["Nach", "Hau\u00b7se", "wan\u00b7delt", "Meis\u00b7ter", "Zwiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Verst\u00e4ndig, wie das seine Art,", "tokens": ["Ver\u00b7st\u00e4n\u00b7dig", ",", "wie", "das", "sei\u00b7ne", "Art", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "PDS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat er den Schl\u00fcssel aufbewahrt.", "tokens": ["Hat", "er", "den", "Schl\u00fcs\u00b7sel", "auf\u00b7be\u00b7wahrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das Schl\u00fcsselloch wird leicht vermi\u00dft,", "tokens": ["Das", "Schl\u00fcs\u00b7sel\u00b7loch", "wird", "leicht", "ver\u00b7mi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn man es sucht, wo es nicht ist.", "tokens": ["Wenn", "man", "es", "sucht", ",", "wo", "es", "nicht", "ist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "$,", "PWAV", "PPER", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Allm\u00e4hlich schneit es auch ein bissel;", "tokens": ["All\u00b7m\u00e4h\u00b7lich", "schneit", "es", "auch", "ein", "bis\u00b7sel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der kalten Hand entf\u00e4llt der Schl\u00fcssel.", "tokens": ["Der", "kal\u00b7ten", "Hand", "ent\u00b7f\u00e4llt", "der", "Schl\u00fcs\u00b7sel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Beschwerlich ist die B\u00fcckerei;", "tokens": ["Be\u00b7schwer\u00b7lich", "ist", "die", "B\u00fc\u00b7cke\u00b7rei", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es l\u00fcftet sich der Hut dabei.", "tokens": ["Es", "l\u00fcf\u00b7tet", "sich", "der", "Hut", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Hut ist na\u00df und \u00e4u\u00dferst kalt;", "tokens": ["Der", "Hut", "ist", "na\u00df", "und", "\u00e4u\u00b7\u00dferst", "kalt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn das so fortgeht, friert es bald.", "tokens": ["Wenn", "das", "so", "fort\u00b7geht", ",", "friert", "es", "bald", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADV", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Noch einmal b\u00fcckt der Meister sich,", "tokens": ["Noch", "ein\u00b7mal", "b\u00fcckt", "der", "Meis\u00b7ter", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch nicht geschickt erweist er sich.", "tokens": ["Doch", "nicht", "ge\u00b7schickt", "er\u00b7weist", "er", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVPP", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Das Wasser in dem Fasse hier", "tokens": ["Das", "Was\u00b7ser", "in", "dem", "Fas\u00b7se", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat etwa null Grad Reaumur.", "tokens": ["Hat", "et\u00b7wa", "null", "Grad", "Re\u00b7au\u00b7mur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Es bilden sich in diesem Falle", "tokens": ["Es", "bil\u00b7den", "sich", "in", "die\u00b7sem", "Fal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die sogenannten Eiskristalle.", "tokens": ["Die", "so\u00b7ge\u00b7nann\u00b7ten", "Eis\u00b7kris\u00b7tal\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Der W\u00e4chter singt: Bewahrt das Licht!", "tokens": ["Der", "W\u00e4ch\u00b7ter", "singt", ":", "Be\u00b7wahrt", "das", "Licht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$.", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der kalte Meister h\u00f6rt es nicht.", "tokens": ["Der", "kal\u00b7te", "Meis\u00b7ter", "h\u00f6rt", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Er sitzt gef\u00fchllos, starr und stumm;", "tokens": ["Er", "sitzt", "ge\u00b7f\u00fchl\u00b7los", ",", "starr", "und", "stumm", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Schnee f\u00e4llt drauf und drum herum.", "tokens": ["Der", "Schnee", "f\u00e4llt", "drauf", "und", "drum", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Der Morgen kommt so tr\u00fcb und grau;", "tokens": ["Der", "Mor\u00b7gen", "kommt", "so", "tr\u00fcb", "und", "grau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Frau Pieter kommt, die Millichfrau;", "tokens": ["Frau", "Pie\u00b7ter", "kommt", ",", "die", "Mil\u00b7lich\u00b7frau", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Auch kommt sogleich mit ihrem Topf", "tokens": ["Auch", "kommt", "sog\u00b7leich", "mit", "ih\u00b7rem", "Topf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Frau Zwiel heraus und neigt den Kopf.", "tokens": ["Frau", "Zwiel", "he\u00b7raus", "und", "neigt", "den", "Kopf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbschau schau!\u00ab ruft sie, in Schmerz versunken,", "tokens": ["\u00bb", "schau", "schau", "!", "\u00ab", "ruft", "sie", ",", "in", "Schmerz", "ver\u00b7sun\u00b7ken", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "PTKVZ", "$.", "$(", "VVFIN", "PPER", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbmein guter Zwiel hat ausgetrunken!", "tokens": ["\u00bb", "mein", "gu\u00b7ter", "Zwiel", "hat", "aus\u00b7ge\u00b7trun\u00b7ken", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Von nun an, liebe Madam Pieter,", "tokens": ["Von", "nun", "an", ",", "lie\u00b7be", "Ma\u00b7dam", "Pie\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PTKVZ", "$,", "VVFIN", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bitt ich nur um ein Viertel Liter!\u00ab", "tokens": ["Bitt", "ich", "nur", "um", "ein", "Vier\u00b7tel", "Li\u00b7ter", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}