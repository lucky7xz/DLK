{"textgrid.poem.63761": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du wei\u00dft es wohl, ich lebe nicht mehr gerne,", "tokens": ["Du", "wei\u00dft", "es", "wohl", ",", "ich", "le\u00b7be", "nicht", "mehr", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PTKNEG", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da Jahr um Jahr so herbe Schl\u00e4ge brachten,", "tokens": ["Da", "Jahr", "um", "Jahr", "so", "her\u00b7be", "Schl\u00e4\u00b7ge", "brach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die wohl auch h\u00e4rtre Schultern m\u00fcrbe machten,", "tokens": ["Die", "wohl", "auch", "h\u00e4r\u00b7tre", "Schul\u00b7tern", "m\u00fcr\u00b7be", "mach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJA", "NN", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ich das Leben bitter fand im Kerne.", "tokens": ["Und", "ich", "das", "Le\u00b7ben", "bit\u00b7ter", "fand", "im", "Ker\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "ADJD", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nichts mehr erquickt mich, was ich schaff' und lerne.", "tokens": ["Nichts", "mehr", "er\u00b7quickt", "mich", ",", "was", "ich", "schaff'", "und", "ler\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich wei\u00df, nur wenig lohnt's, nach Wahrheit trachten,", "tokens": ["Ich", "wei\u00df", ",", "nur", "we\u00b7nig", "lohnt's", ",", "nach", "Wahr\u00b7heit", "trach\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "PIS", "VVFIN", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und jenes Laub, wonach Poeten schmachten,", "tokens": ["Und", "je\u00b7nes", "Laub", ",", "wo\u00b7nach", "Po\u00b7et\u00b7en", "schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "PWAV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "H\u00e4lt nicht den Blitz von Menschenh\u00e4uptern ferne.", "tokens": ["H\u00e4lt", "nicht", "den", "Blitz", "von", "Men\u00b7schen\u00b7h\u00e4up\u00b7tern", "fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "APPR", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und doch, ob ich allein nach Ruhe strebe \u2013", "tokens": ["Und", "doch", ",", "ob", "ich", "al\u00b7lein", "nach", "Ru\u00b7he", "stre\u00b7be", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Sonnenzauber dieser Stadt umgl\u00e4nzt,", "tokens": ["Vom", "Son\u00b7nen\u00b7zau\u00b7ber", "die\u00b7ser", "Stadt", "um\u00b7gl\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gesteh' ich's nur: hier atmen lohnt der M\u00fche.", "tokens": ["Ge\u00b7steh'", "ich's", "nur", ":", "hier", "at\u00b7men", "lohnt", "der", "M\u00fc\u00b7he", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "$.", "ADV", "VVINF", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie gr\u00fc\u00dft den m\u00fcden Ringer gleich der Hebe,", "tokens": ["Sie", "gr\u00fc\u00dft", "den", "m\u00fc\u00b7den", "Rin\u00b7ger", "gleich", "der", "He\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ew'ger Jugend Nektar ihm kredenzt,", "tokens": ["Die", "ew'\u00b7ger", "Ju\u00b7gend", "Nek\u00b7tar", "ihm", "kre\u00b7denzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da\u00df neues Sein im Jenseits ihm erbl\u00fche.", "tokens": ["Da\u00df", "neu\u00b7es", "Sein", "im", "Jen\u00b7seits", "ihm", "er\u00b7bl\u00fc\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}