{"textgrid.poem.32548": {"metadata": {"author": {"name": "Scheffner, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die vorsichtige Agnes.", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein M\u00e4dchen, das sich kaum an Amors langem Seile", "tokens": ["Ein", "M\u00e4d\u00b7chen", ",", "das", "sich", "kaum", "an", "A\u00b7mors", "lan\u00b7gem", "Sei\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADV", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In seiner Reitbahn ausgetrabt,", "tokens": ["In", "sei\u00b7ner", "Reit\u00b7bahn", "aus\u00b7ge\u00b7trabt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das niemals was von seinem Pfeile", "tokens": ["Das", "nie\u00b7mals", "was", "von", "sei\u00b7nem", "Pfei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "PWS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gesehn, viel weniger je ", "tokens": ["Ge\u00b7sehn", ",", "viel", "we\u00b7ni\u00b7ger", "je"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "ADV", "ADV", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Ein M\u00e4dchen das also ganz sonnenklar", "tokens": ["Ein", "M\u00e4d\u00b7chen", "das", "al\u00b7so", "ganz", "son\u00b7nen\u00b7klar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADV", "ADV", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Noch lauter liebe Unschuld war,", "tokens": ["Noch", "lau\u00b7ter", "lie\u00b7be", "Un\u00b7schuld", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Trat vor den Richterstuhl und klagte:", "tokens": ["Trat", "vor", "den", "Rich\u00b7ter\u00b7stuhl", "und", "klag\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbherr Richter, hub sie seufzend an,", "tokens": ["\u00bb", "herr", "Rich\u00b7ter", ",", "hub", "sie", "seuf\u00b7zend", "an", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NN", "$,", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Hier dieser ehrverge\u00dfne Mann,", "tokens": ["Hier", "die\u00b7ser", "ehr\u00b7ver\u00b7ge\u00df\u00b7ne", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der mich bisher umsonst mit Schmeicheleyen plagte,", "tokens": ["Der", "mich", "bis\u00b7her", "um\u00b7sonst", "mit", "Schmei\u00b7che\u00b7le\u00b7yen", "plag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Hat endlich mir das mit Gewalt geraubt,", "tokens": ["Hat", "end\u00b7lich", "mir", "das", "mit", "Ge\u00b7walt", "ge\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "PDS", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Was ich ihm auf sein Fleh'n auch nicht um Gold erlaubt'\u00ab", "tokens": ["Was", "ich", "ihm", "auf", "sein", "Fleh'n", "auch", "nicht", "um", "Gold", "er\u00b7laubt'", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "APPR", "PPOSAT", "NN", "ADV", "PTKNEG", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Beklagter frug wie sie ihm das beweisen wollte?", "tokens": ["Be\u00b7klag\u00b7ter", "frug", "wie", "sie", "ihm", "das", "be\u00b7wei\u00b7sen", "woll\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KOKOM", "PPER", "PPER", "PDS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sie h\u00e4tte ja mit eigner Hand", "tokens": ["Sie", "h\u00e4t\u00b7te", "ja", "mit", "eig\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Den Dolch just nach dem ", "tokens": ["Den", "Dolch", "just", "nach", "dem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Damit er nur nicht fehlen sollte.", "tokens": ["Da\u00b7mit", "er", "nur", "nicht", "feh\u00b7len", "soll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "\u00bbschon recht, ward ihm von Agnes replicirt", "tokens": ["\u00bb", "schon", "recht", ",", "ward", "ihm", "von", "Ag\u00b7nes", "re\u00b7pli\u00b7cirt"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "ADJD", "$,", "VAFIN", "PPER", "APPR", "NE", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "\u00bbschon recht, ich hab ihn auch dahin gef\u00fchrt,", "tokens": ["\u00bb", "schon", "recht", ",", "ich", "hab", "ihn", "auch", "da\u00b7hin", "ge\u00b7f\u00fchrt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "$,", "PPER", "VAFIN", "PPER", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Allein war wohl ein ander Rath zu fassen?", "tokens": ["Al\u00b7lein", "war", "wohl", "ein", "an\u00b7der", "Rath", "zu", "fas\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Du stie\u00dfest ja mit solcher Wut und Eil", "tokens": ["Du", "stie\u00b7\u00dfest", "ja", "mit", "sol\u00b7cher", "Wut", "und", "Eil"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Mahl \u00fcber mahl auf mich, da\u00df ich zu meinem Heil", "tokens": ["Mahl", "\u00fc\u00b7ber", "mahl", "auf", "mich", ",", "da\u00df", "ich", "zu", "mei\u00b7nem", "Heil"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADV", "APPR", "PPER", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Kein andres Rettungsmittel sah,", "tokens": ["Kein", "and\u00b7res", "Ret\u00b7tungs\u00b7mit\u00b7tel", "sah", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Als ihn der alten Wunde einzupa\u00dfen,", "tokens": ["Als", "ihn", "der", "al\u00b7ten", "Wun\u00b7de", "ein\u00b7zu\u00b7pa\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Denn, dacht ich, die ist einmal da,", "tokens": ["Denn", ",", "dacht", "ich", ",", "die", "ist", "ein\u00b7mal", "da", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "$,", "PRELS", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Und sollst du dir jetzt eine frische machen lassen?\u00ab", "tokens": ["Und", "sollst", "du", "dir", "jetzt", "ei\u00b7ne", "fri\u00b7sche", "ma\u00b7chen", "las\u00b7sen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "ADV", "ART", "ADJA", "VVINF", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}