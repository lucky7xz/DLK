{"textgrid.poem.65928": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Die G\u00f6ttin im Garten", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Begegne der G\u00f6ttin der Liebe im Garten!", "tokens": ["Be\u00b7geg\u00b7ne", "der", "G\u00f6t\u00b7tin", "der", "Lie\u00b7be", "im", "Gar\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sie sei dir das sonniglich sorgsame Weib!", "tokens": ["Sie", "sei", "dir", "das", "son\u00b7nig\u00b7lich", "sorg\u00b7sa\u00b7me", "Weib", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie wolkte vom Meere \u2013 verh\u00fcllt war der Leib \u2013", "tokens": ["Sie", "wolk\u00b7te", "vom", "Mee\u00b7re", "\u2013", "ver\u00b7h\u00fcllt", "war", "der", "Leib", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "$(", "VVPP", "VAFIN", "ART", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und trat zu den Pflanzen, die bang ihrer harrten.", "tokens": ["Und", "trat", "zu", "den", "Pflan\u00b7zen", ",", "die", "bang", "ih\u00b7rer", "harr\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "PPOSAT", "ADJA", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Nun hegt sie uns treu, nach fremdhaften Arten:", "tokens": ["Nun", "hegt", "sie", "uns", "treu", ",", "nach", "fremd\u00b7haf\u00b7ten", "Ar\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Seid freudig und fromm, da\u00df lang sie verbleib,", "tokens": ["Seid", "freu\u00b7dig", "und", "fromm", ",", "da\u00df", "lang", "sie", "ver\u00b7bleib", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KON", "ADJD", "$,", "KOUS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Feinde der Leute beim Dorfe vertreib,", "tokens": ["Die", "Fein\u00b7de", "der", "Leu\u00b7te", "beim", "Dor\u00b7fe", "ver\u00b7treib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wir sollten sie morgens, im Taublau, erwarten!", "tokens": ["Wir", "soll\u00b7ten", "sie", "mor\u00b7gens", ",", "im", "Taub\u00b7lau", ",", "er\u00b7war\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "$,", "APPRART", "NN", "$,", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Verehrt sie und haltet im H\u00fchnerhof Tauben;", "tokens": ["Ver\u00b7ehrt", "sie", "und", "hal\u00b7tet", "im", "H\u00fch\u00b7ner\u00b7hof", "Tau\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "APPRART", "NN", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Auch Schildkr\u00f6ten liebt ihre Sachtheit im Gr\u00fcn;", "tokens": ["Auch", "Schild\u00b7kr\u00f6\u00b7ten", "liebt", "ih\u00b7re", "Sacht\u00b7heit", "im", "Gr\u00fcn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Gen\u00fcgsamkeit lobt, wenn sich B\u00fcsche belauben!", "tokens": ["Ge\u00b7n\u00fcg\u00b7sam\u00b7keit", "lobt", ",", "wenn", "sich", "B\u00fc\u00b7sche", "be\u00b7lau\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KOUS", "PRF", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "O w\u00fc\u00dftet ihr, wie sie euch hilft beim Bem\u00fchn,", "tokens": ["O", "w\u00fc\u00df\u00b7tet", "ihr", ",", "wie", "sie", "euch", "hilft", "beim", "Be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-----+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "So w\u00fcrden wir f\u00fcrstliche Obstopfer klauben!", "tokens": ["So", "w\u00fcr\u00b7den", "wir", "f\u00fcrst\u00b7li\u00b7che", "O\u00b7bstop\u00b7fer", "klau\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie bleibt uns ja nah, bis die Trauben ergl\u00fchn.", "tokens": ["Sie", "bleibt", "uns", "ja", "nah", ",", "bis", "die", "Trau\u00b7ben", "er\u00b7gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,", "KOUS", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}