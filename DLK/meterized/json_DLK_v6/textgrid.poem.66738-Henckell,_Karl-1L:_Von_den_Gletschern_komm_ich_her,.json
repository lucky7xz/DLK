{"textgrid.poem.66738": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von den Gletschern komm ich her,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von den Gletschern komm ich her,", "tokens": ["Von", "den", "Glet\u00b7schern", "komm", "ich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Von den wei\u00dfen Bergen,", "tokens": ["Von", "den", "wei\u00b7\u00dfen", "Ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "\u00dcber Tal und Schlucht daher", "tokens": ["\u00dc\u00b7ber", "Tal", "und", "Schlucht", "da\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "PAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von ungef\u00e4hr ...", "tokens": ["Von", "un\u00b7ge\u00b7f\u00e4hr", "..."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "ADJD", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wo die Freude halmesleicht", "tokens": ["Wo", "die", "Freu\u00b7de", "hal\u00b7mes\u00b7leicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihren Reigen f\u00fchret,", "tokens": ["Ih\u00b7ren", "Rei\u00b7gen", "f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Meine eisige Klinge streicht", "tokens": ["Mei\u00b7ne", "ei\u00b7si\u00b7ge", 