{"textgrid.poem.28225": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "40.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Honig ist der Schlaf; als diesen Honig a\u00df,", "tokens": ["Ein", "Ho\u00b7nig", "ist", "der", "Schlaf", ";", "als", "die\u00b7sen", "Ho\u00b7nig", "a\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$.", "KOUS", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geschahs, da\u00df was, ich gl\u00e4ub ein Bienlein etwa, sa\u00df", "tokens": ["Ge\u00b7schahs", ",", "da\u00df", "was", ",", "ich", "gl\u00e4ub", "ein", "Bien\u00b7lein", "et\u00b7wa", ",", "sa\u00df"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "KOUS", "PIS", "$,", "PPER", "VVFIN", "ART", "NN", "ADV", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auff Libitilla Haut; sie hats nicht achten wollen,", "tokens": ["Auff", "Li\u00b7bi\u00b7til\u00b7la", "Haut", ";", "sie", "hats", "nicht", "ach\u00b7ten", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$.", "PPER", "VAFIN", "PTKNEG", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch, wie man nunmehr merckt, so ist sie sehr zerschwollen.", "tokens": ["Doch", ",", "wie", "man", "nun\u00b7mehr", "merckt", ",", "so", "ist", "sie", "sehr", "zer\u00b7schwol\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "wor