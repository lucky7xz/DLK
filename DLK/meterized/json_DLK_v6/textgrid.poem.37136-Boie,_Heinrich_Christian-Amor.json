{"textgrid.poem.37136": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Amor", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer Amor ist? Er ist ein Kind, Belinde.", "tokens": ["Wer", "A\u00b7mor", "ist", "?", "Er", "ist", "ein", "Kind", ",", "Be\u00b7lin\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "NE", "VAFIN", "$.", "PPER", "VAFIN", "ART", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch unterwerfen diesem Kinde", "tokens": ["Doch", "un\u00b7ter\u00b7wer\u00b7fen", "die\u00b7sem", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der K\u00f6nig und der Sclave sich.", "tokens": ["Der", "K\u00f6\u00b7nig", "und", "der", "Scla\u00b7ve", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm bauen G\u00f6tter selbst Alt\u00e4re,", "tokens": ["Ihm", "bau\u00b7en", "G\u00f6t\u00b7ter", "selbst", "Al\u00b7t\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "An Reizen \u00fcbertrift er dich,", "tokens": ["An", "Rei\u00b7zen", "\u00fc\u00b7bert\u00b7rift", "er", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er denkt wie ich und f\u00fchlt wie ich,", "tokens": ["Er", "denkt", "wie", "ich", "und", "f\u00fchlt", "wie", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "KON", "VVFIN", "KOKOM", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch glaub' ich da\u00df er k\u00fchner w\u00e4re.", "tokens": ["Doch", "glaub'", "ich", "da\u00df", "er", "k\u00fch\u00b7ner", "w\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}