{"textgrid.poem.49431": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "Green", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier hast du Zweige, Bl\u00e4tter, Fr\u00fcchte, Blumenspenden", "tokens": ["Hier", "hast", "du", "Zwei\u00b7ge", ",", "Bl\u00e4t\u00b7ter", ",", "Fr\u00fcch\u00b7te", ",", "Blu\u00b7men\u00b7spen\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und hier mein Herz, es schl\u00e4gt ja einzig dir allein.", "tokens": ["Und", "hier", "mein", "Herz", ",", "es", "schl\u00e4gt", "ja", "ein\u00b7zig", "dir", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "ADV", "ADJD", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zerreiss' es nicht mit deinen feinen, weissen H\u00e4nden:", "tokens": ["Zer\u00b7reiss'", "es", "nicht", "mit", "dei\u00b7nen", "fei\u00b7nen", ",", "weis\u00b7sen", "H\u00e4n\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dir Sch\u00f6nen m\u00f6ge lieb die schlichte Gabe sein.", "tokens": ["Dir", "Sch\u00f6\u00b7nen", "m\u00f6\u00b7ge", "lieb", "die", "schlich\u00b7te", "Ga\u00b7be", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VMFIN", "ADJD", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Noch ganz bedeckt von klarem Tau will ich dich gr\u00fcssen,", "tokens": ["Noch", "ganz", "be\u00b7deckt", "von", "kla\u00b7rem", "Tau", "will", "ich", "dich", "gr\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPR", "ADJA", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der meine Stirn erfrischt im k\u00fchlen Morgenwind.", "tokens": ["Der", "mei\u00b7ne", "Stirn", "er\u00b7frischt", "im", "k\u00fch\u00b7len", "Mor\u00b7gen\u00b7wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Lass den Ermatteten ausruhn zu deinen F\u00fcssen,", "tokens": ["Lass", "den", "Er\u00b7mat\u00b7te\u00b7ten", "aus\u00b7ruhn", "zu", "dei\u00b7nen", "F\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Dass seine M\u00fcdigkeit in sel'gem Traum zerrinnt.", "tokens": ["Dass", "sei\u00b7ne", "M\u00fc\u00b7dig\u00b7keit", "in", "sel'\u00b7gem", "Traum", "zer\u00b7rinnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und lass mein Haupt an deinem jungen Busen liegen,", "tokens": ["Und", "lass", "mein", "Haupt", "an", "dei\u00b7nem", "jun\u00b7gen", "Bu\u00b7sen", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Haupt, das noch von deinen letzten K\u00fcssen bebt;", "tokens": ["Mein", "Haupt", ",", "das", "noch", "von", "dei\u00b7nen", "letz\u00b7ten", "K\u00fcs\u00b7sen", "bebt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mag nach dem freien Sturm mein Herz in Ruh sich wiegen", "tokens": ["Mag", "nach", "dem", "frei\u00b7en", "Sturm", "mein", "Herz", "in", "Ruh", "sich", "wie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "APPR", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und schlummern, da auch dich ein leiser Schlaf umwebt.", "tokens": ["Und", "schlum\u00b7mern", ",", "da", "auch", "dich", "ein", "lei\u00b7ser", "Schlaf", "um\u00b7webt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ADV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}