{"dta.poem.18205": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "7.  \n  So ist's!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Auf diesem sch\u00f6nsten der Planeten", "tokens": ["Auf", "die\u00b7sem", "sch\u00f6ns\u00b7ten", "der", "Pla\u00b7ne\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "VVFIN", "ART", "NN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Erheben furchtbar ihr Geschrei", "tokens": ["Er\u00b7he\u00b7ben", "furcht\u00b7bar", "ihr", "Ge\u00b7schrei"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die theegep\u00e4ppelten Poeten", "tokens": ["Die", "thee\u00b7ge\u00b7p\u00e4p\u00b7pel\u00b7ten", "Po\u00b7et\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Der H\u00f6hern-T\u00f6chterclerisei:", "tokens": ["Der", "H\u00f6\u00b7hern\u00b7T\u00f6ch\u00b7ter\u00b7cle\u00b7ri\u00b7sei", ":"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201eschon wieder Einer, der revoltert,", "tokens": ["\u201e", "schon", "wie\u00b7der", "Ei\u00b7ner", ",", "der", "re\u00b7vol\u00b7tert", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "PIS", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schon wieder Einer, der nur schreit:", "tokens": ["Schon", "wie\u00b7der", "Ei\u00b7ner", ",", "der", "nur", "schreit", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "$,", "PRELS", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Menschheit Herz habt ihr gefoltert,", "tokens": ["Der", "Menschheit", "Herz", "habt", "ihr", "ge\u00b7fol\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ich bin der Geist der neuen Zeit!", "tokens": ["Ich", "bin", "der", "Geist", "der", "neu\u00b7en", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was will der Lump? Was? R\u00e4sonniren?", "tokens": ["Was", "will", "der", "Lump", "?", "Was", "?", "R\u00e4\u00b7son\u00b7ni\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "$.", "PWS", "$.", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Kerl, scheint's, hat den gro\u00dfen Floh!", "tokens": ["Der", "Kerl", ",", "scheint's", ",", "hat", "den", "gro\u00b7\u00dfen", "Floh", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$,", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So jung noch und schon kritisiren!", "tokens": ["So", "jung", "noch", "und", "schon", "kri\u00b7ti\u00b7si\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Hm! Jedenfalls sitzt er im Dalles,", "tokens": ["Hm", "!", "Je\u00b7den\u00b7falls", "sitzt", "er", "im", "Dal\u00b7les", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Doch, Teufel ja, wie dem auch sei!", "tokens": ["Doch", ",", "Teu\u00b7fel", "ja", ",", "wie", "dem", "auch", "sei", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "ADV", "$,", "PWAV", "ART", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir dulden alles, alles, alles,", "tokens": ["Wir", "dul\u00b7den", "al\u00b7les", ",", "al\u00b7les", ",", "al\u00b7les", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "PIS", "$,", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Poesie ist keine Pf\u00fctze,", "tokens": ["Die", "Poe\u00b7sie", "ist", "kei\u00b7ne", "Pf\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie brennt nicht wie ein Lampendocht,", "tokens": ["Sie", "brennt", "nicht", "wie", "ein", "Lam\u00b7pen\u00b7docht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nichts gilt uns ein Kopf voll Gr\u00fctze,", "tokens": ["Und", "nichts", "gilt", "uns", "ein", "Kopf", "voll", "Gr\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sie das Herz nicht ", "tokens": ["Wenn", "sie", "das", "Herz", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "So h\u00f6rt doch auf mit euerm Schelten", "tokens": ["So", "h\u00f6rt", "doch", "auf", "mit", "eu\u00b7erm", "Schel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und schlagt mir nicht die Fenster ein!", "tokens": ["Und", "schlagt", "mir", "nicht", "die", "Fens\u00b7ter", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gewi\u00df, ihr Herrn, ich la\u00df es gelten:", "tokens": ["Ge\u00b7wi\u00df", ",", "ihr", "Herrn", ",", "ich", "la\u00df", "es", "gel\u00b7ten", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Mensch lebt nicht von Brot allein!", "tokens": ["Der", "Mensch", "lebt", "nicht", "von", "Brot", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die Lerchen jubeln noch und klettern", "tokens": ["Die", "Ler\u00b7chen", "ju\u00b7beln", "noch", "und", "klet\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An ihren Liedern in die Luft", "tokens": ["An", "ih\u00b7ren", "Lie\u00b7dern", "in", "die", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dunkle Hochgewitter wettern", "tokens": ["Und", "dunk\u00b7le", "Hoch\u00b7ge\u00b7wit\u00b7ter", "wet\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch n\u00e4chtlich \u00fcber Wald und Kluft.", "tokens": ["Noch", "n\u00e4cht\u00b7lich", "\u00fc\u00b7ber", "Wald", "und", "Kluft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Noch immer bl\u00fcht im Lenz der Flieder,", "tokens": ["Noch", "im\u00b7mer", "bl\u00fcht", "im", "Lenz", "der", "Flie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Sommer duftet der Jasmin,", "tokens": ["Im", "Som\u00b7mer", "duf\u00b7tet", "der", "Jas\u00b7min", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Nachtigall singt ihre Lieder", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "singt", "ih\u00b7re", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und jeder Ton ist ein Blutrubin.", "tokens": ["Und", "je\u00b7der", "Ton", "ist", "ein", "Blu\u00b7tru\u00b7bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Und macht der Herbst dann seine Runde,", "tokens": ["Und", "macht", "der", "Herbst", "dann", "sei\u00b7ne", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Umkreist das Adlerweib den Horst,", "tokens": ["Um\u00b7kreist", "das", "Ad\u00b7ler\u00b7weib", "den", "Horst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann wandert um die Mittagsstunde", "tokens": ["Dann", "wan\u00b7dert", "um", "die", "Mit\u00b7tags\u00b7stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sonne golden durch den Forst.", "tokens": ["Die", "Son\u00b7ne", "gol\u00b7den", "durch", "den", "Forst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Dann lieg ich tr\u00e4umerisch im Grase", "tokens": ["Dann", "lieg", "ich", "tr\u00e4u\u00b7me\u00b7risch", "im", "Gra\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und freu mich, da\u00df die Erde rund,", "tokens": ["Und", "freu", "mich", ",", "da\u00df", "die", "Er\u00b7de", "rund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und oft versetzt mich in Extase", "tokens": ["Und", "oft", "ver\u00b7setzt", "mich", "in", "Ex\u00b7ta\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein hei\u00dfer, rother Frauenmund.", "tokens": ["Ein", "hei\u00b7\u00dfer", ",", "ro\u00b7ther", "Frau\u00b7en\u00b7mund", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Und doch \u2014 o heilige Hippokrene! \u2014", "tokens": ["Und", "doch", "o", "hei\u00b7li\u00b7ge", "Hip\u00b7po\u00b7kre\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "$(", "FM", "ADJA", "NN", "$.", "$("], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wenn ihr das Ding so s\u00fc\u00df bereimt,", "tokens": ["Wenn", "ihr", "das", "Ding", "so", "s\u00fc\u00df", "be\u00b7reimt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Goldschnitt \u201egb.\u201c Notabene", "tokens": ["In", "Gold\u00b7schnitt", "\u201e", "gb", ".", "\u201c", "No\u00b7ta\u00b7be\u00b7ne"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word"], "pos": ["APPR", "NN", "$(", "ADJD", "$.", "$(", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Und roth mit Callico beleimt:", "tokens": ["Und", "roth", "mit", "Cal\u00b7li\u00b7co", "be\u00b7leimt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "F\u00e4llt mir der N\u00fcrenberger Trichter", "tokens": ["F\u00e4llt", "mir", "der", "N\u00fc\u00b7ren\u00b7ber\u00b7ger", "Trich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Geibels sch\u00f6ner Wahrspruch ein:", "tokens": ["Und", "Gei\u00b7bels", "sch\u00f6\u00b7ner", "Wahr\u00b7spruch", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eman kann ein guter lyrischer Dichter", "tokens": ["\u201e", "man", "kann", "ein", "gu\u00b7ter", "ly\u00b7ri\u00b7scher", "Dich\u00b7ter"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PIS", "VMFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und doch ein dummer Teufel sein!\u201c", "tokens": ["Und", "doch", "ein", "dum\u00b7mer", "Teu\u00b7fel", "sein", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}