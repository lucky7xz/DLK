{"dta.poem.18551": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "A n - - - -", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Printzessin! Deren Pracht was irdisch heist erf\u00fcllet!", "tokens": ["Print\u00b7zes\u00b7sin", "!", "De\u00b7ren", "Pracht", "was", "ir\u00b7disch", "heist", "er\u00b7f\u00fcl\u00b7let", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "PWS", "ADJD", "ADJD", "VVFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Printzessin! Die die Welt als Wunder-G\u00f6ttin kennt.", "tokens": ["Print\u00b7zes\u00b7sin", "!", "Die", "die", "Welt", "als", "Wun\u00b7der\u00b7G\u00f6t\u00b7tin", "kennt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ART", "NN", "KOUS", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Printzessin! Deren Glantz die Nacht in Tag verh\u00fcllet/", "tokens": ["Print\u00b7zes\u00b7sin", "!", "De\u00b7ren", "Glantz", "die", "Nacht", "in", "Tag", "ver\u00b7h\u00fcl\u00b7let", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PDS", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Und die der Himmel selbst die andre Sonne nennt!", "tokens": ["Und", "die", "der", "Him\u00b7mel", "selbst", "die", "and\u00b7re", "Son\u00b7ne", "nennt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Will meine Niedrigkeit sich bi\u00df zum Sternen tragen/", "tokens": ["Will", "mei\u00b7ne", "Nied\u00b7rig\u00b7keit", "sich", "bi\u00df", "zum", "Ster\u00b7nen", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PRF", "APPR", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und sucht ein blasser Mond hier deinen g\u00fcldnen Schein:", "tokens": ["Und", "sucht", "ein", "blas\u00b7ser", "Mond", "hier", "dei\u00b7nen", "g\u00fcld\u00b7nen", "Schein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Will sich die Finsterni\u00df zu deinen Lichte wagen/", "tokens": ["Will", "sich", "die", "Fins\u00b7ter\u00b7ni\u00df", "zu", "dei\u00b7nen", "Lich\u00b7te", "wa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So dencke doch/ es mu\u00df bey Sonnen-Schatten seyn.", "tokens": ["So", "den\u00b7cke", "doch", "/", "es", "mu\u00df", "bey", "Son\u00b7nen\u00b7Schat\u00b7ten", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$(", "PPER", "VMFIN", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Banden meiner Furcht hat eine Gluht verzehret/", "tokens": ["Die", "Ban\u00b7den", "mei\u00b7ner", "Furcht", "hat", "ei\u00b7ne", "Gluht", "ver\u00b7zeh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die mir zum Lebens-Port und Grabe leuchten kan.", "tokens": ["Die", "mir", "zum", "Le\u00b7bens\u00b7Port", "und", "Gra\u00b7be", "leuch\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Erz\u00fcrnest du/ da\u00df dich ein schlechter Knecht verehret/", "tokens": ["Er\u00b7z\u00fcr\u00b7nest", "du", "/", "da\u00df", "dich", "ein", "schlech\u00b7ter", "Knecht", "ver\u00b7eh\u00b7ret", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So sage mir nur gleich des Todes Urtheil an.", "tokens": ["So", "sa\u00b7ge", "mir", "nur", "gleich", "des", "To\u00b7des", "Ur\u00b7theil", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Verliebt/ und furchtsam seyn/ ist tadlens wehrt zu sch\u00e4tzen/", "tokens": ["Ver\u00b7liebt", "/", "und", "furcht\u00b7sam", "seyn", "/", "ist", "tad\u00b7lens", "wehrt", "zu", "sch\u00e4t\u00b7zen", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$(", "KON", "ADJD", "VAINF", "$(", "VAFIN", "ADV", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wer seine Noht verschweigt/ verdirbt durch eine Schuld.", "tokens": ["Wer", "sei\u00b7ne", "Noht", "ver\u00b7schweigt", "/", "ver\u00b7dirbt", "durch", "ei\u00b7ne", "Schuld", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVPP", "$(", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es kan mich deine Hand ins Reich der Freuden setzen/", "tokens": ["Es", "kan", "mich", "dei\u00b7ne", "Hand", "ins", "Reich", "der", "Freu\u00b7den", "set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Schl\u00e4gt sie es aber ab/ so sterb' ich mit Gedult.", "tokens": ["Schl\u00e4gt", "sie", "es", "a\u00b7ber", "ab", "/", "so", "sterb'", "ich", "mit", "Ge\u00b7dult", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "PTKVZ", "$(", "ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Allein betrachte nur/ was ich allhier begangen/", "tokens": ["Al\u00b7lein", "be\u00b7trach\u00b7te", "nur", "/", "was", "ich", "all\u00b7hier", "be\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$(", "PWS", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und da\u00df du wundersch\u00f6n/ und ich nur Menschlich bin.", "tokens": ["Und", "da\u00df", "du", "wun\u00b7der\u00b7sch\u00f6n", "/", "und", "ich", "nur", "Menschlich", "bin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "$(", "KON", "PPER", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Erwege/ da\u00df dein Strahl mich zwunge Gluth zu fangen/", "tokens": ["Er\u00b7we\u00b7ge", "/", "da\u00df", "dein", "Strahl", "mich", "zwun\u00b7ge", "Gluth", "zu", "fan\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPOSAT", "NN", "PPER", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Es risse seine Macht mich zu den Flammen hin", "tokens": ["Es", "ris\u00b7se", "sei\u00b7ne", "Macht", "mich", "zu", "den", "Flam\u00b7men", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PPER", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ein kaltes Spiegel-Gla\u00df mu\u00df durch die Sonne brennen/", "tokens": ["Ein", "kal\u00b7tes", "Spie\u00b7gel\u00b7Gla\u00df", "mu\u00df", "durch", "die", "Son\u00b7ne", "bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da dessen Gegenstand dem Freuer nicht entgeht:", "tokens": ["Da", "des\u00b7sen", "Ge\u00b7gen\u00b7stand", "dem", "Freu\u00b7er", "nicht", "ent\u00b7geht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELAT", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und Menschen m\u00fcssen den nur einen Unmensch nennen/", "tokens": ["Und", "Men\u00b7schen", "m\u00fcs\u00b7sen", "den", "nur", "ei\u00b7nen", "Un\u00b7mensch", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "ART", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Der als ein Brenngla\u00df nicht bey deiner Sonnen steht.", "tokens": ["Der", "als", "ein", "Brenn\u00b7gla\u00df", "nicht", "bey", "dei\u00b7ner", "Son\u00b7nen", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "ART", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Zwar sucht ein Menschen Mund bey G\u00f6ttern sich zu weyden/", "tokens": ["Zwar", "sucht", "ein", "Men\u00b7schen", "Mund", "bey", "G\u00f6t\u00b7tern", "sich", "zu", "wey\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "APPR", "NN", "PRF", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Geht mein verliebter Geist auff allzuhoher Bahn:", "tokens": ["Geht", "mein", "ver\u00b7lieb\u00b7ter", "Geist", "auff", "all\u00b7zu\u00b7ho\u00b7her", "Bahn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So kan die Venus doch Adonis K\u00fcsse leiden/", "tokens": ["So", "kan", "die", "Ve\u00b7nus", "doch", "A\u00b7do\u00b7nis", "K\u00fcs\u00b7se", "lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "NE", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und Amor schaut den Stand wie Gauckelpossen an.", "tokens": ["Und", "A\u00b7mor", "schaut", "den", "Stand", "wie", "Gau\u00b7ckel\u00b7pos\u00b7sen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ART", "NN", "KOKOM", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der Liljen weisser Schmuck mu\u00df offt bey Nesseln prangen/", "tokens": ["Der", "Lil\u00b7jen", "weis\u00b7ser", "Schmuck", "mu\u00df", "offt", "bey", "Nes\u00b7seln", "pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VMFIN", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Bey Tulipanen Pracht/ bey sch\u00f6nen tausend-sch\u00f6n", "tokens": ["Bey", "Tu\u00b7li\u00b7pa\u00b7nen", "Pracht", "/", "bey", "sch\u00f6\u00b7nen", "tau\u00b7sen\u00b7dsch\u00f6n"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "$(", "APPR", "ADJA", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Kan wilder Thymian auch einen Platz erlangen/", "tokens": ["Kan", "wil\u00b7der", "Thy\u00b7mi\u00b7an", "auch", "ei\u00b7nen", "Platz", "er\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und Majoran darff wohl bey K\u00e4yser Cronen stehn.", "tokens": ["Und", "Ma\u00b7jo\u00b7ran", "darff", "wohl", "bey", "K\u00e4y\u00b7ser", "Cro\u00b7nen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Aus schlechten D\u00fcnsten macht die Sonne Regenbogen;", "tokens": ["Aus", "schlech\u00b7ten", "D\u00fcns\u00b7ten", "macht", "die", "Son\u00b7ne", "Re\u00b7gen\u00b7bo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Der kostbarste Magnet zieht den geringsten Stahl:", "tokens": ["Der", "kost\u00b7bars\u00b7te", "Mag\u00b7net", "zieht", "den", "ge\u00b7rings\u00b7ten", "Stahl", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Die Liebe bleibt dem Rang und Ehrgeitz ungewogen/", "tokens": ["Die", "Lie\u00b7be", "bleibt", "dem", "Rang", "und", "Ehr\u00b7geitz", "un\u00b7ge\u00b7wo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "KON", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und legt den Reben-Stock \u00fcm einen wilden Pfahl.", "tokens": ["Und", "legt", "den", "Re\u00b7ben\u00b7Stock", "\u00fcm", "ei\u00b7nen", "wil\u00b7den", "Pfahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Sie macht die W\u00fcsteney zu einen Lust-Gefilde/", "tokens": ["Sie", "macht", "die", "W\u00fcs\u00b7te\u00b7ney", "zu", "ei\u00b7nen", "Lust\u00b7Ge\u00b7fil\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und unsern Sclaven-Stand den hohen F\u00fcrsten gleich.", "tokens": ["Und", "un\u00b7sern", "Scla\u00b7ven\u00b7Stand", "den", "ho\u00b7hen", "F\u00fcrs\u00b7ten", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Den Herrn zum Unterthan/ den J\u00e4ger selbst zum Wilde/", "tokens": ["Den", "Herrn", "zum", "Un\u00b7ter\u00b7than", "/", "den", "J\u00e4\u00b7ger", "selbst", "zum", "Wil\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$(", "ART", "NN", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Gesetze gelten nicht in ihren K\u00f6nigreich.", "tokens": ["Ge\u00b7set\u00b7ze", "gel\u00b7ten", "nicht", "in", "ih\u00b7ren", "K\u00f6\u00b7nig\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Was hier die Feder setzt/ sind nur erzwungne Zeilen/", "tokens": ["Was", "hier", "die", "Fe\u00b7der", "setzt", "/", "sind", "nur", "er\u00b7zwung\u00b7ne", "Zei\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "