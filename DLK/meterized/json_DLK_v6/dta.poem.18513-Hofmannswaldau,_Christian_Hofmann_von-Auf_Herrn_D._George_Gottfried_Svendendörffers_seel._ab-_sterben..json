{"dta.poem.18513": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf Herrn  D.  George Gottfried  \n Svendend\u00f6rffers seel. ab-  \n sterben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein beyleid/ theures haupt/ ist gro\u00df und ungemein.", "tokens": ["Mein", "bey\u00b7leid", "/", "theu\u00b7res", "haupt", "/", "ist", "gro\u00df", "und", "un\u00b7ge\u00b7mein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ADJA", "NN", "$(", "VAFIN", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die klagen schweben mir nicht nur auf mund und zungen/", "tokens": ["Die", "kla\u00b7gen", "schwe\u00b7ben", "mir", "nicht", "nur", "auf", "mund", "und", "zun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "NN", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es darff das augen-paar nicht nur mein zeuge seyn/", "tokens": ["Es", "darff", "das", "au\u00b7gen\u00b7paar", "nicht", "nur", "mein", "zeu\u00b7ge", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "ADV", "PPOSAT", "VVFIN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein jammer ist bey mir viel tieffer eingedrungen.", "tokens": ["Dein", "jam\u00b7mer", "ist", "bey", "mir", "viel", "tief\u00b7fer", "ein\u00b7ge\u00b7drun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Denn meine seele selbst hat theil an deinem schmertz/", "tokens": ["Denn", "mei\u00b7ne", "see\u00b7le", "selbst", "hat", "theil", "an", "dei\u00b7nem", "schmertz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VAFIN", "ADJD", "APPR", "PPOSAT", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich f\u00fchle/ wie das hertz mir ungew\u00f6hnlich weinet/", "tokens": ["Ich", "f\u00fch\u00b7le", "/", "wie", "das", "hertz", "mir", "un\u00b7ge\u00b7w\u00f6hn\u00b7lich", "wei\u00b7net", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOKOM", "ART", "NN", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nur darum/ weil ich seh/ da\u00df dir dein vater-hertz", "tokens": ["Nur", "da\u00b7rum", "/", "weil", "ich", "seh", "/", "da\u00df", "dir", "dein", "va\u00b7ter\u00b7hertz"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PAV", "$(", "KOUS", "PPER", "VVFIN", "$(", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Um deines sohnes tod itzt recht zu bluten scheinet.", "tokens": ["Um", "dei\u00b7nes", "soh\u00b7nes", "tod", "itzt", "recht", "zu", "blu\u00b7ten", "schei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nicht frage/ grosser Mann/ was dessen ursach sey/", "tokens": ["Nicht", "fra\u00b7ge", "/", "gros\u00b7ser", "Mann", "/", "was", "des\u00b7sen", "ur\u00b7sach", "sey", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$(", "ADJA", "NN", "$(", "PWS", "PDS", "ADV", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und untersuche nicht den ursprung meiner klagen;", "tokens": ["Und", "un\u00b7ter\u00b7su\u00b7che", "nicht", "den", "ur\u00b7sprung", "mei\u00b7ner", "kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Du weist am besten selbst/ was deine lehrer/ treu", "tokens": ["Du", "weist", "am", "bes\u00b7ten", "selbst", "/", "was", "dei\u00b7ne", "leh\u00b7rer", "/", "treu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PTKA", "ADJD", "ADV", "$(", "PWS", "PPOSAT", "NN", "$(", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und seltne g\u00fcte mir vor wucher eingetragen.", "tokens": ["Und", "selt\u00b7ne", "g\u00fc\u00b7te", "mir", "vor", "wu\u00b7cher", "ein\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "APPR", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da nun/ du Ceder-baum/ nach unsers GOttes schlu\u00df", "tokens": ["Da", "nun", "/", "du", "Ce\u00b7der\u00b7baum", "/", "nach", "un\u00b7sers", "Got\u00b7tes", "schlu\u00df"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$(", "PPER", "NN", "$(", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Auf deinen sch\u00f6nsten zweig ein schweres wetter blitzet/", "tokens": ["Auf", "dei\u00b7nen", "sch\u00f6ns\u00b7ten", "zweig", "ein", "schwe\u00b7res", "wet\u00b7ter", "blit\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Was wunder ist es denn/ da\u00df der erschrecken mu\u00df/", "tokens": ["Was", "wun\u00b7der", "ist", "es", "denn", "/", "da\u00df", "der", "er\u00b7schre\u00b7cken", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "PPER", "ADV", "$(", "KOUS", "ART", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der so geruhiglich in deinem schatten sitzet?", "tokens": ["Der", "so", "ge\u00b7ru\u00b7hig\u00b7lich", "in", "dei\u00b7nem", "schat\u00b7ten", "sit\u00b7zet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "APPR", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich stimme ja mit recht in deine seuffzer ein/", "tokens": ["Ich", "stim\u00b7me", "ja", "mit", "recht", "in", "dei\u00b7ne", "seuff\u00b7zer", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJD", "APPR", "PPOSAT", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ich ruffe billich aus: Du hast zu viel verlohren.", "tokens": ["Ich", "ruf\u00b7fe", "bil\u00b7lich", "aus", ":", "Du", "hast", "zu", "viel", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$.", "PPER", "VAFIN", "PTKA", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Di\u00df war der sohn/ durch den du woltest gl\u00fccklich seyn/", "tokens": ["Di\u00df", "war", "der", "sohn", "/", "durch", "den", "du", "wol\u00b7test", "gl\u00fcck\u00b7lich", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$(", "APPR", "ART", "PPER", "VMFIN", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Jtzt scheint es/ da\u00df er dir zum ungl\u00fcck sey gebohren.", "tokens": ["Jtzt", "scheint", "es", "/", "da\u00df", "er", "dir", "zum", "un\u00b7gl\u00fcck", "sey", "ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PPER", "PPER", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Gewi\u00df/ wer iemahls nur hat deinen sohn gekennt/", "tokens": ["Ge\u00b7wi\u00df", "/", "wer", "ie\u00b7mahls", "nur", "hat", "dei\u00b7nen", "sohn", "ge\u00b7kennt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PWS", "ADV", "ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wem sein gelehrtes pfund nicht unbewust gewesen/", "tokens": ["Wem", "sein", "ge\u00b7lehr\u00b7tes", "pfund", "nicht", "un\u00b7be\u00b7wust", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "PTKNEG", "ADJD", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der hat auch darumb dich bi\u00dfher begl\u00fcckt genennt/", "tokens": ["Der", "hat", "auch", "da\u00b7rumb", "dich", "bi\u00df\u00b7her", "be\u00b7gl\u00fcckt", "ge\u00b7nennt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PAV", "PPER", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Weil du gewohnt von ihm/ nur freuden-frucht zu lesen.", "tokens": ["Weil", "du", "ge\u00b7wohnt", "von", "ihm", "/", "nur", "freu\u00b7den\u00b7frucht", "zu", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "APPR", "PPER", "$(", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die gaben der natur/ der sitten artigkeit/", "tokens": ["Die", "ga\u00b7ben", "der", "na\u00b7tur", "/", "der", "sit\u00b7ten", "ar\u00b7tig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die seltne wissenschafft/ die wohl vollbrachte reisen/", "tokens": ["Die", "selt\u00b7ne", "wis\u00b7sen\u00b7schafft", "/", "die", "wohl", "voll\u00b7brach\u00b7te", "rei\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADV", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der flei\u00df/ den mund und hand der jugend hat geweyht/", "tokens": ["Der", "flei\u00df", "/", "den", "mund", "und", "hand", "der", "ju\u00b7gend", "hat", "ge\u00b7weyht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "KON", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und hundert sachen mehr sind nicht genug zu preisen.", "tokens": ["Und", "hun\u00b7dert", "sa\u00b7chen", "mehr", "sind", "nicht", "ge\u00b7nug", "zu", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "VVFIN", "ADV", "VAFIN", "PTKNEG", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Daher war er bereits mit w\u00fcrden angethan/", "tokens": ["Da\u00b7her", "war", "er", "be\u00b7reits", "mit", "w\u00fcr\u00b7den", "an\u00b7ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "APPR", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der hoffnung gr\u00fcner berg war fast nunmehr erstiegen;", "tokens": ["Der", "hoff\u00b7nung", "gr\u00fc\u00b7ner", "berg", "war", "fast", "nun\u00b7mehr", "er\u00b7stie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Er setzte schon den fu\u00df auff seinen gipffel an;", "tokens": ["Er", "setz\u00b7te", "schon", "den", "fu\u00df", "auff", "sei\u00b7nen", "gipf\u00b7fel", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "PTKVZ", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und mu\u00df doch leider ietzt gest\u00fcrtzt im grunde liegen.", "tokens": ["Und", "mu\u00df", "doch", "lei\u00b7der", "ietzt", "ge\u00b7st\u00fcrtzt", "im", "grun\u00b7de", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "ADV", "VVPP", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Er f\u00e4llt/ da ihn das gl\u00fcck am meisten soll erh\u00f6hn/", "tokens": ["Er", "f\u00e4llt", "/", "da", "ihn", "das", "gl\u00fcck", "am", "meis\u00b7ten", "soll", "er\u00b7h\u00f6hn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "ART", "NN", "APPRART", "PIS", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Er sincket/ da er soll des vaters st\u00fctze werden;", "tokens": ["Er", "sin\u00b7cket", "/", "da", "er", "soll", "des", "va\u00b7ters", "st\u00fct\u00b7ze", "wer\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "VMFIN", "ART", "NN", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und nun die rosen itzt in vollem purpur stehn/", "tokens": ["Und", "nun", "die", "ro\u00b7sen", "itzt", "in", "vol\u00b7lem", "pur\u00b7pur", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "ADV", "APPR", "ADJA", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Begr\u00e4bt sein purpur sich in k\u00fchlen schoo\u00df der erden.", "tokens": ["Be\u00b7gr\u00e4bt", "sein", "pur\u00b7pur", "sich", "in", "k\u00fch\u00b7len", "schoo\u00df", "der", "er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJD", "PRF", "APPR", "ADJA", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Hier w\u00fcnscht\u2019 ich mir nicht mehr/ als da\u00df/ gleich wie du mich", "tokens": ["Hier", "w\u00fcnscht'", "ich", "mir", "nicht", "mehr", "/", "als", "da\u00df", "/", "gleich", "wie", "du", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "ADV", "$(", "KOKOM", "KOUS", "$(", "ADV", "KOKOM", "PPER", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Bey meiner tauffe halffst befreyn von s\u00fcnden-ketten/", "tokens": ["Bey", "mei\u00b7ner", "tauf\u00b7fe", "halffst", "be\u00b7freyn", "von", "s\u00fcn\u00b7den\u00b7ket\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADJD", "APPR", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ich itzt hingegen auch/ hochtheurer g\u00f6nner/ dich", "tokens": ["Ich", "itzt", "hin\u00b7ge\u00b7gen", "auch", "/", "hoch\u00b7theu\u00b7rer", "g\u00f6n\u00b7ner", "/", "dich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "ADV", "ADV", "ADV", "$(", "ADJA", "NN", "$(", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Aus dieser creutzes-fluth durch tr\u00f6sten k\u00f6nte retten;", "tokens": ["Aus", "die\u00b7ser", "creut\u00b7zes\u00b