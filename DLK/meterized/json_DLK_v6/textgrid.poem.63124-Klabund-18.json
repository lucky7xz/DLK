{"textgrid.poem.63124": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "18", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin gemartert von Gewissensbissen,", "tokens": ["Ich", "bin", "ge\u00b7mar\u00b7tert", "von", "Ge\u00b7wis\u00b7sens\u00b7bis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ich noch nichts auf dieser Welt getan.", "tokens": ["Da\u00df", "ich", "noch", "nichts", "auf", "die\u00b7ser", "Welt", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit ein paar Fl\u00fcchen, ein paar M\u00e4dchenk\u00fcssen,", "tokens": ["Mit", "ein", "paar", "Fl\u00fc\u00b7chen", ",", "ein", "paar", "M\u00e4d\u00b7chen\u00b7k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "$,", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da h\u00f6rt es auf, da f\u00e4ngt es an.", "tokens": ["Da", "h\u00f6rt", "es", "auf", ",", "da", "f\u00e4ngt", "es", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich aber f\u00fchle Strom mich unter Fl\u00fcssen,", "tokens": ["Ich", "a\u00b7ber", "f\u00fch\u00b7le", "Strom", "mich", "un\u00b7ter", "Fl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch fl\u00f6sse ich bergauf und himmelan \u2013", "tokens": ["Doch", "fl\u00f6s\u00b7se", "ich", "ber\u00b7gauf", "und", "him\u00b7me\u00b7lan", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "KON", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das Aug, das ich zum guten Werk erhoben,", "tokens": ["Das", "Aug", ",", "das", "ich", "zum", "gu\u00b7ten", "Werk", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Es darf nur einer Dirne Br\u00fcste loben.", "tokens": ["Es", "darf", "nur", "ei\u00b7ner", "Dir\u00b7ne", "Br\u00fcs\u00b7te", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie oft, wenn ich mit den Kumpanen zechte,", "tokens": ["Wie", "oft", ",", "wenn", "ich", "mit", "den", "Kum\u00b7pa\u00b7nen", "zech\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Klang eine Trommel dumpf, die Bu\u00dfe bot.", "tokens": ["Klang", "ei\u00b7ne", "Trom\u00b7mel", "dumpf", ",", "die", "Bu\u00b7\u00dfe", "bot", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ich warf mich hin, auf da\u00df mich einer br\u00e4chte", "tokens": ["Ich", "warf", "mich", "hin", ",", "auf", "da\u00df", "mich", "ei\u00b7ner", "br\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "KOUS", "PPER", "ART", "ADJA"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und stelle einsam mich ins Abendrot.", "tokens": ["Und", "stel\u00b7le", "ein\u00b7sam", "mich", "ins", "A\u00b7ben\u00b7drot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der aber klapperte mit W\u00fcrfeln, und die schlechte", "tokens": ["Der", "a\u00b7ber", "klap\u00b7per\u00b7te", "mit", "W\u00fcr\u00b7feln", ",", "und", "die", "schlech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "APPR", "NN", "$,", "KON", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gesellschaft furcht ich, wenn Gel\u00e4chter droht.", "tokens": ["Ge\u00b7sell\u00b7schaft", "furcht", "ich", ",", "wenn", "Ge\u00b7l\u00e4ch\u00b7ter", "droht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich bin so m\u00fcde meiner Spielerein", "tokens": ["Ich", "bin", "so", "m\u00fc\u00b7de", "mei\u00b7ner", "Spie\u00b7ler\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und m\u00f6chte Mensch einst unter Menschen sein.", "tokens": ["Und", "m\u00f6ch\u00b7te", "Mensch", "einst", "un\u00b7ter", "Men\u00b7schen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "ADV", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch niemand ist, der meinen Worten traute,", "tokens": ["Doch", "nie\u00b7mand", "ist", ",", "der", "mei\u00b7nen", "Wor\u00b7ten", "trau\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es wird mein Leichnam erst auf Lorbeer ruhn.", "tokens": ["Es", "wird", "mein", "Leich\u00b7nam", "erst", "auf", "Lor\u00b7beer", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich rei\u00dfe von der Wand die dunkle Laute,", "tokens": ["Ich", "rei\u00b7\u00dfe", "von", "der", "Wand", "die", "dunk\u00b7le", "Lau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um doch in T\u00f6nen eine Tat zu tun.", "tokens": ["Um", "doch", "in", "T\u00f6\u00b7nen", "ei\u00b7ne", "Tat", "zu", "tun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "APPR", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das Lied ist aus. Der gr\u00fcne Morgen graute.", "tokens": ["Das", "Lied", "ist", "aus", ".", "Der", "gr\u00fc\u00b7ne", "Mor\u00b7gen", "grau\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "$.", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Im Hofe bellt der Hund, es kr\u00e4ht das Huhn;", "tokens": ["Im", "Ho\u00b7fe", "bellt", "der", "Hund", ",", "es", "kr\u00e4ht", "das", "Huhn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und w\u00e4hrend alle rings zum Tag erwachen,", "tokens": ["Und", "w\u00e4h\u00b7rend", "al\u00b7le", "rings", "zum", "Tag", "er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Entschlaf ich trunken unter Wein \u2013 und Lachen.", "tokens": ["Ent\u00b7schlaf", "ich", "trun\u00b7ken", "un\u00b7ter", "Wein", "\u2013", "und", "La\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPR", "NN", "$(", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}