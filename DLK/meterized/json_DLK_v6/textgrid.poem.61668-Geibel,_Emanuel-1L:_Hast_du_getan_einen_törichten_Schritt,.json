{"textgrid.poem.61668": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hast du getan einen t\u00f6richten Schritt,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hast du getan einen t\u00f6richten Schritt,", "tokens": ["Hast", "du", "ge\u00b7tan", "ei\u00b7nen", "t\u00f6\u00b7rich\u00b7ten", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "So tu zur\u00fcck ihn schnelle;", "tokens": ["So", "tu", "zu\u00b7r\u00fcck", "ihn", "schnel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du machst ihn nimmer gut damit,", "tokens": ["Du", "machst", "ihn", "nim\u00b7mer", "gut", "da\u00b7mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df du behauptest die Stelle.", "tokens": ["Da\u00df", "du", "be\u00b7haup\u00b7test", "die", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}