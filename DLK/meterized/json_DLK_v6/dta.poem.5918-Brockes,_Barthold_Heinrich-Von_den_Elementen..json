{"dta.poem.5918": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sind aller C\u00f6rper Staub, der so geschwind", "tokens": ["Sind", "al\u00b7ler", "C\u00f6r\u00b7per", "Staub", ",", "der", "so", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "NN", "$,", "PRELS", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und noch viel schneller sich bewegt und reget,", "tokens": ["Und", "noch", "viel", "schnel\u00b7ler", "sich", "be\u00b7wegt", "und", "re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "PRF", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als selbst diejenige, wodurch er wird beweget.", "tokens": ["Als", "selbst", "die\u00b7je\u00b7ni\u00b7ge", ",", "wo\u00b7durch", "er", "wird", "be\u00b7we\u00b7get", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PDS", "$,", "PWAV", "PPER", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bey dem geringsten Druck nimmt jeder", "tokens": ["Bey", "dem", "ge\u00b7rings\u00b7ten", "Druck", "nimmt", "je\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die kaum verlohrene Bewegung wieder.", "tokens": ["Die", "kaum", "ver\u00b7loh\u00b7re\u00b7ne", "Be\u00b7we\u00b7gung", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Er glitschet, weicht, ist immerfort zugegen,", "tokens": ["Er", "glit\u00b7schet", ",", "weicht", ",", "ist", "im\u00b7mer\u00b7fort", "zu\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Durchdringet alle Ding, und f\u00f6rdert ihr Bewegen.", "tokens": ["Durch\u00b7drin\u00b7get", "al\u00b7le", "Ding", ",", "und", "f\u00f6r\u00b7dert", "ihr", "Be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,", "KON", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dies ist es eigentlich, was man mit Wahrheit nennt", "tokens": ["Dies", "ist", "es", "ei\u00b7gent\u00b7lich", ",", "was", "man", "mit", "Wahr\u00b7heit", "nennt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "$,", "PRELS", "PIS", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das erste Element.", "tokens": ["Das", "ers\u00b7te", "E\u00b7le\u00b7ment", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}