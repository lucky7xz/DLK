{"textgrid.poem.36070": {"metadata": {"author": {"name": "Bechstein, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "XxXI. Die Heimkehr.", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wohl macht Trennung traurig, und wohl ist's bittre Pein,", "tokens": ["Und", "wohl", "macht", "Tren\u00b7nung", "trau\u00b7rig", ",", "und", "wohl", "ist's", "bitt\u00b7re", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "NN", "ADJD", "$,", "KON", "ADV", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Von allem was wir lieben so weit entfernt zu sein,", "tokens": ["Von", "al\u00b7lem", "was", "wir", "lie\u00b7ben", "so", "weit", "ent\u00b7fernt", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PWS", "PPER", "VVFIN", "ADV", "ADJD", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "So weit, dass uns die Treue, der wir vertraut, vergass;", "tokens": ["So", "weit", ",", "dass", "uns", "die", "Treu\u00b7e", ",", "der", "wir", "ver\u00b7traut", ",", "ver\u00b7gass", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ein Freund vom Faustus solches mit bitterm Schmerz ermass.", "tokens": ["Ein", "Freund", "vom", "Faus\u00b7tus", "sol\u00b7ches", "mit", "bit\u00b7term", "Schmerz", "er\u00b7mass", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PIAT", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Dem flossen Tage golden dahin und ungetr\u00fcbt,", "tokens": ["Dem", "flos\u00b7sen", "Ta\u00b7ge", "gol\u00b7den", "da\u00b7hin", "und", "un\u00b7ge\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PAV", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Von einer jungen Gattin recht warm und treu geliebt;", "tokens": ["Von", "ei\u00b7ner", "jun\u00b7gen", "Gat\u00b7tin", "recht", "warm", "und", "treu", "ge\u00b7liebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Doch einer dem zu wohl ist geht tanzen auf das Eis;", "tokens": ["Doch", "ei\u00b7ner", "dem", "zu", "wohl", "ist", "geht", "tan\u00b7zen", "auf", "das", "Eis", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "APPR", "ADV", "VAFIN", "VVFIN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und mancher ist, der Ungl\u00fcck, nicht Gl\u00fcck zu tragen weiss.", "tokens": ["Und", "man\u00b7cher", "ist", ",", "der", "Un\u00b7gl\u00fcck", ",", "nicht", "Gl\u00fcck", "zu", "tra\u00b7gen", "weiss", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "$,", "ART", "NN", "$,", "PTKNEG", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Edle sitzt mit Freunden an froher Tafelrund;", "tokens": ["Der", "Ed\u00b7le", "sitzt", "mit", "Freun\u00b7den", "an", "fro\u00b7her", "Ta\u00b7fel\u00b7rund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sie trinken fremde Weine aus Ungarn und Burgund.", "tokens": ["Sie", "trin\u00b7ken", "frem\u00b7de", "Wei\u00b7ne", "aus", "Un\u00b7garn", "und", "Bur\u00b7gund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und fremde L\u00e4nder preiset ein Jeglicher gar sehr,", "tokens": ["Und", "frem\u00b7de", "L\u00e4n\u00b7der", "prei\u00b7set", "ein", "Jeg\u00b7li\u00b7cher", "gar", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ART", "PIS", "ADV", "ADV", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Jemehr der Becher kreiset, das Reisen um so mehr.", "tokens": ["Je\u00b7mehr", "der", "Be\u00b7cher", "krei\u00b7set", ",", "das", "Rei\u00b7sen", "um", "so", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,", "ART", "NN", "APPR", "ADV", "ADV", "$."], "meter": "---+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "\u00bbo Hellas!\u00ab ruft der Eine: \u00bbgebenedeites Land!", "tokens": ["\u00bb", "o", "Hel\u00b7las", "!", "\u00ab", "ruft", "der", "Ei\u00b7ne", ":", "\u00bb", "ge\u00b7be\u00b7ne\u00b7dei\u00b7tes", "Land", "!"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "$.", "$(", "VVFIN", "ART", "NN", "$.", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "K\u00f6nnt' ich nur Dich erschauen! Der Inseln Blumenstrand,", "tokens": ["K\u00f6nnt'", "ich", "nur", "Dich", "er\u00b7schau\u00b7en", "!", "Der", "In\u00b7seln", "Blu\u00b7men\u00b7strand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "VVINF", "$.", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die St\u00e4tten, uns geheiligt durch Mythen und Gesang,", "tokens": ["Die", "St\u00e4t\u00b7ten", ",", "uns", "ge\u00b7hei\u00b7ligt", "durch", "My\u00b7then", "und", "Ge\u00b7sang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die Schl\u00f6sser, laut umtost einst von eh'rner Waffen Klang!\u00ab", "tokens": ["Die", "Schl\u00f6s\u00b7ser", ",", "laut", "um\u00b7tost", "einst", "von", "eh'r\u00b7ner", "Waf\u00b7fen", "Klang", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVFIN", "ADV", "APPR", "ADJA", "NN", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Ein Andrer ruft, und Freude verkl\u00e4rt sein Antlitz ganz:", "tokens": ["Ein", "A\u00b7ndrer", "ruft", ",", "und", "Freu\u00b7de", "ver\u00b7kl\u00e4rt", "sein", "Ant\u00b7litz", "ganz", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbvor allem w\u00fcrd' ich eilen zum g\u00f6ttlichen Byzanz!", "tokens": ["\u00bb", "vor", "al\u00b7lem", "w\u00fcrd'", "ich", "ei\u00b7len", "zum", "g\u00f6tt\u00b7li\u00b7chen", "By\u00b7zanz", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PIS", "VAFIN", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Des Ostlands heilge Roma; die Meerbraut, reich geschm\u00fcckt,", "tokens": ["Des", "Ost\u00b7lands", "heil\u00b7ge", "Ro\u00b7ma", ";", "die", "Meer\u00b7braut", ",", "reich", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die Asien und Europa an ihren Busen dr\u00fcckt!\u00ab", "tokens": ["Die", "A\u00b7sien", "und", "Eu\u00b7ro\u00b7pa", "an", "ih\u00b7ren", "Bu\u00b7sen", "dr\u00fcckt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "KON", "NE", "APPR", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbauf Br\u00fcder, lasst uns reisen!\u00ab ruft eines Dritten Mund;", "tokens": ["\u00bb", "auf", "Br\u00fc\u00b7der", ",", "lasst", "uns", "rei\u00b7sen", "!", "\u00ab", "ruft", "ei\u00b7nes", "Drit\u00b7ten", "Mund", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "$,", "VVFIN", "PPER", "VVINF", "$.", "$(", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbdas giebt ein k\u00f6stlich Leben! Die H\u00e4nde her zum Bund!", "tokens": ["\u00bb", "das", "giebt", "ein", "k\u00f6st\u00b7lich", "Le\u00b7ben", "!", "Die", "H\u00e4n\u00b7de", "her", "zum", "Bund", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "ART", "ADJD", "NN", "$.", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ein Wort, ein Mann! Wie wollen wir frei und fr\u00f6hlich sein!\u00ab", "tokens": ["Ein", "Wort", ",", "ein", "Mann", "!", "Wie", "wol\u00b7len", "wir", "frei", "und", "fr\u00f6h\u00b7lich", "sein", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$.", "PWAV", "VMFIN", "PPER", "ADJD", "KON", "ADJD", "VAINF", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und alle sind's zufrieden, und alle schlagen ein.", "tokens": ["Und", "al\u00b7le", "sin\u00b7d's", "zu\u00b7frie\u00b7den", ",", "und", "al\u00b7le", "schla\u00b7gen", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADJD", "$,", "KON", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Die Wallfahrt ist beschlossen, nur ", "tokens": ["Die", "Wall\u00b7fahrt", "ist", "be\u00b7schlos\u00b7sen", ",", "nur"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie reut den jungen Gatten sein \u00fcbereilter Eid.", "tokens": ["Wie", "reut", "den", "jun\u00b7gen", "Gat\u00b7ten", "sein", "\u00fc\u00b7be\u00b7reil\u00b7ter", "Eid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Doch ist das Wort gegeben, entbunden wird er nicht,", "tokens": ["Doch", "ist", "das", "Wort", "ge\u00b7ge\u00b7ben", ",", "ent\u00b7bun\u00b7den", "wird", "er", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "VVPP", "$,", "VVPP", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ihm w\u00e4chst aus Freude Kummer, aus Leichtsinn schwere Pflicht. \u2013", "tokens": ["Ihm", "w\u00e4chst", "aus", "Freu\u00b7de", "Kum\u00b7mer", ",", "aus", "Leicht\u00b7sinn", "schwe\u00b7re", "Pflicht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "NN", "$,", "APPR", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Nicht was sie froh getr\u00e4umet, den Reisenden erschien;", "tokens": ["Nicht", "was", "sie", "froh", "ge\u00b7tr\u00e4u\u00b7met", ",", "den", "Rei\u00b7sen\u00b7den", "er\u00b7schien", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PWS", "PPER", "ADJD", "VVPP", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wohl sahen sie ges\u00e4umet manch Inselufer gr\u00fcn,", "tokens": ["Wohl", "sa\u00b7hen", "sie", "ge\u00b7s\u00e4u\u00b7met", "manch", "In\u00b7se\u00b7lu\u00b7fer", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Doch davon, was gelesen sie sonst im V\u00e4terhaus,", "tokens": ["Doch", "da\u00b7von", ",", "was", "ge\u00b7le\u00b7sen", "sie", "sonst", "im", "V\u00e4\u00b7ter\u00b7haus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "$,", "PRELS", "VVPP", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Stand nur das Wort ", "tokens": ["Stand", "nur", "das", "Wort"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Wo freie V\u00f6lker waren begl\u00fcckt und reich und stark,", "tokens": ["Wo", "frei\u00b7e", "V\u00f6l\u00b7ker", "wa\u00b7ren", "be\u00b7gl\u00fcckt", "und", "reich", "und", "stark", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VAFIN", "VVPP", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da saugten jetzt Barbaren der Unterjochten Mark.", "tokens": ["Da", "saug\u00b7ten", "jetzt", "Bar\u00b7ba\u00b7ren", "der", "Un\u00b7ter\u00b7joch\u00b7ten", "Mark", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wo gastlich jede Pforte sonst stand dem Wandrer auf,", "tokens": ["Wo", "gast\u00b7lich", "je\u00b7de", "Pfor\u00b7te", "sonst", "stand", "dem", "Wand\u00b7rer", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PIAT", "NN", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Hemmt jetzt die hohe Pforte die Reisenden im Lauf.", "tokens": ["Hemmt", "jetzt", "die", "ho\u00b7he", "Pfor\u00b7te", "die", "Rei\u00b7sen\u00b7den", "im", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.10": {"line.1": {"text": "In Fesseln fand die Endschaft die ganze Pilgerschaft;", "tokens": ["In", "Fes\u00b7seln", "fand", "die", "End\u00b7schaft", "die", "gan\u00b7ze", "Pil\u00b7ger\u00b7schaft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und mehr als einer wurde vom Tod dahingerafft.", "tokens": ["Und", "mehr", "als", "ei\u00b7ner", "wur\u00b7de", "vom", "Tod", "da\u00b7hin\u00b7ge\u00b7rafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "PIS", "VAFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Zur Heimath flog die Kunde, dass alle Freunde todt,", "tokens": ["Zur", "Hei\u00b7math", "flog", "die", "Kun\u00b7de", ",", "dass", "al\u00b7le", "Freun\u00b7de", "todt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "KOUS", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wie klagt die junge Wittwe, und