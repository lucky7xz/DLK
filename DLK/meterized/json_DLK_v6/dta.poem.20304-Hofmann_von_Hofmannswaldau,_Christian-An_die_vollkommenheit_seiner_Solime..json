{"dta.poem.20304": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "An die vollkommenheit seiner  \n Solime.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die sch\u00f6nheit/ welche dir aus allen gliedern blickt/", "tokens": ["Die", "sch\u00f6n\u00b7heit", "/", "wel\u00b7che", "dir", "aus", "al\u00b7len", "glie\u00b7dern", "blickt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der hals/ dem helffenbein und alabaster weichen/", "tokens": ["Der", "hals", "/", "dem", "helf\u00b7fen\u00b7bein", "und", "a\u00b7la\u00b7bas\u00b7ter", "wei\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "KON", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der mund/ vor welchen selbst der purpur will erbleichen/", "tokens": ["Der", "mund", "/", "vor", "wel\u00b7chen", "selbst", "der", "pur\u00b7pur", "will", "er\u00b7blei\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPR", "PWAT", "ADV", "ART", "ADJD", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die augen/ deren blitz fast alle welt entz\u00fcckt/", "tokens": ["Die", "au\u00b7gen", "/", "de\u00b7ren", "blitz", "fast", "al\u00b7le", "welt", "ent\u00b7z\u00fcckt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PDS", "VVFIN", "ADV", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und deren keusche glut die hertzen fest verstrickt/", "tokens": ["Und", "de\u00b7ren", "keu\u00b7sche", "glut", "die", "hert\u00b7zen", "fest", "ver\u00b7strickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "ADJA", "NN", "ART", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die stirne/ die den glantz der perlen kan erreichen/", "tokens": ["Die", "stir\u00b7ne", "/", "die", "den", "glantz", "der", "per\u00b7len", "kan", "er\u00b7rei\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "ART", "ART", "NN", "ART", "VVINF", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die wangen/ welchen nie kein silber zu vergleichen/", "tokens": ["Die", "wan\u00b7gen", "/", "wel\u00b7chen", "nie", "kein", "sil\u00b7ber", "zu", "ver\u00b7glei\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAT", "ADV", "PIAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In denen lieb und huld ihr bildni\u00df eingedr\u00fcckt;", "tokens": ["In", "de\u00b7nen", "lieb", "und", "huld", "ihr", "bild\u00b7ni\u00df", "ein\u00b7ge\u00b7dr\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADJD", "KON", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die wohlgestalte l\u00e4ng/ das anmuths-volle wesen/", "tokens": ["Die", "wohl\u00b7ge\u00b7stal\u00b7te", "l\u00e4ng", "/", "das", "an\u00b7muths\u00b7vol\u00b7le", "we\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die attlas-weiche hand/ die schnee zuschanden macht/", "tokens": ["Die", "att\u00b7las\u00b7wei\u00b7che", "hand", "/", "die", "schnee", "zu\u00b7schan\u00b7den", "macht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der haare kostbarkeit/ und \u00fcber irrd\u2019sche pracht/", "tokens": ["Der", "haa\u00b7re", "kost\u00b7bar\u00b7keit", "/", "und", "\u00fc\u00b7ber", "irrd'\u00b7sche", "pracht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KON", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und was du sonsten mehr zu deinem schmuck erlesen/", "tokens": ["Und", "was", "du", "sons\u00b7ten", "mehr", "zu", "dei\u00b7nem", "schmuck", "er\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Macht/ da\u00df man dich verehrt vor andern weit und breit/", "tokens": ["Macht", "/", "da\u00df", "man", "dich", "ver\u00b7ehrt", "vor", "an\u00b7dern", "weit", "und", "breit", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PIS", "PRF", "VVPP", "APPR", "PIS", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ein fehler bleibt dir nur/ der ist die grausamkeit.", "tokens": ["Ein", "feh\u00b7ler", "bleibt", "dir", "nur", "/", "der", "ist", "die", "grau\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$(", "ART", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}