{"dta.poem.8035": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "In den abendlichen G\u00e4rten", "tokens": ["In", "den", "a\u00b7bend\u00b7li\u00b7chen", "G\u00e4r\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Ging die Gr\u00e4fin Julia.", "tokens": ["Ging", "die", "Gr\u00e4\u00b7fin", "Ju\u00b7lia", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Fatiman, Almansors Neffe,", "tokens": ["Fa\u00b7ti\u00b7man", ",", "Al\u00b7man\u00b7sors", "Nef\u00b7fe", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Hat die Sch\u00f6ne dort erhascht;", "tokens": ["Hat", "die", "Sch\u00f6\u00b7ne", "dort", "er\u00b7hascht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Flieht mit seiner s\u00fc\u00dfen Beute", "tokens": ["Flieht", "mit", "sei\u00b7ner", "s\u00fc\u00b7\u00dfen", "Beu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch die W\u00e4lder, Nacht und Tag,", "tokens": ["Durch", "die", "W\u00e4l\u00b7der", ",", "Nacht", "und", "Tag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Zehn getreue Mohrenritter", "tokens": ["Zehn", "ge\u00b7treu\u00b7e", "Moh\u00b7ren\u00b7rit\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["CARD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Folgen ihm gewappnet nach.", "tokens": ["Fol\u00b7gen", "ihm", "ge\u00b7wapp\u00b7net", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "In des dritten Morgens Fr\u00fche", "tokens": ["In", "des", "drit\u00b7ten", "Mor\u00b7gens", "Fr\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Kommen sie in jenen Wald,", "tokens": ["Kom\u00b7men", "sie", "in", "je\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Wo auf einem gr\u00fcnen H\u00fcgel", "tokens": ["Wo", "auf", "ei\u00b7nem", "gr\u00fc\u00b7nen", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Sankt Georgs Kapelle ragt.", "tokens": ["Sankt", "Ge\u00b7orgs", "Ka\u00b7pel\u00b7le", "ragt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Schon von Weitem blickt die Gr\u00e4fin", "tokens": ["Schon", "von", "Wei\u00b7tem", "blickt", "die", "Gr\u00e4\u00b7fin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Nach des Heil\u2019gen Bild hinan,", "tokens": ["Nach", "des", "Heil'\u00b7gen", "Bild", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Welches ob der Kirchenpforte,", "tokens": ["Wel\u00b7ches", "ob", "der", "Kir\u00b7ch\u00b7en\u00b7pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "KOUS", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.16": {"text": "Gro\u00df in Stein gehauen, prangt:", "tokens": ["Gro\u00df", "in", "Stein", "ge\u00b7hau\u00b7en", ",", "prangt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVPP", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Wie er in des Lindwurms Rachen", "tokens": ["Wie", "er", "in", "des", "Lind\u00b7wurms", "Ra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "M\u00e4chtig sticht den heil\u2019gen Schaft,", "tokens": ["M\u00e4ch\u00b7tig", "sticht", "den", "heil'\u00b7gen", "Schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "W\u00e4hrend, an den Fels gebunden,", "tokens": ["W\u00e4h\u00b7rend", ",", "an", "den", "Fels", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Bang die K\u00f6nigstochter harrt.", "tokens": ["Bang", "die", "K\u00f6\u00b7nig\u00b7stoch\u00b7ter", "harrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Weinend und die H\u00e4nde ringend,", "tokens": ["Wei\u00b7nend", "und", "die", "H\u00e4n\u00b7de", "rin\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Ruft die Gr\u00e4fin Julia:", "tokens": ["Ruft", "die", "Gr\u00e4\u00b7fin", "Ju\u00b7lia", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NE", "$."], "meter": "+-+---", "measure": "unknown.measure.di"}, "line.23": {"text": "\u201esankt Georg, du heil\u2019ger Streiter,", "tokens": ["\u201e", "sankt", "Ge\u00b7org", ",", "du", "heil'\u00b7ger", "Strei\u00b7ter", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "NE", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Hilf mir aus des Drachen Macht!\u201c", "tokens": ["Hilf", "mir", "aus", "des", "Dra\u00b7chen", "Macht", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PPER", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Siehe! wer auf weissem Rosse", "tokens": ["Sie\u00b7he", "!", "wer", "auf", "weis\u00b7sem", "Ros\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$.", "PWS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Sprengt von der Kapell\u2019 herab?", "tokens": ["Sprengt", "von", "der", "Ka\u00b7pell'", "her\u00b7ab", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Goldne Locken wehn im Winde", "tokens": ["Gold\u00b7ne", "Lo\u00b7cken", "wehn", "im", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Und der rothe Mantel wallt.", "tokens": ["Und", "der", "ro\u00b7the", "Man\u00b7tel", "wallt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "M\u00e4chtig ist sein Speer geschwungen,", "tokens": ["M\u00e4ch\u00b7tig", "ist", "sein", "Speer", "ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Trifft den R\u00e4uber Fatiman,", "tokens": ["Trifft", "den", "R\u00e4u\u00b7ber", "Fa\u00b7ti\u00b7man", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Der sich gleich am Boden kr\u00fcmmet,", "tokens": ["Der", "sich", "gleich", "am", "Bo\u00b7den", "kr\u00fcm\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Wie der Lindwurm einst gethan.", "tokens": ["Wie", "der", "Lind\u00b7wurm", "einst", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Und die zehen Mohrenritter", "tokens": ["Und", "die", "ze\u00b7hen", "Moh\u00b7ren\u00b7rit\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "CARD", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.34": {"text": "Hat ein wilder Schreck gefa\u00dft,", "tokens": ["Hat", "ein", "wil\u00b7der", "Schreck", "ge\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Schild und Lanze weggeworfen,", "tokens": ["Schild", "und", "Lan\u00b7ze", "weg\u00b7ge\u00b7wor\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Fliehn sie \u00fcber Berg und Thal.", "tokens": ["Fliehn", "sie", "\u00fc\u00b7ber", "Berg", "und", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Auf den Knieen, wie geblendet,", "tokens": ["Auf", "den", "Kni\u00b7e\u00b7en", ",", "wie", "ge\u00b7blen\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.38": {"text": "Liegt die Gr\u00e4fin Julia:", "tokens": ["Liegt", "die", "Gr\u00e4\u00b7fin", "Ju\u00b7lia", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NE", "$."], "meter": "+-+---", "measure": "unknown.measure.di"}, "line.39": {"text": "\u201esankt Georg, du heil\u2019ger Streiter,", "tokens": ["\u201e", "sankt", "Ge\u00b7org", ",", "du", "heil'\u00b7ger", "Strei\u00b7ter", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "NE", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Sey gepriesen tausendmal!\u201c", "tokens": ["Sey", "ge\u00b7prie\u00b7sen", "tau\u00b7send\u00b7mal", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "VVPP", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Als sie wieder hebt die Augen,", "tokens": ["Als", "sie", "wie\u00b7der", "hebt", "die", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Ist der Heil\u2019ge nicht mehr da,", "tokens": ["Ist", "der", "Heil'\u00b7ge", "nicht", "mehr", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Und es geht nur dumpfe Sage,", "tokens": ["Und", "es", "geht", "nur", "dump\u00b7fe", "Sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Da\u00df es Paskal Vivas war.", "tokens": ["Da\u00df", "es", "Pas\u00b7kal", "Vi\u00b7vas", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "NE", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}