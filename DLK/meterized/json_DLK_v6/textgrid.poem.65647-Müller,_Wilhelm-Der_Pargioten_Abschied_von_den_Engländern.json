{"textgrid.poem.65647": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Pargioten Abschied von den Engl\u00e4ndern", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Br\u00fcder, la\u00dft und f\u00fcrder ziehen aus dem schn\u00f6den Inselland!", "tokens": ["Br\u00fc\u00b7der", ",", "la\u00dft", "und", "f\u00fcr\u00b7der", "zie\u00b7hen", "aus", "dem", "schn\u00f6\u00b7den", "In\u00b7sel\u00b7land", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "KON", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "La\u00dft uns eilig unsre Segel richten nach dem sch\u00f6nen Strand,", "tokens": ["La\u00dft", "uns", "ei\u00b7lig", "uns\u00b7re", "Se\u00b7gel", "rich\u00b7ten", "nach", "dem", "sch\u00f6\u00b7nen", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wo aus langen schweren Banden Hellas ihre Arme ringt", "tokens": ["Wo", "aus", "lan\u00b7gen", "schwe\u00b7ren", "Ban\u00b7den", "Hel\u00b7las", "ih\u00b7re", "Ar\u00b7me", "ringt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "ADJA", "NN", "NE", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und die kettenwunde Rechte gegen die Tyrannen schwingt.", "tokens": ["Und", "die", "ket\u00b7ten\u00b7wun\u00b7de", "Rech\u00b7te", "ge\u00b7gen", "die", "Ty\u00b7ran\u00b7nen", "schwingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Britten, ohne Dank und Segen scheiden wir aus eurem Schutz,", "tokens": ["Brit\u00b7ten", ",", "oh\u00b7ne", "Dank", "und", "Se\u00b7gen", "schei\u00b7den", "wir", "aus", "eu\u00b7rem", "Schutz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "NN", "KON", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "W\u00e4hlen einen andern Herren, und derselbe hei\u00dfet Trutz;", "tokens": ["W\u00e4h\u00b7len", "ei\u00b7nen", "an\u00b7dern", "Her\u00b7ren", ",", "und", "der\u00b7sel\u00b7be", "hei\u00b7\u00dfet", "Trutz", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "KON", "PDS", "VVFIN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Der will uns hin\u00fcber f\u00fchren ohne euren sichern Pa\u00df,", "tokens": ["Der", "will", "uns", "hin\u00b7\u00fc\u00b7ber", "f\u00fch\u00b7ren", "oh\u00b7ne", "eu\u00b7ren", "si\u00b7chern", "Pa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Wo wir P\u00e4ss' uns selber schreiben mit des Blutes rotem Na\u00df.", "tokens": ["Wo", "wir", "P\u00e4ss'", "uns", "sel\u00b7ber", "schrei\u00b7ben", "mit", "des", "Blu\u00b7tes", "ro\u00b7tem", "Na\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NE", "PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Unsre Mauern, unsre Th\u00fcrme, unsre ganze liebe Stadt,", "tokens": ["Uns\u00b7re", "Mau\u00b7ern", ",", "uns\u00b7re", "Th\u00fcr\u00b7me", ",", "uns\u00b7re", "gan\u00b7ze", "lie\u00b7be", "Stadt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "So die heil'ge Mutter Gottes selber sich ersehen hat,", "tokens": ["So", "die", "heil'\u00b7ge", "Mut\u00b7ter", "Got\u00b7tes", "sel\u00b7ber", "sich", "er\u00b7se\u00b7hen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "NN", "ADV", "PRF", "VVPP", "VAFIN", "$,"], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.11": {"text": "Da\u00df sie von der Felsenspitze auf dem letzten Uferrand", "tokens": ["Da\u00df", "sie", "von", "der", "Fel\u00b7sen\u00b7spit\u00b7ze", "auf", "dem", "letz\u00b7ten", "Uf\u00b7er\u00b7rand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Tr\u00f6stend \u00fcberschauen m\u00f6chte das gebeugte Griechenland:", "tokens": ["Tr\u00f6s\u00b7tend", "\u00fc\u00b7bersc\u00b7hau\u00b7en", "m\u00f6ch\u00b7te", "das", "ge\u00b7beug\u00b7te", "Grie\u00b7chen\u00b7land", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VMFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Diese Stadt habt ihr verhandelt, Britten, die ihr sch\u00fctzen wollt,", "tokens": ["Die\u00b7se", "Stadt", "habt", "ihr", "ver\u00b7han\u00b7delt", ",", "Brit\u00b7ten", ",", "die", "ihr", "sch\u00fct\u00b7zen", "wollt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "PPER", "VVPP", "$,", "NN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Britten, habt sie losgeschlagen f\u00fcr des alten Paschen Gold.", "tokens": ["Brit\u00b7ten", ",", "habt", "sie", "los\u00b7ge\u00b7schla\u00b7gen", "f\u00fcr", "des", "al\u00b7ten", "Pa\u00b7schen", "Gold", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "H\u00e4ttet wohl auch unsre H\u00e4upter gern gegeben in den Kauf?", "tokens": ["H\u00e4t\u00b7tet", "wohl", "auch", "uns\u00b7re", "H\u00e4up\u00b7ter", "gern", "ge\u00b7ge\u00b7ben", "in", "den", "Kauf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PPOSAT", "NN", "ADV", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Und der grimme Heide wetzte schon sein Henkerbeil darauf.", "tokens": ["Und", "der", "grim\u00b7me", "Hei\u00b7de", "wetz\u00b7te", "schon", "sein", "Hen\u00b7ker\u00b7beil", "da\u00b7rauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "PAV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Britten, Britten, an den H\u00e4nden klebt es r\u00f6ther euch, als Blut!", "tokens": ["Brit\u00b7ten", ",", "Brit\u00b7ten", ",", "an", "den", "H\u00e4n\u00b7den", "klebt", "es", "r\u00f6\u00b7ther", "euch", ",", "als", "Blut", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "PPER", "$,", "KOUS", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Britten, Britten, das ist jenes S\u00fcndengoldes H\u00f6llengluth!", "tokens": ["Brit\u00b7ten", ",", "Brit\u00b7ten", ",", "das", "ist", "je\u00b7nes", "S\u00fcn\u00b7den\u00b7gol\u00b7des", "H\u00f6l\u00b7len\u00b7gluth", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PDS", "VAFIN", "PDAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.19": {"text": "Und ein hoher Scheiterhaufen stieg auf unsrem Mark empor,", "tokens": ["Und", "ein", "ho\u00b7her", "Schei\u00b7ter\u00b7hau\u00b7fen", "stieg", "auf", "uns\u00b7rem", "Mark", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Und mit Schaufeln und mit Hacken zogen wir aus jedem Thor:", "tokens": ["Und", "mit", "Schau\u00b7feln", "und", "mit", "Ha\u00b7cken", "zo\u00b7gen", "wir", "aus", "je\u00b7dem", "Thor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "APPR", "NN", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.21": {"text": "Jeder grub sich die Gebeine seiner Lieben aus der Gruft,", "tokens": ["Je\u00b7der", "grub", "sich", "die", "Ge\u00b7bei\u00b7ne", "sei\u00b7ner", "Lie\u00b7ben", "aus", "der", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ART", "NN", "PPOSAT", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.22": {"text": "Und in freien Flammen lodernd flog der Staub in freie Luft.", "tokens": ["Und", "in", "frei\u00b7en", "Flam\u00b7men", "lo\u00b7dernd", "flog", "der", "Staub", "in", "frei\u00b7e", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVPP", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.23": {"text": "Ach, wohl h\u00e4tten wir uns selber gern gest\u00fcrzt in seine Gluth,", "tokens": ["Ach", ",", "wohl", "h\u00e4t\u00b7ten", "wir", "uns", "sel\u00b7ber", "gern", "ge\u00b7st\u00fcrzt", "in", "sei\u00b7ne", "Gluth", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VAFIN", "PPER", "PRF", "ADV", "ADV", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.24": {"text": "Doch der Weiber und der Kinder Jammer brach der M\u00e4nner Muth;", "tokens": ["Doch", "der", "Wei\u00b7ber", "und", "der", "Kin\u00b7der", "Jam\u00b7mer", "brach", "der", "M\u00e4n\u00b7ner", "Muth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "ART", "NN", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.25": {"text": "Und so zogen wir von dannen bei der Leichenflammen Schein,", "tokens": ["Und", "so", "zo\u00b7gen", "wir", "von", "dan\u00b7nen", "bei", "der", "Lei\u00b7chen\u00b7flam\u00b7men", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.26": {"text": "Und die Brittenschiffe nahmen unsres Elends Lasten ein.", "tokens": ["Und", "die", "Brit\u00b7ten\u00b7schif\u00b7fe", "nah\u00b7men", "uns\u00b7res", "E\u00b7lends", "Las\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.27": {"text": "Haben nun zwei Jahr' gesessen hier auf ", "tokens": ["Ha\u00b7ben", "nun", "zwei", "Jahr'", "ge\u00b7ses\u00b7sen", "hier", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "CARD", "NN", "VVPP", "ADV", "APPR"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.28": {"text": "Haben nun zwei Jahr' geschauet sehnlich nach der Heimath Strand.", "tokens": ["Ha\u00b7ben", "nun", "zwei", "Jahr'", "ge\u00b7schau\u00b7et", "sehn\u00b7lich", "nach", "der", "Hei\u00b7math", "Strand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "CARD", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.29": {"text": "Britten, habt uns Schutz gegeben, und noch Ketten auch dabei:", "tokens": ["Brit\u00b7ten", ",", "habt", "uns", "Schutz", "ge\u00b7ge\u00b7ben", ",", "und", "noch", "Ket\u00b7ten", "auch", "da\u00b7bei", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "NN", "VVPP", "$,", "KON", "ADV", "NN", "ADV", "PAV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.30": {"text": "Euren Schutz und eure Ketten brechen heute wir entzwei.", "tokens": ["Eu\u00b7ren", "Schutz", "und", "eu\u00b7re", "Ket\u00b7ten", "bre\u00b7chen", "heu\u00b7te", "wir", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN", "ADV", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.31": {"text": "Br\u00fcder, la\u00dft uns f\u00fcrder ziehen! Dr\u00fcben liegt ja unsre Stadt,", "tokens": ["Br\u00fc\u00b7der", ",", "la\u00dft", "uns", "f\u00fcr\u00b7der", "zie\u00b7hen", "!", "Dr\u00fc\u00b7ben", "liegt", "ja", "uns\u00b7re", "Stadt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "ADJD", "VVINF", "$.", "ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.32": {"text": "So die heil'ge Mutter Gottes selber sich ersehen hat,", "tokens": ["So", "die", "heil'\u00b7ge", "Mut\u00b7ter", "Got\u00b7tes", "sel\u00b7ber", "sich", "er\u00b7se\u00b7hen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "NN", "ADV", "PRF", "VVPP", "VAFIN", "$,"], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.33": {"text": "Da\u00df sie von der Felsenspitze auf dem ersten Uferrand", "tokens": ["Da\u00df", "sie", "von", "der", "Fel\u00b7sen\u00b7spit\u00b7ze", "auf", "dem", "ers\u00b7ten", "Uf\u00b7er\u00b7rand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.34": {"text": "Segnend \u00fcberschauen m\u00f6chte das erwachte Griechenland.", "tokens": ["Seg\u00b7nend", "\u00fc\u00b7bersc\u00b7hau\u00b7en", "m\u00f6ch\u00b7te", "das", "er\u00b7wach\u00b7te", "Grie\u00b7chen\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VMFIN", "PDS", "VVFIN", "NE", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.35": {"text": "Br\u00fcder, dahin la\u00dft uns ziehen, eh' der hohe Schutzpatron,", "tokens": ["Br\u00fc\u00b7der", ",", "da\u00b7hin", "la\u00dft", "uns", "zie\u00b7hen", ",", "eh'", "der", "ho\u00b7he", "Schutz\u00b7pat\u00b7ron", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PAV", "VVFIN", "PPER", "VVINF", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.36": {"text": "Uns statt seiner zu besch\u00fctzen, rufe seinen Kerkerfrohn.", "tokens": ["Uns", "statt", "sei\u00b7ner", "zu", "be\u00b7sch\u00fct\u00b7zen", ",", "ru\u00b7fe", "sei\u00b7nen", "Ker\u00b7ker\u00b7frohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "PTKZU", "VVINF", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.37": {"text": "Br\u00fcder, dahin la\u00dft uns ziehen, weil wir noch in unsrer Hand", "tokens": ["Br\u00fc\u00b7der", ",", "da\u00b7hin", "la\u00dft", "uns", "zie\u00b7hen", ",", "weil", "wir", "noch", "in", "uns\u00b7rer", "Hand"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PAV", "VVFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "A