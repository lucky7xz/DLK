{"textgrid.poem.65629": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Meinen Vater, meine Mutter haben sie in's Meer ers\u00e4uft,", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meinen Vater, meine Mutter haben sie in's Meer ers\u00e4uft,", "tokens": ["Mei\u00b7nen", "Va\u00b7ter", ",", "mei\u00b7ne", "Mut\u00b7ter", "ha\u00b7ben", "sie", "in's", "Meer", "er\u00b7s\u00e4uft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Haben ihre heil'gen Leichen durch die Stra\u00dfen hingeschleift;", "tokens": ["Ha\u00b7ben", "ih\u00b7re", "heil'\u00b7gen", "Lei\u00b7chen", "durch", "die", "Stra\u00b7\u00dfen", "hin\u00b7ge\u00b7schleift", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Meine sch\u00f6ne Schwester haben aus der Kammer sie gejagt,", "tokens": ["Mei\u00b7ne", "sch\u00f6\u00b7ne", "Schwes\u00b7ter", "ha\u00b7ben", "aus", "der", "Kam\u00b7mer", "sie", "ge\u00b7jagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Haben auf dem freien Markte sie verkauft als eine Magd.", "tokens": ["Ha\u00b7ben", "auf", "dem", "frei\u00b7en", "Mark\u00b7te", "sie", "ver\u00b7kauft", "als", "ei\u00b7ne", "Magd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "PPER", "VVPP", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "H\u00f6r' ich eine Woge rauschen, ist es mir, als ob's mich ruft,", "tokens": ["H\u00f6r'", "ich", "ei\u00b7ne", "Wo\u00b7ge", "rau\u00b7schen", ",", "ist", "es", "mir", ",", "als", "ob's", "mich", "ruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,", "VAFIN", "PPER", "PPER", "$,", "KOUS", "NE", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Ja, mich rufen meine Eltern aus der tiefen, weiten Gruft,", "tokens": ["Ja", ",", "mich", "ru\u00b7fen", "mei\u00b7ne", "El\u00b7tern", "aus", "der", "tie\u00b7fen", ",", "wei\u00b7ten", "Gruft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Rufen Rache \u2013 und ich schleudre T\u00fcrkenk\u00f6pfe in die Fluth,", "tokens": ["Ru\u00b7fen", "Ra\u00b7che", "\u2013", "und", "ich", "schleud\u00b7re", "T\u00fcr\u00b7ken\u00b7k\u00f6p\u00b7fe", "in", "die", "Fluth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$(", "KON", "PPER", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Bis ges\u00e4ttigt ist die Rache, bis die wilde Woge ruht.", "tokens": ["Bis", "ge\u00b7s\u00e4t\u00b7tigt", "ist", "die", "Ra\u00b7che", ",", "bis", "die", "wil\u00b7de", "Wo\u00b7ge", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVPP", "VAFIN", "ART", "NN", "$,", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Aber wenn die Abendl\u00fcfte k\u00fchl um meine Schl\u00e4fe wehn,", "tokens": ["A\u00b7ber", "wenn", "die", "A\u00b7ben\u00b7dl\u00fcf\u00b7te", "k\u00fchl", "um", "mei\u00b7ne", "Schl\u00e4\u00b7fe", "wehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Ach, sie seufzen in die Ohren mir wie leises, banges Flehn.", "tokens": ["Ach", ",", "sie", "seuf\u00b7zen", "in", "die", "Oh\u00b7ren", "mir", "wie", "lei\u00b7ses", ",", "ban\u00b7ges", "Flehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "PPER", "KOKOM", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Ach, es sind der Schwester Seufzer in der Schmach der Sklaverei:", "tokens": ["Ach", ",", "es", "sind", "der", "Schwes\u00b7ter", "Seuf\u00b7zer", "in", "der", "Schmach", "der", "Skla\u00b7ve\u00b7rei", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ART", "NN", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Bruder, mache deine Schwester aus den schn\u00f6den Banden frei!", "tokens": ["Bru\u00b7der", ",", "ma\u00b7che", "dei\u00b7ne", "Schwes\u00b7ter", "aus", "den", "schn\u00f6\u00b7den", "Ban\u00b7den", "frei", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Ach, da\u00df ich ein Adler w\u00e4re, k\u00f6nnte schweben in den H\u00f6hn,", "tokens": ["Ach", ",", "da\u00df", "ich", "ein", "Ad\u00b7ler", "w\u00e4\u00b7re", ",", "k\u00f6nn\u00b7te", "schwe\u00b7ben", "in", "den", "H\u00f6hn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$,", "VMFIN", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Und mit schnellen, scharfen Blicken durch die St\u00e4dt' und Lande sp\u00e4hn,", "tokens": ["Und", "mit", "schnel\u00b7len", ",", "schar\u00b7fen", "Bli\u00b7cken", "durch", "die", "St\u00e4dt'", "und", "Lan\u00b7de", "sp\u00e4hn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "VVINF", "$,", "VVFIN", "NN", "APPR", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Bis ich meine Schwester f\u00e4nde, und sie aus der Feinde Hand", "tokens": ["Bis", "ich", "mei\u00b7ne", "Schwes\u00b7ter", "f\u00e4n\u00b7de", ",", "und", "sie", "aus", "der", "Fein\u00b7de", "Hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,", "KON", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Frei in meinem Schnabel tr\u00fcge nach dem freien Griechenland!", "tokens": ["Frei", "in", "mei\u00b7nem", "Schna\u00b7bel", "tr\u00fc\u00b7ge", "nach", "dem", "frei\u00b7en", "Grie\u00b7chen\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}