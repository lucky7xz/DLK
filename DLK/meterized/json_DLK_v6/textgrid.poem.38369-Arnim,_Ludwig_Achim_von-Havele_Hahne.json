{"textgrid.poem.38369": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Havele Hahne", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Havele havele Hahne,", "tokens": ["Ha\u00b7ve\u00b7le", "ha\u00b7ve\u00b7le", "Hah\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Fastennacht geht ane,", "tokens": ["Fas\u00b7ten\u00b7nacht", "geht", "a\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Droben in dem Hinkelhaus,", "tokens": ["Dro\u00b7ben", "in", "dem", "Hin\u00b7kel\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4ngt ein Korb mit Eier raus;", "tokens": ["H\u00e4ngt", "ein", "Korb", "mit", "Ei\u00b7er", "raus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Droben in der Firste,", "tokens": ["Dro\u00b7ben", "in", "der", "Firs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "H\u00e4ngen die Bratw\u00fcrste,", "tokens": ["H\u00e4n\u00b7gen", "die", "Brat\u00b7w\u00fcrs\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.7": {"text": "Gebt uns die langen,", "tokens": ["Gebt", "uns", "die", "lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "La\u00dft die kurzen hangen,", "tokens": ["La\u00dft", "die", "kur\u00b7zen", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Ri ra rum,", "tokens": ["Ri", "ra", "rum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Der Winter mu\u00df herum;", "tokens": ["Der", "Win\u00b7ter", "mu\u00df", "he\u00b7rum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Was wollt ihr uns denn geben,", "tokens": ["Was", "wollt", "ihr", "uns", "denn", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Ein gl\u00fcckseligs Leben,", "tokens": ["Ein", "gl\u00fcck\u00b7se\u00b7ligs", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Gl\u00fcck schlag ins Haus,", "tokens": ["Gl\u00fcck", "schlag", "ins", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Komm nimmermehr heraus.", "tokens": ["Komm", "nim\u00b7mer\u00b7mehr", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}