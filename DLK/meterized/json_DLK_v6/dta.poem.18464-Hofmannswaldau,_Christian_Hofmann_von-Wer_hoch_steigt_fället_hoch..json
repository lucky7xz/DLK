{"dta.poem.18464": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Wer hoch steigt/ f\u00e4llet hoch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was f\u00fcr dich ist zu hoch/ das soltu billich fliehen/", "tokens": ["Was", "f\u00fcr", "dich", "ist", "zu", "hoch", "/", "das", "sol\u00b7tu", "bil\u00b7lich", "flie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VAFIN", "PTKA", "ADJD", "$(", "PDS", "VMFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und deines wundsches seil mit flei\u00df zur\u00fccke ziehen.", "tokens": ["Und", "dei\u00b7nes", "wund\u00b7sches", "seil", "mit", "flei\u00df", "zu\u00b7r\u00fc\u00b7cke", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer etwas grosses sucht und hoch hinaus begehrt/", "tokens": ["Wer", "et\u00b7was", "gros\u00b7ses", "sucht", "und", "hoch", "hin\u00b7aus", "be\u00b7gehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJA", "VVFIN", "KON", "ADJD", "APZR", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem ist ein grosser fall und ungel\u00fcck beschert.", "tokens": ["Dem", "ist", "ein", "gros\u00b7ser", "fall", "und", "un\u00b7ge\u00b7l\u00fcck", "be\u00b7schert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}